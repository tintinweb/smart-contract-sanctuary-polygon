pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./ChildFamilySystemInterface.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./HierarchySupporter.sol";
import "./FamilySystemInterfaces.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";

contract TPM_Genesis_Avatar is
    ERC721,
    Pausable,
    AccessControl,
    ERC721Burnable,
    ERC721URIStorage,
    HierarchySupporter,
    AvatarDetailEditor,
    SimpleAvatarMinter,
    SimpleAvatarBurner,
    UriEditor,
    ERC2981
{
    using Counters for Counters.Counter;

    // Counters contract required variable
    Counters.Counter private _tokenIdCounter;

    // Access control roles
    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");
    bytes32 public constant ROYALTIES_EDITOR_ROLE = keccak256("ROYALTIES_EDITOR_ROLE");
    bytes32 public constant TRIBE_EDITOR_ROLE = keccak256("TRIBE_EDITOR_ROLE");
    bytes32 public constant URI_EDITOR_ROLE = keccak256("URI_EDITOR_ROLE");

    // External roles for avatar management
    bytes32 public constant BURNER = keccak256("BURNER");
    bytes32 public constant MINTER = keccak256("MINTER");

    uint256 public constant MAX_GENESIS_ZERO_AVATARS = 1000;

    // Family variables
    address public childContract; // Contract address of the next avatar generation
    uint256 public constant AVATAR_SPLITTING_AMOUNT = 10; // Number of avatars to mint in the next generation when burning one of the current one
    uint256 public constant INITIAL_GENESIS_ROYALTIES = 1 * 10**16; // 0.01% royalties for each avatar

    // Avatar attributes
    mapping(uint256 => uint256) public avatarRoyalties;
    mapping(uint256 => string) public avatarTribe;

    // Family badge token address
    address public familyBadgeTokenAddress;
    uint256 public familyBadgeTokenId;
    address public familyBadgeReceiver;

    // Royalties information
    address public royaltiesReceiver;
    uint96 public constant CONTRACT_ROYALTIES_BPS = 250;

    constructor() ERC721("TPM_Genesis", "TPM") {
        // Add initial roles to the deployer
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MANAGER_ROLE, msg.sender);
        // Set royalties for the collection
        royaltiesReceiver = msg.sender;
        _setDefaultRoyalty(msg.sender, CONTRACT_ROYALTIES_BPS);
    }

    function getHierarchyLevel() external view returns (uint256) {
        return 0;
    }

    // Manage information about the Family Badge
    function updateFamilyBadgeTokenInfo(
        address _newTokenAddress,
        uint256 newTokenId
    ) external onlyRole(MANAGER_ROLE) {
        familyBadgeTokenAddress = _newTokenAddress;
        familyBadgeTokenId = newTokenId;
    }

    function pause() public onlyRole(MANAGER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(MANAGER_ROLE) {
        _unpause();
    }

    function safeMint(address to, uint256 tokenId) private {
        require(_tokenIdCounter.current() < MAX_GENESIS_ZERO_AVATARS, "Max supply reached");
        _safeMint(to, tokenId);
        _setTokenRoyalty(tokenId, royaltiesReceiver, CONTRACT_ROYALTIES_BPS);
    }

    function mergeAvatars(uint256 avatarToMerge, uint256 avatarToKeep)
        public
        returns (uint256)
    {
        uint256 royaltiesToAdd = avatarRoyalties[avatarToMerge];
        uint256 currentRecipientRoyalties = avatarRoyalties[avatarToKeep];
        avatarRoyalties[avatarToKeep] = royaltiesToAdd + currentRecipientRoyalties;
        
        _burn(avatarToMerge);

        return avatarRoyalties[avatarToKeep];
    }

    function setTokenURI(uint256 tokenId, string memory _tokenURI)
        external
        onlyRole(URI_EDITOR_ROLE)
    {
        _setTokenURI(tokenId, _tokenURI);
    }

    function setChildContractAddress(address _childContract)
        external
        onlyRole(MANAGER_ROLE)
    {
        childContract = _childContract;
    }

    function getCurrentTokenId() external view returns (uint256) {
        return _tokenIdCounter.current();
    }

    function updateFamilyBadgeReceiver(address newReceiver) external onlyRole(MANAGER_ROLE) {
        familyBadgeReceiver = newReceiver;
    }

    // ------------ Overridden functions ------------

    function burn(uint256 tokenId) public override(ERC721Burnable) {
        address burner = msg.sender;
        ChildFamilySystemInterface childContractInstance = ChildFamilySystemInterface(
                childContract
            );
        string memory currentAvatarTribe = avatarTribe[tokenId];
        uint256 currentAvatarRoyalties = avatarRoyalties[tokenId];

        // Consume family badge in order to burn and mint a new generation
        ERC1155 familyBadgeContractInstance = ERC1155(familyBadgeTokenAddress);
        require(
            familyBadgeContractInstance.balanceOf(burner, familyBadgeTokenId) >
                0,
            "You don't own any family badge to create a new generation"
        );
        require(
            familyBadgeContractInstance.isApprovedForAll(burner, address(this)),
            "You need to approve the smart contract to burn the badge first"
        );
        familyBadgeContractInstance.safeTransferFrom(
            burner,
            familyBadgeReceiver,
            familyBadgeTokenId,
            1,
            "0x00"
        );

        for (uint256 i = 0; i < AVATAR_SPLITTING_AMOUNT; i++) {
            childContractInstance.mintAvatar(
                burner,
                currentAvatarRoyalties,
                currentAvatarTribe
            );
        }
        _burn(tokenId); // Burn the token with the classic function
        // Update royalties info about the burnt token
        avatarRoyalties[tokenId] = 0;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721URIStorage, ERC2981, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function _baseURI() internal pure override returns (string memory) {
        return "";
    }

    function _burn(uint256 tokenId)
        internal
        virtual
        override(ERC721, ERC721URIStorage)
    {
        super._burn(tokenId);
        avatarRoyalties[tokenId] = 0;
        _resetTokenRoyalty(tokenId);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal override whenNotPaused {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
        // Deleting the royalties if the avatar is sent to the 0x0...0 wallet
        if (to == address(0)) {
            avatarRoyalties[tokenId] = 0;
        }
    }

    // ------------ Access control roles management (Addition) ------------

    function addManager(address newManager) external onlyRole(MANAGER_ROLE) {
        _grantRole(MANAGER_ROLE, newManager);
    }

    function removeManager(address oldManager) external onlyRole(MANAGER_ROLE) {
        _revokeRole(MANAGER_ROLE, oldManager);
    }

    function addMinter(address newMinter) external onlyRole(MANAGER_ROLE) {
        _grantRole(MINTER, newMinter);
    }

    function removeMinter(address oldMinter) external onlyRole(MANAGER_ROLE) {
        _revokeRole(MINTER, oldMinter);
    }

    function addBurner(address newBurner) external onlyRole(MANAGER_ROLE) {
        _grantRole(BURNER, newBurner);
    }

    function removeBurner(address oldBurner) external onlyRole(MANAGER_ROLE) {
        _revokeRole(BURNER, oldBurner);
    }

    function addRoyaltiesEditor(address newRoyaltiesEditor)
        external
        onlyRole(MANAGER_ROLE)
    {
        _grantRole(ROYALTIES_EDITOR_ROLE, newRoyaltiesEditor);
    }

    function removeRoyaltiesEditor(address oldRoyaltiesEditor)
        external
        onlyRole(MANAGER_ROLE)
    {
        _revokeRole(ROYALTIES_EDITOR_ROLE, oldRoyaltiesEditor);
    }

    function addTribeEditor(address newTribeEditor)
        external
        onlyRole(MANAGER_ROLE)
    {
        _grantRole(TRIBE_EDITOR_ROLE, newTribeEditor);
    }

    function removeTribeEditor(address oldTribeEditor)
        external
        onlyRole(MANAGER_ROLE)
    {
        _revokeRole(TRIBE_EDITOR_ROLE, oldTribeEditor);
    }

    function addUriEditor(address newUriEditor)
        external
        onlyRole(MANAGER_ROLE)
    {
        _grantRole(URI_EDITOR_ROLE, newUriEditor);
    }

    function removeUriEditor(address oldUriEditor)
        external
        onlyRole(MANAGER_ROLE)
    {
        _revokeRole(URI_EDITOR_ROLE, oldUriEditor);
    }

    // ------------ FUNCTION TO BE CALLED FROM OTHER CONTRACTS ------------

    function burnAvatarSimple(uint256 tokenId) external onlyRole(BURNER) {
        _burn(tokenId);
    }

    function mintAvatarSimple(address to) external onlyRole(MINTER) {
        uint256 currentTokenId = _tokenIdCounter.current(); // Using counter for safe increments
        _safeMint(to, currentTokenId);
        avatarRoyalties[currentTokenId] = INITIAL_GENESIS_ROYALTIES;
        _tokenIdCounter.increment();
    }

    function updateRoyalties(uint256 tokenId, uint256 newRoyaltiesAmount)
        external
        onlyRole(ROYALTIES_EDITOR_ROLE)
    {
        avatarRoyalties[tokenId] = newRoyaltiesAmount;
    }

    function updateTribe(uint256 tokenId, string calldata newTribe)
        external
        onlyRole(TRIBE_EDITOR_ROLE)
    {
        avatarTribe[tokenId] = newTribe;
    }
}

