pragma solidity ^0.8.0;

import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router01.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./interfaces/IAbundanceToken.sol";
import "./interfaces/IUtopia.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "hardhat/console.sol";

contract AbundancePropertyManager is Ownable, ERC1155Holder {
  using SafeERC20 for IERC20;
  using SafeERC20 for IAbundanceToken;

  struct Listing {
    uint256 propertyId;
    address seller;
    uint256 numberOfShares;
    uint256 pricePerShare;
    bool listed;
    uint256 startTime;
    uint256 endTime;
  }

  struct Property {
    uint256 tokenId;
    uint256 totalShares;
    uint256 listedShares;
  }

  IUtopia utopia;

  IERC20 public paymentToken;
  IAbundanceToken public abundanceToken;

  IUniswapV2Router01 poolRouter;

  uint256 private minInvestmentAmount = 1e18;
  uint256 private maxInvestmentAmount = 50e18;

  uint256 private minAbundancePercentage = 1000;
  uint256 private abundanceDefaultPercentage = 1000;
  uint256 private maxAbundancePercentage = 2000;

  uint256 private PERCENTAGE_DENOMINATOR = 10000;

  uint256 private organizationSharePercentage = 1000;

  address public organizationAddress;

  mapping(uint256 => Property) public properties;
  uint256 public totalProperties;

  // store listing for properties
  // propertyId => listing array
  mapping(uint256 => Listing[]) public listings;
  // uint256 public totalListings;

  // Add a new mapping to store the active listing status
  mapping(uint256 => bool) public activeListings;

  mapping(address => bool) private propertyCreators;

  address ADMIN;

  event PropertyCreated(
    uint256 indexed propertyId,
    uint256 indexed tokenId,
    uint256 totalShares,
    address indexed agent
  );

  event ListingCreated(
    uint256 indexed listingId,
    uint256 indexed propertyId,
    address indexed seller,
    uint256 numbersOfShare,
    uint256 pricePerShare,
    uint256 startTime,
    uint256 endTime,
    bool listed
  );

  event ListingDelisted(uint256 indexed propertyId, uint256 indexed listingId, address indexed seller);

  event ListingPriceUpdated(uint256 indexed propertyId, uint256 indexed listingId, uint256 newPricePerShare);

  event PropertyInvested(
    uint256 indexed propertyId,
    uint256 indexed listingId,
    address indexed investor,
    uint256 numberOfShares,
    uint256 totalInvestment
  );

  constructor(
    address _utopia,
    address _paymentToken,
    address _abundanceToken,
    address _poolRouter,
    address _organizationAddress,
    address _admin
  ) {
    ADMIN = _admin;
    utopia = IUtopia(_utopia);
    totalProperties = 0;
    poolRouter = IUniswapV2Router01(_poolRouter);
    abundanceToken = IAbundanceToken(_abundanceToken);
    paymentToken = IERC20(_paymentToken);
    organizationAddress = _organizationAddress;
  }

  modifier onlyAdmin() {
    require(msg.sender == ADMIN, "only admin");
    _;
  }

  function grantPropertyCreatorRole(address acc) external onlyOwner {
    propertyCreators[acc] = true;
  }

  function revokePropertyCreatorRole(address acc) external onlyOwner {
    propertyCreators[acc] = false;
  }

  function createProperty(uint256 _shares) external {
    totalProperties++;

    uint256 _tokenId = utopia.mint(address(this), _shares, "");

    require(msg.sender != address(0), "zero address");
    require(propertyCreators[msg.sender], "access denied");
    require(_shares > 0, "zero shares");

    properties[totalProperties] = Property({tokenId: _tokenId, totalShares: _shares, listedShares: 0});

    uint256 organizationShares = (_shares * organizationSharePercentage) / PERCENTAGE_DENOMINATOR;
    uint256 creatorShares = _shares - organizationShares;

    // Transfer 10% of the property shares to the organization address
    // Transfer the remaining shares to the property creator
    // You may need to implement a function in the IUtopia contract to handle these transfers

    utopia.safeTransferFrom(address(this), organizationAddress, _tokenId, organizationShares, "");
    utopia.safeTransferFrom(address(this), msg.sender, _tokenId, creatorShares, "");

    propertyCreators[msg.sender] = false;

    emit PropertyCreated(totalProperties, _tokenId, _shares, msg.sender);
  }

  function listProperty(
    uint256 _propertyId,
    uint256 _numberOfShares,
    uint256 _pricePerShare,
    uint256 _startTime,
    uint256 _endTime
  ) external {
    require(msg.sender != address(0), "zero address");
    require(_startTime < _endTime, "Invalid Start time and end time");

    require(_propertyId <= totalProperties, "invalid propertyId");

    uint256 userBalance = utopia.balanceOf(msg.sender, _propertyId);
    require(_numberOfShares <= userBalance, "insufficient utopia balance");

    activeListings[_propertyId] = true;

    if (listings[_propertyId].length <= 0) {
      listings[_propertyId].push(Listing(0, address(0), 0, 0, false, 0, 0));
    }

    properties[_propertyId].listedShares += _numberOfShares;

    listings[_propertyId].push(
      Listing({
        propertyId: _propertyId,
        seller: msg.sender,
        numberOfShares: _numberOfShares,
        pricePerShare: _pricePerShare,
        listed: true, // Set listed to true
        startTime: _startTime,
        endTime: _endTime
      })
    );

    uint256 listingId = listings[_propertyId].length - 1;

    emit ListingCreated(
      listingId,
      _propertyId,
      msg.sender,
      _numberOfShares,
      _pricePerShare,
      _startTime,
      _endTime,
      true
    );
  }

  function delistProperty(uint256 _propertyId, uint256 _listingId) external {
    Listing storage listing = listings[_propertyId][_listingId];

    require(listing.seller == msg.sender, "only seller");
    require(listing.listed, "already delisted");

    listing.listed = false;

    properties[_propertyId].listedShares -= listing.numberOfShares;
    // Check if there are still any active listings for the property
    bool hasActiveListings = false;
    uint256 currentTime = block.timestamp;
    for (uint256 i = 1; i < listings[_propertyId].length; i++) {
      if (
        listings[_propertyId][i].listed &&
        listings[_propertyId][i].startTime <= currentTime &&
        listings[_propertyId][i].endTime >= currentTime
      ) {
        hasActiveListings = true;
        break;
      }
    }

    activeListings[_propertyId] = hasActiveListings;

    emit ListingDelisted(_propertyId, _listingId, msg.sender);
  }

  function relistProperty(uint256 _propertyId, uint256 _listingId, uint256 _startTime, uint256 _endTime) external {
    require(_startTime <= _endTime, "Invalid Start time and end time");

    Listing storage listing = listings[_propertyId][_listingId];

    require(listing.seller == msg.sender, "seller only");
    require(!listing.listed, "already listed");

    uint256 userBalance = utopia.balanceOf(msg.sender, _propertyId);
    require(listing.numberOfShares <= userBalance, "insufficient utopia balance");

    listing.listed = true;
    listing.startTime = _startTime;
    listing.endTime = _endTime;

    // Update the activeListings mapping for the property
    activeListings[_propertyId] = true;

    emit ListingCreated(
      _listingId,
      _propertyId,
      msg.sender,
      listing.numberOfShares,
      listing.pricePerShare,
      _startTime,
      _endTime,
      true
    );
  }

  function updateListingPrice(uint256 _propertyId, uint256 _listingId, uint256 _newPricePerShare) external {
    Listing storage listing = listings[_propertyId][_listingId];

    require(listing.seller == msg.sender, "Only the seller");
    require(listing.listed, "delisted");

    listing.pricePerShare = _newPricePerShare;

    emit ListingPriceUpdated(_propertyId, _listingId, _newPricePerShare);
  }

  function getInvestmentAmounts() public view returns (uint256, uint256) {
    return (minInvestmentAmount, maxInvestmentAmount);
  }

  function setInvesmentAmounts(uint256 _minAmount, uint256 _maxAmount) external onlyOwner {
    require(_maxAmount > _minAmount, "Invalid min investment amount and max investment amount");
    minInvestmentAmount = _minAmount;
    maxInvestmentAmount = _maxAmount;
  }

  function setMinMaxAbundancePercentages(uint256 _minPercentage, uint256 _maxPercentage) external onlyAdmin {
    require(_maxPercentage > _minPercentage, "Invalid min-max");
    require(_minPercentage > 0 && _maxPercentage <= PERCENTAGE_DENOMINATOR, "Invalid min-max");
    minAbundancePercentage = _minPercentage;
    maxAbundancePercentage = _maxPercentage;
  }

  function getMinMaxAbundancePercentages() public view returns (uint256, uint256) {
    return (minAbundancePercentage, maxAbundancePercentage);
  }

  function setAbundanceDefaultPercentage(uint256 _percentage) external onlyOwner {
    require(_percentage >= minAbundancePercentage, "Invalid percentage");
    require(_percentage <= maxAbundancePercentage, "Invalid percentage");
    abundanceDefaultPercentage = _percentage;
  }

  function isPropertyListed(uint256 _propertyId) public view returns (bool) {
    return activeListings[_propertyId];
  }

  function purchaseShares(uint256 _propertyId, uint256 _listingId, uint256 _numberOfShares) external {
    Listing storage listing = listings[_propertyId][_listingId];
    Property storage property = properties[_propertyId];

    require(msg.sender != address(0), "zero address");
    require(listing.listed, "Listing not available");
    require(listing.numberOfShares >= _numberOfShares, "Not enough shares");
    require(_numberOfShares > 0, "Zero shares");

    if (block.timestamp > listing.endTime || listing.startTime > block.timestamp) {
      if (block.timestamp > listing.endTime) {
        listing.listed = false;
      }
      revert("Listing not available");
    }

    uint256 investmentAmount = _numberOfShares * listing.pricePerShare;
    uint256 abundanceAmount = (investmentAmount * abundanceDefaultPercentage) / PERCENTAGE_DENOMINATOR;
    uint256 paymentAmount = investmentAmount - abundanceAmount;

    uint256 requiredAbundanceTokenAmount = getATAmountsOut(abundanceAmount);

    uint256 abundanceBalance = abundanceToken.balanceOf(msg.sender);

    require(investmentAmount >= minInvestmentAmount, "investment amount should be greater than min investment amount");

    require(investmentAmount <= maxInvestmentAmount, "investment amount should be less than max investment amount");

    if (abundanceBalance > requiredAbundanceTokenAmount) {
      // Burn the required abundance tokens
      abundanceToken.safeTransferFrom(msg.sender, address(this), requiredAbundanceTokenAmount);
      abundanceToken.burn(address(this), requiredAbundanceTokenAmount);
    } else {
      uint256 remainingAmount = requiredAbundanceTokenAmount - abundanceBalance;

      uint256 rAmountUSDT = getUSDTAmountsOut(remainingAmount);

      swapExactTokensForTokens(rAmountUSDT);
      abundanceToken.burn(address(this), remainingAmount);
      abundanceToken.burn(msg.sender, abundanceBalance);
    }

    // Transfer the abundance token amount from the investor to the property creator
    // Transfer the payment token amount from the investor to the property creator
    paymentToken.safeTransferFrom(msg.sender, listing.seller, paymentAmount);

    // Transfer the shares from the property creator to the investor
    // You may need to implement a function in the IUtopia contract to handle this transfer
    utopia.safeTransferFrom(listing.seller, msg.sender, property.tokenId, _numberOfShares, "");

    listing.numberOfShares -= _numberOfShares;
    property.listedShares -= _numberOfShares;

    if (property.listedShares == 0) {
      activeListings[_propertyId] = false;
    }

    if (listing.numberOfShares == 0) {
      listing.listed = false;

      emit ListingDelisted(_propertyId, _listingId, listing.seller);
    }

    // console.log(investmentAmount);
    emit PropertyInvested(_propertyId, _listingId, msg.sender, _numberOfShares, investmentAmount);
  }

  function swapExactTokensForTokens(uint256 tokenAmount) private returns (uint256 amountAT) {
    // generate the uniswap pair path of token -> weth
    address[] memory path = new address[](2);
    path[0] = address(paymentToken);
    path[1] = address(abundanceToken);

    // paymentToken.approve(address(poolRouter), tokenAmount);

    // make the swap
    amountAT = poolRouter.swapExactTokensForTokens(
      tokenAmount,
      0, // accept any amount of ETH
      path,
      address(this),
      block.timestamp
    )[1];
  }

  function getATAmountsOut(uint256 tokenAmount) private view returns (uint256 amountAT) {
    // generate the uniswap pair path of token -> weth
    address[] memory path = new address[](2);
    path[0] = address(paymentToken);
    path[1] = address(abundanceToken);

    // paymentToken.approve(address(poolRouter), tokenAmount);

    // make the swap
    amountAT = poolRouter.getAmountsOut(tokenAmount, path)[1];
  }

  function getUSDTAmountsOut(uint256 tokenAmount) private view returns (uint256 amountUSDT) {
    // generate the uniswap pair path of token -> weth
    address[] memory path = new address[](2);
    path[0] = address(abundanceToken);
    path[1] = address(paymentToken);

    // paymentToken.approve(address(poolRouter), tokenAmount);

    // make the swap
    amountUSDT = poolRouter.getAmountsOut(tokenAmount, path)[1];
  }

  function getAbundanceDefaultPercentage() public view returns (uint256) {
    return abundanceDefaultPercentage;
  }
}

