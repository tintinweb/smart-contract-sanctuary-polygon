// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/Strings.sol";



contract club3_V2 is Ownable {
    address defaulSponser;
    address zeroaddress = 0x0000000000000000000000000000000000000000;
    uint256 public fees3 = 150 ether;
    uint club3Levels = 3;
    struct club3 {
        address own;
        address sponser;
        address uplink;
        string side;
        address left;
        address right;
        bool[] isLeftFilled;
        bool[] isRightFilled;
        bool[] areLevelsComplete;
        int club;
        bool allLevelCompleted;
    }
    mapping(int => uint) public rewards;
    mapping(address => club3) public finder3;

    constructor() {
        transferOwnership(msg.sender);
        defaulSponser = msg.sender;
        bool[] memory _isLeftFilled;
        bool[] memory _isRightFilled;
        bool[] memory _areLevelCompleted;
        rewards[3] = 100 ether;
        finder3[defaulSponser] = club3(defaulSponser, defaulSponser,defaulSponser, "center", zeroaddress, zeroaddress,_isLeftFilled,_isRightFilled,_areLevelCompleted, 3, false);
    }

    function newBee(address _own, address _sponser, string memory _side)payable public{
        require(msg.value >= fees3, "Fees not enough");
        require(finder3[_own].own == 0x0000000000000000000000000000000000000000, "Address Already used please use anyother address.");
        club3 storage _spons = finder3[_sponser];
        if (_spons.own == 0x0000000000000000000000000000000000000000) {
            _sponser = defaulSponser;
            _spons = finder3[defaulSponser];
        }
        address _uplink;
        _uplink = findUplink(_sponser, _side);
        bool[] memory _isLeftFilled;
        bool[] memory _isRightFilled;
        bool[] memory _areLevelCompleted;
        club3 memory _newBee = club3(_own, _sponser, _uplink, _side, zeroaddress, zeroaddress, _isLeftFilled, _isRightFilled,_areLevelCompleted, 3, false);
        finder3[_own] = _newBee;
        addLink(_own, _uplink, _side);
        checkIfLevelCompleted(_uplink, _side, 0);
        //checkIfAllLevelCompleted(_own, _uplink);
    }

    function addLink(address _own , address _uplink, string memory _side) internal {
        if(keccak256(bytes(_side)) == keccak256(bytes("left"))) {
            finder3[_uplink].left = _own;
        }
        else if(keccak256(bytes(_side)) == keccak256(bytes("right"))){
            finder3[_uplink].right = _own;
        }
    }

    function findUplink(address _sponser, string memory _side) internal view returns(address uplink) {
        if(keccak256(bytes(_side)) == keccak256(bytes("left"))) {
            if(finder3[_sponser].left == zeroaddress){
                return _sponser;
            }
            return findUplink(finder3[_sponser].left, _side);
        }
        else if(keccak256(bytes(_side)) == keccak256(bytes("right"))) {
            if(finder3[_sponser].right == zeroaddress){
                return _sponser;
            }
            return findUplink(finder3[_sponser].right, _side);
        }
    }
    function checkIfLevelCompleted(address _uplink, string memory _side, uint _level) internal {
        if(keccak256(bytes(_side)) == keccak256(bytes("left"))) {
            if(finder3[_uplink].uplink == _uplink) {
                finder3[_uplink].isLeftFilled.push(true);
                if(!finder3[_uplink].allLevelCompleted){
                    distributeRewards(_uplink, rewards[3]);
                }
                
                return;
            }
            if(finder3[_uplink].isLeftFilled.length <= _level){
                finder3[_uplink].isLeftFilled.push(true);
                if(!finder3[_uplink].allLevelCompleted){
                    distributeRewards(_uplink, rewards[3]);
                }
                return checkIfLevelCompleted(finder3[_uplink].uplink,finder3[_uplink].side, _level+1);
            }
        }
        else if(keccak256(bytes(_side)) == keccak256(bytes("right"))) {
            if(finder3[_uplink].uplink == _uplink) {
                finder3[_uplink].isRightFilled.push(true);
                if(!finder3[_uplink].allLevelCompleted){
                    distributeRewards(_uplink, rewards[3]);
                }
                return;
            }
            if(finder3[_uplink].isRightFilled.length <= _level){
                finder3[_uplink].isRightFilled.push(true);
                if(!finder3[_uplink].allLevelCompleted){
                    distributeRewards(_uplink, rewards[3]);
                }
                return checkIfLevelCompleted(finder3[_uplink].uplink,finder3[_uplink].side, _level+1);
            }
        }
    }
    
    function distributeRewards(address _uplink, uint _reward) internal {
        if(finder3[_uplink].isLeftFilled.length != 0 && finder3[_uplink].isRightFilled.length != 0) {
            uint minimum = Math.min(finder3[_uplink].isLeftFilled.length, finder3[_uplink].isRightFilled.length);
            if(finder3[_uplink].areLevelsComplete.length < minimum) {
                (bool sent, bytes memory data) = _uplink.call{value: _reward }("");
                require(sent);
                finder3[_uplink].areLevelsComplete.push(true);
                if(finder3[_uplink].areLevelsComplete.length >= club3Levels){
                    finder3[_uplink].allLevelCompleted = true;
                }
            }
        }
    }
}