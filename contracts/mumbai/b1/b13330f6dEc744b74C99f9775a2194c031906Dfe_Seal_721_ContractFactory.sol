/**
 * need IERC2981 interface to use "type(IERC2981)" in overridden supportsInterface function
 * we could use ERC2981 and make this contract compatible as "contract Seal_721_Contract is ERC2981..."
 * but it's cheaper to use interface; contract is smaller & less overrides required
 */
import "@openzeppelin/contracts/interfaces/IERC2981.sol";

/////////////////////
//    Errors     //
/////////////////////

error Seal_721__TokenDoesNotExist(uint256 tokenID);
error Seal_721__MaxSupplyReached();
