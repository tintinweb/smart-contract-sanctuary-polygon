contract Seal_721_ContractFactory is Ownable {
    /////////////////////
    //      State      //
    /////////////////////

    /// @dev Events of the contract
    event ContractCreated(
        address owner,
        address contractAddress,
        string name,
        string symbol,
        bool privateView,
        string tokenType,
        uint256 royaltiesPercentage,
        address royaltiesReceiver
    );

    // Store mapping of deployed contract addresses against their owners
    mapping(address => address) public s_deployedContracts;

    ////////////////////////
    //   Main Functions   //
    ////////////////////////

    /**
     *  @notice Method for deploy new Seal_721_Contract
     *  @param _name Name of NFT contract
     *  @param _symbol Symbol of NFT contract
     */
    function create721Contract(
        string memory _name,
        string memory _symbol,
        string memory _baseURI,
        uint256 _maxSupply,
        uint256 _royaltiesPercentage,
        address payable _royaltiesReceiver,
        address _auction,
        address _marketplace,
        address _creator,
        bool _private
    ) external returns (address) {
        Seal_721_Contract newContract = new Seal_721_Contract(
            _name,
            _symbol,
            _baseURI,
            _maxSupply,
            _royaltiesPercentage,
            _royaltiesReceiver,
            _auction,
            _marketplace,
            _private
        );
        // add newContract address to "s_deployedContracts" mapping
        s_deployedContracts[address(newContract)] = _creator;

        // make the _msgSender the owner as opposed to the factory contract
        newContract.transferOwnership(_creator);

        string memory _tokenType = "ERC721";

        emit ContractCreated(
            _creator,
            address(newContract),
            _name,
            _symbol,
            _private,
            _tokenType,
            _royaltiesPercentage,
            _royaltiesReceiver
        );

        return address(newContract);
    }
}

