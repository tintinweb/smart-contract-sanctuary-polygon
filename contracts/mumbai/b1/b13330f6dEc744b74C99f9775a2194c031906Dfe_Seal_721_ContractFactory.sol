//    Contract     //
/////////////////////

/**
 * @title Seal_721_Contract
 */
contract Seal_721_Contract is ERC721, Ownable {
    using Strings for uint256;
    using Counters for Counters.Counter;
    Counters.Counter private supply;

    ////////////////
    //   Events   //
    ////////////////

    event TokenMinted(address indexed nftAddress, address indexed minter, uint256 indexed tokenId);

    string public s_baseURI;
    string public constant C_BASE_EXTENSION = ".json";

    // supply variables
    uint256 public s_maxSupply;

    //public/private
    bool public s_private;

    // need these addresses so we can give approval on deployment
    address private s_auction;
    address private s_marketplace;

    // Percentage of each sale to pay as royalties
    uint256 public s_royaltiesPercentage;
    // Address of the royalties recipient
    address public s_royaltiesReceiver;

    /// @notice Contract constructor
    constructor(
        string memory _name,
        string memory _symbol,
        string memory _baseURI,
        uint256 _maxSupply,
        uint256 _royaltiesPercentage,
        address payable _royaltiesReceiver,
        address _auction,
        address _marketplace,
        bool _private
    ) ERC721(_name, _symbol) {
        s_baseURI = _baseURI;
        s_maxSupply = _maxSupply;
        s_royaltiesPercentage = _royaltiesPercentage;
        s_royaltiesReceiver = _royaltiesReceiver;
        s_auction = _auction;
        s_marketplace = _marketplace;
        s_private = _private;
    }

    /**
     * @dev Owner mints a token to an address with a tokenURI.
     * @dev seller triggers xaction to buyer so as to pay gas & bcos they have final decision to accept deal or not
     * @param _to address of the future owner of the token; will need to come from FE
     */
    function mint(uint256 mintAmount, address _to) external onlyOwner {
        if (supply.current() + mintAmount > s_maxSupply) {
            revert Seal_721__MaxSupplyReached();
        }

        if (mintAmount <= 0) {
            revert Seal_721__InvalidMintAmount();
        }

        _mintLoop(_to, mintAmount);
    }

    function _mintLoop(address _to, uint256 mintAmount) internal {
        for (uint256 i = 0; i < mintAmount; i++) {
            supply.increment();
            _safeMint(_to, supply.current());
            emit TokenMinted(address(this), _to, supply.current());
        }
    }

    function totalSupply() public view returns (uint256) {
        return supply.current();
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        // Currently we just return one URI for all tokens bcos we only allow creation of 1 of 1s in FE.
        // To allow 1 of many we need to upgrade how baseuri works

        if (!_exists(tokenId)) {
            revert Seal_721__TokenDoesNotExist(tokenId);
        }
        string memory currentBaseURI = s_baseURI;
        return
            bytes(currentBaseURI).length > 0
                ? string(abi.encodePacked(currentBaseURI, tokenId.toString(), C_BASE_EXTENSION))
                : "";
    }

    /**
     * @dev this function will be used by our auction & mktplace contracts
     * @param owner the owner of the contract
     * @param operator the address you want to check if it's approved
     */
    function isApprovedForAll(
        address owner,
        address operator
    ) public view override returns (bool) {
        // Whitelist Fantom auction, marketplace contracts for easy trading.
        if (s_auction == operator || s_marketplace == operator) {
            return true;
        }

        // If passed through operator address not s_auction or s_marketplace; check it
        return super.isApprovedForAll(owner, operator);
    }

    /**
     * @notice Informs callers that this contract supports ERC2981
     */
    function supportsInterface(bytes4 interfaceId) public view override returns (bool) {
        return interfaceId == type(IERC2981).interfaceId || super.supportsInterface(interfaceId);
    }

    /**
     * @notice Marketplaces can retrieve information about how much royalties to send to whom by calling the NFT contract's royaltyInfo() function
     * @param _salePrice of the NFT
     */
    function royaltyInfo(
        uint256 /* _tokenId */,
        uint256 _salePrice
    ) external view returns (address receiver, uint256 royaltyAmount) {
        uint256 _royalties = (_salePrice * s_royaltiesPercentage) / 100;
        return (s_royaltiesReceiver, _royalties);
    }

    // change state variable so minted nfts become public
    function togglePrivate(bool _state) public onlyOwner {
        s_private = _state;
    }
}

