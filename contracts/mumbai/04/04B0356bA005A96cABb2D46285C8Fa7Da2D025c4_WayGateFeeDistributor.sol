    function convertWayTokens() public returns (uint) {
        address[] memory path = new address[](2);
        path[0] = address(wayGateToken);
        console.log("Before WETH()");
        path[1] = _dexRouter.WETH();
        console.log("PATH00: ", path[0]);
        console.log("PATH11: ", path[1]);
        uint256 contractWayBalance = wayGateToken.balanceOf(address(this)); //WAY Balance
        console.log("contractWayBalance::::  ", contractWayBalance);
        uint256 initialBalance = address(this).balance; //eth balance
        console.log("contractETHBalance::::  ", initialBalance);
        wayGateToken.approve(address(_dexRouter), contractWayBalance);
        console.log("contractETHBalance::::  ");
        _dexRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
            contractWayBalance,
            0,
            path,
            address(this),
            block.timestamp
        );
        uint256 ethAmount = address(this).balance - initialBalance;

        console.log("After SWAP Contract Balance: ", address(this).balance);
        return ethAmount;
