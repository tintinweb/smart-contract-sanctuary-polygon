pragma solidity 0.8.12;
pragma abicoder v2;

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "./utils/AccessProtectedUpgradable.sol";
import "./ApeironApostle.sol";

contract ApeironApostleSeasonMinting is
    Initializable,
    UUPSUpgradeable,
    OwnableUpgradeable
{
    using ECDSA for bytes32; // for verify signature
    using AddressUpgradeable for address; // for isContract
    using SafeERC20Upgradeable for IERC20Upgradeable; // for safe transfer

    enum SEASON_MINT_TYPE {
        SEASON_TWO_MINT
    }

    // signer address
    address private systemAddress;

    // contract address
    IERC20Upgradeable public token;
    ApeironApostle public apostleContract;

    // contract setting and parameters
    bool public isEnabled; // control the all minting period
    mapping(SEASON_MINT_TYPE => uint256) internal forSaleSchedule; // for sale schedule for each mint type
    mapping(SEASON_MINT_TYPE => uint256[]) public maxClassMintPerAddressMapping; // max mint count for each mint type & class, uint256[] index = ApostleClass
    mapping(SEASON_MINT_TYPE => uint256) public maxMintPerAddressMapping; // max mint count for each mint type
    mapping(SEASON_MINT_TYPE => uint256) public maxFreeMintPerAddressMapping; // max free mint count for each mint type
    mapping(SEASON_MINT_TYPE => uint256[]) public mintPriceMapping; // minting price for each mint type

    // data
    mapping(SEASON_MINT_TYPE => mapping(address => uint256[]))
        public addressMintedClassMapping; // address mint count for each mint type & class
    mapping(SEASON_MINT_TYPE => mapping(address => uint256))
        public addressFreeMintedMapping; // address free mint Count for each mint type

    // event
    event SaleScheduleUpdated(
        SEASON_MINT_TYPE indexed _mintType,
        uint256 _saleSchedule
    );
    event PurchaseSuccess(
        address indexed _tokenOwnerAddress,
        uint256 _dungeonApostleId,
        uint256 indexed _tokenId,
        uint256 _gene,
        uint256 _iv,
        bool _isFreeMint
    );

    // modifier
    modifier onlyDuringSale(SEASON_MINT_TYPE _mintType) {
        _requireArgument(
            isEnabled &&
                (forSaleSchedule[_mintType] > 0 &&
                    forSaleSchedule[_mintType] <= block.timestamp),
            "Sale is not enabled"
        );
        _;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize() external virtual initializer {
        __Ownable_init();
        __UUPSUpgradeable_init();
    }

    /// @dev Required by UUPSUpgradeable
    function _authorizeUpgrade(
        address newImplementation
    ) internal override onlyOwner {}

    /**
     * @notice - Update contract address
     * @dev - callable only by owner
     *
     * @param _nftAddress - NFT contract address
     * @param _tokenAddress - token contract address
     * @param _systemAddress - system contract address
     */
    function updateContractSetting(
        address _nftAddress,
        address _tokenAddress,
        address _systemAddress
    ) external onlyOwner {
        _requireArgument(_nftAddress.isContract(), "invalid address");
        _requireArgument(_tokenAddress.isContract(), "invalid address");
        _requireArgument(_systemAddress != address(0), "invalid address");

        apostleContract = ApeironApostle(_nftAddress);
        token = IERC20Upgradeable(_tokenAddress);
        systemAddress = _systemAddress;
    }

    /**
     * @notice - Enable/Disable Sales
     * @dev - callable only by owner
     *
     * @param _isEnabled - enable? sales
     */
    function setEnabled(bool _isEnabled) external onlyOwner {
        isEnabled = _isEnabled;
    }

    /**
     * @notice - Set sale schedule
     * @dev - callable only by owner
     *
     * @param _mintType - mint type
     * @param _time - sale schedule
     */
    function setForSaleSchedule(
        SEASON_MINT_TYPE _mintType,
        uint256 _time
    ) external onlyOwner {
        forSaleSchedule[_mintType] = _time;

        emit SaleScheduleUpdated(_mintType, _time);
    }

    /**
     * @notice - Set max class, total, and free mint count per address
     * @dev - callable only by owner
     *
     * @param _mintType - mint type
     * @param _maxMintCountArray - max mint count array, index = class
     * @param _maxMintCount - max mint count
     * @param _maxFreeMintCount - max free mint count
     */
    function setMaxMintCountPerAddress(
        SEASON_MINT_TYPE _mintType,
        uint256[] memory _maxMintCountArray,
        uint256 _maxMintCount,
        uint256 _maxFreeMintCount
    ) external onlyOwner {
        maxClassMintPerAddressMapping[_mintType] = _maxMintCountArray;
        maxMintPerAddressMapping[_mintType] = _maxMintCount;
        maxFreeMintPerAddressMapping[_mintType] = _maxFreeMintCount;
    }

    /**
     * @notice - Set mint price
     * @dev - callable only by owner
     *
     * @param _mintType - mint type
     * @param _mintPriceArray - mint price array
     */
    function setMintPriceMapping(
        SEASON_MINT_TYPE _mintType,
        uint256[] memory _mintPriceArray
    ) external onlyOwner {
        mintPriceMapping[_mintType] = _mintPriceArray;
    }

    /**
     * @notice - get mint price, mintPriceMapping cannot be used directly because it is uint256[]
     *
     * @param _mintType - mint type
     */
    function getMintPriceArray(
        SEASON_MINT_TYPE _mintType
    ) external view returns (uint256[] memory) {
        return mintPriceMapping[_mintType];
    }

    /**
     * @notice - init addressMintedClassMapping
     *
     * @param _mintType - mint type
     * @param _address - address
     */
    function _initAddressMintedClassArray(
        SEASON_MINT_TYPE _mintType,
        address _address
    ) internal {
        while (
            addressMintedClassMapping[_mintType][_address].length <
            uint256(type(ApostleMeta.ApostleClass).max) + 1
        ) {
            addressMintedClassMapping[_mintType][_address].push(0);
        }
    }

    /**
     * @notice - get total AddressMintedByAddress by mint type and address
     *
     * @param _mintType - mint type
     * @param _address - class of the NFT
     */
    function getTotalAddressMintedByAddress(
        SEASON_MINT_TYPE _mintType,
        address _address
    ) public view returns (uint256 count) {
        for (
            uint256 i = 0;
            i < addressMintedClassMapping[_mintType][_address].length;
            i++
        ) {
            count += addressMintedClassMapping[_mintType][_address][i];
        }
        return count;
    }

    /**
     * @notice - get addressMintedClassMapping by mint type and address
     *
     * @param _mintType - mint type
     * @param _address - class of the NFT
     */
    function getAddressMintedClassMapping(
        SEASON_MINT_TYPE _mintType,
        address _address
    ) external view returns (uint256[] memory) {
        return addressMintedClassMapping[_mintType][_address];
    }

    /**
     * @notice - request Purchase NFT, it will trigger Chainlink VRF
     * @dev - callable only during sale
     *
     * @param _mintType - mint type
     * @param _apostleClass - class of the NFT
     * @param _dungeonApostleId - Dungeon Apostle Id
     * @param _gene - gene of the NFT
     * @param _iv - iv of the NFT
     * @param _isFreeMint - is free mint?
     * @param _time - time of the signature
     * @param _signature - signature of the system
     */
    function requestPurchase(
        SEASON_MINT_TYPE _mintType,
        ApostleMeta.ApostleClass _apostleClass,
        uint256 _dungeonApostleId,
        uint256 _gene,
        uint256 _iv,
        bool _isFreeMint,
        uint256 _time,
        bytes memory _signature
    ) external onlyDuringSale(_mintType) {
        // init addressMintedClassMapping if possible
        _initAddressMintedClassArray(_mintType, msg.sender);

        uint256 price;
        if (_isFreeMint) {
            // check free mint count
            _requireArgument(
                addressFreeMintedMapping[_mintType][msg.sender] <
                    maxFreeMintPerAddressMapping[_mintType],
                "Exceed maximum free mint count"
            );

            price = 0;
        } else {
            // get price, free mint count will not increase the price
            price = mintPriceMapping[_mintType][
                getTotalAddressMintedByAddress(_mintType, msg.sender) -
                    addressFreeMintedMapping[_mintType][msg.sender]
            ];
        }

        // purchase apostle
        _purchase(
            _mintType,
            _apostleClass,
            _dungeonApostleId,
            _gene,
            _iv,
            price,
            _time,
            _signature
        );
    }

    /**
     * @notice - Purchase NFT, both purchase and free mint will call this function
     *
     * @param _mintType - mint type
     * @param _apostleClass - class of the NFT
     * @param _dungeonApostleId - Dungeon Apostle Id
     * @param _gene - gene of the NFT
     * @param _iv - iv of the NFT
     * @param _price - price of the NFT
     * @param _time - time of the signature
     * @param _signature - signature of the system
     */
    function _purchase(
        SEASON_MINT_TYPE _mintType,
        ApostleMeta.ApostleClass _apostleClass,
        uint256 _dungeonApostleId,
        uint256 _gene,
        uint256 _iv,
        uint256 _price,
        uint256 _time,
        bytes memory _signature
    ) internal {
        // check signature
        bool isFreeMint = (_price == 0);
        bytes32 hash = keccak256(
            abi.encodePacked(
                _mintType,
                _apostleClass,
                _dungeonApostleId,
                _gene,
                _iv,
                isFreeMint,
                _time,
                msg.sender
            )
        );
        _requireArgument(
            hash.toEthSignedMessageHash().recover(_signature) ==
                systemAddress &&
                _time + 600 >= block.timestamp, // valid signature period is 10 minutes
            "Invalid signature for Public Sale"
        );

        // check address minted clase count && total address minted count
        _requireArgument(
            addressMintedClassMapping[_mintType][msg.sender][
                uint256(_apostleClass)
            ] <
                maxClassMintPerAddressMapping[_mintType][
                    uint256(_apostleClass)
                ] &&
                getTotalAddressMintedByAddress(_mintType, msg.sender) <
                maxMintPerAddressMapping[_mintType],
            "Exceed maximum mint count"
        );

        // transfer token if this is not free mint
        if (!isFreeMint) {
            token.safeTransferFrom(msg.sender, address(this), _price);
        }

        // mint NFT
        uint256 tokenId = apostleContract.safeMint(
            _gene,
            _iv,
            false,
            msg.sender
        );

        // update free mint count
        if (isFreeMint) {
            addressFreeMintedMapping[_mintType][msg.sender] += 1;
        }
        // update address minted class count
        addressMintedClassMapping[_mintType][msg.sender][
            uint256(_apostleClass)
        ] += 1;

        // emit event
        emit PurchaseSuccess(
            msg.sender,
            _dungeonApostleId,
            tokenId,
            _gene,
            _iv,
            isFreeMint
        );
    }

    /**
     * @notice require funciton, save file size
     *
     * @param _canPass false will revert
     * @param _error error msg
     */
    function _requireArgument(bool _canPass, string memory _error) internal {
        require(_canPass, _error);
    }

    /**
     * @notice - Withdraw any ERC20
     *
     * @param _amount - amount to withdraw
     * @param _wallet - address to withdraw to
     */
    function withdrawFunds(
        uint256 _amount,
        address _wallet
    ) external onlyOwner {
        token.safeTransfer(_wallet, _amount);
    }
}

