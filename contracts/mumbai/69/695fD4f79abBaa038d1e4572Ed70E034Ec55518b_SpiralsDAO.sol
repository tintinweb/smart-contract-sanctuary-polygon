    /// @dev 
    function restake(uint256) public pure {
        // TODO implement v1 reinvestment (self-staking).
        // lock() -> vote() -> [after 1 epoch] activate()
        return;
    }

    
    function _swap(address _tokenIn, address _tokenOut, uint256 _amountIn) public {

        address[] memory path = new address[](2);
        path[0] = _tokenIn;
        path[1] = _tokenOut;

        IERC20(_tokenIn).approve(SUSHISWAP_ROUTER_ADDRESS, _amountIn);
        uint256[] memory amounts = IUniswapV2Router02(SUSHISWAP_ROUTER_ADDRESS).getAmountsOut(_amountIn, path);
        uint256 _amountOutMin = amounts[amounts.length - 1];
        IUniswapV2Router02(SUSHISWAP_ROUTER_ADDRESS).swapExactTokensForTokens(_amountIn, _amountOutMin, path, address(this), block.timestamp);

        emit TokenSwapped(_tokenIn, _amountIn, _tokenOut, _amountOutMin);
    }

    function getRouterAddress() public view returns (address) {
        return SUSHISWAP_ROUTER_ADDRESS;
    }

    function setRouterAddress(address _address) public {
        SUSHISWAP_ROUTER_ADDRESS = _address;
    }

    /*
     * CELO SMART CONTRACTS
     */

    /// @dev Returns a LockedGold.sol interface for interacting with the smart contract.
    function getLockedGold() internal view returns (ILockedGold) {
        return ILockedGold(LOCKED_GOLD_PROXY_ADDRESS);
    }

    /// @dev Returns an Election.sol interface for interacting with the smart contract. 
    function getElection() internal view returns (IElection) {
        // address electionAddr = getProxy(ELECTION_PROXY_ADDRESS)._getImplementation();
        return IElection(ELECTION_PROXY_ADDRESS);
    }

    /// @notice Returns the total balanceÂ of what the SpiralsDAO validator
    /// group has staked with itself.
    function getSelfStakedBalance() public view returns (uint256) {
        return _getActiveVotesForGroupByAccount(address(this), address(this));
    }

    /// @notice Returns the total number of active votes for a given
    /// validator group and staker address.
    /// @dev Queries Celo's Election.sol smart contract.
    function _getActiveVotesForGroupByAccount(address _validatorGroup, address _account) public view returns (uint256) {
        return getElection().getActiveVotesForGroupByAccount(_validatorGroup, _account);
    }

    /*
     * UTILITY
     */

    /// @dev Returns the total balance of the contract (in wei).
    function getTotalBalance() public view returns (uint256) {
        return address(this).balance;
    }

    /// @dev Returns the current value of `percentClimateFunding`.
    function getPercentClimateFunding() public view returns (uint256) {
        return _convertFracToPct(percentClimateFunding);
    }

    /// @dev Sets `percentClimateFunding` to a new value (percent of 100).
    function setPercentClimateFunding(uint256 newPercent) public {
        percentClimateFunding = FixidityLib.newFixedFraction(newPercent, 100);
    }

    /// @dev Modifier for checking whether function caller is `_owner`.
    modifier onlyOwner() {
        require(msg.sender == _owner, "Only owner can call this function!");
        _;
    }

    /// @dev Transfers ownership to a new owner. Can only be called by current
    /// owner.
    function setOwner(address _newOwner) public onlyOwner() {
        _owner = _newOwner;
    }

    /// @dev Returns the current owner.
    function getOwner() public view returns (address) {
        return _owner;
    }

    /// @dev Allows `_owner` to withdraw full contract balance.
    /// TODO get rid of this eventually, only useful for testing purposes
    function withdraw() public onlyOwner() {
        payable(_owner).transfer(getTotalBalance());
    }

    /// @dev Converts a fraction to a percentage.
    function _convertFracToPct(FixidityLib.Fraction memory _frac) internal pure returns (uint256) {
        return FixidityLib.fromFixed(FixidityLib.multiply(_frac, FixidityLib.newFixed(100)));
    }

    /// @dev Utility function for summing uint32 arrays.
    function _uint32ArraySum(uint32[] memory _array) internal pure returns (uint32) {
        uint32 sum = 0;
        for (uint32 i=0; i<_array.length; i++) {
            sum += _array[i];
        }
        return sum;
    }

    /// @dev Utility function for summing uint256 arrays.
    function _uint256ArraySum(uint256[] memory _array) internal pure returns (uint256) {
        uint256 sum = 0;
        for (uint32 i=0; i<_array.length; i++) {
            sum += _array[i];
        }
        return sum;
    }
}

