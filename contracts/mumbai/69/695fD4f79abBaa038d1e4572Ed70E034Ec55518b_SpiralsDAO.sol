    address LOCKED_GOLD_PROXY_ADDRESS = 0xbd0b5709e863038C6ef388E6c873ae2df00c18F2;
    address ELECTION_PROXY_ADDRESS = 0x1c3eDf937CFc2F6F51784D20DEB1af1F9a8655fA;
    address STABLE_TOKEN_PROXY_ADDRESS = 0x874069Fa1Eb16D44d622F2e0Ca25eeA172369bC1;

    // POLYGON MUMBAI TESTNET
    // https://github.com/sushiswap/sdk/blob/canary/packages/core-sdk/src/constants/addresses.ts
    address SUSHISWAP_ROUTER_ADDRESS = 0x1b02dA8Cb0d097eB8D57A175b88c7D8b47997506;
    address NCT_ADDRESS = 0x1b02dA8Cb0d097eB8D57A175b88c7D8b47997506;
    address USDC_ADDRESS = 0xe11A86849d99F524cAC3E7A0Ec1241828e332C62;

    // POLYGON MAINNET
    // address USDC_ADDRESS = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174;
    // address NCT_ADDRESS = 0xD838290e877E0188a4A44700463419ED96c16107;

    // Percentage of funds allocated towards climate project funding. The
    // rest is reinvested in low-risk yield generating channels like staking.
    FixidityLib.Fraction public percentClimateFunding;

    // Total project funding balance. Every epoch the SpiralsDAO validator
    // group receives epoch rewards and a percentage of that is meant to go
    // directly towards climate projects. We don't do this every epoch though
    // so the total value is being accumulated here until we release funds 
    // (about once a month).
    uint256 public projectFundingBalance;

    // The amount of validator rewards that is meant to be reinvested.
    // This balance is liquid (i.e. not locked via staking or LPs).
    uint256 public pendingInvestmentBalance;

    // Owner of smart contract. Only this account can initiate project funding.
    // See `fundProjects` to see how funds are distributed.
    address public _owner;
    
    // Stores all projects the DAO supports. New project can be added through
    // passing governance proposals.
    Project[] public projects;

    // Number of total projects being managed. We track this separately
    // rather than simply accessing `projects.length` to handle projects
    // that are removed. This counter is always incrementing when 
    uint32 public numProjects;

    // Stores all stakers and their preferences for how funding should be
    // allocated. A staker can update their preferences whenever they want
    // to adjust how their staking yields are distributed. If no preferences
    // are set, yield will go to the DAO treasury.
    mapping (address => Staker) stakers;

    uint256 MAX_INT = 2**256 - 1;

    /*
     * STRUCTS
     */
    struct Staker {
        // Tracks how much each user is staking with us, used to weight
        // user-driven contributions to total project shares. 
        uint256 amountStaked;
        // Sum of all project shares. Used to normalize staker's
        // contributions towards overall project shares.
        uint32 totalShares;
        // Project to shares.
        mapping (uint32 => FixidityLib.Fraction) sharesByProject;
    }
    struct Project {
        // Project ID assigned at creation from `numProjects`.
        uint32 id;
        // Each project has a share over funding distributed through the 
        // treasury, and it is aggregated across staker preferences throughout
        // the protocol (weighted by staked amount). 
        uint256 stakeAdjustedShares;
        string name;
        address payable wallet;
    }

    uint32 public numValidatorGroups;
    struct ValidatorGroup {
        uint32 id;
        address addr;
        uint32 amountApproved;
    }
    // List of validator groups registered with us.
    ValidatorGroup[] public validatorGroups;


    /*
     * EVENTS
     */
    event ProjectAdded(
        uint32 indexed projectId,
        string indexed projectName,
        address indexed projectAddress
    );

    event ProjectFunded(
        address indexed projectAddress,
        uint256 indexed fundingAmount
    );
    event Deposit(
        address indexed sender,
        uint256 amount
    );
    event TokenSwapped(
        address indexed tokenIn,
        uint256 amountIn,
        address indexed tokenOut,
        uint256 amountOut
    );

    event ValidatorGroupUpdated(
        uint32 indexed validatorGroupID,
        address indexed validatorGroupAddress,
        uint32 amountApproved
    );



    constructor () {
        _owner = msg.sender;
        percentClimateFunding = FixidityLib.newFixedFraction(50, 100);
    }

    receive() external payable {
        _deposit();
    }
    
    /// @dev For testing purposes
    function deposit() public payable {
        _deposit();
    }

    function _deposit() internal {
        // Split value from message into project funding (every month) and
        // reinvesting in the treasury (every epoch).
        FixidityLib.Fraction memory valueFixed = FixidityLib.newFixed(msg.value);
        projectFundingBalance += FixidityLib.fromFixed(
            FixidityLib.multiply(percentClimateFunding, valueFixed));

        // Reinvest whatever is leftover.
        uint256 reinvestedBalance = msg.value - projectFundingBalance;
        reinvest(reinvestedBalance);

        emit Deposit(msg.sender, msg.value);
    }

    function updateValidatorGroup(address _validatorGroupAddress, uint32 _amountApproved) external {
        
        bool found;
        uint32 validatorGroupID;
        for (uint32 i=0; i<validatorGroups.length; i++) {
            if (validatorGroups[i].addr == _validatorGroupAddress) {
                found = true;
                validatorGroupID = validatorGroups[i].id;
                validatorGroups[i].amountApproved = _amountApproved;
            }
        }
        if (!found) {
            validatorGroupID = numValidatorGroups;
            validatorGroups.push(ValidatorGroup(validatorGroupID, _validatorGroupAddress, _amountApproved));
            numValidatorGroups++;
        }
        emit ValidatorGroupUpdated(validatorGroupID, _validatorGroupAddress, _amountApproved);
    }

    function getValidatorGroup(address _validatorGroupAddress) public view returns (ValidatorGroup memory, bool) {
        
        for (uint32 i=0; i<validatorGroups.length; i++) {
            if (validatorGroups[i].addr == _validatorGroupAddress) {
                return (validatorGroups[i], true);
            }
        }
        return (ValidatorGroup(0, address(0), 0), false);
    }

    function pull(uint _amount) public {
        for (uint i=0; i<validatorGroups.length; i++) {
            IERC20(STABLE_TOKEN_PROXY_ADDRESS).transferFrom(msg.sender, address(this), _amount);
        }
    }

    /*
     * STAKERS
     */

    /// @notice Updates how a staker wants yield to be allocated across projects.
    function setSharesForStaker(uint32[] calldata _newShares) public {
        uint256 amountStaked = stakers[msg.sender].amountStaked;
        _adjustSharesForStaker(msg.sender, amountStaked, _newShares);
    }

    /// @notice Updates how much a staker is staking.
    function _setAmountForStaker(uint256 _newAmountStaked) internal {
        _adjustSharesForStaker(msg.sender, _newAmountStaked, getPctSharesForStaker());
    }

    /// @notice Updates project allocation and staked amount for staker.
    function setAmountAndSharesForStaker(address _address,
        uint256 _newAmountStaked, uint32[] calldata _newShares) public {
        _adjustSharesForStaker(_address, _newAmountStaked, _newShares);
    }

    /// @notice Helper function for updating staker yields across projects.
    /// @dev Update project shares for a given staker. This also requires 
    /// adjusting the total shares being tracked on each project, which can
    /// be affect by adjusting project share allocation or the staked amount. 
    /// TODO can potentially make this more gas-optimized by skipping no-op
    /// updates to storage (if nothing changes, don't touch storage).
    /// @param _address Address of the staker to make these updates for.
    /// @param _newAmountStaked The new amount staked for user.
    /// @param _newShares The new shares allocated for each project.
    function _adjustSharesForStaker(address _address, uint256 _newAmountStaked, 
        uint32[] memory _newShares) internal {

        require(_newShares.length == projects.length, 
            "New shares update array should be equal to number of active projects");

        uint256 currentAmountStaked = stakers[_address].amountStaked;
        uint32 totalNewShares = _uint32ArraySum(_newShares);

        for (uint32 i=0; i<projects.length; i++) {
            Project memory project = projects[i];

            // Subtract current staker's share contributions to project totals.
            // This ensures we don't go negative when we add the new share's
            // contributions and rebalance.
            FixidityLib.Fraction memory currentContributionToShares = FixidityLib.multiply(
                stakers[_address].sharesByProject[project.id], 
                FixidityLib.newFixed(currentAmountStaked)
            );
            projects[i].stakeAdjustedShares -= FixidityLib.fromFixed(currentContributionToShares);

            // Add current staker's share contributions to total.
            FixidityLib.Fraction memory newContributionToShares = FixidityLib.multiply(
                FixidityLib.newFixedFraction(_newShares[i], totalNewShares), 
                FixidityLib.newFixed(_newAmountStaked)
            );
            projects[i].stakeAdjustedShares += FixidityLib.fromFixed(newContributionToShares);

            // Update project share values in mapping for given staker.
            stakers[_address].sharesByProject[project.id] = 
                FixidityLib.newFixedFraction(_newShares[i], totalNewShares);
        }
        stakers[_address].amountStaked = _newAmountStaked;
        stakers[_address].totalShares = totalNewShares;
    }

    /// @notice Returns the project percentages for caller.
    function getPctSharesForStaker() public view returns(uint32[] memory) { 
        return _getPctSharesForStaker(msg.sender);
    }

    /// @notice Returns the project weights for a given staker.
    /// @dev Iterates through all projects and fetches shares allocated for
    /// each project.
    function _getPctSharesForStaker(address _address) public view returns(uint32[] memory) { 

        uint32[] memory shares = new uint32[](projects.length);
        for (uint32 i=0; i < projects.length; i++) {
            // Safe explicit conversion since we are only storing numbers
            // <=100 as a fraction in `sharesByProject`.
            shares[i] = uint32(_convertFracToPct(
                stakers[_address].sharesByProject[projects[i].id]));
        }
        return shares;
    }

    /*
     * PROJECTS
     */

    /// @notice Returns all projects!
    function getProject(address _address) public view returns (Project memory, bool) {
        for (uint i=0; i<projects.length; i++) {
            if (projects[i].wallet == _address) {
                return (projects[i], true);
            }
        }
        return (Project(0, 0, "", payable(_owner)), false);
    }

    /// @notice Adds a climate project to the SpiralsDAO registry.
    function addProject(string calldata _name, address payable _address) 
        public
    {
        numProjects += 1;
        uint32 projectId = numProjects;
        projects.push(Project(
            projectId,
            0, // all projects start off with 0 shares
            _name,
            _address
        ));
        emit ProjectAdded(projectId, _name, _address);
    }

    /// @notice Removes a climate project from the SpiralsDAO registry. 
    /// @dev No-op if project address is not registered. No need to recompute
    /// project shares. This is sort of handled for us because project shares
    /// are normalized in `fundProjects`. The only weirdness that results from
    /// this is we'll have hanging entries in each staker's share mapping, but
    /// the code only accesses entries for projects it knows are active by
    /// looping over `projects`.
    function removeProject(address _address)
        public
    {
        uint256 indexToDelete;
        uint256 idToDelete;
        bool foundProject;
        for (uint i=0; i<projects.length; i++) {
            if (projects[i].wallet == _address) {
                indexToDelete = i;
                idToDelete = projects[i].id;
                foundProject = true;
                break;
            }
        }
        if (foundProject) {
            _removeProjectAtIndex(indexToDelete);
        }
    }

    /// @dev Utility function for removing project at a given index. Most 
    /// gas-efficient way to do this in Solidity is to swap index to
    /// remove element with last element and pop.
    function _removeProjectAtIndex(uint256 _index) internal {

        require(_index < projects.length, "Out of bounds access in project array!");
        // Only update index to remove if it isn't already the last element.
        if (_index != projects.length - 1) {
            // TODO is this a shallow copy
            projects[_index] = projects[projects.length-1];
        }
        projects.pop();
    }

    /// @notice Triggers a round of funding for projects registered with SpiralsDAO.
    /// @dev Converts total shares tracked on each project into percentages
    /// and transfers fraction of balance reserved for project funding.
    function fundProjects() public {

        require(msg.sender == _owner, "Only owner can call this function!");

        FixidityLib.Fraction[] memory fundingPercentages = _getProjectFundingPercentages();

        FixidityLib.Fraction memory totalFundingFrac = FixidityLib.newFixed(projectFundingBalance);
        for (uint32 i=0; i < projects.length; i++) {

            Project memory project = projects[i];
            uint256 amount = FixidityLib.fromFixed(FixidityLib.multiply(totalFundingFrac, fundingPercentages[i]));

            emit ProjectFunded(project.wallet, amount);
            project.wallet.transfer(amount);
            projectFundingBalance -= amount;
        }
    }
    
    /// @notice Retrive total project funding shares as a percentage.
    function getProjectFundingPercentages() public view returns (uint256[] memory) {
        FixidityLib.Fraction[] memory projectShareFracs = _getProjectFundingPercentages();
        uint256[] memory projectPcts = new uint256[](projects.length);
        for (uint32 i=0; i<projects.length; i++) {
            projectPcts[i] = _convertFracToPct(projectShareFracs[i]);
        }
        return projectPcts;
    }

    /// @notice Returns how much each currently registered project should be
    /// funded, expressed as a percentage.
    function _getProjectFundingPercentages() internal view returns (FixidityLib.Fraction[] memory) {

        uint256[] memory projectShares = new uint256[](projects.length);
        for (uint256 i=0; i<projects.length; i++) {
            projectShares[i] = projects[i].stakeAdjustedShares;
        }
        uint256 totalProjectShares = _uint256ArraySum(projectShares);
        return _convertSharesToPercentages(totalProjectShares, projectShares);
    }

    /// @notice Normalizes a list of shares into percentages.
    /// @dev Utility function for converting uint256 shares into 
    /// FixidityLib.Fraction percentages.
    function _convertSharesToPercentages(uint256 _total, 
        uint256[] memory _shares) internal pure returns (FixidityLib.Fraction[] memory) {

        require(_shares.length > 0, "Need at least 1 share in input array");

        FixidityLib.Fraction memory totalPct = FixidityLib.newFixed(0);
        FixidityLib.Fraction[] memory pcts = new FixidityLib.Fraction[](_shares.length);
        
        for (uint256 i=0; i<_shares.length; i++) {
            FixidityLib.Fraction memory pct = FixidityLib.newFixedFraction(_shares[i], _total);
            pcts[i] = pct;

            totalPct = FixidityLib.add(totalPct, pct);
        }

        if (FixidityLib.lt(totalPct, FixidityLib.fixed1())) {
            // If there is any remainder, add to first element.
            uint256 remainder = 100 - _convertFracToPct(totalPct);
            pcts[0] = FixidityLib.add(pcts[0], FixidityLib.newFixedFraction(remainder, 100));
        }

        return pcts;
    }

    /*
     * REINVESTING
     */

    /// @notice Reinvest validator rewards in yield-generating DeFi assets.
    /// @param _amount The amount (in wei) to reinvest.
    function reinvest(uint256 _amount) public {

        pendingInvestmentBalance += _amount;
        // restake(_amount);
        return;
    }

