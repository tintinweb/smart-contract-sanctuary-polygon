        MINT_PRICE = 0.11 ether;
        MAX_MINT_QTY = 10;
        MAX_COLLECTION_SIZE = 2500;
        BASE_URI = "ipfs://";
        CLAIM_ENABLED = false;
        PUBLIC_MINT_ENABLED = false;
