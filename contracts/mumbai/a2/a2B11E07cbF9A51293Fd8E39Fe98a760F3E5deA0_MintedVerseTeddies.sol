    event MaxCollectionSizeChanged(uint256 maxCollectionSize);
    event TreasuryAddressChanged(address treasuryAddress);
    event RoyaltyInfoChanged(uint96 royalty);

    string public constant SIGNING_DOMAIN = "MINTEDTEDDYNG";
    string public constant SIGNATURE_VERSION = "1";

    // The cost of each teddy.
    uint256 public MINT_PRICE;

    // The max mint quantity determines how many can be minted at a time.
    uint256 public MAX_MINT_QTY;

    // The total number of teddies that are available in this collection
    uint256 public MAX_COLLECTION_SIZE;

    // Teddies minted from our Series 1 collection in this contract are eligible for a preMint.
    address public MINTED_TEDDY_CONTRACT;

    // The baseUri will be used to determine the TokenURI
