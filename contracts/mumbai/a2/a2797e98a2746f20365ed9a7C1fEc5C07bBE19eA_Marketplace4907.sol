pragma solidity ^0.8.6;

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/interfaces/IERC165.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/interfaces/IERC721.sol";
import "@openzeppelin/contracts/interfaces/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./IMarketplace4907.sol";
import "./IERC4907.sol";
import "./ERC4907Sample.sol";

contract Marketplace4907 is ReentrancyGuard, IMarketplace4907, Ownable {
    using Counters for Counters.Counter;
    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.UintSet;

    address private _marketOwner;
    uint256 private _leasingFee = 0 ether;
    // uint256 private _leasingFee = 1 ether;
    Counters.Counter private _nftsLeased;
    mapping(address => mapping(bytes32 => Leasing)) private _leasingMap;
    mapping(address => mapping(bytes32 => mapping(uint8 => Renting))) _rentingMap;
    mapping(address => mapping(bytes32 => uint8)) public _numberOfRentMap;
    EnumerableSet.AddressSet private _nftContracts;

    constructor() {
        _marketOwner = msg.sender;
    }

    function getBalance() public view returns (uint256) {
        return address(this).balance;
    }

    function _verifySignature(
        address walletAddress,
        address nftContractAddress,
        address currencyContractAddress,
        uint256 tokenId,
        bytes memory _signature
    ) private pure returns (address) {
        return
            ECDSA.recover(
                keccak256(
                    abi.encode(
                        walletAddress,
                        nftContractAddress,
                        currencyContractAddress,
                        tokenId
                    )
                ),
                _signature
            );
    }

    function withdrawBalance() external payable onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    /**
     * @dev See {IMarketplace-leaseNFT}.
     */
    function leaseNFT(
        bytes32 listId,
        address nftContract,
        uint256 tokenId,
        address currencyContractAddress,
        uint256 price,
        uint256 availableFrom,
        uint256 availableTo,
        bytes memory _signature
    ) public payable virtual override nonReentrant {
        require(
            IERC721(nftContract).ownerOf(tokenId) == msg.sender,
            "Marketplace4907: You are not the owner of nft"
        );
        require(msg.value >= _leasingFee, "Not enough ether for leasing fee");
        require(
            availableFrom >= block.timestamp,
            "Marketplace4907: Start date cannot be in the past"
        );
        require(
            availableTo > availableFrom,
            "Marketplace4907: End date cannot be before the start date"
        );
        // Leasing storage leasing = _leasingMap[nftContract][tokenId];
        require(
            nftContract == address(0) ||
                block.timestamp > availableTo ||
                block.timestamp < availableFrom,
            "Marketplace4907: This NFT has already been leased in this time"
        );

        require(
            _marketOwner ==
                _verifySignature(
                    _marketOwner,
                    nftContract,
                    currencyContractAddress,
                    tokenId,
                    _signature
                ),
            "Marketplace4907: Invalid signature"
        );

        // clean up date before leasing
        _cleanData(nftContract, listId);

        payable(_marketOwner).transfer(_leasingFee);

        _leasingMap[nftContract][listId] = Leasing(
            listId,
            msg.sender,
            nftContract,
            tokenId,
            currencyContractAddress,
            price,
            availableFrom,
            availableTo
        );

        _nftsLeased.increment();
        EnumerableSet.add(_nftContracts, nftContract); // add nftContract to lease tracked contract

        emit LeasingNFT(
            listId,
            IERC721(nftContract).ownerOf(tokenId),
            IERC4907Sample(nftContract).getOriginalNFTContract(),
            nftContract,
            tokenId,
            currencyContractAddress,
            price,
            availableFrom,
            availableTo
        );
    }

    function unleaseNFT(
        address nftContract,
        bytes32 listId
    ) external virtual override nonReentrant {
        Leasing storage leasing = _leasingMap[nftContract][listId];
        require(
            leasing.owner != address(0),
            "Marketplace4907: You cannot unlease unleased nfts"
        );
        require(
            msg.sender == leasing.owner,
            "Marketplace4907: You are not allowed to unlease these Nfts"
        );
        delete _leasingMap[nftContract][listId];
        EnumerableSet.remove(_nftContracts, nftContract); // add nftContract to lease tracked contract
        emit UnleaseNFT(listId);
    }

    function updateNFT(
        bytes32 listId,
        address nftContractAddress,
        address newCurrencyContractAddress,
        uint256 newPrice,
        uint256 newStartDate,
        uint256 newEndDate
    ) external {
        Leasing storage listedNft = _leasingMap[nftContractAddress][listId];

        require(
            msg.sender == listedNft.owner,
            "Marketplace: You cannot update this batch of nfts"
        );
        require(newPrice > 0, "Marketplace: You cannot sell this nft for free");
        require(
            newStartDate >= block.timestamp,
            "Marketplace4907: Start date cannot be in the past"
        );
        require(
            newEndDate > newStartDate,
            "Marketplace4907: End date cannot be before the start date"
        );
        // Leasing storage leasing = _leasingMap[nftContract][tokenId];
        require(
            listedNft.nftContract == address(0) ||
                block.timestamp > newEndDate ||
                block.timestamp < newStartDate,
            "Marketplace4907: This NFT has already been leased in this time"
        );

        listedNft.price = newPrice;
        listedNft.currencyContractAddress = newCurrencyContractAddress;

        emit UpdateNFT(
            listId,
            newCurrencyContractAddress,
            newPrice,
            newStartDate,
            newEndDate
        );
    }

    /**
     * @dev See {IMarketplace-rentNFT}.
     */
    function rentNFT(
        address nftContract,
        bytes32 listId,
        uint256 startDate,
        uint256 endDate
    ) public payable virtual override nonReentrant {
        Leasing storage leasing = _leasingMap[nftContract][listId];
        uint256 price = leasing.price;
        uint256 tokenId = leasing.tokenId;
        require(
            isRentableNFT(nftContract),
            "Marketplace4907: Contract is not an ERC4907"
        );
        require(
            msg.sender != IERC721(nftContract).ownerOf(tokenId),
            "Marketplace4907: You can't rent your own token"
        );
        require(
            startDate >= block.timestamp,
            "Marketplace4907: Rental period cannot be in the past"
        );
        require(
            endDate <= leasing.availableTo,
            "Marketplace4907: Rental period exceeds max date rentable"
        );
        require(
            startDate >= leasing.availableFrom,
            "Marketplace4907: Rental period less than min date rentable"
        );

        if (leasing.currencyContractAddress == address(0)) {
            require(
                msg.value >= leasing.price,
                "Marketplace4907: Not enough money to cover rental period"
            );
            payable(leasing.owner).transfer(leasing.price);
        } else {
            uint256 balance = IERC20(leasing.currencyContractAddress).balanceOf(
                msg.sender
            );
            require(
                balance >= leasing.price,
                "Marketplace: You dont have enough money to buy this nft"
            );
            IERC20(leasing.currencyContractAddress).transferFrom(
                msg.sender,
                leasing.owner,
                leasing.price
            );
        }

        // Update rent info
        IERC4907(nftContract).setUser(tokenId, msg.sender, startDate, endDate);
        uint8 rentId = _numberOfRentMap[nftContract][listId];
        _rentingMap[nftContract][listId][rentId] = Renting(
            listId,
            msg.sender,
            nftContract,
            tokenId,
            startDate,
            endDate,
            leasing.currencyContractAddress,
            leasing.price
        );
        _numberOfRentMap[nftContract][listId]++;

        delete _leasingMap[nftContract][listId];
        EnumerableSet.remove(_nftContracts, nftContract); // add nftContract to lease tracked contract

        emit RentingNFT(
            listId,
            rentId,
            IERC4907Sample(nftContract).getOriginalNFTContract(),
            IERC721(nftContract).ownerOf(tokenId),
            msg.sender,
            nftContract,
            tokenId,
            startDate,
            endDate,
            leasing.currencyContractAddress,
            price
        );
    }

    /**
     * @dev See {IMarketplace-isRentableNFT}.
     */
    function isRentableNFT(
        address nftContract
    ) public view virtual override returns (bool) {
        bool _isRentable = false;
        bool _isNFT = false;
        try
            IERC165(nftContract).supportsInterface(type(IERC4907).interfaceId)
        returns (bool rentable) {
            _isRentable = rentable;
        } catch {
            return false;
        }
        try
            IERC165(nftContract).supportsInterface(type(IERC721).interfaceId)
        returns (bool nft) {
            _isNFT = nft;
        } catch {
            return false;
        }
        return _isRentable && _isNFT;
    }

    /**
     * @dev See {IMarketplace-getLeasingTime}.
     */
    function getLeasingTime(
        address nftContract,
        bytes32 listId
    ) public view virtual override returns (uint256, uint256) {
        uint256 availableFrom = _leasingMap[nftContract][listId].availableFrom;
        uint256 availableTo = _leasingMap[nftContract][listId].availableTo;
        return (availableFrom, availableTo);
    }

    /**
     * @dev See {IMarketplace-getLeasingFee}.
     */
    function getLeasingFee() public view virtual override returns (uint256) {
        return _leasingFee;
    }

    function setLeasingFee(uint256 newFee) external onlyOwner {
        _leasingFee = newFee;
    }

    /// @notice function to get leasing fee
    function _isDuplicateTimeWithOthers(
        address nftContract,
        bytes32 listId,
        uint256 startDate,
        uint256 endDate
    ) internal view returns (bool) {
        uint8 numberOfRent = _numberOfRentMap[nftContract][listId];
        bool isDuplicateTime = false;

        for (uint8 i = 0; i < numberOfRent; i++) {
            uint256 startDateOfCurrentRent = _rentingMap[nftContract][listId][i]
                .startDate;
            uint256 endDateOfCurrentRent = _rentingMap[nftContract][listId][i]
                .endDate;

            if (
                (endDate <= startDateOfCurrentRent &&
                    startDate <= endDateOfCurrentRent) ||
                (startDate >= endDateOfCurrentRent &&
                    startDate > startDateOfCurrentRent)
            ) {
                isDuplicateTime = false;
            } else {
                isDuplicateTime = true;
                break;
            }
        }
        return isDuplicateTime;
    }

    // @notice function to clean up data before leasing
    /// @param nftContract  Contract address of NFT
    /// @param listId  ID of Token
    function _cleanData(address nftContract, bytes32 listId) internal {
        uint8 numberOfRent = _numberOfRentMap[nftContract][listId];
        for (uint8 i = 0; i <= numberOfRent; i++) {
            delete _rentingMap[nftContract][listId][i];
        }
        _numberOfRentMap[nftContract][listId] = 0;
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.6;

interface IMarketplace4907 {
    struct Leasing {
        bytes32 listId;
        address owner;
        address nftContract;
        uint256 tokenId;
        address currencyContractAddress;
        uint256 price;
        uint256 availableFrom;
        uint256 availableTo;
    }

    struct Renting {
        bytes32 listId;
        address user;
        address nftContract;
        uint256 tokenId;
        uint256 startDate;
        uint256 endDate;
        address currencyContractAddress;
        uint256 rentalFee;
    }

    event LeasingNFT(
        bytes32 listId,
        address owner,
        address originalNftContract,
        address nftContract,
        uint256 tokenId,
        address currencyContractAddress,
        uint256 price,
        uint256 availableFrom,
        uint256 availableTo
    );

    event RentingNFT(
        bytes32 listId,
        uint8 rentId,
        address owner,
        address user,
        address originalNftContract,
        address nftContract,
        uint256 tokenId,
        uint256 startDate,
        uint256 endDate,
        address currencyContractAddress,
        uint256 rentalFee
    );

    event UpdateNFT(
        bytes32 listId,
        address newCurrencyContractAddress,
        uint256 newPrice,
        uint256 newStartDate,
        uint256 newEndDate
    );

    event UnleaseNFT(bytes32 listId);

    /// @notice function to lease NFT for rental
    /// @param nftContract  Contract address of NFT
    /// @param listId  ID of Token
    /// @param tokenId  ID of Token
    /// @param currencyContractAddress  Currency contract address
    /// @param availableFrom  When the nft can start being rented
    /// @param availableTo  When the nft can no longer be rented
    /// @param signature  Signature verify nft
    function leaseNFT(
        bytes32 listId,
        address nftContract,
        uint256 tokenId,
        address currencyContractAddress,
        uint256 price,
        uint256 availableFrom,
        uint256 availableTo,
        bytes memory signature
    ) external payable;

    function unleaseNFT(address nftContract, bytes32 listId) external;

    /// @notice function to rent an NFT
    /// @param nftContract  Contract address of NFT
    /// @param listId  ID of Token
    /// @param startDate   timestamp, The new user could star use the NFT
    /// @param endDate   timestamp, The new user could use the NFT before expires
    function rentNFT(
        address nftContract,
        bytes32 listId,
        uint256 startDate,
        uint256 endDate
    ) external payable;

    /// @notice function to check if the NFT contract is of the rentable type
    /// @param nftContract  Contract address of NFT
    function isRentableNFT(address nftContract) external view returns (bool);

    /// @notice function to get time leasing of rental NFT
    /// @param nftContract  Contract address of NFT
    /// @param listId  ID of Token
    function getLeasingTime(
        address nftContract,
        bytes32 listId
    ) external view returns (uint256, uint256);

    /// @notice function to get leasing fee
    function getLeasingFee() external view returns (uint256);
}

