            keccak256(
                abi.encode(
                    keccak256("Refer(address referee,address referal)"),
                    msg.sender
                )
            )
        );
        require(
            SignatureChecker.isValidSignatureNow(relayer, hash, signature),
            "invalid signature"
        );
        return true;
    }

