        require(msg.sender == governance, "!governance");
        tokensPerBlock = _tokensPerBlock;
}

function changeReserveFundPercentage(uint256 _amountForReserve) public {
        require(msg.sender == governance, "!governance");
        RESERVE_FUND_PERCENTAGE = _amountForReserve;
}

