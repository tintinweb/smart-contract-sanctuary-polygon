// SPDX-License-Identifier: MIT
pragma solidity ^0.8.15;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

import "./interfaces/ISafeWallet.sol";
import "./SafeWalletAccount.sol";
import "./interfaces/ISafeWalletAccount.sol";

contract SafeWallet is Ownable {
    using Counters for Counters.Counter;

    struct Account {
        uint256 id;
        address instance;
    }

    Counters.Counter private _accountIdCounter;
    mapping(uint256 => Account) private _accounts;
    mapping(uint256 => mapping(address => bool)) private _unavailables;

    constructor(address owner_) {
        _transferOwnership(owner_);
    }

    function _createAccount() internal returns (Account memory) {
        uint256 id = _accountIdCounter.current();
        SafeWalletAccount instance = new SafeWalletAccount();
        Account memory account = Account(id, address(instance));
        _accounts[id] = account;
        _accountIdCounter.increment();
        return account;
    }

    function createAccounts(uint256 number_) external onlyOwner {
        require(number_ <= 50, "number limit is 50");
        for (uint256 i; i < number_; i++) _createAccount();
    }

    function _getAccounts() internal view returns (Account[] memory) {
        Account[] memory accounts = new Account[](_accountIdCounter.current());
        for (uint256 i; i < _accountIdCounter.current(); i++)
            accounts[i] = _accounts[i];
        return accounts;
    }

    function getAccount(
        uint256 seed_
    ) external view onlyOwner returns (address) {
        require(seed_ != 0, "seed must not zero");
        Account[] memory accounts = _getAccounts();
        require(accounts.length > 0, "account not enough");
        if (block.timestamp < seed_) seed_ = seed_ - block.timestamp;
        return accounts[(block.timestamp % seed_) % accounts.length].instance;
    }

    function _isAvailable(
        uint256 accountId_,
        address token_
    ) internal view returns (bool) {
        return !_unavailables[accountId_][token_];
    }

    function getAccountWithout(
        address token_
    ) external view onlyOwner returns (address) {
        Account[] memory accounts = _getAccounts();
        for (uint256 i; i < accounts.length; i++) {
            if (!_isAvailable(accounts[i].id, token_)) continue;
            if (ISafeWalletAccount(accounts[i].instance).balanceOf(token_) > 0)
                continue;
            return accounts[i].instance;
        }
        revert("account not enough");
    }

    function getUnavailableAccounts(
        address token_
    ) external view onlyOwner returns (address[] memory) {
        Account[] memory accounts = _getAccounts();
        address[] memory temp = new address[](accounts.length);
        uint256 count;
        for (uint256 i; i < accounts.length; i++) {
            Account memory account = accounts[i];
            if (!_isAvailable(account.id, token_)) {
                temp[i] = account.instance;
                count++;
            }
        }
        address[] memory unavailables = new address[](count);
        uint256 index;
        for (uint256 i; i < temp.length; i++) {
            address account = temp[i];
            if (account == address(0x0)) continue;
            else {
                unavailables[index] = account;
                index++;
            }
        }
        return unavailables;
    }

    function _balanceOf(address token_) internal view returns (uint256) {
        Account[] memory accounts = _getAccounts();
        uint256 balance;
        for (uint256 i; i < accounts.length; i++) {
            balance += ISafeWalletAccount(accounts[i].instance).balanceOf(
                token_
            );
        }
        return balance;
    }

    function balanceOf(
        address token_
    ) external view onlyOwner returns (uint256) {
        return _balanceOf(token_);
    }

    event Transfer(address token_, address to_, uint256 amount_);

    function _transfer(address token_, address to_, uint256 amount_) internal {
        require(amount_ <= _balanceOf(token_), "insufficient balance");
        Account[] memory accounts = _getAccounts();
        uint256 transferred;
        for (uint256 i; i < accounts.length; i++) {
            uint256 balance = ISafeWalletAccount(accounts[i].instance)
                .balanceOf(token_);
            if (balance <= 0) continue;
            uint256 amount;
            if (amount_ - transferred > balance) amount = balance;
            else amount = amount_ - transferred;
            try
                ISafeWalletAccount(accounts[i].instance).transfer(
                    token_,
                    to_,
                    amount
                )
            returns (bool success_) {
                if (success_) {
                    transferred += amount;
                    _unavailables[accounts[i].id][token_] = false;
                } else {
                    _unavailables[accounts[i].id][token_] = true;
                }
            } catch {
                _unavailables[accounts[i].id][token_] = true;
            }
            if (amount_ - transferred <= 0) break;
        }
    }

    function transfer(
        address token_,
        address to_,
        uint256 amount_
    ) external onlyOwner {
        _transfer(token_, to_, amount_);
        emit Transfer(token_, to_, amount_);
    }

    function x() public view returns (address[] memory) {
        Account[] memory accounts = _getAccounts();
        address[] memory xx = new address[](accounts.length);
        for (uint256 i; i < accounts.length; i++) {
            xx[i] = accounts[i].instance;
        }
        return xx;
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.15;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/Address.sol";

contract SafeWalletAccount is Ownable {
    using Address for address;

    constructor() {
        _transferOwnership(msg.sender);
    }

    receive() external payable {}

    function balanceOf(address token_) external view returns (uint256) {
        if (token_ == address(0x0)) return address(this).balance;
        else {
            try IERC20(token_).balanceOf(address(this)) returns (
                uint256 balance_
            ) {
                return balance_;
            } catch {
                revert("invaild token address");
            }
        }
    }

    function transfer(
        address token_,
        address to_,
        uint256 amount_
    ) external onlyOwner returns (bool) {
        if (token_ == address(0x0)) {
            return payable(to_).send(amount_);
        } else {
            IERC20 token = IERC20(token_);
            (bool success, bytes memory returndata) = token_.call(
                abi.encodeWithSelector(token.transfer.selector, to_, amount_)
            );
            return success || abi.decode(returndata, (bool));
        }
    }
}