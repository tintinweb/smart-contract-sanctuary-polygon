    function getFundingPayment(address user) public view returns (uint256) {
        (, uint256 amountOfToken1) = plugin.getUnderlyingAmounts(uniswapTokenId);
        return getFundingPaymentForUser(user, (_position[user].lpConversion * amountOfToken1) / plugin.getLiquidityOfPosition(uniswapTokenId));
    }

    function decimals() public view override returns (uint8) {
        return _decimals;
    }

    /****************************************************************/
    /****************** EXTERNAL PUBLIC FUNCTIONS  ******************/
    /****************************************************************/

    function userDepositShortTwoSided(uint256 amountAToAdd, uint256 amountBToAdd) external nonReentrant {
        require(amountAToAdd > 0, "Error: Deposit amount cannot be 0");
        uint256 liquidityMinted = 0;
        uint256 amount0 = 0;
        uint256 amount1 = 0;
        // Transfer tokens from user and approve them for plugin
        IERC20(underlyingAssetAddressA).transferFrom(msg.sender, address(this), amountAToAdd);
        IERC20(underlyingAssetAddressB).transferFrom(msg.sender, address(this), amountBToAdd);
        IERC20(underlyingAssetAddressA).approve(address(plugin), amountAToAdd);
        IERC20(underlyingAssetAddressB).approve(address(plugin), amountBToAdd);

        if (uniswapTokenId != 0) {
            // a position already exists
            (liquidityMinted, amount0, amount1) = plugin.increaseLiq(amountAToAdd, amountBToAdd, msg.sender);
        } else {
            // mint new position
            Params memory _params = Params({
                token0: underlyingAssetAddressA,
                token1: underlyingAssetAddressB,
                fee: feePool,
                amount0Desired: amountAToAdd,
                amount1Desired: amountBToAdd,
                recipient: msg.sender
            });
            (liquidityMinted, amount0, amount1, uniswapTokenId) = plugin.mintNewPosition(_params);
        }
        mintShort(amount0, amount1);
        updateFundingAggregate();
        updateFundingRate(getUtilizationRate());
        emit ShortDeposit(msg.sender, liquidityMinted, amount0, amount1);
    }

    /**
     * @dev External function called by the short volatility user to withdraw an amount of DXPsh tokens minted by the pool. DXPsh are being burned
     * @notice No approval is required
     * @param lpTokens The amount of liquidity to remove.
     **/
    function userWithdrawShort(uint256 lpTokens) public nonReentrant {
        require(lpTokens > 0, "Error: Withdrawal amount cannot be 0");
        // TODO: Collect and distribute fees first before calling decreaseLiquidity

        uint256 liquidityToRemove = lpTokens*1e5/totalSupply() * (plugin.getLiquidityOfPosition(uniswapTokenId) +totalLpBorrowed) /1e5;

        require(plugin.getLiquidityOfPosition(uniswapTokenId) >= liquidityToRemove, "Error: Can't exit position as it has been converted to Buy & Hold");
        (uint256 amount0, uint256 amount1) = decreaseLiquidity(uint128(liquidityToRemove));
        IERC20(underlyingAssetAddressA).transfer(msg.sender, amount0);
        IERC20(underlyingAssetAddressB).transfer(msg.sender, amount1);
        _burn(msg.sender, lpTokens); // burn tokens 1:1
        updateFundingAggregate();
        updateFundingRate(getUtilizationRate());
    }

    /**
     * @dev External function called by the long volatility user to deposit collateral and specify the amount of LP tokens to use.
     * @notice Approval is required on the y token contract from msg.sender for the amount of collateral
     * @notice Collateral can only be deposited in y token for now
     * @param lpAmount The amount of the LP tokens to convert to buy and hold.
     * @param collateral The amount of collateral in y token to deposit.
     **/
    function userDepositLong(uint256 lpAmount, uint128 collateral) external nonReentrant {
        require(lpAmount > 0, "Error: LP amount cannot be 0");
        require(collateral > ((lpAmount * loanToValueBps) / 10000), "Error: Collateral less that minimum required");
        //transfer collateral in the pool
        IERC20(underlyingAssetAddressB).transferFrom(msg.sender, address(this), collateral);
        _position[msg.sender].collateralDeposit = _position[msg.sender].collateralDeposit.add(collateral);
        // settle the funding payment, if not zero
        // Using entryPrice because of a bug in lpConversions
        if (_position[msg.sender].entryPrice != 0) {
            settleFundingPayment(msg.sender);
        }
        _position[msg.sender].lpConversion = _position[msg.sender].lpConversion.add(lpAmount);
        totalLpBorrowed = totalLpBorrowed.add(lpAmount);
        (uint256 amountAConverted, uint256 amountBConverted) = buyAndHold(uint128(lpAmount), msg.sender);
        _position[msg.sender].entryPrice = plugin.getExchangeRate(uniswapTokenId);
        _position[msg.sender].entryTimestamp = block.timestamp;
        // fees also collected at this point
        updateFundingAggregate();
        updateFundingRate(getUtilizationRate());
        setEntryFundingRate(msg.sender, getCurrentFundingRateAggregate());

        emit LongDeposit(msg.sender, lpAmount, collateral, amountAConverted, amountBConverted);
    }

    function userWithdrawLongFull() public nonReentrant {
        bool collectFee = false;
        if (shouldLiquidate(msg.sender)) {
            collectFee = true;
        }
        exitLongPositionFull(msg.sender);
        if (_position[msg.sender].xConversion > 0) {
            uint amountToTransfer = _position[msg.sender].xConversion;
            _position[msg.sender].xConversion = 0;
            if (collectFee) {
                uint256 amountCollected = collectLiquidationFeeTreasury(msg.sender, amountToTransfer, underlyingAssetAddressA);
                IERC20(underlyingAssetAddressA).transfer(msg.sender, amountToTransfer.sub(amountCollected));
            } else {
                IERC20(underlyingAssetAddressA).transfer(msg.sender, amountToTransfer);
            }
        }
        if (_position[msg.sender].yConversion.add(_position[msg.sender].collateralDeposit) > 0) {
            uint amountToTransfer = _position[msg.sender].collateralDeposit.add(_position[msg.sender].yConversion);
            delete _position[msg.sender].yConversion; // todo: check or revert to = zero
            delete _position[msg.sender].collateralDeposit;
            if (collectFee) {
                uint256 amountCollected = collectLiquidationFeeTreasury(msg.sender, amountToTransfer, underlyingAssetAddressB);
                IERC20(underlyingAssetAddressB).transfer(msg.sender, amountToTransfer.sub(amountCollected));
            } else {
                IERC20(underlyingAssetAddressB).transfer(msg.sender, amountToTransfer);
            }
        }
    }

    function increaseCollateral(uint256 amount) public {
        require(_position[msg.sender].lpConversion > 0, "User doesn't have a position in the pool");
        IERC20(underlyingAssetAddressB).transferFrom(msg.sender, address(this), amount);
        _position[msg.sender].collateralDeposit = _position[msg.sender].collateralDeposit.add(amount);
        settleFundingPayment(msg.sender);
        setEntryFundingRate(msg.sender, getCurrentFundingRateAggregate());
    }

    function liquidatePosition(address user) public nonReentrant {
        require(shouldLiquidate(user), "Error: Position is not liquidatable");
        exitLongPositionFull(user);
        if (_position[user].xConversion > 0) {
            uint amountToTransfer = _position[user].xConversion;
            delete _position[user].xConversion;
            // Collect fees for treasury and reporter
            uint256 amountCollectedTreasury = collectLiquidationFeeTreasury(user, amountToTransfer, underlyingAssetAddressA);
            uint256 amountCollectedReporter = collectLiquidationFeeReporter(user, amountToTransfer, underlyingAssetAddressA);
            // Deduct fees and transfer remaining amount to user
            IERC20(underlyingAssetAddressB).transfer(user, amountToTransfer.sub(amountCollectedTreasury).sub(amountCollectedReporter));
        }
        if (_position[user].yConversion.add(_position[user].collateralDeposit) > 0) {
            uint amountToTransfer = _position[user].collateralDeposit.add(_position[user].yConversion);
            delete _position[user].yConversion;
            delete _position[user].collateralDeposit;
            // Collect fees for treasury and reporter
            uint256 amountCollectedTreasury = collectLiquidationFeeTreasury(user, amountToTransfer, underlyingAssetAddressB);
            uint256 amountCollectedReporter = collectLiquidationFeeReporter(user, amountToTransfer, underlyingAssetAddressB);
            // Deduct fees and transfer remaining amount to user
            IERC20(underlyingAssetAddressB).transfer(user, amountToTransfer.sub(amountCollectedTreasury).sub(amountCollectedReporter));
        }
        emit PositionLiquidated(user);
    }

    /********************************************************/
    /****************** INTERNAL FUNCTIONS ******************/
    /********************************************************/

    function mintShort(uint256 amount0, uint256 amount1) internal {
        uint256 depositValueInY = (plugin.getExchangeRateForBaseAmount(uniswapTokenId, uint128(amount0))).add(amount1);
        if (totalSupply() == 0) {
            _mint(msg.sender, depositValueInY); //We define the price of DXP to be 1$.
        } else {
            collectAllFees(true);
            (uint256 pAmountA, uint256 pAmountB) = plugin.getUnderlyingAmounts(uniswapTokenId);
            uint256 poolValueWithFees = (plugin.getExchangeRateForBaseAmount(uniswapTokenId, uint128(pAmountA.add(xAmountOfFeesHeld)))).add(pAmountB).add(yAmountOfFeesHeld);

            uint256 priceOfToken = ((((poolValueWithFees.sub(depositValueInY)) * (10 ** decimals())) / (totalSupply())));
            uint256 amountToMint = (depositValueInY * 10 ** decimals()) / priceOfToken;
            _mint(msg.sender, amountToMint);
        }
    }

    function decreaseLiquidity(uint128 liquidity) internal returns (uint amount0, uint amount1) {
        // Collect fees before reducing liquidity to account them for the short vol. users
        collectAllFees(true);

        (amount0, amount1) = plugin.decreaseLiq(liquidity);
        // Call callectAllFees() to withdraw the amount of liquidity that was removed from Uniswap, and don't account it as fees.
        (uint256 amount0fromFees, uint256 amount1FromFees) = collectAllFees(false);
        require(amount0 == amount0fromFees && amount1 == amount1FromFees, "Fees collected and decreaseLiquidity amounts do not match");
    }

    function buyAndHold(uint128 lpAmount, address user) internal returns (uint256, uint256) {
        (uint256 amountA, uint256 amountB) = decreaseLiquidity(lpAmount);
        _position[user].xConversion = _position[user].xConversion.add(amountA);
        _position[user].yConversion = _position[user].yConversion.add(amountB);
        totalXConversions = totalXConversions.add(amountA);
        totalYConversions = totalYConversions.add(amountB);
        return (amountA, amountB);
    }

    function balancePosition(address user, uint256 x0, uint256 x1) internal {
        uint256 delta = 1e10 / (1e5 + ((_position[user].lpConversion * 1e5) / plugin.getTotalLiquidity(uniswapTokenId)));
        if (_position[user].xConversion < x0) {
            uint x0needed = ((x0.sub(_position[user].xConversion)) * delta) / 1e5;
            uint256 maxAmount = _position[user].yConversion.sub(x1);
            IERC20(underlyingAssetAddressB).approve(address(plugin), maxAmount);
            uint256 amountX1used = plugin.swapExactOutput(x0needed, maxAmount, underlyingAssetAddressA, underlyingAssetAddressB, feePool);

            // Update accounting
            _position[user].yConversion = _position[user].yConversion.sub(amountX1used);
            _position[user].xConversion = _position[user].xConversion.add(x0needed);
            totalXConversions = totalXConversions.add(x0needed);
            totalYConversions = totalYConversions.sub(amountX1used);
        }
        if (_position[user].yConversion < x1) {
            uint x1needed = ((x1.sub(_position[user].yConversion)) * delta) / 1e5;
            if (x1needed > 0) {
                uint256 amountX0used = plugin.swapExactOutput(x1needed, (_position[user].xConversion.sub(x0)), underlyingAssetAddressB, underlyingAssetAddressA, feePool);

                // Update accounting
                _position[user].xConversion = _position[user].xConversion.sub(amountX0used);
                _position[user].yConversion = _position[user].yConversion.add(x1needed);
                totalXConversions = totalXConversions.sub(amountX0used);
                totalYConversions = totalYConversions.add(x1needed);
            }
        }
    }

    /// @notice Collects the fees associated with provided liquidity
    /// @return amount0 The amount of fees collected in token0
    /// @return amount1 The amount of fees collected in token1
    function collectAllFees(bool fees) internal returns (uint256 amount0, uint256 amount1) {
        (amount0, amount1) = plugin.collectFees();
        if ((fees) && (amount0 > 0 || amount1 > 0)) {
            xAmountOfFeesHeld = xAmountOfFeesHeld.add(amount0);
            yAmountOfFeesHeld = yAmountOfFeesHeld.add(amount1);
            (uint256 pAmountA, uint256 pAmountB) = plugin.getUnderlyingAmounts(uniswapTokenId);
            uint256 valueOfUnderlyingAmountInToken1 = plugin.getExchangeRateForBaseAmount(uniswapTokenId, uint128(pAmountA)) + pAmountB;
            setPctOfFeesOnLiquidity(((plugin.getExchangeRateForBaseAmount(uniswapTokenId, uint128(amount0)).add(amount1)) * 10 ** 12) / valueOfUnderlyingAmountInToken1);
            emit CollectFees(amount0, amount1);
        }
    }

    /**
     * @dev External function called by the long volatility user to deposit collateral and specify the amount of LP tokens to use.
     * @notice eg. bpsToWithdraw = 7500 is 75%
     * @notice Approval is required on the y token contract from msg.sender for the amount of collateral
     * @notice Collateral can only be deposited in y token for now
     **/
    // TODO: Make non-reentrant
    function exitLongPositionFull(address user) internal {
        (uint256 x0, uint256 x1) = getX0X1ForUser(user);
        balancePosition(user, x0, x1);
        collectAllFees(true);
        updateFundingAggregate();
        settleFundingPayment(user);

        // recalculate x0 and x1 because the position was balanced
        (x0, x1) = getX0X1ForUser(user);
        IERC20(underlyingAssetAddressA).approve(address(plugin), x0);
        IERC20(underlyingAssetAddressB).approve(address(plugin), x1);
        (uint128 liquidityAdded, uint amount0deposited, uint amount1deposited) = plugin.increaseLiq(x0, x1, user);

        updateFundingRate(getUtilizationRate());
        // Update accounting
        // In case of liquidations, there might be small rounding errors
        _position[user].xConversion = (amount0deposited > _position[user].xConversion) ? 0 : _position[user].xConversion.sub(amount0deposited);
        _position[user].yConversion = (amount1deposited > _position[user].yConversion) ? 0 : _position[user].yConversion.sub(amount1deposited);

        totalXConversions = totalXConversions.sub(amount0deposited);
        totalYConversions = totalYConversions.sub(amount1deposited);
        totalLpBorrowed = totalLpBorrowed.sub(_position[user].lpConversion);
        delete _position[user].lpConversion;
        delete _position[user].entryPrice;
        delete _position[user].entryTimestamp;
        delete _position[user].fundingPaid;
        deleteEntryFundingRate(user);

        emit LongWithdrawal(msg.sender, liquidityAdded, amount0deposited, amount1deposited);
    }

    function settleFundingPayment(address user) internal {
        uint256 fundingPayment = getFundingPayment(user);
        _position[user].collateralDeposit = _position[user].collateralDeposit.sub(fundingPayment);
        yAmountOfFeesHeld = yAmountOfFeesHeld.add(fundingPayment);
        _position[user].fundingPaid = _position[user].fundingPaid.add(fundingPayment);
        emit FundingPaymentSettled(user, fundingPayment);
        // setEntryFundingRate should not happen here
        // TODO: Reinvest if fees get too high
    }

    function collectLiquidationFeeTreasury(address user, uint256 amount, address token) internal returns (uint256) {
        // calculate percentage of the users position for diatomix's treasury
        uint256 fee = (amount * liquidationTreasuryFeeBps) / 10000;
        IERC20(token).transfer(TREASURY_ADDRESS, fee);
        emit LiquidationFeeTreasury(user, fee, token);
        return fee;
    }

    function collectLiquidationFeeReporter(address user, uint256 amount, address token) internal returns (uint256) {
        // calculate the reward to be paid out to the reporter (msg.sender)
        uint256 fee = (amount * liquidationReporterFeeBps) / 10000;
        IERC20(token).transfer(msg.sender, fee);
        emit LiquidationFeeReporter(user, fee, token, msg.sender);
        return fee;
    }

    /**
     * @dev Internal function to get the amount of the two underlyings the user currently owes the pool.
     * @dev used for rebalancing the users position
     */
    function getX0X1ForUser(address user) internal view returns (uint256 x0, uint256 x1) {
        uint256 liquidity = plugin.getLiquidityOfPosition(uniswapTokenId);
        (uint256 amountOfToken0, uint256 amountOfToken1) = plugin.getUnderlyingAmounts(uniswapTokenId);
        x0 = ((_position[user].lpConversion * amountOfToken0) * 10000) / liquidity / 10000;
        x1 = ((_position[user].lpConversion * amountOfToken1) * 10000) / liquidity / 10000;
    }

    /*
    s = optimal swap amount
    r = amount of reserve for token a
    a = amount of token a the user currently has (not added to reserve yet)
    f = swap fee percent
    s = (sqrt(((2 - f)r)^2 + 4(1 - f)ar) - (2 - f)r) / (2(1 - f))
    */
    function sqrt(uint256 y) private pure returns (uint256 z) {
        if (y > 3) {
            z = y;
            uint256 x = y / 2 + 1;
            while (x < z) {
                z = x;
                x = (y / x + x) / 2;
            }
        } else if (y != 0) {
            z = 1;
        }
    }

    function onERC721Received(address operator, address from, uint tokenId, bytes calldata) external override returns (bytes4) {
        return IERC721Receiver.onERC721Received.selector;
    }
}

