    address public underlyingAssetAddressB;
    uint256 public uniswapTokenId;
    uint8 private _decimals; // decimals of diatomix pool issued token
    uint24 public feePool;
    uint256 public loanToValueBps;

    // Short Volatility related vars
    uint256 public xAmountOfFeesHeld; // Amount of fees collected for short volatility users in x - convert to private
    uint256 public yAmountOfFeesHeld; // Amount of fees collected for short volatility users in y - convert to private

    uint256 public totalXConversions;
    uint256 public totalYConversions;
    uint256 public totalLpBorrowed;

    struct Position {
        uint256 entryPrice;
        uint256 entryTimestamp;
        uint256 collateralDeposit;
        uint256 lpConversion;
        uint256 xConversion;
        uint256 yConversion;
        uint256 fundingPaid;
    }

    mapping(address => Position) private _position;

    /**
     * @dev Initialized upgradeable contract. Replaced the constructor
     * @param _tokenA The address of the underlying token A.
     * @param _tokenB The address of the underlying token B.
     * @param _feePool The fee of the position that will be created
     * @param _name The name of the ERC20 token.
     * @param _symbol The symbol of the ERC20 token.
     * @param _plugin The address of the plugin that will be used for the pool.
     **/
    function initialize(address _tokenA, address _tokenB, uint24 _feePool, uint256 _loanToValueBps, string memory _name, string memory _symbol, address _plugin) public initializer {
        underlyingAssetAddressA = _tokenA;
        underlyingAssetAddressB = _tokenB;
        if (_tokenA > _tokenB) {
            invertedPair = true;
        }
        feePool = _feePool;
        loanToValueBps = _loanToValueBps;
