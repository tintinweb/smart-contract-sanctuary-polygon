     * @param user The address of the user to get the PnL for.
     * @return PnL denominated in token B.
     **/
    function getPnL(address user) public view returns (uint256) {
        uint256 vBh = (_position[user].xConversion * ((plugin.getExchangeRate(uniswapTokenId) * 1e5) / _position[user].entryPrice + 1e5)) / 1e5;
        uint256 vLp = ((_position[user].xConversion * 2 * sqrt(plugin.getExchangeRate(uniswapTokenId)) * 1e5) / sqrt(_position[user].entryPrice)) / 1e5;
        return vBh.sub(vLp);
    }

