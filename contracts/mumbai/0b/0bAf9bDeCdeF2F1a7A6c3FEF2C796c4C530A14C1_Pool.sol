    event LongDeposit(address user, uint256 liquidity, uint128 collateral, uint256 amount0, uint256 amount1);
    event LongWithdrawal(address user, uint256 liquidityAdded, uint256 amount0deposited, uint256 amount1deposited);
    event CollectFees(uint256 amount0, uint256 amount1);
    event LiquidationFeeTreasury(address user, uint256 amount, address token);
    event LiquidationFeeReporter(address user, uint256 amount, address token, address reporter);
    event FundingPaymentSettled(address user, uint256 amount);
    event PositionLiquidated(address user);

    /**************************************************/
    /****************** PUBLIC VIEWS ******************/
    /**************************************************/

    function getUtilizationRate() public view returns (uint256) {
        return ((totalLpBorrowed * 10 ** 8) / (totalLpBorrowed.add(plugin.getLiquidityOfPosition(uniswapTokenId))));
    }

    function getPosition(address user) public view returns (uint256, uint256, uint256, uint256, uint256, uint256, uint256) {
        Position storage position = _position[user];
        return (position.entryPrice, position.entryTimestamp, position.collateralDeposit, position.lpConversion, position.xConversion, position.yConversion, position.fundingPaid);
