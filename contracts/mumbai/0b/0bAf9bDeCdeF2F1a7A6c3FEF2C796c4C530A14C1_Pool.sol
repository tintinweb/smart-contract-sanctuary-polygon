contract Pool is Initializable, ERC20Upgradeable, ReentrancyGuardUpgradeable, IERC721Receiver, FundingRateModule {
    using SafeMath for uint256;
    //using WadRayMath for uint256;

    /***
    STORAGE VARIABLES
    */
    uint256 private constant UINT_MAX_VALUE = 2 ** 256 - 1;
    int24 private constant MIN_TICK = -887272;
    int24 private constant MAX_TICK = -MIN_TICK;
    int24 private constant TICK_SPACING = 60;

    uint256 private constant liquidationMultiplier = 2;
    uint256 private constant liquidationTreasuryFeeBps = 400;
    uint256 private constant liquidationReporterFeeBps = 100;
    address private constant TREASURY_ADDRESS = 0x0ee9a06B8221cfdCD5c608d36cb5c6eD95EC939b;

