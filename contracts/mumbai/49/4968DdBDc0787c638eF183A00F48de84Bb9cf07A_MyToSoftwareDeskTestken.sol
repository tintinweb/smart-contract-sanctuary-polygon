// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract MyToSoftwareDeskTestken is ERC20, ERC20Burnable, Ownable {
    uint256 public hardcap = 1000 ether;
    uint256 public start = 1684273806;
    uint256 public stop = 1784273806;
    uint256 public maxPurchase = 200 ether;
    uint256 public minPurchase = 10 ether;
    uint256 public tokenPerUsdt = 5;
    address public usdtContractAddress =
        address(0x2e2D6c08688353397D941520bFd40e37a0f7b77b);
    uint256 public totalUsdt;

    struct LockTokens {
        uint256 lockedAmount;
        uint256 lastClaimTime;
        uint256 availableAmount;
    }
    LockTokens lockTokens;

    struct userICOTokens {
        uint256 totalBoughtTokens;
        uint256 claimTime;
        uint256 lastClaimTime;
        uint256 maxClaim;
        uint256 claimPercentage;
        bool status;
    }
    mapping(address => userICOTokens) amountTokens;

    struct userReferal {
        uint256 totalReferals;
        address owner;
        uint256 totalTokens;
        address[] ReferalUsedFriends;
        bool status;
    }

    mapping(uint256 => userReferal) userReferals;

    constructor() ERC20("MyToSoftware Desk Testken", "SDT") {
        _mint(msg.sender, 10000 * 10 ** decimals());
    }

    function mint(address to, uint256 amount) public onlyOwner {
        _mint(to, amount);
    }

    function buyTokenByPublicICO(uint256 _RID) public payable {
        require(userReferals[_RID].status, "Referal Id not valid");
        require(
            userReferals[_RID].owner != msg.sender,
            "You are a owner of this referal ID"
        );
        require(
            !amountTokens[msg.sender].status,
            "You are already bought tokens, You can not buy again"
        );
        require(msg.sender != address(0), "address not valid");
        require(msg.value >= minPurchase, "USDT must be  greater than 10$");
        require(msg.value <= maxPurchase, "USDT must be less than 200$");
        require(
            msg.value * tokenPerUsdt <= hardcap,
            "Tokens are not available for ICO"
        );
        IERC20(usdtContractAddress).transferFrom(
            msg.sender,
            address(this),
            msg.value
        );

        totalUsdt += msg.value;

        userICOTokens storage userDetails = amountTokens[msg.sender];
        userDetails.totalBoughtTokens = msg.value * tokenPerUsdt;
        userDetails.claimTime = 0;
        userDetails.lastClaimTime = block.timestamp;
        userDetails.maxClaim = 10;
        userDetails.claimPercentage = 10;
        userDetails.status = true;

        if (userReferals[_RID].totalReferals < 5) {
            uint256 sendTokens = ((msg.value * tokenPerUsdt) *
                (5 - userReferals[_RID].totalReferals)) / 100;
            transfer(userReferals[_RID].owner, sendTokens);
            userReferals[_RID].totalTokens += msg.value * tokenPerUsdt;
        }
        userReferals[_RID].totalReferals += 1;
        userReferals[_RID].ReferalUsedFriends.push(msg.sender);

        bytes32 hash = keccak256(abi.encodePacked(msg.sender));
        uint256 randomNumber = uint256(hash);
        uint256 random8DigitNumber = randomNumber % 10 ** 16;

        userReferals[random8DigitNumber].totalReferals = 0;
        userReferals[random8DigitNumber].owner = msg.sender;
        userReferals[random8DigitNumber].status = true;

        hardcap -= msg.value * tokenPerUsdt;
    }

    function claimICOTokens() public payable {
        require(
            amountTokens[msg.sender].status == true,
            "First buy a some tokens"
        );

        require(msg.sender != address(0), "address not valid");
        userICOTokens storage userDetails = amountTokens[msg.sender];
        require(
            amountTokens[msg.sender].claimTime <
                amountTokens[msg.sender].maxClaim,
            "You have not any tokens"
        );

        require(
            amountTokens[msg.sender].lastClaimTime + 60 * 60 * 24 >=
                block.timestamp,
            "You cannot claim, wait until you reach the end of you last claim time"
        );

        transfer(
            msg.sender,
            userDetails.totalBoughtTokens /
                amountTokens[msg.sender].claimPercentage
        );
        userDetails.claimTime += 1;
        userDetails.lastClaimTime = userDetails.lastClaimTime + 60 * 60 * 24;
    }

    function setRateOfTokenPerUsdt(uint256 rate) public onlyOwner {
        tokenPerUsdt = rate;
    }

    function transferCollectedUsdt(uint256 amount) public onlyOwner {
        require(
            totalUsdt >= amount,
            "amount is high than total available usdt"
        );
        IERC20(usdtContractAddress).transfer(msg.sender, amount);
        totalUsdt -= amount;
    }

    function setDefaultRID() public onlyOwner {
        bytes32 hash = keccak256(abi.encodePacked(msg.sender));
        uint256 randomNumber = uint256(hash);
        uint256 random8DigitNumber = randomNumber % 10 ** 16;
        require(
            !userReferals[random8DigitNumber].status,
            "already you have generated default Referal ID"
        );
        userReferals[random8DigitNumber].totalReferals = 0;
        userReferals[random8DigitNumber].owner = msg.sender;
        userReferals[random8DigitNumber].status = true;
    }

    function getRID() public view returns (uint256) {
        bytes32 hash = keccak256(abi.encodePacked(msg.sender));
        uint256 randomNumber = uint256(hash);
        uint256 random8DigitNumber = randomNumber % 10 ** 16;
        require(
            userReferals[random8DigitNumber].status,
            " you have not generated default Referal ID"
        );
        return random8DigitNumber;
    }

    function lockMonthlyTokens() public payable onlyOwner {
        transfer(address(this), 1200 * 1000000000000000000);
        lockTokens = LockTokens(
            1200 * 1000000000000000000,
            block.timestamp,
            1200 * 1000000000000000000
        );
    }

    // Release Every months 100 Coin Over 12 Month's
    function ReleaseMonthlyTokens() public payable onlyOwner {
        require(
            lockTokens.availableAmount > 0,
            "tokens not available for release"
        );
        require(
            (lockTokens.lastClaimTime + 60 * 60 * 24 * 30) < block.timestamp,
            "Please wait untill month will not be end"
        );
        transfer(msg.sender, 1200 * 1000000000000000000);
        LockTokens memory newlockToken = LockTokens(
            lockTokens.lockedAmount,
            lockTokens.lastClaimTime + 60 * 60 * 24 * 30,
            lockTokens.availableAmount - 100 * 1000000000000000000
        );

        lockTokens = newlockToken;
    }
}