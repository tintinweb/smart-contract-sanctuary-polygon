import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "closedsea/src/OperatorFilterer.sol";

contract CryptoMoriesIsERC721A is
    ERC721AUpgradeable,
    Initializable,
    UUPSUpgradeable,
    OwnableUpgradeable,
    ERC2981Upgradeable,
    OperatorFilterer
{
    using StringsUpgradeable for uint256;

    /// @notice Base URI for the NFT
    string public baseURI;

    /// @notice Maximum supply for the collection
    uint256 public constant MAX_SUPPLY = 10000;

    function initialize(
        string memory _baseURI_
    ) public initializerERC721A initializer {
        __ERC721A_init("CryptoMories", "CRYPTOMORIES");
        __Ownable_init();
        __ERC2981_init();

        _registerForOperatorFiltering();

        baseURI = _baseURI_;
    }

    /*
     * @notice Set the Base URI
     * @param _baseURI : Base URI
     */
    function setBaseUri(string memory _baseURI) external onlyOwner {
        baseURI = _baseURI;
    }

    /*
     * @notice return NFTs URI
     * @param _tokenId : id of NFT
     */
    function tokenURI(
        uint _tokenId
    ) public view virtual override returns (string memory) {
        require(_exists(_tokenId), "URI query for nonexistent token");

        return string(abi.encodePacked(baseURI, _tokenId.toString(), ".json"));
    }

    /*
     * @notice aidrop NFTs to wallet
     * @param _wallet : array of address
     * @param _quantity : array of
     */
    function airdrop(
        address[] calldata _wallet,
        uint256[] calldata _quantity
    ) public onlyOwner {
        require(_wallet.length == _quantity.length, "Invalid input");
        require(
            totalSupply() + _wallet.length <= MAX_SUPPLY,
            "Max supply exceeded"
        );
        for (uint256 i = 0; i < _wallet.length; i++) {
            _mint(_wallet[i], _quantity[i]);
        }
    }

    /* ********************************** */
    /*     OPERATOR FILTER REGISTRY       */
    /* ********************************** */

    function setApprovalForAll(
        address operator,
        bool approved
    ) public override onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    function approve(
        address operator,
        uint256 tokenId
    ) public payable override onlyAllowedOperatorApproval(operator) {
        super.approve(operator, tokenId);
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public payable override onlyAllowedOperator(from) {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public payable override onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    ) public payable override onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId, data);
    }

    /* ********************************** */
    /*       EIP2981 royalties            */
    /* ********************************** */

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        virtual
        override(ERC721AUpgradeable, ERC2981Upgradeable)
        returns (bool)
    {
        return
            ERC721AUpgradeable.supportsInterface(interfaceId) ||
            ERC2981Upgradeable.supportsInterface(interfaceId);
    }

    function setDefaultRoyalty(
        address receiver,
        uint96 feeNumerator
    ) public onlyOwner {
        _setDefaultRoyalty(receiver, feeNumerator);
    }

    /* ********************************** */
    /*          UUPS UPGRADEABLE           */
    /* ********************************** */

    function _authorizeUpgrade(
        address newImplementation
    ) internal override onlyOwner {}

    /**
     * @notice Return the implementation contract
     * @return address The implementation contract address
     */
    function getImplementation() external view returns (address) {
        return _getImplementation();
    }

    /* ********************************** */
    /*               Others            */
    /* ********************************** */

    /*
     * @notice withdraw ether from contract
     */
    function withdraw() external payable onlyOwner {
        (bool success, ) = payable(msg.sender).call{
            value: address(this).balance
        }("");
        require(success, "transfer failed!");
    }

    receive() external payable {}
}

