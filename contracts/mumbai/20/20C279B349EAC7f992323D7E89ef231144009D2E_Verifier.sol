            new PublicBeaconProxy(address(tokenContractsBeacon), "")
        );

        ITokenContract(newTokenContract_).__TokenContract_init(
            ITokenContract.TokenContractInitParams(
                params_.tokenName,
                params_.tokenSymbol,
                address(this),
                msg.sender
            )
        );

        _tokenContracts.add(newTokenContract_);
