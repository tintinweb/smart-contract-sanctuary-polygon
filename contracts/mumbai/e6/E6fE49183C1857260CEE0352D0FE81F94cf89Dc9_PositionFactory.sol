pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "./UserPosition.sol";

contract PositionFactory {
    using SafeERC20 for IERC20;

    event PositionCreated(address sender, address positionContract);

    mapping(address => address) public userToPosition;
    address public borrowToken; // e.g xUSDC
    address public marketplace;
    address public priceFeed;
    address public rentShare;

    constructor(
        address _xUSDC,
        address _marketplace,
        address _priceFeed,
        address _rentShare
    ) {
        borrowToken = _xUSDC;
        marketplace = _marketplace;
        priceFeed = _priceFeed;
        rentShare = _rentShare;
    }

    function createUserPosition() external {
        require(
            userToPosition[msg.sender] == address(0),
            "User already has a position"
        );
        address userPosition = address(
            new UserPosition(
                borrowToken,
                marketplace,
                msg.sender,
                priceFeed,
                address(this),
                rentShare
            )
        );
        userToPosition[msg.sender] = userPosition;

        emit PositionCreated(msg.sender, userPosition);
    }

    // TODO: add access control
    function mintXUSDC(uint amount) external {
        IMintableBurnableERC20(borrowToken).mint(msg.sender, amount);
    }
}

