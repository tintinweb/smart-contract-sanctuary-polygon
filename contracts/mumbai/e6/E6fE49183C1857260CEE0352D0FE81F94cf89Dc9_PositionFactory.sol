interface AggregatorV3Interface {
    function latestRoundData()
        external
        view
        returns (
            uint80 roundId,
            int256 answer,
            uint256 startedAt,
            uint256 updatedAt,
            uint80 answeredInRound
        );
}

// TODO: add access control to all functions accordingly
contract UserPosition {
    using SafeERC20 for IERC20;
    using FullMath for uint256;

    struct Collateral {
        IERC20 token; // ERC20 token used as collateral
        uint256 amount; // Amount of collateral token deposited
    }

    struct BorrowPosition {
        uint amountBorrowed;
        uint borrowTimeStamp;
        uint borrowDuration;
        address propertyTokenBought;
        uint amountOfPropertyTokens;
        uint amountRepaid; // amount repaid by the user against this position
    }

    mapping(uint => BorrowPosition) public borrowCounterToBorrowPosition;
    mapping(address => mapping(address => uint)) public userToPropertyBalance;
    address public owner;

    uint public borrowCounter;
    address public positionFactory;

    address public borrowToken; // Address of the token to be borrowed
    address public marketPlace; // marketplace to buy tokens from
    address public rentShare; // rentshare
    mapping(address => Collateral[]) public userCollateral; // Collateral deposited by each user
    uint public borrowedAmount; // Amount borrowed by each user
    mapping(address => bool) public isPropertyToken;

    uint256 public loanToValueRatio = 80; // Loan-to-value ratio in percentage
    uint256 public interestRate = 5; // Interest rate charged on borrowed amount in percentage
    address public priceFeed; // to get prices of tokens
    uint public lastPaidPositionCounter; // keeps track of last position to which repayment was done
    mapping(address => uint) public propertyToBuyCount; // keeps track of properties buy count
    uint public totalPropertiesBought;

    event CollateralDeposited(
        address indexed user,
        address token,
        uint256 amount
    );
    event Borrowed(address indexed user, uint256 amount);

    constructor(
        address _borrowToken,
        address _marketPlace,
        address _positionOwner,
        address _priceFeed,
        address _positionFactory,
        address _rentShare
    ) {
        borrowToken = _borrowToken;
        marketPlace = _marketPlace;
        owner = _positionOwner;
        priceFeed = _priceFeed;
        console.log(msg.sender, "this is msg.sender");
        positionFactory = _positionFactory;
        rentShare = _rentShare;
    }

    /**
     * @dev Deposits collateral tokens to the lending platform contract.
     * @param tokens Array of ERC20 token addresses used as collateral
     * @param amounts Array of corresponding collateral token amounts
     */
    function depositCollateral(
        address[] calldata tokens,
        uint256[] calldata amounts,
        bool[] calldata isCollateralTokenProperty
    ) external {
        require(tokens.length == amounts.length, "Arrays length mismatch");

        for (uint256 i = 0; i < tokens.length; i++) {
            address token = tokens[i];
            uint256 amount = amounts[i];

            // Transfer the collateral tokens from the user to the lending platform
            SafeERC20.safeTransferFrom(
                IERC20(token),
                msg.sender,
                address(this),
                amount
            );

            // Check if the user already has deposited the same token
            bool isTokenAlreadyDeposited = false;
            for (uint256 j = 0; j < userCollateral[msg.sender].length; j++) {
                if (userCollateral[msg.sender][j].token == IERC20(token)) {
                    userCollateral[msg.sender][j].amount += amount;
                    isTokenAlreadyDeposited = true;
                    break;
                }
            }

            // If the user has not deposited the token before, add it to the userCollateral array
            if (!isTokenAlreadyDeposited) {
                userCollateral[msg.sender].push(
                    Collateral(IERC20(token), amount)
                );
                isPropertyToken[token] = isCollateralTokenProperty[i];
            }

            emit CollateralDeposited(msg.sender, token, amount);
        }
    }

    /**
     * @dev Borrows tokens from the lending platform.
     * @param noOfTokensToBuy Amount  property tokens to buy
     * @param loanDuration For how much duration the loan is intended to be taken
     * @param propertyTokenToBuy Address of property token to buy
     */
    function borrow(
        uint256 noOfTokensToBuy,
        uint loanDuration,
        address propertyTokenToBuy
    ) external {
        // calculating how many tokens Will It Need to buy asked property tokens
        uint loanAmount = getPropertyPriceWithFees(
            borrowToken,
            propertyTokenToBuy,
            true,
            noOfTokensToBuy,
            0
        );
        console.log(loanAmount, "this is loana amount");
        require(loanAmount > 0, "Loan amount must be greater than zero");
        require(loanDuration > 0, "Loan Duration must be greater than zero");
        require(
            loanDuration <= 3 * (365 days),
            "Duration can not be more than 3 years"
        );

        uint256 maxLoanAmount = calculateMaxLoanAmount(msg.sender);

        require(
            borrowedAmount + loanAmount <= maxLoanAmount,
            "Loan amount exceeds maximum"
        );

        borrowedAmount += loanAmount;
        totalPropertiesBought += noOfTokensToBuy;
        propertyToBuyCount[propertyTokenToBuy] += noOfTokensToBuy;

        borrowCounterToBorrowPosition[borrowCounter] = BorrowPosition(
            loanAmount,
            block.timestamp,
            loanDuration,
            propertyTokenToBuy,
            noOfTokensToBuy,
            0
        );

        borrowCounter++;
        userToPropertyBalance[msg.sender][
            propertyTokenToBuy
        ] += noOfTokensToBuy;
        // prrparing propertyBuyStruct
        IMarketplace.swapArgs memory _swapArgs = IMarketplace.swapArgs(
            borrowToken,
            propertyTokenToBuy,
            noOfTokensToBuy,
            address(this)
        );

        console.log(loanAmount, "this is loan amoun to be minted");
        IPositionFactory(positionFactory).mintXUSDC(loanAmount);
        console.log(loanAmount, "mint hogya");
        IERC20(borrowToken).safeApprove(marketPlace, loanAmount);
        console.log(loanAmount, "approve hogua");
