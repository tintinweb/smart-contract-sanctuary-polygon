import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "../space/ISpaceManager.sol";
import "../license/ILicense.sol";
import "./IAllocator.sol";

// One derivative colletcion corresponds to one specific revenue allocator.
// All revenue from the contract will be stored in the allocator temporarily,
// the creator,token holder or space can obtain revenue through the allocateRevenue interface
contract RevenueAllocator is Initializable, IRevenueAllocator {
    // collection's address => Recipient
    mapping(address => Claimer) public claimers;

    address[] public collections;
    ISpaceManager public spaceManager;

    constructor() {
        _disableInitializers();
    }

    function __RevenueAllocator_init(
        address _licenseContract,
        address _spaceManager
    ) internal onlyInitializing {
        spaceManager = ISpaceManager(_spaceManager);
        License.AuthorizedLicense memory license = ILicense(_licenseContract)
            .GetCollectionLicense(address(this));
        uint256 remain = 1e9;
        if (license.authorizedContract == address(0)) {
            collections.push(address(this));
            claimers[address(this)].profitRatio = remain;
        }
        while (license.authorizedContract != address(0)) {
            uint256 profitRatio = remain - (remain * license.split) / 1e9;

            claimers[license.authorizedContract].profitRatio = profitRatio;
            collections.push(license.authorizedContract);
            remain = remain - profitRatio;

            License.Registry memory registry = ILicense(_licenseContract)
                .GetRegisty(license.registryId);

            // if token Holder, the owner of the derivative collection claim by collection
            // the owner of the original collection claim by token
            if (registry.licensor.licenseType == License.LicensorType.Holder) {
                address collection = registry.licensor.collection;

                claimers[collection].isToken = true;
                claimers[collection].tokenId = registry.licensor.id;
                claimers[collection].profitRatio = remain;
                collections.push(registry.licensor.collection);
                break;
            }
            license = ILicense(_licenseContract).GetCollectionLicense(
                registry.licensor.collection
            );

            // if creator, the owner of the derivative collection claim by collection
            // the owner of the original collection claim by collection
            if (license.authorizedContract == address(0)) {
                claimers[registry.licensor.collection].profitRatio = remain;
                collections.push(registry.licensor.collection);
                break;
            }
        }
    }

    /** ========== view function ========== */
    function getClaimed(
        address collection,
        address paymentToken
    ) public view returns (uint256) {
        return claimers[collection].claimed[paymentToken];
    }

    /**
     * @notice the collection owners check claimable reward balance from nft minting fee
     * @param erc20, claimable reward could be paid by erc20 token if any erc20 token existed on collection
     * @param collection, whose owner will get the reward balance
     */
    function availableBalance(
        address erc20,
        address collection
    ) public view returns (uint256) {
        uint256 balance = erc20 == address(0)
            ? address(this).balance
            : IERC20(erc20).balanceOf(address(this));

        return _claimableBalance(balance, erc20, collection);
    }

    /** ========== main function ========== */
    // All amount value of the allocator will be allocated to the creator,holder and space
    // through the collection's license(including parent licenses) split agreement recursively.
    // If the argument erc20 is an empty address, native token will be allocated.
    function claimByCollection(
        address erc20,
        address collection
    ) external payable {
        if (msg.sender != spaceManager.getCollectionOwner(collection))
            revert ErrNoPermission();
        if (claimers[collection].isToken) revert ErrOnlyByCollection();

        uint256 reward = _claim(erc20, collection, payable(msg.sender));

        emit CollectionRewardClaimed(msg.sender, collection, reward);
    }

    function claimByToken(
        address erc20,
        address collection,
        uint256 tokenId
    ) external payable {
        if (IERC721(collection).ownerOf(tokenId) != msg.sender)
            revert ErrNoPermission();
        if (
            !claimers[collection].isToken ||
            claimers[collection].tokenId != tokenId
        ) revert ErrOnlyByToken();

        uint256 reward = _claim(erc20, collection, payable(msg.sender));

        emit TokenRewardClaimed(msg.sender, collection, tokenId, reward);
    }

    /** ========== internal functions ========== */
    function _claimableBalance(
        uint256 _balance,
        address _erc20,
        address _collection
    ) internal view returns (uint256) {
        if (claimers[_collection].profitRatio == 0) return 0;
        uint256 totalClaimed;
        for (uint256 i = 0; i < collections.length; i++) {
            totalClaimed += claimers[collections[i]].claimed[_erc20];
        }

        uint256 totalCurrentCollecitonReward = ((_balance + totalClaimed) *
            claimers[_collection].profitRatio) / 1e9;

        // if there is no token on collection, return 0
        if (
            totalCurrentCollecitonReward <=
            claimers[_collection].claimed[_erc20]
        ) return 0;
        totalCurrentCollecitonReward -= claimers[_collection].claimed[_erc20];
        return totalCurrentCollecitonReward;
    }

    function _claim(
        address _erc20,
        address _collection,
        address payable _to
    ) private returns (uint256 claimBalance) {
        claimBalance = availableBalance(_erc20, _collection);
        if (claimBalance == 0) {
            revert ErrNothingClaim();
        }
        claimers[_collection].claimed[_erc20] += claimBalance;

        _to.transfer(claimBalance);
    }

    receive() external payable {
        //emit Received(msg.sender, msg.value);
    }

    uint256[50] private __gap;
}

