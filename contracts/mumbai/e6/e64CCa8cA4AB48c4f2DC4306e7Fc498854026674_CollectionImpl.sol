// original or derivative collection implmentation
contract CollectionImpl is
    ICollection,
    CantBeEvilUpgradeable,
    ERC721AUpgradeable,
    OwnableUpgradeable,
    ERC2981Upgradeable,
    RevenueAllocator,
    EIP712Upgradeable,
    DefaultOperatorFiltererUpgradeable
{
    string public tokenBaseURI;

    uint256 public maxMintQuota;

    uint256 public mintPrice;

    mapping(uint256 => bool) public nonces;

    // token id => token metadata url
    mapping(uint256 => string) public tokenUrls;

    using ECDSAUpgradeable for bytes32;
    bytes32 public constant MINT_TYPE_HASH =
        keccak256(
            "MintOrder(uint256 nonce,address nftContract,address paymentContract,uint256 expiredAt,uint256 value,address to,uint256 amount,string[] uri)"
        );

    constructor() {
        _disableInitializers();
    }

    function initialize(
        Collection.InitParams memory params
    ) external initializerERC721A initializer {
        __ERC721A_init(params.name, params.symbol);
        if (params.owner == address(0)) {
            params.owner = msg.sender;
        }
        __CBE_init(params.cbeLicense);
        _transferOwnership(params.owner);
        tokenBaseURI = params.tokenBaseURI;
        maxMintQuota = params.maxMintQuota;
        mintPrice = params.mintPrice;
        __RevenueAllocator_init(params.licenseManger, params.spaceManager);
        if (params.paymentRecipient == address(0)) {
            params.paymentRecipient = address(this);
        }
        _setDefaultRoyalty(params.paymentRecipient, params.royaltyFeeNumerator);
        __EIP712_init("Collection", "1");
        __DefaultOperatorFilterer_init();
    }

    /** ========== view functions ========== */
    function owner() public view virtual override returns (address) {
        SpaceLibrary.Collection memory c = spaceManager.getCollection(
            address(this)
        );
        (address spaceOwner, , ) = spaceManager.spaces(c.spaceId);
        if (spaceOwner != address(0)) {
            return spaceOwner;
        }
        return super.owner();
    }

    function getMintSigner(
        MintOrder calldata _mint,
        bytes calldata _signature
    ) public view returns (address) {
        bytes32 digest = getMintDigest(_mint);
        // return digest.recover(_signature);
        return ECDSAUpgradeable.recover(digest, _signature);
    }

    function tokenURI(
        uint256 tokenId
    ) public view virtual override returns (string memory) {
        if (bytes(tokenUrls[tokenId]).length != 0) {
            return tokenUrls[tokenId];
        }
        return super.tokenURI(tokenId);
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        virtual
        override(CantBeEvilUpgradeable, ERC2981Upgradeable, ERC721AUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    /** ========== main functions ========== */

    // urls为每个token的url.如果不想针对特定token设置url则可以传空,会自动使用baseTokenUrl+token_id的方式返回tokenUrl
    function airdrop(
        address _addresses,
        uint256 _nums,
        string[] calldata _urls
    ) public onlyOwner {
        if (maxMintQuota != 0) {
            if (_totalMinted() + _nums > maxMintQuota)
                revert ErrQuotaExceeded();
        }
        _mintToken(_addresses, _nums, _urls);
    }

    // only the call signed by owner or spaceManager is legal
    function mint(
        MintOrder calldata _mint,
        bytes calldata _signature
    ) external payable {
        if (_mint.nftContract != address(this)) revert ErrInvalidArgument();
        if (_mint.expiredAt < block.timestamp) revert ErrMintExpired();
        // address signer = getMintSigner(_mint, _signature);
        // if (
        //     signer != owner() &&
        //     signer != OwnableUpgradeable(address(spaceManager)).owner()
        // ) {
        //     revert ErrInvalidSignature();
        // }
        if (maxMintQuota != 0) {
            if (_totalMinted() + _mint.amount > maxMintQuota)
                revert ErrQuotaExceeded();
        }
        if (nonces[_mint.nonce]) revert ErrUsedNonce();
        nonces[_mint.nonce] = true;

        if (_mint.value > 0) {
            (address recevier, ) = royaltyInfo(0, 0);
            if (recevier == address(0)) {
                recevier = owner();
            }
            if (_mint.paymentContract != address(0)) {
                // using erc20
                IERC20(_mint.paymentContract).transferFrom(
                    msg.sender,
                    recevier,
                    _mint.value
                );
            } else {
                // using native token
                if (msg.value != _mint.value) revert ErrValueMismatched();
                payable(recevier).transfer(msg.value);
            }
        }
        _mintToken(_mint.to, _mint.amount, _mint.uri);
        return;
    }

    /** ========== admin functions ========== */

    function setBaseURI(string calldata _tokenBaseURI) external onlyOwner {
        tokenBaseURI = _tokenBaseURI;
        emit BaseURIChanged(_tokenBaseURI);
    }

    /**
     * @dev Overrides the `_startTokenId` function from ERC721A
     *      to start at token id `1`.
     *
     *      This is to avoid future possible problems since `0` is usually
     *      used to signal values that have not been set or have been removed.
     */
    function _startTokenId() internal view virtual override returns (uint256) {
        return 1;
    }

    /** ========== internal functions ========== */
    function _baseURI() internal view virtual override returns (string memory) {
        return tokenBaseURI;
    }

    function _mintToken(
        address _addresses,
        uint256 _nums,
        string[] calldata _urls
    ) internal {
        uint256 nextIndex = _totalMinted() + _startTokenId();
        _safeMint(_addresses, _nums);
        for (uint256 i = 0; i < _urls.length; i++) {
            if (bytes(_urls[i]).length != 0) {
                tokenUrls[nextIndex] = _urls[i];
                nextIndex++;
            }
        }
    }

    function hashStringArray(
        string[] calldata array
    ) internal pure returns (bytes32 result) {
        bytes32[] memory _array = new bytes32[](array.length);
        for (uint256 i = 0; i < array.length; ++i) {
            _array[i] = keccak256(bytes(array[i]));
        }
        result = keccak256(abi.encodePacked(_array));
    }

    function getMintDigest(
        MintOrder calldata _mint
    ) internal view returns (bytes32) {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        MINT_TYPE_HASH,
                        _mint.nonce,
                        _mint.nftContract,
                        _mint.paymentContract,
                        _mint.expiredAt,
                        _mint.value,
                        _mint.to,
                        _mint.amount,
                        hashStringArray(_mint.uri)
                    )
                )
            );
    }

    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
     */
    uint256[50] private __gap;
}

