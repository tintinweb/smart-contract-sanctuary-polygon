contract CantBeEvilUpgradeable is
    Initializable,
    ERC165Upgradeable,
    ICantBeEvil
{
    using Strings for uint256;
    string internal constant _BASE_LICENSE_URI =
        "ar://zmc1WTspIhFyVY82bwfAIcIExLFH5lUcHHUN0wXg4W8/";
    CBE.LicenseVersion public licenseVersion; // return string

    constructor() {
        _disableInitializers();
    }

    function __CBE_init(CBE.LicenseVersion _licenseVersion)
        internal
        onlyInitializing
    {
        licenseVersion = _licenseVersion;
    }

    function getLicenseURI() public view returns (string memory) {
        return
            string.concat(
                _BASE_LICENSE_URI,
                uint256(licenseVersion).toString()
            );
    }

    function getLicenseName() public view returns (string memory) {
        return _getLicenseVersionKeyByValue(licenseVersion);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC165Upgradeable)
        returns (bool)
    {
        return
            interfaceId == type(ICantBeEvil).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function _getLicenseVersionKeyByValue(CBE.LicenseVersion _licenseVersion)
        internal
        pure
        returns (string memory)
    {
        require(uint8(_licenseVersion) <= 6);
        if (CBE.LicenseVersion.PUBLIC == _licenseVersion) return "PUBLIC";
        if (CBE.LicenseVersion.EXCLUSIVE == _licenseVersion) return "EXCLUSIVE";
        if (CBE.LicenseVersion.COMMERCIAL == _licenseVersion)
            return "COMMERCIAL";
        if (CBE.LicenseVersion.COMMERCIAL_NO_HATE == _licenseVersion)
            return "COMMERCIAL_NO_HATE";
        if (CBE.LicenseVersion.PERSONAL == _licenseVersion) return "PERSONAL";
        else return "PERSONAL_NO_HATE";
    }

    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
     */
    uint256[48] private __gap;
}

