    // For a list of available gas lanes on each network,
    // see https://docs.chain.link/docs/vrf/v2/subscription/supported-networks/#configurations
    bytes32 keyHash;

    uint32 callbackGasLimit;

    uint16 requestConfirmations;

    uint32 numWords;

    // Base URI
    string private baseUri;

    string private xpBaseUri;

    string public override versionRecipient;

    constructor(
        address admin,
        address gfiContract_,
        address vrfCoordinator_,
        uint64 subscriptionId_,
        bytes32 keyHash_,
        string memory baseURI_
    ) ERC721("GFMaskBox", "GFMB") VRFConsumerBaseV2(vrfCoordinator_) {
        _setupContractId("GoldFeverMaskBox");
        _setupRole(DEFAULT_ADMIN_ROLE, admin);
        gfi = GoldFeverItem(gfiContract_);
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator_);
        subscriptionId = subscriptionId_;
        keyHash = keyHash_;
        baseUri = baseURI_;
        callbackGasLimit = 800000;
        requestConfirmations = 3;
        numWords = 6;
        versionRecipient = "2.2.5";

        // box id count from uint128
        _boxIds._value = type(uint128).max;
    }

    event ForwarderContractUpdated(address forwarder_);

    function setForwarderContract(
        address forwarder_
    ) external only(DEFAULT_ADMIN_ROLE) {
        _setTrustedForwarder(forwarder_);
        emit ForwarderContractUpdated(forwarder_);
    }

    // Both AccessControlMixin and BaseRelayRecipient defined _msgSender() _msgData()
    function _msgSender()
        internal
        view
        override(Context, BaseRelayRecipient)
        returns (address sender)
    {
        sender = BaseRelayRecipient._msgSender();
    }

    function _msgData()
        internal
        view
        override(Context, BaseRelayRecipient)
        returns (bytes calldata)
    {
        return BaseRelayRecipient._msgData();
    }

    // overrides required by Solidity
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        override(ERC721, ERC721Enumerable, AccessControl, ERC721Royalty)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function _baseURI() internal view override returns (string memory) {
        return baseUri;
    }

    function tokenURI(
        uint256 tokenId
    ) public view override(ERC721, ERC721URIStorage) returns (string memory) {
        if (tokenId < MAX_UINT128) {
            return string(abi.encodePacked(xpBaseUri, tokenId.toString()));
        }
        return super.tokenURI(tokenId);
    }

    function _burn(
        uint256 tokenId
    ) internal override(ERC721, ERC721URIStorage, ERC721Royalty) {
        ERC721._burn(tokenId);
    }

    uint256[] private maskShapesPool;
    uint256[] private maskMaterialsPool;
    uint256[] private otherMaskElementsPool;

    event MaskBoxCreated(uint256 indexed boxId, address owner);
    event MaskBoxOpened(uint256 indexed boxId, uint256[] allMaskPartIds);

    function _createBoxes(
        uint256[] calldata maskShapes,
        uint256[] calldata maskMaterials,
        uint256[] calldata otherMaskElements
    ) internal only(MINTER_ROLE) {
        require(
            maskShapes.length > 0,
            "GoldFeverMaskBox: Must have at least 1 mask blueprint"
        );
        require(
            maskMaterials.length == maskShapes.length,
            "GoldFeverMaskBox: Must have same number of mask materials as mask blueprints"
        );
        require(
            maskShapes.length * 4 == otherMaskElements.length,
            "GoldFeverMaskBox: Must have 4 other mask parts for each mask"
        );
        uint256 numBoxes = maskShapes.length;
        address msgSender = _msgSender();

        for (uint256 i = 0; i < numBoxes; i++) {
            _boxIds.increment();
            uint256 boxId = _boxIds.current();
            _mint(msgSender, boxId);
            emit MaskBoxCreated(boxId, msgSender);
        }

        // transfer ownership of mask blueprints to the mask box
        for (uint256 i = 0; i < maskShapes.length; i++) {
            gfi.safeTransferFrom(msgSender, address(this), maskShapes[i]);
            maskShapesPool.push(maskShapes[i]);
        }
        // transfer ownership of mask materials to the mask box
        for (uint256 i = 0; i < maskMaterials.length; i++) {
            gfi.safeTransferFrom(msgSender, address(this), maskMaterials[i]);
            maskMaterialsPool.push(maskMaterials[i]);
        }
        // transfer ownership of other mask parts to the mask box
        for (uint256 i = 0; i < otherMaskElements.length; i++) {
            gfi.safeTransferFrom(
                msgSender,
                address(this),
                otherMaskElements[i]
            );
            otherMaskElementsPool.push(otherMaskElements[i]);
        }
    }

    function createBoxes(
        uint256[] calldata maskShapes,
        uint256[] calldata maskMaterials,
        uint256[] calldata otherMaskElements
    ) external only(MINTER_ROLE) {
        _createBoxes(maskShapes, maskMaterials, otherMaskElements);
    }

    // mint function for bridge
    function mint(
        address to,
        uint256 id,
        bytes calldata
    ) external only(BRIDGE_ROLE) {
        _safeMint(to, id);
        emit MaskBoxCreated(id, to);
    }

    // burn function for bridge
    function burnFor(address to, uint256 id) external only(BRIDGE_ROLE) {
        require(
            _isApprovedOrOwner(_msgSender(), id),
            "ERC721: caller is not token owner or approved"
        );
        _burn(id);
    }

    // open and burn box
    function openBox(uint256 boxId) external {
        address msgSender = _msgSender();
        require(
            msgSender == ownerOf(boxId),
            "GoldFeverMaskBox: Only owner can open box"
        );
        require(
            boxIdToLockStatus[boxId] != LOCKED,
            "GoldFeverMaskBox: Box is already locked or opened"
        );
        boxIdToLockStatus[boxId] = LOCKED;

        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );
        statuses[requestId] = RequestStatus({
            randomWords: new uint256[](0),
            exists: true,
            fulfilled: false,
            boxId: boxId,
            boxOwner: msgSender
        });
        requestIds.push(requestId);
        lastRequestId = requestId;
        _burn(boxId);
        emit RequestOpenBoxSent(requestId, numWords, boxId);
    }

    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory _randomWords
    ) internal override {
        require(statuses[_requestId].exists, "request not found");
        statuses[_requestId].fulfilled = true;
        statuses[_requestId].randomWords = _randomWords;
        uint256 boxId = statuses[_requestId].boxId;
        address boxOwner = statuses[_requestId].boxOwner;
        uint256[] memory allMaskPartIds = new uint256[](6);
        uint256 rnd_shape = statuses[_requestId].randomWords[0] %
            maskShapesPool.length;

        gfi.safeTransferFrom(
            address(this),
            boxOwner,
            maskShapesPool[rnd_shape]
        );
        allMaskPartIds[0] = maskShapesPool[rnd_shape];
        maskShapesPool[rnd_shape] = maskShapesPool[maskShapesPool.length - 1];
        maskShapesPool.pop();

        uint256 rnd_material = statuses[_requestId].randomWords[1] %
            maskMaterialsPool.length;
        gfi.safeTransferFrom(
            address(this),
            boxOwner,
            maskMaterialsPool[rnd_material]
        );
        allMaskPartIds[1] = maskMaterialsPool[rnd_material];
        maskMaterialsPool[rnd_material] = maskMaterialsPool[
            maskMaterialsPool.length - 1
        ];
        maskMaterialsPool.pop();
        for (uint256 i = 2; i <= 5; i++) {
            uint256 rnd_element = statuses[_requestId].randomWords[i] %
                otherMaskElementsPool.length;
            gfi.safeTransferFrom(
                address(this),
                boxOwner,
                otherMaskElementsPool[rnd_element]
            );
            allMaskPartIds[i] = otherMaskElementsPool[rnd_element];
            otherMaskElementsPool[rnd_element] = otherMaskElementsPool[
                otherMaskElementsPool.length - 1
            ];
            otherMaskElementsPool.pop();
        }

        emit MaskBoxOpened(boxId, allMaskPartIds);
    }

    function getRequestStatus(
        uint256 _requestId
    ) external view returns (bool fulfilled, uint256[] memory randomWords) {
        require(statuses[_requestId].exists, "request not found");
        RequestStatus memory request = statuses[_requestId];
        return (request.fulfilled, request.randomWords);
    }

    // function set xpBaseUri
    function setXpBaseUri(
        string memory _xpBaseUri
    ) external only(DEFAULT_ADMIN_ROLE) {
        xpBaseUri = _xpBaseUri;
    }

    // function set baseUri
    function setBaseUri(
        string memory _baseUri
    ) external only(DEFAULT_ADMIN_ROLE) {
        baseUri = _baseUri;
    }

    function baseURI() external view returns (string memory) {
        return string(abi.encodePacked(xpBaseUri, "{id}"));
    }

    // function set setRoyaltyInfo
    function setRoyalty(
        address _yourRoyaltyAddress,
        uint96 _feeRate
    ) external only(DEFAULT_ADMIN_ROLE) {
        require(_feeRate <= 10000, "Invalid fee rate");
        _setDefaultRoyalty(_yourRoyaltyAddress, _feeRate);
    }

    // function transfer mask part to mask part pool
    function depositMaskParts(
        uint256[] calldata maskShapes,
        uint256[] calldata maskMaterials,
        uint256[] calldata otherMaskElements
    ) external only(DEFAULT_ADMIN_ROLE) {
        require(
            maskShapes.length > 0,
            "GoldFeverMaskBox: Must have at least 1 mask blueprint"
        );
        require(
            maskMaterials.length == maskShapes.length,
            "GoldFeverMaskBox: Must have same number of mask materials as mask blueprints"
        );
        require(
            maskShapes.length * 4 == otherMaskElements.length,
            "GoldFeverMaskBox: Must have 4 other mask parts for each mask"
        );

        address msgSender = _msgSender();

        // transfer ownership of mask blueprints to the mask box
        for (uint256 i = 0; i < maskShapes.length; i++) {
            gfi.safeTransferFrom(msgSender, address(this), maskShapes[i]);
            maskShapesPool.push(maskShapes[i]);
        }
        // transfer ownership of mask materials to the mask box
        for (uint256 i = 0; i < maskMaterials.length; i++) {
            gfi.safeTransferFrom(msgSender, address(this), maskMaterials[i]);
            maskMaterialsPool.push(maskMaterials[i]);
        }
        // transfer ownership of other mask parts to the mask box
        for (uint256 i = 0; i < otherMaskElements.length; i++) {
            gfi.safeTransferFrom(
                msgSender,
                address(this),
                otherMaskElements[i]
            );
            otherMaskElementsPool.push(otherMaskElements[i]);
        }
    }
}

