import "../interfaces/IERC20Mintable.sol";

// Auxiliary constants
uint constant MAX_BPS = 10_000;

/// @title Company
contract Company is ICompany, OwnableUpgradeable {

    // Attaching functions to types
    using SafeERC20 for IERC20;
    using MerkleProof for bytes32[];

    // Contract global variables
    address public token;
    address public want;
    address public staking;
    address public pair;
    address public swapFactory;
    address public swapRouter;

    uint public totalSupply;
    uint public listingPrice;
    uint public liquidityShare;
    uint public currentRound;

    mapping(uint => Round) public rounds;
    mapping(address => mapping(uint => Contribution)) contributions;


    // Initialize function (constructor analogue)
    function initialize(
        address _owner,
        address _token,
        address _want,
        address _staking,
        uint _totalSupply,
        uint _listingPrice,
        uint _liquidityShare,
        address _swapFactory,
        address _swapRouter
    ) external override initializer {
        token = _token;
        want = _want;
        staking = _staking;

        totalSupply = _totalSupply;
        listingPrice = _listingPrice;
        liquidityShare = _liquidityShare;

        swapFactory = _swapFactory;
        swapRouter = _swapRouter;

        __Ownable_init();
        transferOwnership(_owner);
    }

    // Access modifier (Whitelisted) used MerkleProof
    modifier onlyWhitelisted(
        address user,
        uint roundId,
        bytes32[] calldata merkleProof
    ) {
        if (rounds[currentRound].whitelist != 0x0) {
            // Check user is whitelisted
        }

        require(isWhitelisted(user, roundId, merkleProof), "Company: user not in the whitelist");

        _;
    }

    // Access modifier (RoundContributor)
    modifier onlyRoundContributor(address user, uint roundId) {
        require(contributions[user][roundId].contributed > 0, "Company: only round contributors");

        _;
    }

    // Access modifier (RoundStatus)
    modifier onlyRoundStatus(uint roundId, RoundStatus status) {
        require(getRoundStatus(roundId) == status, "Company: wrong round status");

        _;
    }

    /// @notice Function for get round details
    /// @param roundId          - round id
    /// @return round details
    function getRoundDetails(
        uint roundId
    ) external view returns (Round memory) {
        return rounds[roundId];
    }

    /// @notice Get round status. It's based on the network time and round parameters
    /// Current time is less than `startTimestamp` - `Initialized`
    /// Current time is between `startTimestamp` and `endTimestamp` - `Pending`
    /// Round is ended and `minCap` is not reached - `Failed`
    /// Round is ended and `minCap` is reached - `Succeeded`
    /// @param roundId Round ID
    /// @return status Round status
    function getRoundStatus(
        uint roundId
    )
    public
    view
    returns (
        RoundStatus status
    ) {
        require(roundId > 0 && roundId <= currentRound, "Company: wrong round ID");

        uint startTimestamp = rounds[roundId].startTimestamp;
        uint endTimestamp = rounds[roundId].endTimestamp;
        uint minCap = rounds[roundId].minCap;
        uint totalContribution = rounds[roundId].totalContribution;

        if (block.timestamp < startTimestamp) {
            status = RoundStatus.Initialized;
        } else if (block.timestamp >= startTimestamp && block.timestamp <= endTimestamp) {
            status = RoundStatus.Active;
        } else if (block.timestamp > endTimestamp && totalContribution >= minCap) {
            status = RoundStatus.Succeeded;
        } else if (block.timestamp > endTimestamp && totalContribution < minCap) {
            status = RoundStatus.Failed;
        }
    }

    /// @notice Contribute to the current round
    /// @param amount Amount of `want` tokens to contribute
    /// @param merkleProof Merkle proof for whitelist root, in case round is whitelisted. Otherwise set to empty array.
    function contribute(
        uint amount,
        bytes32[] calldata merkleProof
    )
    external
    override
    onlyRoundStatus(currentRound, RoundStatus.Active)
    onlyWhitelisted(msg.sender, currentRound, merkleProof)
    {
        IStaking(staking).considerContribution(
            msg.sender,
            amount,
            contributions[msg.sender][currentRound].contributed
        );

        IERC20(want).safeTransferFrom(msg.sender, address(this), amount);

        rounds[currentRound].totalContribution += amount;

        contributions[msg.sender][currentRound].contributed += amount;
        contributions[msg.sender][currentRound].lastContribution = block.number;

        emit RoundContributed(currentRound, msg.sender, amount);
    }

    /// @notice Refund round contribution partially or completely
    /// Can be called only by round participant, in case round is still in `Active` status
    /// @param amount Amount to refund
    function refund(
        uint amount
    )
    external
    override
    onlyRoundStatus(currentRound, RoundStatus.Active)
    onlyRoundContributor(msg.sender, currentRound)
    {
        require(amount <= contributions[msg.sender][currentRound].contributed);

        IERC20(want).safeTransfer(msg.sender, amount);

        rounds[currentRound].totalContribution -= amount;
        contributions[msg.sender][currentRound].contributed -= amount;
        contributions[msg.sender][currentRound].lastContribution = block.number;

        emit RoundRefunded(currentRound, msg.sender, amount);
    }

    /// @notice Withdraw contribution from the failed round
    /// Can be called only by round contributor, in case round is in `Failed` status
    /// @param roundId Round ID
    function withdraw(
        uint roundId
    )
    external
    override
    onlyRoundStatus(roundId, RoundStatus.Failed)
    onlyRoundContributor(msg.sender, roundId)
    {
        require(contributions[msg.sender][roundId].withdrawn == false, "Company: already withdrawn");

        contributions[msg.sender][roundId].withdrawn = true;

        uint amount = contributions[msg.sender][roundId].contributed;

        IERC20(want).safeTransfer(msg.sender, amount);

        emit RoundWithdrawn(roundId, msg.sender, amount);
    }

    /// @notice Claim exceeding `want` tokens from the round
    /// Can be called only by round contributor, in case round is in `Succeeded` status
    /// @param roundId Round ID
    function claimExceeding(
        uint roundId
    )
    external
    override
    onlyRoundStatus(roundId, RoundStatus.Succeeded)
    onlyRoundContributor(msg.sender, roundId)
    {
        require(!contributions[msg.sender][roundId].claimedExceeding, "Company: already claimed exceeding");
        require(rounds[roundId].totalContribution > rounds[roundId].maxCap, "Company: total contribution less than max cap");

        uint amount = getExceeding(msg.sender, roundId);

        IERC20(want).safeTransfer(msg.sender, amount);

        contributions[msg.sender][roundId].claimedExceeding = true;

        emit RoundClaimedExceeding(roundId, msg.sender, amount);
    }

    /// @notice Get amount of exceeding contribution for specific round and user
    /// In case round contribution is less than `maxCap`, than exceeding is 0
    /// @param user User address
    /// @param roundId Round ID
    /// @return amount How much `want` tokens is exceeding
    function getExceeding(
        address user,
        uint roundId
    )
    public
    view
    override
    onlyRoundContributor(user, roundId)
    returns (uint amount) {
        Round storage round = rounds[roundId];

        // Total round contribution is less than `maxCap`
        if (round.totalContribution <= round.maxCap) {
            return 0;
        }

        uint contributed = contributions[user][roundId].contributed;
        uint exceeding = contributed * round.maxCap / round.totalContribution;

        return contributed - exceeding;
    }

    /// @notice Get user's contribution without exceeding part
    /// @param roundId Round ID
    /// @param user User address
    /// @return amount Normalized contribution amount
    function getContributionWithoutExceeding(
        uint roundId,
        address user
    )
    public
    view
    override
    returns (uint amount) {
        uint exceeding = getExceeding(user, roundId);
        uint contribution = contributions[user][roundId].contributed;

        return contribution - exceeding;
    }

    /// @notice Check user whitelisted at specific round
    /// @dev True if round has no whitelist
    /// @param user User address
    /// @param roundId Round ID
    /// @return status Whitelisted or not
    function isWhitelisted(
        address user,
        uint roundId,
        bytes32[] memory merkleProof
    )
    public
    view
    override
    returns (bool status) {
        Round storage round = rounds[roundId];

        if (round.whitelist == bytes32(0)) return true;

        return merkleProof.verify(
            round.whitelist,
            keccak256(abi.encodePacked(user))
        );
    }

    /// @notice Claim company tokens from the succeeded round
    /// Can be called only by round contributor, in case round succeeded
    /// @param roundId Round ID
    function claim(
        uint roundId
    )
    external
    override
    onlyRoundContributor(msg.sender, roundId)
    onlyRoundStatus(roundId, RoundStatus.Succeeded)
    {
        Round storage round = rounds[roundId];
        VestingCheckpoint memory checkpoint = round.vesting[contributions[msg.sender][roundId].checkpoint];

        require(contributions[msg.sender][roundId].checkpoint <= round.vesting.length, "Company: all checkpoints claimed");
        require(block.timestamp >= checkpoint.timestamp, "Company: checkpoint not started");

        uint contributionNormalized = getContributionWithoutExceeding(roundId, msg.sender);
        uint amount = (contributionNormalized / round.price) * MAX_BPS;

        IERC20Mintable(token).mint(msg.sender, amount);

        contributions[msg.sender][roundId].checkpoint++;
        emit RoundClaimed(roundId, msg.sender, amount);
    }

    /// @notice Claim owner share
    /// Can be called only by owner in case round succeeded
    /// @param roundId Round ID
    /// @param receiver Owner share receiver
    function claimOwnerShare(
        uint roundId,
        address receiver
    )
    external
    override
    onlyRoundStatus(roundId, RoundStatus.Succeeded)
    onlyOwner
    {
        require(rounds[roundId].ownerShareDistributed == false, "Company: owner share already claimed");

        rounds[roundId].ownerShareDistributed = true;

        uint roundContribution = getRoundNormalizedContribution(roundId);

        uint ownerShare = roundContribution * (MAX_BPS - liquidityShare) / MAX_BPS;

        IERC20(want).safeTransfer(receiver, ownerShare);

        emit RoundOwnerShareClaimed(roundId, msg.sender, ownerShare);
    }

    function getRoundNormalizedContribution(
        uint roundId
    ) public view returns (uint amount) {
        Round storage round = rounds[roundId];

        if (round.totalContribution > round.maxCap) {
            return round.maxCap;
        }

        return round.totalContribution;
    }

    /// @notice Start new round
    /// Can be called only by `owner`
    /// Can be called only after previous round has ended
    /// @param price Round price.
    /// @param startTimestamp New round start timestamp
    /// @param endTimestamp New round end timestamp
    /// @param minCap New round minimum cap, in `want` tokens
    /// @param maxCap New round maximum cap, in `want` tokens
    /// @param isFinal Boolean flag, if the round will be the last one
    /// @param vesting List of vesting checkpoints
    /// @param whitelist Whitelist merkle root. 0x0 if no whitelist required.
    function startRound(
        uint price,
        uint startTimestamp,
        uint endTimestamp,
        uint minCap,
        uint maxCap,
        bool isFinal,
        VestingCheckpoint[] memory vesting,
        bytes32 whitelist
    ) external override onlyOwner {
        require(startTimestamp >= block.timestamp, "Company: start timestamp too low");
        require(startTimestamp < endTimestamp, "Company: end timestamp too low");

        if (currentRound > 0) {
            Round memory round = rounds[currentRound];

            require(
                round.endTimestamp < block.timestamp,
                "Company: current round not finished"
            );

            require(
                round.endTimestamp < startTimestamp,
                "Company: new round should start after previous one"
            );

            if (round.isFinal) {
                // Cant create new rounds if the previous one was final and succeeded
                require(getRoundStatus(currentRound) == RoundStatus.Failed);
            }
        }

        // current round = 0 means this round gonna be the first one
        currentRound++;

        rounds[currentRound].activation = block.number;
        rounds[currentRound].price = price;
        rounds[currentRound].startTimestamp = startTimestamp;
        rounds[currentRound].endTimestamp = endTimestamp;
        rounds[currentRound].minCap = minCap;
        rounds[currentRound].maxCap = maxCap;
        rounds[currentRound].isFinal = isFinal;
        rounds[currentRound].whitelist = whitelist;

        // Vesting checks
        uint vestingTotalShare = 0;
        uint lastCheckpointTimestamp = 0;

        for (uint i = 0; i < vesting.length; i++) {
            vestingTotalShare += vesting[i].share;

            require(vesting[i].timestamp > lastCheckpointTimestamp, "Company: wrong vesting checkpoints");
            lastCheckpointTimestamp = vesting[i].timestamp;

            rounds[currentRound].vesting.push(vesting[i]);
        }

        require(vestingTotalShare == MAX_BPS, "Company: vesting total share less than 100%");

        emit RoundStarted(
            currentRound,
            price,
            startTimestamp,
            endTimestamp,
            minCap,
            maxCap,
            isFinal
        );
    }

    function finalize()
    external
    onlyRoundStatus(currentRound, RoundStatus.Succeeded)
    {
        Round storage round = rounds[currentRound];

        require(round.isFinal);

        pair = IUniswapV2Factory(swapFactory).createPair(want, token);

        uint want_share = 0;

        // Calculate total contribution, through all previous rounds
        for (uint i = 1; i <= currentRound; i++) {
            uint roundContribution = getRoundNormalizedContribution(i);

            want_share += roundContribution * (liquidityShare) / MAX_BPS;
        }

        // Calculate amount of `token` to be used as liquidity
        // according to the `listingPrice`
        uint token_share = want_share * MAX_BPS / listingPrice;

        IERC20Mintable(token).mint(address(this), token_share);

        IERC20(token).approve(swapRouter, token_share);
        IERC20(want).approve(swapRouter, want_share);

        // Add liquidity to the pair
        IUniswapV2Router02(swapRouter).addLiquidity(
            want,
            token,
            want_share,
            token_share,
            want_share,
            token_share,
            address(this),
            block.timestamp
        );
    }
}

