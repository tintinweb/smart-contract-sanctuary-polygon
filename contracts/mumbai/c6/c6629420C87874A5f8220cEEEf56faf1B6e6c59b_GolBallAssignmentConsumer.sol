pragma solidity ^0.8.7;

import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";

interface IGolBallPieces {
    // @dev this function returns the selected number requires that the assignment is active
    function setSelectedNumber(uint256) external;

    // @dev see: {GollBallPieces-whitelistedSupply} number of existing whitelisted tokens
    function whitelistedSupply() external view returns (uint256);

    function ownerOf(uint256 tokenId) external view returns (address);

    function whitelist(address _address) external view returns (bool);
}

/*
    Contract written to assign a random number to a GolBall NFT token. 
    The contract imports the Chainlink VRF (Verifiable Random Function) 
    library and the OpenZeppelin AccessControlEnumerable library. 
    The contract has an interface for the GolBall NFT token with functions 
    such as setting a selected number, getting the total supply, getting 
    the token by index, and getting the owner of a token.
*/
contract GolBallAssignmentConsumer is
    VRFConsumerBaseV2,
    AccessControlEnumerable
{
    bytes32 public constant DEALER_ROLE = keccak256("DEALER_ROLE");

    uint256 private constant NOT_ASSIGNED = 0;
    uint256 private constant ASSIGNMENT_IN_PROGRESS = 42;
    uint256 private constant ASSIGNMENT_DONE = 1;

    VRFCoordinatorV2Interface COORDINATOR;

    uint64 s_subscriptionId;
    bytes32 public s_keyHash;
    uint32 public callbackGasLimit = 150000;
    uint16 public requestConfirmations = 3;
    uint32 numWords = 1;

    /* Assignment variables */
    mapping(uint256 => address) private golBallPieces;
    mapping(address => uint256) private whitelistedSupply;
    mapping(address => uint256) private assignmentStatus;
    mapping(address => uint256) private resultNumber;

    /* Events */
    event AssignmentStarted(
        uint256 indexed requestId,
        address indexed golBallPieces
    );
    event AssignmentEnded(
        uint256 indexed requestId,
        address indexed golBallPieces,
        uint256 indexed result
    );

    // see https://docs.chain.link/docs/vrf-contracts/#configurations

    /*
     * Initialize the contract with the id number of the oracle
     * The handling is done by vrfConsumerBaseV2
     */
    constructor(
        address vrfCoordinator,
        bytes32 sKeyHash,
        uint64 subscriptionId,
        address adminAddress,
        address ozRelayer
    ) VRFConsumerBaseV2(vrfCoordinator) {
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
        s_keyHash = sKeyHash;
        s_subscriptionId = subscriptionId;

        _grantRole(DEFAULT_ADMIN_ROLE, adminAddress);
        _grantRole(DEALER_ROLE, ozRelayer);
    }

    /*
     * @dev This function starts the initial assignment
     * emits the ´AssignmentStarted´ event after the flow is finished of the function
     */
    function runAssignment(
        address golBallPieces_
    ) public onlyRole(DEALER_ROLE) returns (uint256 requestId) {
        require(
            assignmentStatus[golBallPieces_] == NOT_ASSIGNED,
            "already assigned"
        );

        uint256 _whitelistedSupply = whitelistedSupply[
            golBallPieces_
        ] = IGolBallPieces(golBallPieces_).whitelistedSupply();
        require(_whitelistedSupply > 0, "zero items in collection");

        requestId = COORDINATOR.requestRandomWords(
            s_keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );

        golBallPieces[requestId] = golBallPieces_;
        assignmentStatus[golBallPieces_] = ASSIGNMENT_IN_PROGRESS;
        emit AssignmentStarted(requestId, golBallPieces_);
    }

    /*
     * @dev This is the function that Chainlink VRF node
     * @dev This function calls random numbers, calls ´randomWords´ and divides by the number of balls minted
     * each ball is set to ´results´ and set to ´setSelectedNumber´
     * Then a assignment closed event is emitted require by parameters ´requestId´, ´golBall´, ´randomNumber
     */
    function fulfillRandomWords(
        uint256 requestId,
        uint256[] memory randomWords
    ) internal override {
        address golBallPiece = golBallPieces[requestId];
        uint256 randomNumber = (randomWords[0] %
            whitelistedSupply[golBallPiece]) + 1;

        assignmentStatus[golBallPiece] = ASSIGNMENT_DONE;
        resultNumber[golBallPiece] = randomNumber;
        IGolBallPieces(golBallPiece).setSelectedNumber(randomNumber - 1); // Selected number array position 0 - [whitelistedSupply - 1]
        emit AssignmentEnded(requestId, golBallPiece, randomNumber);
    }

    /*
     * @dev
     * This function stores the result of each assigned goal direction
     */
    function result(address golBallPieces_) public view returns (uint256) {
        require(
            assignmentStatus[golBallPieces_] != NOT_ASSIGNED,
            "assignment not started"
        );
        require(
            assignmentStatus[golBallPieces_] != ASSIGNMENT_IN_PROGRESS,
            "assignment in progress"
        );
        return resultNumber[golBallPieces_];
    }

    function setKeyHash(
        bytes32 _newKeyHash
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        s_keyHash = _newKeyHash;
    }

    function setCallbackGasLimit(
        uint32 _newCallbackGasLimit
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        callbackGasLimit = _newCallbackGasLimit;
    }

    function setRequestConfirmations(
        uint16 _newRequestConfirmations
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        requestConfirmations = _newRequestConfirmations;
    }
}