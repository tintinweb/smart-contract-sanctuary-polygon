import "./ILayerZeroEndpoint.sol";
import "./ILayerZeroReceiver.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "../lyncrent.sol";
import "hardhat/console.sol";

contract LayerZeroImpl is ILayerZeroReceiver, Ownable {
    address refundAddress = 0xd31Efa8F0f53e7EB8187694a12B3413A63f6b284;

    ILayerZeroEndpoint public endpointPoly;
    ILayerZeroEndpoint public endpointRink;

    event ReceiveMsg(uint16 _srcChainId, address _from, bytes _payload);

    mapping(address => bool) public source;

    //0xf69186dfBa60DdB133E91E9A4B5673624293d8F8 polygon mumbai endpoint
    constructor(address _endpointPoly, address _endpointRink) {
        endpointPoly = ILayerZeroEndpoint(_endpointPoly);
        endpointRink = ILayerZeroEndpoint(_endpointRink);
    }

    function estimateFees(
        uint16 _dstChainId,
        address _userApplication,
        bytes calldata _payload
    ) external view returns (uint256 nativeFee, uint256 zroFee) {
        return
            endpointPoly.estimateFees(
                _dstChainId,
                _userApplication,
                _payload,
                false,
                "0x"
            );
    }

    function setSource(address _source) external onlyOwner {
        source[_source] = true;
    }

    function changeEndpoint(address _endpoint) external onlyOwner {
        endpointRink = ILayerZeroEndpoint(_endpoint);
    }

    function sendMsg(
        uint16 _dstChainId,
        bytes calldata _destination,
        bytes calldata _payload // address refundAd //onlyPool
    ) public payable {
        endpointPoly.send{value: msg.value}(
            _dstChainId,
            _destination,
            _payload,
            payable(refundAddress),
            address(0x0),
            bytes("")
        );
    }

    function lzReceive(
        uint16 _srcChainId,
        bytes memory _from,
        uint64,
        bytes memory _payload
    ) external override {
        require(msg.sender == address(endpointPoly));
        address from;
        assembly {
            from := mload(add(_from, 20))
        }
        // // require(source[from], "Invalid source address");
        if (
            keccak256(abi.encodePacked((_payload))) ==
            keccak256(abi.encodePacked((bytes10("ff"))))
        ) {
            endpointRink.receivePayload(
                1,
                bytes(""),
                address(0x0),
                1,
                1,
                bytes("")
            );
        }
        emit ReceiveMsg(_srcChainId, from, _payload);
    }
}

