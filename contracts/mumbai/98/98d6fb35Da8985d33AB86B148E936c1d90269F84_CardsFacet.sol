contract CardsFacet is Modifiers {
    AppStorage s;

    struct CreateCardArgsStruct {
        uint256 projectId;
        string name;
        string imageCID;
        string description;
        string role;
        string category;
        string twitter;
        string opensea;
        string discord;
        string asset;
    }

    struct CardStruct {
        uint256 tokenId;
        uint256 createdAt;
        uint256 mintedAt;
        uint256 projectId;
        string projectName;
        string projectImageCID;
        string name;
        string imageCID;
        string description;
        string role;
        string category;
        string twitter;
        string opensea;
        string discord;
        address createdBy;
        address mintedBy;
        string projectDisplayName;
        string asset;
    }

    struct CardUserStruct {
        address walletAddress;
        address mintedBy;
        uint256 mintedAt;
        bool isMinted;
    }

    struct UserCardStruct {
        uint256 tokenId;
        uint256 createdAt;
        uint256 mintedAt;
        uint256 projectId;
        string projectName;
        string projectImageCID;
        string name;
        string imageCID;
        string description;
        string role;
        string category;
        string twitter;
        string opensea;
        string discord;
        address createdBy;
        address mintedBy;
        string projectDisplayName;
        string asset;
    }

    struct ProjectCardStruct {
        uint256 tokenId;
        uint256 createdAt;
        uint256 projectId;
        string projectName;
        string projectImageCID;
        string name;
        string imageCID;
        string description;
        string role;
        string category;
        string twitter;
        string opensea;
        string discord;
        address createdBy;
        string projectDisplayName;
        string asset;
    }

    function createToken(CreateCardArgsStruct calldata _args) external payable onlyTrustedForwarder {
        address msgSender = LibERC2771Context.msgSender();

        require(LibProjects.getAdminByProjectId(_args.projectId, msgSender) == 1, "CardsFacet: Not authorized");

        require(s.projectCardIds[_args.projectId].length < LibCards.cardLimit(), "CardsFacet: Card limit reached");

        ++s.userExecutionCount[msgSender]["createCard"];

        uint256 tokenId = LibCards.currentCardId();
        LibCards.incrementCardId();

        s.cardMetadata[tokenId] = MetadataStruct({
            cardId: tokenId,
            projectId: _args.projectId,
            createdAt: block.timestamp,
            name: _args.name,
            imageCID: _args.imageCID,
            description: _args.description,
            role: _args.role,
            category: _args.category,
            twitter: _args.twitter,
            opensea: _args.opensea,
            discord: _args.discord,
            createdBy: msgSender,
            asset: _args.asset
        });
        s.userLatestCardId[msgSender] = tokenId;
        s.projectCardIds[_args.projectId].push(tokenId);
    }

    function setImageCID(uint256 _cardId, string calldata _imageCID) external payable onlyTrustedForwarder {
        uint256 projectId = s.cardMetadata[_cardId].projectId;
        address msgSender = LibERC2771Context.msgSender();
        require(LibProjects.getAdminByProjectId(projectId, msgSender) == 1, "CardsFacet: Not authorized");

        s.cardMetadata[_cardId].imageCID = _imageCID;
    }

    function getCreatedCardId(address _walletAddress) external view returns (uint256) {
        return s.userLatestCardId[_walletAddress];
    }

    function getCard(uint256 cardId_, address _walletAddress) external view returns (CardStruct memory) {
        MetadataStruct memory card = s.cardMetadata[cardId_];
        return
            CardStruct({
                tokenId: card.cardId,
                createdAt: card.createdAt,
                mintedAt: s.mintDetails[_walletAddress][card.cardId].mintedAt,
                projectId: card.projectId,
                projectName: s.projects[card.projectId].name,
                projectImageCID: s.projects[card.projectId].imageCID,
                name: card.name,
                imageCID: card.imageCID,
                description: card.description,
                role: card.role,
                category: card.category,
                twitter: card.twitter,
                opensea: card.opensea,
                discord: card.discord,
                createdBy: card.createdBy,
                mintedBy: s.mintDetails[_walletAddress][card.cardId].mintedBy,
                projectDisplayName: s.projects[card.projectId].displayName,
                asset : card.asset
            });
    }

    function getCardUsers(uint256 cardId_) external view returns (CardUserStruct[] memory) {
        address[] memory users = s.cardUsers[cardId_];
        uint256 count = users.length;
        CardUserStruct[] memory cardUsers = new CardUserStruct[](count);

        for (uint256 i; i < count; ) {
            address user = users[i];
            MintDetailStruct memory mintDetail = s.mintDetails[user][cardId_];
            cardUsers[i] = CardUserStruct({
                walletAddress: user,
                mintedBy: mintDetail.mintedBy,
                mintedAt: mintDetail.mintedAt,
                isMinted: mintDetail.isMinted == 1
            });
            unchecked {
                ++i;
            }
        }

        return cardUsers;
    }

    function getUserCards(address _walletAddress) external view returns (UserCardStruct[] memory) {
        uint256[] memory userCardIds = s.userCardsIds[_walletAddress];
        uint256 count = userCardIds.length;
        UserCardStruct[] memory userCards = new UserCardStruct[](count);

        for (uint256 i; i < count; ) {
            MetadataStruct memory userCard = s.cardMetadata[userCardIds[i]];
            MintDetailStruct memory mintDetail = s.mintDetails[_walletAddress][userCardIds[i]];
            userCards[i] = UserCardStruct({
                tokenId: userCard.cardId,
                createdAt: userCard.createdAt,
                mintedAt: mintDetail.mintedAt,
                projectId: userCard.projectId,
                projectName: s.projects[userCard.projectId].name,
                projectImageCID: s.projects[userCard.projectId].imageCID,
                name: userCard.name,
                imageCID: userCard.imageCID,
                description: userCard.description,
                role: userCard.role,
                category: userCard.category,
                twitter: userCard.twitter,
                opensea: userCard.opensea,
                discord: userCard.discord,
                createdBy: userCard.createdBy,
                mintedBy: mintDetail.mintedBy,
                projectDisplayName: s.projects[userCard.projectId].displayName,
                asset : userCard.asset
            });
            unchecked {
                ++i;
            }
        }

        return userCards;
    }

    function getProjectCards(uint256 _projectId) external view returns (ProjectCardStruct[] memory) {
        uint256[] memory projectCardIds = s.projectCardIds[_projectId];
        uint256 count = projectCardIds.length;
        ProjectCardStruct[] memory projectCards = new ProjectCardStruct[](count);

        for (uint256 i; i < count; ) {
            MetadataStruct memory projectCard = s.cardMetadata[projectCardIds[i]];
            projectCards[i] = ProjectCardStruct({
                tokenId: projectCard.cardId,
                createdAt: projectCard.createdAt,
                projectId: projectCard.projectId,
                projectName: s.projects[projectCard.projectId].name,
                projectImageCID: s.projects[projectCard.projectId].imageCID,
                name: projectCard.name,
                imageCID: projectCard.imageCID,
                description: projectCard.description,
                role: projectCard.role,
                category: projectCard.category,
                twitter: projectCard.twitter,
                opensea: projectCard.opensea,
                discord: projectCard.discord,
                createdBy: projectCard.createdBy,
                projectDisplayName: s.projects[projectCard.projectId].displayName,
                asset : projectCard.asset
            });
            unchecked {
                ++i;
            }
        }

        return projectCards;
    }

    function setCardImageCID(string calldata _cardImageCID) external {
        LibDiamond.enforceIsContractOwner();
        s.cardImageCID = _cardImageCID;
    }

    function cardImageCID() external view returns (string memory) {
        return s.cardImageCID;
    }

    function setCardAnimationCID(string calldata _cardAnimationCID) external {
        LibDiamond.enforceIsContractOwner();
        s.cardAnimationCID = _cardAnimationCID;
    }

    function cardAnimationCID() external view returns (string memory) {
        return s.cardAnimationCID;
    }

    function setKeyChainImageCID(string calldata _keyChainImageCID) external {
        LibDiamond.enforceIsContractOwner();
        s.keyChainImageCID = _keyChainImageCID;
    }

    function keyChainImageCID() external view returns (string memory) {
        return s.keyChainImageCID;
    }

    function setKeyChainAnimationCID(string calldata _keyChainAnimationCID) external {
        LibDiamond.enforceIsContractOwner();
        s.keyChainAnimationCID = _keyChainAnimationCID;
    }

    function keyChainAnimationCID() external view returns (string memory) {
        return s.keyChainAnimationCID;
    }

    function setCardLimit(uint256 _limit) external {
        LibDiamond.enforceIsContractOwner();
        LibCards.setCardLimit(_limit);
    }

    function cardLimit() external view returns (uint256) {
        return LibCards.cardLimit();
    }

    function getMintedCount() external view returns (uint256) {
        uint256 mintedCount;
        uint256 count = LibCards.currentCardId();
        for (uint256 i; i < count; ) {
            mintedCount += s.cardUsers[i + 1].length;
            unchecked {
                ++i;
            }
        }
        return mintedCount;
    }
}

