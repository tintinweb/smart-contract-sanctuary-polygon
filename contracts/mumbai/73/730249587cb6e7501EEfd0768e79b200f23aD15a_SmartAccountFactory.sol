pragma solidity ^0.8.18;

import '@openzeppelin/contracts/utils/Create2.sol';
import './SmartAccount.sol';
import './SmartAccountProxy.sol';
import './handler/FallbackHandler.sol';

/**
 * A factory acontract for SmartAccount
 * The factory's createAccount returns the target account address even if it is already installed.
 */
contract SmartAccountFactory {
    IEntryPoint public immutable entryPoint;
    address public immutable singleton;
    FallbackHandler public immutable callback;
    address public immutable operator;

    event AccountCreated(address indexed account, address indexed owner, uint salt);

    constructor(IEntryPoint _entryPoint, address _singleton, address _operator) {
        entryPoint = _entryPoint;
        singleton = _singleton;
        callback = new FallbackHandler();
        operator = _operator;
    }

    function createAccount(address owner, uint256 salt) public returns (address account) {
        address addr = getAddress(owner, salt);
        uint codeSize = addr.code.length;
        if (codeSize > 0) {
            return addr;
        }
        account = address(new SmartAccountProxy{ salt: bytes32(salt) }(singleton, getInitializer(owner)));

        emit AccountCreated(account, owner, salt);
    }

    function getInitializer(address owner) internal view returns (bytes memory) {
        return abi.encodeCall(SmartAccount.initialize, (entryPoint, owner, operator, address(callback)));
    }

    /**
     * calculate the counterfactual address of this account as it would be returned by createAccount()
     */
    function getAddress(address owner, uint256 salt) public view returns (address) {
        bytes memory initializer = getInitializer(owner);
        bytes memory deploymentData = abi.encodePacked(
            type(SmartAccountProxy).creationCode,
            abi.encode(singleton, initializer)
        );
        return Create2.computeAddress(bytes32(salt), keccak256(deploymentData));
    }
}

