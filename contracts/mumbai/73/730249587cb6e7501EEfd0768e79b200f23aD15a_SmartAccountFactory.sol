import '@openzeppelin/contracts/utils/cryptography/ECDSA.sol';
import '@openzeppelin/contracts/interfaces/IERC1271.sol';
import './TokenCallbackHandler.sol';
import '../SmartAccount.sol';

/**
 * @notice FallbackHandler is used to fallback if SmartAccount has no implementation
 */
contract FallbackHandler is TokenCallbackHandler, IERC1271 {
    using ECDSA for bytes32;
    // bytes4(keccak256("isValidSignature(bytes32,bytes)")
    bytes4 internal constant ERC1271_MAGIC_VALUE = 0x1626ba7e;

    /**
     * @notice support EIP-1271
     */
    function isValidSignature(bytes32 _hash, bytes memory _signature) external view override returns (bytes4) {
        bytes32 hash = _hash.toEthSignedMessageHash();
        address recovered = hash.recover(_signature);

        SmartAccount account = SmartAccount(payable(msg.sender));

        // Validate signatures
        if (account.isOwner(recovered)) {
            return ERC1271_MAGIC_VALUE;
        } else {
            return 0xffffffff;
        }
    }
}

