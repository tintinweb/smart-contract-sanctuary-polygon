library AccountStorage {
    // keccak256("smartaccount.contracts.AccountStorage");
    bytes32 private constant ACCOUNT_SLOT = 0x8f136c122bbcf4e4b0f1998ca3ebe07ac08f37bd97d243ee2db73a3afdc1ccdf;

    struct RoleData {
        mapping(address => bool) members;
        bytes32 adminRole;
    }

