        /// ┌───────────────────┐
        /// │     base data     │
        IEntryPoint entryPoint; // entryPoint
        address fallbackHandler; // fallbackHandler
        uint256 operationNonce; // nonce of operation transaction
        uint256[50] __gap_0;
        /// └───────────────────┘

