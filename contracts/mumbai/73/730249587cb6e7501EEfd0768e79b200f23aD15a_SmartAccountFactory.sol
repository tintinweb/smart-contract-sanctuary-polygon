/**
 * @dev Gaudiy creates an address for Guardian on behalf of the user and grants role to that address.
 *      The Guardian is kept by the user.
 */
abstract contract GuardianManager is IGuardianManager, SelfAuthorized {
    using AccountStorage for AccountStorage.Layout;

    // Sentinel value used to specify the end of a linked list.
    address internal constant SENTINEL_GUARDIANS = address(0x1);

    /**
     * @notice Setup Guardians for recovery.
     * @dev This function can only be called once. For now, only one Guardian is assumed to be available for recovery.
     * @param _guardians List of Guardians for recovery.
     * @param _threshold Number of Guardians required for recovery.
     */
    function setupGuardians(address[] memory _guardians, uint16 _threshold) public authorized {
        GuardianLayout storage layout = AccountStorage.layout().guardianLayout;
        // Check ensures that setup function can only be called once.
        require(layout.threshold == 0, 'GuardianManager: guardians already setup');
        require(_threshold <= _guardians.length, 'GuardianManager: threshold is bigger than the number of guardians');
        // At this stage, there is only one Guardian.
        require(_threshold == 1, 'GuardianManager: threshold must be 1');

        address currentGuardian = SENTINEL_GUARDIANS;
        for (uint256 i = 0; i < _guardians.length; ) {
            address guardian = _guardians[i];
            require(
                guardian != address(0) && guardian != SENTINEL_GUARDIANS && guardian != address(this),
                'GuardianManager: invalid guardian address'
            );
            require(layout.guardians[guardian] == address(0), 'GuardianManager: guardian already added');
            layout.guardians[guardian] = currentGuardian;
            currentGuardian = guardian;
            unchecked {
                i++;
            }
        }
        layout.guardians[currentGuardian] = SENTINEL_GUARDIANS;
        layout.count = uint16(_guardians.length);
        layout.threshold = _threshold;

        emit SetupGuardians(_guardians);
    }

    /**
     * @notice Returns the number of required confirmations for a recovery request aka the threshold.
     */
    function getThreshold() public view returns (uint16) {
        return AccountStorage.layout().guardianLayout.threshold;
    }

    /**
     * @notice Returns if `guardian` is a valid guardian.
     * @param guardian The address of the guardian to check.
     */
    function isGuardian(address guardian) public view returns (bool) {
        GuardianLayout storage layout = AccountStorage.layout().guardianLayout;
        return guardian != SENTINEL_GUARDIANS && layout.guardians[guardian] != address(0);
    }

