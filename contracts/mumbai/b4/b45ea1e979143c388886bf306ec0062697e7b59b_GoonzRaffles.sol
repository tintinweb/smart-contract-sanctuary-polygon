pragma solidity 0.8.17;

import "hardhat/console.sol";

import { IERC20 } from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import { LinkTokenInterface } from "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import { VRFCoordinatorV2Interface } from "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import { VRFConsumerBaseV2 } from "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";

import { MultisigOwnable } from "../util/MultisigOwnable.sol";

error SoldOut();
error RaffleHasNotStarted(uint256 startTime, uint256 currentTime);
error RaffleClosed(uint256 endTime, uint256 currentTime);
error RandomnessRequestAlreadyExists(uint256 requestId);
error RandomnessRequestDoesNotExist(uint256 requestId);

contract GoonzRaffles is VRFConsumerBaseV2, MultisigOwnable {
    // Chainlink Settings
    VRFCoordinatorV2Interface public COORDINATOR;
    LinkTokenInterface public LINKTOKEN;
    struct VRFRequestConfig {
        bytes32 keyHash;
        uint64 subscriptionId;
        uint16 requestConfirmations;
        uint32 callbackGasLimit;
    }
    VRFRequestConfig public requestConfig;
    mapping(uint256 => RandomnessRequest) public randomnessRequests;
    
    uint256 private raffleIdCounter;
    mapping(uint256 => Raffle) public raffles;

    constructor(
        address _vrfCoordinator,
        address _linkToken,
        bytes32 _keyHash,
        uint64 _subscriptionId
    ) VRFConsumerBaseV2(_vrfCoordinator) {
        //hardcoded to mumbai network
        COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
        LINKTOKEN = LinkTokenInterface(_linkToken);
        requestConfig = VRFRequestConfig(
            _keyHash,
            _subscriptionId,
            3,
            uint32(500_000)
        );
    }

    function createRaffle(CreateRaffle calldata newRaffle) external returns (uint256 raffleId) {
        unchecked {
            raffleId = ++raffleIdCounter;
        }
        Raffle storage raffle = raffles[raffleId];

        raffle.minEntries = newRaffle.minEntries;
        raffle.maxEntriesPerWallet = newRaffle.maxEntriesPerWallet;
        raffle.maxTotalEntries = newRaffle.maxTotalEntries;
        raffle.startTime = newRaffle.startTime;
        raffle.endTime = newRaffle.endTime;
        raffle.entryFee = newRaffle.entryFee;
        raffle.entryFeeToken = newRaffle.entryFeeToken;
        raffle.totalWinners = newRaffle.totalWinners;

        for (uint256 i = 0; i < newRaffle.prizes.length; i++) {
            CreatePrize memory create = newRaffle.prizes[i];
            raffle.prizes.push(Prize(create.numOfWinners, create.tierRank));
        }

        emit RaffleCreated({ raffleId: raffleId });
    }

    function enterRaffle(address participant, uint256 raffleId, uint256 amount) external {
        Raffle storage raffle = raffles[raffleId];

        if (raffle.entries.length + amount > raffle.maxTotalEntries) {
            revert SoldOut();
        }

        if (block.timestamp < raffle.startTime) {
            revert RaffleHasNotStarted({ startTime: raffle.startTime, currentTime: block.timestamp });
        }

        if (block.timestamp >= raffle.endTime) {
            revert RaffleClosed({ endTime: raffle.endTime, currentTime: block.timestamp });
        }

        raffle.entries.push(Entry(participant, amount));
        IERC20(raffle.entryFeeToken).transferFrom(participant, address(this), raffle.entryFee);
    }

    function getEntries(uint256 raffleId) external view returns (Entry[] memory entries) {
        entries = raffles[raffleId].entries;
    }

    function drawWinners(uint256 raffleId) external onlyRealOwner returns (uint256 requestId) {
        Raffle storage raffle = raffles[raffleId];
        raffle.drawnAt = uint40(block.timestamp);

        uint256 _requestId = COORDINATOR.requestRandomWords(
            requestConfig.keyHash,
            requestConfig.subscriptionId,
            requestConfig.requestConfirmations,
            requestConfig.callbackGasLimit,
            uint32(1)
        );

        if (randomnessRequests[_requestId].exists) {
            revert RandomnessRequestAlreadyExists({ requestId: _requestId });
        }

        randomnessRequests[_requestId].exists = true;
        randomnessRequests[_requestId].raffleId = raffleId;

        emit RandomnessRequested(raffleId, _requestId);
        return _requestId;
    }

    function fulfillRandomWords(uint256 _requestId, uint256[] memory _randomWords) internal override {
        RandomnessRequest memory req = randomnessRequests[_requestId];
        if (!req.exists) {
            revert RandomnessRequestDoesNotExist({ requestId: _requestId });
        }

        uint256 raffleId = req.raffleId;
        Raffle storage raffle = raffles[raffleId];
        
        Prize[] storage prizes = raffle.prizes;

        uint256 randomWord = uint256(_randomWords[0]);
        for (uint256 i = 0; i < prizes.length; i++) {
            Prize storage prize = prizes[i];
            for (uint256 j = 0; j < prize.numOfWinners; j++) {
                uint256 winningEntryIdx = randomWord % raffle.entries.length;
                raffle.winners.push(raffle.entries[winningEntryIdx].participant);
                randomWord = uint256(keccak256(abi.encodePacked(randomWord)));
            }
        }
    }

    function getWinners(uint256 raffleId) external view returns (address[] memory winners) {
        winners = raffles[raffleId].winners;
    }

    function setVRFRequestConfig(VRFRequestConfig calldata _requestConfig) public onlyRealOwner {
        requestConfig = _requestConfig;
    }

    struct CreateRaffle {
        uint256 minEntries;
        uint256 maxEntriesPerWallet;
        uint256 maxTotalEntries;
        uint256 startTime;
        uint256 endTime;
        uint256 entryFee;
        address entryFeeToken;
        uint256 totalWinners;
        CreatePrize[] prizes;
    }

    struct CreatePrize {
        uint256 numOfWinners;
        uint256 tierRank;
    }

    struct Raffle {
        uint256 minEntries;
        uint256 maxEntriesPerWallet;
        uint256 maxTotalEntries;
        uint256 startTime; // need to decide mechanism for starting the raffle
        uint256 endTime;
        uint256 entryFee;
        address entryFeeToken;
        Entry[] entries;
        uint256 totalWinners;
        Prize[] prizes;
        uint256 drawnAt;
        address[] winners;
    }

    struct Entry {
        address participant;
        uint256 amount;
    }

    struct Prize {
        uint256 numOfWinners;
        uint256 tierRank;
    }

    struct OnChainPrize {
        Prize prize;
        address prizeToken;
        uint256 amount;
        TokenType tokenType;
    }

    enum TokenType {
        ERC721,
        ERC1155,
        ETH,
        ERC20
    }

    struct RandomnessRequest {
        bool exists;
        uint248 randomWord;
        uint256 raffleId;
    }

    event RaffleCreated(uint256 raffleId);
    event RandomnessRequested(uint256 raffleId, uint256 requestId);
}

