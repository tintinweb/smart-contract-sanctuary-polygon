// SPDX-License-Identifier: MIT
pragma solidity 0.8.7;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./libs/IDohyo.sol";
import "hardhat/console.sol";

// BonsaiChef is the master of Bonsai. He manages games and he is a fair guy.
//
// Note that it's ownable and the owner wields tremendous power. The ownership
// will be transferred to the timelock once the contract deployed.
//
// Have fun reading it. Hopefully it's bug-free. God bless.

contract BonsaiChef is ReentrancyGuard {
  using SafeERC20 for IERC20;

  /// @notice Gets incremented for each created game
  uint public gameCounter = 0;

  /// @dev Gets incremented for each ended game
  uint public endedGameCounter = 0;

  IDohyo public dohyo;

  /// @notice Set on game start
  struct GameMeta {
    uint gameId; // Unique id of the game
    address tokenAddress; // Address of the token used for the game
    bool boosted; // True if the game is played in boosted mode
    bool ultraBoosted; // True if the game is played in ultra boosted mode
    uint startTime; // Time when the game was started
    address player1; // Player1 is the one who creates the game
    address player2; // Player2 is the one who starts the game
  }

  /// @notice Changes during game
  struct GameState {
    uint bet; // Amount of tokens to be deposited by each player
    uint pot; // Total amount of tokens deposited by both players
    address nextPlayer; // Next player to extend the game
    uint nextDeadline; // Deadline for the next player to extend the game
    uint deadLinesMet; // Number of times the deadline was met
    bool started; // True if the game is started
    bool ended; // True if the game is ended
    bool cancelled; // True if the game was cancelled
    address winner; // Address of the winner
    address[] gamePlayers; // List of players in game
  }

  /// @notice gameId => GameState
  mapping(uint => GameState) public states;

  /// @notice gameId => GameMeta
  mapping(uint => GameMeta) public metas;

  /// @notice An event emitted when a new game is created
  event GameCreated(uint indexed gameId, address indexed creator);

  /// @notice An event emitted when a game is started
  event GameStarted(
    uint indexed gameId,
    address indexed creator,
    address indexed player2
  );

  /// @notice An event emitted when a game is extended
  event GameExtended(uint indexed gameId);

  /// @notice An event emitted when a game ends
  event GameEnded(uint indexed gameId, address indexed winner, uint indexed pot);

  /// @notice An event emitted when a game is cancelled
  event GameCancelled(uint indexed gameId);

  constructor(address _dohyo) {
    dohyo = IDohyo(_dohyo);
  }

  function createGame(
    address _tokenAddress,
    uint _amount,
    bool _boosted,
    bool _ultraBoosted,
    address _player2
  ) public nonReentrant {
    require(
      !dohyo.isDeprecatedGame(address(this)),
      "BonsaiChef::createGame: Game contract deprecated, please use the newer version"
    );
    require(
      !dohyo.isBlacklisted(msg.sender),
      "BonsaiChef::createGame: You are blacklisted"
    );
    require(
      dohyo.isSupportedToken(_tokenAddress),
      "BonsaiChef::createGame: Unsupported token"
    );
    require(_amount > 0, "BonsaiChef::createGame: Bet must be greater than 0");
    require(
      _player2 != msg.sender,
      "BonsaiChef::createGame: Player2 cannot be the same as Player1"
    );
    require(
      !_boosted || !_ultraBoosted,
      "BonsaiChef::createGame: Cannot be both boosted and ultra boosted"
    );

    uint gameId = gameCounter;
    gameCounter++;

    uint preTransferBalance = IERC20(_tokenAddress).balanceOf(address(this));
    IERC20(_tokenAddress).safeTransferFrom(msg.sender, address(this), _amount);
    uint amount = IERC20(_tokenAddress).balanceOf(address(this)) - preTransferBalance;

    metas[gameId] = GameMeta(
      gameId,
      _tokenAddress,
      _boosted,
      _ultraBoosted,
      0,
      msg.sender,
      _player2
    );
    states[gameId] = GameState(
      _amount,
      amount,
      address(0),
      0,
      0,
      false,
      false,
      false,
      address(0),
      new address[](1)
    );

    GameState storage state = states[gameId];

    state.gamePlayers.push(msg.sender);

    dohyo.recordEntry(_tokenAddress, msg.sender, amount, gameId, false, 0);

    emit GameCreated(gameId, msg.sender);
  }

  function joinGame(uint _gameId) public nonReentrant {
    require(
      !dohyo.isBlacklisted(msg.sender),
      "BonsaiChef::joinGame: You are blacklisted"
    );
    require(
      _gameId >= 0 && _gameId < gameCounter,
      "BonsaiChef::joinGame: Invalid gameId"
    );

    GameMeta storage meta = metas[_gameId];
    GameState storage state = states[_gameId];

    require(!state.ended, "BonsaiChef::joinGame: Game already ended");
    require(!state.started, "BonsaiChef::joinGame: Game already started");
    require(
      msg.sender != meta.player1,
      "BonsaiChef::joinGame: Player1 cannot start his own game"
    );

    if (meta.player2 == address(0)) {
      meta.player2 = msg.sender;
    } else {
      require(
        msg.sender == meta.player2,
        "BonsaiChef::joinGame: You are not allowed to join this game"
      );
    }

    uint preTransferBalance = IERC20(meta.tokenAddress).balanceOf(address(this));
    IERC20(meta.tokenAddress).safeTransferFrom(msg.sender, address(this), state.bet);
    uint _amount = IERC20(meta.tokenAddress).balanceOf(address(this)) -
      preTransferBalance;

    dohyo.recordEntry(meta.tokenAddress, msg.sender, _amount, _gameId, false, 0);

    state.pot = state.pot + _amount;
    state.nextPlayer = meta.player1;
    meta.startTime = block.timestamp;
    state.nextDeadline = block.timestamp + 1 days;
    state.started = true;
    state.gamePlayers.push(msg.sender);

    emit GameStarted(_gameId, meta.player1, meta.player2);
  }

  function extendGame(uint _gameId) public nonReentrant {
    require(
      _gameId >= 0 && _gameId < gameCounter,
      "BonsaiChef::extendGame: Invalid gameId"
    );

    GameMeta storage meta = metas[_gameId];
    GameState storage state = states[_gameId];

    require(state.started, "BonsaiChef::extendGame: Game not started yet");
    require(!state.ended, "BonsaiChef::extendGame: Game already ended");

    if (block.timestamp < state.nextDeadline) {
      if (msg.sender == state.nextPlayer) {
        if (meta.boosted || meta.ultraBoosted) {
          if (meta.ultraBoosted) {
            state.bet = (state.bet * 110) / 100;
          }

          uint preTransferBalance = IERC20(meta.tokenAddress).balanceOf(address(this));
          IERC20(meta.tokenAddress).safeTransferFrom(
            msg.sender,
            address(this),
            state.bet
          );
          uint _amount = IERC20(meta.tokenAddress).balanceOf(address(this)) -
            preTransferBalance;

          state.pot = state.pot + _amount;

          dohyo.recordBet(msg.sender, meta.tokenAddress, _amount, false, 0);
        }

        state.nextPlayer = (state.nextPlayer == meta.player1)
          ? meta.player2
          : meta.player1;

        if (meta.ultraBoosted) {
          state.deadLinesMet++;
        }

        state.nextDeadline = state.deadLinesMet >= 24
          ? block.timestamp + 1 hours
          : block.timestamp + 1 days - (state.deadLinesMet * 1 hours);

        emit GameExtended(_gameId);
      }
    } else {
      endGame(state, meta);
    }
  }

  function endGame(GameState storage _state, GameMeta storage _meta) internal {
    address winner = (_state.nextPlayer == _meta.player1) ? _meta.player2 : _meta.player1;
    _state.winner = winner;

    (, bool isPartner_, address tokenPartner_, uint tokenEdge_) = dohyo.getSupportedToken(
      _meta.tokenAddress
    );
    uint totalEdgeAmount = 0;
    uint partnerEdgeAmount = 0;
    uint dohyoEdgeAmount = 0;

    if (tokenEdge_ > 0) {
      totalEdgeAmount = (_state.pot * tokenEdge_) / 1000;
      dohyoEdgeAmount = totalEdgeAmount;

      if (isPartner_) {
        partnerEdgeAmount = totalEdgeAmount / 2;
        dohyoEdgeAmount = totalEdgeAmount / 2;
      }
    }

    uint winAmount = _state.pot - totalEdgeAmount;

    uint preTransferBalance = IERC20(_meta.tokenAddress).balanceOf(winner);
    safeDohyoTransfer(_meta.tokenAddress, winner, winAmount);
    uint postTransferBalance = IERC20(_meta.tokenAddress).balanceOf(winner);
    winAmount = winAmount - (winAmount - (postTransferBalance - preTransferBalance));

    if (dohyoEdgeAmount > 0) {
      safeDohyoTransfer(_meta.tokenAddress, dohyo.getTreasuryAddress(), dohyoEdgeAmount);
    }
    if (partnerEdgeAmount > 0) {
      safeDohyoTransfer(_meta.tokenAddress, tokenPartner_, partnerEdgeAmount);
    }

    dohyo.recordWin(winner, _meta.tokenAddress, winAmount, false, 0);

    _state.ended = true;
    endedGameCounter++;

    emit GameEnded(_meta.gameId, winner, _state.pot);
  }

  function cancelGame(uint _gameId) public nonReentrant {
    require(
      _gameId >= 0 && _gameId < gameCounter,
      "BonsaiChef::cancelGame: Invalid gameId"
    );

    GameMeta storage meta = metas[_gameId];
    GameState storage state = states[_gameId];

    require(
      msg.sender == meta.player1,
      "BonsaiChef::cancelGame: Only creator can cancel the game"
    );
    require(!state.started, "BonsaiChef::cancelGame: Game has already been started");
    require(!state.cancelled, "BonsaiChef::cancelGame: Game has already been cancelled");

    safeDohyoTransfer(meta.tokenAddress, meta.player1, state.pot);

    dohyo.recordEntryCancel(msg.sender, meta.tokenAddress, state.pot);

    state.ended = true;
    state.cancelled = true;
    endedGameCounter++;

    emit GameCancelled(_gameId);
  }

  // Safe transfer function, just in case if rounding error causes contract to not have enough tokens.
  function safeDohyoTransfer(address _tokenAddress, address _to, uint _amount) internal {
    uint contractBalance = IERC20(_tokenAddress).balanceOf(address(this));
    if (_amount > contractBalance) {
      IERC20(_tokenAddress).safeTransfer(_to, contractBalance);
    } else {
      IERC20(_tokenAddress).safeTransfer(_to, _amount);
    }
  }

  function getGames(
    uint[] calldata _gameIds
  ) public view returns (GameMeta[] memory, GameState[] memory) {
    uint length = _gameIds.length;
    GameMeta[] memory gameMetas = new GameMeta[](length);
    GameState[] memory gameStates = new GameState[](length);

    for (uint i = 0; i < length; i++) {
      require(_gameIds[i] < gameCounter, "BonsaiChef::getGames: Invalid gameId");
      gameMetas[i] = metas[_gameIds[i]];
      gameStates[i] = states[_gameIds[i]];
    }

    return (gameMetas, gameStates);
  }
}

