import "hardhat/console.sol";

contract Swapper {

    // =================================
    // This contract is stateless. It is called by Swap contract using delegatecall,
    // so it can't have any storage variables.
    // constants are stored in code, so they are not stored in storage.
    // =================================

    address public constant uniRouter = 0xE592427A0AEce92De3Edee1F18E0157C05861564;
    // IWETH9 public constant wmatic = IWETH9(0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270);
    IWETH9 public constant wmatic = IWETH9(0x9c3C9283D3e44854697Cd22D3Faa240Cfb032889);
    // IUniswapOracleV3 public constant oracle = IUniswapOracleV3(0xE77c7F14e8EB9925ca418bF80c0a81a5B9C87683);
    IUniswapOracleV3 public constant oracle = IUniswapOracleV3(0x5108412Dd50A6ea79d2F13D5d1A23FDD9bF532db);

    // =================================
    // Errors
    // =================================

    error SWAPPER_AMOUNTIN_TOO_BIG();

    // =================================
    // Functions
    // =================================

    function swapNFT(IERC20 tokenIn, IERC20 priceToken, IERC20 tokenOut, uint256 priceInPriceToken, address seller) external payable {

        uint256 maxPriceToPay = oracle.consult((address(priceToken) == address(0) ? address(wmatic) : address(priceToken)), priceInPriceToken, (address(tokenIn) == address(0) ? address(wmatic) : address(tokenIn))) * 101 / 100; // 1% slippage
        (maxPriceToPay == 0) ? maxPriceToPay = priceInPriceToken : maxPriceToPay; // maxPriceToPay == 0 only if priceToken == tokenIn

        if (address(tokenIn) == address(0)) {
            wmatic.deposit{value: maxPriceToPay}();
        } else {
            tokenIn.transferFrom(msg.sender, address(this), maxPriceToPay);
        }

        if (
            (address(priceToken) == address(0) ? address(wmatic) : address(priceToken)) != (address(tokenIn) == address(0) ? address(wmatic) : address(tokenIn))
        ) {
            ISwapRouter(uniRouter).exactOutput(
                ISwapRouter.ExactOutputParams({
                    path: abi.encodePacked((address(priceToken) == address(0) ? address(wmatic) : address(priceToken)), uint24(500), (address(tokenIn) == address(0) ? address(wmatic) : address(tokenIn))),
                    recipient: address(this),
                    deadline: block.timestamp,
                    amountOut: priceInPriceToken,
                    amountInMaximum: maxPriceToPay
                })
            );
        }

        uint256 outAmount;

        if (
            (address(priceToken) == address(0) ? address(wmatic) : address(priceToken)) != (address(tokenOut) == address(0) ? address(wmatic) : address(tokenOut))
        ) {
            outAmount = ISwapRouter(uniRouter).exactInput(
                ISwapRouter.ExactInputParams({
                    path: abi.encodePacked((address(priceToken) == address(0) ? address(wmatic) : address(priceToken)), uint24(500), (address(tokenOut) == address(0) ? address(wmatic) : address(tokenOut))),
                    recipient: address(this),
                    deadline: block.timestamp,
                    amountIn: priceInPriceToken,
                    amountOutMinimum: 0
                })
            );
        } else {
            outAmount = priceInPriceToken;
        }

        // отправка суммы продавцу
        if (address(tokenOut) == address(0)) {
            wmatic.withdraw(outAmount);
            payable(seller).transfer(outAmount);
        } else {
            tokenOut.transfer(seller, outAmount);
        }

        // возврат остатка покупателю
        if (address(tokenIn) == address(0)) {
            wmatic.withdraw(wmatic.balanceOf(address(this)));
            payable(msg.sender).transfer(address(this).balance);
        } else {
            tokenIn.transfer(msg.sender, tokenIn.balanceOf(address(this)));
        }

    }

    function swapToken(IERC20 tokenIn, IERC20 priceToken, IERC20 tokenOut, uint256 priceInPriceToken, address buyer) external {

        uint256 maxPriceToPay = oracle.consult((address(priceToken) == address(0) ? address(wmatic) : address(priceToken)), priceInPriceToken, (address(tokenIn) == address(0) ? address(wmatic) : address(tokenIn))) * 101 / 100; // 1% slippage
        (maxPriceToPay == 0) ? maxPriceToPay = priceInPriceToken : maxPriceToPay; // maxPriceToPay == 0 only if priceToken == tokenIn
        tokenIn.transferFrom(buyer, address(this), maxPriceToPay);

        if (
            (address(priceToken) == address(0) ? address(wmatic) : address(priceToken)) != (address(tokenIn) == address(0) ? address(wmatic) : address(tokenIn))
        ) {
            ISwapRouter(uniRouter).exactOutput(
                ISwapRouter.ExactOutputParams({
                    path: abi.encodePacked((address(priceToken) == address(0) ? address(wmatic) : address(priceToken)), uint24(500), (address(tokenIn) == address(0) ? address(wmatic) : address(tokenIn))),
                    recipient: address(this),
                    deadline: block.timestamp,
                    amountOut: priceInPriceToken,
                    amountInMaximum: maxPriceToPay
                })
            );
        }

        uint256 outAmount;

        if (
            (address(priceToken) == address(0) ? address(wmatic) : address(priceToken)) != (address(tokenOut) == address(0) ? address(wmatic) : address(tokenOut))
        ) {
            outAmount = ISwapRouter(uniRouter).exactInput(
                ISwapRouter.ExactInputParams({
                    path: abi.encodePacked((address(priceToken) == address(0) ? address(wmatic) : address(priceToken)), uint24(500), (address(tokenOut) == address(0) ? address(wmatic) : address(tokenOut))),
                    recipient: address(this),
                    deadline: block.timestamp,
                    amountIn: priceInPriceToken,
                    amountOutMinimum: 0
                })
            );
        } else {
            outAmount = priceInPriceToken;
        }

        // отправка суммы продавцу
        if (address(tokenOut) == address(0)) {
            wmatic.withdraw(outAmount);
            payable(msg.sender).transfer(outAmount);
        } else {
            tokenOut.transfer(msg.sender, outAmount);
        }

        // возврат остатка покупателю
        if (address(tokenIn) == address(0)) {
            wmatic.withdraw(wmatic.balanceOf(address(this)));
            payable(buyer).transfer(address(this).balance);
        } else {
            tokenIn.transfer(buyer, tokenIn.balanceOf(address(this)));
        }

    }

}

