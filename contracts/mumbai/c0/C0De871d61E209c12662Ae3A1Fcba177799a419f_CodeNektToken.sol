pragma solidity 0.8.17;
pragma experimental ABIEncoderV2;


library LibCodeNektNFT {
    bytes32 constant DIAMOND_CODENEKT_NFT_STORAGE_POSITION = keccak256("codenekt.standard.diamond.nftstorage");

    struct CodeNektNFTStorage {
        mapping (string=>bool) isVinExist;

    }

    function codeNektNftIsVinExists() internal pure returns (CodeNektNFTStorage storage codeNektNFTStorage) {
        bytes32 position = DIAMOND_CODENEKT_NFT_STORAGE_POSITION;
        assembly {
            codeNektNFTStorage.slot := position
        }
    }

