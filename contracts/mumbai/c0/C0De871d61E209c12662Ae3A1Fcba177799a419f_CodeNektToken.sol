pragma solidity 0.8.17;

import { ERC721MetadataInternal } from '../internal/ERC721MetadataInternal.sol';
import {ERC721MetadataStorage} from "../libraries/ERC721MetadataStorage.sol";
import {CodeNektAccessControl} from "../facets/CodeNektAccessControl.sol";
import {AccessControlStorage} from "../libraries/AccessControlStorage.sol";
 contract CodeNektToken is ERC721MetadataInternal {
    constructor (address _accessControl)  {
        ERC721MetadataStorage.setAccessControl(_accessControl);
    }
    function getNFTUriByVin(
        string memory vin
    ) external view returns (string memory) {
        return _tokenURIByVin(vin);
    }

    function getNFTUriById(
        uint256 tokenId
    ) external view returns (string memory) {
        return _tokenURIById(tokenId);
    }

    function minNFT(string memory _uri ,string memory _vin) public {
        require( CodeNektAccessControl(ERC721MetadataStorage.layout().accessControl).hasRole(AccessControlStorage.DEFAULT_ADMIN_ROLE, msg.sender) || CodeNektAccessControl(ERC721MetadataStorage.layout().accessControl).hasRole(AccessControlStorage.ADMIN_ROLE, msg.sender) || CodeNektAccessControl(ERC721MetadataStorage.layout().accessControl).hasRole(AccessControlStorage.MINTER, msg.sender));
        _mint(_uri,_vin);
    }
    function updateNFT(string memory _uri ,string memory _vin) public {
        require( CodeNektAccessControl(ERC721MetadataStorage.layout().accessControl).hasRole(AccessControlStorage.DEFAULT_ADMIN_ROLE, msg.sender) || CodeNektAccessControl(ERC721MetadataStorage.layout().accessControl).hasRole(AccessControlStorage.ADMIN_ROLE, msg.sender) || CodeNektAccessControl(ERC721MetadataStorage.layout().accessControl).hasRole(AccessControlStorage.UPDATER, msg.sender));
        _update(_uri,_vin);
    }
}

