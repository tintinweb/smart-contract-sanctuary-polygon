import { IERC721BaseInternal } from '../interfaces/IERC721BaseInternal.sol';
import { EnumerableMap } from '@solidstate/contracts/data/EnumerableMap.sol';
import { EnumerableSet } from '@solidstate/contracts/data/EnumerableSet.sol';
import { AddressUtils } from '@solidstate/contracts/utils/AddressUtils.sol';
import {LibCodeNektNFT} from "../libraries/LibCodeNektNFT.sol";
import {ERC721MetadataStorage} from "../libraries/ERC721MetadataStorage.sol";
/** 
 * @title Base ERC721 internal functions
 */
abstract contract ERC721BaseInternal is IERC721BaseInternal {
    using AddressUtils for address;
    using EnumerableMap for EnumerableMap.UintToAddressMap;
    using EnumerableSet for EnumerableSet.UintSet;


    function _mint(string memory _uri ,string memory _vin) internal virtual {
        if (LibCodeNektNFT.codeNektNftIsVinExists().isVinExist[_vin]) revert ERC721Base__TokenAlreadyMinted();
        ERC721MetadataStorage.incrementCount();
        ERC721MetadataStorage.setNFTUriTokenMinted(ERC721MetadataStorage.diamondStorageCounter(),_uri, _vin);
        LibCodeNektNFT.newVin(_vin);
    }

    function _update(string memory _uri ,string memory _vin) internal virtual {
        if (!LibCodeNektNFT.codeNektNftIsVinExists().isVinExist[_vin] ) revert ERC721Base__TokenNotExist();
        uint256 token_Id = ERC721MetadataStorage.layout().vinUri[_vin];
        string memory tokenUri =  ERC721MetadataStorage.layout().tokenURIs[token_Id];
        if(keccak256(abi.encodePacked(tokenUri))  == keccak256(abi.encodePacked(_uri)) )  revert ERC721Base__TokenUrlNotUpdated();
        ERC721MetadataStorage.setNFTUri(_uri, _vin);
    }
   
}

