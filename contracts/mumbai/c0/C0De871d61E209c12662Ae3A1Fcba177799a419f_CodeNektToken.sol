pragma solidity 0.8.17;

import { UintUtils } from '@solidstate/contracts/utils/UintUtils.sol';
import { IERC721MetadataInternal } from '../interfaces/IERC721MetadataInternal.sol';
import { ERC721MetadataStorage } from '../libraries/ERC721MetadataStorage.sol';
import {LibCodeNektNFT} from "../libraries/LibCodeNektNFT.sol";
import {ERC721BaseInternal} from "./ERC721BaseInternal.sol";
/**
 * @title ERC721Metadata internal functions
 */
abstract contract ERC721MetadataInternal is
    IERC721MetadataInternal , ERC721BaseInternal 
{
    using UintUtils for uint256;

    /**
     * @notice get generated URI for given token
     * @return token URI
     */
    function _tokenURIByVin(
        string memory vin
    ) internal view virtual returns (string memory) {
        if (!LibCodeNektNFT.codeNektNftIsVinExists().isVinExist[vin]) revert ERC721Metadata__NonExistentToken();

        ERC721MetadataStorage.Layout storage l = ERC721MetadataStorage.layout();
        return _tokenURIById(l.vinUri[vin]) ;
    }
    function _tokenURIById(
        uint256 tokenId
  ) internal view virtual returns (string memory) {
        if (ERC721MetadataStorage.diamondStorageCounter() < tokenId) revert ERC721Metadata__NonExistentToken();

        ERC721MetadataStorage.Layout storage l = ERC721MetadataStorage.layout();

        string memory tokenIdURI = l.tokenURIs[tokenId];
        string memory baseURI = l.baseURI;

        if (bytes(baseURI).length == 0) {
            return tokenIdURI;
        } else if (bytes(tokenIdURI).length > 0) {
            return string(abi.encodePacked(baseURI, tokenIdURI));
        } else {
            return string(abi.encodePacked(baseURI, tokenId.toString()));
        }
    }
}

