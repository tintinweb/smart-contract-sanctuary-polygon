abstract contract BlackList {
    using EnumerableSet for EnumerableSet.AddressSet;

    EnumerableSet.AddressSet private _blackList;

    event AddedToBlackList(address indexed account, uint256 indexed timestamp);
    event RemovedFromBlackList(
        address indexed account,
        uint256 indexed timestamp
    );

    function _addToBlackListBatch(address[] calldata account) internal {
        uint256 length = account.length;
        require(length != 0, "addToBlackListBatch: No address available");
        for (uint256 i; i < length; i++) {
            _addToBlackList(account[i]);
        }
    }

    function _removeFromBlackListBatch(address[] calldata account) internal {
        uint256 length = account.length;
        require(length != 0, "removeFromBlackListBatch: No address available");
        for (uint256 i; i < length; i++) {
            _removeFromBlackList(account[i]);
        }
    }

    function totalBlackListedUser() external view returns (uint256) {
        return _blackList.length();
    }

    function blackListedUserByIndex(uint256 index)
        external
        view
        returns (address)
    {
        return _blackList.at(index);
    }

    function isBlackListed(address account) public view returns (bool) {
        return _blackList.contains(account);
    }

    function _addToBlackList(address account) internal {
        require(account != address(0), "account: Zero Address");
        require(!isBlackListed(account), "account: Already BlackListed");
        _blackList.add(account);
        emit AddedToBlackList(account, block.timestamp);
    }

    function _removeFromBlackList(address account) internal {
        require(account != address(0), "account: Zero Address");
        require(isBlackListed(account), "account: Not BlackListed");
        _blackList.remove(account);
        emit RemovedFromBlackList(account, block.timestamp);
    }
}

contract APELandNFT is
    ERC1155,
    AccessControlEnumerable,
    Pausable,
    ERC1155Burnable,
    ERC1155Supply,
    BlackList
{
    using ECDSA for bytes32;

    bytes32 public constant URI_SETTER_ROLE = keccak256("URI_SETTER_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant APELAND_HASH = keccak256("APELAND_HASH");
    bytes32 public constant BLOCKER_ROLE = keccak256("BLOCKER_ROLE");
    bytes32 public constant SIGNER_ROLE = keccak256("SIGNER_ROLE");

    mapping(bytes32 => bool) public isSigExist;

    constructor(address account) ERC1155("") {
        _grantRole(DEFAULT_ADMIN_ROLE, account);
        _grantRole(URI_SETTER_ROLE, account);
        _grantRole(PAUSER_ROLE, account);
        _grantRole(BLOCKER_ROLE, account);
        _grantRole(SIGNER_ROLE, account);
    }

    function setURI(string memory newuri) public onlyRole(URI_SETTER_ROLE) {
        _setURI(newuri);
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function lazyMint(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data,
        bytes memory signature
    ) external returns (bool) {
        require(
            hasRole(SIGNER_ROLE, getSigner(to, ids, amounts, data, signature)),
            "lazyMint: Invalid signer Address"
        );

        _mintBatch(to, ids, amounts, data);
        return true;
    }

    function addToBlackList(address[] calldata account)
        external
        onlyRole(BLOCKER_ROLE)
    {
        _addToBlackListBatch(account);
    }

    function removeFromBlackList(address[] calldata account)
        external
        onlyRole(BLOCKER_ROLE)
    {
        _removeFromBlackListBatch(account);
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155, ERC1155Supply) whenNotPaused {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);

        require(
            !isBlackListed(operator),
            "beforeTokenTransfer: operator is BlackListed"
        );
        require(
            !isBlackListed(from),
            "beforeTokenTransfer: from is BlackListed"
        );
        require(!isBlackListed(to), "beforeTokenTransfer: to is BlackListed");
        require(
            !isBlackListed(_msgSender()),
            "beforeTokenTransfer: msgSender is BlackListed"
        );
    }

    // The following functions are overrides required by Solidity.

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155, AccessControlEnumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function prepareHash(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public pure returns (bytes32) {
        return
            keccak256(abi.encodePacked(to, ids, amounts, data, APELAND_HASH));
    }

    function getSigner(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data,
        bytes memory signature
    ) private returns (address) {
        bytes32 hash = prepareHash(to, ids, amounts, data)
            .toEthSignedMessageHash();
        require(!isSigExist[hash], "getSigner: Signature is exist");
        isSigExist[hash] = true;
        return hash.recover(signature);
    }
}

contract APELandFACTORY is AccessControlEnumerable, BlackList {
    using EnumerableSet for EnumerableSet.AddressSet;

    bytes32 public constant DEPLOYER_ROLE = keccak256("DEPLOYER_ROLE");
    EnumerableSet.AddressSet private _users;

    mapping(address => EnumerableSet.AddressSet) private _factory;

    event Deploy(
        address indexed account,
        address indexed deployedContract,
        uint256 time
    );

    constructor(address account) {
        _grantRole(DEFAULT_ADMIN_ROLE, account);
        _grantRole(DEPLOYER_ROLE, account);
    }

    function deploy(address account) external onlyRole(DEPLOYER_ROLE) {
        require(account != address(0), "deploy: Zero Address");
        require(!isBlackListed(account), "deploy: account is BlackListed");

        bytes memory bytecode = type(APELandNFT).creationCode;
        bytes memory constructorArgs = abi.encode(account);
        bytes memory deploymentData = abi.encodePacked(
            bytecode,
            constructorArgs
        );

        address deployedContract = Create2.deploy(
            0,
            getSalt(getCreatedCount(account), account),
            deploymentData
        );

        _factory[account].add(deployedContract);
        _users.add(account);

        emit Deploy(account, deployedContract, block.timestamp);
    }

    function computeAddress(bytes32 salt, bytes32 bytecodeHash)
        external
        view
        returns (address)
    {
        return Create2.computeAddress(salt, bytecodeHash);
    }

    function isCreatedContract(address account, address contractAddr)
        external
        view
        returns (bool)
    {
        return _factory[account].contains(contractAddr);
    }

    function getCreatedAddress(address account, uint256 index)
        external
        view
        returns (address)
    {
        return _factory[account].at(index);
    }

    function getAllCreatedAddress(address account)
        external
        view
        returns (address[] memory)
    {
        return _factory[account].values();
    }

    function getAllUsers() external view returns (address[] memory) {
        return _users.values();
    }

    function getUserByIndex(uint256 index) external view returns (address) {
        return _users.at(index);
    }

    function getUsersCount() external view returns (uint256) {
        return _users.length();
    }

    function getHash(address account) external pure returns (bytes32) {
        bytes memory bytecode = type(APELandNFT).creationCode;
        bytes memory constructorArgs = abi.encode(account);
        bytes memory deploymentData = abi.encodePacked(
            bytecode,
            constructorArgs
        );
        return keccak256(deploymentData);
    }

    function getSalt(uint256 nonce, address account)
        public
        pure
        returns (bytes32)
    {
        return keccak256(abi.encodePacked(nonce, account));
    }

    function getCreatedCount(address account) public view returns (uint256) {
        return _factory[account].length();
    }
}