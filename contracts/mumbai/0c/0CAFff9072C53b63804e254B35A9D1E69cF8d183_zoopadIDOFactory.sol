        if (buyCount >= 10) {
            participantClaimableIDOtokens += (allocatedToken * 20) / 100; // 20% of allocatedToken to be distributed
        }

        // Check the market cap and add the appropriate percentage of tokens to the claimable tokens
        if (
            marketCap >= idoInfo.mcapCheckpoints[0] &&
            marketCap < idoInfo.mcapCheckpoints[1]
        ) {
            participantClaimableIDOtokens += (allocatedToken * 20) / 100; // 20% of allocatedToken to be distributed
        } else if (
            marketCap >= idoInfo.mcapCheckpoints[1] &&
            marketCap < idoInfo.mcapCheckpoints[2]
        ) {
            participantClaimableIDOtokens += (allocatedToken * 40) / 100; // 40% of allocatedToken to be distributed
        } else if (
