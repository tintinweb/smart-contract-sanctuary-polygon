pragma solidity ^0.8.10;

import "./zoopadIDOStorage.sol";
import "./interfaces/IMemeToken.sol";
import "./interfaces/IStableCoin.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";

/**
 * @title Zoopad IDO contract
 * @author Pedals Up Team
 * @notice This contract is used for the IDO of the project.
 */
contract ZoopadIDO is zoopadIDOStorage, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using SafeERC20 for IStableCoin;
    using SafeERC20 for IMemeToken;


    modifier onlyIdoOwner() {
        require(
            msg.sender == idoInfo.idoOwner,
            "zoopadIDO: Only IDO owner can call this function"
        );
        _;
    }

    modifier onlyFactory {
        require(
            msg.sender == factory,
            "zoopadIDO: Only factory can initialize IDO"
        );
        _;
    }

    modifier checkIDOState() {
        require(
            idoState == zoopadIDOLib.IDOState.STARTED,
            "zoopadIDO: IDO has not started"
        );

        require(
            block.timestamp >= idoTimeInfo.idoStartTime,
            "zoopadIDO: IDO has not started"
        );
        _;
    }

    constructor() {
        factory = msg.sender;
    }

    /**
     * 
     * @param idoData is the struct of IDOInfo
     * @param idoTokens is the struct of Tokens
     * @param idoTimeData is the struct of IDOTimeInfo
     */
    function initialize(
        zoopadIDOLib.IDOInfo memory idoData,
        zoopadIDOLib.Tokens memory idoTokens,
        zoopadIDOLib.IDOTimeInfo memory idoTimeData,
        zoopadIDOLib.ProjectInfo memory _projectInfo
    ) external onlyFactory { 
        require(!isInitialized, "zoopadIDO: Already initialized");
        require(
            idoTokens.memeToken != address(0),
            "zoopadIDO: Invalid meme token address"
        );
        require(
            idoTokens.idoToken != address(0),
            "zoopadIDO: Invalid token address"
        );
        require(
            idoData.idoTokenPercent > 0,
            "zoopadIDO: Invalid tokens for sale"
        );
        require(idoData.tokenPrice > 0, "zoopadIDO: Invalid token price");

        require(
            idoTokens.stablecoin != address(0),
            "zoopadIDO: Invalid stablecoin address"
        );

        require(
            idoData.mcapCheckpoints.length == 4,
            "zoopadIDO: Invalid mcap checkpoints"
        );

        require(
            idoTimeData.idoStartTime >= block.timestamp && idoTimeData.idoEndTime > idoTimeData.idoStartTime,
            "zoopadIDO: Invalid IDO time"
        );

        require(
            idoTimeData.claimableTime > idoTimeData.idoEndTime &&
                idoTimeData.claimableTime > idoTimeData.idoStartTime,
            "zoopadIDO: Invalid claimable time"
        );

        require(
            idoData.minimumMemeTokens > 0,
            "zoopadIDO: Invalid minimum meme tokens"
        );

        require(
            idoData.minMemeTokenToParticipate > 0,
            "zoopadIDO: Invalid minimum meme tokens to participate"
        );

        idoInfo = idoData;
        tokens = idoTokens;
        idoTimeInfo = idoTimeData;
        projectInfo = _projectInfo;

        idoState = zoopadIDOLib.IDOState.NOT_STARTED;
        tokensLeft = (idoData.idoTokenPercent * idoData.tokenTotalSupply) / 100;
        isInitialized = true;

        emit IDOInitialized(
            idoData.idoOwner,
            idoTokens.idoToken,
            idoTokens.stablecoin,
            idoData.tokenTotalSupply,
            idoData.tokenPrice,
            idoData.minimumMemeTokens,
            idoData.minMemeTokenToParticipate,
            idoTimeData.idoStartTime,
            idoTimeData.idoEndTime,
            idoTimeData.claimableTime
        );
    }

    /**
     * @notice Start the IDO
     * @dev This function can only be called by the IDO owner
     * @dev This function can only be called once
     */
    function startIDO() public onlyIdoOwner {
        require(
            idoState == zoopadIDOLib.IDOState.NOT_STARTED,
            "zoopadIDO: IDO has already started"
        );

        uint256 tokenForSale = (idoInfo.idoTokenPercent * idoInfo.tokenTotalSupply) / 100;
        IERC20(tokens.idoToken).safeTransferFrom(
            idoInfo.idoOwner,
            address(this),
            tokenForSale
        );
        idoState = zoopadIDOLib.IDOState.STARTED;

        emit IDOStarted(
            idoInfo.idoOwner,
            block.timestamp
        );
    }

    /**
     * 
     * @param _memeTokensAmount is the amount of meme tokens sent by the user
     * @notice This function is used to participate in the IDO
     * @dev This function can only be called when the IDO is in STARTED state
     * @dev This function can only be called before the IDO end time
     * @dev This function can only be called if the user has enough meme tokens  @audit 
     */
    function participate(uint256 _memeTokensAmount) external checkIDOState nonReentrant{
// need to discuss that how many times a user can participate in the IDO with jimish
        require(
            IMemeToken(tokens.memeToken).balanceOf(msg.sender) >= _memeTokensAmount,
            "zoopadIDO: Not enough meme tokens"
        );

        require(
            _memeTokensAmount >= idoInfo.minMemeTokenToParticipate,
            "zoopadIDO: Minimum meme tokens to participate not met"
        );

        IMemeToken(tokens.memeToken).safeTransferFrom(
                msg.sender,
                address(this),
                _memeTokensAmount
        );

        totalMemeTokensSent += _memeTokensAmount;
        participants[msg.sender].memeTokensSent = _memeTokensAmount;

        emit ParticipatedInIDO(
            msg.sender,
            _memeTokensAmount,
            block.timestamp
        );
    }

    /**
     * @notice This function is used to claim the IDO tokens
     * @dev This function can only be called when the IDO is in CLAIMABLE state
     * @dev This function can only be called after the claimable time
     * @dev This function can only be called if the user has participated in the IDO
     * @dev This function can only be called if the user has not claimed the IDO tokens
     */
    function sendStablecoins(uint256 stablecoinTokenAmount) external checkIDOState nonReentrant {
        require(
            IStableCoin(tokens.stablecoin).balanceOf(msg.sender) >= stablecoinTokenAmount, "zoopadIDO: Not enough stablecoins"
        );

        require(
            block.timestamp > idoTimeInfo.idoEndTime,
            "zoopadIDO: IDO allocation has not ended"
        );

        require(
            participants[msg.sender].memeTokensSent > 0,
            "zoopadIDO: User has not participated in the IDO"
        );

        IStableCoin(tokens.stablecoin).safeTransferFrom(
            msg.sender,
            address(this),
            stablecoinTokenAmount
        );

        uint256 userClaimableTokens = claimableTokens();
        uint256 sCoinPrice = idoInfo.tokenPrice * userClaimableTokens;

        if(stablecoinTokenAmount > sCoinPrice){
            uint256 extraStablecoins = stablecoinTokenAmount - sCoinPrice;
            IStableCoin(tokens.stablecoin).safeTransfer(
                msg.sender,
                extraStablecoins
            );
            stablecoinTokenAmount = sCoinPrice;
        } 

        totalStablecoinsReceived += stablecoinTokenAmount;
        participants[msg.sender].isStableCoinLocked = true;
        participants[msg.sender].stablecoinsLockedTime = block.timestamp;

        IMemeToken(tokens.memeToken).burn(participants[msg.sender].memeTokensSent);


        emit StableCoinLocked(
            msg.sender,
            block.timestamp,
            stablecoinTokenAmount
        );
    }

    /**
     * @notice claimableTokens is used to calculate the claimable IDO tokens
     * @dev This function can only be called when the IDO is in CLAIMABLE state
     * @dev This function can only be called after the claimable time
     */
    function claimableTokens() public view checkIDOState returns (uint256) {
        require(
            participants[msg.sender].isStableCoinLocked,
            "zoopadIDO: Stablecoins not locked"
        );

        uint256 participantClaimableIDOtokens = 0;
        uint256 allocatedToken = allocatedTokens();
        uint256 claimTimeLimit = 72 hours;
        uint256 lastBuyTimestamp = participants[msg.sender]
            .stablecoinsLockedTime; //@audit need review

