            marketCap < idoInfo.mcapCheckpoints[3]
        ) {
            participantClaimableIDOtokens += (allocatedToken * 60) / 100; // 60% of allocatedToken to be distributed
        } else if (marketCap >= idoInfo.mcapCheckpoints[3]) {
            participantClaimableIDOtokens += (allocatedToken * 80) / 100; // 80% of allocatedToken to be distributed
        }

        // Check if the time limit for claiming tokens has passed
        if (block.timestamp >= (lastBuyTimestamp + claimTimeLimit)) {
            participantClaimableIDOtokens += (allocatedToken * 20) / 100; // 20% of allocatedToken to be distributed
        }

        return participantClaimableIDOtokens;
    }

    /**
     * @notice allocatedTokens is used to calculate the allocated tokens
     */
    function allocatedTokens() public view checkIDOState returns (uint256) {
        require(
            participants[msg.sender].memeTokensSent > 0,
            "zoopadIDO: User has not participated in the IDO"
        );

        //(tokensForSale * memeTokensSent) / totalMemeTokensSent
        uint256 tokenForSale = (idoInfo.idoTokenPercent * idoInfo.tokenTotalSupply) / 100;
        uint256 participantClaimableIDOtokens = (tokenForSale *
            participants[msg.sender].memeTokensSent) /
            totalMemeTokensSent;

        return participantClaimableIDOtokens;
    }

    /**
     * @notice getMcap is used to calculate the market cap
     */
    function getMcap() public view returns (uint256) {      //@audit 
        //can use external price oracle for this func @audit need to fix
        return IERC20(tokens.idoToken).totalSupply() * idoInfo.tokenPrice;
    }

    /**
     * @notice claimIDOtokens is used to claim the IDO tokens
     * @dev This function can only be called when the IDO is in CLAIMABLE state
     */
    function claimTokens() external nonReentrant {
        require(
            participants[msg.sender].memeTokensSent > 0,
            "zoopadIDO: You do not have any meme tokens"
        );
        require(
            participants[msg.sender].isStableCoinLocked,
            "zoopadIDO: Stablecoin not locked"
        );

        require(
            block.timestamp >= idoTimeInfo.claimableTime,
            "Claimable time has not passed yet"
        );

        uint256 claimableTokenAmount = claimableTokens();

        participants[msg.sender].totalClaimableIDOTokens = allocatedTokens();

        require(
            participants[msg.sender].totalClaimableIDOTokens > 0,
            "zoopadIDO: You do not have any IDO tokens to claim"
        );

        participants[msg.sender].totalClaimableIDOTokens -= claimableTokenAmount;

        require(claimableTokenAmount > 0, "No IDO tokens to claim");
            IERC20(tokens.idoToken).safeTransfer(msg.sender, claimableTokenAmount);
        tokensLeft = tokensLeft - claimableTokenAmount; // Reduce the tokensForSale by the amount claimed

        emit IDOTokensClaimed(msg.sender, claimableTokenAmount);
    }

    /**
     * @notice refundMemeToken is used to refund the meme tokens
     * @param _roots is the root of the merkle tree
     * @param proof is the proof of the merkle tree
     */
    function refundMemeToken(
        bytes32 _roots,
        bytes32[] calldata proof
    ) external {
        require(
            participants[msg.sender].memeTokensSent > 0,
            "zoopadIDOToken: User has not participated in the IDO"
        );

        require(
            block.timestamp >= idoTimeInfo.idoEndTime,
            "zoopadIDOToken: IDO has not ended yet"
        );

        require(
            totalMemeTokensSent < idoInfo.minimumMemeTokens,
            "zoopadIDOToken: Minimum meme token reached"
        );

        bytes32 _leaf = keccak256(abi.encodePacked(msg.sender));

        require(
            MerkleProof.verify(proof, _roots, _leaf),
            "zoopadIDOToken: Invalid proof"
        );

        require(
            IMemeToken(tokens.memeToken).transfer(
                msg.sender,
                participants[msg.sender].memeTokensSent
            ), //@audit It must be store offchain if have multipla part
            "zoopadIDOToken: Token transfer failed"
        );

        participants[msg.sender].memeTokensSent = 0;
        totalMemeTokensSent =
            totalMemeTokensSent -
            participants[msg.sender].memeTokensSent;
    }

    /**
     * @notice setBuyCount is used to set the buy count
     * @param _buyCount is the number of times the user wants to buy
     */
    function setBuyCount(uint256 _buyCount) external onlyIdoOwner {
        buyCount = _buyCount;
    }

    /**
     * @notice updateMinMemeTokens is used to update the minimum meme tokens
     * @param _minMemeTokens is the minimum meme tokens
     */
    function updateMinMemeTokens(uint256 _minMemeTokens) public onlyIdoOwner {
        idoInfo.minimumMemeTokens = _minMemeTokens;
    }

    /**
     * @notice getClaimableTime is used to get the claimable time
     * @return claimableTime is the claimable time
     */
    function getClaimableTime() public view returns (uint256) {
        return idoTimeInfo.claimableTime;
    }

    /**
     * @notice setClaimableTime is used to set the claimable time
     * @param _claimableTime is the claimable time
     * @dev This function can only be called by the owner
     */
    function updateClaimableTime(uint256 _claimableTime) public onlyIdoOwner {
        idoTimeInfo.claimableTime = _claimableTime;
    }


    /**
     * @notice withdrawStablecoins is used to withdraw the stablecoins
     */
    function withdrawStablecoins() public onlyIdoOwner {
        require(
            IMemeToken(tokens.stablecoin).transfer(
                msg.sender,
                IMemeToken(tokens.stablecoin).balanceOf(address(this))
            ),
            "Token transfer failed"
        );
    }

    function getIdoOwner() public view returns (address) {
        return idoInfo.idoOwner;
    }

    function getIdoStatus() public view returns (zoopadIDOLib.IDOState) {
        return idoState;
    }
}

