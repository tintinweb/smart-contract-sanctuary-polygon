//deboershapes


pragma solidity ^0.8.12;

contract shapesboy_games is ERC1155, Ownable {
    //counter for setting next game_id
    uint256 public nextgameid=1;
    
    //shapeboy fees
    uint256 public shapeboy_mint_fee=0;  //shapesboy add on fee to each game mint
    uint256 public shapeboy_publish_fee=0;  //fee to publish game under this contract
    
    //game supply control mapping varaibales
    mapping(uint256=>uint256) public gameid2supply;
    mapping(uint256=>uint256) public gameid2minted;
    mapping(uint256=>uint256) public gameid2mintprice;
    
    //game info data
    mapping(uint256=>string) public gameid2gamename;
    mapping(uint256=>address) public gameid2publisher;
    mapping(uint256=>string) public gameid2gameuri;
    mapping(uint256=>string) public gameid2gamelocation;
    mapping(string=>bool) public gamename2taken;
    mapping(string=>uint256) public gamename2gameid;

    //contract uri
    string public contract_uri="";
    
    //balances
    uint256 public shapesboy_balance=0;
    uint256 public publishers_balance=0;
    mapping(uint256=>uint256) public gameid2balance;

    //store data
    uint256 public store_version=0;
    string public store_download_location="";

    
    //uri settings
    //initial uri setting
    constructor() ERC1155("https://deboershapes.com/shapesboy/gamenftdata/shapesboy.json") {}
    
    //uri override function
    function uri(uint256 tokenId) override public view returns (string memory) {
        return string(gameid2gameuri[tokenId]);
    }
    
    //contract uri
    function contractURI() public view returns (string memory) {
        return contract_uri;
    }
    //update contract uri function
    function update_contract_uri(string memory _new_contract_uri) public onlyOwner{
        //update uri
        contract_uri=_new_contract_uri;
    }

    //publish game functions
    function publish_game(string memory _game_name, uint256 _supply, string memory _game_location, string memory _game_uri, uint256 _mint_fee) public payable{
        //chech for publishing fee
        require(msg.value>=shapeboy_publish_fee, "Value sent did not cover publishing fee");
        //require orginal game name
        require(gamename2taken[_game_name]==false, "game name already exist");
        //shapeboy accounting
        shapesboy_balance=shapesboy_balance +msg.value;

        //set supply 
        gameid2supply[nextgameid]=_supply;
        //set mint price
        gameid2mintprice[nextgameid]=_mint_fee;
        
        //set game info
        gameid2gamename[nextgameid]=_game_name;
        gameid2publisher[nextgameid]=msg.sender;
        gameid2gameuri[nextgameid]=_game_uri;
        gameid2gamelocation[nextgameid]=_game_location;
        gamename2gameid[_game_name]=nextgameid;
        gamename2taken[_game_name]=true;
        
        //manage game id
        nextgameid=nextgameid+1;
    }
    
    //update game uri function
    function update_game_uri(uint256 _game_id,  string memory _new_game_uri) public{
        //require publisher of game
        require(gameid2publisher[_game_id]==msg.sender, "Not sent by Publisher");

        //update uri
        gameid2gameuri[_game_id]=_new_game_uri;
    }
    
    //update game location
    function update_game_location(uint256 _game_id,  string memory _new_game_location) public{
        //require publisher of game
        require(gameid2publisher[_game_id]==msg.sender, "Not sent by Publisher");

        //update location
        gameid2gamelocation[_game_id]=_new_game_location;
    }
    
    //update game mint fee function
    function update_mintfee(uint256 _game_id, uint256 _new_mint_fee) public{
        //only the game publisher can change mint price
        require(gameid2publisher[_game_id]==msg.sender , "Not sent by Publisher");
        //update mint fee
        gameid2mintprice[nextgameid]=_new_mint_fee;
    }
    
    //update game publisher (game ownership transfer)
    function game_publisher_transfer(uint256 _game_id, address _new_publisher) public{
        //require current game publisher
        require(gameid2publisher[_game_id]==msg.sender, "Not sent by Publisher");
        //set new publisher
        gameid2publisher[_game_id]=_new_publisher;
    }
    
    //minting function (limited to one mint at a time)
    function mint(uint256 _game_id) public payable{
        //require _game_id to exist
        require(_game_id<nextgameid, "game id doesn't exist");
        require(_game_id >= 0, "game id doesn't exist");

        //require enough supply
        require(gameid2minted[_game_id]<gameid2supply[_game_id], "No more copies");
        //require enough funds
        require(msg.value>=gameid2mintprice[_game_id]+shapeboy_mint_fee, "Not enough funds to cover fee");

        //execute mint
        _mint(msg.sender, _game_id, 1, "");
        gameid2minted[_game_id]=gameid2minted[_game_id]+1;

        //accounting
        shapesboy_balance=shapesboy_balance+msg.value-gameid2mintprice[_game_id];
        publishers_balance=publishers_balance+gameid2mintprice[_game_id];
        gameid2balance[_game_id]=gameid2balance[_game_id]+gameid2mintprice[_game_id];
    }
    
    //withdrawal functions
    //game_id withdrawal functions
    function gameid_withdrawal(uint256 _game_id) public{
        //require publisher of game
        require(gameid2publisher[_game_id]==msg.sender, "Not sent by Publisher");
        // require funds are available
        require(address(this).balance>=gameid2balance[_game_id], "Funds not Available");

        //Send the value to the address
        payable(gameid2publisher[_game_id]).transfer(gameid2balance[_game_id]);

        //accounting
        gameid2balance[_game_id]=0;
    }
    
    //shapesboy withdrawal functions
    function shapesboy_withdrawal() public onlyOwner {
        // require funds are available
        require(address(this).balance>=shapesboy_balance, "Funds not Available");
        //tramsfer funds
        payable(owner()).transfer(shapesboy_balance);

        //accounting
        shapesboy_balance=0;
    }
    
    //extrafunds withdrawal functions
    function extrafunds_withdrawal() public onlyOwner {
        //require extra funds
        uint256 _funds_due=shapesboy_balance+publishers_balance;
        require(address(this).balance>_funds_due, "No Extra Funds");
        uint256 _extra_funds=address(this).balance-_funds_due;
        //tramsfer funds
        payable(owner()).transfer(_extra_funds);
    }
    
    //update shapeboy fees function
    function update_shapeboy_mint_fee(uint256 _new_mint_fee) public onlyOwner {
        shapeboy_mint_fee=_new_mint_fee;
    }
    
    function update_shapeboy_publish_fee(uint256 _new_publish_fee) public onlyOwner {
        shapeboy_publish_fee=_new_publish_fee;
    }
    
    //game hide control
    mapping(uint256=>bool) public gameid2hide;
    function hide_game(uint256 _game_id) public onlyOwner {
        gameid2hide[_game_id]=true;
    }
    function unhide_game(uint256 _game_id) public onlyOwner {
        gameid2hide[_game_id]=false;
    }
    
    //read functions
    //game ids
    function games_ids() public view returns(uint[] memory _game_ids){
        _game_ids=new uint[](nextgameid-1);
        for(uint i=1; i<nextgameid; i++) {
            _game_ids[i-1]=i;
        }
    }
    
    //game names
    function games_names() public view returns(string[] memory _game_names){
        _game_names=new string[](nextgameid-1);
        for(uint i=1; i<nextgameid; i++) {
            _game_names[i-1]=gameid2gamename[i];
        }
    }
    
    //game publishers
    function games_publishers() public view returns(address[] memory _game_publishers){
        _game_publishers=new address[](nextgameid-1);
        for(uint i=1; i<nextgameid; i++) {
            _game_publishers[i-1]=gameid2publisher[i];
        }
    }
    
    //publisher's game id list
    function publishers_game_list(address _publisher) public view returns(uint256[] memory _publisher_game_ids){
        //count number of games publisher has
        uint256 _publisher_game_count=0;
        for(uint i=1; i<nextgameid; i++) {
            if(gameid2publisher[i]==_publisher){
                _publisher_game_count=_publisher_game_count+1;
            }
        }
        //create array to hold publisher game ids
        _publisher_game_ids=new uint[](_publisher_game_count);
        //grab publisher game ids
        uint256 _publisher_games_grabbed=0;
        uint256 _game_index=0;
        while (_publisher_games_grabbed<_publisher_game_count){
            if(gameid2publisher[_game_index]==_publisher){
                _publisher_game_ids[_publisher_games_grabbed]=_game_index;
                _publisher_games_grabbed=_publisher_games_grabbed+1;
            }
            _game_index=_game_index+1;
            if(_game_index==nextgameid){
                break;
            }
        }
    }

    //user's game id list
    function users_game_list(address _user) public view returns(uint256[] memory _owned_game_ids){
        //count number of games user owns
        uint256 _owned_game_count=0;
        for(uint i=1; i<nextgameid; i++) {
            if(balanceOf(_user, i)>0){
                _owned_game_count=_owned_game_count+1;
            }
        }
        //create array to hold user owned game ids
        _owned_game_ids=new uint[](_owned_game_count);
        //grab publisher game ids
        uint256 _owned_games_grabbed=0;
        uint256 _game_index=0;
        while (_owned_games_grabbed<_owned_game_count){
            if(balanceOf(_user, _game_index)>0){
                _owned_game_ids[_owned_games_grabbed]=_game_index;
                _owned_games_grabbed=_owned_games_grabbed+1;
            }
            _game_index=_game_index+1;
            if(_game_index==nextgameid){
                break;
            }
        }
    }
    
    //games hidden
    function games_hidden() public view returns(bool[] memory _game_hidden){
        _game_hidden=new bool[](nextgameid-1);
        for(uint i=1; i<nextgameid; i++) {
            _game_hidden[i-1]=gameid2hide[i];
        }
    }
    
    //extra funds
    function extra_funds() public onlyOwner view returns(uint _extra_funds){
        uint256 _funds_due=shapesboy_balance+publishers_balance;
        _extra_funds=address(this).balance-_funds_due;
    }

    //update store functions
    function update_store_version() public onlyOwner {
        store_version=store_version+1;
    }
    function update_store_download_location(string memory _new_location) public onlyOwner {
        store_download_location=_new_location;
    }
    
    //fallback functions
    fallback() external payable{}
    receive() external payable{}
    
}

