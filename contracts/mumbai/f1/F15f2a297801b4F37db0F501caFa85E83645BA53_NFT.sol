// SPDX-License-Identifier: MIT
pragma solidity ^0.8.11;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract NFT is ERC721, ERC721Enumerable, ERC721URIStorage, Ownable, AccessControlEnumerable {
    using SafeMath for uint256;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    
    string public baseTokenURI;
    uint public mintPrice;

    struct SaleItem {
        address seller;
        uint256 price;
    }
    
    mapping(address => uint256[]) public tokenIdOf;
    mapping(uint256 => uint256) public tokenPrices;
    mapping(uint256 => SaleItem) public saleItems;


    constructor(string memory name, string memory symbol) ERC721(name, symbol) {
        _tokenIds.increment();
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId, uint256 batchSize)
        internal
        override(ERC721, ERC721Enumerable)
    {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function mint(
        uint256 _tokenURI
    ) public payable {
        require(msg.value <= mintPrice, "Not enough");

        (bool success,) = payable(address(this)).call{value: mintPrice}("");
        require(success, "Matic transfer failed");

        _safeMint(msg.sender, _tokenIds.current());
        _setTokenURI(_tokenIds.current(),  string(abi.encodePacked(baseTokenURI, "/", Strings.toString(_tokenURI))));
        addTokenIdTo(msg.sender, _tokenIds.current());
        _tokenIds.increment();
    }

    function addTokenIdTo(address _to, uint256 _tokenId) internal {
        tokenIdOf[_to].push(_tokenId);
        tokenPrices[_tokenId] = mintPrice;
    }

    function removeTokenIdFrom(address from, uint256 _tokenId) internal {
        tokenIdOf[from][_tokenId] = tokenIdOf[from][tokenIdOf[from].length - 1];
        tokenIdOf[from].pop();
    }

    function setBaseURI(string memory _baseURI) external onlyOwner {
        baseTokenURI = _baseURI;
    }

    function setMintPrice(uint256 _price) external onlyOwner {
        mintPrice = _price.mul(10**18);
    }

    function setTokenPrice(uint256 tokenId, uint256 _price) external {
        bool exists = false;

        for (uint256 i = 0; i < tokenIdOf[msg.sender].length; i++) {
            if (tokenIdOf[msg.sender][i] == tokenId) {
                exists = true;
            }
        }

        require(exists, "Not Owner");

        if (tokenPrices[tokenId] < _price) {
            uint256 requiredPrice = _price - tokenPrices[tokenId];

            (bool success,) = payable(address(this)).call{value: requiredPrice}("");
            require(success, "Matic transfer failed");

            tokenPrices[tokenId] = _price;
        } else if (tokenPrices[tokenId] > _price) {
            uint256 returnPrice = tokenPrices[tokenId] - _price;
            payable(msg.sender).transfer(returnPrice);
        }
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return
            string(abi.encodePacked(baseTokenURI, "/", Strings.toString(tokenId)));
    }

    function updateTokenURI(
        uint256 tokenId,
        string memory uri
    ) public {
        _setTokenURI(tokenId, uri);
    }

    function getTotalTokenCount() external view returns (uint256) {
        return _tokenIds.current() - 1;
    }

    function putNFTForSale(uint256 _tokenId, uint256 _price) external {
        require(ownerOf(_tokenId) == msg.sender, "You are not the owner of this NFT.");
        require(_price > 0, "Price must be greater than 0.");

        saleItems[_tokenId] = SaleItem({
            seller: msg.sender,
            price: _price
        });
    }

    function buyNFT(uint256 _itemId) external payable {
        SaleItem memory item = saleItems[_itemId];

        require(msg.value >= item.price, "Not enough funds sent.");
        require(ownerOf(_itemId) == item.seller, "NFT is no longer owned by the seller.");

        payable(item.seller).transfer(msg.value);
        tokenPrices[_itemId] = item.price;
        _transfer(item.seller, msg.sender, _itemId);
    }

    function _transfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override {
        require(from == address(0), "You can't transfer this NFT.");

        super._transfer(from, to, tokenId);
    }

    function _burn(
        uint256 tokenId
    ) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    function supportsInterface(
        bytes4 interfaceId
    ) public view override(ERC721, ERC721Enumerable, ERC721URIStorage, AccessControlEnumerable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}