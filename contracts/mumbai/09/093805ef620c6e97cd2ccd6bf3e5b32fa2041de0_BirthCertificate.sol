contract BirthCertificate is ERC721URIStorage, Ownable {
  using Strings for uint256;
  using Counters for Counters.Counter;
  Counters.Counter private _tokenIds;

  function diffYears(uint fromTimestamp, uint toTimestamp) public pure returns (uint _years) {
    _years = BokkyPooBahsDateTimeLibrary.diffYears(fromTimestamp, toTimestamp);
  }
  // function for human readable date (WIP)
  // function _daysToDate(uint _days) public pure returns (uint year, uint month, uint day) {
  //   return BokkyPooBahsDateTimeLibrary._daysToDate(_days);
  // }

  struct Registro {
    string name;
    string country;
    string city;
    string hash;
  }

    mapping (uint256 => Registro) public tokenIdtoRegistro;
  mapping (address => uint256) public addresstoTokenId;

  constructor() ERC721("NFT Collection", "NFTC") {}

// on chain NFT Certificate
  function generateRegistro(uint256 tokenId) public view returns(string memory) {
    require(_exists(tokenId), "tokenId not exist!");
    bytes memory svg = abi.encodePacked(
      '<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 350 350">',
      '<style>.base { fill: white; font-family: serif; font-size: 14px; }</style>',
      '<rect width="100%" height="100%" fill="black" />',
      '<text x="50%" y="30%" class="base" dominant-baseline="middle" text-anchor="middle">',"Name: ",tokenIdtoRegistro[tokenId].name,'</text>',
      '<text x="50%" y="50%" class="base" dominant-baseline="middle" text-anchor="middle">', "Country: ",tokenIdtoRegistro[tokenId].country,'</text>',
      '<text x="50%" y="60%" class="base" dominant-baseline="middle" text-anchor="middle">', "City: ",tokenIdtoRegistro[tokenId].city,'</text>',
      '<text x="50%" y="60%" class="base" dominant-baseline="middle" text-anchor="middle">', "Hash: ",tokenIdtoRegistro[tokenId].hash,'</text>',
      '</svg>'
    );

    return string(
      abi.encodePacked(
        "data:image/svg+xml;base64,",
        Base64.encode(svg)
      )
    );
  }

  

  // token URI
  function getTokenURI(uint256 tokenId) public view returns (string memory){
    bytes memory dataURI = abi.encodePacked(
        '{',
            '"name": "NFT Collection', tokenId.toString(), '",',
            '"description": "NFT Collection",',
            '"image": "', generateRegistro(tokenId), '"',
        '}'
    );
    return string(
        abi.encodePacked(
            "data:application/json;base64,",
            Base64.encode(dataURI)
        )
    );
  }

  function mint(address _address, string memory _name, string memory _country, string memory _city, string memory _hash) public onlyOwner {
    
    uint256 newTokenId = _tokenIds.current();
    _safeMint(_address, newTokenId);
    
    tokenIdtoRegistro[newTokenId] = Registro(_name, _country, _city, _hash);
    addresstoTokenId[_address] = newTokenId;
    _setTokenURI(newTokenId, getTokenURI(newTokenId));
    _tokenIds.increment();
  }


}