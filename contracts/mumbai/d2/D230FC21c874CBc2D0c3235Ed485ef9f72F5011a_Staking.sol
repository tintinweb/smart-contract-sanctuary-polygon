pragma solidity ^0.8.9;
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";

contract Staking is ReentrancyGuard, Ownable {
    // staking threshold
    uint256 public constant maximumStake = 300000 * 10 ** 18;
    uint256 public constant minimumStake = 100 * 10 ** 18;
    uint256 public constant periodLength = 10 seconds;
    uint256 public constant lockLength = 20 seconds;

    // address of staking token
    address stakingToken;

    // total user currently staking in platform
    uint256 totalUsersStaking;

    // total staked amount in our platform
    uint256 totalPlatformStakedAmount;

    // total rewarded amount in our platform
    uint256 totalPlatformRewardedAmount;

    // staking days => profit rate
    // 30 days => 12%
    mapping(uint256 => uint256) stakingRates;

    // user address => staking positions
    mapping(address => Position[]) stakingPositions;

    struct Position {
        uint256 positionId;
        uint256 period;
        uint256 stakedAmount;
        uint256 timeStart;
        uint256 timeEnd;
        uint256 lastTimeReward;
        uint256 totalReward; // new field to store total reward
    }

    event Staked(address indexed user, uint256 stakedAmount);
    event Harvested(address indexed user, uint256 amount, uint256 positionId);
    event Withdrawn(address indexed user, uint256 amount, uint256 positionId);

    constructor(address _stakingTokenAddress) {
        require(
            address(_stakingTokenAddress) != address(0),
            "Staking and reward token addresses must be non-zero"
        );

        // pour reward to our platform

        stakingToken = _stakingTokenAddress;

        // Initialize staking rates for predefined periods.
        stakingRates[30] = 12; // 30 days = 12%
        stakingRates[60] = 25; // 60 days = 25%
        stakingRates[90] = 40; // 90 days = 40%
        stakingRates[180] = 80; // 180 days = 80%
        stakingRates[365] = 162; // 365 days = 162%
    }

    // Set the staking rate for a specific period. Only callable by the contract owner.
    function setStakingRate(
        uint256 _stakingPeriod,
        uint256 _rate
    ) external onlyOwner {
        require(
            _stakingPeriod > 0,
            "Staking period must be greater than 0 days"
        );
        require(_rate > 0, "Staking rate must be greater than 0%");
        stakingRates[_stakingPeriod] = _rate;
    }

    function roundUp(uint256 value) public pure returns (uint256) {
        uint256 decimalPlaces = 18; // Assuming 18 decimal places
        uint256 factor = 10 ** decimalPlaces;
        uint256 intValue = value / factor; // 39999999999999999996 / 10^18 = 40
        uint256 fractionalPart = value % factor; // 99999999999999

        if (fractionalPart > 0) {
            intValue += 1;
        }

        return intValue * factor;
    }

    // Returns the count of staking positions for the user at the specified address.
    // total Position of user
    function getPositionsCount(
        address _userAddress
    ) public view returns (uint256) {
        return stakingPositions[_userAddress].length;
    }

    // Returns the staking position at the specified index for the user at the specified address.
    function getPositionsByIndex(
        address _userAddress,
        uint256 _positionIndex
    ) public view returns (Position memory) {
        return stakingPositions[_userAddress][_positionIndex];
    }

    // Allows a user to deposit tokens and create a new staking position.
    function deposit(uint256 _amount, uint256 _period) external nonReentrant {
        // Ensure the staking amount is within the allowed range
        require(
            minimumStake <= _amount && _amount <= maximumStake,
            "Staking amount must be within the allowed range"
        );
        // Ensure the user has enough balance of the staking token
        require(
            IERC20(stakingToken).balanceOf(msg.sender) >= _amount,
            "Insufficient staking token balance"
        );
        // Ensure the chosen staking period is valid
        require(stakingRates[_period] != 0, "Invalid staking period");

        // Convert the period (in days) to seconds
        uint256 stakingPeriod = _period * 1 seconds;

        // Get the current number of staking positions for the user
        uint256 currentPositionCount = getPositionsCount(msg.sender);

        // If this is the user's first staking position, increment the total user staking count
        if (currentPositionCount == 0) {
            totalUsersStaking++;
        }

        // Create a new staking position for the user
        Position memory newPosition = Position({
            positionId: currentPositionCount,
            period: _period,
            stakedAmount: _amount,
            timeStart: block.timestamp,
            timeEnd: block.timestamp + stakingPeriod,
            lastTimeReward: block.timestamp,
            totalReward: 0
        });

        // Add the new position to the user's staking positions
        // push newPosition to mapping
        stakingPositions[msg.sender].push(newPosition);

        // Update the total staked amount on the platform
        totalPlatformStakedAmount += _amount;

        // Transfer the staking tokens from the user to the contract
        IERC20(stakingToken).transferFrom(msg.sender, address(this), _amount);

        // Emit a Staked event
        emit Staked(msg.sender, _amount);
    }

    // Calculates the reward and updates the position's last time reward.
    // Returns the total reward for the given position.
    function _calculateRewardAndUpdate(
        Position storage _position
    ) internal returns (uint256) {
        // Variables to store time since last harvest and remaining days
        uint256 timeSinceLastHarvest; // last time user can harvested
        uint256 remainingDays = 0; // for case 365, remaining 5 days
        uint256 rewardRemainingDays = 0;

        // Calculate the time since the last harvest and remaining days based on the position's timeEnd
        if (block.timestamp >= _position.timeEnd) {
            timeSinceLastHarvest = _position.timeEnd - _position.lastTimeReward; // timeEnd - depositTime
            remainingDays = _position.period % 10;
        } else {
            timeSinceLastHarvest =
                block.timestamp -
                _position.lastTimeReward -
                lockLength;
            remainingDays = 0;
        }

        console.log("remaining day case 2", remainingDays);
        console.log("Time since last harvest:", timeSinceLastHarvest);

        // Calculate the profit period (rounding down)
        uint256 profitPeriod = timeSinceLastHarvest / periodLength; /// Note ❌ 10 days each period (đợt lãi ) - mục đích để tính số đợt lãi user có thể nhận
        console.log("profitPeriod", profitPeriod);

        // Calculate the total number of periods and the staking rate for the position's period
        uint256 totalPeriod = _position.period / periodLength;
        console.log("Total Period", totalPeriod);
        uint256 periodRate = stakingRates[_position.period];
        console.log("Period Rate", periodRate);
        // Calculate the reward based on the position's staked amount and the staking rate
        // multiply with 100 in order to avoid decimals numbe
        console.log("position.period value", _position.period);
        // uint256 rewardBaseOnPeriod = ((
        //     ((((_position.stakedAmount * periodRate))))
        // ) / (totalPeriod * 100 + (_position.period % 10) * 10));
        uint256 rewardBaseOnPeriod = ((_position.stakedAmount * periodRate) /
            100) / (totalPeriod * 10);

        console.log("Reward for each Period", rewardBaseOnPeriod);
        // Calculate the total reward for the profit period
        uint256 totalReward = profitPeriod * rewardBaseOnPeriod;
        console.log("total reward");
        console.log(totalReward);
        // If there are remaining days, calculate the reward for the remaining days
        // if (remainingDays > 0) {
        //     rewardRemainingDays = (rewardBaseOnPeriod * remainingDays) / 10;
        //     totalReward += rewardRemainingDays;
        //     console.log("Reward for remaining days", rewardRemainingDays);
        // }

        // Update the last time the position was rewarded
        console.log("last time reward before", _position.lastTimeReward);

        _position.lastTimeReward +=
            (periodLength * profitPeriod) +
            remainingDays *
            1 seconds;

        console.log("last time reward after", _position.lastTimeReward);
        console.log("Total reward on calc function", totalReward);
        // Return the total reward for the given position
        return (totalReward);
    }

    /*
        For example : User deposit 1000 and  register for pack 60 days with 25% for profit rate
        User require to claim at day 45
        We got timeSinceLastHarvest less than time End (45 < 60 )
        timeSinceLastHarvest = block.timeStamp (45) - lastTimeReward (0) - lockLengh(20) = 25
        profitPeriod = 25 / 10 = 2 (profit period)
        get total profit period in packed = period / 10 = 60 / 10 = 6
        period Rate = 25%
        calculate reward based on each period 
            amount staked * rate / total Period
            1000 * 0.25 / 6
        Total reward user received = profit Period (2) * rewardBasedOnEachPeriod (41,6777777)
                    = 83.33333333333
    */

    // Allows a user to harvest their rewards from a staking position.
    function harvest(
        uint256 _positionIndex
    ) external nonReentrant returns (uint256) {
        // Ensure the position index is within the bounds of the staking positions array
        require(
            _positionIndex < stakingPositions[msg.sender].length,
            "Staking position index out of bounds"
        );

        // Get the position to be harvested
        Position storage harvestPosition = stakingPositions[msg.sender][
            _positionIndex
        ];

        // Calculate the time passed since the position was created
        uint256 timeRequest = block.timestamp - harvestPosition.timeStart;
        // Ensure that at least 30 days have passed before allowing the harvest
        require(
            timeRequest >= 30 seconds,
            "Harvest not allowed before 30 days of staking"
        );

        uint256 timeSinceLastHarvest = harvestPosition.timeEnd -
            harvestPosition.lastTimeReward;
        require(timeSinceLastHarvest != 0, "Already harvested all reward!");

        uint256 totalReward = _calculateRewardAndUpdate(harvestPosition);
        harvestPosition.totalReward += totalReward; // update totalReward in struct
        console.log("Total reward for harvesttttttttttttttttttt", totalReward);
        totalPlatformRewardedAmount -= totalReward;

        require(
            totalPlatformRewardedAmount > 0,
            "Reward remain in our platform is not available"
        );

        emit Harvested(msg.sender, totalReward, _positionIndex);
        // console.log(totalReward);
        IERC20(stakingToken).transfer(msg.sender, totalReward);
        return totalReward;
    }

    // Allows a user to withdraw their staking position and any rewards earned.
    function withdraw(
        uint256 _positionIndex
    ) external nonReentrant returns (uint256) {
        // Ensure the position index is within the bounds of the staking positions array for the user
        require(
            _positionIndex < stakingPositions[msg.sender].length,
            "Staking position index out of bounds"
        );

        // Get the staking position to withdraw
        Position storage withdrawPosition = stakingPositions[msg.sender][
            _positionIndex
        ];

        // Get the current number of staking positions for the user
        uint256 currentPositionCount = getPositionsCount(msg.sender);

        // If this is the user's last staking position, decrement the total user staking count
        if (currentPositionCount == 0) {
            totalUsersStaking--;
        }

        // Check if the user has staked for at least 30 days
        uint256 timeRequest = block.timestamp - withdrawPosition.timeStart;
        uint256 totalReward;
        if (timeRequest >= 30 seconds) {
            totalReward = _calculateRewardAndUpdate(withdrawPosition);
            // IERC20(stakingToken).transfer(msg.sender, totalReward);
            // If the platform out of funds
            if (totalReward > totalPlatformRewardedAmount) {
                totalReward = 0;
            } else {
                totalPlatformRewardedAmount -= totalReward;
            }
        }

        uint256 totalWithdraw = withdrawPosition.stakedAmount + totalReward;

        // Remove the staking position from the user's stakingPositions array
        // uint256 lastIndex = stakingPositions[msg.sender].length - 1;
        // stakingPositions[msg.sender][_positionIndex] = stakingPositions[
        //     msg.sender
        // ][lastIndex];
        // stakingPositions[msg.sender].pop();
        emit Withdrawn(msg.sender, totalWithdraw, _positionIndex);
        IERC20(stakingToken).transfer(msg.sender, totalWithdraw);
        return totalWithdraw;
    }

    function getTotalPlatformStakedAmount() public view returns (uint256) {
        return totalPlatformStakedAmount;
    }

    function getTotalUserStaking() public view returns (uint256) {
        return totalUsersStaking;
    }

    function getTotalRewardAmount() public view returns (uint256) {
        return totalPlatformRewardedAmount;
    }

    // contract owner use this function in order to pour token directly to our platform
    function pourTokens(uint256 amount) public onlyOwner returns (uint256) {
        require(amount > 0, "Amount must be greater than 0");
        // Assuming your token contract has a transferFrom function to transfer tokens from the owner's account
        require(
            IERC20(stakingToken).transferFrom(
                msg.sender,
                address(this),
                amount
            ),
            "Token transfer failed"
        );
        //IERC20(stakingToken).transferFrom(msg.sender, address(this), amount);
        totalPlatformRewardedAmount += amount;
        return totalPlatformRewardedAmount;
    }

