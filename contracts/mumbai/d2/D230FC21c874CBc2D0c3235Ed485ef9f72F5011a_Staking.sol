    function getUserStakingHistory(
        address _userAddress
    ) public view returns (Position[] memory) {
        return stakingPositions[_userAddress];
    }

    // used for update totalReward variables based on claimableReward
    function updateTotalReward(uint256 _positionIndex) external {
