pragma solidity ^0.8.9;

import "ERC1155.sol";
import "IERC721Receiver.sol";
import "IERC721.sol";
import "AccessControl.sol";
import "Strings.sol";
import "LinkTokenInterface.sol";
import "VRFCoordinatorV2Interface.sol";
import "VRFConsumerBaseV2.sol";

contract OpenBoxVRF is
    VRFConsumerBaseV2,
    ERC1155,
    AccessControl,
    IERC721Receiver
{
    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR");
    bytes32 public constant WITHDRAW_ROLE = keccak256("WITHDRAW");

    VRFCoordinatorV2Interface private COORDINATOR;
    LinkTokenInterface private LINKTOKEN;

    // Your subscription ID.
    uint64 private s_subscriptionId;

    bytes32 private s_keyHash;
    uint32 private s_callbackGasLimit = 400000;
    uint16 private s_requestConfirmations = 3;
    // uint32 private s_numWords = 1;

    // NFT name
    string public name;

    // NFT symbol
    string public symbol;

    uint256 public constant PRICE_PER_KEY = 0.1 ether;

    uint256 private nonce;

    // 1ï¼š box
    uint256 public constant GAMMA = 1;
    uint256 public constant SPECTRUM = 2;
    uint256 public constant MYSTERY = 3;
    uint256 public constant RAISE = 4;

    uint256 public constant POOL = 9999;

    mapping(uint256 => uint256) box_price_eth;
    mapping(uint256 => uint256) box_price_nft;

    mapping(uint256 => bool) box_buy_with_eth;
    mapping(uint256 => bool) box_buy_with_nft;

    // box => collection = > token_ids
    mapping(uint256 => mapping(address => uint256[])) box_nfts;
    // pool, gold
    mapping(uint256 => address[]) box_collections;
    // gamma 0.026%     260 1000000
    // SPECTRUM 0.036%  360 1000000
    // MYSTERY 0.04%    400 1000000
    // RAISE 0.02%     200 1000000
    mapping(uint256 => uint256) box_probability;

    // box price
    // 0.001
    // 5

    // airdrop key

    // 2: BoxKey
    uint256 public constant BOXKEY = 5;

    mapping(uint256 => bool) mintable;

    struct VRFRequest {
        address sender;
        uint256[] box_ids;
    }

    // VRF => mint address
    mapping(uint256 => VRFRequest) public requestToSender;

    event KeyPurchased(address indexed buyer, uint256 keyId);

    event BoxMint(address indexed owner, uint256 boxId, uint256 quantity);
    event BoxDestroyed(uint256 indexed boxId, address indexed owner);

    event RequestedRandomness(uint256 requestId);

    event ReturnedRandomness(uint256 indexed requestId, uint256[] randomWords);
    event OpenedBox(uint256 indexed box_id, address nft_addr, uint256 token_id);
    event Test(uint256[] indexed boxids);

    function GrantWithdrawRole(address account) external onlyAdmin {
        grantRole(WITHDRAW_ROLE, account);
    }

    function RevokeWithdrawOperator(address account) external onlyAdmin {
        revokeRole(WITHDRAW_ROLE, account);
    }

    function GrantOperatorRole(address account) external onlyAdmin {
        grantRole(OPERATOR_ROLE, account);
    }

    function RevokeOperator(address account) external onlyAdmin {
        revokeRole(OPERATOR_ROLE, account);
    }

    modifier onlyAdmin() {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "not admin");
        _;
    }

    modifier onlyWithdrawRole() {
        require(hasRole(WITHDRAW_ROLE, msg.sender), "not minter");
        _;
    }

    modifier onlyOperatorRole() {
        require(hasRole(OPERATOR_ROLE, msg.sender), "not operator");
        _;
    }

    constructor(
        string memory _name,
        string memory _symbol,
        string memory _uri,
        // chainLink
        uint64 subscriptionId,
        address vrfCoordinator,
        address link,
        bytes32 keyHash
    ) ERC1155(_uri) VRFConsumerBaseV2(vrfCoordinator) {
        __OpenBoxVRF_init_unchained(
            _name,
            _symbol,
            subscriptionId,
            vrfCoordinator,
            link,
            keyHash
        );
    }

    function __OpenBoxVRF_init_unchained(
        string memory _name,
        string memory _symbol,
        // chainLink
        uint64 subscriptionId,
        address vrfCoordinator,
        address link,
        bytes32 keyHash
    ) public {
        name = _name;
        symbol = _symbol;

        mintable[GAMMA] = true;
        mintable[SPECTRUM] = true;
        mintable[MYSTERY] = true;
        mintable[RAISE] = true;
        mintable[BOXKEY] = true;

        box_probability[GAMMA] = 260;
        box_probability[SPECTRUM] = 360;
        box_probability[MYSTERY] = 400;
        box_probability[RAISE] = 200;

        box_price_eth[GAMMA] = 0.001 ether;
        box_price_eth[SPECTRUM] = 0.001 ether;
        box_price_eth[MYSTERY] = 0.001 ether;
        box_price_eth[RAISE] = 0.001 ether;

        box_price_nft[GAMMA] = 5;

        box_buy_with_eth[GAMMA] = true;
        box_buy_with_eth[SPECTRUM] = true;
        box_buy_with_eth[MYSTERY] = true;
        box_buy_with_eth[RAISE] = true;

        box_buy_with_nft[GAMMA] = true;

        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
        LINKTOKEN = LinkTokenInterface(link);
        s_keyHash = keyHash;
        s_subscriptionId = subscriptionId;

        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(OPERATOR_ROLE, msg.sender);
        _setupRole(WITHDRAW_ROLE, msg.sender);
    }

    // Tokens can be transferred without authorization
    function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }

    function depositToken(
        uint256 box_id,
        address nft_addr,
        uint256[] calldata token_ids
    ) external onlyOperatorRole {
        uint256 len = token_ids.length;
        for (uint256 i; i < len; ) {
            IERC721(nft_addr).safeTransferFrom(
                msg.sender,
                address(this),
                token_ids[i],
                new bytes(0)
            );
            if (box_nfts[box_id][nft_addr].length == 0) {
                box_collections[box_id].push(nft_addr);
            }
            box_nfts[box_id][nft_addr].push(token_ids[i]);
            unchecked {
                ++i;
            }
        }
    }

    function buyBoxWithEth(uint256 box_id, uint256 quantity) external payable {
        require(box_buy_with_eth[box_id], "need eth");
        require(
            msg.value == box_price_eth[box_id] * quantity,
            "Insufficient funds"
        );
        _mint(msg.sender, box_id, quantity, new bytes(0));
        emit BoxMint(msg.sender, box_id, quantity);
    }

    function buyBoxWithNFT(
        uint256 box_id,
        uint256 quantity,
        address nft_addr,
        uint256[] calldata token_ids
    ) external {
        uint256 len = token_ids.length;
        require(box_buy_with_nft[box_id], "need nft");
        require(len == box_price_nft[box_id] * quantity, "Insufficient funds");
        for (uint256 i; i < len; ) {
            IERC721(nft_addr).safeTransferFrom(
                msg.sender,
                address(this),
                token_ids[i],
                new bytes(0)
            );
            if (box_nfts[box_id][nft_addr].length == 0) {
                box_collections[box_id].push(nft_addr);
            }
            box_nfts[POOL][nft_addr].push(token_ids[i]);
            unchecked {
                ++i;
            }
        }

        _mint(msg.sender, box_id, quantity, new bytes(0));
        emit BoxMint(msg.sender, box_id, quantity);
    }

    function enableOrDisable(uint256 id, bool s) external onlyOperatorRole {
        mintable[id] = s;
    }

    function setBaseURI(string calldata _uri) external onlyOperatorRole {
        _setURI(_uri);
    }

    function uri(uint256 id) public view override returns (string memory) {
        string memory baseUri = super.uri(0);
        return string(abi.encodePacked(baseUri, Strings.toString(id)));
    }

    function getRandomNumber(
        uint32 s_numWords
    ) internal returns (uint256 requestId) {
        // s_numWords
        requestId = COORDINATOR.requestRandomWords(
            s_keyHash,
            s_subscriptionId,
            s_requestConfirmations,
            s_callbackGasLimit,
            s_numWords
        );

        emit RequestedRandomness(requestId);
    }

    /**
     * @dev Opens a random box using the provided box ID and key ID.
     * Burns the box and key tokens. Requests randomness from Chainlink VRF
     * and stores the request ID and sender address in `requestToSender` mapping.
     *
     * Requirements:
     * - Caller must be the owner of the key token.
     *
     * @return _requestId The request ID generated for the Chainlink VRF request.
     */

    function openRandomBox(
        uint256[] calldata box_ids,
        uint32 quantity
    ) external returns (uint256 _requestId) {
        require(
            quantity == 1 || quantity == 10 || quantity == 100,
            "error quantity"
        );

        uint256 total_balances;
        for (uint256 i; i < box_ids.length; ) {
            unchecked {
                total_balances += balanceOf(msg.sender, box_ids[i]);
                ++i;
            }
        }

        require(balanceOf(msg.sender, BOXKEY) >= quantity, "Insufficient keys");
        require(total_balances >= quantity, "Insufficient boxs");

        // Burn the box and key tokens
        uint256 temp_quantity = quantity;
        for (uint256 i; i < box_ids.length; ) {
            uint256 box_balance = balanceOf(msg.sender, box_ids[i]);
            if (box_balance >= temp_quantity) {
                _burn(msg.sender, box_ids[i], temp_quantity);
                break;
            } else {
                temp_quantity = temp_quantity - box_balance;
                _burn(msg.sender, box_ids[i], box_balance);
            }
            unchecked {
                ++i;
            }
        }

        _burn(msg.sender, BOXKEY, quantity);

        // Request randomness from Chainlink VRF
        _requestId = getRandomNumber(quantity);
        requestToSender[_requestId] = VRFRequest(msg.sender, box_ids);

        return _requestId;
    }

    /**
     * @dev Callback function used by VRF Coordinator to return the random number.
     * Randomly selects an NFT from this contract's NFTs and assigns it to the user `sender`.
     */
    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory randomWords
    ) internal override {
        VRFRequest memory vrf_requesst = requestToSender[_requestId];
        address sender = vrf_requesst.sender;
        uint256[] memory box_ids = vrf_requesst.box_ids;
        uint256 remains = randomWords.length;
        uint256 skip;
        for (uint256 i; i < box_ids.length; ) {
            uint256 box_balance = balanceOf(sender, box_ids[i]);
            uint256 used;
            if (box_balance <= remains) {
                used = box_balance;
            } else {
                used = remains;
            }

            open(i, used, skip, box_ids, randomWords, sender);
            skip = skip + used;
            remains = remains - used;
            if (remains == 0) {
                break;
            }
            unchecked {
                ++i;
            }
        }
        emit Test(box_ids);

        emit ReturnedRandomness(_requestId, randomWords);
    }

    function open(
        uint256 i,
        uint256 used,
        uint256 skip,
        uint256[] memory box_ids,
        uint256[] memory randomWords,
        address sender
    ) internal {
        for (uint256 j; j < used; ) {
            uint256 collecton_rnd = uint256(
                keccak256(abi.encodePacked(nonce, sender, block.timestamp))
            );
            ++nonce;
            uint256 token_id_rnd = uint256(
                keccak256(abi.encodePacked(nonce, sender, block.timestamp))
            );
            ++nonce;
            uint256 op_id;
            if (box_probability[box_ids[i]] > randomWords[skip + j] % 1000000) {
                // é‡‘
                op_id = i;
            } else {
                // æ™®
                op_id = POOL;
            }

            uint256 collection_idx = collecton_rnd %
                box_collections[op_id].length;
            address collection_addr = box_collections[op_id][collection_idx];

            uint256 token_id_idx = token_id_rnd %
                box_nfts[op_id][collection_addr].length;
            uint256 token_id = box_nfts[op_id][collection_addr][token_id_idx];

            IERC721(collection_addr).safeTransferFrom(
                address(this),
                sender,
                token_id
            );
            delete box_nfts[op_id][collection_addr][token_id_idx];

            if (box_nfts[op_id][collection_addr].length == 0) {
                delete box_nfts[op_id][collection_addr];
                delete box_collections[op_id][collection_idx];
            }

            unchecked {
                ++j;
            }

            emit OpenedBox(op_id, collection_addr, token_id);
        }
    }

    function purchaseKey(uint quantity) external payable {
        require(mintable[BOXKEY], "Invalid ID");
        require(msg.value == PRICE_PER_KEY * quantity, "Insufficient funds");

        _mint(msg.sender, BOXKEY, quantity, new bytes(0));
        emit KeyPurchased(msg.sender, BOXKEY);
    }

    function withdrawETH(uint _amount) public onlyWithdrawRole {
        payable(msg.sender).transfer(_amount);
    }

    function withdrawToken(
        address _token,
        uint256 _token_id
    ) public onlyWithdrawRole {
        IERC721(_token).safeTransferFrom(
            address(this),
            msg.sender,
            _token_id,
            new bytes(0)
        );
    }

    function supportsInterface(
        bytes4 interfaceId
    ) public view override(AccessControl, ERC1155) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function test() external onlyAdmin {
        // Initialize 4 days worth of random words
        _mint(msg.sender, 1, 10, new bytes(0));
        _mint(msg.sender, 2, 10, new bytes(0));
        _mint(msg.sender, 3, 10, new bytes(0));
        _mint(msg.sender, 4, 10, new bytes(0));
        _mint(msg.sender, 5, 10, new bytes(0));

        uint i = 1;
        uint requestId = 200 + i;

        uint[] memory box_ids = new uint[](1);
        box_ids[0] = 1;

        requestToSender[requestId] = VRFRequest(msg.sender, box_ids);

        uint[] memory _randomWords = new uint[](1);
        _randomWords[0] = uint(
            blockhash(block.number - 4 + i) ^
                0x3632d8eba811d69784e6904a58de6e0ab55f32638189623b309895beaa6920c4
        );
        fulfillRandomWords(requestId, _randomWords);
    }
}

