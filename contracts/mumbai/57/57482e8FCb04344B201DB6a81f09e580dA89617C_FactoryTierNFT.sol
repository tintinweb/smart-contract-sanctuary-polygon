    mapping(address => uint256[]) private userFilmProposalIds;        // (studio => filmId list)
    mapping(address => uint256[]) private userApprovedFilmIds;        // (studio => filmId list)    

    uint256 public StudioPool; 
    uint256 public finalFilmCalledTime;

