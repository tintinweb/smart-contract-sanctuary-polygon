pragma solidity ^0.8.0;
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./interfaces/LottoFundInterface.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";
import "./Randomness.sol";

// This contract was written during an on-campus internship program for college students, and the design may not be perfect.
contract LottoFund is ERC20, Ownable, LottoFundInterface {
    using SafeMath for uint256;
    IERC20 public immutable token; // Record the address of ERC20 TOKEN
    Randomness internal randomness; // Random number contract
    uint256 public minParticipants; // Record the minimum number of participants
    address public yieldSource; // Record the contract address of the income source
    uint256 public bonusRate; // Winning rate, percentage of total participants
    uint256 public bonusAllocation; // Bonus allocation, percentage of profit used to issue bonuses
    uint256 public amountPerTicket; // Amount of locked balance needed to obtain each ticket
    uint256 public duration; // lotto cycle
    bool private isLiquidated; // Record whether it has been settled to safely close the project
    bool private isPollPassed; // Record whether the vote has passed to close the project
    uint256 public lockDuration; // Define the variable for the lock period, in seconds
    bool public isUseTestRandomNubmer; // Whether to use a given number to test the contract
    uint256 public bonusRound; // Record the latest round of lotto activities, that is, which round is currently in progress
    mapping(uint256 => uint256) private totalTickets; // Record the total number of lotto tickets for each round
    mapping(uint256 => uint256) private participantsNumber; // Record the number of participants in each round
    mapping(uint256 => address[]) participantAddr; // Record the user address that participated in order to draw lots
    mapping(uint256 => mapping(address => bool)) private isParticipated; // Record whether a user has participated in a certain round or not
    mapping(uint256 => mapping(address => uint256)) private numberOfTickets; // Record the number of lotto tickets for each user, queried by function
    mapping(uint256 => mapping(address => uint256)) private lockedShares; // Record the number of shares locked by each user in each round. Cannot be withdrawn or transferred during lotto period.
    mapping(uint256 => uint256) private totalLockedShares; // Total shares for each round.
    mapping(uint256 => uint256) private startTime; // Record the start time of each Lotto round.
    mapping(uint256 => uint256) private randomNumbers; // Record the random numbers obtained for each round.
    mapping(uint256 => address[]) private winners; // Record winners for each round.
    mapping(uint256 => uint256) private profits; // Record profits for each round.
    enum Stages {
        PROJECT_START,
        DEPOSITE_OPEN,
        DEPOSITE_CLOSED,
        LOTTO_OPEN,
        LOTTO_CLOSED,
        LOCK_OPEN,
        LOCK_CLOSED,
        PROJECT_END
    }
    Stages private depositStage;
    Stages private lockStage;
    Stages private lottoStage;
    Stages private projectStage;
    event Deposit(address indexed user, uint256 amount);
    event Withdraw(address indexed user, uint256 amount);
    event Participate(address indexed user, uint256 amount);

    // Constructor, initialize the contract’s parameters and variables
    constructor(
        address _token,
        address payable _randomness,
        uint256 _amountPerTicket,
        uint256 _minParticipants,
        uint256 _bonusRate,
        uint256 _bonusAllocation,
        uint256 _duration
    ) ERC20("LOFUN", "LOFUN") {
        token = IERC20(_token);
        amountPerTicket = _amountPerTicket;
        minParticipants = _minParticipants;
        bonusRate = _bonusRate;
        bonusAllocation = _bonusAllocation;
        duration = _duration;
        projectStage = Stages.PROJECT_START;
        depositStage = Stages.DEPOSITE_CLOSED;
        lockStage = Stages.LOCK_CLOSED;
        lottoStage = Stages.LOTTO_CLOSED;
        randomness = Randomness(_randomness);
    }

    // Convert the enum type stage to a string
    function getStagesString(Stages _stage)
        private
        pure
        returns (string memory stageStr)
    {
        if (_stage == Stages.PROJECT_START) {
            stageStr = "PROJECT_START";
        } else if (_stage == Stages.DEPOSITE_OPEN) {
            stageStr = "DEPOSITE_OPEN";
        } else if (_stage == Stages.DEPOSITE_CLOSED) {
            stageStr = "DEPOSITE_CLOSED";
        } else if (_stage == Stages.LOTTO_OPEN) {
            stageStr = "LOTTO_OPEN";
        } else if (_stage == Stages.LOTTO_CLOSED) {
            stageStr = "LOTTO_CLOSED";
        } else if (_stage == Stages.LOCK_OPEN) {
            stageStr = "LOCK_OPEN";
        } else if (_stage == Stages.LOCK_CLOSED) {
            stageStr = "LOCK_CLOSED";
        } else if (_stage == Stages.PROJECT_END) {
            stageStr = "PROJECT_END";
        }
        return stageStr;
    }

    // Stages status. change modifier to a function
    function atStage(Stages stageVariable, Stages _stage) private view {
        require(
            stageVariable == depositStage ||
                stageVariable == lockStage ||
                stageVariable == lottoStage ||
                stageVariable == projectStage,
            "Invalid stageVariable!"
        );
        string memory str = string(
            abi.encodePacked(
                "Function can only be called at stage: ",
                getStagesString(_stage),
                ". Current stage: ",
                getStagesString(stageVariable)
            )
        );
        require(_stage == stageVariable, str);
    }

    // Get the status of the Deposit Stage
    function getDepositStage() public view returns (string memory) {
        return getStagesString(depositStage);
    }

    // Get the status of the Lock Stage
    function getLockStage() public view returns (string memory) {
        return getStagesString(lockStage);
    }

    // Get the status of the Lotto Stage
    function getLottoStage() public view returns (string memory) {
        return getStagesString(lottoStage);
    }

    // Get the status of the Project Stage
    function getProjectStage() public view returns (string memory) {
        return getStagesString(projectStage);
    }

    // Get the total number of tickets for a round
    function getTotalTickets(uint256 _round) public view returns (uint256) {
        return totalTickets[_round];
    }

    // Get the number of participating users for a round
    function getParticipantsNumber(uint256 _round)
        public
        view
        returns (uint256)
    {
        return participantsNumber[_round];
    }

    // Get the user address corresponding to an index for a round
    function getParticipantAddr(uint256 _round, uint256 _index)
        public
        view
        returns (address)
    {
        return participantAddr[_round][_index];
    }

    // Get whether a user has participated in a round
    function getIsParticipated(uint256 _round, address _user)
        public
        view
        returns (bool)
    {
        return isParticipated[_round][_user];
    }

    // Get the number of tickets for a user in a round
    function getNumberOfTickets(uint256 _round, address _user)
        public
        view
        returns (uint256)
    {
        return numberOfTickets[_round][_user];
    }

    // Get the number of locked shares for a user in a round
    function getLockedShares(uint256 _round, address _user)
        public
        view
        returns (uint256)
    {
        return lockedShares[_round][_user];
    }

    // Get the total number of shares for a round
    function getTotalLockedShares(uint256 _round)
        public
        view
        returns (uint256)
    {
        return totalLockedShares[_round];
    }

    // Get the start time of the lotto for a round
    function getStartTime(uint256 _round) public view returns (uint256) {
        return startTime[_round];
    }

    // Get the random number obtained for a round
    function getRandomNumbers(uint256 _round) public view returns (uint256) {
        return randomNumbers[_round];
    }

    // Get the winner address corresponding to an index for a round
    function getWinners(uint256 _round, uint256 _index)
        public
        view
        returns (address)
    {
        return winners[_round][_index];
    }

    // Get the profit amount for a round
    function getProfits(uint256 _round) public view returns (uint256) {
        return profits[_round];
    }

    // Test
    function toggleIsUseTestRandomNubmer() public onlyOwner returns (bool) {
        if (isUseTestRandomNubmer) {
            isUseTestRandomNubmer = false;
        } else {
            isUseTestRandomNubmer = true;
        }
        return isUseTestRandomNubmer;
    }

    // Set the winning rate, only the contract owner can call, and can only be called before the project starts
    function setBonusRate(uint256 _amount) public onlyOwner {
        atStage(projectStage, Stages.PROJECT_START);
        bonusRate = _amount;
    }

    // Set the minimum number of participants, only the contract owner can call, and can only be called before the project starts
    function setMinParticipants(uint256 _amount) public onlyOwner {
        atStage(projectStage, Stages.PROJECT_START);
        minParticipants = _amount;
    }

    // Set the lotto cycle, only the contract owner can call, and can only be called before the project starts
    function setDuration(uint256 _amount) public onlyOwner {
        atStage(projectStage, Stages.PROJECT_START);
        duration = _amount;
    }

    // Set the amount corresponding to each ticket, only the contract owner can call, and can only be called before the project starts
    function setAmountPerTicket(uint256 _amount) public onlyOwner {
        atStage(projectStage, Stages.PROJECT_START);
        amountPerTicket = _amount;
    }

    // Calculate the number of tickets that can be obtained based on the input amount
    function caculateTicketsAvailable(uint256 _amount)
        public
        view
        override
        returns (uint256 caculatedTickets)
    {
        atStage(projectStage, Stages.PROJECT_START);
        caculatedTickets = _amount.div(amountPerTicket);
    }

    // Start the deposit stage, only the contract owner can call, and can only be called before the project starts
    function startDeposit() public onlyOwner {
        atStage(projectStage, Stages.PROJECT_START);
        atStage(depositStage, Stages.DEPOSITE_CLOSED);
        depositStage = Stages.DEPOSITE_OPEN;
    }

    // Start the lock stage, private function, internal call
    function _startLock() private {
        atStage(projectStage, Stages.PROJECT_START);
        atStage(lockStage, Stages.LOCK_CLOSED);
        lockStage = Stages.LOCK_OPEN;
    }

    // End the lock stage, private function, internal call
    function _endLock() private {
        atStage(projectStage, Stages.PROJECT_START);
        atStage(lockStage, Stages.LOCK_OPEN);
        lockStage = Stages.LOCK_CLOSED;
    }

    // Start the lotto stage, private function, internal call
    function _startLotto() private {
        atStage(projectStage, Stages.PROJECT_START);
        atStage(lottoStage, Stages.LOTTO_CLOSED);
        lottoStage = Stages.LOTTO_OPEN;
        startTime[bonusRound] = block.timestamp;
    }

    // End the lotto stage, private function, internal call
    function _endLotto() private {
        atStage(projectStage, Stages.PROJECT_START);
        atStage(lottoStage, Stages.LOTTO_OPEN);
        require(block.timestamp > startTime[bonusRound] + duration);
        lottoStage = Stages.LOTTO_CLOSED;
    }

    // End the deposit stage, private function, internal call
    function _endDeposit() private {
        atStage(projectStage, Stages.PROJECT_START);
        atStage(depositStage, Stages.DEPOSITE_CLOSED);
        depositStage = Stages.DEPOSITE_CLOSED;
    }

    // End the project stage, private function, internal call
    function _endProject() private {
        atStage(projectStage, Stages.PROJECT_START);
        require(isLiquidated && isPollPassed, "must liquidate and poll pass!");
        depositStage = Stages.DEPOSITE_CLOSED;
        lockStage = Stages.LOCK_CLOSED;
        lottoStage = Stages.LOTTO_CLOSED;
        projectStage = Stages.PROJECT_END;
    }

    // Deposit function, user call, transfer ERC20 tokens to the contract and get corresponding shares
    function deposit(uint256 _amount) public override {
        atStage(depositStage, Stages.DEPOSITE_OPEN);
        require(_amount > 0, "Invalid amount!");
        token.transferFrom(msg.sender, address(this), _amount);
        _mint(msg.sender, _amount);
        emit Deposit(msg.sender, _amount);
    }

    // Withdraw function, user call, transfer shares out of the contract and get corresponding ERC20 tokens
    function withdraw(address to, uint256 _amount) public override {
        atStage(projectStage, Stages.PROJECT_START);
        require(_amount > 0, "Invalid amount!");
        require(_amount <= balanceOf(msg.sender), "Insufficient balance");
        require(
            _amount <=
                balanceOf(msg.sender).sub(lockedShares[bonusRound][msg.sender]),
            "Insufficient Unlocked shares"
        );
        token.transfer(to, _amount);
        _burn(msg.sender, _amount);
        emit Withdraw(msg.sender, _amount);
    }

    // Lock function, private function, internal call, lock the user’s shares and calculate the number of tickets
    function _lock(address _user, uint256 _amount) private {
        atStage(lockStage, Stages.LOCK_OPEN);
        require(_amount > 0, "Invalid amount!");
        require(_amount <= balanceOf(msg.sender), "Insufficient balance");
        require(
            _amount <=
                balanceOf(msg.sender).sub(lockedShares[bonusRound][msg.sender]),
            "Insufficient Unlocked shares"
        );
        lockedShares[bonusRound][_user] = lockedShares[bonusRound][_user].add(
            _amount
        );
        totalLockedShares[bonusRound] = totalLockedShares[bonusRound].add(
            _amount
        );
    }

    // Unlock function, private function, internal call, unlock the user’s shares
    function _unlock(address _user, uint256 _amount) private {
        atStage(lockStage, Stages.LOCK_CLOSED);
        require(_amount > 0, "Invalid amount!");
        require(
            _amount <= lockedShares[bonusRound][_user],
            "Insufficient lockedShares"
        );
        lockedShares[bonusRound][_user] = lockedShares[bonusRound][_user].sub(
            _amount
        );
        totalLockedShares[bonusRound] = totalLockedShares[bonusRound].sub(
            _amount
        );
    }

    // Get the number of locked shares for the user himself
    function getMyLockedShares() public view override returns (uint256) {
        atStage(projectStage, Stages.PROJECT_START);
        return lockedShares[bonusRound][msg.sender];
    }

    // Get the number of tickets for the user himself
    function getMyNumberOfTickets() public view override returns (uint256) {
        atStage(projectStage, Stages.PROJECT_START);
        return numberOfTickets[bonusRound][msg.sender];
    }

    // Mint ticket function, private function, internal call, mint corresponding tickets according to the user’s shares number
    function _mintTickets(address _user, uint256 _ticketsAmount) private {
        atStage(projectStage, Stages.PROJECT_START);
        if (!isParticipated[bonusRound][_user]) {
            isParticipated[bonusRound][_user] = true;
            participantAddr[bonusRound].push(_user);
            participantsNumber[bonusRound] = participantsNumber[bonusRound].add(
                1
            );
        }
        totalTickets[bonusRound] = totalTickets[bonusRound].add(_ticketsAmount);
        numberOfTickets[bonusRound][_user] = numberOfTickets[bonusRound][_user]
            .add(_ticketsAmount);
    }

    // Start a new round of activities, only the contract owner can call
    function startNewRound() public onlyOwner {
        atStage(projectStage, Stages.PROJECT_START);
        atStage(depositStage, Stages.DEPOSITE_OPEN);
        if (bonusRound == 0) {
            bonusRound = bonusRound.add(1);
        }
        _startLock();
    }

    // Participate in the activity function, user call, lock their own shares and get tickets
    function participate(uint256 _amount) public override {
        atStage(lockStage, Stages.LOCK_OPEN);
        atStage(projectStage, Stages.PROJECT_START);
        uint256 _caculatedTickets = caculateTicketsAvailable(_amount);
        require(_caculatedTickets > 0, "Insufficient amount to participate!");
        _lock(msg.sender, _amount);
        _mintTickets(msg.sender, _caculatedTickets);
        emit Participate(msg.sender, _amount);
    }

    // Stop participating and start the lotto, only the contract owner can call
    function stopParticipationAndStartLotto() public onlyOwner {
        atStage(projectStage, Stages.PROJECT_START);
        require(
            participantsNumber[bonusRound] >= minParticipants,
            "Insufficient participants!"
        );
        _endLock();
        _startLotto();
    }

    // Stop the lotto round, only the contract owner can call
    function StopLottoRound() public onlyOwner {
        atStage(projectStage, Stages.PROJECT_START);
        _generateWinner();
        _profit();
        _payBonus();
        _endLotto();
        bonusRound = bonusRound.add(1);
    }

    // Random number generator function, private function, internal call, get random numbers from random number contract
    function _RandomNumberGenerater() private returns (uint256 randomNumber) {
        randomness.getRandomNumber();
        randomNumber = randomness.getRandomResult();
        return randomNumber;
    }

    // Random number generator test function, private function, internal call, use a given number to test the contract
    function _RandomNumberGeneraterTest() private pure returns (uint256) {
        return 666;
    }

    // Generate winner function, private function, internal call, generate winner address based on random number and participant list
    function _generateWinner() private {
        uint256 _newRandomNumber;
        // Test
        if (isUseTestRandomNubmer) {
            _newRandomNumber = _RandomNumberGeneraterTest();
        } else {
            _newRandomNumber = _RandomNumberGenerater();
        }
        randomNumbers[bonusRound] = _newRandomNumber;
        uint256 _winnerNumber = participantsNumber[bonusRound]
            .mul(bonusRate)
            .div(100);
        uint256 firstWinnerNumber = _newRandomNumber.mod(
            participantsNumber[bonusRound]
        );
        for (uint256 i = 0; i < _winnerNumber; i++) {
            winners[bonusRound].push(
                participantAddr[bonusRound][
                    firstWinnerNumber.add(i).mod(participantsNumber[bonusRound])
                ]
            );
        }
    }

    // Profit function, private function, internal call, calculate the profit amount and transfer the corresponding ERC20 tokens from the contract owner
    function _profit() private {
        require(totalLockedShares[bonusRound] > 0, "No fund for profit!");
        uint256 _profits = caculateProfits();
        token.transferFrom(msg.sender, address(this), _profits);
        _mint(address(this), _profits);
        profits[bonusRound] = _profits;
    }

    // Calculate profit amount function, private function, internal call, calculate profit amount based on total shares number
    function caculateProfits() private view returns (uint256 _profits) {
        _profits = totalLockedShares[bonusRound].mul(5).div(10);
    }

    // Pay bonus function, private function, internal call, transfer ERC20 tokens to the winners according to the winner list and bonus allocation
    function _payBonus() private {
        uint256 _profits = profits[bonusRound];
        uint256 _totalBonus = _profits.mul(bonusAllocation).div(100);
        uint256 _winnerShares;
        uint256 _winnerNumber = participantsNumber[bonusRound]
            .mul(bonusRate)
            .div(100);
        for (uint256 i = 0; i < _winnerNumber; i++) {
            _winnerShares = _winnerShares.add(
                lockedShares[bonusRound][winners[bonusRound][i]]
            );
        }
        for (uint256 i = 0; i < _winnerNumber; i++) {
            IERC20(address(this)).transfer(
                winners[bonusRound][i],
                _totalBonus
                    .mul(lockedShares[bonusRound][winners[bonusRound][i]])
                    .div(_winnerShares)
            );
        }
    }

    // Jump to the next round test function, only the contract owner can call, used to test the contract
    function jumpToNextTurnTest() public onlyOwner {
        lockStage = Stages.LOCK_CLOSED;
        lottoStage = Stages.LOTTO_CLOSED;
        bonusRound++;
    }
}

