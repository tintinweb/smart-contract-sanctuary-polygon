pragma solidity ^0.8.0;

/*
    @Author: youngzhenhao
    项目流程说明：
    部署合约项目开始 => 调用开始储蓄(不关闭) => 调用开始锁定(第一次) => 调用停止锁定,同时调用开始Lotto(第一次),满足最小人数
     => 调用停止Lotto => 结算(第一次)奖励 => 调用开始锁定(第二次) ...
    
    若某次锁定没有达到最小人数,(暂不考虑)
    
    循环直到,某一次结算完成,并且下一次没有开始锁定,此时可以偿清并投票关闭项目;
*/


    /*
     @Author:王铮
     活动状态
     划分一个事件为两个阶段
       1.储蓄阶段
         这个阶段用户操作(可参与储蓄，查看可查看信息)，活动抽奖阶段处于关闭状态
         那么可查看的信息有哪些呢？
       2.活动抽奖阶段
         这个阶段属于系统操作阶段
         这个阶段对应的活动id结束后，新(下)一轮的事件才可开始
    */

    /*
        // @Author:王铮
        // 活动处于储蓄阶段 => 用户可以进行储蓄行为 => 储蓄阶段结束后即可打开这一轮的抽奖阶段
        // 活动处于抽奖阶段 => 用户不能进行储蓄行为 => 抽奖结束后需要设置
        // @Edit: youngzhenhao
        // 修改定义，用户永远可以进行储蓄，参加抽奖是通过锁定余额进行，储蓄阶段不会结束，改为对锁定阶段的控制
    */
    
abstract contract LottoFundInterface{

    // 充值 ERC20 TOKEN
    function deposit(uint256 _amount)public virtual;
    // 参加锁定余额幸运抽奖
    function participate(uint256 _amount)public virtual;
    // 计算可以获得的奖票数量
    function caculateTicketsAvailable (uint256 _amount)public virtual returns (uint256);
    // 用户可以提取没有被锁定的余额
    function withdraw(address to, uint256 _amount)public virtual;
    // 查询自己的锁定额
    function getMyLockedShares() public view virtual returns(uint256);
    // 查询自己本轮的奖票数
    function getMyNumberOfTickets()public view virtual returns(uint256);

}

