// 合约功能:获取随机数
// 据说获取的随机数很大 => 需要考虑处理吗

//这个合约接入治理功能

contract Randomness is VRFConsumerBase {
    Governance internal governanceContract;

    bytes32 internal keyHash;
    uint256 internal fee;
    bytes32 public requestID;
    uint256 internal randomResult;

    constructor(
        address _governance,
        address _vrfCoordinator,
        address _link,
        uint256 _vrfFee   
        ) 
        VRFConsumerBase(
           _vrfCoordinator,_link
        ) 
    {
        governanceContract = Governance(_governance);
        keyHash = 0x6e75b569a01ef56d18cab6a8e71e6600d6ce853834d4a5748b720d06f878b3a4;
        fee = _vrfFee; 
    }

    function getRandom() public returns (bytes32 requestId){
        require(LINK.balanceOf(address(this)) > fee, "Not enough LINK - fill contract with faucet"); 
        return requestRandomness(keyHash, fee);
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal virtual override{
        requestID = requestId;
        randomResult = randomness;
    }
    
    function getRandomNumber() external returns (uint256){
        require(governanceContract.getAdmin() == msg.sender, "not-candyStore-address");
        getRandom();
        return randomResult;
    }

    function getRequestId() external view returns (bytes32){
        return requestID;
    } 

    function getRandomResult() external view returns (uint256){
        return randomResult;
    }

    receive() external payable {}

    fallback() external payable {}
}

