// SPDX-License-Identifier: MIT
pragma solidity ^0.8.17;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";

contract RandomNumber is Ownable {
    using SafeMath for uint;
    using EnumerableSet for EnumerableSet.AddressSet;
    IUniswapV2Pair public uniswapPair;
    IUniswapV2Pair[] public uniswapPairs;

    EnumerableSet.AddressSet private pairs;

    // (uint256 _reserve0, uint256 _reserve1, ) = IUniswapV2Pair(pairAddress).getReserves();
    event SendPrice(string, uint256);

    // constructor(address _pairAddress) {
    //     uniswapPair = IUniswapV2Pair(_pairAddress);
    // }

    function addPair(address _pair) public onlyOwner returns (bool) {
        require(_pair != address(0), "PAIR IS ZERO ADDRESS");
        return pairs.add(_pair);
    }

    function removePair(address _pair) public onlyOwner returns (bool) {
        require(_pair != address(0), "PAIR IS ZERO ADDRESS");
        return pairs.remove(_pair);
    }

    function getPair(uint256 index) public view returns (address) {
        require(index <= pairs.length() - 1, "INDEX OUT OF RANGE");
        return pairs.at(index);
    }

    function isPair(address _pair) public view returns (bool) {
        return pairs.contains(_pair);
    }

    function getRandomNumber22() public view returns (uint256) {
        uint256 random = uint256(
            keccak256(abi.encodePacked(block.timestamp, block.number))
        );
        return random;
    }

    function getRandomNumber77() public view returns (uint256) {
        uint256 price = getReserves77();
        uint256 random = uint256(
            keccak256(abi.encodePacked(block.timestamp, price, block.number))
        );
        return random;
    }

    function getCurrentPrice() public view returns (uint256) {
        (uint112 _reserve0, uint112 _reserve1, ) = uniswapPair.getReserves();
        return uint256(_reserve1).mul(1e18).div(_reserve0);
    }

    function getReserves77() public view returns (uint256) {
        (uint256 _reserve0, uint256 _reserve1, ) = uniswapPair.getReserves();

        return uint256(_reserve1).mul(1e18).div(_reserve0);
    }

    function getReserves88() public view returns (uint256) {
        (
            uint256 _reserve0,
            uint256 _reserve1,
            uint256 _blockTimestampLast
        ) = uniswapPair.getReserves();

        return uint256(_reserve0).add(_reserve1).add(_blockTimestampLast);
    }

    function getReserves99() public view returns (uint256) {
        (
            uint256 _reserve0,
            uint256 _reserve1,
            uint256 _blockTimestampLast
        ) = uniswapPair.getReserves();

        return
            uint256(_reserve1).mul(1e18).div(_reserve0).add(
                _blockTimestampLast
            );
    }

    function getReserves3() public view returns (uint256, uint256, uint256) {
        (
            uint256 _reserve0,
            uint256 _reserve1,
            uint256 _blockTimestampLast
        ) = uniswapPair.getReserves();

        return (_reserve0, _reserve1, _blockTimestampLast);
    }

    function getCurrentPrice_0div1() public view returns (uint256) {
        (uint112 reserve0, uint112 reserve1, ) = uniswapPair.getReserves();
        return uint256(reserve0).mul(1e18).div(reserve1);
    }

    function getCurrentPrice_1div0() public view returns (uint256) {
        (uint112 reserve0, uint112 reserve1, ) = uniswapPair.getReserves();
        return uint256(reserve1).mul(1e18).div(reserve0);
    }

    function getCurrentPrice3addtimestamp() public view returns (uint256) {
        (uint256 reserve0, uint256 reserve1, uint256 timestamp) = uniswapPair
            .getReserves();
        return uint256(reserve1).mul(1e18).div(reserve0).add(timestamp);
    }
}

