contract AirdropCoffee is ERC721A, Ownable {
    using SafeMath for uint256;
    using Strings for uint256;

    uint256 public supplyLimit = 10000;
    uint256 public maximumTransfer = 500;

    address payable public withdrawalWallet;
    address public distributorWallet;
    string private _baseURIextended;

    mapping(address => uint256) public mintCount;

    constructor(
        address payable _withdrawalWallet,
        address _distributorWallet,
        string memory _name,
        string memory _ticker,
        string memory baseURI_
    ) ERC721A(_name, _ticker) {
        require(_withdrawalWallet != address(0));
        withdrawalWallet = _withdrawalWallet;
        distributorWallet = _distributorWallet;
        _baseURIextended = baseURI_;
    }

    /*=======================================
    =            onlyOwner Functions        =
    =======================================*/
    function setWhithdrawalWallet(
        address payable _withdrawalWallet
    ) external onlyOwner {
        withdrawalWallet = _withdrawalWallet;
    }

    function setDistributorWallet(
        address _distributorWallet
    ) external onlyOwner {
        distributorWallet = _distributorWallet;
    }

    function setSupplyLimit(uint256 _supplyLimit) external onlyOwner {
        supplyLimit = _supplyLimit;
    }

    function setBaseURI(string memory baseURI_) external onlyOwner {
        _baseURIextended = baseURI_;
    }

    function withdraw() external onlyOwner {
        uint256 contractBalance = address(this).balance;
        withdrawalWallet.transfer(contractBalance);
    }

    function reserveMint(address receiver, uint256 _amount) external onlyOwner {
        require(
            totalSupply().add(_amount) <= supplyLimit,
            "Not enough tokens left."
        );
        _safeMint(receiver, _amount);
    }

    function modifyMintCount(address user, uint256 _amount) external onlyOwner {
        mintCount[user] = _amount;
    }

    /*=======================================
    =              Distributor              =
    =======================================*/
    function batchDistribute(
        address[] calldata receivers,
        uint256[] calldata tokenIds
    ) external {
        require(
            _msgSender() == distributorWallet || _msgSender() == owner(),
            "only distributor or owner can distribute"
        );

        uint256 length = tokenIds.length;

        require(
            receivers.length <= maximumTransfer,
            "the length of receivers must be less than or equal to maximumTransfer"
        );

        require(
            receivers.length == length,
            "the length of receivers and tokenIds must be equal"
        );

        for (uint256 i; i < length; ) {
            uint256 tokenId = tokenIds[i];
            address owner = ownerOf(tokenId);
            address to = receivers[i];
            require(_msgSender() == owner, "only owner can transfer");
            safeTransferFrom(owner, to, tokenId);
            mintCount[to] += 1; // mint count

            unchecked {
                ++i;
            }
        }
    }

    /*=======================================
    =                Transfer               =
    =======================================*/
    function safeBatchTransfer(
        address[] calldata receivers,
        uint256[] calldata tokenIds
    ) external {
        uint256 length = tokenIds.length;
        require(
            receivers.length == length,
            "the length of receivers and tokenIds must be equal"
        );

        require(
            receivers.length <= maximumTransfer,
            "the length of receivers must be less than or equal to maximumTransfer"
        );

        for (uint256 i; i < length; ) {
            uint256 tokenId = tokenIds[i];
            address owner = ownerOf(tokenId);
            address to = receivers[i];
            require(_msgSender() == owner, "only owner can transfer");
            safeTransferFrom(owner, to, tokenId);

            unchecked {
                ++i;
            }
        }
    }

    /*=======================================
    =                   View                =
    =======================================*/
    function _baseURI() internal view virtual override returns (string memory) {
        return _baseURIextended;
    }
}