pragma solidity ^0.8.4;

contract UpbondDID is
    Initializable,
    ERC721AUpgradeable,
    ERC2771ContextUpgradeable,
    Ownable,
    PermissionsEnumerable,
    MulticallUpgradeable,
    UpbondDIDOpt,
    UUPSUpgradeable
{
    using StringsUpgradeable for uint256;

    /**
     * @notice this mapping will store status claim of claimer address.
     */
    mapping(address => bool) public claimed;

    /**
     * @dev This function is an initialize at deployment, and will be initialize info like :
     * - NFT detail
     * - Meta-tx trust forwarder
     *
     * @param name_ is nft did name
     * @param symbol_ id nft did symbol
     */
    function initialize(
        string memory name_,
        string memory symbol_
    ) external initializer onlyProxy {
        address[] memory tfw = new address[](2);
        tfw[0] = 0xc82BbE41f2cF04e3a8efA18F7032BDD7f6d98a81;
        tfw[1] = 0x9399BB24DBB5C4b782C70c2969F58716Ebbd6a3b;

        _setupOwner(_msgSender());

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());

        __ERC721A_init(name_, symbol_);
        __ERC2771Context_init(tfw);
    }

    /**
     * @dev This function for claim free nft for 1 address 1 nft.
     */
    function claim() external {
        require(claimed[_msgSender()] == false, "!ALLOWED");

        claimed[_msgSender()] = true;
        _safeMint(_msgSender(), 1);
    }

    /**
     * @dev This function is an adoption of Thirdweb's DropERC721 but have some modification,
     * So this smartcontract can return metadata url and remove reveal url library compartibility.
     *
     * @param _tokenId is nft token id
     */
    function tokenURI(
        uint256 _tokenId
    ) public view override returns (string memory) {
        require(ownerOf(_tokenId) != address(0), "!EXIST");

        return
            string(
                abi.encodePacked(
                    "https://upbond-did.standard.us-east-1.oortech.com/metadata/",
                    _tokenId.toString()
                )
            );
    }

    /**
     * @dev this function will return Encrypted VC URL from every dapps publickey to every nft id
     *
     * @param _tokenId is nft token id
     * @param _publicKey is dapps public key
     */
    function getEncryptedVCURL(
        uint256 _tokenId,
        bytes memory _publicKey
    ) public view returns (string memory) {
        require(
            ownerOf(_tokenId) != address(0) &&
                publicKeyUsed[_tokenId][_publicKey] == true &&
                keccak256(bytes(optData[_tokenId][_publicKey])) !=
                keccak256(bytes("")),
            "!EXIST"
        );

        return
            string(
                abi.encodePacked(
                    "https://upbond-did.standard.us-east-1.oortech.com/encryptedvc/",
                    _tokenId.toString(),
                    "/",
                    _publicKey
                )
            );
    }

    /**
     * @dev This function is an adoption of Thirdweb's DropERC721 but have some modification,
     * So this smartcontract can not be transferable but can be mint or burn condition,
     * We remove toggle SBT to Fixed Non-Transferable.
     *
     * @param from is from adddress asset sender
     * @param to is destination receive asset
     */
    function _beforeTokenTransfers(
        address from,
        address to,
        uint256,
        uint256
    ) internal virtual override {
        if (from != address(0) && to != address(0)) {
            revert("!TRANSFERABLE");
        }
    }

    /**
     * @dev This function is an adoption of Thirdweb's DropERC721,
     * So this smartcontract can handle validation as setuped roles for change owner of smartcontract,
     * This is default overriden function code from thidweb.
     */
    function _canSetOwner() internal view virtual override returns (bool) {
        return hasRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    /**
     * @dev This is an custom overrided function,
     * So this smartcontract can handle validation as opt setter.
     *
     * @param tokenId use token id to check '_msgSender' come from nft holder or not.
     */
    function _canSetOpt(
        uint256 tokenId
    ) internal view virtual override returns (bool) {
        return ownerOf(tokenId) == _msgSender();
    }

    /**
     * @dev We only override because of conflict from ContextUpgradeable and ERC2771ContextUpgradeable,
     * But in this case only want to use function from ERC2771ContextUpgradeable for meta tx compartible.
