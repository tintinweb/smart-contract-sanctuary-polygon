pragma solidity 0.8.14;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "./DocElaboration.sol";

contract DocElaborationFactory is Ownable, AccessControl {

    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");

    uint256 public numDocument;

    mapping(uint256 => address) public legalElaboratedDocuments;

    event NewElaboratedDocument(address originator, uint256 numSections,  string[] _ids, address[] _addresses);

    constructor() {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MANAGER_ROLE, msg.sender);
    }

    function addManager(address _account)
        external 
        onlyOwner
    {
        _grantRole(MANAGER_ROLE, _account);
    }

     function addDocumentElaborated(
        address originator,
        uint256 numSections,
        string[] memory _ids,
        address[] memory _addresses)
        external 
        onlyRole(MANAGER_ROLE) 
    {
        DocElaboration document = new DocElaboration(
            originator, 
            numSections, 
            _ids, 
            _addresses);

        legalElaboratedDocuments[numDocument] = address(document);
        numDocument++;

        emit NewElaboratedDocument(originator, numSections, _ids, _addresses);

    }

    function revokeManager(address _account)
        external 
        onlyOwner
    {
        _revokeRole(MANAGER_ROLE, _account);
    }

    
}

