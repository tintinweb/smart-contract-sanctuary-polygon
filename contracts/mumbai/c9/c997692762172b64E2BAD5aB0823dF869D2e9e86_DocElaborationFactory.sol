pragma solidity 0.8.14;

import "hardhat/console.sol";

contract DocElaboration {

    enum TypeOperation{ NONE, INCLUDE, APPROVE, OMIT }

    struct Editor {
        string id;
        bool omitted;
    }

    struct Snapshot {
        uint256 numSections;
        uint256[] sectionsOrder;
        uint256[] sectionsVersion;
    }

    struct VersionSection {
        uint256 idSection;
        uint256 version;
        uint256 numApprovals;
        string cid;
        string title;
        bool approved;
        bool rejected;
    }

    struct VersionSectionApprover {
        uint256 dateApproved;
        string signature;
        bool approved;
    }

    address public originator;

    uint256 public completedDate;
    uint256 public creationDate;
    uint256 public numSections;
    uint256 public numEditors;
    uint256 public version;

    string public cid; // IPFS CID of Final document
    string public signature; // Signature of originator of Final document

    bool public completed;
    bool public declined;

    mapping(address => Editor) public editors;
    mapping(uint256 => uint256) public lastVersionSection;
    mapping(uint256 => VersionSection[]) public sections;
    mapping(uint256 => mapping(uint256 => mapping(address => VersionSectionApprover))) public versionSectionApprovers;
    mapping(uint256 => Snapshot) public docSnapshot;

    uint256[] public sectionsOrder;
    address[] public approvers;

    error AccessNotAuthotized();
    error AddressIsZero();
    error DocumentAlreadySigned();
    error DocumentNotApprovedByEditors();
    error DocumentWasDeclined();
    error InconsistentArrayLengths(uint array1, uint array2);
    error NumSignersIsZero();
    error OnlyOriginatorCanDeclinedDocument();
    error OnlyOriginatorCanEditEditors();
    error OnlyOriginatorCanSignDocument();
    error SignerNotInDocument();
    error UserNotAuthorizedToSign();
    error SectionNotExist();
    error SectionWasAlreadyApproved();
    error VersionSectionNotExist();
    error VersionSectionAlreadySigned();

        
    constructor(
        address _originator,
        uint256 _numSections,
        string[] memory _ids, 
        address[] memory _addresses)
    {
        originator = _originator;
        numSections = _numSections;
        creationDate = block.timestamp;

        _addEditors(_ids, _addresses);

        if (_numSections > 0){
            _initSections(_numSections);
        } 
    }

    function addSigners(
        string[] memory _ids, 
        address[] memory _addresses) 
        external 
    {
        if (msg.sender != originator) revert OnlyOriginatorCanEditEditors();
        _addEditors(_ids, _addresses);
    }   

    // function addSectionsInBatch(
    //     uint256[] memory _positions,
    //     string[] memory _cids,
    //     string[] memory _signatures,
    //     bool _isNewSections) 
    //     external 
    // {  
    //     if (_positions.length != _positions.length) revert InconsistentArrayLengths(_positions.length, _cids.length);
    //     if (_positions.length != _positions.length) revert InconsistentArrayLengths(_positions.length, _signatures.length);
        
    //     for (uint x; x < _cids.length; x++) {
    //         addNewVersionToSection(_positions[x] , _cids[x], _signatures[x], _isNewSections);
    //     }
    // }

    function declineDocument() external{
        if (originator != msg.sender) revert OnlyOriginatorCanDeclinedDocument();
        declined = true;
    }

    function getSectionsOrder() 
        view
        external
        returns (uint256[] memory) 
    {
        return sectionsOrder;
    }

    function getAllVersionsOnSection(
        uint256 _position)
        view
        external
        returns (VersionSection[] memory) 
    {
        uint256 _idSection =  sectionsOrder[_position];
        uint256 _lastVersion = lastVersionSection[_idSection];
        VersionSection[] memory _versions = new VersionSection[](_lastVersion);
        for (uint x = 0; x < _lastVersion; x++) {
            VersionSection memory versionSection = sections[_idSection][x];
            _versions[x] = versionSection;
        }
        return _versions;
    }

    function omitEditors(
        address[] memory _addresses, 
        bool[] memory _value) 
        external 
    {
        if (originator != msg.sender) revert OnlyOriginatorCanEditEditors();
        if (_addresses.length != _value.length) revert InconsistentArrayLengths(_addresses.length, _value.length);

        for (uint x = 0; x < _addresses.length; x++) {
            editors[_addresses[x]].omitted = _value[x];
            // If ommited is true, then decrease numEditors to sign the document
            numEditors = _value[x] ? numEditors-- : numEditors++;
        }
    }

    // function rejectVersionSectionInBatch(
    //     uint256[] memory _positions,
    //     uint256[] memory _version) 
    //     external 
    // {  
    //     if (_positions.length != _version.length) revert InconsistentArrayLengths(_positions.length, _version.length);

    //     for (uint x; x < _positions.length; x++) {
    //         rejectVersionSection(_positions[x] , _version[x]);
    //     }
    // }

    // function signSectionsInBatch(
    //     uint256[] memory _positions,
    //     uint256[] memory _version,
    //     string[] memory _signature) 
    //     external 
    // {  
    //     if (_positions.length != _version.length) revert InconsistentArrayLengths(_positions.length, _version.length);
    //     if (_positions.length != _signature.length) revert InconsistentArrayLengths(_positions.length, _signature.length);

    //     for (uint x; x < _positions.length; x++) {
    //         signVersionSection(_positions[x] , _version[x], _signature[x]);
    //     }
    // }

    function signDocument(
        string memory _cid, 
        string memory _signature) 
        external 
    {
        if (declined == true) revert DocumentWasDeclined();
        // New requirement:  any editor can create the final document
        // if (originator != msg.sender) revert OnlyOriginatorCanSignDocument();
        if (originator != msg.sender && bytes(editors[msg.sender].id).length == 0) revert UserNotAuthorizedToSign(); 

        if (!isDocumentApprovedByEditors()) revert DocumentNotApprovedByEditors();
        if (completed == true) revert DocumentAlreadySigned();

        cid = _cid;
        signature = _signature;
        completed = true;
        completedDate = block.timestamp;

    }

    // @dev: Add new version to section
    // @param: _position: section id
    // @param: _cid: IPFS CID of the new version
    // @param: _signature: signature of the new version
    // @return: new version
    function addNewVersionToSection(
        uint256 _position, 
        string memory _title,
        string memory _cid, 
        string memory _signature,
        bool _isNewSection) 
        public 
    {
        if (bytes(editors[msg.sender].id).length == 0) revert UserNotAuthorizedToSign(); 
        
        uint256 _idSection = _position;

        // if (_isNewSection) {
        //     sectionsOrder.push(_idSection);
        //     numSections = sectionsOrder.length;
        //     sectionsOrder[_position] = _idSection;
        // }

        VersionSection[] storage versionSections = sections[_idSection];
        uint256 newVersion = versionSections.length + 1;

        VersionSection memory versionSection = VersionSection({
            idSection: _idSection,
            version: newVersion,
            numApprovals: 1,
            cid: _cid,
            title: _title,
            approved: false,
            rejected: false
        });
        versionSections.push(versionSection);

        lastVersionSection[_idSection] = newVersion;
        // Msg.sender add new version to the section and approve it
        // restriction: check msg.sender has not sign the version
        VersionSectionApprover storage versionSectionApprover = versionSectionApprovers[_idSection][newVersion][msg.sender];
        versionSectionApprover.dateApproved =  block.timestamp;
        versionSectionApprover.signature = _signature;
        versionSectionApprover.approved = true;

        // version++;
        // _addDocSnapshot();
    }

    function changeOrderPosition(
        uint256[] memory _newPosition) 
        public
    {
        if (originator != msg.sender && bytes(editors[msg.sender].id).length == 0) revert UserNotAuthorizedToSign(); 
        // if (_newPosition.length != sectionsOrder.length) revert InconsistentArrayLengths(_newPosition.length, sectionsOrder.length);

        sectionsOrder = _newPosition;
    }

     function getVersionSectionApproved(
        uint256 _section) 
        public 
        view 
        returns (bool, uint256) 
    {
        for (uint x = 0; x < sections[_section].length; x++) {
            VersionSection memory versionSection = sections[_section][x];

            if (versionSection.approved == true) return (true, x);
        }
        return (false, 0);
    }

    function getSnapshotByVersion(uint256 _version) 
        public 
        view 
        returns (Snapshot memory) 
    {
        return docSnapshot[_version];
    }

    function isDocumentApprovedByEditors() 
        public 
        view 
        returns (bool)
    {
        for (uint x = 0; x < numSections; x++) {
            (bool isVersionApproved,) = getVersionSectionApproved(x);
            if (isVersionApproved == false) return false;
        }
        return true;
    }

     function rejectVersionSection( 
        uint256 _position, 
        uint256 _version)
        public {
        if (bytes(editors[msg.sender].id).length == 0) revert UserNotAuthorizedToSign(); 
        if (editors[msg.sender].omitted == true) revert UserNotAuthorizedToSign(); 

        uint256 _idSection =  _position;
        if (sections[_idSection].length < _version) revert VersionSectionNotExist();
        
        VersionSection storage versionSection = sections[_idSection][_version-1];
        versionSection.rejected = true;

    }

    function signVersionSection(
        uint256 _position, 
        uint256 _version,
        string memory _signature) 
        public 
    {
        //Restriction: check Section-version exits      
        if (bytes(editors[msg.sender].id).length == 0) revert UserNotAuthorizedToSign(); 
        if (editors[msg.sender].omitted == true) revert UserNotAuthorizedToSign(); 

        uint256 _idSection =  _position;
        if (sections[_idSection].length < _version) revert VersionSectionNotExist();

        VersionSectionApprover storage versionSectionApprover = versionSectionApprovers[_idSection][_version][msg.sender];
        
        if (versionSectionApprover.approved) revert VersionSectionAlreadySigned();

        versionSectionApprover.dateApproved =  block.timestamp;
        versionSectionApprover.signature = _signature;
        versionSectionApprover.approved = true;

        VersionSection storage versionSection = sections[_idSection][_version-1];

        versionSection.numApprovals++;

        if(versionSection.numApprovals == numEditors) versionSection.approved = true;
    }

    function singleOperation(
        TypeOperation _typeOperation,
        uint256 _idSection,
        uint256 _version,
        string memory _title,  
        string memory _cid, 
        string memory _signature,
        bool _isNewSection)
        public 
    {
        if (_typeOperation == TypeOperation.INCLUDE) {
            addNewVersionToSection(_idSection, _title, _cid, _signature, _isNewSection);
    
        } else if (_typeOperation == TypeOperation.APPROVE) {
            signVersionSection(_idSection, _version, _signature);
            
        } else if (_typeOperation == TypeOperation.OMIT) {
            rejectVersionSection(_idSection, _version);
        }
    }

    function singleOperationInBatch(
        TypeOperation[] memory _typeOperation,
        uint256[] memory _positions,
        uint256[] memory _versions,
        string[] memory _titles,
        string[] memory _cids,
        string[] memory _signatures,
        bool[] memory _isNewSections) 
        external 
    {  
        // if (_positions.length != _positions.length) revert InconsistentArrayLengths(_positions.length, _cids.length);
        // if (_positions.length != _positions.length) revert InconsistentArrayLengths(_positions.length, _signatures.length);
        for (uint x; x < _typeOperation.length; x++) {
            if (_isNewSections[x]) {
                sectionsOrder.push(numSections);
                numSections = numSections +1;
            } else {
                sectionsOrder[x] = _positions[x];
            }
            
            singleOperation(_typeOperation[x], _positions[x], _versions[x], _titles[x], _cids[x], _signatures[x], _isNewSections[x]);      
        }
        version++;
        _addDocSnapshot();
    }


    function _addDocSnapshot()  
        internal 
    {
        (uint256[] memory order, uint256[] memory versions) = _getCurrentSnapshotTracking();
        Snapshot memory snapshot = Snapshot({
            numSections: numSections,
            sectionsOrder: order,
            sectionsVersion: versions
        });

        docSnapshot[version] = snapshot;        
    }

    function _addEditors(
        string[] memory _ids, 
        address[] memory _addresses) 
        internal 
    {
        if (_ids.length == 0 || _addresses.length == 0) revert NumSignersIsZero();
        if (_ids.length != _addresses.length) revert InconsistentArrayLengths(_ids.length, _addresses.length);
        numEditors = _ids.length;

        for (uint x; x < _ids.length; x++) {
            if (_addresses[x] == address(0)) revert AddressIsZero();
            
            Editor memory editor;
            editor.id =_ids[x];

            editors[_addresses[x]] = editor;   
            approvers.push(_addresses[x]); 
        }
    }

    function _getCurrentSnapshotTracking() 
        view 
        internal 
        returns(uint256[] memory, uint256[] memory) 
    {
        uint256[] memory order = new uint[](numSections);
        uint256[] memory versions = new uint[](numSections);

        for (uint x = 0; x < sectionsOrder.length; x++) {
            uint256 section = sectionsOrder[x];
            uint256 lastVersionsection = lastVersionSection[section];
            uint256 currentVersion;
            if(lastVersionsection != 0) {
                VersionSection memory versionSection = sections[section][lastVersionsection-1];
                currentVersion = versionSection.version;
            }
            order[x] = section;
            versions[x] = currentVersion;
        }
        return (order, versions);
    }

    function _initSections(
        uint256 _numSections) 
        internal 
    {  
        for (uint x; x < _numSections; x++) {
            sectionsOrder.push(x);
            // console.log("init section", x, sectionsOrder[x]);
        }
    }
}

