contract InvestmentContract is ERC721 {
    address private owner;
    uint256 private tokenIdCounter;
    mapping(uint256 => address) private investors;
    mapping(address => bool) private hasInvested;
    uint256 private totalInvested;

    constructor() ERC721("InvestmentContract", "IC") {
        owner = msg.sender;
        tokenIdCounter = 1;
    }

    modifier onlyOwner() {
        require(msg.sender == owner, "Only the contract owner can call this function");
        _;
    }

    function invest() external payable {
        _invest(msg.value);
    }

    function _invest(uint256 investment) internal {
        require(investment > 0, "Investment amount must be greater than zero");

        if (!hasInvested[msg.sender]) {
            _mint(msg.sender, tokenIdCounter);
            investors[tokenIdCounter] = msg.sender;
            hasInvested[msg.sender] = true;
            tokenIdCounter++;
        }

        totalInvested += investment;
    }

    function depositFunds() external payable onlyOwner {
        require(msg.value > 0, "Deposit amount must be greater than zero");
    }

    function withdraw() external onlyOwner {
        uint256 contractBalance = address(this).balance;
        require(contractBalance > 0, "Contract has no funds to withdraw");
        payable(owner).transfer(contractBalance);
    }

    function distributeProfitsAndLosses(int256 percentage) external onlyOwner {
        require(totalInvested > 0, "No investments made yet");

        for (uint256 i = 1; i < tokenIdCounter; i++) {
            uint256 tokenId = i;
            address investor = ownerOf(tokenId);

            if (investor != address(0)) {
                uint256 investment = getInvestedAmount(investor);
                int256 payment;

                if (percentage >= 0) {
                    payment = int256(investment) + (int256(investment) * percentage) / 100;
                } else {
                    payment = -int256(investment) * percentage / 100;
                }

                require(payment >= 0, "Payment amount cannot be negative");

                // Convert the payment amount to Wei
                uint256 paymentWei = uint256(payment) * 1 ether;

                // Transfer the payment to the investor
                payable(investor).transfer(paymentWei);
            }
        }

        // Clear all investor data after distribution is complete
        clearAllInvestors();
    }



    function clearAllInvestors() internal {
        for (uint256 i = 1; i < tokenIdCounter; i++) {
            address investor = investors[i];

            if (_isApprovedOrOwner(investor, i)) {
                delete investors[i];
                _burn(i);
                hasInvested[investor] = false;
            }
        }
        tokenIdCounter = 1;
        totalInvested = 0;
    }

    function getInvestedAmount(address investor) public view returns (uint256) {
        return hasInvested[investor] ? address(this).balance / totalInvested : 0;
    }

    function getTotalInvested() external view returns (uint256) {
        return totalInvested;
    }
}