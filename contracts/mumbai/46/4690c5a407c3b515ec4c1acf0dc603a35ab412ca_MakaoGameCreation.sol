        _callOptionalReturn(token, abi.encodeWithSelector(token.approve.selector, spender, oldAllowance + value));
    }

    /**
     * @dev Decrease the calling contract's allowance toward `spender` by `value`. If `token` returns no value,
     * non-reverting calls are assumed to be successful.
     */
    function safeDecreaseAllowance(IERC20 token, address spender, uint256 value) internal {
        unchecked {
            uint256 oldAllowance = token.allowance(address(this), spender);
            require(oldAllowance >= value, "SafeERC20: decreased allowance below zero");
            _callOptionalReturn(token, abi.encodeWithSelector(token.approve.selector, spender, oldAllowance - value));
        }
    }

    /**
     * @dev Set the calling contract's allowance toward `spender` to `value`. If `token` returns no value,
     * non-reverting calls are assumed to be successful. Compatible with tokens that require the approval to be set to
     * 0 before setting it to a non-zero value.
     */
    function forceApprove(IERC20 token, address spender, uint256 value) internal {
        bytes memory approvalCall = abi.encodeWithSelector(token.approve.selector, spender, value);

        if (!_callOptionalReturnBool(token, approvalCall)) {
            _callOptionalReturn(token, abi.encodeWithSelector(token.approve.selector, spender, 0));
            _callOptionalReturn(token, approvalCall);
        }
    }

    /**
     * @dev Use a ERC-2612 signature to set the `owner` approval toward `spender` on `token`.
     * Revert on invalid signature.
     */
    function safePermit(
        IERC20Permit token,
        address owner,
        address spender,
        uint256 value,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) internal {
        uint256 nonceBefore = token.nonces(owner);
        token.permit(owner, spender, value, deadline, v, r, s);
        uint256 nonceAfter = token.nonces(owner);
        require(nonceAfter == nonceBefore + 1, "SafeERC20: permit did not succeed");
    }

    /**
     * @dev Imitates a Solidity high-level call (i.e. a regular function call to a contract), relaxing the requirement
     * on the return value: the return value is optional (but if data is returned, it must not be false).
     * @param token The token targeted by the call.
     * @param data The call data (encoded using abi.encode or one of its variants).
     */
    function _callOptionalReturn(IERC20 token, bytes memory data) private {
        // We need to perform a low level call here, to bypass Solidity's return data size checking mechanism, since
        // we're implementing it ourselves. We use {Address-functionCall} to perform this call, which verifies that
        // the target address contains contract code and also asserts for success in the low-level call.

        bytes memory returndata = address(token).functionCall(data, "SafeERC20: low-level call failed");
        require(returndata.length == 0 || abi.decode(returndata, (bool)), "SafeERC20: ERC20 operation did not succeed");
    }

    /**
     * @dev Imitates a Solidity high-level call (i.e. a regular function call to a contract), relaxing the requirement
     * on the return value: the return value is optional (but if data is returned, it must not be false).
     * @param token The token targeted by the call.
     * @param data The call data (encoded using abi.encode or one of its variants).
     *
     * This is a variant of {_callOptionalReturn} that silents catches all reverts and returns a bool instead.
     */
    function _callOptionalReturnBool(IERC20 token, bytes memory data) private returns (bool) {
        // We need to perform a low level call here, to bypass Solidity's return data size checking mechanism, since
        // we're implementing it ourselves. We cannot use {Address-functionCall} here since this should return false
        // and not revert is the subcall reverts.

        (bool success, bytes memory returndata) = address(token).call(data);
        return
            success && (returndata.length == 0 || abi.decode(returndata, (bool))) && Address.isContract(address(token));
    }
}

interface AggregatorV3Interface {
  function decimals() external view returns (uint8);

  function description() external view returns (string memory);

  function version() external view returns (uint256);

  function getRoundData(uint80 _roundId)
    external
    view
    returns (
      uint80 roundId,
      int256 answer,
      uint256 startedAt,
      uint256 updatedAt,
      uint80 answeredInRound
    );

  function latestRoundData()
    external
    view
    returns (
      uint80 roundId,
      int256 answer,
      uint256 startedAt,
      uint256 updatedAt,
      uint80 answeredInRound
    );
}


//This contract is for Chain: Polygon Testnet / Mumbai

contract MakaoGameCreation is Ownable, ReentrancyGuarded, ContextMixin, AccessControlEnumerable{
    // Library usage
    using SafeERC20 for IERC20;
    using Counters for Counters.Counter;
    using Address for address;
    using Strings for uint256;
    bytes32 public constant DELEGATE_ROLE = keccak256("DELEGATE_ROLE");
    AggregatorV3Interface internal priceFeed;
    uint256 public protocol_fee_percentage;
    Counters.Counter private _eventIdTracker;
    uint256 public event_Creation_Access_Token_Amount;
    IERC20 public makaoToken;
    IERC20 public usdc;

    
   struct choice{
       uint8 choice_id;
       bool approved;
   }

     struct bet{
     //address participant;    
     uint8 choice_id;
     uint256 bet_amount;
     bool approved;
     bool winning_stat;
    }

    struct makaoEvent{
        uint256 id; //unique event id
        uint256 betting_start_time; //start time of betting
        uint256 betting_end_time; //end time of betting
        uint256 decision_time_limit; //decision_declaration_time_limit , duration timestamp from event creation
        bool ispublic; //true: public, false: private
        bool cancelled; //default: false
        bool decision_declared;
        uint8 organizer_fee_percentage; //optional - default : 0 
        uint8 number_of_choices; //number of options to pace bet on
        //mapping(address => bet[]) players;
        //participant[] players;
        mapping(address => bet[]) players;
        bet[] bets;
        choice[] choices;
        choice[] winning_choices;
        mapping(uint8 => address[]) option_selector; //choice id to address mapping
        mapping(address => uint256) winner ; //winner address to winning amount mapping
        mapping(uint256 => bool) choiceindex;
    }
    mapping (uint256 => address[]) winners; //uint256 event_id => participant array , winners
    mapping(uint256 => makaoEvent) private Events; // Event id and Event structure mapping
    mapping(address => uint256[]) public organizedEvents;
    mapping(uint256 => address) public organizerOf; // event_id => Organizer's wallet address
    mapping(uint256 => bet[]) public winning_bets;
    mapping(uint256 => address[]) public eventPlayers; 
    mapping (uint256 => bool) public status;
    mapping (address => uint256) public user_usdc_balance; //balance of user mapping
    //mapping (address => uint256) public user_makao_balance;

    // event Id to its metadata description
         mapping(uint256 => string) private _eventMetadataURIs; //metadata will consist event image, video link, event title, event description
    //address public linkAddress = 0x326C977E6efc84E512bB9C30f76E30c160eD06FB ; //make it dynamic later, pass through constructor
    //address public VRFwrapper = 0x99aFAf084eBA697E584501b8Ed2c0B37Dd136693;
     event createEvent(uint256 _eventID, address _organizer, uint256 start_time);
     event betPlaced(uint256 _eventID,address _participant,uint256 _choiceId);

    constructor()
    {
            priceFeed = AggregatorV3Interface(
            0x572dDec9087154dC5dfBB1546Bb62713147e0Ab0 //USDC/USD
        ); 
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(DELEGATE_ROLE, _msgSender());
       makaoToken = IERC20(0xbd039bbB5bf89c90b71d00C3ED843f4DAa314953);
       usdc = IERC20(0xe6b8a5CF854791412c1f6EFC7CAf629f5Df1c747);
       event_Creation_Access_Token_Amount = 100000000000000000000;
    }
     function _msgSender()
        internal
        override
        view
        returns (address sender)
    {
        return ContextMixin.msgSender();
    }

    /**
     * Returns the latest price.
     */
    function getLatestPrice() public view returns (int) {
        // prettier-ignore
        (
            /* uint80 roundID */,
            int price,
            /*uint startedAt*/,
            /*uint timeStamp*/,
            /*uint80 answeredInRound*/
        ) = priceFeed.latestRoundData();
        return price;
    }

    /* 
     * This function returns the current BNB value converted from the USD amount passed as input parameter
     * _valueinUSD  must be formatted with 8 trailing zeroes
     * e.g. - if the USD value which of we want to get the equivalent BNB amount is $1 USD then the input value must be 
     * 100000000

    */
    function getPrice(uint256 _valueinUSD) public view returns(uint256 _p) 
    {
        return ((_valueinUSD / uint256(getLatestPrice())) * 1000000000000000000);
    }

    /*
     Create event

    */

    function createMakaoEvent(uint256 _endtimeduration, uint256 _decision_duration, uint8 _num_of_choices, uint8 publicflag, uint8[] memory _options, string memory _eventURI ) external {
        require(_num_of_choices == _options.length, "The number of choices don't match available options");
        require(makaoToken.balanceOf(_msgSender()) >= event_Creation_Access_Token_Amount, "You don't have enough MAKAO balance to get access");
        uint256 currentEventId = _eventIdTracker.current();
        Events[currentEventId].id = currentEventId;
        Events[currentEventId].betting_start_time = block.timestamp;
        Events[currentEventId].betting_end_time = block.timestamp + _endtimeduration;
        Events[currentEventId].decision_time_limit = block.timestamp + _decision_duration;
        if(publicflag == 1)
            Events[currentEventId].ispublic = true;
        else 
            Events[currentEventId].ispublic = false;   
        Events[currentEventId].organizer_fee_percentage = 0;     
        Events[currentEventId].number_of_choices = _num_of_choices; 

        for(uint256 i; i<_num_of_choices; i++ )
        {
            Events[currentEventId].choices.push(choice(_options[i],true));
            Events[currentEventId].choiceindex[_options[i]] = true;
        }

        Events[currentEventId].cancelled = false;
        Events[currentEventId].decision_declared = false;
        organizedEvents[_msgSender()].push(currentEventId);
        organizerOf[currentEventId] = _msgSender();
        _eventMetadataURIs[currentEventId] = _eventURI;
        _eventIdTracker.increment();
        emit createEvent(currentEventId,_msgSender(),block.timestamp);
    }
  
  function placeBet(uint256 _eventId,uint256 _betAmount, uint8 _option) public nonReentrant{
      //require(msg.value == _betAmount,"Bet amount and recieved amount are not matched!");
      require(!Events[_eventId].cancelled,"The event is cancelled");
      require(Events[_eventId].betting_end_time > block.timestamp,"betting time ended");
      require(_betAmount > 0,"bet amount should be nonzero");
      if(!Events[_eventId].choiceindex[_option])
          {
            Events[_eventId].choiceindex[_option] = true;
            Events[_eventId].choices.push(choice(_option,false));
          } 
      //Events[_eventId].bets.push(bet(_msgSender(),_option,_betAmount,true)); 
      Events[_eventId].players[_msgSender()].push(bet(_option,_betAmount,true, false));
      eventPlayers[_eventId].push(_msgSender());
      Events[_eventId].option_selector[_option].push(_msgSender());
      if(user_usdc_balance[_msgSender()] >= _betAmount)
          user_usdc_balance[_msgSender()] = user_usdc_balance[_msgSender()] - _betAmount;
      else    
      {
        require(_betAmount <= usdc.balanceOf(_msgSender()), "Not enough USDC tokens in your wallet, please try lesser amount");
        usdc.safeTransferFrom(_msgSender(), address(this), _betAmount); 
      }
      emit betPlaced(_eventId, _msgSender(), _option);
  }

  function cancelEvent(uint256 _eventId) external payable{ 

  }
   //value must be set after multiplied by 10^18
    function set_event_creation_access_token_amount(uint256 _value) public{
        require(hasRole(DELEGATE_ROLE, _msgSender()), "Must have Delegate Access");
        event_Creation_Access_Token_Amount = _value;
    }

// _fee_percent should be multiplied with 100. Ex: if protocol fee is 2% , then 200 should be passed as parameter
    function setProtocolFee(uint256 _fee_percent) external {
        protocol_fee_percentage = _fee_percent;
    }
    function setUpDelegateRole(address _delegate) external onlyOwner{

        _setupRole(DELEGATE_ROLE, _delegate);
    }

    function revokeDelegateRole(address _delegate) external onlyOwner{
        _revokeRole(DELEGATE_ROLE, _delegate);
    }
    function declare_winning_choices(uint8[] memory _winning_choices, uint256 _event_id, uint256 _num_of_choices) external {
        require(Events[_event_id].decision_time_limit <= block.timestamp, "Decision declaration time has not come yet");
        require(Events[_event_id].betting_end_time <= block.timestamp,"Betting time is not over");
        require(organizerOf[_event_id] == _msgSender(),"Only Organizer can Pick winner");
             for(uint256 i; i<_num_of_choices; i++ )
        {
            Events[_event_id].winning_choices.push(choice(_winning_choices[i],true));
        }
        Events[_event_id].decision_declared = true;
    }
    function pick_winner(uint256 _event_id) external {
        require(Events[_event_id].decision_declared,"Result not declared yet");
        require(organizerOf[_event_id] == _msgSender() || hasRole(DELEGATE_ROLE, _msgSender()),"Only Organizer and delegates can Pick winner");
        
        for(uint256 i=0; i < Events[_event_id].winning_choices.length; i++)
        {
           for(uint256 j=0; j<Events[_event_id].option_selector[Events[_event_id].winning_choices[i].choice_id].length; j++ )
            winners[_event_id].push(Events[_event_id].option_selector[Events[_event_id].winning_choices[i].choice_id][j]);
        }
    }
    


}