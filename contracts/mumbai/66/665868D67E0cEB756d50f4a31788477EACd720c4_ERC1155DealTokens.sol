        require(to != address(0) && amount > 0, "Invalid Arguments");
        address sender = _msgSender();
        require(
            IERC20Upgradeable(_payoutAddress).transferFrom(sender, to, amount),
            "Transfer Failed"
        );
        emit PayoutDelivered(sender, to, amount);
    }
}

