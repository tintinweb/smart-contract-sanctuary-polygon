    bytes32 public constant URI_SETTER_ROLE = keccak256("URI_SETTER_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
    bytes32 public constant PAYOUT_ROLE = keccak256("PAYOUT_ROLE");
    address public compliTO;
    uint public constant DEFAULT_ID = 0;

    error InsufficientTransferableBalance(
        uint256 id,
        address account,
        uint256 transferableBalance,
        uint256 required
    );

    using StringToAddress for string;
    using AddressToString for address;
    error AlreadyInitialized();

    event FalseSender(string sourceChain, string sourceAddress);

    IAxelarGasService public gasService;

    // IAxelarGateway public gateway;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor(
        uint amount,
        address compliance,
        address gateway_,
        address gasReceiver_
    ) // address gateway_
    // AxelarExecutable(gateway_)
    {
        // _disableInitializers();
        initialize(amount, compliance, gateway_, gasReceiver_);
    }

    function initialize(
        uint amount,
        address compliance,
        address gateway_,
        address gasReceiver_
    ) public initializer {
        __ERC1155_init("Someurl");
        gasService = IAxelarGasService(gasReceiver_);
        SetGateway(gateway_);
        __Pausable_init();
        __ERC1155Burnable_init();
        __ERC1155Supply_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(URI_SETTER_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);
        compliTO = compliance;
        _mint(msg.sender, DEFAULT_ID, amount, "");
    }

    function RemoteSafeTransfer(
        string calldata destinationChain,
        address destinationAddress,
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public payable {
        if (canTransfer(from, to, id, amount, data)) {
            if (from != _msgSender()) {
                _spendAllowance(from, _msgSender(), id, amount);
            }
            _increaseNonce(from, id);
            _burn(from, id, amount);
            bytes memory payload = abi.encode(from, to, id, amount, data);
            string memory stringAddress = destinationAddress.toString();
            gasService.payNativeGasForContractCall{value: msg.value}(
                address(this),
                destinationChain,
                stringAddress,
                payload,
                msg.sender
            );
            gateway.callContract(destinationChain, stringAddress, payload);
        }
    }

    function _execute(
        string calldata /*sourceChain*/,
        string calldata sourceAddress,
        bytes calldata payload
    ) internal override {
        if (sourceAddress.toAddress() != address(this)) {
            emit FalseSender(sourceAddress, sourceAddress);
            return;
        }
        (
            address from,
            address to,
            uint256 id,
            uint256 amount,
            bytes memory data
        ) = abi.decode(payload, (address, address, uint256, uint256, bytes));
        // @data : signature from the source chain
        if (from != to) {
            _restrict(id);
        }
        _mint(to, id, amount, data);
    }

    function contractId() external pure returns (bytes32) {
        return keccak256("ERC1155DealTokens");
    }
   
    function setURI(string memory newuri) public onlyRole(URI_SETTER_ROLE) {
        _setURI(newuri);
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function mint(
        address account,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public whenNotPaused onlyRole(MINTER_ROLE) {
        _mint(account, id, amount, data);
    }

    function mintBatch(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public onlyRole(MINTER_ROLE) {
        _mintBatch(to, ids, amounts, data);
        _restrictBatch(ids);
    }

    /**
     * @dev Safely transfers tokens from one address to another, following specific checks and validations.*
     * @param from The address from which the tokens are being transferred.
     * @param to The address to which the tokens are being transferred.
     * @param id The identifier of the tokens being transferred.
     * @param amount The amount of tokens being transferred.
     * @param data Additional data to be passed along with the transfer.
     */
    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public virtual override whenNotPaused {
        canTransfer(from, to, id, amount, data);

        if (from != _msgSender()) {
            _spendAllowance(from, _msgSender(), id, amount);
        }
        _increaseNonce(from, id);
        _safeTransferFrom(from, to, id, amount, data);
    }

    function lockTokens(
        address account,
        uint id,
        uint amount,
        uint releaseTime
    ) public override returns (bool) {
        require(
            transferableBalance(account, id) >= amount,
            "Insufficient balance"
        );
        _lockTokens(account, id, amount, releaseTime);
        return true;
    }

    function canTransfer(
        address from,
        address to,
        uint256 id,
        uint amount,
        bytes memory data
    ) public view override returns (bool) {
        require(
            transferableBalance(from, id) >= amount,
            "Insufficient transferable balance"
        );
        require(!isRestricted(id), "Transfer restricted");
        bool result = ICompliTo(compliTO).verifyTransfer(
            from,
            to,
            id,
            amount,
            getNonceFor(from, id),
            data
        );
        require(result, "CompliTo: Invalid Signature");
        require(!isFrozen(from) && !isFrozen(to), "Account is frozen");
        return true;
    }

    /**
     * @dev Retrieves the transferable balance of tokens for the specified account and ID.
     *
     * @param account The address of the account for which the transferable balance is being retrieved.
     * @param id The identifier or reference associated with the tokens.
     *
     * @notice This public view function is used to retrieve the transferable balance of tokens
     * for the specified account and ID. The transferable balance represents the amount of tokens
     * that can be transferred or spent by the account without considering any locked or restricted balances.
     *
     * @return The transferable balance of tokens for the specified account and ID.
     */
    function transferableBalance(
        address account,
        uint id
    ) public view returns (uint) {
        return balanceOf(account, id) - lockedBalanceOf(account, id);
    }

    /**
     * @dev Freezes the specified account.
     *
     * @param account The address of the account to be frozen.
     * @param data Additional data related to the freeze action.
     *
     * @notice This public function is used to freeze the specified account.
     * Only accounts with the DEFAULT_ADMIN_ROLE are allowed to call this function.
     * When an account is frozen, it may have certain restrictions or limitations imposed on it.
     *
     * @return A boolean value indicating the success of the freeze operation.
     */
    function freeze(
        address account,
        bytes memory data
    ) public onlyRole(DEFAULT_ADMIN_ROLE) returns (bool) {
        _freeze(account);
        return true;
    }

    /**
     * @dev Unfreezes the specified account.
     *
     * @param account The address of the account to be unfrozen.
     * @param data Additional data related to the unfreeze action.
     *
     * @notice This public function is used to unfreeze the specified account.
     * Only accounts with the DEFAULT_ADMIN_ROLE are allowed to call this function.
     * When an account is unfrozen, it regains normal functionality.
     *
     * @return A boolean value indicating the success of the unfreeze operation.
     */
    function unFreeze(
        address account,
        bytes memory data
    ) public onlyRole(DEFAULT_ADMIN_ROLE) returns (bool) {
        _unfreeze(account);
        return true;
    }

    function payout(
        address to,
        uint256 amount
    ) public onlyRole(PAYOUT_ROLE) returns (bool) {
        _payout(to, amount);
        return true;
    }

    function batchPayout(
        address[] calldata to,
        uint256[] calldata amount
    ) public onlyRole(PAYOUT_ROLE) returns (bool) {
        require(to.length == amount.length, "Invalid Lengths");
        for (uint256 i = 0; i < to.length; i++) {
            _payout(to[i], amount[i]);
        }
        return true;
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    )
        internal
        override(ERC1155Upgradeable, ERC1155SupplyUpgradeable)
        whenNotPaused
    {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    function approve(
        address spender,
        uint id,
        uint256 amount
    ) external returns (bool) {
        _approve(_msgSender(), spender, id, amount);
        return true;
    }

    function forceTokenUnlock(
        address account,
        uint id
    ) public onlyRole(DEFAULT_ADMIN_ROLE) returns (bool) {
        _forceUnlockTokens(_msgSender(), account, id);
        return true;
    }

    function forceTransfer(
        address from,
        address to,
        uint id,
        uint amount,
        bytes memory data
    ) public onlyRole(DEFAULT_ADMIN_ROLE) returns (bool) {
        _safeTransferFrom(from, to, id, amount, data);
        return true;
    }

    /**
     * @dev Restricts the transferability of tokens with the specified ID.
     *
     * @param id The identifier or reference associated with the tokens to be restricted.
     *
     * @notice This public function is used to restrict the transferability of tokens
     * with the specified ID. Only accounts with the DEFAULT_ADMIN_ROLE are allowed to call this function.
     * When a token is restricted, it cannot be transferred or spent until the restriction is removed.
     *
     * @return A boolean value indicating the success of the restriction operation.
     */
    function restrictTransfer(
        uint id
    ) public onlyRole(DEFAULT_ADMIN_ROLE) returns (bool) {
        _restrict(id);
        return true;
    }

    /**
     * @dev Removes the transferability restriction of tokens with the specified ID.
     *
     * @param id The identifier or reference associated with the tokens for which the restriction is being removed.
     *
     * @notice This public function is used to remove the transferability restriction of tokens
     * with the specified ID. Only accounts with the DEFAULT_ADMIN_ROLE are allowed to call this function.
