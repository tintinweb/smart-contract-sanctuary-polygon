// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.9;

// @title:      Fat Mike Music
// @twitter:    https://twitter.com/FatMikeMusic
// @url:        https://www.fatmike.com/

import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/access/AccessControl.sol';
import "erc721a/contracts/extensions/ERC721AQueryable.sol";
import '@openzeppelin/contracts/utils/cryptography/EIP712.sol';
import '@openzeppelin/contracts/security/ReentrancyGuard.sol';

// import "hardhat/console.sol";

contract FatMikeNFT is ERC721AQueryable, Ownable, ReentrancyGuard, AccessControl, EIP712 {

    using ECDSA for bytes32;
    bytes32 public constant MINTER_ROLE = keccak256('MINTER_ROLE');

    event Withdrawal(address account, uint256 amount);
    event PacksCreated(address minter, uint256 pack, uint256 amount, uint256 currentId);

    // uint256 private immutable _chainId;
    string private _uri;

    // track numbe of packs minted in the following order packA, packB, packC, packD, bundle, random_memo, random_demo
    mapping(uint256 => uint256) private packsTracker;
    uint256[7] public maxPacksSupply = [1000, 500, 200, 100, 100, 700, 700];

    struct Token {
        address minter;
        uint256 minFee;
        uint256 pack;
        uint256 quantity;
        uint256 expiredAt;
    }

    constructor(string memory name, string memory symbol, string memory uri, address minter) 
        ERC721A(name, symbol)
        EIP712(name, "1") {
        _uri = uri;

        _setupRole(MINTER_ROLE, minter);
    }

    /**
     * @notice modifier to ensure that callers are human
     */
    modifier callerIsUser() {
        require(tx.origin == msg.sender, "No contract call");
        _;
    }
    
    /**
     * @notice modifier to ensure that the signature is a valid one
     */
    modifier sigIsValid(Token calldata token, bytes memory signature) {

        // use msg.sender instead of token.minter to ensure that the sender is the minter
        bytes32 digest = _hashTypedDataV4(keccak256(abi.encode(keccak256("Token(address minter,uint256 minFee,uint256 pack,uint256 quantity,uint256 expiredAt)"),
        token.minter, token.minFee, token.pack, token.quantity, token.expiredAt)));

        // get the signer address
        address signer = ECDSA.recover(digest, signature);

        // confirm it is a minter address
        require(hasRole(MINTER_ROLE, signer), 'Invalid signature');
        require(block.timestamp < token.expiredAt, "Signature has expired");
        _;
    }

    /**
     * @notice Override _baseUri that return an empty string
     */
    function _baseURI() internal view virtual override returns (string memory) {
        return _uri;
    }

    //------------------//
    // Custom overrides //
    //------------------//
    /**
     * @dev See {IERC721Metadata-tokenURI}.
     */
    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override(IERC721A, ERC721A)
        returns (string memory)
    {
        if (!_exists(tokenId)) revert URIQueryForNonexistentToken();

        string memory baseURI = _baseURI();
        // return bytes(baseURI).length != 0 ? string(abi.encodePacked(baseURI, _toString(tokenId), '.json')) : '';
        return string(abi.encodePacked(baseURI, _toString(tokenId), '.json'));
        // return bytes(baseURI).length != 0 ? string(abi.encodePacked(baseURI, _toString(tokenId))) : '';
    }

    /**
     * @notice Change the the uri o the base
     * @param newUri to replace the base uri
     */
    function updateBaseUri(string memory newUri) public onlyOwner {
        _uri = newUri;
    }

    /**
     * @notice mint new NFTs
     * @param quantity of new NFTs to mint
     */
    function mint(uint256 quantity) external onlyOwner {
        require(quantity > 0, 'Invalid quantity');

        _safeMint(owner(), quantity);
    }

    /**
     * @notice mint new NFTs
     * @param token data that has the minting information 
     * @param signature to ensure that right data is preovided
     */
    function buyPacks(Token calldata token, bytes memory signature) external payable nonReentrant callerIsUser sigIsValid(token, signature) {
        uint256 _minFee = msg.value;
        require(_minFee >= token.minFee, 'Insufficient fund');
        require(token.pack > 0 && token.pack < 8, "Invalid pack ID");
        require(packsTracker[token.pack] < maxPacksSupply[token.pack - 1], "Pack maximum supply reached");

        _safeMint(token.minter, token.quantity);

        packsTracker[token.pack] += 1;
        emit PacksCreated(token.minter, token.pack, token.quantity, _nextTokenId() - 1);
    }

    function withdraw() onlyOwner callerIsUser external {
        emit Withdrawal(owner(), block.timestamp);
        payable(msg.sender).transfer(address(this).balance);
    }

    function packTotalSupply(uint256 packId) public view returns (uint256) {
        return packsTracker[packId];
    }

    function burn(uint256 tokenId) external callerIsUser {
        _burn(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(IERC721A, ERC721A, AccessControl)
        returns (bool)
    {
        return
            interfaceId == type(IAccessControl).interfaceId ||
            interfaceId == type(IERC165).interfaceId ||
            interfaceId == type(IERC721A).interfaceId ||
            super.supportsInterface(interfaceId);
    }
}

