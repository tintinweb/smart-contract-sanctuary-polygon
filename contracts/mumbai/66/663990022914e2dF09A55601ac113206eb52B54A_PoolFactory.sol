pragma solidity ^0.7.4;
pragma abicoder v2;

import "hardhat/console.sol";

contract FundingRateModule {
    uint256 private fundingRateAggregate;
    uint256 private fundingRate;
    uint256 private lastPctOfFeesOnLpCollected;
    uint256 public blockNumOfLastFRAdjustment;

    mapping(address => uint) private entryFundingRate;

    function setEntryFundingRate(address _user, uint _rate) internal {
        // require(entryFundingRate[_user] == 0, "Entry Funding Rate for user not 0");
        entryFundingRate[_user] = _rate;
    }

    function deleteEntryFundingRate(address _user) internal {
        delete entryFundingRate[_user];
    }

    function getEntryFundingRate(address _user) public view returns (uint) {
        return entryFundingRate[_user];
    }

    // TODO: Make non-reentrant
    function updateFundingRate(uint256 _utilizationRate) internal {
        uint256 blocksPerYear = 2102400; // 2102400 is the number of blocks per year on goerli testnet. blocktime = 15 sec.
        fundingRate = ((((_utilizationRate ** 2) / 10 ** 8)) * 10 ** 4) / blocksPerYear; // utilizationRate and APYs are given in 8 digits but then divided for blocksPerYear. That division makes it really small so we add accuracy 10**4 meaning the fundingrate is given in 12 decimals
    }

    function getCurrentFundingRate() public view returns (uint256) {
        return fundingRate;
    }

    // TODO: Make non-reentrant
    function updateFundingAggregate() internal {
        uint256 temp = fundingRateAggregate;
        // console.log("fundingRateAggregate before", temp);
        // console.log("getLastFeesOnLpCollected()", getPctOfFeesOnLiquidity());
        // console.log("getCurrentFundingRate()", getCurrentFundingRate());
        fundingRateAggregate = temp + (getCurrentFundingRate() * (block.number - blockNumOfLastFRAdjustment) + getPctOfFeesOnLiquidity());
        blockNumOfLastFRAdjustment = block.number;
        //updateFundingRate(_utilizationRate);
        // console.log("newFundingRate() per block", getCurrentFundingRate());
        // console.log("newFundingRate() per year", getCurrentFundingRate()*2102400);
    }

    function getCurrentFundingRateAggregate() public view returns (uint256) {
        // TODO: Convert to internal?
        return fundingRateAggregate;
    }

    function getPctOfFeesOnLiquidity() public view returns (uint256) {
        // represented as 12 decimals
        return lastPctOfFeesOnLpCollected;
    }

    function setPctOfFeesOnLiquidity(uint _fees) internal {
        lastPctOfFeesOnLpCollected = _fees;
        // console.log("lastFeesOnLpCollected: ", lastPctOfFeesOnLpCollected);
    }

    function getFundingPaymentForUser(address user, uint256 x1) internal view returns (uint256) {
        return ((getCurrentFundingRateAggregate() - getEntryFundingRate(user)) * x1 * 2) / 10 ** 12;
        // TODO: made decimals dynamic
    }

}

