    function uniSwapExactOutput(uint256 amountOut, uint256 amountInMaximum, address quoteToken) internal returns (uint256 amountIn) {
        // amountInMaximum = type(uint256).max; // TODO: Calculate a minimum value
        address baseToken;
        if (quoteToken == underlyingAssetAddressA) {
