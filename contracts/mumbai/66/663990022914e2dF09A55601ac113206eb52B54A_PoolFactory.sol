        } else {
            baseToken = underlyingAssetAddressA;
        }
        IERC20(baseToken).approve(address(swapRouter), amountInMaximum);
        // console.log("token in, token out", amountInMaximum, amountOut);
        ISwapRouter.ExactOutputSingleParams memory params = ISwapRouter.ExactOutputSingleParams({
            tokenIn: baseToken,
            tokenOut: quoteToken,
            fee: feePool,
            recipient: address(this),
            deadline: block.timestamp,
            amountOut: amountOut,
            amountInMaximum: amountInMaximum,
            sqrtPriceLimitX96: 0
        });

        amountIn = swapRouter.exactOutputSingle(params);
    }

