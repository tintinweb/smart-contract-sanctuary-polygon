    function mintShort(address to, uint256 amount0, uint256 amount1) internal returns (uint256) {
        uint256 depositValueInY = (getExchangeRateForBaseAmount(uint128(amount0))) + amount1;
        // console.log("depositValueInY during mintShort: ", depositValueInY);
        if (totalSupply() == 0) {
