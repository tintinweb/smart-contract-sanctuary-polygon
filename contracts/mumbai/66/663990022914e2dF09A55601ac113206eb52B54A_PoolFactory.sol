    function collectAllFees(bool fees) internal returns (uint256 amount0, uint256 amount1) {
        // set amount0Max and amount1Max to uint256.max to collect all fees
        // alternatively can set recipient to msg.sender and avoid another transaction in `sendToOwner`
        INonfungiblePositionManager.CollectParams memory params = INonfungiblePositionManager.CollectParams({
            tokenId: uniswapTokenId,
            recipient: address(this),
            amount0Max: type(uint128).max,
            amount1Max: type(uint128).max
        });
        blockNumOfLastFeeCollection = block.number; // not sure this is needed
        (amount0, amount1) = nonfungiblePositionManager.collect(params);
        (amount0, amount1) = underlyingAssetAddressA < underlyingAssetAddressB ? (amount0, amount1) : (amount1, amount0);
        if ((fees) && (amount0 > 0 || amount1 > 0)) {
            xAmountOfFeesHeld += amount0;
            yAmountOfFeesHeld += amount1;
            // console.log("amount0 and amount1 from fees collected: ", amount0, amount1);
            calcPctOfFeesOnLiquidity(amount0, amount1);
