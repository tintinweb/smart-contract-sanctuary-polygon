        address _swapRouter,
        address _quoter,
        address _nonfungiblePositionManager,
        string memory _name,
        string memory _symbol
    ) external onlyOwner {
        Pool poolContractInstance = new Pool();
        poolContractInstance.initialize(_tokenA, _tokenB, _feePool, _name, _symbol, _uniswapFactory, _swapRouter, _quoter, _nonfungiblePositionManager);
        EnumerableSet.add(_pools, address(poolContractInstance));
        emit PoolDeployed(address(poolContractInstance));
    }

    function countPoolInstances() external view returns (uint256) {
        return EnumerableSet.length(_pools);
    }

    function getAllPoolInstances() external view returns (address[] memory) {
        return EnumerableSet.values(_pools);
    }

    function isPoolDeployed(address _contract) external view returns (bool) {
        return EnumerableSet.contains(_pools, _contract);
    }

    function addPool(address _contractAddress) external onlyOwner {
        EnumerableSet.add(_pools, _contractAddress);
    }

    function removePool(address _contract) external onlyOwner {
        EnumerableSet.remove(_pools, _contract);
    }
}

