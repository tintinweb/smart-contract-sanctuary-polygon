        } else {
            collectAllFees(true);
            (uint256 pAmountA, uint256 pAmountB) = getUnderlyingAmountsOfPosition();
            uint256 poolValueWithFees = (getExchangeRateForBaseAmount(uint128(pAmountA + xAmountOfFeesHeld))) + pAmountB + yAmountOfFeesHeld;

            uint256 priceOfToken = ((((poolValueWithFees - depositValueInY) * (10 ** decimals())) / (totalSupply())));
            uint256 amountToMint = (depositValueInY * 10 ** decimals()) / priceOfToken;
            _mint(msg.sender, amountToMint);
        }
    }

    function getSqrtRatioAtTick(int24 tick) public view returns (uint160) {
        require(tick >= MIN_TICK && tick <= MAX_TICK, "tick out of bounds");

        uint256 sqrtRatioX96 = TickMath.getSqrtRatioAtTick(tick);
        uint160 sqrtPriceX96 = (uint160(sqrtRatioX96) * (uint160(sqrtRatioX96))) / (2 ** 96);

        return sqrtPriceX96;
    }

    function getPositionPriceRange() public view returns (uint256 priceLower, uint256 priceUpper) {
        // Get the position information
        (, , address token0, address token1, , int24 tickLower, int24 tickUpper, , , , , ) = nonfungiblePositionManager.positions(uniswapTokenId);

        // Get the price corresponding to the tick values
        uint160 sqrtRatioX96Lower = TickMath.getSqrtRatioAtTick(tickLower);
        uint160 sqrtRatioX96Upper = TickMath.getSqrtRatioAtTick(tickUpper);
        uint256 price0 = FullMath.mulDiv(sqrtRatioX96Lower, sqrtRatioX96Lower, FixedPoint96.Q96);
        uint256 price1 = FullMath.mulDiv(sqrtRatioX96Upper, sqrtRatioX96Upper, FixedPoint96.Q96);

        // Determine which token is token0 and which is token1
        (address tokenLower, address tokenUpper) = token0 < token1 ? (token0, token1) : (token1, token0);

        // Compute the price range in terms of token0
        if (tokenLower == token0) {
            priceLower = price0;
            priceUpper = price1;
        } else {
            priceLower = price1;
            priceUpper = price0;
        }

        return (priceLower, priceUpper);
    }

    function getExchangeRateForBaseAmount(uint128 baseAmount) public view returns (uint256) {
        // Convert base amount based on decimals
        uint256 adjustedBaseAmount = baseAmount * 10**(18 - IERC20Full(underlyingAssetAddressA).decimals());
        return (adjustedBaseAmount * getExchangeRate()) / 10**18;
    }

    function getExchangeRate() public view returns (uint256) {
        (uint160 _sqrtPriceX96 ,,,,,,) = IUniswapV3Pool(getPoolAddress()).slot0();
        uint256 numerator1 = uint256(_sqrtPriceX96) * uint256(_sqrtPriceX96);
        uint256 numerator2 = 10** IERC20Full(underlyingAssetAddressA < underlyingAssetAddressB ? underlyingAssetAddressA : underlyingAssetAddressB).decimals();
        return FullMath.mulDiv(numerator1, numerator2, 1 << 192);
    }

    function convertSqrtPriceX96() public view returns (uint256) {
        IUniswapV3Pool uniswapV3Pool = IUniswapV3Pool(getPoolAddress());
        (uint160 sqrtPriceX96, , , , , , ) = uniswapV3Pool.slot0();
        return (sqrtPriceX96 / 2 ** 96) ** 2;
    }

    function getUnderlyingAmountsOfPosition() public view returns (uint256, uint256) {
        (, , , , , int24 tickLower, int24 tickUpper, uint128 liquidity, , , , ) = nonfungiblePositionManager.positions(uniswapTokenId);

        IUniswapV3Pool uniswapV3Pool = IUniswapV3Pool(getPoolAddress());
        (uint160 sqrtPriceX96, , , , , , ) = uniswapV3Pool.slot0();

        uint160 sqrtRatioX96Lower = TickMath.getSqrtRatioAtTick(tickLower);
        uint160 sqrtRatioX96Upper = TickMath.getSqrtRatioAtTick(tickUpper);
        (uint amount0, uint amount1) = LiquidityAmounts.getAmountsForLiquidity(sqrtPriceX96, sqrtRatioX96Lower, sqrtRatioX96Upper, liquidity);
        if(invertedPair){
            return (amount1, amount0);
        }else{
            return (amount0, amount1);
        }
    }

    function getUtilizationRate() public view returns (uint256) {
        return ((totalLpBorrowed * 10 ** 8) / (totalLpBorrowed + getCurrentLiquidity()));
    }

    function userDepositShortTwoSided(uint256 amountAToAdd, uint256 amountBToAdd) external {
        require(amountAToAdd > 0, "Error: Deposit amount cannot be 0");
        // uint256  poolBalanceInYBeforeDeposit = getBalanceOfPoolDenotedInY();
        uint256 liquidityMinted = 0;
        uint amount0 = 0;
        uint amount1 = 0;
        if (uniswapTokenId != 0) {
            // a position already exists
            IERC20(underlyingAssetAddressA).transferFrom(msg.sender, address(this), amountAToAdd);
            IERC20(underlyingAssetAddressB).transferFrom(msg.sender, address(this), amountBToAdd);
            (liquidityMinted, amount0, amount1) = increaseLiquidityCurrentRange(amountAToAdd, amountBToAdd);
        } else {
            (liquidityMinted, amount0, amount1) = mintNewPosition(amountAToAdd, amountBToAdd);
        }
        // return leftovers
        if (amount0 < amountAToAdd) {
            IERC20(underlyingAssetAddressA).approve(address(nonfungiblePositionManager), 0);
            uint refund0 = amountAToAdd - amount0;
            IERC20(underlyingAssetAddressA).transfer(msg.sender, refund0);
        }
        if (amount1 < amountBToAdd) {
            IERC20(underlyingAssetAddressB).approve(address(nonfungiblePositionManager), 0);
            uint refund1 = amountBToAdd - amount1;
            IERC20(underlyingAssetAddressB).transfer(msg.sender, refund1);
        }
        // console.log("Amounts deposited to Uni: ", amount0, amount1);
