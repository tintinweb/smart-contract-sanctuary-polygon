    /**
     * @dev External function called by the short volatility user to withdraw an amount of DXPsh tokens minted by the pool. DXPsh are being burned
     * @notice No approval is required
     * @param liquidityToRemove The amount of liquidity to remove.
     **/
    function userWithdrawShort(uint128 liquidityToRemove) public {
        require(liquidityToRemove > 0, "Error: Deposit amount cannot be 0");
        require(getCurrentLiquidity() >= liquidityToRemove, "Error: Can't exit position as it has been converted to Buy & Hold");
        // TODO: Collect and distribute fees first before calling decreaseLiquidity

        (uint256 amount0, uint256 amount1) = decreaseLiquidityCurrentRange(liquidityToRemove);
        IERC20(underlyingAssetAddressA).transfer(msg.sender, amount0);
        IERC20(underlyingAssetAddressB).transfer(msg.sender, amount1);
        _burn(msg.sender, liquidityToRemove); // burn tokens 1:1
        updateFundingAggregate();
        updateFundingRate(getUtilizationRate());
    }

    /**
     * @dev External function called by the long volatility user to deposit collateral and specify the amount of LP tokens to use.
     * @notice Approval is required on the y token contract from msg.sender for the amount of collateral
     * @notice Collateral can only be deposited in y token for now
     * @param lpAmount The amount of the LP tokens to convert to buy and hold.
     * @param collateral The amount of collateral in y token to deposit.
     **/
    function userDepositLong(uint256 lpAmount, uint128 collateral) external {
        // TODO: Check that user doesn't already have a position before depositing
        require(lpAmount > 0, "Error: LP amount cannot be 0");
        //require(lpAmount <= IERC20(underlyingAssetAddressB).balanceOf(address(this)), "Error: LP amount cannot be 0");
        require(collateral > 0, "Error: Collateral cannot be 0");
        // settle the funding payment, if not zero
        if (lpConversions[msg.sender] != 0) {
            settleFundingPayment(msg.sender);
        }
        //transfer collateral in the pool
        IERC20(underlyingAssetAddressB).transferFrom(msg.sender, address(this), collateral);
        collateralDeposits[msg.sender] += collateral;
        lpConversions[msg.sender] += lpAmount;
        totalLpBorrowed += lpAmount;
        (uint256 amountAConverted, uint256 amountBConverted) = buyAndHold(uint128(lpAmount), msg.sender);

        // fees also collected at this point
        updateFundingAggregate();
        updateFundingRate(getUtilizationRate());
        setEntryFundingRate(msg.sender, getCurrentFundingRateAggregate());
        // console.log("Get entry funding rate: ", getEntryFundingRate(msg.sender));

        emit LongDeposit(msg.sender, lpAmount, collateral, amountAConverted, amountBConverted);
    }

