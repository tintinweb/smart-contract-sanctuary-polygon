        Trait[] memory _traits
    )
        internal
    {
        require(bytes(_version).length > 0, "ERC721GenericMetadata: invalid schema name");

        for (uint8 i = 0; i < _traits.length; i++) {
            defaultTraits[_version].push(_traits[i]);
        }
    }

