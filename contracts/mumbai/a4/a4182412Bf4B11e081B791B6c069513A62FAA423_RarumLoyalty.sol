import "./IRarumLoyalty.sol";

/**
 * @dev ERC721-compatible contract to control on-chain benefits
 */
contract RarumLoyalty is IRarumLoyalty, TieredMetadataToken {

    using ModifiedEnumerableMapUpgradeable for ModifiedEnumerableMapUpgradeable.StringToBytesMap;

    bytes4 public constant RARUM_LOYALTY_INTERFACE_ID = type(IRarumLoyalty).interfaceId;

    function initialize(
        string calldata _name,
        string calldata _symbol,
        address _owner
    )
        public
        initializer
    {
        __TieredMetadataToken_init(_name, _symbol, _owner);
    }

    /**
     * @dev creates a new tier attached to a schema and default values
     */
    function createTier(
        string memory _cid,
        string memory _schema,
        Trait[] calldata _traits
    )
        public
        virtual
        onlyRole(OPERATOR_ROLE)
    {
        require(!isSchema(_schema), "RarumLoyalty: schema version exists");

        tierSchemaVersion[uint8(tierCIDs.length)] = _schema;

        _setSchema(_schema, _traits);

        _addTierURI(_cid);
    }

    /**
     * @dev sets the traits of a schema
     */
     function setSchema(
        string calldata _version,
        Trait[] memory _traits
    )
        external
        onlyRole(OPERATOR_ROLE)
    {
        _setSchema(_version, _traits);
    }

    /**
     * @dev updates the schema of a tier
     */
    function updateTierSchema(
        uint8 _tier,
        string memory _schema
    )
        public
        virtual
        onlyRole(OPERATOR_ROLE)
    {
        require(isSchema(_schema), "RarumLoyalty: schema version does not exist");

        string memory previousSchema = tierSchemaVersion[_tier];
        tierSchemaVersion[_tier] = _schema;

        (uint256[] memory previousSchemaTokens, uint256 length) = tokensBySchema(previousSchema);

        for (uint256 i = 0; i < length; i++) {
            _assemble(previousSchemaTokens[i], _schema);
        }
    }

    /**
     * @dev redeems a token benefit
     */
    function redeem(
        uint256 _tokenId,
        string memory _benefit,
        uint256 _amount
    )
        public
        onlyRole(OPERATOR_ROLE)
    {
        require(_hasAttribute(_tokenId, _benefit), "RarumLoyalty: invalid benefit");

        uint256 value = _getAttribute(_tokenId, _benefit);

        require(value >= _amount, "RarumLoyalty: benefit cannot be redeemed");

        _beforeRedeem(_tokenId, _benefit, value);

        uint256 remaining = value - _amount;

        setAttribute(_tokenId, _benefit, remaining);

        emit Redeem(_tokenId, _benefit, _amount, remaining);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override
        returns (bool)
    {
        return
            interfaceId == RARUM_LOYALTY_INTERFACE_ID ||
            super.supportsInterface(interfaceId);
    }

