pragma solidity 0.8.12;
pragma abicoder v2;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Burnable.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";

import "./utils/AccessProtectedUpgradable.sol";
import "./ApeironApostle.sol";
import "./utils/ChainlinkRngUpgradeable.sol";

contract ApeironApostleTicketMinting is
    Initializable,
    UUPSUpgradeable,
    OwnableUpgradeable,
    ChainlinkRngUpgradeable
{
    using AddressUpgradeable for address; // for isContract

    enum TICKET_MINT_TYPE {
        ERROR_TICKET,
        TICKET_ORIGIN,
        TICKET_S2_GOLDEN_APOSTLE,
        TICKET_S2_APOSTLE,
        TICKET_ZOMBIE_SUMMONER
    }

    // each ticket has its own gene range
    struct TicketMintTypeGeneStruct {
        uint256[] classArray;
        uint256 minIV;
        uint256 maxIV;
        uint256 minSkillLevel;
        uint256 maxSkillLevel;
        uint256[] skillIds;
        uint256 totalSkillLevel;
    }

    // ticket minting data, use for chainlink callback
    struct TicketMintingStruct {
        TICKET_MINT_TYPE mintType;
        address ticketContractAddress;
        uint256 ticketId;
        uint256 gene;
        uint256 apostleId;
    }

    struct TicketMintingRequestStruct {
        address userAddress;
        TicketMintingStruct[] ticketMintingStructs;
        bool isDone; // for client check breed is finish or not;
    }

    // contract address
    ApeironApostle public apostleContract;

    // contract setting and parameters
    bool public isEnabled; // control the all minting period
    mapping(TICKET_MINT_TYPE => uint256) internal forSaleSchedule; // for sale schedule for each mint type
    mapping(address => mapping(uint256 => TICKET_MINT_TYPE))
        public ticketContractIdToMintTypeMapping; // Ticket address + id to map mint type
    mapping(address => mapping(uint256 => ApostleMeta.ApostleClass))
        public ticketContractIdToApostleClassMapping; // Ticket address + id to map mint type
    mapping(TICKET_MINT_TYPE => TicketMintTypeGeneStruct)
        public ticketMintTypeGeneMapping; // mapping mint type to ticket gene Struct

    // data
    mapping(uint256 => TicketMintingRequestStruct)
        public TicketMintingRequestStructMap; // uint256: Chainlink requestId

    // event
    event SaleScheduleUpdated(
        TICKET_MINT_TYPE indexed _mintType,
        uint256 _saleSchedule
    );
    event RequestTicketMinting(uint256 indexed _requestId);
    event TicketMintingSuccess(
        address indexed _userAddress,
        uint256 indexed _tokenId,
        uint256 _gene,
        uint256 _iv
    );
    event TicketMintSummary(
        address indexed _userAddress,
        uint256 indexed _requestId,
        uint256[] _successApostleIdArray,
        uint256[] _failTicketIdArray
    );

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() ChainlinkRngUpgradeable() initializer {}

    function initialize() external virtual initializer {
        __Ownable_init();
        __UUPSUpgradeable_init();
    }

    /// @dev Required by UUPSUpgradeable
    function _authorizeUpgrade(
        address newImplementation
    ) internal override onlyOwner {}

    /**
     * @notice - Update contract address
     * @dev - callable only by owner
     *
     * @param _nftAddress - NFT contract address
     * @param _chainlinkStruct - chainlink struct
     */
    function updateContractSetting(
        address _nftAddress,
        ChainlinkStruct memory _chainlinkStruct
    ) external onlyOwner {
        _requireArgument(_nftAddress.isContract(), "invalid address");

        apostleContract = ApeironApostle(_nftAddress);

        // update chainlink
        updateChainlinkStruct(_chainlinkStruct);
    }

    /**
     * @notice - Enable/Disable Sales
     * @dev - callable only by owner
     *
     * @param _isEnabled - enable? sales
     */
    function setEnabled(bool _isEnabled) external onlyOwner {
        isEnabled = _isEnabled;
    }

    /**
     * @notice - Set sale schedule
     * @dev - callable only by owner
     *
     * @param _mintType - mint type
     * @param _time - sale schedule
     */
    function setForSaleSchedule(
        TICKET_MINT_TYPE _mintType,
        uint256 _time
    ) external onlyOwner {
        forSaleSchedule[_mintType] = _time;

        emit SaleScheduleUpdated(_mintType, _time);
    }

    /**
     * @notice - Update ticket contract address
     * @dev - callable only by owner
     *
     * @param _mintType - mint type
     * @param _ticketMintTypeGeneStruct - ticket mint type gene struct
     */
    function updateTicketMintTypeInfo(
        TICKET_MINT_TYPE _mintType,
        TicketMintTypeGeneStruct memory _ticketMintTypeGeneStruct
    ) external onlyOwner {
        ticketMintTypeGeneMapping[_mintType] = _ticketMintTypeGeneStruct;
    }

    /**
     * @notice - Update ticket contract + id map to mint type
     * @dev - callable only by owner
     *
     * @param _mintTypeArray - mint type Array
     * @param _ticketContractAddressArray - ticket contract address Array
     * @param _ticketIdArray - ticket Id Array
     */
    function updateTicketContractIdToMintTypeMapping(
        TICKET_MINT_TYPE[] memory _mintTypeArray,
        address[] memory _ticketContractAddressArray,
        uint256[] memory _ticketIdArray
    ) external onlyOwner {
        for (uint256 i = 0; i < _mintTypeArray.length; i++) {
            _requireArgument(
                _ticketContractAddressArray[i].isContract(),
                "invalid address"
            );

            ticketContractIdToMintTypeMapping[_ticketContractAddressArray[i]][
                _ticketIdArray[i]
            ] = _mintTypeArray[i];
        }
    }

    /**
     * @notice - Update ticket contract & Id to apostle class mapping
     * @dev - callable only by owner
     *
     * @param _classArray - apostle class Array
     * @param _ticketContractAddressArray - ticket contract address Array
     * @param _ticketIdArray - ticket Id Array
     */
    function updateTicketContractIdToApostleClassMapping(
        ApostleMeta.ApostleClass[] memory _classArray,
        address[] memory _ticketContractAddressArray,
        uint256[] memory _ticketIdArray
    ) external onlyOwner {
        for (uint256 i = 0; i < _classArray.length; i++) {
            _requireArgument(
                _ticketContractAddressArray[i].isContract(),
                "invalid address"
            );

            ticketContractIdToApostleClassMapping[
                _ticketContractAddressArray[i]
            ][_ticketIdArray[i]] = _classArray[i];
        }
    }

    /**
     * @notice - Request ticket minting
     *
     * @param _ticketContractAddressArray - ticket contract address
     * @param _ticketIdArray - ticket Id
     * @param _ticketCountArray - ticket count
     */
    function requestTicketMinting(
        address[] memory _ticketContractAddressArray,
        uint256[] memory _ticketIdArray,
        uint256[] memory _ticketCountArray
    ) external {
        // init ticketMintingStructs
        uint256 ticketCount;
        for (uint256 i = 0; i < _ticketCountArray.length; i++) {
            ticketCount += _ticketCountArray[i];
        }
        TicketMintingStruct[]
            memory ticketMintingStructs = new TicketMintingStruct[](
                ticketCount
            );

        // create ticketMintingStruct
        uint256 currentCount = 0;
        for (uint256 i = 0; i < _ticketContractAddressArray.length; i++) {
            // get the target mint type, if _ticketContractAddress and _ticketId is not valid,
            // mintType will be ERROR_TICKET
            TICKET_MINT_TYPE mintType = ticketContractIdToMintTypeMapping[
                _ticketContractAddressArray[i]
            ][_ticketIdArray[i]];
            // check ticket is valid
            _requireArgument(
                mintType != TICKET_MINT_TYPE.ERROR_TICKET,
                "invalid ticket"
            );
            // check sale is enabled
            _requireArgument(
                isEnabled &&
                    (forSaleSchedule[mintType] > 0 &&
                        forSaleSchedule[mintType] <= block.timestamp),
                "Sale is not enabled"
            );
            // check ticket balance
            _requireArgument(
                ERC1155(_ticketContractAddressArray[i]).balanceOf(
                    msg.sender,
                    _ticketIdArray[i]
                ) >= _ticketCountArray[i],
                "Insufficient ticket balance"
            );

            // create ticketMintingStruct
            for (uint256 j = 0; j < _ticketCountArray[i]; j++) {
                TicketMintingStruct
                    memory ticketMintingStruct = TicketMintingStruct({
                        mintType: mintType,
                        ticketContractAddress: _ticketContractAddressArray[i],
                        ticketId: _ticketIdArray[i],
                        gene: 0,
                        apostleId: 0
                    });
                ticketMintingStructs[currentCount] = ticketMintingStruct;
                currentCount++;
            }
        }

        // request random words
        uint256 requestId = requestRandomWords();
        // save the request info
        TicketMintingRequestStructMap[requestId].userAddress = msg.sender;
        // hardhat do not support direct assign struct array
        for (uint256 i = 0; i < ticketMintingStructs.length; i++) {
            TicketMintingRequestStructMap[requestId].ticketMintingStructs.push(
                ticketMintingStructs[i]
            );
        }
        TicketMintingRequestStructMap[requestId].isDone = false;

        // emit event
        emit RequestTicketMinting(requestId);
    }

    /**
     * @notice - Chainlink VRF callback function
     * @dev - callable only by Chainlink VRF
     *
     * @param _requestId - requestId of the Chainlink VRF
     */
    function _rngCallBack(uint256 _requestId) internal virtual override {
        // check request info
        _requireArgument(
            TicketMintingRequestStructMap[_requestId].userAddress !=
                address(0) &&
                TicketMintingRequestStructMap[_requestId].isDone == false,
            "Invalid requestId"
        );

        // record the success apostle id / fail ticket id
        uint256[] memory successApostleIdArray = new uint256[](
            TicketMintingRequestStructMap[_requestId]
                .ticketMintingStructs
                .length
        );
        uint256[] memory failTicketIdArray = new uint256[](
            TicketMintingRequestStructMap[_requestId]
                .ticketMintingStructs
                .length
        );
        uint256 successCounter = 0;
        uint256 failCounter = 0;
        for (
            uint256 i = 0;
            i <
            TicketMintingRequestStructMap[_requestId]
                .ticketMintingStructs
                .length;
            i++
        ) {
            // cannot try catch internal function, check ticket balance for the fail
            if (
                ERC1155(
                    TicketMintingRequestStructMap[_requestId]
                        .ticketMintingStructs[i]
                        .ticketContractAddress
                ).balanceOf(
                        TicketMintingRequestStructMap[_requestId].userAddress,
                        TicketMintingRequestStructMap[_requestId]
                            .ticketMintingStructs[i]
                            .ticketId
                    ) > 0
            ) {
                // mint NFT
                (uint256 tokenId, uint256 gene) = _ticketMinting(
                    TicketMintingRequestStructMap[_requestId].userAddress,
                    TicketMintingRequestStructMap[_requestId]
                        .ticketMintingStructs[i]
                        .mintType,
                    TicketMintingRequestStructMap[_requestId]
                        .ticketMintingStructs[i]
                        .ticketContractAddress,
                    TicketMintingRequestStructMap[_requestId]
                        .ticketMintingStructs[i]
                        .ticketId
                );
                // save apostle id/gene
                TicketMintingRequestStructMap[_requestId]
                    .ticketMintingStructs[i]
                    .apostleId = tokenId;
                TicketMintingRequestStructMap[_requestId]
                    .ticketMintingStructs[i]
                    .gene = gene;

                // record it
                successApostleIdArray[successCounter] = tokenId;
                successCounter++;
            } else {
                // save error ticket Id
                // record it
                failTicketIdArray[failCounter] = TicketMintingRequestStructMap[
                    _requestId
                ].ticketMintingStructs[i].ticketId;
                failCounter++;
            }
        }
        TicketMintingRequestStructMap[_requestId].isDone = true;

        emit TicketMintSummary(
            TicketMintingRequestStructMap[_requestId].userAddress,
            _requestId,
            successApostleIdArray,
            failTicketIdArray
        );
    }

    /**
     * @notice - Ticket Minting NFT
     * @dev - callable only during sale
     *
     * @param _userAddress - address of the user
     * @param _mintType - mint type
     * @param _ticketContractAddress - ticket contract address
     * @param _ticketId - ticket Id
     */
    function _ticketMinting(
        address _userAddress,
        TICKET_MINT_TYPE _mintType,
        address _ticketContractAddress,
        uint256 _ticketId
    ) internal returns (uint256 tokenId, uint256 gene) {
        // update _gene
        uint256[] memory attributes = _convertToAttributes(gene);
        TicketMintTypeGeneStruct memory geneStruct = ticketMintTypeGeneMapping[
            _mintType
        ];
        // class gene
        uint256[] memory classIds = geneStruct.classArray;
        if (classIds.length == 0) {
            // if geneStruct.classArray length = 0,
            // classId should be ticketContractIdToApostleClassMapping
            attributes[0] = uint256(
                ticketContractIdToApostleClassMapping[_ticketContractAddress][
                    _ticketId
                ]
            );
        } else {
            // shuffle the class and get the first one
            classIds = _shuffleOrdering(classIds, classIds.length);
            attributes[0] = classIds[0];
        }
        _requireArgument(attributes[0] != 0, "error class Id");

        // skill gene, Reduce the stack depth
        attributes = _calcSkillGene(attributes, geneStruct);

        // Head, Cloth, Weapon; Fixed range
        attributes[1] = _randomRange(0, 1);
        attributes[2] = _randomRange(0, 5);
        attributes[3] = _randomRange(0, 5);

        // convert attributes to gene
        gene = _convertToGeneId(attributes);

        // mint NFT
        uint256 iv = _randomRange(geneStruct.minIV, geneStruct.maxIV); // calculate IV
        tokenId = apostleContract.safeMint(
            gene,
            iv,
            (_mintType == TICKET_MINT_TYPE.TICKET_ORIGIN), // check origin tag
            _userAddress // mint to
        );

        // burn ticket
        ERC1155Burnable(_ticketContractAddress).burn(
            _userAddress, // burt from
            _ticketId, // burt ticket id
            1 // burn 1 ticket
        );

        // emit event
        emit TicketMintingSuccess(_userAddress, tokenId, gene, iv);

        // return tokenId
        return (tokenId, gene);
    }

    /**
     * @notice - Calculate skill gene
     *
     * @param _attributes - attributes
     * @param _geneStruct - gene struct
     */
    function _calcSkillGene(
        uint256[] memory _attributes,
        TicketMintTypeGeneStruct memory _geneStruct
    ) internal returns (uint256[] memory) {
        uint256 totalSkillLevelLeft = _geneStruct.totalSkillLevel;
        // skill 1 level, need to calculate skill 2,3 level later, dont exceed max skill level
        _attributes[5] = _randomRange(
            _geneStruct.minSkillLevel,
            Math.min(
                _geneStruct.maxSkillLevel,
                totalSkillLevelLeft - (2 * _geneStruct.minSkillLevel)
            )
        );
        totalSkillLevelLeft -= _attributes[5];
        // skill 2 level, need to calculate skill 3 level later, dont exceed max skill level
        _attributes[7] = _randomRange(
            _geneStruct.minSkillLevel,
            Math.min(
                _geneStruct.maxSkillLevel,
                totalSkillLevelLeft - _geneStruct.minSkillLevel
            )
        );
        totalSkillLevelLeft -= _attributes[7];
        // skill 3 level, dont exceed max skill level
        _attributes[9] = _randomRange(
            _geneStruct.minSkillLevel,
            Math.min(_geneStruct.maxSkillLevel, totalSkillLevelLeft)
        );
        // random Skill Id from skill _geneStruct.skillIds
        // 3 skill id should be different, _shuffleOrdering _geneStruct.skillIds and get first 3 index
        uint256[] memory skillIds = _geneStruct.skillIds;
        skillIds = _shuffleOrdering(skillIds, skillIds.length);
        _attributes[4] = skillIds[0];
        _attributes[6] = skillIds[1];
        _attributes[8] = skillIds[2];

        return _attributes;
    }

    /**
     * @notice - convert geneId to attributes
     *
     * @param _gene - geneId
     */
    function _convertToAttributes(
        uint256 _gene
    ) internal returns (uint256[] memory) {
        uint256[] memory attributes = new uint256[](11);

        for (uint256 i = 0; i < attributes.length; i++) {
            attributes[i] = _gene % (16 ** 2);
            _gene /= (16 ** 2);
        }

        return attributes;
    }

    /**
     * @notice - convert attributes to geneId
     *
     * @param _attributes - attributes
     */
    function _convertToGeneId(
        uint256[] memory _attributes
    ) internal returns (uint256) {
        uint256 geneId = 0;

        for (uint256 id = 0; id < _attributes.length; id++) {
            geneId += _attributes[id] << (8 * id);
        }

        return geneId;
    }

    /**
     * @notice random shuffle array ordering
     * @param _input input array
     * @param _availableSize array size
     */
    function _shuffleOrdering(
        uint256[] memory _input,
        uint256 _availableSize
    ) internal returns (uint256[] memory) {
        uint256 wrapindex;
        for (uint256 i = 0; i < _availableSize - 1; i++) {
            // wrapindex = _randomRange(i + 1, _availableSize - 1);
            wrapindex = _randomRange(i, _availableSize - 1);
            (_input[i], _input[wrapindex]) = (_input[wrapindex], _input[i]);
        }

        return _input;
    }

    /**
     * @notice - getSkillIds, uint[] cant be returned directly from struct
     *
     * @param _mintType - mint type
     */
    function getSkillIds(
        TICKET_MINT_TYPE _mintType
    ) external view returns (uint256[] memory) {
        return ticketMintTypeGeneMapping[_mintType].skillIds;
    }

    /**
     * @notice - getSkillIds, TicketMintingStruct[] cant be returned directly from struct
     *
     * @param _requestId - request Id
     */
    function getTicketMintingStructsByRequestId(
        uint256 _requestId
    ) external view returns (TicketMintingStruct[] memory) {
        return TicketMintingRequestStructMap[_requestId].ticketMintingStructs;
    }

    /**
     * @notice require funciton, save file size
     *
     * @param _canPass false will revert
     * @param _error error msg
     */
    function _requireArgument(bool _canPass, string memory _error) internal {
        require(_canPass, _error);
    }
}

