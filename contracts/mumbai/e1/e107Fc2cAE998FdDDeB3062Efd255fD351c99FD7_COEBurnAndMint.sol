      ];
    }
    // for (uint256 i = 0; i < 5; i++) {
    //   cardsToMint[i] = commonCardTypes[
    //     randomWords[2 + i] % commonCardTypes.length
    //   ];
    // }

    //MINT 1 uncommon rarity card
    cardsToMint[5] = rarityToCardTypes[Library.Rarity.Uncommon][
      randomWords[8] % rarityToCardTypes[Library.Rarity.Uncommon].length
    ];
    // cardsToMint[5] = uncommonCardTypes[
    //   randomWords[7] % uncommonCardTypes.length
    // ];

    //MINT 1 rare/epic/leggo PROMO card
    uint256[] memory promoCardIds = CardInterface(cardsAddress).getPromoTypes();
    uint256 randomPromoCardId = promoCardIds[
      randomWords[8] % promoCardIds.length
    ];
    cardsToMint[6] = randomPromoCardId;

    //add basic cards to mint
    for (
      uint256 i = 0;
      i < rarityToCardTypes[Library.Rarity.Basic].length;
      i++
    ) {
      cardsToMint[7 + i] = rarityToCardTypes[Library.Rarity.Basic][i];
    }
    // for (uint256 i = 0; i < basicCardTypes.length; i++) {
    //   cardsToMint[i + 7] = basicCardTypes[i];
    // }

    console.log("minting cards...", cardsToMint.length);
    CardInterface(cardsAddress).packMint(msg.sender, cardsToMint);

    console.log("minted cards...", cardsToMint.length);

    addressOpenedFP[msg.sender]++;
  }

  /** OTHER ASSETS MINTING **/

  function startMintRandom(
    AssetType _assetType
  ) public nonReentrant whenNotPaused {
    //only allow adventurer and ethernal for now
    require(
      _assetType == AssetType.Adventurer || _assetType == AssetType.Ethernal,
      "Invalid asset type."
    );
    requestRandomWords(_assetType);
  }

  function finishMintRandom(
    AssetType _assetType,
    address _paymentTokenAddress
  ) public nonReentrant {
    //need to check asset type they wish to finish opening
    uint256[] memory requests = getFulfilledByAddressAndType(
      msg.sender,
      _assetType
    );
    require(requests.length > 0, "No fulfilled requests.");
    //require they have enough aeg tokens to open
    address _address;
    if (_assetType == AssetType.Adventurer) {
      _address = adventurersAddress;
    } else if (_assetType == AssetType.Ethernal) {
      _address = ethernalsAddress;
    } else if (_assetType == AssetType.CardBack) {
      _address = cardBacksAddress;
    } else {
      revert("Invalid asset type.");
    }
    uint256 assetPrice = FunctionInterface(_address).randomPrice();

    FunctionInterface(helperAddress).purchaseWithToken(
      assetPrice,
      _paymentTokenAddress, // chosen payment token
      msg.sender // buyer
    );

    NftInterface(_address).adminMint(
      msg.sender,
      s_requests[requests[0]].randomWords[0] %
        NftInterface(_address).totalTypes(),
      1,
      1
    );
  }

  /** GETTERS **/
  // frontend can use this to display how many packs are available to open
  function getFulfilledByAddressAndType(
    address _address,
    AssetType _assetType
  ) public view returns (uint256[] memory) {
    //get all s_userRequests for this address
    uint256[] memory requestIds = s_userRequests[_address];
    uint256[] memory fulfilledRequestIds = new uint256[](requestIds.length);
    uint256 fulfilledRequestIdsCount = 0;
    for (uint256 i = 0; i < requestIds.length; i++) {
      if (
        s_requests[requestIds[i]].fulfilled &&
        !s_requests[requestIds[i]].used &&
        s_requests[requestIds[i]].assetType == _assetType
      ) {
        fulfilledRequestIds[fulfilledRequestIdsCount] = requestIds[i];
        fulfilledRequestIdsCount++;
      }
    }

    return
      FunctionInterface(helperAddress).trim(
        fulfilledRequestIdsCount,
        fulfilledRequestIds
      );
  }

  /** ACTIVATION **/

  function pause() public onlyOwner {
    _pause();
  }

  function unpause() public onlyOwner {
    _unpause();
  }

  function requestRandomWords(
    AssetType _assetType
  ) internal returns (uint256 requestId) {
    //get number of words to return from asset type
    uint32 numWords = 10; // FP requires 9
    if (
      _assetType == AssetType.Ethernal ||
      _assetType == AssetType.Adventurer ||
      _assetType == AssetType.CardBack
    ) {
      numWords = 1;
    }

    //TEST REQUEST ID
    requestId = 83132445710567745731713214319446805456689061274219614556348005953705680908145; // <----------------TESTING

    // Will revert if subscription is not set and funded. <------------------PROD: NEED TO UNCOMMENT FOR VRF
    // requestId = COORDINATOR.requestRandomWords(
    //   keyHash,
    //   s_subscriptionId,
    //   requestConfirmations,
    //   callbackGasLimit,
    //   numWords //number of words to return
    // );
    // console.log("msg.sender in requestRandomWords", msg.sender);
    s_requests[requestId] = RequestStatus({
      opener: msg.sender,
      randomWords: new uint256[](0),
      exists: true,
      fulfilled: false,
      used: false,
      assetType: _assetType
    });
    s_userRequests[msg.sender].push(requestId);
    // console.log(s_userRequests[msg.sender][0]);
    // emit RequestSent(requestId, numWords);
    return requestId;
  }

  function fulfillRandomWords(
    uint256 _requestId,
    uint256[] memory _randomWords
  ) internal override {
    // console.log("IN FULFILLRANDOMWORDS");
    require(s_requests[_requestId].exists, "request not found");
    s_requests[_requestId].fulfilled = true;
    s_requests[_requestId].randomWords = _randomWords;

    // console.log("fulfillRandomWords DONE", _requestId, _randomWords.length);
    emit RequestFulfilled(
      s_requests[_requestId].opener,
      s_requests[_requestId].assetType
    );
  }

  //setGasLimit
  function setCallbackGasLimit(uint32 _callbackGasLimit) public onlyOwner {
    callbackGasLimit = _callbackGasLimit;
  }
}

