pragma solidity ^0.8.15;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "@openzeppelin/contracts/interfaces/IERC2981.sol";

import "./RoyaltyInfo.sol";
import "./Avatar.sol";
import "./AvatarTypeConfig.sol";

/**
 * @author Softbinator Technologies
 * @notice This contract should never be called by an user to mint/update an avatar
 */
contract NftContract is ERC721Enumerable, AccessControl, IERC2981 {
    using Strings for uint256;
    using Strings for uint16;
    using Counters for Counters.Counter;
    using EnumerableMap for EnumerableMap.Bytes32ToBytes32Map;

    Counters.Counter public tokenIdCounter;

    /// @notice default state of the royalty info
    RoyaltyInfo public defaultRoyaltyInfo;

    /**
     * @notice Mapping used for storing all used combinations of attributes used to create a Genesis nft.
     * @notice An entry in this mapping should never be modified from true to false
     */
    mapping(uint16 => mapping(bytes32 => bool)) public attributesTaken;

    /// @notice Mapping used to store all created avatars. The key represents the tokenId
    mapping(uint256 => Avatar) public avatars;

    /// @notice Mapping used to store the names of the parts and the name of the type for each avatar type id
    mapping(uint16 => AvatarTypeConfig) public avatarTypeConfig;

    /// @notice Mapping used to store the names of mintType used
    EnumerableMap.Bytes32ToBytes32Map private mintTypeNames;

    /// @notice base uri for nfts
    string public baseUri;

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant BURNER_ROLE = keccak256("BURNER_ROLE");
    bytes32 public constant UPDATE_AVATAR_ROLE = keccak256("UPDATE_AVATAR_ROLE");

    /// @notice Error when an address is trying to access a non-existent token
    error NonExistentToken();

    /// @notice Error when parts of an avatarType are not defined
    error PartsNotDefined();

    /// @notice Error when mintType was not defined
    error MintTypeNotDefined();

    /// @notice Error when setting invalid fee numerator
    error InvalidFeeNumerator();

    /// @notice Error when setting invalid receiver
    error InvalidReceiver();

    /// @notice Event triggered on updating an avatar
    event UpdateAvatar(uint256 indexed tokenId, uint16 mintType, uint64[] newAttributes);

    /// @notice Event triggered on creating an avatar
    event CreateAvatar(uint256 indexed tokenId, uint16 mintType, uint16 avatarType, uint64[] attributes);

    /// @notice Event triggered on burning an avatar
    event BurnNft(uint256 indexed tokenId);

    /// @notice Event triggered on burning an avatar
    event UpdateAvatarTypeParts(uint16 avatarType, string name, string[] newParts);

    /// @notice Event triggered on updateing the mintTypeNames mapping
    event UpdateMintTypeName(uint256 mintType, string name);

    /// @notice Event triggered on changing base URI
    event BaseURIChanged(string newUri);

    /// @notice Event triggered on changing Royalty Info
    event SetRoyaltyInfo(address receiver, uint16 feeNumerator);

    constructor(
        string memory _name,
        string memory _symbol,
        address _feeReceiver
    ) ERC721(_name, _symbol) {
        if (_feeReceiver == address(0)) {
            revert InvalidReceiver();
        }
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        _grantRole(BURNER_ROLE, msg.sender);
        _grantRole(UPDATE_AVATAR_ROLE, msg.sender);
        mintTypeNames.set(bytes32(0), bytes32("GENESIS"));
        mintTypeNames.set(bytes32(uint256(1)), bytes32("REFURBISHED"));
        setRoyalty(_feeReceiver, 1000);
    }

    /**
     * @notice All the avatars minted are Genesis
     * @dev After an avatar is created with some attributes, those attributes should never be available again
     * @param _to represents the future owner of the nft
     * @param _attributes represents an array with the attributes
     */
    function mint(
        address _to,
        uint16 _avatarType,
        uint64[] calldata _attributes
    ) external onlyRole(MINTER_ROLE) {
        tokenIdCounter.increment();
        uint256 currentTokenId = tokenIdCounter.current();

        /// @dev Create the avatar and store it
        avatars[currentTokenId] = Avatar(1, 0, _avatarType, _attributes); // 0 represents GENESIS

        /// @dev Store used attributes for this avatar
        attributesTaken[_avatarType][keccak256(abi.encodePacked(_attributes))] = true;

        _mint(_to, currentTokenId);
        emit CreateAvatar(currentTokenId, 0, _avatarType, _attributes);
    }

    /**
     * @notice Burn a token by id
     * @param _tokenId the id of the token which will be burned
     */
    function burn(uint256 _tokenId) external onlyRole(BURNER_ROLE) {
        _burn(_tokenId);

        _resetTokenAvatar(_tokenId);

        emit BurnNft(_tokenId);
    }

    /**
     * @notice Update an avatar with new attributes
     * @param _targetId represents the id of the Nft that will be updated
     * @param _attributes represents an array with the new atributes
     */
    function updateAvatar(
        uint256 _targetId,
        uint16 _mintType,
        uint64[] calldata _attributes
    ) external onlyRole(UPDATE_AVATAR_ROLE) {
        if (!_exists(_targetId)) {
            revert NonExistentToken();
        }

        /// [TODO] how the avatar type change de updating?

        /// @dev Change target avatar attributes with the new ones
        Avatar memory targetAvatar = avatars[_targetId];
        targetAvatar.attributes = _attributes;
        targetAvatar.mintType = _mintType; // 1 represents REFURBISHED
        avatars[_targetId] = targetAvatar;

        emit UpdateAvatar(_targetId, _mintType, _attributes);
    }

    /**
     * @notice Update avatar parts for a type
     * @param _avatarType represents type of the avatar that get the parts changed
     * @param _newParts represents an array with the new set of parts name
     */
    function updateAvatarTypeParts(
        uint16 _avatarType,
        string calldata _name,
        string[] memory _newParts
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        avatarTypeConfig[_avatarType].partsName = _newParts;
        avatarTypeConfig[_avatarType].name = _name;
        emit UpdateAvatarTypeParts(_avatarType, _name, _newParts);
    }

    /**
     * @notice Revert in case that the avatarType has no parts specified
     */
    function checkAvatarTypeHasParts(uint16 _avatarType) public view returns (bool) {
        return (avatarTypeConfig[_avatarType].partsName.length != 0 &&
            bytes(avatarTypeConfig[_avatarType].name).length != 0);
    }

    function invalidNumberOfAttribute(uint16 _avatarType, uint64[] calldata _attributes) external view returns (bool) {
        return avatarTypeConfig[_avatarType].partsName.length != _attributes.length;
    }

    /**
     * @notice Check if an array of attributes are taken or not
     */
    function checkAttributesTaken(uint16 _avatarType, uint64[] calldata _attributes) external view returns (bool) {
        return attributesTaken[_avatarType][keccak256(abi.encodePacked(_attributes))];
    }

    /**
     * @notice View function for getting an avatar by id
     * @param _tokenId represents the id of the avatar that we want to view
     * @return {avatars[_tokenId]} is a struct that contains creation flag, mint type and an array of attributes
     */
    function getAvatar(uint256 _tokenId) external view returns (Avatar memory) {
        return avatars[_tokenId];
    }

    /**
     * @notice Update mintTypeNames mapping
     * @param _mintType represents the value of mintType
     * @param _name represents the name of the mintType
     */
    function updateMintType(uint256 _mintType, string memory _name) external onlyRole(DEFAULT_ADMIN_ROLE) {
        mintTypeNames.set(bytes32(_mintType), bytes32(bytes(_name)));
        emit UpdateMintTypeName(_mintType, _name);
    }

    /**
     * @notice Get the values and the names of mintType
     */
    function getMintTypes() external view returns (uint16[] memory keys, string[] memory values) {
        uint256 arrayLength = mintTypeNames.length();
        string[] memory mintTypes = new string[](arrayLength);
        uint16[] memory mintTypeKeys = new uint16[](arrayLength);
        for (uint256 i=0; i < mintTypeNames.length(); i++) {
            (bytes32 key, bytes32 name) = mintTypeNames.at(i);
            mintTypes[i] = (bytes32ToString(name));
            mintTypeKeys[i] = uint16(uint256(key));
        }
        return (mintTypeKeys, mintTypes);
    }

    function bytes32ToString(bytes32 _bytes32) private pure returns (string memory) {
        uint8 i = 0;
        while (i < 32 && _bytes32[i] != 0) {
            i++;
        }
        bytes memory bytesArray = new bytes(i);
        for (i = 0; i < 32 && _bytes32[i] != 0; i++) {
            bytesArray[i] = _bytes32[i];
        }
        return string(bytesArray);
    }

    /**
     * @notice View token ids of an address
     * @param _owner represents the address
     * @return tokenIds represents the array of ids owned by _owner
     */
    function tokensByOwner(address _owner) public view returns (uint256[] memory) {
        unchecked {
            uint256 balanceOfOwner = balanceOf(_owner);
            uint256[] memory tokenIds = new uint256[](balanceOfOwner);
            for (uint256 i=0; i < balanceOfOwner; i++) {
                tokenIds[i] = tokenOfOwnerByIndex(_owner, i);
            }
            return tokenIds;
        }
    }

    /**
     * @notice View base URI
     */
    function _baseURI() internal view override returns (string memory) {
        return baseUri;
    }

    /**
     * @notice Set base URI
     */
    function setBaseURI(string calldata _newBaseUri) external onlyRole(DEFAULT_ADMIN_ROLE) {
        baseUri = _newBaseUri;
        emit BaseURIChanged(_newBaseUri);
    }

    /**
     * @notice Return the token URI of a nft
     */
    function tokenURI(uint256 _tokenId) public view override returns (string memory) {
        if (!_exists(_tokenId)) {
            revert NonExistentToken();
        }

        string memory cachedBaseURI = _baseURI();

        return bytes(cachedBaseURI).length > 0 ? string(abi.encodePacked(cachedBaseURI, _tokenId.toString())) : "";
    }

    function getAvatarTypeParts(uint16 _avatarType) external view returns (string[] memory) {
        if (!checkAvatarTypeHasParts(_avatarType)) {
            revert PartsNotDefined();
        }

        return avatarTypeConfig[_avatarType].partsName;
    }

    /**
     * @notice Return true if an interface is supporteed by the contract
     */
    function supportsInterface(bytes4 _interfaceId)
        public
        view
        override(ERC721Enumerable, AccessControl, IERC165)
        returns (bool)
    {
        if (_interfaceId == type(IAccessControl).interfaceId) return true;
        if (_interfaceId == type(IERC2981).interfaceId) return true;
        return super.supportsInterface(_interfaceId);
    }

    /**
     * @inheritdoc IERC2981
     */
    function royaltyInfo(uint256 _tokenId, uint256 _salePrice) public view returns (address, uint256) {
        if (!_exists(_tokenId)) {
            revert NonExistentToken();
        }

        RoyaltyInfo memory royalty = defaultRoyaltyInfo;

        uint256 royaltyAmount = (_salePrice * royalty.royaltyFraction) / 10000;

        return (royalty.receiver, royaltyAmount);
    }

    /**
     * @notice Sets the royalty information that all ids in this contract will default to.
     * @param _receiver cannot be the zero address.
     * @param _feeNumerator cannot be greater than 1000, which is 10% from price.
     */
    function setRoyalty(address _receiver, uint16 _feeNumerator) public onlyRole(DEFAULT_ADMIN_ROLE) {
        if (_feeNumerator > 1000) {
            revert InvalidFeeNumerator();
        }
        if (_receiver == address(0)) {
            revert InvalidReceiver();
        }

        defaultRoyaltyInfo = RoyaltyInfo(_receiver, _feeNumerator);

        emit SetRoyaltyInfo(_receiver, _feeNumerator);
    }

    /**
     * @notice Resets avatar information for the token id back to the global default.
     */
    function _resetTokenAvatar(uint256 _tokenId) internal {
        delete avatars[_tokenId];
    }
}

