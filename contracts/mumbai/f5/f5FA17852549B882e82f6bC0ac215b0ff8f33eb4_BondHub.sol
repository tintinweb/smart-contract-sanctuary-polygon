pragma solidity 0.8.18;

import {FollowNFTProxy} from '../upgradeability/FollowNFTProxy.sol';
import {Helpers} from './Helpers.sol';
import {DataTypes} from './DataTypes.sol';
import {Errors} from './Errors.sol';
import {Events} from './Events.sol';
import {Constants} from './Constants.sol';
import {IFollowNFT} from '../interfaces/IFollowNFT.sol';
import {IFollowModule} from '../interfaces/IFollowModule.sol';
import {Clones} from '@openzeppelin/contracts/proxy/Clones.sol';
import {Strings} from '@openzeppelin/contracts/utils/Strings.sol';
import {IBondHub} from '../interfaces/IBondHub.sol';
import {ApplicationFollowModuleProxy} from '../upgradeability/ApplicationFollowModuleProxy.sol';
import {ApplicationFollowModule} from '../core/modules/follow/ApplicationFollowModule.sol';

import 'hardhat/console.sol';

/**
 * @title InteractionLogic
 * @author Bond Protocol
 *
 * @notice This is the library that contains the logic for follows & collects. 
 
 * @dev The functions are external, so they are called from the hub via `delegateCall` under the hood.
 */
library InteractionLogic {
    using Strings for uint256;

    /**
     * @notice Follows the given profiles, executing the necessary logic and module calls before minting the follow
     * NFT(s) to the follower.
     *
     * @param followType The type of Follow Relationship.
     * @param follower The address executing the follow.
     * @param profileIds The array of profile token IDs to follow.
     * @param followModuleDatas The array of follow module data parameters to pass to each profile's follow module.
     * @param _profileById A pointer to the storage mapping of profile structs by profile ID.
     * @param _profileIdByHandleHash A pointer to the storage mapping of profile IDs by handle hash.
     *
     * @return uint256[] An array of integers representing the minted follow NFTs token IDs.
     */
    function follow(
        uint256 followType,
        address follower,
        uint256[] calldata profileIds,
        bytes[] calldata followModuleDatas,
        mapping(uint256 => DataTypes.ProfileStruct) storage _profileById,
        mapping(bytes32 => uint256) storage _profileIdByHandleHash
    ) external returns (uint256[] memory) {
        if (profileIds.length != followModuleDatas.length) revert Errors.ArrayMismatch();
        uint256[] memory tokenIds = new uint256[](profileIds.length);
        for (uint256 i = 0; i < profileIds.length; ) {
            tokenIds[i] = _followSingle(
                followType,
                follower,
                profileIds[i],
                followModuleDatas[i],
                _profileById,
                _profileIdByHandleHash
            );

            unchecked {
                ++i;
            }
        }
        emit Events.Followed(
            follower,
            followType,
            profileIds,
            followModuleDatas,
            tokenIds,
            block.timestamp
        );

        return tokenIds;
    }

    function _followSingle(
        uint256 followType,
        address follower,
        uint256 profileId,
        bytes calldata followModuleData,
        mapping(uint256 => DataTypes.ProfileStruct) storage _profileById,
        mapping(bytes32 => uint256) storage _profileIdByHandleHash
    ) private returns (uint256) {
        string memory handle = _profileById[profileId].handle;
        if (_profileIdByHandleHash[keccak256(bytes(handle))] != profileId)
            revert Errors.TokenDoesNotExist();

        address followModule = _profileById[profileId].followModule;
        address followNFT = _getFollowNFT(followType, profileId, _profileById);

        if (followNFT == address(0)) {
            followNFT = _deployFollowNFT(profileId, followType);
            _setFollowNFT(followType, profileId, followNFT, _profileById);
        }
        uint256 tokenId = IFollowNFT(followNFT).tokenOfOwner(follower);
        if (0 == tokenId) {
            tokenId = IFollowNFT(followNFT).mint(follower);
            IFollowNFT(followNFT).emitTransferEvent(followType, tokenId, follower);
            if (followModule != address(0)) {
                IFollowModule(followModule).processFollow(follower, profileId, followModuleData);
            }
        }
        return tokenId;
    }

    function getFollowNFT(
        uint256 followType,
        uint256 profileId,
        mapping(uint256 => DataTypes.ProfileStruct) storage _profileById
    ) external view returns (address) {
        return _getFollowNFT(followType, profileId, _profileById);
    }

    function _getFollowNFT(
        uint256 followType,
        uint256 profileId,
        mapping(uint256 => DataTypes.ProfileStruct) storage _profileById
    ) private view returns (address) {
        address nftAddress;
        for (uint256 i = 0; i < _profileById[profileId].followType.length; i++) {
            if (_profileById[profileId].followType[i] == followType) {
                nftAddress = _profileById[profileId].followNFT[i];
                break;
            }
        }
        return nftAddress;
    }

    function _setFollowNFT(
        uint256 followType,
        uint256 profileId,
        address nftAddress,
        mapping(uint256 => DataTypes.ProfileStruct) storage _profileById
    ) private {
        for (uint256 i = 0; i < _profileById[profileId].followType.length; i++) {
            if (_profileById[profileId].followType[i] == followType) {
                _profileById[profileId].followNFT[i] = nftAddress;
                return;
            }
        }
        _profileById[profileId].followType.push(followType);
        _profileById[profileId].followNFT.push(nftAddress);
    }

    /**
     * @notice Deploys the given profile's Follow NFT contract.
     *
     * @param profileId The token ID of the profile which Follow NFT should be deployed.
     *
     * @return address The address of the deployed Follow NFT contract.
     */
    function _deployFollowNFT(uint256 profileId, uint256 followType) private returns (address) {
        bool burnSign = true;
        if (followType == uint256(DataTypes.FollowType.Referral)) {
            burnSign = false;
        }

        bytes memory functionData = abi.encodeWithSelector(
            IFollowNFT.initialize.selector,
            profileId,
            followType,
            burnSign
        );

        address followNFT = address(new FollowNFTProxy(functionData));
        emit Events.FollowNFTDeployed(profileId, followNFT, block.timestamp);
        return followNFT;
    }

}

