// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
//Importo per pagamento in stablecoins
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
//Importo per exchange rate Matic/USD
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155HolderUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/IERC1155MetadataURIUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "hardhat/console.sol";

/*  
    NB: 
    NFTMarketplace è il contratto che svolge il ruolo marketplace. Non ha più bisogno di essere token holder.
    NFTMint è il contratto che svolge il ruolo di token creator
*/

/*
    Il contratto registra tutte le informazioni riguardati i token messi in vendita. 
    Inoltre ne permette il loro acquisto e trasferimento al nuovo possessore. 
*/

contract NFTMarketplace is Initializable{
    /*                                              VARIABLES, MODIFIERS & CONSTRUCTOR DEFINITIONS                                             */

    using CountersUpgradeable for CountersUpgradeable.Counter;
    CountersUpgradeable.Counter private tokenIds;

    //Fees del marketplace.
    uint256 FirstSaleFee;
    uint256 SecondarySalesFee;

    /* 
        Qui sono riportati i tre smart contract con cui questo contratto interagisce:
        1)NFTMint: contratto/i "privato/i" da noi creati
        2)usdc: contratto esterno che permette scambi della valuta USDC su Polygon/Mumbai
        3)exchangeRateMaticUsd (oracle): contratto esterno per accedere all'exchange rate tra Matic e USD
    */
    //1)
    IERC1155MetadataURIUpgradeable private NFTMint;
    //2)
    IERC20Upgradeable public usdc;
    //3)
    AggregatorV3Interface public exchangeRateMaticUsd;
    
    // Funzione initializable per rendere upgradable lo smart contract (constructors
    // non ammessi)
    function initialize(
        address _USDCAddress,
        address _exchangeRateAddress,
        address mintingContract
        ) 
        public initializer {
            FirstSaleFee = 20;
            SecondarySalesFee = 3;
            usdc = IERC20Upgradeable(_USDCAddress);
            exchangeRateMaticUsd = AggregatorV3Interface(_exchangeRateAddress);
            _owner = payable(msg.sender);
            storeMintingContracts(mintingContract);
    }

    function updateFirstSaleFee(uint256 _FirstSaleFee) public onlyOwner {
        FirstSaleFee = _FirstSaleFee;
    }

    function updateSecondarySalesFee(
        uint256 _SecondarySalesFee
    ) public onlyOwner {
        SecondarySalesFee = _SecondarySalesFee;
    }

    address payable private _owner;

    /*
        Soltanto i contratti da noi creati possono chiamare determinate funzioni di questo smart contract.
        In questo array sono salvati tutti i contratti che hanno questo diritto.  
    */
    address[] contractsAllowed;

    /*
        Dato tokenId e address del contratto che lo ha creato, ritorna informazioni del token. 
            INPUT: tokenId e address del contratto che ha mintato l'NFT
            OUTPUT: autore dell'NFT, royalties, supply del token, uri, numero di prime vendite(parametro dopo meglio spiegato)
    */
    mapping(uint256 => mapping(address => TokenInfo)) public idToTokenInfo;
    struct TokenInfo {
        uint256 tokenId;
        address smartContract;
        address payable author;
        uint256 royalties;
        uint256 supply;
        string uri;
        uint256 numberOfFirstSells;
    }

    /*
        Dato tokenId e l'address del contratto che lo ha creato, ritorna un array degli addresses che stanno attualmente cercando di vendere il token.
        INPUT: tokenId e address del contratto che ha mintato l'NFT
        OUTPUT: lista degli attuali address che voglio vendere l'NFT, quantità che vogliono vendere e prezzo. 
     */
    mapping(uint256 => mapping(address => TokenSellers)) private idToSellers;
    struct TokenSellers {
        address payable[] sellers;
        uint256[] sellingQuantity;
        uint256[] sellingPrice;
    }

    /*
        Dato l'address di un contratto NFTMinting, ritorna tutti i tokenId creati. 
     */
    mapping(address => uint256[]) private smartContractToTokenIds;

    //Evento emesso quando un NFT è creato
    event MarketItemCreated(
        uint256 indexed tokenId,
        address smartContract,
        address author,
        address seller,
        uint256 price,
        uint256 royalties,
        uint256 supply
    );

    //Evento emesso quando un NFT è RE-listato
    event MarketItemReCreated(
        uint256 indexed tokenId,
        address smartContract,
        uint256 price,
        address seller
    );

    //Evento emesso quando un NFT è venduto
    event MarketItemSold(
        uint256 indexed tokenId,
        address smartContract,
        address seller,
        uint256 price,
        address owner
    );

    modifier onlyOwner() {
        require(
            msg.sender == _owner,
            "Only owner of the marketplace can call this function"
        );
        _;
    }

    //Funzione chiamata per aggiornare i contratti che possono interagire con determinate funzioni dello smart contract.
    function storeMintingContracts(address contracts) public onlyOwner {
        contractsAllowed.push(contracts);
    }

    //Modifier per garantire interazione solo da parte di certi smart contract
    modifier onlyContracts() {
        require(
            findElementIndexInContracts(msg.sender) == 1,
            "only contracts allowed can call this function"
        );
        _;
    }

    function findElementIndexInContracts(
        address contractAddress
    ) internal view returns (int) {
        for (uint i = 0; i < contractsAllowed.length; i++) {
            if (contractsAllowed[i] == contractAddress) {
                return int(1);
            }
        }
        return -1;
    }

    /*                                                     FUNCTIONS TO LIST, DELIST OR CHANGE NFT PRICE                                                */

    /*
        Funzione che si occupa di listare per la prima volta un NFT. 
        La funzione viene chiamata dal contratto NFTMint. Può essere chiamata soltanto da contratti "amici". Passaggi principali:
        1) Check che il creatore del token abbia i tokens che dichiara e il contratto NFTMarkeplace abbia l'autorizzazione a trasferirlo.
        2) Creazione oggetto TokenInfo: tutte le informazioni del token sono salvate.
        3) Creazione oggetto TokenSellers: salvo informazioni su chi vende token, quantità e prezzo
    */
    function createMarketItem(
        uint256 tokenId,
        uint256 price,
        uint256 royalties,
        uint256 supply,
        uint256 amount,
        address payable tokenCreator
    ) external onlyContracts {
        //1)
        NFTMint = IERC1155MetadataURIUpgradeable(msg.sender);
        require(
            NFTMint.balanceOf(tokenCreator, tokenId) >= amount,
            "You do not owned enoght tokens"
        );
        require(
            NFTMint.isApprovedForAll(tokenCreator, address(this)) == true,
            "The contract was not approve to future transfers of tokens"
        );
        //2)
        string memory uri = NFTMint.uri(tokenId);
        tokenIds.increment();
        idToTokenInfo[tokenId][msg.sender] = TokenInfo(
            tokenId,
            msg.sender,
            tokenCreator,
            royalties,
            supply,
            uri,
            0
        );
        //3)
        idToSellers[tokenId][msg.sender].sellers.push(payable(tokenCreator));
        idToSellers[tokenId][msg.sender].sellingQuantity.push(amount);
        idToSellers[tokenId][msg.sender].sellingPrice.push(price);

        smartContractToTokenIds[msg.sender].push(tokenId);

        emit MarketItemCreated(
            tokenId,
            msg.sender,
            tokenCreator,
            address(this),
            price,
            royalties,
            supply
        );
    }

    /*
        Funzione che si occupa di RE-listare un NFT. 
        La funzione viene chiamata dal contratto NFTMint. Può essere chiamata soltanto da contratti "amici". Passaggi principali:
        1) Check che il contratto abbia possibilità di trasferire il token.
        2) Check se il venditore era già registrato: il creatore del token ha abbastanza tokens?
        3) Se già registrato aggiorna quantità in vendita e prezzo, altrimenti crea un nuovo seller.
    */
    function resellTokens(
        uint256 tokenId,
        uint256 amount,
        uint256 price,
        address payable ownerAddress
    ) external onlyContracts {
        //1)
        NFTMint = IERC1155MetadataURIUpgradeable(msg.sender);
        require(
            NFTMint.isApprovedForAll(ownerAddress, address(this)) == true,
            "The contract was not approve transferring tokens"
        );
        int indexSeller = findElementIndexInSellers(
            ownerAddress,
            tokenId,
            msg.sender
        );
        //2)
        // findElementIndexInSellers: ritorna -1 se non esiste un venditore di quel token con quell'address
        //                           altrimenti ritorna il valore dell'indice dell'array in cui è stato trovato
        if (indexSeller == -1) {
            require(
                NFTMint.balanceOf(ownerAddress, tokenId) >= amount,
                "You do not owned enough tokens"
            );
            idToSellers[tokenId][msg.sender].sellers.push(
                payable(ownerAddress)
            );
            idToSellers[tokenId][msg.sender].sellingQuantity.push(amount);
            idToSellers[tokenId][msg.sender].sellingPrice.push(price);
        } else {
            require(
                NFTMint.balanceOf(ownerAddress, tokenId) >=
                    amount +
                        idToSellers[tokenId][msg.sender].sellingQuantity[
                            uint(indexSeller)
                        ],
                "You do not owned enough tokens"
            );
            idToSellers[tokenId][msg.sender].sellingQuantity[
                uint(indexSeller)
            ] += amount;
            idToSellers[tokenId][msg.sender].sellingPrice[
                uint(indexSeller)
            ] = price;
        }

        emit MarketItemReCreated(tokenId, msg.sender, price, ownerAddress);
    }

    /*
        Funzione per delistare un una quantità pari a 1 di un token
        Passaggi principali:
        1) Check che il msg.sender sia attualmente registrato come venditore di un token
        2) Aggiorno informazioni venditori (TokenSellers)
    */
    function cancelOneListedToken(
        uint256 tokenId,
        address contractAddress
    ) public {
        //1)
        int indexSeller = findElementIndexInSellers(
            msg.sender,
            tokenId,
            contractAddress
        );
        require(
            indexSeller != -1 &&
                idToSellers[tokenId][contractAddress].sellingQuantity[
                    uint(indexSeller)
                ] >
                0,
            "To delist a token, you must have listed one"
        );
        //2)
        idToSellers[tokenId][contractAddress].sellingQuantity[
            uint(indexSeller)
        ] -= 1;
        //  Casistica in cui il venditore ha venduto il suo ultimo token => elimina dall'array la sua riga
        if (
            idToSellers[tokenId][contractAddress].sellingQuantity[
                uint(indexSeller)
            ] == 0
        ) {
            cancelRowInTokenSeller(tokenId, contractAddress, indexSeller);
        }
    }

    /*
        Funzione per delistare tutti i token messi in vendita
        Passaggi principali: vedi sopra
    */
    function cancelAllListedTokens(
        uint256 tokenId,
        address contractAddress
    ) public {
        //1)
        int indexSeller = findElementIndexInSellers(
            msg.sender,
            tokenId,
            contractAddress
        );
        require(
            indexSeller != -1 &&
                idToSellers[tokenId][contractAddress].sellingQuantity[
                    uint(indexSeller)
                ] >
                0,
            "To delist tokens, you must have listed one"
        );
        //2)
        idToSellers[tokenId][contractAddress].sellingQuantity[
            uint(indexSeller)
        ] = 0;
        //Il venditore ha venduto il suo ultimo token => elimina dall'array la sua riga
        cancelRowInTokenSeller(tokenId, contractAddress, indexSeller);
    }

    /*
        Funzione per cambiare prezzo di token messo in vendita
        Passaggi principali:
        1) Check che il msg.sender stia vendendo un token
        2) Cambio prezzo
    */
    function changePrice(
        uint256 tokenId,
        address contractAddress,
        uint256 price
    ) public {
        //1)
        int indexSeller = findElementIndexInSellers(
            msg.sender,
            tokenId,
            contractAddress
        );
        require(
            indexSeller != -1 &&
                idToSellers[tokenId][contractAddress].sellingQuantity[
                    uint(indexSeller)
                ] >
                0,
            "To change tokens price, you must have listed one"
        );
        //2)
        idToSellers[tokenId][contractAddress].sellingPrice[
            uint(indexSeller)
        ] = price;
    }

    /*                                                               FUNCTIONS TO PURCHASE NFT                                                    */

    /*
        Funzione per comprare un NFT in Matic/native blockchain currency.
        Passaggi principali:
        1) Trova il venditore del token e verifica che abbia ancora il token, lo stia vendendo e a quale prezzo
        
        N.B.: il prezzo viene dato come input e viene considerato in USDC. Non vogliamo che una persona che vuole vendere il token a 5 euro finisca con il venderlo a 3 o 10.
        Solo quando qualcuno compra il token si calcola la conversione in Matic. 
        Per fare in modo che il prezzo concida con il msg.value c'è bisogno di una uguale exchange rate Matic/USD tra front-end e blockchain. 
        Per ottenerla facciamo in modo che il roundId (misura temporare che indentifica un certo exchange rate) sia uguale. 
        (p.s. funziona tutto utilizzando la nostra front-end).

        2) Calcolo prezzo in Matic con RoundId messo in Input.
        3) Aggiorno informazioni venditori (TokenSellers) e trasferimento token al nuovo propietario (il transferimento del token è all'interno della funzione). 
        4) Divisione del message value tra gli attori 

     */
    function createMarketSaleMatic(
        uint256 tokenId,
        address contractAddress,
        address payable tokenSeller,
        uint80 _roundId
    ) public payable {
        //1)
        int indexSeller = findElementIndexInSellers(
            tokenSeller,
            tokenId,
            contractAddress
        );
        NFTMint = IERC1155MetadataURIUpgradeable(contractAddress);
        if (NFTMint.balanceOf(tokenSeller, tokenId) == 0) {
            cancelRowInTokenSeller(tokenId, contractAddress, indexSeller);
            require(
                NFTMint.balanceOf(tokenSeller, tokenId) >= 1,
                "The seller of the token has already transfer the token"
            );
        }

        require(
            indexSeller != -1 &&
                idToSellers[tokenId][contractAddress].sellingQuantity[
                    uint(indexSeller)
                ] >
                0,
            "To buy the token, the owner must have listed one"
        );
        //2)
        uint256 price = getLatestPriceGivenRound(
            _roundId,
            idToSellers[tokenId][contractAddress].sellingPrice[
                uint(indexSeller)
            ]
        );
        require(
            msg.value == price,
            "Please submit the asking price in order to complete the purchase"
        );
        //3)
        updateIdToOwner(tokenId, contractAddress, tokenSeller);
        //4)
        uint256 royalties = idToTokenInfo[tokenId][contractAddress].royalties;
        uint256 sellerLost = SecondarySalesFee + royalties;
        address payable author = idToTokenInfo[tokenId][contractAddress].author;
        address payable seller = tokenSeller;

        /*  
            Caso seconda vendita
            Il venditore del token non è l'autore del token stesso => è una seconda vendita
                                                O 
            sono stati venduti più token della supply => è una seconda vendita
        */
        if (
            (author != seller) ||
            ((idToTokenInfo[tokenId][contractAddress].numberOfFirstSells ==
                idToTokenInfo[tokenId][contractAddress].supply) &&
                (author == seller))
        ) {
            seller.transfer(msg.value - ((msg.value * sellerLost) / 100));
            author.transfer((msg.value * royalties) / 100);
        } else {
            /*  
            Caso prima vendita
        */
            seller.transfer(msg.value - ((msg.value * FirstSaleFee) / 100));
            idToTokenInfo[tokenId][contractAddress].numberOfFirstSells += 1;
        }

        emit MarketItemSold(
            tokenId,
            contractAddress,
            tokenSeller,
            price,
            msg.sender
        );
    }

    /*
        Funzione che ritorna il prezzo di un NFT in Matic da USD dato un certo round. 
        Per la funzione è richiesto che il roundId sia stato generato in un intervallo di 7 minuti (l'exchange rate deve essere recente)(il roundId è un numero progressivo)
    */
    function getLatestPriceGivenRound(
        uint80 _roundId,
        uint256 USDprice
    ) public view returns (uint256) {
        (uint80 roundID, ) = getLatestPrice();
        require(
            roundID - _roundId < 5,
            "Your transaction has taken too long. Please try again"
        );
        (, int256 price, , , ) = exchangeRateMaticUsd.getRoundData(_roundId);
        uint256 MaticPrice = USDprice * uint256(price) * 10 ** 4;
        return MaticPrice;
    }

    /*
        Funzione che ritorna il più aggiornato Matic/USD exchange rate e il relativo roundID.
     */
    function getLatestPrice() public view returns (uint80, int256) {
        (uint80 roundID, int256 price, , , ) = exchangeRateMaticUsd
            .latestRoundData();
        return (roundID, price);
    }

    /*
        Funzione per comprare un NFT in USDC.
        Questa funzione è preceduta dalla funzione approve() del ERC-20 smart contract permettendo allo smart contract di accedere ai token USDC del compratore.
        Qui non c'è un msg.value ma la quantità di USDC token è passata in una variabile.
        Passaggi principali:
        1) Trova il venditore del token, verifica che abbia ancora il token, lo stia vendendo e a quale prezzo
        2) Verifica che il prezzo coincida con "msg.value"=value.
        3) Aggiorno informazioni venditori (TokenOwners) e trasferimento token al nuovo propietario (il transferimento del token è all'interno della funzione). 
        4) Divisione del message value. Guarda caso matic per maggiori spiegazioni
     */
    function createMarketSaleUSDC(
        uint256 tokenId,
        address contractAddress,
        address payable tokenSeller,
        uint256 value
    ) public {
        //1)
        int indexSeller = findElementIndexInSellers(
            tokenSeller,
            tokenId,
            contractAddress
        );
        NFTMint = IERC1155MetadataURIUpgradeable(contractAddress);
        if (NFTMint.balanceOf(tokenSeller, tokenId) == 0) {
            cancelRowInTokenSeller(tokenId, contractAddress, indexSeller);
            require(
                NFTMint.balanceOf(tokenSeller, tokenId) >= 1,
                "The seller of the token do not own anymore the token"
            );
        }

        require(
            indexSeller != -1 &&
                idToSellers[tokenId][contractAddress].sellingQuantity[
                    uint(indexSeller)
                ] >
                0,
            "To buy the token, the owner must have listed one"
        );
        //2)
        uint256 price = idToSellers[tokenId][contractAddress].sellingPrice[
            uint(indexSeller)
        ];
        require(
            value == price,
            "Please submit the asking price in order to complete the purchase"
        );
        //3)
        updateIdToOwner(tokenId, contractAddress, tokenSeller);

        //4)
        uint256 royalties = idToTokenInfo[tokenId][contractAddress].royalties;
        uint256 sellerLost = SecondarySalesFee + royalties;
        address payable author = idToTokenInfo[tokenId][contractAddress].author;
        address payable seller = tokenSeller;

        if (
            (author != seller) ||
            ((idToTokenInfo[tokenId][contractAddress].numberOfFirstSells >
                idToTokenInfo[tokenId][contractAddress].supply) &&
                (author == seller))
        ) {
            //Second sale
            usdc.transferFrom(
                msg.sender,
                seller,
                value - ((value * sellerLost) / 100)
            );
            usdc.transferFrom(msg.sender, author, (value * royalties) / 100);
            usdc.transferFrom(
                msg.sender,
                address(this),
                (value * SecondarySalesFee) / 100
            );
            // Al contrario è un first sale
        } else {
            // Al contrario è un first sale
            usdc.transferFrom(
                msg.sender,
                seller,
                value - ((value * FirstSaleFee) / 100)
            );
            usdc.transferFrom(
                msg.sender,
                address(this),
                (value * FirstSaleFee) / 100
            );
            idToTokenInfo[tokenId][contractAddress].numberOfFirstSells += 1;
        }
        emit MarketItemSold(
            tokenId,
            contractAddress,
            tokenSeller,
            price,
            msg.sender
        );
    }

    /*
        Funzione chiamata CreateMarketSale con Matic e USDC.
        Passaggi principali:
        1) Trasferisci token al nuovo propietario
        2) Aggiorna dati del venditore del token 
     */
    function updateIdToOwner(
        uint256 tokenId,
        address contractAddress,
        address tokenSeller
    ) private {
        //1)
        NFTMint = IERC1155MetadataURIUpgradeable(contractAddress);
        NFTMint.safeTransferFrom(tokenSeller, msg.sender, tokenId, 1, "");
        //2)
        int indexSeller = findElementIndexInSellers(
            tokenSeller,
            tokenId,
            contractAddress
        );
        idToSellers[tokenId][contractAddress].sellingQuantity[
            uint(indexSeller)
        ] -= 1;
        //  Casistica in cui il venditore ha venduto il suo ultimo token => elimina dall'array la sua riga
        if (
            idToSellers[tokenId][contractAddress].sellingQuantity[
                uint(indexSeller)
            ] == 0
        ) {
            cancelRowInTokenSeller(tokenId, contractAddress, indexSeller);
        }
    }

    /*Function that cancel the row of the arrays in TokenSellers */
    function cancelRowInTokenSeller(
        uint256 tokenId,
        address contractAddress,
        int indexSeller
    ) private {
        for (
            uint j = uint(indexSeller);
            j < idToSellers[tokenId][contractAddress].sellers.length - 1;
            j++
        ) {
            idToSellers[tokenId][contractAddress].sellers[j] = idToSellers[
                tokenId
            ][contractAddress].sellers[j + 1];
            idToSellers[tokenId][contractAddress].sellingQuantity[
                    j
                ] = idToSellers[tokenId][contractAddress].sellingQuantity[
                j + 1
            ];
            idToSellers[tokenId][contractAddress].sellingPrice[j] = idToSellers[
                tokenId
            ][contractAddress].sellingPrice[j + 1];
        }
        idToSellers[tokenId][contractAddress].sellers.pop();
        idToSellers[tokenId][contractAddress].sellingQuantity.pop();
        idToSellers[tokenId][contractAddress].sellingPrice.pop();
    }

    /*
        Funzione chiamata per avere la posizione di un seller nell'array tokenSellers. Ritorna -1 se non esiste. 
     */
    function findElementIndexInSellers(
        address owner,
        uint256 tokenId,
        address contractAddress
    ) private view returns (int) {
        for (
            uint i = 0;
            i < idToSellers[tokenId][contractAddress].sellers.length;
            i++
        ) {
            if (idToSellers[tokenId][contractAddress].sellers[i] == owner) {
                return int(i);
            }
        }
        return -1;
    }

    /*                                                               FUNCTIONS TO FETCH NFT OWNERS,...                                                    */

    /*
        Funzione chiamata per ottenere tutti i venditori di un NFT 
     */
    function getSellers(
        uint256 tokenId,
        address contractAddress
    ) public view returns (TokenSellers memory) {
        return idToSellers[tokenId][contractAddress];
    }

    /*
        Funzione chiamata per ottenere tutti gli nft relativi a un certo contratto messi in vendita da un address 
     */
    function getTokensCurrentlySelling(
        address owner,
        address contractAddress
    ) public view returns (uint256[] memory, uint[] memory) {
        uint256 lunghezza = smartContractToTokenIds[contractAddress].length;
        uint256 lastTokenId = smartContractToTokenIds[contractAddress][
            lunghezza - 1
        ];
        uint256[] memory tokenIdArray = new uint256[](lastTokenId);
        uint[] memory amountTokenSelling = new uint[](lastTokenId);
        for (uint i = 0; i < lastTokenId; i++) {
            tokenIdArray[i] = i + 1;
            int index = findElementIndexInSellers(
                owner,
                i + 1,
                contractAddress
            );
            if (index != -1) {
                amountTokenSelling[i] = idToSellers[i + 1][contractAddress]
                    .sellingQuantity[uint(index)];
            } else {
                amountTokenSelling[i] = 0;
            }
        }
        return (tokenIdArray, amountTokenSelling);
    }

    /*
        Funzione che dato un NFTMint smart contract ritorna un array di tutti i tokenId creati 
     */
    function getTokenIds(
        address smartContract
    ) public view returns (uint256[] memory) {
        return smartContractToTokenIds[smartContract];
    }

    function withdraw() public payable onlyOwner {
        _owner.transfer(address(this).balance);
    }
}

