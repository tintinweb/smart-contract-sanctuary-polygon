    address _burnTokenAddress,
    address _cardPacksAddress,
    address _adventurersAddress,
    address _ethernalsAddress,
    // address _cardBacksAddress,
    address _cardsAddress,
    uint64 subscriptionId
  )
    VRFConsumerBaseV2(0x7a1BaC17Ccc5b313516C5E16fb24f7659aA5ebed)
    ConfirmedOwner(msg.sender)
  {
    COORDINATOR = VRFCoordinatorV2Interface( // <------------------NEED TO SWITCH FOR VRF
      0x7a1BaC17Ccc5b313516C5E16fb24f7659aA5ebed //mumbai
      // 0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266 //local test
    );
    s_subscriptionId = subscriptionId;

    aegAddress = _aegAddress;
    burnTokenAddress = _burnTokenAddress;
    cardPacksAddress = _cardPacksAddress;
    adventurersAddress = _adventurersAddress;
    ethernalsAddress = _ethernalsAddress;
    // cardBacksAddress = _cardBacksAddress;
    cardsAddress = _cardsAddress;
  }

  /** BURN INTERACTION **/
  address public aegAddress;
  address public cardPacksAddress;
  address public adventurersAddress;
  address public ethernalsAddress;
  // address public cardBacksAddress;
  address public cardsAddress;
  address public burnTokenAddress;

  function setCardPacksAddress(address _address) external onlyOwner {
    cardPacksAddress = _address;
  }

  function setAdventurersAddress(address _address) external onlyOwner {
    adventurersAddress = _address;
  }

  // function setCardBacksAddress(address _address) public onlyOwner {
  //   cardBacksAddress = _address;
  // }

  function setEthernalsAddress(address _address) public onlyOwner {
    ethernalsAddress = _address;
  }

  function setBurnTokenAddress(address _address) public onlyOwner {
    burnTokenAddress = _address;
  }

  function setCardsAddress(address _address) public onlyOwner {
    cardsAddress = _address;
  }

  /** MINTING **/
  //mapping for address to minted token count
  mapping(address => uint256) public addressOpenedFP;

  // function getTokenIdFromBurnContract() public view returns (uint256) {
  //   uint256 tokenId = FunctionInterface(burnTokenAddress).tokenOfOwnerByIndex(
  //     msg.sender,
  //     0
  //   );
  //   return tokenId;
  // }

  /** FOUNDER PACK OPENING **/

  function startOpenFP() public nonReentrant whenNotPaused {
    require(
      FunctionInterface(burnTokenAddress).balanceOf(msg.sender, 1) > 0,
      "No pack."
    );
    requestRandomWords(AssetType.FounderPack);
    console.log("STARTING OPENING FP");
  }

  function finishOpenFP() public nonReentrant {
    console.log("FINISHING OPENING FP");
    require(
      FunctionInterface(burnTokenAddress).balanceOf(msg.sender, 1) > 0,
      "No pack."
    );
    uint256[] memory fulfilledNotUsed = getFulfilledByAddressAndType(
      msg.sender,
      AssetType.FounderPack
    );
    require(fulfilledNotUsed.length > 0, "No fulfilled requests.");

    //get the random words with the request id (there should be 3) TODO: check there are enough random words fior each asset type!
    uint256[] memory randomWords = s_requests[fulfilledNotUsed[0]].randomWords;

    //burn fp
    FunctionInterface(burnTokenAddress).burn(msg.sender, 1, 1);

    //mint new tokens
    console.log("minting basic...");
    FunctionInterface(cardPacksAddress).fpMintBasic(msg.sender);

    console.log("minting ethernal...");
    FunctionInterface(ethernalsAddress).fpMint(
      msg.sender,
      randomWords[0] % FunctionInterface(ethernalsAddress).totalTokens(),
      1
    );
    console.log("minting adventurer...");
    FunctionInterface(adventurersAddress).fpMint(
      msg.sender,
      randomWords[1] % FunctionInterface(adventurersAddress).totalTokens(),
      1
    );

    uint256[] memory cardsToMint = new uint256[](7);
    uint256[] memory cardsToMintAmounts = new uint256[](7);

    //MINT 5 common rarity cards
    uint256[] memory commonCardIds = FunctionInterface(cardsAddress)
      .getCardIdsOfRarity(Library.Rarity.Common);
    for (uint256 i = 0; i < 5; i++) {
      uint256 randomCardId = commonCardIds[
        randomWords[2 + i] % commonCardIds.length
      ];
      cardsToMint[i] = randomCardId;
      cardsToMintAmounts[i] = 1;
    }

    //MINT 1 uncommon rarity card
    uint256[] memory uncommonCardIds = FunctionInterface(cardsAddress)
      .getCardIdsOfRarity(Library.Rarity.Uncommon);
    uint256 randomUncommonCardId = uncommonCardIds[
      randomWords[7] % uncommonCardIds.length
    ];
    cardsToMint[5] = randomUncommonCardId;
    cardsToMintAmounts[5] = 1;

    //MINT 1 rare/epic/leggo PROMO card
    uint256[] memory promoCardIds = FunctionInterface(cardsAddress)
      .getPromoCardIds();
    //choose 1 random one
    uint256 randomPromoCardId = promoCardIds[
      randomWords[8] % promoCardIds.length
    ];
    cardsToMint[6] = randomPromoCardId;
    cardsToMintAmounts[6] = 1;
    FunctionInterface(cardsAddress).mintBatch(
      msg.sender,
      cardsToMint,
      cardsToMintAmounts
    );
    console.log("minted cards...", cardsToMint.length);

    // console.log(
    //   "ethernals id",
    //   randomWords[0] % FunctionInterface(ethernalsAddress).totalTokens()
    // );
    // console.log(
    //   "adventurers id",
    //   randomWords[1] % FunctionInterface(adventurersAddress).totalTokens()
    // );

    addressOpenedFP[msg.sender]++;
  }

  /** OTHER ASSETS MINTING **/

  function startMintRandom(
    AssetType _assetType
  ) public nonReentrant whenNotPaused {
    //only allow adventurer and ethernal for now
    require(
      _assetType == AssetType.Adventurer || _assetType == AssetType.Ethernal,
      "Invalid asset type."
    );
    requestRandomWords(_assetType);
  }

  function finishMintRandom(AssetType _assetType) public nonReentrant {
    //need to check asset type they wish to finish opening
    uint256[] memory requests = getFulfilledByAddressAndType(
      msg.sender,
      _assetType
    );
    require(requests.length > 0, "No fulfilled requests.");
    //require they have enough aeg tokens to open
    address _address;
    if (_assetType == AssetType.Adventurer) {
      _address = adventurersAddress;
    } else if (_assetType == AssetType.Ethernal) {
      _address = ethernalsAddress;
      // } else if (_assetType == AssetType.CardBack) {
      //   _address = cardBacksAddress;
    } else {
      revert("Invalid asset type.");
    }
    uint256 assetPrice = FunctionInterface(_address).randomPrice();

    require(
      AEGInterface(aegAddress).balanceOf(msg.sender) >= assetPrice,
      "Not enough AEG."
    );
    //transfer funds to this contract or owner
    FunctionInterface(aegAddress).transferFrom(
      msg.sender,
      address(this),
      assetPrice
    ); //CHECK TO BE SURE THIS WORKS -----------------------------IT DOESN"T
    FunctionInterface(_address).fpMint(
      msg.sender,
      s_requests[requests[0]].randomWords[0] %
        FunctionInterface(_address).totalTokens(),
      1
    );
    console.log(
      "FINSHED MINTING RANDOM ID: ",
      s_requests[requests[0]].randomWords[0] %
        FunctionInterface(_address).totalTokens()
    );
  }

  /** GETTERS **/
  // frontend can use this to display how many packs are available to open
  function getFulfilledByAddressAndType(
    address _address,
    AssetType _assetType
  ) public view returns (uint256[] memory) {
    //get all s_userRequests for this address
    uint256[] memory requestIds = s_userRequests[_address];
    uint256[] memory fulfilledRequestIds = new uint256[](requestIds.length);
    uint256 fulfilledRequestIdsCount = 0;
    for (uint256 i = 0; i < requestIds.length; i++) {
      if (
        s_requests[requestIds[i]].fulfilled &&
        !s_requests[requestIds[i]].used &&
        s_requests[requestIds[i]].assetType == _assetType
      ) {
        fulfilledRequestIds[fulfilledRequestIdsCount] = requestIds[i];
        fulfilledRequestIdsCount++;
      }
    }
    //remove empty elements from array
    uint256[] memory returnArray = new uint256[](fulfilledRequestIdsCount);
    for (uint256 i = 0; i < fulfilledRequestIdsCount; i++) {
      returnArray[i] = fulfilledRequestIds[i];
    }

    return returnArray;
  }

  /** ACTIVATION **/

  function pause() public onlyOwner {
    _pause();
  }

  function unpause() public onlyOwner {
    _unpause();
  }

  function requestRandomWords(
    AssetType _assetType
  ) internal returns (uint256 requestId) {
    //get number of words to return from asset type
    uint32 numWords = 10; // FP requires 9
    if (
      _assetType == AssetType.Ethernal ||
      _assetType == AssetType.Adventurer ||
      _assetType == AssetType.CardBack
    ) {
      numWords = 1;
    }

    //TEST REQUEST ID
    // requestId = 83132445710567745731713214319446805456689061274219614556348005953705680908145; // <----------------TESTING

    // Will revert if subscription is not set and funded. <------------------PROD: NEED TO UNCOMMENT FOR VRF
    requestId = COORDINATOR.requestRandomWords(
      keyHash,
      s_subscriptionId,
      requestConfirmations,
      callbackGasLimit,
      numWords //number of words to return
    );
    // console.log("msg.sender in requestRandomWords", msg.sender);
    s_requests[requestId] = RequestStatus({
      opener: msg.sender,
      randomWords: new uint256[](0),
      exists: true,
      fulfilled: false,
      used: false,
      assetType: _assetType
    });
    s_userRequests[msg.sender].push(requestId);
    // console.log(s_userRequests[msg.sender][0]);
    // emit RequestSent(requestId, numWords);
    return requestId;
  }

  function fulfillRandomWords(
    uint256 _requestId,
    uint256[] memory _randomWords
  ) internal override {
    // console.log("IN FULFILLRANDOMWORDS");
    require(s_requests[_requestId].exists, "request not found");
    s_requests[_requestId].fulfilled = true;
    s_requests[_requestId].randomWords = _randomWords;

    // console.log("fulfillRandomWords DONE", _requestId, _randomWords.length);
    emit RequestFulfilled(
      s_requests[_requestId].opener,
      s_requests[_requestId].assetType
    );
  }

  //setGasLimit
  function setCallbackGasLimit(uint32 _callbackGasLimit) public onlyOwner {
    callbackGasLimit = _callbackGasLimit;
  }
}

