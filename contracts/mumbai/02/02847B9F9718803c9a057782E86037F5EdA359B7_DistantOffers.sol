contract DistantOffers is Context, Ownable, Pausable, ReentrancyGuard {
   // Libraries
    using EnumerableSet for EnumerableSet.AddressSet;
    using Address for address;
    using SafeERC20 for IERC20;

   // Variables
    address private immutable WKCS; // Native ERC20 token for trades
    uint16 private tradeFee; // marketplace fee
    address private admin; // marketplace controller
    IMarketplace private Marketplace; // Marketplace Contract address for the IMarketplace interface
   // Events
    event ItemSold(
        address indexed seller,
        address indexed collection,
        address indexed buyer,
        uint256 tokenId,
        uint256 price
    );
    event TokenOfferCreated(
        address indexed creator,
        address indexed collection,
        uint256 tokenId,
        uint256 value,
        uint256 validity
    );
    event TokenOfferUpdated(
        address indexed creator,
        address indexed collection,
        uint256 tokenId,
        uint256 value,
        uint256 validity
    );
    event TokenOfferCancelled(address indexed creator, address indexed collection, uint256 tokenId);
    event TokenOfferAccepted(
        address indexed seller,
        address indexed creator,
        address indexed collection,
        uint256 token,
        uint256 value
    );
    event CollectionOfferCreated(
        address indexed creator,
        address indexed collection,
        uint256 value,
        uint256 total,
        uint256 validity
    );
    event CollectionOfferDeleted(
        address indexed creator,
        address indexed collection,
        uint256 value
    );
    event CollectionOfferAccepted(
        address indexed creator,
        address indexed seller,
        address indexed collection,
        uint256 tokenId,
        uint256 value
    );
    event TradeFeeUpdated(uint256 fees);

   // Struct
    struct TokenOffer {
        uint256 price;
        uint256 validity;
    }
    struct CollectionOffer {
        uint256 amount;
        uint256 totalPurchase;
        uint256 validity;
    }

   // Mappings
    // mapping from a collection to a tokenId to an offer creator which maps to the details of the Offer created
    mapping(address => mapping(uint256 => mapping(address => TokenOffer)))
        private tokenOffer;
    // mapping from a collection to a tokenId that stores all addresses that has created an offer for that NFT. Used to track and limit users from creating multiple offer instances and allow offer updates only if the offer was created before
    mapping(address => mapping(uint256 => EnumerableSet.AddressSet))
        private tokenOfferCreators;
    // mapping collection address to creator to Collection Offers struct
    mapping(address => mapping(address => CollectionOffer))
        private collectionOffer;
    // mapping from a collection to to an enumerable set of all addresses that has created an offer for the collection. Used to track and limit users from creating multiple offer instances and allow offer updates only if the offer was created before
    mapping(address => EnumerableSet.AddressSet)
        private collectionOffersCreators;

    constructor(
        uint16 _tradeFee,
        address _WKCS,
        address _Marketplace
    ) {
        tradeFee = _tradeFee;
        admin = _msgSender();
        WKCS = _WKCS;
        Ownable(_msgSender());
        Marketplace = IMarketplace(_Marketplace);
    }

   // Modifiers
    modifier isTokenOfferValid(
        address _collection,
        uint256 _tokenId,
        uint256 _price
    ) {
        Collection memory collection = IMarketplace(Marketplace).getCollectionData(
            _collection
        );
        require(
            collection.collectionAddress != address(0)
        );
        require(
            IERC721(_collection).ownerOf(_tokenId) != _msgSender()
        );
        require(!tokenOfferCreators[_collection][_tokenId].contains(_msgSender()));
        require(_price > 0);
        _;
    }
    modifier isTokenOfferUpdateable(
        address _collection,
        uint256 _tokenId,
        uint256 _price
    ) {
        
        require(tokenOfferCreators[_collection][_tokenId].contains(_msgSender()));
        require(_price > 0);
        _;
    }
    modifier isTokenOfferCancellable(address _collection, uint256 _tokenId) {
        require(
            tokenOfferCreators[_collection][_tokenId].contains(_msgSender()));
        require(
            block.timestamp >
                tokenOffer[_collection][_tokenId][_msgSender()].validity        );
        _;
    }
    modifier isTokenOfferAcceptable(
        address _collection,
        uint256 _tokenId,
        address _buyer
    ) {
        require(
            IERC721(_collection).ownerOf(_tokenId) == _msgSender()
        );
        require(
            tokenOfferCreators[_collection][_tokenId].contains(_buyer)
        );
        require(
            block.timestamp < tokenOffer[_collection][_tokenId][_buyer].validity        );
        _;
    }
    modifier isCollectionOfferCancellable(address _collection) {
        require(
            collectionOffersCreators[_collection].contains(_msgSender())
        );
        require(
            block.timestamp > collectionOffer[
            _collection
        ][_msgSender()].validity        );
        _;
    }
    modifier collectionOfferCanBeCreated(
        address _collection,
        uint256 _validity
    ) {
        Collection memory collection = IMarketplace(Marketplace).getCollectionData(
            _collection
        );
        require(
            collection.collectionAddress != address(0)
        );
        require(_validity > 6);
        require(
            !collectionOffersCreators[_collection].contains(_msgSender())
        );
        _;
    }
    modifier canInstantSell(
        address _collection,
        address _buyer,
        uint256 _tokenId
    ) {
        require(
            collectionOffersCreators[_collection].contains(_buyer)
        );
        require(
            block.timestamp < collectionOffer[_collection][_buyer].validity
        );
        _;
    }
    modifier isProxy() {
        require(_msgSender() == admin);
        _;
    }
    modifier isProxyCollectionOfferRemovable(address _collection, address _creator) {
        require(
            block.timestamp > collectionOffer[_collection][_creator].validity
        );
        _;
    }
    modifier isProxyTokenOfferRemovable(
        address _collection,
        address _creator,
        uint256 _tokenId
    ) {
        require(
            block.timestamp > tokenOffer[_collection][_tokenId][_creator].validity
        );
        _;
    }

   // Read functions
    /**
     * @notice Generate all offers available for a tokenId in a collection
     * @param _collection address to check offers from
     * @param _tokenId uint256 to check offers from
     */
    function getTokenOffers(address _collection, uint256 _tokenId)
        public
        view
        returns (TokenOffer[] memory tokenOfferInfo)
    {
        uint256 length = tokenOfferCreators[_collection][_tokenId].length();
        tokenOfferInfo = new TokenOffer[](length);
        for (uint256 i = 0; i < length;) {
            address creator = tokenOfferCreators[_collection][_tokenId].at(i);
            tokenOfferInfo[i] = tokenOffer[_collection][_tokenId][creator];
            unchecked{i++;}
        }
    }

    function getCollectionOffers(address _collection)
        external
        view
        returns (CollectionOffer[] memory _collectionOffers)
    {
        uint256 length = collectionOffersCreators[_collection].length();
        _collectionOffers = new CollectionOffer[](length);
        for (uint256 i; i < length;) {
            address creator = collectionOffersCreators[_collection].at(i);
            _collectionOffers[i] = collectionOffer[_collection][creator];
            unchecked{i++;}
        }
    }

    function createTokenOfferInKCS(
        address _collection,
        uint256 _tokenId,
        uint256 _validity
    ) external payable {
        IWKCS(WKCS).deposit{value: msg.value}();
        _createTokenOffer(_collection, _tokenId, msg.value, _validity);
    }

    function createTokenOfferInWKCS(
        address _collection,
        uint256 _tokenId,
        uint256 _value,
        uint256 _validity
    ) external {
        IERC20(WKCS).safeTransferFrom(_msgSender(), address(this), _value);
        _createTokenOffer(_collection, _tokenId, _value, _validity);
    }

    /**
     * @notice Create an Offer for a NFT
     * @param _collection address to create an offer for
     * @param _tokenId uint256 of the tokenId
     * @param _value uint256 of the offer value
     */
    function _createTokenOffer(
        address _collection,
        uint256 _tokenId,
        uint256 _value,
        uint256 _validity
    )
        private
        whenNotPaused
        nonReentrant
        isTokenOfferValid(_collection, _tokenId, _value)
    {
        uint validity = block.timestamp + _validity * 1 days;
        tokenOffer[_collection][_tokenId][_msgSender()] = TokenOffer(_value, validity);
        tokenOfferCreators[_collection][_tokenId].add(_msgSender());
        emit TokenOfferCreated(_msgSender(), _collection, _tokenId, _value, validity);
    }

    function updateTokenOfferInKCS(address _collection, uint256 _tokenId)
        external
        payable
    {
        IWKCS(WKCS).deposit{value: msg.value}();
        _updateTokenOffer(_collection, _tokenId, msg.value);
    }

    function updateTokenOfferInWKCS(
        address _collection,
        uint256 _tokenId,
        uint256 _newValue
    ) external {
        IERC20(WKCS).safeTransferFrom(_msgSender(), address(this), _newValue);
        _updateTokenOffer(_collection, _tokenId, _newValue);
    }

    function _updateTokenOffer(
        address _collection,
        uint256 _tokenId,
        uint256 _newValue
    )
        private
        whenNotPaused
        nonReentrant
        isTokenOfferUpdateable(_collection, _tokenId, _newValue)
    {
        TokenOffer storage newTokenOffer = tokenOffer[_collection][_tokenId][_msgSender()];
        newTokenOffer.price += _newValue;
        newTokenOffer.validity += 7 days;
        emit TokenOfferUpdated(_msgSender(), _collection, _tokenId, newTokenOffer.price, newTokenOffer.validity);
    }

    /**
     * @notice Cancel an Offer for a NFT
     * @param _collection address to cancel an offer for
     * @param _tokenId uint256 of the tokenId
     */
    function cancelTokenOffer(address _collection, uint256 _tokenId)
        external
        nonReentrant
        isTokenOfferCancellable(_collection, _tokenId)
    {
        _cancelTokenOffer(_collection, _tokenId, _msgSender());
    }

    function _cancelTokenOffer(
        address _collection,
        uint256 _tokenId,
        address _creator
    ) private {
        uint256 amount = tokenOffer[_collection][_tokenId][_creator].price;
        delete (tokenOffer[_collection][_tokenId][_creator]);
        tokenOfferCreators[_collection][_tokenId].remove(_creator);
        IERC20(WKCS).safeTransfer(_creator, amount);
        emit TokenOfferCancelled(_creator, _collection, _tokenId);
    }

    /**
     * @notice Accept an Offer for a NFT
     * @param _collection address to accept an offer for
     * @param _tokenId uint256 of the tokenId
     * @param _creator address of the offer creator
     */
    function acceptTokenOffer(
        address _collection,
        uint256 _tokenId,
        address _creator
    )
        external
        whenNotPaused
        nonReentrant
        isTokenOfferAcceptable(_collection, _tokenId, _creator)
    {
        uint256 value = tokenOffer[_collection][_tokenId][_creator].price;
        delete (tokenOffer[_collection][_tokenId][_creator]);
        tokenOfferCreators[_collection][_tokenId].remove(_creator);
        Marketplace.delist(_collection, _tokenId);
        _acceptTokenOffer(_collection, _tokenId, value, _creator);
    }

    function _acceptTokenOffer(
        address _collection,
        uint256 _tokenId,
        uint256 _price,
        address _creator
    ) private {
        (
            uint256 amount,
            uint256 marketplaceFee,
            uint256 collectionFee,
            uint256 tokenFee
        ) = Marketplace.calculateFee(_collection, _tokenId, _price, tradeFee);
        uint256 totalFees = marketplaceFee + collectionFee + tokenFee;
        IERC20(WKCS).safeTransfer(address(Marketplace), totalFees);
        Marketplace.updateRevenue(
            marketplaceFee,
            collectionFee,
            tokenFee,
            _collection,
            _tokenId
        );
        IERC20(WKCS).safeTransfer(_msgSender(), amount);
        IERC721(_collection).safeTransferFrom(
            _msgSender(),
            _creator,
            _tokenId
        );
        emit TokenOfferAccepted(
            _msgSender(),
            _creator,
            _collection,
            _tokenId,
            _price
        );
    }

    function pauseMarketplace() external whenNotPaused onlyOwner {
        _pause();
    }

    function unpauseMarketplace() external whenPaused onlyOwner {
        _unpause();
    }

    function recoverToken(address _token)
        external
        whenPaused
        onlyOwner
    {
        require(_token != WKCS, "Cannot recover");
        if(_token == address(0)){
            uint etherBalance = address(this).balance;
            require(etherBalance > 0, "Ether balance is 0");
            (bool sent, ) = admin.call{value: etherBalance}("");
            require(sent, "Failed to send Ether");
        } else{
            IERC20 token = IERC20(_token);
            uint balanceOf = token.balanceOf(address(this));
            token.safeTransfer(_msgSender(), balanceOf);
        }
    }

    function createCollectionOfferInKCS(
        address _collection,
        uint256 _total,
        uint256 _validity
    ) external payable {
        IWKCS(WKCS).deposit{value: msg.value}();
        _createCollectionOffer(_collection, msg.value, _total, _validity);
    }

    function createCollectionOfferInWKCS(
        address _collection,
        uint256 _amount,
        uint256 _total,
        uint256 _validity
    ) external {
        IERC20(WKCS).safeTransferFrom(_msgSender(), address(this), _amount);
        _createCollectionOffer(_collection, _amount, _total, _validity);
    }

    function _createCollectionOffer(
        address _collection,
        uint256 _amount,
        uint256 _total,
        uint256 _validity
    )
        private
        whenNotPaused
        nonReentrant
        collectionOfferCanBeCreated(_collection, _validity)
    {
        require(_amount > 0);
        uint validity = block.timestamp + _validity * 1 days;
        collectionOffer[_collection][_msgSender()] = CollectionOffer(
            _amount,
            _total,
            validity
        );
        collectionOffersCreators[_collection].add(_msgSender());
        emit CollectionOfferCreated(_msgSender(), _collection, _amount, _total, validity);
    }

    function cancelCollectionOffer(address _collection)
        external
        whenNotPaused
        nonReentrant
        isCollectionOfferCancellable(_collection)
    {
        _cancelCollectionOffer(_collection, _msgSender());
    }

    function _cancelCollectionOffer(address _collection, address _creator) private {
        CollectionOffer storage _collectionOffers = collectionOffer[
            _collection
        ][_creator];
        uint256 amount = _collectionOffers.amount;
        collectionOffersCreators[_collection].remove(_creator);
        delete (collectionOffer[_collection][_creator]);
        IERC20(WKCS).safeTransfer(_creator, amount);
        emit CollectionOfferDeleted(_creator, _collection, amount);
    }

    function instantSale(
        address _buyer,
        address _collection,
        uint256 _tokenId
    )
        external
        whenNotPaused
        nonReentrant
        canInstantSell(_collection, _buyer, _tokenId)
    {
        CollectionOffer storage _collectionOffers = collectionOffer[
            _collection
        ][_buyer];
        uint256 total = _collectionOffers.totalPurchase;
        uint256 amount = _collectionOffers.amount;
        uint256 value = amount / total;
        if (total > 1) {
            _collectionOffers.totalPurchase -= 1;
            _collectionOffers.amount -= value;
        } else {
            collectionOffersCreators[_collection].remove(_buyer);
            delete (collectionOffer[_collection][_buyer]);
        }
        _instantSell(_collection, _tokenId, value, _buyer);
        emit CollectionOfferAccepted(
            _buyer,
            _msgSender(),
            _collection,
            _tokenId,
            value
        );
    }

    function _instantSell(
        address _collection,
        uint256 _tokenId,
        uint256 _price,
        address _buyer
    ) private {
        Marketplace.delist(_collection, _tokenId);
        (
            uint256 amount,
            uint256 marketplaceFee,
            uint256 collectionFee,
            uint256 tokenFee
        ) = Marketplace.calculateFee(_collection, _tokenId, _price, tradeFee);
        uint256 totalFees = marketplaceFee + collectionFee + tokenFee;
        IERC20 token = IERC20(WKCS);
        token.safeTransfer(address(Marketplace), totalFees);
        Marketplace.updateRevenue(
            marketplaceFee,
            collectionFee,
            tokenFee,
            _collection,
            _tokenId
        );
        IERC721(_collection).safeTransferFrom(_msgSender(), _buyer, _tokenId);
        token.safeTransfer(_msgSender(), amount);
    }

    function cartWithKCS(
        address[] calldata _collections,
        uint256[][] calldata _tokens
    ) external payable {
        IWKCS(WKCS).deposit{value: msg.value}();
        _cartMultipleTokens(_collections, _tokens, msg.value);
    }

    function cartWithWKCS(
        address[] calldata _collections,
        uint256[][] calldata _tokens,
        uint256 _price
    ) external {
        IERC20(WKCS).safeTransferFrom(
            msg.sender,
            address(this),
            _price
        );
        _cartMultipleTokens(_collections, _tokens, _price);
    }

    function _cartMultipleTokens(
        address[] calldata _collections,
        uint256[][] calldata _tokens,
        uint256 amount
    )
        private
        whenNotPaused
        nonReentrant
    {
        uint256 collectionCount = _collections.length;
        for (uint256 i; i < collectionCount;) {
            address collection = _collections[i];
            uint256 tokenCount = _tokens[i].length;
            for (uint256 j; j < tokenCount;) {
                uint256 tokenId = _tokens[i][j];
                Listing memory listingData = Marketplace.getListing(collection, tokenId);
                address seller = listingData.seller;
                uint256 price = listingData.price;
                require(amount >= price);
                IERC20(WKCS).safeTransfer(address(Marketplace), price);
                try Marketplace.buyNFT(collection, tokenId, price, _msgSender()) {
                    amount -= price;
                    emit ItemSold(seller, collection, _msgSender(), tokenId, price);
                } catch (bytes memory) {
                    continue;
                }
                unchecked{j++;}
            }
            unchecked{i++;}
        }
        if (amount > 0) IERC20(WKCS).safeTransfer(_msgSender(), amount);
    }

    function updateMarketplaceAddress(address _Marketplace) external onlyOwner {
        Marketplace = IMarketplace(_Marketplace);
    }

    function updateTradeFee(uint8 _newTradeFee) external whenPaused onlyOwner {
        tradeFee = _newTradeFee;
        emit TradeFeeUpdated(_newTradeFee);
    }

    function updateAdmin(address _newAdmin) external whenPaused onlyOwner {
        admin = _newAdmin;
    }

    function proxyRemoveCollectionOffer(address _collection, address _creator)
        external
        isProxy
        isProxyCollectionOfferRemovable(_collection, _creator)
    {
        _cancelCollectionOffer(_collection, _creator);
    }

    function proxyRemoveTokenOffer(
        address _collection,
        address _user,
        uint256 _tokenId
    ) external isProxy isProxyTokenOfferRemovable(_collection, _user, _tokenId) {
        _cancelTokenOffer(_collection, _tokenId, _user);
    }
}