// File: LK.sol



pragma solidity ^0.8.19;









interface IUSDT {
    function balanceOf(address who) external view returns (uint);

    function transfer(address to, uint value) external;

    function allowance(
        address owner,
        address spender
    ) external view returns (uint);

    function transferFrom(address from, address to, uint value) external;
}

error invalidAmount();
error increaseAllowance();
error maxPurchased();

contract LuckyKey is VRFConsumerBaseV2, ConfirmedOwner {


    using SafeMath for uint256;
    using Strings for uint256;
    using Counters for Counters.Counter;
    
    event RequestSent(uint256 requestId, uint32 numWords);
    event RequestFulfilled(uint256 requestId, uint256[] randomWords);

    struct RequestStatus {
        bool fulfilled; // whether the request has been successfully fulfilled
        bool exists; // whether a requestId exists
        uint256[] randomWords;
    }

    struct Treasure {
        address treasureWinner;
        uint256 revealDate;
        uint256 treasurePrize;
        uint256 totalBottles;
        uint256 bottlesSold;
        uint256 bottlePrice;
        string  treasureKey;
        string[] treasureTickets;
        mapping(string => address) codedKeys;
    }

    uint32 constant callbackGasLimit = 150000;

    uint16 constant requestConfirmations = 3;
    uint32 constant numWords = 1;



    uint64 immutable s_subscriptionId;
    bytes32 immutable keyHash;

    // past requests Id.
    uint256[] public requestIds;
    uint256 public lastRequestId;


    IUSDT public immutable USDT;
    address public immutable linkToken;
    VRFCoordinatorV2Interface immutable COORDINATOR;



    constructor(address _USDT,uint64 subscriptionId,address _linkToken) 
        VRFConsumerBaseV2(0x2Ca8E0C643bDe4C2E08ab1fA0da3401AdAD7734D)
        ConfirmedOwner(msg.sender) {

            USDT = IUSDT(_USDT);


            COORDINATOR = VRFCoordinatorV2Interface(
            0x2Ca8E0C643bDe4C2E08ab1fA0da3401AdAD7734D
            );

            s_subscriptionId = subscriptionId;

            keyHash = 0x79d3d8832d904592c0bf9818b621522c988bb8b0c05cdc3b15aea1b6e8db0c15; // we alread set this
            linkToken = _linkToken;

    }

    address constant INVESTOR_WALLET =
        0x5B38Da6a701c568545dCfcB03FcB875f56beddC4; // The address of the investor wallet.
    address constant GAME_REWARD_WALLET =
        0x5B38Da6a701c568545dCfcB03FcB875f56beddC4; // The address of the game reward wallet.
    address constant DONATION_WALLET =
        0x5B38Da6a701c568545dCfcB03FcB875f56beddC4; // The address of the charity donation wallet.
    address constant OP_MRK_DV_WALLET = 0x5B38Da6a701c568545dCfcB03FcB875f56beddC4; // The address of the operations/marketing/development wallet.


    mapping (uint256=>Treasure) public treasures;
    mapping(address => mapping(uint256 => string[])) private playerStats;
    mapping(uint256 => RequestStatus) public s_requests; /* requestId --> requestStatus */
    string[] private Winners;
    Counters.Counter public totalTreasures;

    function addNewTreasure(uint256 _revealDate, uint256 _treasurePrize, uint256 _totalBottles, uint256 _bottlePrice, string calldata _treasureKey) public onlyOwner {
        if(_revealDate <= block.timestamp) revert invalidAmount();
        if(_treasurePrize == 0) revert invalidAmount();
        if(_totalBottles == 0) revert invalidAmount();
        if(_bottlePrice == 0) revert invalidAmount();
        if(_totalBottles > 10000) revert invalidAmount();
        
        totalTreasures.increment();

        Treasure storage _treasure = treasures[totalTreasures.current()];
        
        _treasure.revealDate = _revealDate;
        _treasure.treasurePrize = _treasurePrize;
        _treasure.totalBottles = _totalBottles;
        _treasure.bottlePrice = _bottlePrice;
        _treasure.treasureKey = string(
            abi.encodePacked(_treasureKey, "-000")
        );

    }

    function purchaseBottle(uint256 _selectTreasure) external {
        if (_selectTreasure > totalTreasures.current()) revert invalidAmount();
        if (treasures[_selectTreasure].bottlesSold >= treasures[_selectTreasure].totalBottles) revert maxPurchased();
        if (treasures[_selectTreasure].revealDate == 0)  revert invalidAmount();

        uint256 totalPrice = treasures[_selectTreasure].bottlePrice;

        uint allowed = USDT.allowance(msg.sender, address(this));
        if(allowed < totalPrice) revert increaseAllowance();
    
        // TODO
        // push ticket to treasures ticket string array
        // assign that ticket string to address in coded keys mapping
        //  push that ticket to player stats mapping
        // generate player ticket and use current soldbottle number as its key don't make another private counter
       
        treasures[_selectTreasure].bottlesSold++;
        string memory ticket = string(abi.encodePacked(treasures[_selectTreasure].treasureKey, treasures[_selectTreasure].bottlesSold.toString()));
        treasures[_selectTreasure].treasureTickets.push(ticket);
        treasures[_selectTreasure].codedKeys[ticket] = msg.sender;

        playerStats[msg.sender][_selectTreasure].push(ticket);



        // Transfer the USDT tokens to the appropriate wallets
        transferUSDTtoWallets(totalPrice);

    }

     function requestRandomWords() public onlyOwner returns (uint256 requestId) {
        requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );
        s_requests[requestId] = RequestStatus({
            randomWords: new uint256[](0),
            exists: true,
            fulfilled: false
        });
        requestIds.push(requestId);
        lastRequestId = requestId;
        emit RequestSent(requestId, numWords);
        return requestId; // requestID is a uint.
    }

    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory _randomWords
    ) internal override {
        require(s_requests[_requestId].exists, "request not found");
        s_requests[_requestId].fulfilled = true;
        s_requests[_requestId].randomWords = _randomWords;
        // randomWordsNum = _randomWords[0]; // Set array-index to variable, easier to play with
        emit RequestFulfilled(_requestId, _randomWords);
    }

    // to check the request status of random number call.
    function getRequestStatus(
        uint256 _requestId
    ) external view returns (bool fulfilled, uint256[] memory randomWords) {
        require(s_requests[_requestId].exists, "request not found");
        RequestStatus memory request = s_requests[_requestId];
        return (request.fulfilled, request.randomWords);
    }

    function getPlayerStats(address _player, uint256 _treasureID) public view returns (string[] memory) {
        return playerStats[_player][_treasureID];
    }

    function getTicketOwner(uint256 treasureID ,string memory ticket) public view returns(address ticketOwner) {
        return ticketOwner = treasures[treasureID].codedKeys[ticket];
    }

     function getAllTickets(uint256 _treasureID) public view returns (string[] memory) {
        return treasures[_treasureID].treasureTickets;
    }

    function treasureWinners() public view returns (string[] memory) {
        return Winners;
    }

    function transferUSDTtoWallets(
        uint256 amount
    ) private  {
        uint256 forInvestor = amount.mul(1500).div(10000);
        uint256 forGameReward = amount.mul(5000).div(10000);
        uint256 forDonation = amount.mul(1500).div(10000);
        uint256 forOpMkDvCosts = amount.mul(2000).div(10000);

        USDT.transferFrom(msg.sender, address(this), amount);

        USDT.transfer(INVESTOR_WALLET, forInvestor);

        USDT.transfer(GAME_REWARD_WALLET, forGameReward);

        USDT.transfer(DONATION_WALLET, forDonation);

        USDT.transfer(OP_MRK_DV_WALLET, forOpMkDvCosts);

    }

}