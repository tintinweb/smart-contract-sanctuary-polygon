pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";

library Utils {
    struct Price {
        address tokenAddress;
        uint256 price;
    }
    struct TokenData {
        uint256 id;
        bytes32 privateCID;
        bytes32 publicCID;
    }
    struct UserBalance {
        uint256 fulfilled;
        uint256 waiting;
    }
    struct SaleInfo {
        uint64 StartDate;
        uint64 EndDate;
        bool isActive;
        Utils.Price[] prices;
        uint256 NFTToSell;
        bool isLiveSale;
        uint256 MaxPerAccount;
    }
    struct Ticket {
        address account;
        bool fulfilled;
        bool rejected;
        uint256 saleIndex;
        bytes32 privateCID;
        bytes32 publicCID;
        uint256 id;
    }
    struct TicketList {
        uint256 index;
        Ticket ticket;
    }
    struct SaleConf {
        uint64 StartDate;
        uint64 EndDate;
        bool isActive;
        uint8 _size;
        uint8 _hashFunction;
        bool isLiveSale;
        uint256 MaxPerAccount;
        Price[] prices;
    }
    struct Sale {
        SaleConf conf;
        uint256 idIndex;
        mapping(address => UserBalance) balances;
        TokenData[] data;
        Ticket[] tickets;
    }
    struct Track {
        uint256 saleIndex;
        uint256 DataIndex;
    }
    bytes private constant _ALPHABET =
        "123456789ABCDEFGHJKLMNPQRSTUVWXYZabcdefghijkmnopqrstuvwxyz";

    function getNFTByUserUpgradeable(
        address userAddress,
        ERC721EnumerableUpgradeable nft
    ) internal view returns (uint256[] memory) {
        uint256 nftBalance = nft.balanceOf(userAddress);
        uint256[] memory results = new uint256[](nftBalance);
        if (nftBalance > 0) {
            for (uint256 i; i < nftBalance; i++) {
                uint256 tokenID = nft.tokenOfOwnerByIndex(userAddress, i);
                results[i] = tokenID;
            }
        }
        return results;
    }

    function getNFTByUser(address userAddress, ERC721Enumerable nft)
        internal
        view
        returns (uint256[] memory)
    {
        uint256 nftBalance = nft.balanceOf(userAddress);
        uint256[] memory results = new uint256[](nftBalance);
        if (nftBalance > 0) {
            for (uint256 i; i < nftBalance; i++) {
                uint256 tokenID = nft.tokenOfOwnerByIndex(userAddress, i);
                results[i] = tokenID;
            }
        }
        return results;
    }

    function toBase58(bytes memory source)
        internal
        pure
        returns (bytes memory)
    {
        if (source.length == 0) return new bytes(0);
        uint8[] memory digits = new uint8[](64); //TODO: figure out exactly how much is needed
        digits[0] = 0;
        uint8 digitlength = 1;
        for (uint256 i = 0; i < source.length; ++i) {
            uint256 carry = uint8(source[i]);
            for (uint256 j = 0; j < digitlength; ++j) {
                carry += uint256(digits[j]) * 256;
                digits[j] = uint8(carry % 58);
                carry = carry / 58;
            }

            while (carry > 0) {
                digits[digitlength] = uint8(carry % 58);
                digitlength++;
                carry = carry / 58;
            }
        }
        return toAlphabet(reverse(truncate(digits, digitlength)));
    }

    function toAlphabet(uint8[] memory indices)
        internal
        pure
        returns (bytes memory)
    {
        bytes memory output = new bytes(indices.length);
        for (uint256 i = 0; i < indices.length; i++) {
            output[i] = _ALPHABET[indices[i]];
        }
        return output;
    }

    function reverse(uint8[] memory input)
        internal
        pure
        returns (uint8[] memory)
    {
        uint8[] memory output = new uint8[](input.length);
        for (uint256 i = 0; i < input.length; i++) {
            output[i] = input[input.length - 1 - i];
        }
        return output;
    }

    function truncate(uint8[] memory array, uint8 length)
        internal
        pure
        returns (uint8[] memory)
    {
        uint8[] memory output = new uint8[](length);
        for (uint256 i = 0; i < length; i++) {
            output[i] = array[i];
        }
        return output;
    }

    function getHash(
        bytes32 hashed,
        uint8 _hashFunction,
        uint8 _size
    ) internal pure returns (string memory) {
        bytes memory bytesString = new bytes(34);
        uint256 charCount = 0;
        bytesString[0] = bytes1(_hashFunction);
        bytesString[1] = bytes1(_size);
        for (uint8 i = 0; i < 32; i++) {
            bytes1 char = hashed[i];
            bytesString[charCount + 2] = char;
            charCount++;
        }
        return string(Utils.toBase58(bytesString));
    }

    function verifPrices(Price[] memory prices) internal pure {
        address[] memory used = new address[](prices.length);
        uint256 usedLength = 0;
        bool duplication = false;
        for (uint256 priceIndex = 0; priceIndex < prices.length; priceIndex++) {
            for (uint256 usedIndex = 0; usedIndex < usedLength; usedIndex++) {
                if (prices[priceIndex].tokenAddress == used[usedIndex]) {
                    duplication = true;
                }
            }
            if (!duplication) {
                used[usedLength] = prices[priceIndex].tokenAddress;
                usedLength++;
            }
        }
        require(!duplication, "duplication");
    }
}