pragma solidity ^0.8.4;

import './AdminManager.sol';
import '@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol';
import '@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/cryptography/MerkleProofUpgradeable.sol';
import './MasterNFT.sol';

contract LotterySale is Initializable, UUPSUpgradeable, AdminManager {
  struct Price {
    address tokenAddress;
    uint256 price;
  }
  struct TokenData {
    uint256 id;
    bytes32 privateCID;
    bytes32 publicCID;
  }
  struct SaleInfo {
    Price[] prices;
    uint64 StartDate;
    uint64 EndDate;
    uint256 NFTToSell;
    uint256 TotalNFTToSell;
    bool isActive;
    uint256 MaxPerAccount;
  }
  struct SaleInfoState {
    uint64 StartDate;
    uint64 EndDate;
    Price[] prices;
    bool isActive;
    uint256 NFTToSell;
    uint256 NFTSold;
    uint256 MaxPerAccount;
    uint256 MaxPerTransaction;
    mapping(address => uint256) balances;
  }
  bytes32 public merkleRoot;
  SaleInfoState[] sales;
  address public MasterNFTAddress;
  TokenData[] data;
  uint256 idIndex;
  mapping(uint256 => bool) ReservedID;
  uint8 public currentStage;

  function initialize(address _masterNFTAddress) external initializer {
    __UUPSUpgradeable_init();
    __AdminManager_init();
    MasterNFTAddress = _masterNFTAddress;

    sales.push();

    sales.push();
    currentStage = 0;
  }

  /**
   * @dev edit sale function , edit the stage passed as parameter (Only Admin)
   * @param stage , the stage to edit
   * @param startDate , the start date of the sale
   * @param endDate , the end date of the sale
   * @param maxPerAccount , the max number of NFTs that can be bought per account
   * @param _prices , an array of {tokenAddress , price}
   */

  function editSale(
    uint8 stage,
    uint64 startDate,
    uint64 endDate,
    uint256 _NFTToSell,
    bool _isActive,
    uint256 maxPerAccount,
    uint256 MaxPerTransaction,
    Price[] memory _prices
  ) public onlyRole(DEFAULT_ADMIN_ROLE) {
    require(stage < sales.length, 'stage not exist');
    SaleInfoState storage sale = sales[stage];
    require(startDate < endDate, 'start date must be before end date');
    require(
      _NFTToSell >= sale.NFTSold,
      'NFTToSell must be greater than NFTSold'
    );
    verifPrices(_prices);
    sale.NFTToSell = _NFTToSell;
    sale.StartDate = startDate;
    sale.EndDate = endDate;
    sale.isActive = _isActive;
    sale.MaxPerAccount = maxPerAccount;
    sale.MaxPerTransaction = MaxPerTransaction;
    delete sale.prices;
    for (uint256 index = 0; index < _prices.length; index++) {
      sale.prices.push(_prices[index]);
    }
  }

  /**
   * @dev set merkle root function , set the merkle root passed as parameter (Only Admin)
   * @param _merkleRoot , the merkle root to set
   */
  function setMerkleRoot(
    bytes32 _merkleRoot
  ) external onlyRole(DEFAULT_ADMIN_ROLE) {
    merkleRoot = _merkleRoot;
  }

  /**
   * @dev add ids to sale function , add ids to the stage passed as parameter (Only Admin)
   * @param newIds , an array of {id , privateCID , publicCID}
   */
  function addIDsToSale(
    TokenData[] memory newIds
  ) public onlyRole(DEFAULT_ADMIN_ROLE) {
    verifyDuplicants(newIds);
    for (uint256 index = 0; index < newIds.length; index++) {
      require(
        !MasterNFT(MasterNFTAddress).exists(newIds[index].id),
        'id exist'
      );
      MasterNFT(MasterNFTAddress).reserv(newIds[index].id);
      data.push(newIds[index]);
    }
  }

  /**
   * @dev set sale stage function , set the current stage to the stage passed as parameter (Only Admin)
   * @param stage , the stage to set
   */
  function setSaleStage(uint8 stage) public onlyRole(DEFAULT_ADMIN_ROLE) {
    require(stage < sales.length, 'stage not exist');
    currentStage = stage;
  }

  /**
   * @dev set sale stage Prices function , set the stage prices to the prices passed as parameter (Only Admin)
   * @param stage , the stage to set
   * @param _prices , an array of {tokenAddress , price}
   */
  function setStagePrices(
    uint8 stage,
    Price[] memory _prices
  ) public onlyRole(DEFAULT_ADMIN_ROLE) {
    require(stage < sales.length, 'stage not exist');
    verifPrices(_prices);
    SaleInfoState storage sale = sales[stage];
    delete sale.prices;
    for (uint256 index = 0; index < _prices.length; index++) {
      sale.prices.push(_prices[index]);
    }
  }

  /**
   * @dev get stage Prices function , get the stage prices of the stage passed as parameter
   * @param stage , the stage to get
   * @return an array of {tokenAddress , price}
   */
  function getStagePrices(uint8 stage) public view returns (Price[] memory) {
    require(stage < sales.length, 'stage not exist');
    return sales[stage].prices;
  }

  function getTotalAdded() public view returns (uint256) {
    return data.length;
  }

  /**
   * @dev get sale stage function , get the current stage
   * @return currentStage , the current stage
   */
  function saleStage() public view returns (uint8) {
    return currentStage;
  }

  /**
   * @dev get sale info function , get the sale info of the stage passed as parameter
   * @param stage , the stage to get
   * @return result , a struct of {prices , StartDate , EndDate , NFTToSell , isActive , MaxPerAccount}
   */
  function getSalesInfo(
    uint8 stage
  ) public view returns (SaleInfo memory result) {
    require(stage < sales.length, 'stage not exist');
    result.prices = sales[stage].prices;
    result.StartDate = sales[stage].StartDate;
    result.EndDate = sales[stage].EndDate;
    result.TotalNFTToSell = sales[stage].NFTToSell;
    result.NFTToSell = sales[stage].NFTToSell - sales[stage].NFTSold;
    result.isActive = isActiveSale(stage);
    result.MaxPerAccount = sales[stage].MaxPerAccount;
    return result;
  }

  /**
   * @dev buy function , buy NFTs from the current stage
   * @param ticketsNumber , the number of NFTs to buy
   */
  function BUY(
    uint256 ticketsNumber,
    bytes32[] memory proof
  ) public payable nonReentrant canBuy(ticketsNumber) {
    require(
      currentStage != 0 ||
        MerkleProofUpgradeable.verify(
          proof,
          merkleRoot,
          keccak256(abi.encodePacked(_msgSender()))
        ),
      'Invalid proof in PreSale'
    );
    require(
      ticketsNumber <=
        sales[currentStage].NFTToSell - sales[currentStage].NFTSold,
      'Stage Sold'
    );
    require(ticketsNumber <= data.length - idIndex, 'Not enough NFTs');
    pay(_msgSender(), ticketsNumber);
    for (uint256 i = 0; i < ticketsNumber; i++) {
      mintTo(_msgSender());
    }
  }

  // add comment to adminAirDrop

  /**
   * @dev adminAirDrop function , mint NFTs to users passed as parameter (Only Admin)
   * @param ticketsNumber , an array of number of NFTs to mint
   * @param users , an array of user address
   */
  function adminAirDrop(
    uint256[] memory ticketsNumber,
    address[] memory users
  ) public payable onlyRole(DEFAULT_ADMIN_ROLE) {
    require(ticketsNumber.length == users.length, 'Invalid input');
    // calculate total tickets
    uint256 totalTickets = 0;
    for (uint256 i = 0; i < ticketsNumber.length; i++) {
      totalTickets += ticketsNumber[i];
    }

    require(totalTickets <= data.length - idIndex, 'Not enough NFTs');
    require(
      totalTickets <=
        sales[currentStage].NFTToSell - sales[currentStage].NFTSold,
      'Not enough NFTs in stage'
    );
    // mint to users
    for (uint256 i = 0; i < ticketsNumber.length; i++) {
      for (uint256 j = 0; j < ticketsNumber[i]; j++) {
        mintTo(users[i]);
      }
    }
  }

  /**
   * @dev isActiveSale function , check if the stage passed as parameter is active
   * @param stage , the stage to check
   * @return a boolean
   */
  function isActiveSale(uint8 stage) public view returns (bool) {
    return
      stage < sales.length &&
      (idIndex < data.length) &&
      sales[stage].NFTToSell - sales[stage].NFTSold > 0 &&
      sales[stage].isActive &&
      block.timestamp >= sales[stage].StartDate &&
      block.timestamp < sales[stage].EndDate;
  }

  modifier canBuy(uint256 amount) {
    require(!paused(), 'NFT Sale Paused');
    require(isActiveSale(currentStage), 'Sale Closed');
    require(
      !maxPerUserReached(_msgSender(), amount, currentStage),
      'Max Per User Reached'
    );
    require(
      sales[currentStage].MaxPerTransaction == 0 ||
        amount <= sales[currentStage].MaxPerTransaction,
      'Max Per Transaction Reached'
    );
    _;
  }

  /**
   * @dev maxPerUserReached function , check if the user reached the max number of NFTs to buy
   * @param account , the user address
   * @param amount , the number of NFTs to buy
   * @param stage , the stage to check
   * @return a boolean
   */
  function maxPerUserReached(
    address account,
    uint256 amount,
    uint8 stage
  ) public view returns (bool) {
    bool stillCanBuy = sales[stage].MaxPerAccount == 0 ||
      userPurchases(stage, account) + amount <= sales[stage].MaxPerAccount;
    return !stillCanBuy;
  }

  /**
   * @dev userPurchases function , get the number of NFTs bought by the user
   * @param stage , the stage to check
   * @param account , the user address
   * @return the number of NFTs bought by the user
   */
  function userPurchases(
    uint8 stage,
    address account
  ) public view returns (uint256) {
    return sales[stage].balances[account];
  }

  function canPay(
    address buyer,
    uint256 nativeVal,
    uint256 nbTickets
  ) internal view {
    SaleInfoState storage sale = sales[currentStage];
    for (uint256 index = 0; index < sale.prices.length; index++) {
      if (sale.prices[index].tokenAddress == address(0)) {
        require(
          nativeVal == sale.prices[index].price * nbTickets,
          'invalid Native Value'
        );
      } else {
        require(
          IERC20Upgradeable(sale.prices[index].tokenAddress).allowance(
            buyer,
            address(this)
          ) >= sale.prices[index].price * nbTickets,
          'invalide allowance'
        );
      }
    }
  }

  function pay(address buyer, uint256 nbTickets) internal {
    SaleInfoState storage sale = sales[currentStage];
    canPay(_msgSender(), msg.value, nbTickets);
    for (uint256 index = 0; index < sale.prices.length; index++) {
      if (sale.prices[index].tokenAddress != address(0)) {
        IERC20Upgradeable(sale.prices[index].tokenAddress).transferFrom(
          buyer,
          address(this),
          sale.prices[index].price * nbTickets
        );
      }
    }
  }

  function mintTo(address reciver) internal {
    bool minted = false;
    SaleInfoState storage sale = sales[currentStage];
    while (!minted && idIndex < data.length) {
      if (!MasterNFT(MasterNFTAddress).exists(data[idIndex].id)) {
        minted = true;
      }
      idIndex++;
    }
    require(minted);
    MasterNFT(MasterNFTAddress).mintNFT(
      reciver,
      data[idIndex - 1].id,
      data[idIndex - 1].publicCID,
      data[idIndex - 1].privateCID
    );
    sale.balances[reciver]++;
    sale.NFTSold++;
  }

  function verifPrices(Price[] memory _prices) internal pure {
    address[] memory used = new address[](_prices.length);
    uint256 usedLength = 0;
    bool duplication = false;
    for (uint256 priceIndex = 0; priceIndex < _prices.length; priceIndex++) {
      for (uint256 usedIndex = 0; usedIndex < usedLength; usedIndex++) {
        if (_prices[priceIndex].tokenAddress == used[usedIndex]) {
          duplication = true;
        }
      }
      if (!duplication) {
        used[usedLength] = _prices[priceIndex].tokenAddress;
        usedLength++;
      }
    }
    require(!duplication, 'price duplication');
  }

  function verifyDuplicants(TokenData[] memory Ids) internal {
    uint256 index = 0;
    bool duplicant = false;
    while (index < Ids.length && !duplicant) {
      if (ReservedID[Ids[index].id]) {
        duplicant = true;
      } else {
        ReservedID[Ids[index].id] = true;
      }
      index++;
    }
    require(!duplicant, 'duplications in ids');
  }

  function _authorizeUpgrade(
    address newImplementation
  ) internal override onlyRole(DEFAULT_ADMIN_ROLE) {}
}

//SPDX-License-Identifier:MIT
pragma solidity ^0.8.4;

import './AdminManager.sol';
import './Utils.sol';
import '@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol';
import '@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol';

contract MasterNFT is
  Initializable,
  UUPSUpgradeable,
  ERC721EnumerableUpgradeable,
  AdminManager
{
  struct CIDs {
    bytes32 publicCID;
    bytes32 privateCID;
  }
  bytes32 internal constant SALE_CONTRACTR_ROLE = keccak256('SALE_CONTRACTR');
  mapping(uint256 => CIDs) internal tracker;
  mapping(uint256 => bool) private reservation;
  address[] public saleContracts;
  struct CIDToUpdate {
    uint256 ID;
    bytes32 publicCID;
    bytes32 privateCID;
  }

  function initialize(
    string memory name_,
    string memory symbol_
  ) public initializer {
    __ERC721_init(name_, symbol_);
    __ERC721Enumerable_init();
    __UUPSUpgradeable_init();
    __AdminManager_init();
  }

  /**
    OnlyAdmin Function, used to add a new sale contract address to the list of authorised sale contract addresses
    @param newAddress the address of the new Contract
   */
  function AddSaleContractAddress(
    address newAddress
  ) external onlyRole(DEFAULT_ADMIN_ROLE) {
    grantRole(SALE_CONTRACTR_ROLE, newAddress);
    uint256 index = getSaleContractIndex(newAddress);
    if (index > saleContracts.length) {
      saleContracts.push(newAddress);
    }
  }

  function getAllSaleContracts() external view returns (address[] memory) {
    return saleContracts;
  }

  function getSaleContractIndex(
    address contractAddress
  ) internal view returns (uint256) {
    uint256 index = saleContracts.length + 1;
    for (uint256 i = 0; i < saleContracts.length; i++) {
      if (saleContracts[i] == contractAddress) {
        index = i;
      }
    }
    return index;
  }

  /**
    OnlyAdmin Function, used to remove sale contract address from the list of authorised sale contract addresses
    @param newAddress the address of the new Contract
   */
  function revokeSaleContractAddress(
    address newAddress
  ) external onlyRole(DEFAULT_ADMIN_ROLE) {
    uint256 index = getSaleContractIndex(newAddress);
    if (index < saleContracts.length) {
      saleContracts[index] = saleContracts[saleContracts.length - 1];
      saleContracts.pop();
    }
    revokeRole(SALE_CONTRACTR_ROLE, newAddress);
  }

  /**
    Only an autuorised Sale contract can use the function, call this function to mint an nft to a user
    @param reciver the address of the user
    @param id the ID of the NFT
    @param publicCID public CID for the NFT
    @param privateCID private CID for the NFT
   */
  function mintNFT(
    address reciver,
    uint256 id,
    bytes32 publicCID,
    bytes32 privateCID
  ) external onlyRole(SALE_CONTRACTR_ROLE) whenNotPaused {
    require(reservation[id], 'id not reserved');
    tracker[id] = CIDs({publicCID: publicCID, privateCID: privateCID});
    _mint(reciver, id);
  }

  /**
    Only Admin can use the function, call this function to mint an nft to a user
    @param reciver the address of the user
    @param id the ID of the NFT
    @param publicCID public CID for the NFT
    @param privateCID private CID for the NFT
   */
  function mintNFTAdmin(
    address reciver,
    uint256 id,
    bytes32 publicCID,
    bytes32 privateCID
  ) external onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
    require(!reservation[id] && !_exists(id), 'id invalid');
    tracker[id] = CIDs({publicCID: publicCID, privateCID: privateCID});
    _mint(reciver, id);
  }

  /**
    Only an autuorised Sale contract can use the function, reserve an id for a sale , can only mint an NFT with reserved id
    @param id the id to reserve
   */
  function reserv(uint256 id) external onlyRole(SALE_CONTRACTR_ROLE) {
    require(reservation[id] == false, 'id reserved');
    reservation[id] = true;
  }

  /**
    Only an autuorised Sale contract can use the function, cancle the reservation of an ID
    @param id the id to unreserve
   */
  function unreserv(uint256 id) external onlyRole(SALE_CONTRACTR_ROLE) {
    require(reservation[id] == true, 'id not reserved');
    reservation[id] = false;
  }

  /**
    Only an autuorised Sale contract can use the function, return if an id is reserved
    @param id the id to verify
   */
  function isReserved(
    uint256 id
  ) external view onlyRole(SALE_CONTRACTR_ROLE) returns (bool) {
    return reservation[id];
  }

  /**
    only Admin Sale contract can use the function, reserve an id for a sale , can only mint an NFT with reserved id
    @param id the id to reserve
   */
  function reservAdmin(uint256 id) external onlyRole(DEFAULT_ADMIN_ROLE) {
    require(reservation[id] == false, 'id reserved');
    reservation[id] = true;
  }

  /**
    only Admin Sale contract can use the function, cancle the reservation of an ID
    @param id the id to unreserve
   */
  function unreservAdmin(uint256 id) external onlyRole(DEFAULT_ADMIN_ROLE) {
    require(reservation[id] == true, 'id not reserved');
    reservation[id] = false;
  }

  /**
    only Admin Sale contract can use the function, return if an id is reserved
    @param id the id to verify
   */
  function isReservedAdmin(
    uint256 id
  ) external view onlyRole(DEFAULT_ADMIN_ROLE) returns (bool) {
    return reservation[id];
  }

  /**
    Only an Admin can use the function, Modify the CIDs for a particular NFT
    @param id the id of the NFT
    @param publicCID the new publicCID for the NFT
    @param privateCID the new privateCID for the NFT
   */
  function updateTokenCIDs(
    uint256 id,
    bytes32 publicCID,
    bytes32 privateCID
  ) public onlyRole(DEFAULT_ADMIN_ROLE) {
    tracker[id] = CIDs({publicCID: publicCID, privateCID: privateCID});
  }

  /**
    return the public URI for an nft
    @param tokenId the id of the NFT
   */
  function tokenURI(
    uint256 tokenId
  ) public view virtual override returns (string memory) {
    require(_exists(tokenId), 'token not found');
    return string(abi.encodePacked('ipfs://', getPublicCID(tokenId)));
  }

  /**
    return the private URI for an nft (only user with Platform Role)
    @param tokenId the id of the NFT
   */
  function privateTokenURI(
    uint256 tokenId
  ) public view virtual onlyRole(PLATFORM_ROLE) returns (string memory) {
    require(_exists(tokenId), 'token not found');
    return string(abi.encodePacked('ipfs://', getPrivateCID(tokenId)));
  }

  /**
    return the private URI for an nft (only when UserAccess Set to true)
    @param tokenId the id of the NFT
   */
  function userPrivateTokenURI(
    uint256 tokenId
  ) public view virtual returns (string memory) {
    require(_exists(tokenId), 'token not found');
    require(userAccess, 'Not Autuorized');

    return string(abi.encodePacked('ipfs://', getPrivateCID(tokenId)));
  }

  /**
    return a list of the nft owned by a user
    @param userAddress address of the user
   */
  function getNFTByUser(
    address userAddress
  ) external view returns (uint256[] memory) {
    return Utils.getNFTByUserUpgradeable(userAddress, this);
  }

  /**
    return if an id is already minted
    @param tokenId the id to verify
   */
  function exists(uint256 tokenId) external view returns (bool) {
    return _exists(tokenId);
  }

  function getPrivateCID(
    uint256 tokenId
  ) internal view returns (string memory) {
    return Utils.getHash(tracker[tokenId].privateCID, 18, 32);
  }

  function getPublicCID(uint256 tokenId) internal view returns (string memory) {
    return Utils.getHash(tracker[tokenId].publicCID, 18, 32);
  }

  function _authorizeUpgrade(
    address newImplementation
  ) internal override onlyRole(DEFAULT_ADMIN_ROLE) {}

  function owner() public view returns (address) {
    return getAdmin();
  }

  function supportsInterface(
    bytes4 interfaceId
  )
    public
    view
    virtual
    override(AccessControlUpgradeable, ERC721EnumerableUpgradeable)
    returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }
}

