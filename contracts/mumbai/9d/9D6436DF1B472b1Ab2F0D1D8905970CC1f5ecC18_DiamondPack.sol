pragma solidity ^0.8.19;

import "@openzeppelin/contracts/utils/Counters.sol";

/**
 * @title Contract to store and retrieve order information.
 * @author Dalton Tan
 */
abstract contract Buyable {
	using Counters for Counters.Counter;

	/// @notice Order information
	struct Order {
		address buyer;
		uint typeId;
		uint quantity;
		uint cost;
		uint timestamp;
		uint blockNumber;
	}
	struct OrderWithId {
		uint id;
		address buyer;
		uint typeId;
		uint quantity;
		uint cost;
		uint timestamp;
		uint blockNumber;
	}

	/// @dev Emitted when an order is made.
	event Ordered(
		uint indexed orderId,
		address indexed buyer,
		uint indexed typeId,
		uint quantity,
		uint cost
	);

	/// @dev Keep track of order ID.
	Counters.Counter internal _orderCounter;
	/// @notice All orders in sequential ID.
	mapping(uint => Order) public orders;
	/// @notice Mapping of all order IDs by each account.
	mapping(address => uint[]) public ordersByAccount;
	/// @notice Mapping of all total spending volume for each account.
	mapping(address => uint) public spendingsByAccount;

	/**
	 * @return Total number of orders made.
	 */
	function totalOrders() public view returns (uint) {
		return _orderCounter.current();
	}

	/**
	 * @param id Order ID.
	 * @return Order information.
	 */
	function getOrder(
		uint id
	) external view returns (address, uint, uint, uint, uint, uint) {
		Order memory o = orders[id];
		return (
			o.buyer,
			o.typeId,
			o.quantity,
			o.cost,
			o.timestamp,
			o.blockNumber
		);
	}

	/**
	 * @param buyer Address of the buyer.
	 * @return Array of all order information with ID from the buyer.
	 */
	function getOrdersFor(address buyer) external view returns (OrderWithId[] memory) {
		uint count = ordersByAccount[buyer].length;
		OrderWithId[] memory list = new OrderWithId[](count);
		for (uint i = 0; i < count; i++) {
			uint id = ordersByAccount[buyer][i];
			Order storage order = orders[id];
			list[i] = OrderWithId({
				id: id,
				buyer: order.buyer,
				typeId: order.typeId,
				quantity: order.quantity,
				cost: order.cost,
				timestamp: order.timestamp,
				blockNumber: order.blockNumber
			});
		}
		return list;
	}

	/**
	 * @param buyer Address of the buyer.
	 * @return Array of all order IDs from the buyer.
	 */
	function getOrderIdsFor(address buyer) external view returns (uint[] memory) {
		return ordersByAccount[buyer];
	}

	/**
	 * @notice Saving order information.
	 * @param buyer Account that is making the order.
	 * @param typeId Type ID.
	 * @param quantity Number of token ordered.
	 * @param cost Total cost of the order.
	 * @return Order ID.
	 */
	function _saveOrder(address buyer, uint typeId, uint quantity, uint cost) internal returns (uint) {
		_orderCounter.increment();
		uint id = _orderCounter.current();
		orders[id] = Order(
			buyer,
			typeId,
			quantity,
			cost,
			block.timestamp,
			block.number
		);
		ordersByAccount[buyer].push(id);
		spendingsByAccount[buyer] += cost;
		emit Ordered(id, buyer, typeId, quantity, cost);
		return id;
	}
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.19;

import "@opengsn/contracts/src/ERC2771Recipient.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Permit.sol";
import "./BaseNft.sol";
import "./Buyable.sol";

/**
 * @title ERC1155 contract for DiamondPack by Brillianz.
 * @author Dalton Tan
 * @dev The ID is using this format 0x<token type index: 240 bits><NFT Index: 16 bits>. This will give us a max supply
 * of 65k for each type. All IDs are unique across token types and NFTs. There is no 0 ID. The index portion will start
 * from 1, therefore all token type IDs will have index of 0.
 *
 * Features of this contract in addition to BaseNft:
 * - ERC2981 - Royalties
 * - Whitelisted OpenSea to transact on users' behalf
 * - Max supply
 * - Max per mint
 * - Price
 */
contract DiamondPack is BaseNft, Buyable, ERC1155Upgradeable {
	using CountersUpgradeable for CountersUpgradeable.Counter;

	/// @notice Cannot use zero address for recipient.
	error InvalidAddress();
	/// @notice ERC2981Royalties: Royalty amount must be less than or equal to 10,000
	error InvalidRoyalty();
	/// @notice Type ID does not exists.
	error TypeDoesNotExist(uint id);
	/// @notice The max supply cannot be lower than current supply.
	error MaxSupplyTooLow();
	/// @notice The max supply cannot be higher than the MAX_PER_TYPE.
	error MaxSupplyTooHigh();
	/// @notice Cannot mint more that the allowed amount.
	error MintingTooMany();
	/// @notice Amount cannot be zero.
	error ZeroAmount();

	// ERC2981 royalty info
	struct RoyaltyInfo {
		address recipient;
		uint24 amount; // 2 decimal percentage out of 10,000 e.g. 34.88% = 3488
	}

	/// @dev Emitted when token type `id` is defined.
	event DefineToken(uint indexed id, address indexed operator);
	/// @dev Emitted when setting max supply.
	event SetMaxSupply(address indexed operator, uint indexed id, uint supply);
	/// @dev Emitted when setting royalty info.
	event SetRoyaltyInfo(uint indexed id, address recipient, uint24 amount);

	// User roles
	bytes32 public constant DEFINER_ROLE = keccak256("DEFINER_ROLE");

	/// @notice Maximum supply for NFT
	uint public constant MAX_PER_TYPE = type(uint16).max - 1; // -1 because we start from 1
	/// @notice Number of bits we use for index.
	uint public constant INDEX_BITS = 16;
	/// @dev Name is used by OpenSea for storefront name
	string public name;

	/// @dev Keep track of token type ID
	CountersUpgradeable.Counter internal _typeIdCounter;
	/// @dev Used to keep tracking of new IDs for each NFT
	mapping(uint => CountersUpgradeable.Counter) internal _indexCounters;
	/// @notice ERC2981 Royalty Info.
	RoyaltyInfo internal _royaltyInfo;

	/// @notice Max supply of each token type.
	mapping(uint => uint) public maxSupplies;
	/// @notice Total supply of each token type.
	mapping(uint => uint) public totalSupplies;
	/// @notice Max tokens per mint of each token type. Setting to 0 will disable minting.
	mapping(uint => uint) public maxPerMints;
	/// @notice Price of each token type.
	mapping(uint => uint) public prices;

	function initialize(
		address _trustedForwarder,
		ERC20Permit _paymentToken,
		address admin
	) public initializer {
		__ERC1155_init("");
		__BaseNft_init(_trustedForwarder, _paymentToken);

		name = "Diamond Pack";
		_royaltyInfo = RoyaltyInfo(address(this), 0);

		_grantRole(DEFAULT_ADMIN_ROLE, admin);

		_grantRole(DEFINER_ROLE, msg.sender);
		defineNft(2400, 1080_000_000, type(uint).max); // yellow
		defineNft(1200, 5310_000_000, type(uint).max); // pink
		defineNft(360, 10530_000_000, type(uint).max); // blue
		_revokeRole(DEFINER_ROLE, msg.sender);
	}

	// ---------- Modifiers ----------

	modifier onlyExistingType(uint id) {
		uint index = id >> INDEX_BITS;
		if (index == 0 || index > _typeIdCounter.current()) {
			revert TypeDoesNotExist(id);
		}
		_;
	}

	modifier onlyExistingTypes(uint[] memory ids) {
		for (uint i = 0; i < ids.length; i++) {
			uint index = ids[i] >> INDEX_BITS;
			if (index == 0 || index > _typeIdCounter.current()) {
				revert TypeDoesNotExist(ids[i]);
			}
		}
		_;
	}

	// ---------- Administration ----------

	/**
	 * @notice Define a new NFT type.
	 * @param maxSupply Maximum tokens for this type.
	 * @param price Price for each NFT.
	 * @param maxPerMint Maximum tokens allowed per minting request.
	 * @return The token type ID.
	 */
	function defineNft(
		uint maxSupply,
		uint price,
		uint maxPerMint
	) public onlyRole(DEFINER_ROLE) returns (uint) {
		if (maxSupply > MAX_PER_TYPE) {
			revert MaxSupplyTooHigh();
		}
		_typeIdCounter.increment(); // increment first so we start from 1
		uint id = _typeIdCounter.current() << INDEX_BITS;
		prices[id] = price;
		maxSupplies[id] = maxSupply;
		maxPerMints[id] = maxPerMint;
		emit DefineToken(id, _msgSender());
		return id;
	}

	/**
	 * @notice Get the total number of types we have defined.
	 */
	function totalTypes() public view returns (uint) {
		return _typeIdCounter.current();
	}

	/**
	 * @notice Setting the maximum supply for token type `id` to `max`.
	 * @param id Token type ID.
	 * @param max Maximum supply.
	 */
	function setMaxSupply(
		uint id,
		uint max
	) external onlyRole(MANAGER_ROLE) onlyExistingType(id) {
		if (max < totalSupplies[id]) {
			revert MaxSupplyTooLow();
		}
		if (max > MAX_PER_TYPE) {
			revert MaxSupplyTooHigh();
		}
		maxSupplies[id] = max;
		emit SetMaxSupply(_msgSender(), id, max);
	}

	/**
	 * @notice Setting max tokens per transaction to `max` for token type `id`.
	 * @param id Token type ID.
	 * @param max Maximum supply.
	 */
	function setMaxPerMint(
		uint id,
		uint max
	) external onlyRole(MANAGER_ROLE) onlyExistingType(id) {
		maxPerMints[id] = max;
	}

	/**
	 * @notice Setting price to `price` for token type `id`.
	 * @param id Token type ID.
	 * @param price Price for this type of token.
	 */
	function setPrice(
		uint id,
		uint price
	) external onlyRole(MANAGER_ROLE) onlyExistingType(id) {
		prices[id] = price;
	}

	// ---------- Metadata ----------

	/**
	 * @dev Overriding default implementation to return token specific URI if available.
	 */
	function uri(
		uint _id
	) public view override(ERC1155Upgradeable) returns (string memory) {
		return tokenUris[_id];
	}

	// ---------- ERC2981 ----------

	/**
	 * @dev IERC2981 For marketplaces to get the amount of commission for this sale.
	 */
	function royaltyInfo(
		uint,
		uint value
	) external view returns (address, uint) {
		return (_royaltyInfo.recipient, (value * _royaltyInfo.amount) / 10000);
	}

	/**
	 * @notice Setting royalty to `@withDecimals(amount, 2)`% per sale. Royalties will be sent to `recipient`
	 * for all tokens. This will allow market places to determine how much commission is required per sale.
	 * @param recipient Recipient of the royalties.
	 * @param amount Percentage out of 10,000, where 100% = 10,000 and 0% = 0.
	 */
	function setRoyaltyInfo(
		address recipient,
		uint24 amount
	) external onlyRole(MONEY_ROLE) {
		if (recipient == address(0)) {
			revert InvalidAddress();
		}
		if (amount > 10000) {
			revert InvalidRoyalty();
		}
		_royaltyInfo = RoyaltyInfo(recipient, amount);
	}

	// ---------- Mint ----------

	/**
	 * @notice Minting `amount` new token(s) for token token type `id` to `to`. This will cost `msg.value` wei.
	 * @param to Address to send the tokens to.
	 * @param id Token type ID.
	 * @param amount Number of tokens to mint.
	 */
	function mint(
		address to,
		uint id,
		uint amount
	) public onlyExistingType(id) returns (uint[] memory) {
		if (amount > maxPerMints[id]) {
			revert MintingTooMany();
		}
		uint cost = prices[id] * amount;
		paymentToken.transferFrom(_msgSender(), address(this), cost);
		_saveOrder(_msgSender(), id, 1, cost);
		uint[] memory ids = new uint[](1);
		uint[] memory amounts = new uint[](1);
		ids[0] = id;
		amounts[0] = amount;
		return _mintAuto(to, ids, amounts);
	}

	/**
	 * @notice Calling mint() but with the additional ERC20Permit call before.
	 */
	function mintWithPermit(
		address to,
		uint id,
		uint amount,
		address owner,
		address spender,
		uint value,
		uint deadline,
		uint8 v,
		bytes32 r,
		bytes32 s
	) public onlyExistingType(id) returns (uint[] memory) {
		paymentToken.permit(owner, spender, value, deadline, v, r, s);
		return mint(to, id, amount);
	}

	/**
	 * @notice Minting new tokens in batch.
	 * @param to Address to send the tokens to.
	 * @param ids Token types ID.
	 * @param amounts Number of tokens to mint for the corresponding ID.
	 */
	function mintBatch(
		address to,
		uint[] memory ids,
		uint[] memory amounts
	) public onlyExistingTypes(ids) returns (uint[] memory) {
		uint totalCost = 0;
		for (uint i = 0; i < ids.length; i++) {
			uint price = prices[ids[i]];
			totalCost += price * amounts[i];
		}
		paymentToken.transferFrom(_msgSender(), address(this), totalCost);
		for (uint i = 0; i < ids.length; i++) {
			uint id = ids[i];
			uint quantity = amounts[i];
			if (quantity > maxPerMints[id]) {
				revert MintingTooMany();
			}
			_saveOrder(_msgSender(), id, quantity, prices[id] * quantity);
		}
		return _mintAuto(to, ids, amounts);
	}

	/**
	 * @notice Calling mintBatch() but with the additional ERC20Permit call before.
	 */
	function mintBatchWithPermit(
		address to,
		uint[] memory ids,
		uint[] memory amounts,
		address owner,
		address spender,
		uint value,
		uint deadline,
		uint8 v,
		bytes32 r,
		bytes32 s
	) public onlyExistingTypes(ids) returns (uint[] memory) {
		paymentToken.permit(owner, spender, value, deadline, v, r, s);
		return mintBatch(to, ids, amounts);
	}

	/**
	 * @notice Minting free tokens in batch. This will bypass price requirements, and max per mint, but still respect
	 * the max supply.
	 * @param to Address to send the tokens to.
	 * @param ids Token types ID.
	 * @param amounts Number of tokens to mint for the corresponding ID.
	 */
	function mintBypass(
		address to,
		uint[] memory ids,
		uint[] memory amounts
	)
		public
		onlyRole(MINTER_ROLE)
		onlyExistingTypes(ids)
		returns (uint[] memory)
	{
		return _mintAuto(to, ids, amounts);
	}

	/**
	 * @dev This is the function that does the actual minting.
	 */
	function _mintAuto(
		address to,
		uint[] memory ids,
		uint[] memory amounts
	) internal onlyExistingTypes(ids) returns (uint[] memory) {
		for (uint i = 0; i < amounts.length; i++) {
			if (amounts[i] <= 0) {
				revert ZeroAmount();
			}
		}
		for (uint i = 0; i < ids.length; i++) {
			if (totalSupplies[ids[i]] + amounts[i] > maxSupplies[ids[i]]) {
				revert MintingTooMany();
			}
		}

		// count how many new NFTs we need to mint
		uint totalMinting = 0;
		for (uint i = 0; i < amounts.length; i++) {
			totalMinting += amounts[i];
		}
		uint[] memory tokenIds = new uint[](totalMinting);
		uint[] memory tokenAmounts = new uint[](totalMinting);
		uint index = 0;
		for (uint i = 0; i < ids.length; i++) {
			uint typeId = ids[i];
			for (uint j = 0; j < amounts[i]; j++) {
				tokenIds[index] = _generateNextTokenId(typeId);
				tokenAmounts[index] = 1; // NFT amount will always be 1
				index++;
			}
		}

		// mint using correct function
		if (tokenIds.length == 1) {
			_mint(to, tokenIds[0], tokenAmounts[0], "");
		} else {
			_mintBatch(to, tokenIds, tokenAmounts, "");
		}
		return tokenIds;
	}

	/**
	 * @dev Returns the next token ID for NFT and increases the index counter. ID will start from 1 so that token type
	 * ID and NFT ID can share the same format while being unique.
	 * @param typeId Token type ID.
	 * @return Token ID.
	 */
	function _generateNextTokenId(
		uint typeId
	) internal onlyExistingType(typeId) returns (uint) {
		_indexCounters[typeId].increment(); // increment first so it doesn't start from 0
		return typeId | _indexCounters[typeId].current();
	}

	// ---------- Burn ----------

	/**
	 * @notice Burning token `id`.
	 * @param from Account to burn the token from.
	 * @param id Token ID.
	 */
	function burn(
		address from,
		uint id
	) external onlyRole(BURNER_ROLE) onlyExistingType(id) {
		_burn(from, id, 1);
	}

	/**
	 * @notice Burning tokens in batch.
	 * @param from Account to burn the tokens from.
	 * @param ids Token IDs.
	 */
	function burnBatch(
		address from,
		uint[] memory ids
	) external onlyRole(BURNER_ROLE) onlyExistingTypes(ids) {
		uint[] memory amounts = new uint[](ids.length);
		for (uint i = 0; i < ids.length; i++) {
			amounts[i] = 1;
		}
		_burnBatch(from, ids, amounts);
	}

	// ---------- Others ----------

	/**
	 * @notice Getting the type ID of token `id`.
	 * @param id Token ID.
	 * @return Type ID.
	 */
	function typeOf(uint id) public pure returns (uint) {
		return (id >> INDEX_BITS) << INDEX_BITS;
	}

	/**
	 * @dev Override to allow some addresses to bypass the approval.
	 */
	function isApprovedForAll(
		address owner,
		address operator
	) public view override returns (bool) {
		if (_canBypassApproval(owner, operator)) {
			return true;
		}
		return super.isApprovedForAll(owner, operator);
	}

	function _beforeTokenTransfer(
		address operator,
		address from,
		address to,
		uint[] memory ids,
		uint[] memory amounts,
		bytes memory data
	) internal virtual override(ERC1155Upgradeable) {
		for (uint i = 0; i < ids.length; i++) {
			_blockIfFrozen(ids[i]);
		}
		super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
		// mint token
		if (from == address(0)) {
			for (uint i = 0; i < ids.length; ++i) {
				uint id = (ids[i] >> INDEX_BITS) << INDEX_BITS;
				totalSupplies[id] += amounts[i];
			}
		}
		// burn token
		if (to == address(0)) {
			for (uint256 i = 0; i < ids.length; ++i) {
				uint256 id = (ids[i] >> INDEX_BITS) << INDEX_BITS;
				totalSupplies[id] -= amounts[i];
			}
		}
	}

	// The following functions are overrides required by Solidity.

	function _msgSender()
		internal
		view
		virtual
		override(ContextUpgradeable, BaseNft)
		returns (address)
	{
		return super._msgSender();
	}

	function _msgData()
		internal
		view
		virtual
		override(ContextUpgradeable, BaseNft)
		returns (bytes calldata)
	{
		return super._msgData();
	}

	function supportsInterface(
		bytes4 interfaceId
	)
		public
		view
		override(ERC1155Upgradeable, AccessControlUpgradeable)
		returns (bool)
	{
		return super.supportsInterface(interfaceId);
	}
}