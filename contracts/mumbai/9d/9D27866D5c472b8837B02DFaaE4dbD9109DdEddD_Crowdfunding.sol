pragma solidity ^0.8.11;

/// @author vensarevich

import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@thirdweb-dev/contracts/extension/ContractMetadata.sol";
import "@thirdweb-dev/contracts/extension/Ownable.sol";

contract StartupHub is
    ContractMetadata,
    Ownable,
    AccessControlEnumerableUpgradeable
{
    struct Project {
        uint256 id;
        string uri;
        address owner;
    }

    struct FileInfo {
        string fileHash;
        string fileKey;
        string fileType;
        string fileName;
        bool isPrivate;
    }

    bytes32 private constant MODULE_TYPE = bytes32("StartupHub");
    uint128 private constant VERSION = 5;

    /// @dev Project level metadata.
    uint256 public projectCount = 0;
    mapping(uint256 => Project) private _projects;
    mapping(string => FileInfo) private _files;
    mapping(string => uint256) private _fileHashToProjectId;

    /// @dev Events
    event ProjectCreated(
        uint256 indexed projectId,
        address indexed owner,
        string uri
    );
    event ProjectEdited(
        uint256 indexed projectId,
        string uri
    );
    event FilesUploaded(
        uint256 indexed projectId,
        FileInfo[] files
    );

    constructor() {
        _setupOwner(msg.sender);
    }

    /// @dev Creates a new project.
    /// @param uri The project URI.
    /// @param files The project files.
    function createProject(string memory uri, FileInfo[] memory files) public {
        require(bytes(uri).length > 0, "URI must not be empty.");
        for (uint256 i = 0; i < files.length; i++) {
            require(_fileHashToProjectId[files[i].fileHash] == 0, "File hash already exists in another project.");
        }

        uint256 projectId = projectCount++;
        _projects[projectId] = Project({
            id: projectId,
            uri: uri,
            owner: msg.sender
        });
        emit ProjectCreated(projectId, msg.sender, uri);

        if (files.length == 0) {
            return;
        }

        for(uint256 i = 0; i < files.length; i++) {
            _fileHashToProjectId[files[i].fileHash] = projectId;
            _files[files[i].fileHash] = files[i];
        }

        emit FilesUploaded(projectId, files);
    }

    /// @dev Edits a project.
    /// @param projectId The project ID.
    /// @param uri The project URI.
    function editProject(uint256 projectId, string memory uri) public {
        require(bytes(uri).length > 0, "URI must not be empty.");
        require(_projects[projectId].owner == msg.sender, "Only the owner can edit the project.");

        _projects[projectId].uri = uri;

        emit ProjectEdited(projectId, uri);
    }

    /// @dev Upload files to a project.
    /// @param projectId The project ID.
    /// @param files The project files.
    function uploadFiles(uint256 projectId, FileInfo[] memory files) public {
        require(bytes(_projects[projectId].uri).length > 0, "Project does not exist.");
        require(_projects[projectId].owner == msg.sender, "Only the owner can upload files to the project.");
        require(files.length > 0, "Must upload at least one file.");

        for (uint256 i = 0; i < files.length; i++) {
            require(_fileHashToProjectId[files[i].fileHash] == 0, "File hash already exists in another project.");
        }

        for(uint256 i = 0; i < files.length; i++) {
            _fileHashToProjectId[files[i].fileHash] = projectId;
            _files[files[i].fileHash] = files[i];
        }

        emit FilesUploaded(projectId, files);
    }

    /// @dev Returns the project count.
    function getProjectCount() public view returns (uint256) {
        return projectCount;
    }

    /// @dev Returns the project.
    /// @param projectId The project ID.
    function getProject(uint256 projectId) public view returns (Project memory) {
        return _projects[projectId];
    }

    /// @dev Returns the project uri.
    /// @param projectId The project ID.
    function getProjectUri(uint256 projectId) public view returns (string memory) {
        return _projects[projectId].uri;
    }

    /// @dev Returns the project uri by file hash.
    /// @param fileHash The file hash.
    function getProjectUriByFileHash(string memory fileHash) public view returns (string memory) {
        return _projects[_fileHashToProjectId[fileHash]].uri;
    }

    /// @dev Returns the project uri by file hash.
    /// @param fileHash The file hash.
    function getProjectIdByFileHash(string memory fileHash) public view returns (uint256) {
        return _fileHashToProjectId[fileHash];
    }

    /// @dev Verifies the file hash.
    /// @param fileHash The file hash.
    function verifyFileHash(string memory fileHash) public view returns (bool) {
        return _fileHashToProjectId[fileHash] != 0;
    }

    /// @dev Returns the file info.
    /// @param fileHash The file hash.
    function getFileInfo(string memory fileHash) public view returns (string memory, string memory, string memory, string memory, bool) {
        return (
            _files[fileHash].fileHash,
            _files[fileHash].fileKey,
            _files[fileHash].fileType,
            _files[fileHash].fileName,
            _files[fileHash].isPrivate
        );
    }

    /// @dev Returns the module type of the contract.
    function contractType() external pure returns (bytes32) {
        return MODULE_TYPE;
    }

    /// @dev Returns the version of the contract.
    function contractVersion() external pure returns (uint128) {
        return VERSION;
    }

    /// @dev Checks whether contract metadata can be set in the given execution context.
    function _canSetContractURI() internal view virtual override returns (bool) {
        return msg.sender == owner();
    }

    /// @dev Returns whether owner can be set in the given execution context.
    function _canSetOwner() internal view virtual override returns (bool) {
        return msg.sender == owner();
    }
}