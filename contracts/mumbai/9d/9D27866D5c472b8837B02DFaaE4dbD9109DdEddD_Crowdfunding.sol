pragma solidity ^0.8.11;

/// @author vensarevich

import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@thirdweb-dev/contracts/extension/ContractMetadata.sol";
import "@thirdweb-dev/contracts/extension/Ownable.sol";
import "./StartupHub.sol";

contract Crowdfunding is
    ContractMetadata,
    Ownable,
    AccessControlEnumerableUpgradeable
{
    /// @dev Donation status: 0 - success, 1 - withdrawn, 2 - refunded.
    struct Donation {
        uint256 id;
        address sender;
        uint amount;
        uint status;
    }

    /// @dev Campaign status: 0 - active, 1 - claimed, 2 - refunded, 3 - cancelled.
    struct Campaign {
        uint256 id;
        uint projectId;
        address creator;
        uint goal;
        uint amount;
        uint32 startAt;
        uint32 endAt;
        uint status;
    }

    struct MappedCampaign {
        uint256 id;
        uint projectId;
        address creator;
        uint goal;
        uint amount;
        uint32 startAt;
        uint32 endAt;
        uint status;
        Donation[] donations;
    }

    bytes32 private constant MODULE_TYPE = bytes32("Crowdfunding");
    uint128 private constant VERSION = 2;

    /// @dev Campaign level metadata.
    uint public campaignsCount;
    uint public donationsCount;
    uint public maxDuration;
    mapping(uint => Campaign) private campaigns;
    mapping(uint => uint) private projectToCampaign;
    mapping(uint => Donation[]) private campaignDonations;
    mapping(address => uint[]) private ownerCampaigns;
    mapping(uint => uint[]) private projectCampaigns;


    /// @dev Events
    event CampaignLaunched(uint256 indexed id, uint indexed projectId, address indexed creator, uint goal, uint32 startAt, uint32 endAt);
    event CampaignClaimed(uint256 id);
    event CampaignRefunded(uint256 id, uint amount);
    event CampaignCancelled(uint256 id);
    event DonationReceived(uint256 indexed id, uint256 indexed campaignId, address indexed caller, uint amount);
    event DonationWithdrawn(uint256 id, uint256 indexed campaignId, address indexed caller, uint amount);
    event DonationRefunded(uint256 id, uint256 indexed campaignId, address indexed caller, uint amount);

    constructor(uint _maxDuration) {
        maxDuration = _maxDuration;
        _setupOwner(msg.sender);
    }

    /// @dev Launch a new campaign. Only one campaign can be started per project.
    /// @param _projectId id of project.
    /// @param _goal Campaign goal.
    /// @param _startAt Campaign start timestamp.
    /// @param _endAt Campaign end timestamp.
    function launchCampaign(uint _projectId, uint _goal, uint32 _startAt, uint32 _endAt) external {
        require(_startAt > block.timestamp, "Campaign start time must be in the future");
        require(_endAt > _startAt, "Campaign end time must be after start time");
        require(_endAt < _startAt + maxDuration, "Campaign duration is too long");
        require(projectToCampaign[_projectId] == 0, "Project already has a campaign");

        campaignsCount++;
        campaigns[campaignsCount] = Campaign(campaignsCount, _projectId, msg.sender, _goal, 0, _startAt, _endAt, 0);
        projectToCampaign[_projectId] = campaignsCount;
        ownerCampaigns[msg.sender].push(campaignsCount);
        projectCampaigns[_projectId].push(campaignsCount);

        emit CampaignLaunched(campaignsCount, _projectId, msg.sender, _goal, _startAt, _endAt);
    }

    /// @dev Cancel a campaign. Contract Admin or Campaign Creator can apply.
    /// @param _campaignId Campaign ID.
    function cancelCampaign(uint _campaignId) external {
        Campaign storage campaign = campaigns[_campaignId];
        require(campaign.status == 0, "Campaign is not active");
        require(campaign.creator == msg.sender || hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "Caller is not the campaign creator or admin");
        require(block.timestamp < campaign.startAt, "Campaign has already started");

        delete projectToCampaign[campaign.projectId];
        campaigns[_campaignId].status = 3;

        emit CampaignCancelled(_campaignId);
    }

    /// @dev Claim a campaign. Contract Admin or Campaign Creator can apply.
    /// @param _campaignId Campaign ID.
    function claimCampaign(uint _campaignId) external {
        Campaign storage campaign = campaigns[_campaignId];
        require(campaign.status == 0, "Campaign is not active");
        require(campaign.creator == msg.sender || hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "Caller is not the campaign creator or admin");
        require(block.timestamp > campaign.endAt, "Campaign has not ended yet");
        require(campaign.amount >= campaign.goal, "Campaign goal has not been reached");

        campaigns[_campaignId].status = 1;
        delete projectToCampaign[campaign.projectId];

        uint amount = campaign.amount;
        (bool success, ) = payable(campaign.creator).call{value: amount}("");
        require(success, "Failed to send Ether");

        emit CampaignClaimed(_campaignId);
    }

    /// @dev Refund a campaign. Refund all applied Donations. Contract Admin or Campaign Creator can apply.
    /// @param _campaignId Campaign ID.
    function refundCampaign(uint _campaignId) external {
        Campaign storage campaign = campaigns[_campaignId];
        require(campaign.status == 0, "Campaign is not active");
        require(campaign.creator == msg.sender || hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "Caller is not the campaign creator or admin");

        for (uint i = 0; i < campaignDonations[_campaignId].length; i++) {
            Donation storage donation = campaignDonations[_campaignId][i];
            if (donation.status == 0) {
                campaignDonations[_campaignId][i].status = 2;

                (bool success, ) = payable(donation.sender).call{value: donation.amount}("");
                require(success, "Failed to send Ether");

                emit DonationRefunded(donation.id, campaign.id, msg.sender, donation.amount);
            }
        }

        campaigns[_campaignId].status = 2;
        delete projectToCampaign[campaign.projectId];

        emit CampaignRefunded(_campaignId, campaign.amount);
    }

    /// @dev Donate to a campaign.
    /// @param _campaignId Campaign ID.
    function donate(uint _campaignId) external payable {
        Campaign storage campaign = campaigns[_campaignId];
        require(campaign.status == 0, "Campaign is not active");
        require(block.timestamp >= campaign.startAt, "Campaign has not started yet");
        require(block.timestamp <= campaign.endAt, "Campaign has ended");
        require(msg.value > 0, "Donation amount must be greater than 0");

        campaign.amount += msg.value;
        donationsCount++;
        campaignDonations[_campaignId].push(Donation(donationsCount, msg.sender, msg.value, 0));

        emit DonationReceived(donationsCount, campaign.id, msg.sender, msg.value);
    }

    /// @dev Withdraw a donation. Donation can be withdrawn if campaign is active and has not ended.
    /// @param _campaignId Campaign ID.
    /// @param _donationId Donation ID.
    function withdrawDonation(uint _campaignId, uint _donationId) external {
        Donation storage donation = campaignDonations[_campaignId][_donationId];
        Campaign storage campaign = campaigns[_campaignId];
        require(campaign.status == 0, "Campaign is not active");
        require(block.timestamp < campaign.endAt, "Campaign has ended");
        require(donation.status == 0, "Donation is not active");
        require(donation.sender == msg.sender, "Caller is not the donation sender");

        campaignDonations[_campaignId][_donationId].status = 1;

        (bool success, ) = payable(donation.sender).call{value: donation.amount}("");
        require(success, "Failed to send Ether");

        emit DonationWithdrawn(_donationId, campaign.id, msg.sender, donation.amount);
    }

    /// @dev Refund a donation. Donation can be refunded if campaign is ended and goal has not been reached.
    /// @param _campaignId Campaign ID.
    /// @param _donationId Donation ID.
    function refundDonation(uint _campaignId, uint _donationId) external {
        Donation storage donation = campaignDonations[_campaignId][_donationId];
        Campaign storage campaign = campaigns[_campaignId];

        require(campaign.status == 0, "Campaign is not active");
        require(block.timestamp > campaign.endAt, "Campaign has not ended yet");
        require(campaign.amount < campaign.goal, "Campaign goal has been reached");
        require(donation.status == 0, "Donation is not active");
        require(donation.sender == msg.sender, "Caller is not the donation sender");

        campaignDonations[_campaignId][_donationId].status = 2;

        (bool success, ) = payable(donation.sender).call{value: donation.amount}("");
        require(success, "Failed to send Ether");

        emit DonationRefunded(_donationId, campaign.id, msg.sender, donation.amount);
    }

    // Private function getCampaign by ID.
    function getCampaignById(uint _campaignId) private view returns (MappedCampaign memory) {
        Campaign storage campaign = campaigns[_campaignId];
        MappedCampaign memory mappedCampaign = MappedCampaign(
            campaign.id,
            campaign.projectId,
            campaign.creator,
            campaign.goal,
            campaign.amount,
            campaign.startAt,
            campaign.endAt,
            campaign.status,
            campaignDonations[_campaignId]
        );

        return mappedCampaign;
    }

    /// @dev Returns the Campaign with mapped Donations.
    /// @param _campaignId Campaign ID.
    /// @return MappedCampaign Campaign with mapped Donations.
    function getCampaign(uint _campaignId) external view returns (MappedCampaign memory) {
        return getCampaignById(_campaignId);
    }

    /// @dev Returns Campaigns with mapped Donations by Owner.
    /// @param _owner Campaign Owner.
    /// @return MappedCampaign Campaigns with mapped Donations.
    function getCampaignsByOwner(address _owner) external view returns (MappedCampaign[] memory) {
        uint size = ownerCampaigns[_owner].length;
        MappedCampaign[] memory mappedCampaigns = new MappedCampaign[](size);

        for (uint i = 0; i < size; i++) {
            mappedCampaigns[i] = getCampaignById(ownerCampaigns[_owner][i]);
        }

        return mappedCampaigns;
    }

    /// @dev Returns the Campaigns by Project Id with mapped Donations.
    /// @param _projectId Campaign Owner.
    /// @return MappedCampaign Campaigns with mapped Donations.
    function getCampaignsByProjectId(uint _projectId) external view returns (MappedCampaign[] memory) {
        uint size = projectCampaigns[_projectId].length;
        MappedCampaign[] memory mappedCampaigns = new MappedCampaign[](size);

        for (uint i = 0; i < size; i++) {
            mappedCampaigns[i] = getCampaignById(projectCampaigns[_projectId][i]);
        }

        return mappedCampaigns;
    }


    /// @dev Set new value for Max Duration
    /// @param _maxDuration The new value for Max Duration
    function setMaxDuration(uint _maxDuration) external onlyOwner {
        maxDuration = _maxDuration;
    }

    /// @dev Returns the module type of the contract.
    function contractType() external pure returns (bytes32) {
        return MODULE_TYPE;
    }

    /// @dev Returns the version of the contract.
    function contractVersion() external pure returns (uint128) {
        return VERSION;
    }

    /// @dev Checks whether contract metadata can be set in the given execution context.
    function _canSetContractURI() internal view virtual override returns (bool) {
        return msg.sender == owner();
    }

    /// @dev Returns whether owner can be set in the given execution context.
    function _canSetOwner() internal view virtual override returns (bool) {
        return msg.sender == owner();
    }
}

