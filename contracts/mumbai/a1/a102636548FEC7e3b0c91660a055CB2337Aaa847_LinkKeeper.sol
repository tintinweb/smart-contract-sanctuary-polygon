        );
        _chainlinkEconomics = ChainlinkEconomics(
            requireAndGetAddress(CHAINLINK_ECONOMICS_CONTRACT)
        );
    }

    /**
     * @notice Set new minCountTx value
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setMinCountTx(uint256 minCountTx) external onlyOwner {
        _minCountTx = minCountTx;
    }

    /**
     * @notice Set new maxCountTx value
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setMaxCountTx(uint256 maxCountTx) external onlyOwner {
        _maxCountTx = maxCountTx;
    }

    /**
     * @notice Set new estimateGasLiquidation value
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setEstimateGasLiquidation(uint256 value) external onlyOwner {
        _estimateGasLiquidation = value;
    }

    /**
     * @notice Set new estimateGasLinkFund value
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setEstimateGasLinkFund(uint256 value) external onlyOwner {
        _estimateGasLinkFund = value;
    }

    /**
     * @notice Set new estimateGasCreditRefund value
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setEstimateGasCreditRefund(uint256 value) external onlyOwner {
        _estimateGasCreditRefund = value;
    }

    /**
     * @notice Check if contract have to increase LINK balance on Chainlink
     * and calculate amount to increase
     *
     * @return upkeepNeeded boolean to indicate whether the keeper should call
     * performUpkeep or not.
     * @return performData Amount of LINK to exchange in bytes that the keeper
     * should call performUpkeep with
     *
     * @dev This function calculates minimum of LINKs that is needed to have on keeper
     * balance. If not enough the performUpkeep is called to cover the blance.
     * This function contains the logic that will be executed
     * off-chain by Chainlink Upkeep to see if performUpkeep should be executed.
     */
    function checkUpkeep(
        bytes calldata checkData
    )
        external
        view
        override
        returns (bool upkeepNeeded, bytes memory performData)
    {
        //TODO: DEX problem
        uint256 fee = _chainlinkEconomics.getFee(
            address(_exchange),
            tx.gasprice,
            _estimateGasLiquidation +
                _estimateGasLinkFund +
                _estimateGasCreditRefund
        );

        uint256 minLink = fee * _minCountTx;

        (, , uint96 balance) = _keeperRegistry.getKeeperInfo(
            address(_liquidationKeeper)
        );

        return (balance < minLink, abi.encode(fee * _maxCountTx - balance));
    }

    /**
     * @notice Execute increasing LINK balance on Chainlink
     *
     * @param performData is the data which was passed back from the checkUpkeep
     *
     * @dev This function calculates balances for each token and then tries to
     * exchange tokens to LINK. First try to resupply LINK with free tokens,
     * then if not enough with credit.
     * This function will be executed by Chainlink Upkeep
     * when checkUpkeep function retunrs true.
     */
    function performUpkeep(bytes calldata performData) external override {
        uint256 amount = abi.decode(performData, (uint256));

        address[] memory tokens = _factory.getTokens();
        uint256[] memory balances = _calculateFreeBalances(tokens, amount);

        // Swap not credit tokens
        uint256 sum = _swapByBalanceList(tokens, balances, amount, false);

        // If not enough swap credit tokens
        if (sum < amount) {
            tokens = _creditSystem.getCoins();
            balances = _calculateCreditBalances(tokens, amount - sum);

            if (balances.length > 0)
                sum = _swapByBalanceList(tokens, balances, amount - sum, true);
        }

        // Transfer funds to Chanlink
        IERC20(LINK).approve(address(_keeperRegistry), sum);
        _keeperRegistry.addFunds(_keeperId, uint96(sum));
    }

    /**
     * @notice Swap by sorted balance list and accumulate sum of LINK
     *
     * @param tokens Tokens with which the LINK can be exchanged
     * @param balances Tokens balances
     * @param amount Amount of LINK to exchange
     * @param isCredit True if balances are credit, otherwise - False
     * @return amount Total amountOut
     *
     */
    function _swapByBalanceList(
        address[] memory tokens,
        uint256[] memory balances,
        uint amount,
        bool isCredit
    ) internal returns (uint256) {
        uint256 sum = 0;
        uint i = 0;

        // Trying to swap while total amountOut is less then amount and
        // while balances exist and while token balance greater then 0
        while (sum < amount && i < balances.length && balances[i] > 0) {
            if (isCredit) {
                _creditSystem.transferCreditCoins(tokens[i], balances[i]);
            } else {
                _creditSystem.transferFreeCoins(tokens[i], balances[i]);
            }

            // if current token is LINK no need to swap, just increase sum
            if (tokens[i] == LINK) {
                sum += balances[i++];
                continue;
            }

            TransferHelper.safeApprove(
                tokens[i],
                address(_exchange),
                balances[i]
            );

            (, uint256 amountOut) = _exchange.swap(
                IExchange.SwapParams({
                    amountIn: balances[i],
                    amountOut: 0,
                    tokenIn: tokens[i],
                    tokenOut: LINK,
                    timestamp: block.timestamp,
                    path: "0x",
                    dexType: IExchange.DEX.UNISWAP,
                    recipient: address(this)
                })
            );

            sum += amountOut;
            i++;
        }

        return sum;
    }

    /**
     * @notice Receives a structured sorted list of coins and balances which
     * could be used for covering. Takes only free balances from Insurance fund (not credit)
     *
     * @param tokens Tokens with which the LINK can be exchanged
     * @param amount Amount of LINK to exchange
     * @return array `tokens` balances.
     *
     * @dev This function gets credit balances for each token, calculates
     * valuations for each token and then sort arrays by valuations.
     *
     */
    function _calculateFreeBalances(
        address[] memory tokens,
        uint256 amount
    ) internal view returns (uint256[] memory) {
        uint256[] memory rates = new uint256[](tokens.length);
        uint256[] memory balances = new uint256[](tokens.length);

        uint256 sum = 0;

        for (uint256 i = 0; i < tokens.length; i++) {
            // Get free amount of token
            balances[i] = _creditSystem.getFreeBalance(tokens[i]);

            // Get amountIn of token in LINK equivalent with amountOut = amount
            //rates[i] = _exchange.getAmountIn(tokens[i], LINK, amount);
            rates[i] = _exchange.getAmountOut(tokens[i], LINK, 1e18); // ?dai - amountIn,amount ? in LINK

            sum += balances[i] * rates[i];
        }

        if (sum == 0) return new uint256[](0);

        uint256[] memory valuations = new uint256[](rates.length);

        // Calculate valuation for each token
        for (uint256 i = 0; i < rates.length; i++) {
            valuations[i] = (balances[i] * rates[i] * 1e18) / sum;
        }

        // Sort by valuations
        valuations.quickSort(tokens, balances, 0, int(valuations.length) - 1);

        return balances;
    }

    /**
     * @notice Receives a structured sorted list of coins and balances which
     * could be used for swaping. Takes only credit balances from Insurance fund
     * If there are credit amount of LINKs, LINK will be first element of array
     *
     * @param tokens Tokens with which the LINK can be exchanged
     * @param amount Amount of LINK to exchange
     * @return array `tokens` balances.
     *
     * @dev This function gets credit balances for each token, calculates
     * valuations for each token and then sort arrays by valuations.
     *
     */
    function _calculateCreditBalances(
        address[] memory tokens,
        uint256 amount
    ) internal view returns (uint256[] memory) {
        uint256 size = tokens.length;

        uint256[] memory rates = new uint256[](size);
        uint256[] memory balances = new uint256[](size);

        uint256 sum = 0;
        uint256 linkBalance = 0;

        for (uint256 i = 0; i < size; i++) {
            // Get available credit amount
            balances[i] = _creditSystem.getAvaliableCreditBalance(tokens[i]);

            // Get amountOut of token in LINK equivalent with amountIn = 1
            rates[i] = _exchange.getAmountOut(tokens[i], LINK, 1e18);
            sum += balances[i] * rates[i];

            if (tokens[i] == LINK) {
                linkBalance = balances[i];
            }
        }

        if (sum == 0) return new uint256[](0);

        uint256[] memory valuations = new uint256[](rates.length);

        // Calculate valuation for each token
        for (uint256 i = 0; i < rates.length; i++) {
            valuations[i] = (balances[i] * rates[i] * 1e18) / sum;
        }

        // Sort by valuations
        valuations.quickSort(tokens, balances, 0, int(valuations.length) - 1);

        // Make LINK first element in arrays
        if (linkBalance > 0) {
            bool isLink = false;

            for (uint256 i = tokens.length - 1; i > 0; i--) {
                if (tokens[i] == LINK) isLink = true;

                if (isLink) {
                    balances[i] = balances[i - 1];
                    tokens[i] = tokens[i - 1];
                }
            }

            balances[0] = linkBalance;
            tokens[0] = LINK;
        }

        return balances;
    }
}

