import "./interfaces/exchange/IExchange.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

/**
 * @title Chainlink Economics
 *
 * This contract provides an opportunity to calculate a
 * how much commission is needed for Chainlink at the
 * moment.
 *
 **/
contract ChainlinkEconomics is Ownable {
    uint256 internal constant PERCENT_DECIMALS = 1e10;
    uint256 internal constant GAS_DECIMALS = 1e10;

    uint256 internal _premiumPercent = 1e9;
    uint256 internal _chainlinkConstant = 80000;

    address internal _native;
    address internal LINK;

    constructor(address native, address link) {
        _native = native;
        LINK = link;
    }

    /**
     * @notice Set new premium percent value
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setPremiumPercent(uint256 value) external onlyOwner {
        _premiumPercent = value;
    }

    /**
     * @notice Set new chainlink constant value
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setChainlinkConstant(uint256 value) external onlyOwner {
        _chainlinkConstant = value;
    }

    /**
     * @notice Get Fee
     *
     * @param exchange Adress of exchange
     * @param gasPrice Transaction gas price
     * @param gasUsed Amount of used gas
     * @return amount Fee
     * @dev This function is needed for LinkKeeper checkUpkeep function
     *
     */
    function getFee(
        address exchange,
        uint256 gasPrice,
        uint256 gasUsed
    ) external view returns (uint256) {
        uint256 rate = IExchange(exchange).getAmountOut(_native, LINK, 1);

        // TODO: 1 + _premiumPercent ?
        return
            ((gasUsed * gasPrice) / GAS_DECIMALS) *
            (1 + _premiumPercent / PERCENT_DECIMALS) +
            (((_chainlinkConstant * gasPrice) / GAS_DECIMALS) * rate);
    }
}

