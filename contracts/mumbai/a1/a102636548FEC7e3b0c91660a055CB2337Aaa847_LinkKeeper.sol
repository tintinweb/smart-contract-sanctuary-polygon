pragma solidity ^0.8.9;

library Utils {
    /**
     * @notice Quick Sort by `arr`
     * TODO: search best implementation
     */
    function quickSort(
        uint[] memory arr,
        address[] memory tokens,
        uint[] memory balances,
        int left,
        int right
    ) internal pure {
        int i = left;
        int j = right;
        if (i == j) return;
        uint pivot = arr[uint(left + (right - left) / 2)];
        while (i <= j) {
            while (arr[uint(i)] > pivot) i++;
            while (pivot > arr[uint(j)]) j--;
            if (i <= j) {
                (arr[uint(i)], arr[uint(j)]) = (arr[uint(j)], arr[uint(i)]);
                (tokens[uint(i)], tokens[uint(j)]) = (
                    tokens[uint(j)],
                    tokens[uint(i)]
                );
                (balances[uint(i)], balances[uint(j)]) = (
                    balances[uint(j)],
                    balances[uint(i)]
                );
                i++;
                j--;
            }
        }
        if (left < j) quickSort(arr, tokens, balances, left, j);
        if (i < right) quickSort(arr, tokens, balances, i, right);
    }
}

