pragma solidity ^0.8.4;
import "@openzeppelin/contracts-upgradeable/access/Ownable2StepUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";

contract HigherEducation is
    Initializable,
    Ownable2StepUpgradeable,
    AccessControlEnumerableUpgradeable
{
    // user roles for this Smart contracts
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN");
    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR");
    bytes32 public constant UNIVERSITY_ROLE = keccak256("UNIVERSITY");

    struct RotateRequest {
        address currentAddress;
        address newAddress;
        uint8 status; // 0 for pending and 1 for approved
    }

    mapping(address => RotateRequest) public rotateRequests;
    mapping(address => bool) public blackList;

    error InvalidUserRole();
    error NotUniversity();
    error ZeroAddress();



/// @custom:oz-upgrades-unsafe-allow constructor
constructor() {
    _disableInitializers();
}
    // Init smart contract intiale setup
    function __HigherEducation_init() internal onlyInitializing {
        _setRoleAdmin(DEFAULT_ADMIN_ROLE, ADMIN_ROLE);
        __Ownable_init_unchained();
    }

    function __HigherEducation_init_unchained() internal onlyInitializing {}

    // Intialize the contract
    function initialize() public  initializer {
        __HigherEducation_init();
    }

    // add uni users as operators
    function addOperator(address _operator) external onlyRole(ADMIN_ROLE) {
        _setupRole(OPERATOR_ROLE, _operator);
    }

    // add uni users as operators
    function removeOperator(address _operator) external onlyRole(ADMIN_ROLE) {
        revokeRole(OPERATOR_ROLE, _operator);
    }

    modifier onlyOwnerOrOperator() {
        if (
            hasRole(OPERATOR_ROLE, _msgSender()) ||
            hasRole(ADMIN_ROLE, _msgSender())
        ) revert InvalidUserRole();
        _;
    }

    modifier onlyUniversity() {
        if (hasRole(UNIVERSITY_ROLE, _msgSender())) revert InvalidUserRole();
        _;
    }

    // add uni users as operators
    function addUniversity(address _uinveristy) external onlyOwnerOrOperator {
        _setupRole(UNIVERSITY_ROLE, _uinveristy);
    }

    function addToBlackList(address _uinveristy) external onlyOwnerOrOperator {
        if (!hasRole(UNIVERSITY_ROLE, _uinveristy)) revert NotUniversity();
        blackList[_uinveristy] = true;
        revokeRole(UNIVERSITY_ROLE, _uinveristy);
    }

    function addRotationRequest(
        address _newUinveristy
    ) external onlyUniversity {
        RotateRequest memory _rotateRequests;
        _rotateRequests.currentAddress = _msgSender();
        _rotateRequests.newAddress = _newUinveristy;
        _rotateRequests.status = 0;
        rotateRequests[_msgSender()] = _rotateRequests;
    }

    function approveRotateRequest(
        address _currentAddress
    ) external onlyRole(ADMIN_ROLE) {
        RotateRequest memory _rotateRequests = rotateRequests[_currentAddress];
        if (_rotateRequests.newAddress == address(0)) revert ZeroAddress();
        _rotateRequests.status = 1;
        revokeRole(UNIVERSITY_ROLE, _currentAddress);
        _setupRole(UNIVERSITY_ROLE, _rotateRequests.newAddress);
    }

    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
     */
    uint256[50] private __gap;
}