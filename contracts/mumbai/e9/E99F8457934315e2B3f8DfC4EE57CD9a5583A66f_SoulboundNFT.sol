pragma solidity ^0.8.19;

import "@interfaces/ISoulboundNFT.sol";
import "@contracts/AbstractSoulboundNFT.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts/proxy/beacon/UpgradeableBeacon.sol";

/**
 * @title RTL
 * @dev A ERC721 & ERC5192 based contract for minting tokens.
 */
contract SoulboundNFT is
  PausableUpgradeable,
  OwnableUpgradeable,
  ReentrancyGuardUpgradeable,
  ISoulboundNFT,
  AbstractSoulboundNFT
{
  //--------------------------------------------------
  // Variables
  //--------------------------------------------------
  address public minter;
  uint256 public maxTotalMints;
  bool internal _isMinting;

  // URI
  string public baseTokenURI;

  //--------------------------------------------------
  // Constructor
  //--------------------------------------------------

  /**
   * @dev Initializes the contract.
   * @param name The name of the ERC721 token.
   * @param symbol The symbol of the ERC721 token.
   * @param _minter The address of the minter.
   */
  function initialize(
    string memory name,
    string memory symbol,
    address _minter,
    address _owner,
    uint256 _maxTotalMints,
    string memory _baseTokenURI
  ) public initializer {
    __AbstractSoulbound_init(name, symbol);
    __Pausable_init();
    __Ownable_init();
    __ReentrancyGuard_init();
    transferOwnership(_owner);

    _isMinting = true;
    minter = _minter;
    maxTotalMints = _maxTotalMints;
    baseTokenURI = _baseTokenURI;
  }

  //--------------------------------------------------
  // Minting
  //--------------------------------------------------
  /// @inheritdoc ISoulboundNFT
  function mint(
    address to
  )
    external
    virtual
    override
    whenNotPaused
    nonReentrant
    returns (uint256 tokenId)
  {
    require(
      _msgSender() == minter,
      "Soulbound NFT cannot be transferred, only minted by minter"
    );
    uint256 amount = 1;
    uint256 totalSupply = totalSupply();
    require(
      totalSupply + amount <= maxTotalMints,
      "Mint would reach max mints"
    );

    uint256 nextToken = _nextTokenId();
    _safeMint(to, amount);
    emit Mint(to, amount);

    _setLock(true, nextToken);
    return nextToken;
  }

  /**
   * @dev Returns the starting token ID.
   */
  function _startTokenId() internal pure virtual override returns (uint256) {
    return 0; // https://github.com/estarriolvetch/ERC721Psi/issues/7
  }

  //--------------------------------------------------
  // Soulbound
  //--------------------------------------------------

  /// @notice Ensures only unlocked (not yet minted in this case) tokens can be transfered.
  /// @dev Is called in _beforeTokenTransfers().
  function _tokenTransferGuard(
    address from,
    address to,
    uint256 startTokenId,
    uint256 quantity
  ) internal virtual override(AbstractSoulboundNFT) {
    for (uint256 i = 0; i < quantity; i++) {
      require(
        _locked[startTokenId + i] == false,
        "Soulbound NFT cannot be transferred, only minted by minter"
      );
    }
  }

  //--------------------------------------------------
  // Sale related
  //--------------------------------------------------
  /// @inheritdoc ISoulboundNFT
  function setMaxTotalMints(
    uint256 newMax
  ) external virtual override onlyOwner {
    require(newMax >= totalSupply());
    maxTotalMints = newMax;
    emit SetMaxTotalMints(maxTotalMints);
  }

  //--------------------------------------------------
  // URI related
  //--------------------------------------------------

  /**
   * @dev Returns the base URI for token metadata.
   */
  function _baseURI() internal view virtual override returns (string memory) {
    return baseTokenURI;
  }

  /// @inheritdoc ISoulboundNFT
  function setBaseURI(
    string calldata newBaseURI
  ) external virtual override onlyOwner {
    require(bytes(newBaseURI).length > 0, "URI cannot be empty");
    baseTokenURI = newBaseURI;
    emit SetBaseURI();
  }

  /**
   * @dev Returns the token URI for a given token ID.
   * @param tokenId The ID of the token.
   * @return The token URI.
   */
  function tokenURI(
    uint256 tokenId
  ) public view virtual override returns (string memory) {
    // To avoid an overflow when adding 1
    require(tokenId < type(uint256).max);
    require(_exists(tokenId), "URI query for nonexistent token");

    string memory baseURI = _baseURI();
    string memory _tokenURI = bytes(baseURI).length > 0
      ? string(
        abi.encodePacked(baseURI, StringsUpgradeable.toString(tokenId), ".json")
      )
      : "";

    return _tokenURI;
  }

  //--------------------------------------------------
  // Setters
  //--------------------------------------------------
  /// @inheritdoc ISoulboundNFT
  function changeMinter(address newMinter) external virtual onlyOwner {
    require(newMinter != address(0), "newMinter cannot be address(0)");
    require(newMinter != minter, "newMinter cannot be current minter");
    minter = newMinter;

    emit ChangeMinter(newMinter);
  }

  /// @inheritdoc ISoulboundNFT
  function changeOwner(address newOwner) external virtual onlyOwner {
    require(newOwner != address(0), "newOwner cannot be address(0)");
    require(newOwner != owner(), "newowner cannot be current owner");
    transferOwnership(newOwner);
  }

  /// @inheritdoc ISoulboundNFT
  function pauseContract() external virtual onlyOwner {
    _pause();
  }

  /// @inheritdoc ISoulboundNFT
  function unpauseContract() external virtual override onlyOwner {
    _unpause();
  }

  /// @dev This empty reserved space is put in place to allow future versions to add new
  /// variables without shifting down storage in the inheritance chain.
  /// See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
  uint256[46] private __gap;
}