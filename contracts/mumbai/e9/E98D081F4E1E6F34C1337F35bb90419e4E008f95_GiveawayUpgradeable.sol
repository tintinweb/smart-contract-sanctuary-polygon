        return
            verifier.verifyProof(
                proof_.a,
                proof_.b,
                proof_.c,
                [
                    withdrawInput_.root,
                    withdrawInput_.nullifierHash,
                    uint256(uint160(withdrawInput_.recipient)),
                    uint256(uint160(withdrawInput_.asset)),
                    withdrawInput_.value
