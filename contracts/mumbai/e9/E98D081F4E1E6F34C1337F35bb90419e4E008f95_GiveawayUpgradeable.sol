abstract contract VerifierGatewayUpgradeable is IVerifierGatewayUpgradeable, BaseUpgradeable {
    using BitMapsUpgradeable for BitMapsUpgradeable.BitMap;

    IVerifier public verifier;
    BitMapsUpgradeable.BitMap internal _roots;

    function __VerifierGateway_init(IVerifier verifier_) internal onlyInitializing {
        __VerifierGateway_init_unchained(verifier_);
    }

    function __VerifierGateway_init_unchained(IVerifier verifier_) internal onlyInitializing {
        _setVerifier(verifier_);
    }

