// SPDX-License-Identifier: MIT
pragma solidity ^0.8.2;

import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "./interface/IEODToken.sol";

contract EODInvestment is ERC1155Holder, Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    IEODToken private nftContract;


/* ========== EVENTS ========== */
    event ItemCreated(
        address indexed owner,
        uint256 indexed investType,
        uint256 indexed nftId,
        uint256 itemId
    );

    event ItemListedForSale(
        address indexed owner,
        uint256 indexed investType,
        uint256 indexed itemId
    );
    event ItemRemovedFromSale(
        address indexed owner,
        uint256 indexed investType,
        uint256 indexed itemId
    );

    event PaymentMade(
        address indexed payer,
        uint256 indexed investType,
        uint256 indexed itemId,
        uint256 amount,
        uint256 paymentNumber
    );

    address public admin;
    mapping(uint256 => mapping(uint256 => Item)) private items_;
    mapping(uint256 => uint256) public itemIds;
    address public currencyPayment;

    struct Item {
        address owner;
        address buyer;
        uint256 investType;
        uint256 nftId;
        uint256 itemId;
        string title;
        uint256 marketValue;
        uint256 downPayment;
        uint256 fullPayment;
        uint256 numPayments;
        uint256 numPaid;
        string imageURL;
        uint256 purchaseDate;
        bool forSale;
        bool sold;
        mapping(uint256 => bool) paymentHistory;
    }

    struct ItemView {
        address owner;
        address buyer;
        uint256 investType;
        uint256 nftId;
        uint256 itemId;
        string title;
        uint256 marketValue;
        uint256 downPayment;
        uint256 fullPayment;
        uint256 numPayments;
        uint256 numPaid;
        string imageURL;
        uint256 purchaseDate;
        bool forSale;
        bool sold;
    }

    constructor(address nftContract_, address currencyPayment_) {
        admin = msg.sender;
        nftContract = IEODToken(nftContract_);
        currencyPayment = currencyPayment_;
    }

    // ============ MODIFIER ============
    modifier onlyAdmin() {
        require(admin == msg.sender, "Ownable: caller is not the admin");
        _;
    }

    // ============ OWNER ============

    function setAdmin(address admin_) public onlyOwner {
        require(admin_ != address(0), "SetAdmin: Zero address owner");
        admin = admin_;
    }

    function setCurrencyPayment(address currencyPayment_) public onlyOwner {
        require(currencyPayment_ != address(0), "SetCurrencyPayment: Zero address currency payment");
        currencyPayment = currencyPayment_;
    }

    // ============ ADMIN ============
    function listForSale(uint256 investType_, uint256 itemId_) external onlyAdmin {
        require(
            items_[investType_][itemId_].owner == msg.sender,
            "Only the owner can list an item for sale"
        );
        items_[investType_][itemId_].forSale = true;
        emit ItemListedForSale(msg.sender, investType_, itemId_);
    }

    function removeItemFromSale(uint256 investType_, uint256 itemId_) external onlyAdmin {
        require(
            items_[investType_][itemId_].owner == msg.sender,
            "Only the owner can remove an item from sale"
        );

        require(
            items_[investType_][itemId_].forSale == false,
            "Item must NOT in list sale"
        );

        nftContract.safeTransferFrom(
            address(this),
            msg.sender,
            items_[investType_][itemIds[investType_]].nftId,
            items_[investType_][itemIds[investType_]].numPayments,
            ""
        );

        emit ItemRemovedFromSale(msg.sender, investType_, itemId_);
    }

    function listItem(
        uint256 investType_,
        uint256 nftId_,
        uint256 fullPayment_,
        uint256 numPayments_
    ) external  onlyAdmin {
        require(nftId_ > 0, "Token does not exist");
        itemIds[investType_]++;
        IEODToken.Metadata memory nftMetadata = nftContract.getMetadata(nftId_);
        items_[investType_][itemIds[investType_]].owner = msg.sender;
        items_[investType_][itemIds[investType_]].buyer = address(0);
        items_[investType_][itemIds[investType_]].investType = nftMetadata.investType;
        items_[investType_][itemIds[investType_]].nftId = nftId_;
        items_[investType_][itemIds[investType_]].itemId = itemIds[investType_];
        items_[investType_][itemIds[investType_]].title = nftMetadata.title;
        items_[investType_][itemIds[investType_]].marketValue = nftMetadata.marketValue;
        items_[investType_][itemIds[investType_]].downPayment = fullPayment_ / numPayments_;
        items_[investType_][itemIds[investType_]].fullPayment = fullPayment_;
        items_[investType_][itemIds[investType_]].numPayments = numPayments_;
        items_[investType_][itemIds[investType_]].purchaseDate = 0;
        items_[investType_][itemIds[investType_]].imageURL = nftMetadata.imageURL;
        items_[investType_][itemIds[investType_]].forSale = false;
        items_[investType_][itemIds[investType_]].sold = false;

        emit ItemCreated(msg.sender, investType_, nftId_, itemIds[investType_]);

        nftContract.safeTransferFrom(
            msg.sender,
            address(this),
            items_[investType_][itemIds[investType_]].nftId,
            items_[investType_][itemIds[investType_]].numPayments,
            ""
        );
    }

    // ============ PUBLIC ============
    
    function makePayment(
        uint256 investType_,
        uint256 itemId_,
        uint256 amount_
    ) public nonReentrant {
        require(
            items_[investType_][itemId_].forSale == true,
            "Item not made for sale"
        );
        require(
            items_[investType_][itemId_].numPaid <
                items_[investType_][itemId_].numPayments,
            "All payments have been made"
        );
        require(
            amount_ == items_[investType_][itemId_].downPayment ||
                amount_ == items_[investType_][itemId_].fullPayment,
            "Invalid payment amount"
        );

        uint256 amountAllowce_ = IERC20(currencyPayment).allowance(
            msg.sender,
            address(this)
        );
        require(amount_ <= amountAllowce_, "Payment: insufficient allowance");

        IERC20(currencyPayment).safeTransferFrom(
            msg.sender,
            address(this),
            amount_
        );

        uint256 paymentNumber = items_[investType_][itemId_].numPaid + 1;
        items_[investType_][itemId_].paymentHistory[paymentNumber] = true;
        items_[investType_][itemId_].numPaid++;
        items_[investType_][itemId_].purchaseDate = block.timestamp;
        items_[investType_][itemId_].sold = true;
        items_[investType_][itemId_].buyer = msg.sender;

        if (amount_ == items_[investType_][itemId_].fullPayment) {
            nftContract.safeTransferFrom(
                address(this),
                msg.sender,
                items_[investType_][itemId_].nftId,
                items_[investType_][itemId_].numPayments,
                ""
            );
        } else {
            nftContract.safeTransferFrom(
                address(this),
                msg.sender,
                items_[investType_][itemId_].nftId,
                1,
                ""
            );
        }

        emit PaymentMade(
            msg.sender,
            investType_,
            itemId_,
            amount_,
            paymentNumber
        );
    }

    // ============ INTERNALS ============

    // ============ VIEWS ============
    function getItem(uint256 investType_, uint256 itemId_)
        public
        view
        returns (ItemView memory)
    {
        ItemView memory itemView_ = ItemView({
            owner: items_[investType_][itemId_].owner,
            buyer: items_[investType_][itemId_].buyer,
            investType: items_[investType_][itemId_].investType,
            itemId: items_[investType_][itemId_].itemId,
            nftId: items_[investType_][itemId_].nftId,
            title: items_[investType_][itemId_].title,
            marketValue: items_[investType_][itemId_].marketValue,
            downPayment: items_[investType_][itemId_].downPayment,
            fullPayment: items_[investType_][itemId_].fullPayment,
            numPayments: items_[investType_][itemId_].numPayments,
            numPaid: items_[investType_][itemId_].numPaid,
            imageURL: items_[investType_][itemId_].imageURL,
            purchaseDate: items_[investType_][itemId_].purchaseDate,
            forSale: items_[investType_][itemId_].forSale,
            sold: items_[investType_][itemId_].sold
        });
        return itemView_;
    }

    function getListItems(
        uint256 investType_,
        uint256 limit_,
        uint256 offset_
    ) public view returns (ItemView[] memory) {
        ItemView[] memory itemViews_ = new ItemView[](limit_);
        for (uint256 index_ = 0; index_ < limit_; index_++) {
            uint256 itemId_ = offset_ + index_;
            itemViews_[index_].owner =  items_[investType_][itemId_].owner;
            itemViews_[index_].buyer =  items_[investType_][itemId_].buyer;
            itemViews_[index_].investType =  items_[investType_][itemId_].investType;
            itemViews_[index_].itemId =  items_[investType_][itemId_].itemId;
            itemViews_[index_].nftId =  items_[investType_][itemId_].nftId;
            itemViews_[index_].title =  items_[investType_][itemId_].title;
            itemViews_[index_].marketValue =  items_[investType_][itemId_].marketValue;
            itemViews_[index_].downPayment =  items_[investType_][itemId_].downPayment;
            itemViews_[index_].fullPayment =  items_[investType_][itemId_].fullPayment;
            itemViews_[index_].numPayments =  items_[investType_][itemId_].numPayments;
            itemViews_[index_].numPaid =  items_[investType_][itemId_].numPaid;
            itemViews_[index_].imageURL =  items_[investType_][itemId_].imageURL;
            itemViews_[index_].purchaseDate =  items_[investType_][itemId_].purchaseDate;
            itemViews_[index_].forSale =  items_[investType_][itemId_].forSale;
            itemViews_[index_].sold =  items_[investType_][itemId_].sold;
        }
        return itemViews_;
    }

    function getPaymentHistory(
        uint256 investType_,
        uint256 itemId_,
        uint256 paymentId_
    ) public view returns (bool) {
        require(
            items_[investType_][itemId_].itemId == itemId_,
            "Item not found"
        );

        return items_[investType_][itemId_].paymentHistory[paymentId_];
    }
}

