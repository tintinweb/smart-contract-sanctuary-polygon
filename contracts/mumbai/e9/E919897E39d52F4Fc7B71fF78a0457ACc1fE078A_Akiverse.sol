pragma solidity ^0.8.18;

import "./ERC721Template.sol";

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";

contract Akiverse is ERC721Template {
    using ECDSA for bytes32;
    uint256 public constant FREE_MINT_FLAG = 10;
    mapping(address => uint256) public freeMintMax;
    mapping(address => uint256) public freeMintCount;

    constructor(
        string memory _name,
        string memory _symbol,
        string memory _baseURI,
        uint16 maxSupply_,
        address withdrawAddress,
        address _whitelistSignerAddress,
        uint256 _whitelistMintPrice,
        uint256 _publicMintPrice,
        address[] memory _freeMintAddresses,
        uint256[] memory _freeMintMax
    )
        ERC721Template(
            _name,
            _symbol,
            _baseURI,
            maxSupply_,
            withdrawAddress,
            _whitelistSignerAddress,
            _whitelistMintPrice,
            _publicMintPrice
        )
    {
        for (uint256 i; i < _freeMintAddresses.length; i++) {
            freeMintMax[_freeMintAddresses[i]] = _freeMintMax[i];
        }
    }

    function whitelistMint2(
        uint256 _amount,
        bytes calldata nonce,
        bytes calldata signature
    ) external payable {
        // Check if user is whitelisted
        require(
            whitelistSigned(msg.sender, nonce, signature, stage),
            "Invalid Signature!"
        );

        // Check if whitelist sale is open
        require(stage == 2, "Whitelist Mint 2 is not open");

        // Check if enough ETH is sent
        require(msg.value == _amount * whitelistMintPrice, "Insufficient ETH!");

        // Check if mints does not exceed MAX_SUPPLY
        require(totalSupply() + _amount <= MAX_SUPPLY, "Exceeded Max Supply!");

        for (uint256 i; i < _amount; ) {
            uint256 tokenId = _useRandomAvailableTokenId();
            _safeMint(msg.sender, tokenId);
            unchecked {
                ++i;
            }
        }
    }

    function authenticate(
        address sender,
        bytes calldata nonce,
        bytes calldata signature
    ) private view returns (bool) {
        bytes32 _hash = keccak256(
            abi.encodePacked(sender, nonce, FREE_MINT_FLAG)
        );
        return
            WHITELIST_SIGNER_ADDRESS ==
            ECDSA.toEthSignedMessageHash(_hash).recover(signature);
    }

    function freeMint(
        uint256 _amount,
        bytes calldata nonce,
        bytes calldata signature
    ) external {
        require(
            authenticate(msg.sender, nonce, signature),
            "Invalid Signature for free mint!"
        );

        // Check if mints does not exceed MAX_SUPPLY
        require(totalSupply() + _amount <= MAX_SUPPLY, "Exceeded Max Supply!");
        // Check if amount is not more than freeMintMax
        require(
            freeMintCount[msg.sender] + _amount <= freeMintMax[msg.sender],
            "Exceeded Free Mint Max!"
        );
        freeMintCount[msg.sender] += _amount;
        for (uint256 i; i < _amount; ) {
            uint256 tokenId = _useRandomAvailableTokenId();
            _safeMint(msg.sender, tokenId);
            unchecked {
                ++i;
            }
        }
    }

    function setFreeMintMax(
        address[] memory _addresses,
        uint256[] memory _max
    ) external onlyOwner {
        for (uint256 i; i < _addresses.length; i++) {
            freeMintMax[_addresses[i]] = _max[i];
        }
    }
}

