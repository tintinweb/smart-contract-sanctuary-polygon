        _transferNFT(msg.sender, poolContracts[_id], totalSupply);

        // Update the NFT metadata for the participant's pick
        _updateTokenURI(poolContracts[_id], totalSupply, metadata);

        // Emit EnteredPool event when a player enters a pool
        emit EnteredPool(_id, msg.sender, pools[_id].spots);
    }

    function changePick(uint256 _id, uint256 _newPick) public {
        require(_id != 0);
        require(_id <= totalPools);
        require(hasEntered[_id][msg.sender], "Participant has not entered the pool");

        // Calculate the valid range of picks based on the pool ID
        uint256 validRangeStart = (_id - 1) * (_id - 1) + 1;
        uint256 validRangeEnd = _id * _id;

        require(_newPick >= validRangeStart && _newPick <= validRangeEnd, "Invalid pick");

        // Check if the pick change is allowed before the deadline
        require(block.timestamp < pools[_id].pickDeadline, "Pick deadline has passed");

        // Update the participant's pick
        participantPicks[_id][msg.sender] = _newPick;

        // Generate the metadata for the updated pick
        string memory metadata = generateTokenURI(_id, _newPick);

        // Update the NFT metadata for the participant's pick
        _updateTokenURI(poolContracts[_id], totalSupply, metadata);

        // Emit PickChanged event
        emit PickChanged(_id, msg.sender, _newPick);
    }

    function generateTokenURI(
        uint256 _id,
        uint256 _pick
    ) private pure returns (string memory) {
        // Generate the metadata JSON string
        string memory metadata = string(
            abi.encodePacked(
                '{"name":"Week ',
                Strings.toString(_id),
                '","attributes":[{"trait_type":"Pick ',
                Strings.toString(_pick),
                '","value":"Pick ',
                Strings.toString(_pick),
                '"}]}'
            )
        );

        return metadata;
    }

    // Placeholder functions - Replace with your own implementation
    function _transferNFT(
