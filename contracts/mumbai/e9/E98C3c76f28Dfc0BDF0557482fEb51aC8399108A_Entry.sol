    uint256 public totalSupply;
    uint256 public totalPicks;

    Entry public entryContract; // instance of the Entry contract

    // Add these debugging events
    event PickChanged(uint256 indexed id, address indexed participant, uint256 newPick);
    event EnteredPool(uint256 poolId, address entrant, uint256 remainingSpots);
    event ListedPool(
        uint256 poolId,
        uint256 weekId,
        string name,
        uint256 cost,
        uint256 spots
    );

    struct Pool {
        uint256 id;
        uint256 weekId;
        string name;
        uint256 cost;
        uint256 spots;
        string date;
        string time;
        uint256 entryDeadline;
        uint256 pickDeadline;
    }

    struct Week {
        uint256 id;
        string name;
        uint256 entryDeadline;
        uint256 pickDeadline;
        uint256 selectionDeadline;
        bool hasPassed;
    }

