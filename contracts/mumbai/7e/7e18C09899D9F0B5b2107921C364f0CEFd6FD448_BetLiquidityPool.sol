pragma solidity ^0.8.14;

import "./LiquidityBox.sol";
import "./interfaces/ILiquidityPool.sol";
import "./interfaces/tokens/ILPToken.sol";


/**
 * @title Liquidity Pool
 * @author Michael Larsen
 * @notice This is the liquidity provider contract that owns tokens.
 * @notice Tokens are deposited by users, that will receive LP tokens
 *         as proof-of-deposit.
 * @notice The depoisited tokens can be retrieved, by swapping the
 *         LP tokens back.
 * @notice LiquidityBox is Accesshandler, Accesshandler is Initializable.
 */
contract LiquidityPool is LiquidityBox, ILiquidityPool {
    using SafeMath for uint256;

    constructor() LiquidityBox() {}

    /**
     * @notice Deposits tokens and updates the users locked amount.
     * @param tokenAdd The address of the token type to deposit.
     * @param amount The amount to add, including the fee.
     */
    function depositToken(address tokenAdd, uint256 amount)
        external
        override
        onlyAllowedToken(tokenAdd)
        isInitialized
    {
        address owner = msg.sender;
        if (amount == 0) {
            emit DepositZero(owner, tokenAdd);
            return;
        }
        deposit(owner, tokenAdd, amount);
    }

    /**
     * @notice Let the owner of deposited tokens withdraw them all again.
     * @param lpTokenAdd The lpToken type to return for the withdraw.
     */
    function withDrawAllTokens(address lpTokenAdd)
        external
        override
        onlyAllowedDistToken(lpTokenAdd)
    {
        address owner = msg.sender;
        uint256 lpTokenBalance = ILPToken(lpTokenAdd).balanceOf(owner);
        if (lpTokenBalance == 0) {
            emit WithdrawZero(owner, lpTokenAdd);
            return;
        }
        withdraw(owner, lpTokenAdd, lpTokenBalance);
    }

    /**
     * @notice Let the owner of deposited tokens withdraw them again.
     * @param lpTokenAdd The address of the lpToken type to return.
     * @param amount The amount of lpTokens to return.
     */
    function withDrawTokens(address lpTokenAdd, uint256 amount)
        external
        override
        onlyAllowedDistToken(lpTokenAdd)
    {
        address owner = msg.sender;
        withdraw(owner, lpTokenAdd, amount);
    }

    /**
     * @notice Let the owner of this contract return all tokens to a user.
     * @param owner The user to update.
    *  @param lpTokenAdd The address of the lpToken type to return.
     */
    function returnAllLPTokensAsAdmin(address owner, address lpTokenAdd)
        external
        override
        onlyAllowedDistToken(lpTokenAdd)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {   // TODO: Determine if this is sufficient admin override to support our needs.
        uint256 lpTokenBalance = ILPToken(lpTokenAdd).balanceOf(owner);
        withdraw(owner, lpTokenAdd, lpTokenBalance);
    }

    /**
     * @notice Let the owner of this contract return all tokens to a user.
     * @param owner The user to update.
     * @param lpTokenAdd The address of the lpToken type to return.
     */
    function returnMaxLPTokensAsAdmin(address owner, address lpTokenAdd)
        external
        override
        onlyAllowedDistToken(lpTokenAdd)
        onlyRole(DEFAULT_ADMIN_ROLE)
    {   // TODO: Determine if this is sufficient admin override to support our needs.
        ILPToken lpToken = ILPToken(lpTokenAdd);
        uint256 lpTokenBalance = lpToken.balanceOf(owner);

        // Calc the token amount available for withdrawal, translated into LPToken amount
        address tokenAdd = lpToken.tokenAdd();
        uint256 tokenBalanceLP = getTokenBalance(tokenAdd);
        uint256 matchingLPTokenBalanceLP = getMatchingDistTokenAmount(tokenAdd, tokenBalanceLP);
        uint256 allowance = lpToken.allowance(owner, address(this));

        uint256 available = lpTokenBalance;
        if (matchingLPTokenBalanceLP < available) available = matchingLPTokenBalanceLP;
        if (allowance < available) available = allowance;
        if (available == 0) {
            emit WithdrawZero(owner, lpTokenAdd);
            return;
        }

        withdraw(owner, lpTokenAdd, available);
    }

}

