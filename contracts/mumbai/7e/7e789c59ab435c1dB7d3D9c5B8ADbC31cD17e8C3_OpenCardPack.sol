//-------------------------- OpenCardPack --------------------------//

contract OpenCardPack is
  VRFConsumerBaseV2,
  ConfirmedOwner,
  Pausable,
  ReentrancyGuard
{
  event PackOpened(
    address indexed user,
    uint256 indexed packId,
    uint256[] cardIds,
    uint256[] cardAmounts
  );
  event RequestFulfilled(address indexed opener);

  VRFCoordinatorV2Interface COORDINATOR;
  // chainlink subscription ID.
  uint64 s_subscriptionId;
  bytes32 private keyHash =
    0x4b09e658ed251bcafeebbc69400383d49f344ace09b9576fe248bb02c003fe9f; //MuMBAI
  uint32 private callbackGasLimit = 2000000;
  uint16 private requestConfirmations = 3;

  //LOCAL VRF CONSUMER BASE 0X = 0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266
  //MUMBAI VRF CONSUMER BASE 0X = 0x7a1BaC17Ccc5b313516C5E16fb24f7659aA5ebed
  constructor(
    uint64 subscriptionId
  )
    VRFConsumerBaseV2(0x7a1BaC17Ccc5b313516C5E16fb24f7659aA5ebed)
    ConfirmedOwner(msg.sender)
  {
    COORDINATOR = VRFCoordinatorV2Interface( // <------------------NEED TO SWITCH FOR VRF
      0x7a1BaC17Ccc5b313516C5E16fb24f7659aA5ebed //mumbai
      // 0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266 //local test
    );
    s_subscriptionId = subscriptionId;
  }

  /** BURN INTERACTION **/

  address public mintTokenAddress; //cards contract
  address public burnTokenAddress; //fp contract

  //set mint token address (cards contract)
  function setMintTokenAddress(address _mintTokenAddress) external onlyOwner {
    mintTokenAddress = _mintTokenAddress;
  }

  //set burn token address (pack contract)
  function setBurnTokenAddress(address _burnTokenAddress) public onlyOwner {
    burnTokenAddress = _burnTokenAddress;
  }

  /** OPENING PACKS **/
  //mapping for address to minted token count
  // mapping(address => uint256) public addressOpened;

  function startOpenPack(uint256 _packId) public nonReentrant whenNotPaused {
    //require user has a token to burn
    require(
      FunctionInterface(burnTokenAddress).balanceOf(msg.sender, _packId) > 0,
      "No pack."
    );

    Library.CardPack memory pack = FunctionInterface(burnTokenAddress).packInfo(
      _packId
    );

    if (pack.packType == Library.PackType.Basic) {
      console.log("basic pack");
      //TODO: get all basic type cards and mint them (JUST 1 OF EACH???) ---------------THIS SHOULD BE HARDCODED FOR BASIC??????
      uint256[] memory cardIds = FunctionInterface(mintTokenAddress)
        .getCardIdsOfRarity(Library.Rarity.Basic); //there should be 30 of these

      console.log("cardIds Length: %s", cardIds.length);

      uint256[] memory cardAmounts = new uint256[](cardIds.length); // ------------------------------HARDCODED FOR BASIC TEMPORARY????
      for (uint256 i = 0; i < cardIds.length; i++) {
        cardAmounts[i] = 1;
      }

      FunctionInterface(burnTokenAddress).burn(msg.sender, _packId, 1);

      FunctionInterface(mintTokenAddress).mintBatch(
        msg.sender,
        cardIds,
        cardAmounts
      );
      //emit event
      emit PackOpened(msg.sender, _packId, cardIds, cardAmounts);
      console.log("basic pack opened");
      return;
    } else {
      // it's a random pack
      //check something about user already requesting all they needed so they can't spam
      // if(s_userRequests[msg.sender].length >= FunctionInterface(burnTokenAddress).balanceOf(msg.sender, _packId)) { //not this
      //   revert("You've already requested all the cards you can.");
      // }
      //get random number with vrf and rarity percentages -----------------MOVE REQUEST TO MINT MAYBE
      requestRandomWords(_packId); //the tokens get minted in the callback function
    }
  }

  function finishOpenPack(uint256 _packId) public nonReentrant whenNotPaused {
    //require user has a token to burn
    require(
      FunctionInterface(burnTokenAddress).balanceOf(msg.sender, _packId) > 0,
      "No pack."
    );

    //get request id from mapping with user address
    uint256[] memory userRequestIds = s_userRequests[msg.sender];
    require(userRequestIds.length > 0, "Start opening a pack first.");

    //check each id until it finds one that hasn't been used yet AND IS FOR THE PACK ID
    uint256 chosenId;
    for (uint256 i = 0; i < userRequestIds.length; i++) {
      if (
        s_requests[userRequestIds[i]].used == false &&
        s_requests[userRequestIds[i]].packId == _packId &&
        s_requests[userRequestIds[i]].fulfilled == true
      ) {
        chosenId = userRequestIds[i];
        s_requests[userRequestIds[i]].used = true;
        break;
      }
      //revert if all requests have been used
      if (i == userRequestIds.length - 1) {
        revert("All requests have been used or still processing.");
      }
    }
    //burn token
    FunctionInterface(burnTokenAddress).burn(msg.sender, _packId, 1);

    doMintingLogic(chosenId);
  }

  // uint32 private numWords = 30;

  //setGasLimit
  function setCallbackGasLimit(uint32 _callbackGasLimit) public onlyOwner {
    callbackGasLimit = _callbackGasLimit;
  }

  struct RequestStatus {
    address opener;
    uint256 packId;
    uint256[] randomWords;
    uint256[] cardIds;
    uint256[] cardAmounts;
    bool exists;
    bool fulfilled;
    bool used;
  }
  mapping(uint256 => RequestStatus) public s_requests; //requestId to request status struct
  mapping(address => uint256[]) public s_userRequests; //user to request ids

  // Assumes the subscription is funded sufficiently.
  function requestRandomWords(
    uint256 _packId
  ) internal returns (uint256 requestId) {
    uint32 cardCount = FunctionInterface(burnTokenAddress).getCardCount(
      _packId
    );

    //TEST REQUEST ID
    // requestId = 83132445710567745731713214319446805456689061274219614556348005953705680908145; // <----------------TESTING

    // Will revert if subscription is not set and funded. <------------------NEED TO UNCOMMENT FOR VRF
    requestId = COORDINATOR.requestRandomWords(
      keyHash,
      s_subscriptionId,
      requestConfirmations,
      callbackGasLimit,
      cardCount //number of words to return
    );
    console.log("msg.sender in requestRandomWords", msg.sender);
    s_requests[requestId] = RequestStatus({
      opener: msg.sender,
      packId: _packId,
      randomWords: new uint256[](0),
      cardIds: new uint256[](0),
      cardAmounts: new uint256[](0),
      exists: true,
      fulfilled: false,
      used: false
    });
    s_userRequests[msg.sender].push(requestId);
    // console.log(s_userRequests[msg.sender][0]);
    // emit RequestSent(requestId, numWords);
    return requestId;
  }

  // //I DONT THINK THIS IS NEEDED BUT MAYBE SOMETHING THAT RETURNS ONLY FULFILLED BUT NOT USED REQUESTS
  // function getRequestIdsByOpener(address _opener)
  //   public
  //   view
  //   returns (uint256[] memory)
  // {
  //   return s_userRequests[_opener];
  // }

  function fulfillRandomWords(
    uint256 _requestId,
    uint256[] memory _randomWords
  ) internal override {
    // console.log("IN FULFILLRANDOMWORDS");
    require(s_requests[_requestId].exists, "request not found");
    s_requests[_requestId].fulfilled = true;
    s_requests[_requestId].randomWords = _randomWords;

    // console.log("fulfillRandomWords DONE", _requestId, _randomWords.length);
    emit RequestFulfilled(s_requests[_requestId].opener);
  }

  /** ACTIVATION **/

  function pause() public onlyOwner {
    _pause();
  }

  function unpause() public onlyOwner {
    _unpause();
  }

  Library.CardType[] private cardTypes;

  function doMintingLogic(uint256 _requestId) private {
    Library.CardPack memory pack = FunctionInterface(burnTokenAddress).packInfo(
      s_requests[_requestId].packId
    );

    if (pack.packType == Library.PackType.Any) {
      console.log("ANY");
      cardTypes.push(Library.CardType.Creature);
      cardTypes.push(Library.CardType.Spell);
      cardTypes.push(Library.CardType.Relic);
    } else if (pack.packType == Library.PackType.Creatures) {
      console.log("CREATURES");
      cardTypes.push(Library.CardType.Creature);
    } else if (pack.packType == Library.PackType.Spells_Relics) {
      console.log("SPELLS RELICS");
      cardTypes.push(Library.CardType.Spell);
      cardTypes.push(Library.CardType.Relic);
    } else revert("Pack type not found");

    uint256 count = 0;
    bool hasLeggo = false;

    console.log(
      "magic number: ",
      s_requests[_requestId].randomWords[count] % 10000
    );
    //if rnadom number is less than the leggo percentage, then they get a leggo
    if (
      s_requests[_requestId].randomWords[count] % 10000 <
      pack.rarityAllocations[5]
    ) {
      hasLeggo = true;
    }
    console.log("LEGGO???:", hasLeggo);

    for (uint256 i = pack.rarityAllocations.length - 1; i >= 0; i--) {
      if (pack.rarityAllocations[i] == 0 && i == 0) break;
      if (pack.rarityAllocations[i] == 0) continue;

      if (hasLeggo && i == 5) {
        uint256[] memory leggos = FunctionInterface(mintTokenAddress)
          .getCardsOfRarityTypeRaceElement(
            Library.Rarity(i),
            cardTypes,
            pack.races,
            pack.elements
          );
        addCardToBeMinted(
          _requestId,
          leggos[s_requests[_requestId].randomWords[count] % leggos.length]
        );

        // because a leggo was chosen, we need to remove an allocation from the next rarity down
        pack.rarityAllocations[i - 1] > 0
          ? pack.rarityAllocations[i - 1] -= 1
          : pack.rarityAllocations[i - 2] -= 1;

        count++;
        continue;
      } else if (!hasLeggo && i == 5) {
        continue; //skip if no leggo and we are on the leggo rarity (5
      } else {
        uint256[] memory cardsOfRarityTypeRaceElement = FunctionInterface(
          mintTokenAddress
        ).getCardsOfRarityTypeRaceElement(
            Library.Rarity(i),
            cardTypes,
            pack.races,
            pack.elements
          );

        // if (cardsOfRarityTypeRaceElement.length == 0) continue; // TODO: REVERT HERE? - skip if no cards of that rarity/race/elem exist

        for (uint256 j = 0; j < pack.rarityAllocations[i]; j++) {
          addCardToBeMinted(
            _requestId,
            cardsOfRarityTypeRaceElement[
              s_requests[_requestId].randomWords[count] %
                cardsOfRarityTypeRaceElement.length
            ]
          );
          count++;
        }
      }
    }
    clearCardTypesArray();

    // loop and log card ids and amounts
    uint256 totalAmount;
    for (uint256 i = 0; i < s_requests[_requestId].cardIds.length; i++) {
      console.log(
        "card id: ",
        s_requests[_requestId].cardIds[i],
        "amount: ",
        s_requests[_requestId].cardAmounts[i]
      );
      totalAmount += s_requests[_requestId].cardAmounts[i];
    }
    console.log("total amount: ", totalAmount);

    //mint cards
    FunctionInterface(mintTokenAddress).mintBatch(
      s_requests[_requestId].opener,
      s_requests[_requestId].cardIds,
      s_requests[_requestId].cardAmounts
    );

    //emit packopened event
    emit PackOpened(
      s_requests[_requestId].opener,
      s_requests[_requestId].packId,
      s_requests[_requestId].cardIds,
      s_requests[_requestId].cardAmounts
    );
  }

  // frontend can use this to display how many packs are available to open
  function getFulfilledByAddress(
    address _address
  ) public view returns (uint256[] memory) {
    //get all s_userRequests for this address
    uint256[] memory requestIds = s_userRequests[_address];
    uint256[] memory fulfilledRequestIds = new uint256[](requestIds.length);
    uint256 fulfilledRequestIdsCount = 0;
    for (uint256 i = 0; i < requestIds.length; i++) {
      if (
        s_requests[requestIds[i]].fulfilled && !s_requests[requestIds[i]].used
      ) {
        fulfilledRequestIds[fulfilledRequestIdsCount] = requestIds[i];
        fulfilledRequestIdsCount++;
      }
    }
    //remove empty elements from array
    uint256[] memory returnArray = new uint256[](fulfilledRequestIdsCount);
    for (uint256 i = 0; i < fulfilledRequestIdsCount; i++) {
      returnArray[i] = fulfilledRequestIds[i];
    }

    return returnArray;
  }

  function addCardToBeMinted(
    uint256 _requestId,
    uint256 _randomCardId
  ) private {
    //check if rabdom card iud is already in data.cardIdsToMint array, if it is increment the amount, if it doesn't exist in the array add it
    bool cardIdExists = false;
    for (uint256 k = 0; k < s_requests[_requestId].cardIds.length; k++) {
      if (
        //if the card id already exists in the array then increment the amount
        s_requests[_requestId].cardIds[k] == _randomCardId
      ) {
        // console.log("card id exists");
        s_requests[_requestId].cardAmounts[k]++; //increment the amount
        cardIdExists = true;
        break;
      }
    }

    //if the card id doesn't exist in the array then add it
    if (!cardIdExists) {
      // console.log("card id doesn't exist");
      s_requests[_requestId].cardIds.push(_randomCardId); //add the card id to the array
      s_requests[_requestId].cardAmounts.push(1); //add the amount to the array
    }
  }

  function clearCardTypesArray() private {
    for (uint256 k = 0; k < cardTypes.length; k++) {
      delete cardTypes[k];
    }
  }
}

