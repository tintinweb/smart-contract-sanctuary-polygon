import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";

import "./AssetCore.sol";
import "../interfaces/ILandLPToken.sol";

contract TemplateCore is AssetCore {
    using ECDSA for bytes32;

    // random box id => template ids
    mapping(uint256 => uint256[]) public randomTemplateIds;
    // random box id => required LP token amount
    mapping(uint256 => uint256) public requiredLPTokenAmount;
    // template id => required material ids
    mapping(uint256 => uint256[]) public mergeRequiredMaterials;
    // template id => required material id => required material amount
    mapping(uint256 => mapping(uint256 => uint256))
        public mergeRequiredMaterialAmount;

    // address => minted amount
    mapping(address => uint256) internal _templateMintedAmount;
    // address => random box id => finish timestamp
    mapping(address => mapping(uint256 => uint256)) public finishTimestampe;

    struct RequiredMaterialInfo {
        uint256 materialId;
        uint256 amount;
    }

    struct RequiredMaterialInfoInput {
        uint256 templateId;
        uint256[] materialIds;
        uint256[] materialAmounts;
    }

    struct RandomTemplateIdsInput {
        uint256 randomBoxId;
        uint256[] templateIds;
        uint256 requiredLPTokenAmount;
    }

    function mergeTemplateMint(
        uint256 templateId,
        uint256 amount
    ) public onlyTemplate(templateId) {
        uint256[] memory materialIds = mergeRequiredMaterials[templateId];

        require(materialIds.length > 0, "TemplateCore: not merge template");

        for (uint256 i = 0; i < materialIds.length; i++) {
            require(
                balanceOf(msg.sender, materialIds[i]) >=
                    mergeRequiredMaterialAmount[templateId][materialIds[i]] *
                        amount,
                "TemplateCore: insufficient material"
            );
            _burn(
                msg.sender,
                materialIds[i],
                mergeRequiredMaterialAmount[templateId][materialIds[i]] * amount
            );
        }

        _mint(msg.sender, templateId, amount, "");

        _templateMintedAmount[msg.sender] += amount;
    }

    function randomTemplateMint(uint256 randomBoxId, uint256 amount) public {
        require(
            finishTimestampe[msg.sender][randomBoxId] < block.timestamp,
            "TemplateCore: not yet"
        );

        uint256 _requiredLPTokenAmount = requiredLPTokenAmount[randomBoxId];

        require(
            _requiredLPTokenAmount > 0,
            "TemplateCore: not random template"
        );

        require(
            IERC20(lpToken).balanceOf(msg.sender) >= _requiredLPTokenAmount,
            "TemplateCore: insufficient LP token"
        );

        ILandLPToken(lpToken).burn(msg.sender, _requiredLPTokenAmount);

        for (uint256 i = 0; i < amount; i++) {
            uint256 randomIndex = uint256(
                keccak256(
                    abi.encodePacked(
                        uint160(address(this)),
                        uint256(block.timestamp),
                        uint256(block.number),
                        tx.gasprice,
                        blockhash(block.number - 1),
                        msg.sender,
                        i
                    )
                )
            ) % randomTemplateIds[randomBoxId].length;
            uint256 templateId = randomTemplateIds[randomBoxId][randomIndex];

            _mint(msg.sender, templateId, 1, "");
        }

        finishTimestampe[msg.sender][randomBoxId] = block.timestamp + 1 days;

        _templateMintedAmount[msg.sender] += amount;
    }

    /**
     * ===================
     * Get Functions
     * ==================
     */

    function getTier(uint256 templateId) public pure returns (uint256) {
        return templateId / 2 + 1;
    }

    /**
     *
     * @return gender 0: female, 1: mall
     */
    function getGender(uint256 templateId) public pure returns (uint256) {
        return templateId % 2;
    }

    function getRemainTimestamp(
        address account,
        uint256 templateCategory
    ) external view returns (uint256) {
        if (
            finishTimestampe[account][templateCategory] == 0 ||
            finishTimestampe[account][templateCategory] >= block.timestamp
        ) {
            return 0;
        }
        return block.timestamp - finishTimestampe[account][templateCategory];
    }

    function getMintedAmount(address account) external view returns (uint256) {
        return _templateMintedAmount[account];
    }

    function getRequiredMaterials(
        uint256 _templateId
    )
        public
        view
        onlyTemplate(_templateId)
        returns (RequiredMaterialInfo[] memory)
    {
        uint256[] memory materialIds = mergeRequiredMaterials[
            uint256(_templateId)
        ];
        RequiredMaterialInfo[]
            memory requiredMaterials = new RequiredMaterialInfo[](
                materialIds.length
            );
        for (uint256 i = 0; i < materialIds.length; i++) {
            requiredMaterials[i] = RequiredMaterialInfo({
                materialId: materialIds[i],
                amount: mergeRequiredMaterialAmount[uint256(_templateId)][
                    materialIds[i]
                ]
            });
        }
        return requiredMaterials;
    }

    /**
     * ===================
     * Set Functions
     * ==================
     */

    function setRandomTemplateInfo(
        RandomTemplateIdsInput[] memory randomTemplateIdsInput
    ) public onlyOwner {
        for (uint256 i = 0; i < randomTemplateIdsInput.length; i++) {
            randomTemplateIds[
                randomTemplateIdsInput[i].randomBoxId
            ] = randomTemplateIdsInput[i].templateIds;
            requiredLPTokenAmount[
                randomTemplateIdsInput[i].randomBoxId
            ] = randomTemplateIdsInput[i].requiredLPTokenAmount;
        }
    }

    function setMergeRequiredMaterials(
        RequiredMaterialInfoInput[] memory requiredMaterialInfoInputs
    ) public onlyOwner {
        for (uint256 i = 0; i < requiredMaterialInfoInputs.length; i++) {
            uint256 templateId = requiredMaterialInfoInputs[i].templateId;
            uint256[] memory materialIds = requiredMaterialInfoInputs[i]
                .materialIds;
            uint256[] memory materialAmounts = requiredMaterialInfoInputs[i]
                .materialAmounts;
            mergeRequiredMaterials[templateId] = materialIds;
            for (uint256 j = 0; j < materialIds.length; j++) {
                mergeRequiredMaterialAmount[templateId][
                    materialIds[j]
                ] = materialAmounts[j];
            }
        }
    }

    uint256[44] private __gap;
}

