pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "../interfaces/IAssetNFT.sol";

contract AssetCore is
    ERC1155URIStorageUpgradeable,
    OwnableUpgradeable,
    IAssetNFT
{
    enum AssetType {
        Building,
        Material,
        Template
    }
    address public tavaToken;
    address public lpToken;
    address public farmingContract;

    modifier onlyBuildingFarmingContract() {
        require(
            msg.sender == farmingContract,
            "AssetCore: caller is not the farming contract"
        );
        _;
    }

    modifier onlyBuilding(uint256 id) {
        require(
            getAssetType(id) == AssetType.Building,
            "AssetCore: id is not the building"
        );
        _;
    }

    modifier onlyMaterial(uint256 id) {
        require(
            getAssetType(id) == AssetType.Material,
            "AssetCore: id is not the material"
        );
        _;
    }

    modifier onlyTemplate(uint256 id) {
        require(
            getAssetType(id) == AssetType.Template,
            "AssetCore: id is not the template"
        );
        _;
    }

    function mintByFarmingContract(
        address account,
        uint256 id,
        uint256 amount
    ) public onlyBuildingFarmingContract {
        _mint(account, id, amount, "");
    }

    function burnByFarmingContract(
        uint256 id,
        uint256 amount
    ) public onlyBuildingFarmingContract {
        _burn(farmingContract, id, amount);
    }

    function burn(address account, uint256 id, uint256 amount) public {
        require(
            account == _msgSender() || isApprovedForAll(account, _msgSender()),
            "AssetCore: caller is not token owner or approved"
        );
        _burn(account, id, amount);
    }

    function getAssetType(uint256 id) public pure returns (AssetType) {
        if (id < 10000) {
            return AssetType.Building;
        } else if (id < 20000) {
            return AssetType.Material;
        } else {
            return AssetType.Template;
        }
    }

    function supportsInterface(
        bytes4 interfaceId
    ) public view virtual override(ERC1155Upgradeable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    /**
     * ========================
     * Owner Functions
     * ========================
     */
    function tavaWithdraw(uint256 amount) public onlyOwner {
        IERC20(tavaToken).transfer(msg.sender, amount);
    }

    function setFarmingContract(address _farmingContract) public onlyOwner {
        farmingContract = _farmingContract;
    }

    uint256[47] private __gap;
}

