pragma solidity ^0.8.11;
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "hardhat/console.sol";
import "./interfaces/IStaking.sol";

contract Fundraise is AccessControl, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using ECDSA for bytes32;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    bytes32 public constant VALIDATOR_ROLE = keccak256("VALIDATOR_ROLE");

    enum CampaignState {
        Unknown,
        Active,
        CompletedSuccessfully,
        CompletedUnsuccessfully
    }

    // Сampaign contains initial information about the project.
    struct Campaign {
        uint256 StartIn; // Campaign start time
        uint256 EndIn; // Campaign end time
        address AcceptedToken; // Token accepted for exchange
        address RewardTokenAddress; // Project token
        uint256 RewardAmount; // Amount to be exchanged
        uint256 TokenRate; // Exchange rate
        uint256 MinGoal; // The minimum exchange amount to consider the campaign successful
        uint256 MaxGoal; // Maximum exchange amount stable coin
        address Owner; // Project owner
        CampaignState State; // Current state
    }

    // CampaignInfo contains information about tokens in campaign.
    struct CampaignInfo {
        uint256 ReceivedFromUsers; //Total amount of stable coin(accepted token)
        uint256 AccumulatedForPayout; //Total amount of tokens which users can claim in campaign
        uint256 PaidOut; //Total amount of reward tokens, already, paid to user
        uint256 Refunded; //Total amount of tokens refunded in unsuccessful campaign
        VestingRule[] Vesting; //Array of struct Vesting rule which contain info about vesting in this campaign
    }

    struct User {
        uint256 Accumulated; //Await `claimToken`
        uint256 PaidOut; //Paid to user in `claimToken`
        uint256 ReceivedFromUser; //Received from user in stable coin
    }

    struct VestingRule {
        uint256 UnlockTime; //Number of block which give ability to claim reward tokens
        uint256 UnlockPercent; //Number of opened percent
    }

    //List of information about users
    //users[userAddress][campaignId] = User struct
    mapping(address => mapping(uint256 => User)) public users;

    //List of information about Campaign
    //campaignInfo[campaignId] = struct campaignInfo
    mapping(uint256 => CampaignInfo) public campaignsInfo;

    //List of tokens accepted for exchange
    //AcceptedTokenList[_AcceptedToken] = true or false
    mapping(address => bool) public AcceptedTokenList;

    //Messages which had ever use
    //messages_ids[message_hash] = true or false
    mapping(bytes32 => bool) messages_ids;

    // Structures with information about the Campaign
    Campaign[] private campaigns;

    //Array of stakings
    address[] private stakings;

    address public treasury;

    /**
     * @dev Emitted when `setStakings` set's new addresses for stakings
     */
    event StakingAddressChanged(address[] newStaking);

    /**
     * @dev Emitted when user `registration` in Campaign
     */
    event UserRegistered(address user, uint256 unlockTime);

    /**
     * @dev Emitted when `createCampaign` created new Campaign
     */
    event campaignCreated(
        uint256 CampaignID,
        uint256 StartIn,
        uint256 EndIn,
        address AcceptedToken,
        address RewardTokenAddress,
        uint256 RewardAmount,
        uint256 TokenRate,
        address Owner,
        uint256 MinGoal,
        uint256 MaxGoal
    );

    /**
     * @dev Emitted when someone has refund token amount
     */
    event Refunded(
        uint256 indexed CampaignID,
        address indexed Sender,
        address indexed Token,
        uint256 Amount
    );

    /**
     * @dev Emitted when someone has claimed token amount
     */
    event TokenClaimed(
        uint256 indexed CampaignID,
        address indexed Sender,
        address indexed Token,
        uint256 Amount
    );

    /**
     * @dev Emitted when `finishCampaign` finished active Campaign
     */
    event campaignFinished(
        uint256 indexed CampaignId,
        CampaignState State,
        uint256 ReceivedFromUsers,
        uint256 AccumulatedForPayout
    );

    /**
     * @dev Emitted when `buyTokens` swap tokens to user
     */
    event Swapped(
        address indexed sender,
        uint256 indexed campaignId,
        uint256 amount,
        uint256 receivedTokens,
        uint256 receivedFromUser,
        uint256 accumulated,
        uint256 timestamp
    );

    /**
     * @dev Emitted when `setAdmin` seted new admin address
     */
    event AdminSeted(address newAdmin);

    /**
     * @dev Emitted when `updateTokenList` change state of token
     */
    event TokenListUpdated(address indexed updatedToken, bool state);

    /**
     * @dev Contract constructor
     */
    constructor(address _validator) public {
        // Grant the contract deployer the default admin role: it will be able
        // to grant and revoke any roles
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(ADMIN_ROLE, msg.sender);
        _setRoleAdmin(VALIDATOR_ROLE, ADMIN_ROLE);
        // Sets `ADMIN_ROLE` as ``'s admin role.
        _grantRole(VALIDATOR_ROLE, _validator);
        treasury = msg.sender;
    }

    /** @notice Initialize ido.
     * @dev  Create new capaign using campaign struct and array of Vesting structs.
     * @dev  emit campaignCreated event.
     * @param _startIn Time then campaign start
     * @param _endIn Time then campaign end
     * @param _acceptedToken Address of token, which users will sell for rewardTokens
     * @param _rewardAmount Amount of reward tokens selled in campaign
     * @param _tokenRate Reward tokens attitude to acceptedTokens
     * @param _owner Address of Ido owner.
     * @param _minGoal Minimum amount of reward tokens for successful campaign
     * @param _maxGoal Maximum amount of rewardTokens, which owner want to sell
     * @param Vesting Array of `Vesting` structs which define `claim` rules.
    */
    function createCampaign(
        uint256 _startIn,
        uint256 _endIn,
        address _acceptedToken,
        address _rewardTokenAddress,
        uint256 _rewardAmount,
        uint256 _tokenRate,
        address _owner,
        uint256 _minGoal,
        uint256 _maxGoal,
        VestingRule[] calldata Vesting
    )
        external
        returns (
            uint256 _CampaignId
        )
    {
        require(hasRole(ADMIN_ROLE, msg.sender), "Caller is not an admin.");
        require(
            _startIn < _endIn,
            "The start campaign time must be less then end time."
        );
        require(
            block.timestamp < _startIn,
            "The start campaign time must be in the future."
        );
        require(
            _minGoal < _maxGoal,
            "The min goal amount must be less then max goal amount."
        );
        require(
            AcceptedTokenList[_acceptedToken] == true,
            "Token is not in the list of accepted tokens."
        );
        require(
            AcceptedTokenList[_rewardTokenAddress] == true,
            "Token is not in the list of accepted tokens."
        );
        require(_owner != address(0), "Owner address must be defined.");
        require(_rewardAmount > 0, "Token amount must be greater than zero.");
        require(_tokenRate > 0, "Token Rate must be greater than zero.");

        _CampaignId = campaigns.length;
        
        {
            uint256 initBalance = IERC20(_rewardTokenAddress).balanceOf(
                address(this)
            );

            // transfer specified amount of project tokens to the contract
            IERC20(_rewardTokenAddress).transferFrom(
                msg.sender,
                address(this),
                _rewardAmount
            );

            uint256 finalBalance = IERC20(_rewardTokenAddress).balanceOf(
                address(this)
            ) - initBalance;

            if (_rewardAmount != finalBalance) {
                _rewardAmount = finalBalance;
            }
        }

        campaigns.push(
            Campaign({
                StartIn: _startIn,
                EndIn: _endIn,
                AcceptedToken: _acceptedToken,
                RewardTokenAddress: _rewardTokenAddress,
                RewardAmount: _rewardAmount,
                TokenRate: _tokenRate,
                Owner: _owner,
                MinGoal: _minGoal,
                MaxGoal: _maxGoal,
                State: CampaignState.Active
            })
        );
        
        {
            CampaignInfo storage campaignInfo = campaignsInfo[_CampaignId];
        
            uint256 totalPercent;

            for (uint128 i; i < Vesting.length; i++) {
                require(
                    Vesting[i].UnlockTime > _endIn,
                    "Vesting time can not be less then finish time"
                );
                campaignInfo.Vesting.push(Vesting[i]);
                totalPercent += Vesting[i].UnlockPercent;
            }
            require(totalPercent == 100000, "Invalid vesting rules");
        }
      
        emit campaignCreated(
            _CampaignId,
            _startIn,
            _endIn,
            _acceptedToken,
            _rewardTokenAddress,
            _rewardAmount,
            _tokenRate,
            _owner,
            _minGoal,
            _maxGoal
        );
    }

    /** @notice Register user on campaign.
     * @dev  Used signed message for confimation transaction.
     * @dev  emit UserRegistered event.
     * @param _campaignId Id of choisen campaign.
     * @param v part of signed message
     * @param r part of signed message
     * @param s part of signed message
     */
    function registration(
        uint256 _campaignId,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external {
        bytes32 message = keccak256(abi.encodePacked(msg.sender, _campaignId));
        require(messages_ids[message] == false, "Transaction in process");

        require(
            hasRole(
                VALIDATOR_ROLE,
                message.toEthSignedMessageHash().recover(v, r, s)
            ),
            "Validator address is invalid"
        );
        require(_campaignId < campaigns.length, "Invalid campaign id");

        messages_ids[message] = true;
        uint256 campaignEndTime = campaigns[_campaignId].EndIn;
        lockStakesOfUser(msg.sender, campaignEndTime);

        emit UserRegistered(msg.sender, campaignEndTime);
    }

    /** @notice Buy tokens by the user.
     * @dev  Used signed message for confimation transaction.
     * @dev  can finish campaign if reached `maxGoal`.
     * @dev  emit Swapped event.
     * @param _campaignId Id of choisen campaign.
     * @param _amount Amount of `Accepted` tokens that user pay for `Reward` tokens of campaign.
     * @param v part of signed message
     * @param r part of signed message
     * @param s part of signed message
     * @param nonce random number for signed message
     */
    function buyTokens(
        uint256 _campaignId,
        uint256 _amount,
        uint8 v,
        bytes32 r,
        bytes32 s,
        uint256 nonce
    ) external {
        bytes32 message = keccak256(
            abi.encodePacked(msg.sender, _campaignId, _amount, nonce)
        );
        require(messages_ids[message] == false, "Transaction in process");

        require(
            hasRole(
                VALIDATOR_ROLE,
                message.toEthSignedMessageHash().recover(v, r, s)
            ),
            "Validator address is invalid"
        );

        Campaign storage campaign = campaigns[_campaignId];
        CampaignInfo storage campaignInfo = campaignsInfo[_campaignId];
        User storage userInfo = users[msg.sender][_campaignId];

        require(
            campaign.StartIn <= block.timestamp &&
                campaign.EndIn >= block.timestamp,
            "Campaign time wrong"
        );

        lockStakesOfUser(msg.sender, campaign.EndIn);

        messages_ids[message] = true;

        IERC20(campaign.AcceptedToken).safeTransferFrom(
            msg.sender,
            address(this),
            _amount
        );

        userInfo.ReceivedFromUser += _amount;
        campaignInfo.ReceivedFromUsers += _amount;

        require(
            campaignInfo.ReceivedFromUsers <= campaign.MaxGoal,
            "Exchange amount exceeds MaxGoal."
        );

        uint256 acceptDecimals = calcDecimal(campaign.AcceptedToken);
        uint256 rewardDecimals = calcDecimal(campaign.RewardTokenAddress);

        uint256 tokens = (_amount 
            * rewardDecimals 
            * (1000)) 
            / (campaign.TokenRate) 
            / acceptDecimals;

        userInfo.Accumulated += tokens;
        campaignInfo.AccumulatedForPayout += tokens;

        emit Swapped(
            msg.sender,
            _campaignId,
            _amount,
            tokens,
            userInfo.ReceivedFromUser,
            userInfo.Accumulated,
            block.timestamp
        );

        if (campaignInfo.ReceivedFromUsers == campaign.MaxGoal) {
            finishCampaignAndSetState(_campaignId);
        }
    }

    /** @notice Finish campaign.
     * @dev can finish campaign if reached `maxGoal` or `endIn`.
     * @dev emit campaignFinished event.
     * @param _campaignId Id of choisen campaign.
     */
    function finishCampaign(uint256 _campaignId) external {
        require(hasRole(ADMIN_ROLE, msg.sender), "Caller is not an admin.");
        Campaign storage campaign = campaigns[_campaignId];
        require(
            block.timestamp >= campaign.EndIn,
            "Campaign end time has not yet arrived"
        );

        require(
            campaign.State == CampaignState.Active,
            "The campaign status must be active"
        );

        finishCampaignAndSetState(_campaignId);
    }

    /** @notice Refund `Accepted` tokens if campaign finished unsuccessful.
     * @dev  emit Refunded event.
     * @param _campaignId Id of choisen campaign.
     */
    function claimRefund(uint256 _campaignId) external nonReentrant {
        Campaign storage campaign = campaigns[_campaignId];
        CampaignInfo storage campaignInfo = campaignsInfo[_campaignId];
        User storage userInfo = users[msg.sender][_campaignId];

        require(
            campaign.State == CampaignState.CompletedUnsuccessfully,
            "Campaign must be completed unsuccessfully."
        );

        require(
            userInfo.ReceivedFromUser > 0,
            "There is nothing to claiming, the balance is zero."
        );

        uint256 _amount = userInfo.ReceivedFromUser;

        IERC20(campaign.AcceptedToken).safeTransfer(msg.sender, _amount);

        campaignInfo.Refunded += _amount;
        campaignInfo.ReceivedFromUsers -= _amount;
        campaignInfo.AccumulatedForPayout -= userInfo.Accumulated;

        userInfo.ReceivedFromUser = 0;
        userInfo.Accumulated = 0;

        emit Refunded(_campaignId, msg.sender, campaign.AcceptedToken, _amount);
    }

    /** @notice Claim `Reward` tokens if campaign finished successful.
     * @dev  emit TokenClaimed event.
     * @param _campaignId Id of choisen campaign.
     */
    function claimToken(uint256 _campaignId) external nonReentrant {
        Campaign storage campaign = campaigns[_campaignId];
        CampaignInfo storage campaignInfo = campaignsInfo[_campaignId];
        User storage userInfo = users[msg.sender][_campaignId];

        require(
            campaign.State == CampaignState.CompletedSuccessfully,
            "Campaign must be completed successfully"
        );

        require(
            userInfo.Accumulated - (userInfo.PaidOut) > 0,
            "There is nothing to claiming, the balance is zero."
        );

        uint256 amount = ((userInfo.Accumulated *
            calcAvailablePercentToClaim(_campaignId)) / 100000) -
            userInfo.PaidOut;

        userInfo.PaidOut += amount;
        campaignInfo.PaidOut += amount;

        IERC20(campaign.RewardTokenAddress).safeTransfer(msg.sender, amount);

        emit TokenClaimed(
            _campaignId,
            msg.sender,
            campaign.RewardTokenAddress,
            amount
        );
    }

    /** @notice Withdraw amount of choisen tokens from contract
     * @param _token Amount of tokens for withdraw.
     * @param _receiver Address of tokens receiver.
     * @param _amount Amount of tokens for withdraw.
     */
    function withdrawToken(
        address _token,
        address _receiver,
        uint256 _amount
    ) external nonReentrant {
        require(
            hasRole(ADMIN_ROLE, msg.sender),
            "Caller is not an token controller."
        );
        require(_receiver != address(0), "Invalid receiver address");

        IERC20(_token).safeTransfer(_receiver, _amount);
    }

    /**
     * @dev Add or remove a token address from the list of allowed to be accepted for exchange
     */
    function UpdateTokenList(address _token, bool _state) external {
        require(hasRole(ADMIN_ROLE, msg.sender), "Caller is not an admin.");
        require(_token != address(0), "Token address can't be address(0).");

        AcceptedTokenList[_token] = _state;

        emit TokenListUpdated(_token, _state);
    }

    /** @notice Set new addresses of stakigns on contract.
     * @dev  emit `StakingAddressChanged` event.
     * @param _newStaking array of address's.
     */
    function setStakings(address[] calldata _newStaking) external {
        require(hasRole(ADMIN_ROLE, msg.sender), "Caller is not an admin.");
        stakings = _newStaking;

        emit StakingAddressChanged(_newStaking);
    }

    /** @notice Set new address for admin.
     * @dev  emit `AdminSeted` event.
     * @param _newAdmin Address of new admin
     */
    function setAdmin(address _newAdmin) external {
        require(hasRole(ADMIN_ROLE, msg.sender), "Caller is not an admin.");
        require(_newAdmin != address(0), "Invalid address");

        treasury = _newAdmin;

        emit AdminSeted(_newAdmin);
    }

    function lockStakesOfUser(address _user, uint256 _unlockTime) private {
        require(_unlockTime > block.timestamp, "Invalid unlock time.");
        for (uint8 i; i < stakings.length; i++) {
            if (IStaking(stakings[i]).getUserAmountStake(_user) != 0) {
                IStaking(stakings[i]).setHardLockTimeForUser(
                    _user,
                    _unlockTime
                );
            }
        }
    }

    /** @notice Finish campaign and set new state `successful` or `unsuccessful`.
     * @dev  emit `campaignFinished` event.
     * @param _campaignId Id of choisen campaign.
     */
    function finishCampaignAndSetState(uint256 _campaignId) private {
        Campaign storage campaign = campaigns[_campaignId];
        CampaignInfo storage campaignInfo = campaignsInfo[_campaignId];
        if (campaignInfo.ReceivedFromUsers >= campaign.MinGoal) {
            campaign.State = CampaignState.CompletedSuccessfully;

            IERC20(campaign.AcceptedToken).safeTransfer(
                treasury,
                campaignInfo.ReceivedFromUsers
            );

            if (campaign.RewardAmount > campaignInfo.AccumulatedForPayout) {
                IERC20(campaign.RewardTokenAddress).safeTransfer(
                    treasury,
                    (campaign.RewardAmount - campaignInfo.AccumulatedForPayout)
                );
            }
        } else {
            campaign.State = CampaignState.CompletedUnsuccessfully;

            IERC20(campaign.RewardTokenAddress).safeTransfer(
                treasury,
                campaign.RewardAmount
            );
        }

        emit campaignFinished(
            _campaignId,
            campaign.State,
            campaignInfo.ReceivedFromUsers,
            campaignInfo.AccumulatedForPayout
        );
    }

    function calcAvailablePercentToClaim(uint256 _campaignId)
        private
        view
        returns (uint256)
    {
        CampaignInfo storage campaignInfo = campaignsInfo[_campaignId];
        require(
            campaignInfo.Vesting[0].UnlockTime <= block.timestamp,
            "Time have not came"
        );

        uint256 availablePercent;
        for (uint256 i = 0; i < campaignInfo.Vesting.length; i++) {
            if (campaignInfo.Vesting[i].UnlockTime <= block.timestamp) {
                availablePercent += campaignInfo.Vesting[i].UnlockPercent;
                continue;
            }
            break;
        }
        return availablePercent;
    }

    function calcDecimal(address _token) private view returns (uint256) {
        uint8 tokenDecimal = IERC20Metadata(_token).decimals();
        require(tokenDecimal <= 18, "Too high decimals.");
        return 10**tokenDecimal;
    }

    /**
     * @dev Returns campaigns info
     *
     * Parameters:
     *
     * - `_campaign_id` - list of campaign identifiers
     */
    function getCampaignInfo(uint256[] memory _campaign_id)
        external
        view
        returns (CampaignInfo[] memory campaignsInfoList)
    {
        campaignsInfoList = new CampaignInfo[](_campaign_id.length);

        for (uint256 i = 0; i < _campaign_id.length; i++) {
            campaignsInfoList[i] = CampaignInfo({
                ReceivedFromUsers: campaignsInfo[_campaign_id[i]]
                    .ReceivedFromUsers,
                AccumulatedForPayout: campaignsInfo[_campaign_id[i]]
                    .AccumulatedForPayout,
                PaidOut: campaignsInfo[_campaign_id[i]].PaidOut,
                Refunded: campaignsInfo[_campaign_id[i]].Refunded,
                Vesting: campaignsInfo[_campaign_id[i]].Vesting
            });
        }

        return campaignsInfoList;
    }

    function getStakings() external view returns (address[] memory) {
        return stakings;
    }

    /**
     * @dev Returns campaigns info
     *
     * Parameters:
     *
     * - `_campaign_id` - list of campaign identifiers
     */
    function getCampaigns(uint256[] memory _campaign_id)
        external
        view
        returns (Campaign[] memory campaignsList)
    {
        campaignsList = new Campaign[](_campaign_id.length);

        for (uint256 i = 0; i < _campaign_id.length; i++) {
            campaignsList[i] = Campaign({
                StartIn: campaigns[_campaign_id[i]].StartIn,
                EndIn: campaigns[_campaign_id[i]].EndIn,
                MinGoal: campaigns[_campaign_id[i]].MinGoal,
                MaxGoal: campaigns[_campaign_id[i]].MaxGoal,
                AcceptedToken: campaigns[_campaign_id[i]].AcceptedToken,
                RewardTokenAddress: campaigns[_campaign_id[i]]
                    .RewardTokenAddress,
                RewardAmount: campaigns[_campaign_id[i]].RewardAmount,
                TokenRate: campaigns[_campaign_id[i]].TokenRate,
                Owner: campaigns[_campaign_id[i]].Owner,
                State: campaigns[_campaign_id[i]].State
            });
        }

        return campaignsList;
    }

    /**
     * @dev Returns user info for specified campaign ID
     *
     * Parameters:
     *
     * - `_user` - user address
     * - `_campaign_id` - campaign identifier
     */
    function getUserInfo(address[] memory _users, uint256 _campaign_id)
        external
        view
        returns (User[] memory userList)
    {
        userList = new User[](_users.length);
        for (uint256 i; i < _users.length; i++) {
            userList[i] = User({
                Accumulated: users[_users[i]][_campaign_id].Accumulated,
                PaidOut: users[_users[i]][_campaign_id].PaidOut,
                ReceivedFromUser: users[_users[i]][_campaign_id]
                    .ReceivedFromUser
            });
        }

        return (userList);
    }

    function getAcceptedTokenList(address _token) external view returns (bool) {
        return AcceptedTokenList[_token];
    }
}

