pragma solidity ^0.8.9;
pragma abicoder v2;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "@uniswap/lib/contracts/libraries/TransferHelper.sol";

import "./CreditSystem.sol";

import "./interfaces/IInsuranceFund.sol";

import "./MixinResolver.sol";

/**
 * @title D4X Insurance Fund
 *
 * This contract contains the main profit of the service that can be used to cover the risks
 * of the protocol. If there are not enough funds on this contract, the risks will be assigned
 * to the ALP contract. This contract is not public because there are restrictions on calling
 * methods.
 *
 **/
contract InsuranceFund is IInsuranceFund, Ownable, CreditSystem {
    bytes32 private constant D4X_CONTRACT = "D4X";
    address public _d4x;

    constructor(address resolver) CreditSystem(resolver) {}

    /**
     * @notice to set the addresses required for the contract
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function resolverAddressesRequired()
        public
        pure
        override
        returns (bytes32[] memory addresses)
    {
        bytes32[] memory prev = super.resolverAddressesRequired();
        addresses = new bytes32[](prev.length + 1);

        uint256 i = 0;
        for (; i < prev.length; i++) {
            addresses[i] = prev[i];
        }

        addresses[i] = D4X_CONTRACT;
    }

    /**
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function _resolveAddressesFinish() internal override {
        super._resolveAddressesFinish();

        _d4x = requireAndGetAddress(D4X_CONTRACT);
    }

    /**
     * @param token Token to be sent to cover loss
     * @param amount Amount of tokens
     * @return Amount of the tokens transfered to msg.sender
     * @dev The minimum value between 'amount' and balancOf of will be transfer to msg.sender
     */
    function coverLoss(
        address token,
        uint256 amount
    ) external returns (uint256) {
        require(_d4x == msg.sender, "InsuranceFund: Caller is not d4x");

        uint256 balance = IERC20(token).balanceOf(address(this));

        if (balance == 0) return 0;

        uint256 oldCreditBalance = getAvailableCreditBalance(token);

        if (balance < amount) {
            amount = balance;
        }

        TransferHelper.safeTransfer(token, msg.sender, amount);
        uint256 creditBalance = getAvailableCreditBalance(token);

        emit CoverLoss(
            msg.sender,
            token,
            amount,
            oldCreditBalance - creditBalance
        );

        return amount;
    }

    /**
     * @notice Withdraw `value` of `token` to owner
     * @param token Token to be sent to withdraw
     * @param value Amount of the token to withdraw
     *
     * Requirements:
     *
     * - `value` cannot be less then token balance of
     * - can only be called by the current owner.
     *
     */
    function withdraw(address token, uint256 value) external onlyOwner {
        uint256 freeBalance = this.getFreeBalance(token);
        require(freeBalance >= value, "InsuranceFund: NOT_ENOUGH");

        TransferHelper.safeTransfer(token, msg.sender, value);

        emit Withdraw(msg.sender, token, value);
    }

    // LINE_SALT_PROD_{1687522099}

    // LINE_SALT_PROD_{1687522208}
}

