pragma solidity ^0.8.9;
pragma abicoder v2;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@uniswap/lib/contracts/libraries/TransferHelper.sol";

import "./MixinResolver.sol";

import "./interfaces/ICreditSystem.sol";
import "./interfaces/IFactory.sol";
import "./interfaces/exchange/IExchange.sol";

import "hardhat/console.sol";

/**
 * @title Credit System
 *
 * This contract provides an opportunity for anyone to make a
 * credit deposit in the Insurance Fund
 *
 **/
abstract contract CreditSystem is ICreditSystem, MixinResolver, Ownable {
    bytes32 internal constant FACTORY_CONTRACT = "Factory";
    bytes32 internal constant CREDIT_KEEPER_CONTRACT = "CreditKeeper";
    bytes32 internal constant LINK_KEEPER_CONTRACT = "LinkKeeper";
    bytes32 internal constant LINK_TOKEN = "LinkToken";

    /// @dev Holds a mapping of users to coins to amounts.
    mapping(address => mapping(address => uint)) _credits;

    /// @dev Holds a mapping of coins to credit amounts.
    mapping(address => uint) _creditCoins;

    /// @dev Array of all coins which is in credit position
    address[] _coins;

    /// @dev Holds a mapping of coins to indexes (+1) in array `_coins`.
    mapping(address => uint) _coinsIndexes;

    bool public _keeperSupported;

    IFactory public _factory;
    address _creditKeeper;
    address _linkKeeper;

    address LINK;

    modifier onlyKeeper() {
        require(
            msg.sender == _linkKeeper ||
                msg.sender == _creditKeeper ||
                msg.sender == address(this),
            "CreditSystem: No access"
        );
        _;
    }

    constructor(address resolver) MixinResolver(resolver) {}

    function keeperSupport(bool val) external {
        _keeperSupported = val;
    }

    /**
     * @notice to set the addresses required for the contract
     *
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function resolverAddressesRequired()
        public
        pure
        virtual
        override
        returns (bytes32[] memory addresses)
    {
        addresses = new bytes32[](4);
        addresses[0] = FACTORY_CONTRACT;
        addresses[1] = LINK_TOKEN;

        addresses[2] = CREDIT_KEEPER_CONTRACT;
        addresses[3] = LINK_KEEPER_CONTRACT;
    }

    /**
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function _resolveAddressesFinish() internal virtual override {
        _factory = IFactory(requireAndGetAddress(FACTORY_CONTRACT));
        LINK = requireAndGetAddress(LINK_TOKEN);

        if (_keeperSupported) {
            _creditKeeper = requireAndGetAddress(CREDIT_KEEPER_CONTRACT);
            _linkKeeper = requireAndGetAddress(LINK_KEEPER_CONTRACT);
        }
    }

    /**
     * @notice Get all credit coins
     * @return coins Array of coins
     * @dev This function provides all coins that have
     * credits at the moment
     */
    function getCoins() external view returns (address[] memory) {
        return _coins;
    }

    /**
     * @notice Get all used credit coins
     * @return coins Array of coins
     * @return amounts Array of amounts
     * @dev This function provides all coins that have
     * used credits at the moment and which needs to be covered
     */
    function getUsedCoins()
        external
        view
        returns (address[] memory coins, uint256[] memory amounts)
    {
        uint size;

        for (uint i = 0; i < _coins.length; i++) {
            uint256 usedBalance = this.getUsedCreditBalance(_coins[i]);

            if (usedBalance > 0) size++;
        }

        coins = new address[](size);
        amounts = new uint256[](size);

        uint j = 0;

        for (uint i = 0; i < _coins.length; i++) {
            uint256 usedBalance = this.getUsedCreditBalance(_coins[i]);

            if (usedBalance > 0) {
                coins[j] = _coins[i];
                amounts[j++] = usedBalance;
            }
        }
    }

    /**
     * @notice Get used credit `amount` by `coin`
     * @param coin Token address
     * @param amount Token used amount
     */
    function getUsedCreditBalance(
        address coin
    ) external view returns (uint amount) {
        uint256 balance = IERC20(coin).balanceOf(address(this));
        return _creditCoins[coin] > balance ? _creditCoins[coin] - balance : 0;
    }

    /**
     * @notice Get available credit balance by `coin`
     * @param coin Token address
     * @return amount Token credit balance
     */
    function getAvailableCreditBalance(
        address coin
    ) public view returns (uint256) {
        uint256 balance = IERC20(coin).balanceOf(address(this));
        return _creditCoins[coin] > balance ? balance : _creditCoins[coin];
    }

    /**
     * @notice Get free `amount` by `coin` (not credit)
     * @param coin Token address
     * @param amount Token used amount
     */
    function getFreeBalance(address coin) external view returns (uint amount) {
        uint256 balance = IERC20(coin).balanceOf(address(this));
        return _creditCoins[coin] > balance ? 0 : balance - _creditCoins[coin];
    }

    /**
     * @notice Transfer credit `coin` `amount`
     * @param coin Token address
     * @param amount Token amount
     * @dev This function calls when `coin` credits
     * needs to be used
     *
     * Requirements:
     *
     * - only LinkKeeper or IF can call this function
     *
     */
    function transferCreditCoins(
        address coin,
        uint256 amount
    ) external onlyKeeper {
        uint256 creditBalance = this.getAvailableCreditBalance(coin);

        if (creditBalance >= amount) {
            TransferHelper.safeTransfer(coin, msg.sender, amount);

            emit TransferCreditCoins(coin, amount);
        }
    }

    /**
     * @notice Transfer free `coin` `amount`
     * @param coin Token address
     * @param amount Token amount
     *
     * Requirements:
     *
     * - only LinkKeeper or InsuranseFund can call this function
     *
     */
    function transferFreeCoins(
        address coin,
        uint256 amount
    ) external onlyKeeper {
        uint256 free = this.getFreeBalance(coin);

        if (free >= amount) {
            TransferHelper.safeTransfer(coin, msg.sender, amount);

            emit TransferFreeCoins(coin, amount);
        }
    }

    /**
     * @notice Deposit `amount` of coins as credit to Insurance Fund.
     * @param coin Token address to be sent
     * @param amount Amount of tokens
     *
     * Requirements:
     *
     * - `amount` cannot be less then msg.sender balance.
     * - the caller must approve `coin` for at least `amount`.
     * - `coin` must be ALP token or LINK
     *
     */
    function depositCredit(address coin, uint256 amount) external {
        address[] memory tokens = _factory.getTokens();
        bool isCoinAvailable = false;

        if (coin == LINK) isCoinAvailable = true;
        else {
            for (uint i = 0; i < tokens.length; i++) {
                if (coin == tokens[i]) {
                    isCoinAvailable = true;
                    break;
                }
            }
        }

        require(isCoinAvailable, "CreditSystem: Incorrect token");

        TransferHelper.safeTransferFrom(
            coin,
            msg.sender,
            address(this),
            amount
        );

        _credits[msg.sender][coin] += amount;
        _creditCoins[coin] += amount;

        emit DepositCredit(msg.sender, coin, amount);

        if (_coinsIndexes[coin] == 0) {
            _coins.push(coin);
            _coinsIndexes[coin] = _coins.length;
        }
    }

    /**
     * @notice Withdraw `amount` of credited tokens from IF.
     * @param coin Token address to be sent
     * @param amount Amount of tokens
     *
     * Requirements:
     *
     * - balance of `coin` cannot be less then `amount`.
     * - credit balance of `coin` by msg.sender cannot be less then `amount`.
     *
     */
    function withdrawCredit(address coin, uint256 amount) public {
        uint256 balance = IERC20(coin).balanceOf(address(this));
        require(balance >= amount, "InsuranceFund: NOT_ENOUGH_BALANCE");
        require(
            _credits[msg.sender][coin] >= amount,
            "InsuranceFund: INCORRECT_AMOUNT"
        );

        TransferHelper.safeTransfer(coin, msg.sender, amount);
        _credits[msg.sender][coin] -= amount;
        _creditCoins[coin] -= amount;

        emit WithdrawCredit(msg.sender, coin, amount);

        if (_creditCoins[coin] == 0) {
            uint256 coinIndex = _coinsIndexes[coin] - 1;
            address lastCoin = _coins[_coins.length - 1];

            _coinsIndexes[lastCoin] = coinIndex + 1;
            _coinsIndexes[coin] = 0;

            _coins[coinIndex] = lastCoin;

            _coins.pop();
        }
    }

    /**
     * @notice Withdraw all amount of `coin` (which was credited by msg.sender
     * from Insurance Fund)
     * @param coin Token address to be sent
     */
    function withdrawAll(address coin) external {
        uint256 amount = _credits[msg.sender][coin];
        withdrawCredit(coin, amount);
    }

    /**
     * @notice Get credit balance for all coins
     * @return coins Array of current balance for all coins
     */
    function getCreditBalance()
        public
        view
        returns (CreditCoinParams[] memory)
    {
        CreditCoinParams[] memory result = new CreditCoinParams[](
            _coins.length
        );

        for (uint i = 0; i < _coins.length; i++) {
            result[i] = CreditCoinParams({
                coin: _coins[i],
                amount: _creditCoins[_coins[i]]
            });
        }

        return result;
    }

    /**
     * @notice Get credit balance by `coin` and `creditor`
     * @param coin Token address
     * @param creditor Wallet address
     * @return amount Token credit balance by wallet
     */
    function getCreditBalanceByCreditor(
        address coin,
        address creditor
    ) external view returns (uint256) {
        return _credits[creditor][coin];
    }

    /**
     * @notice Get total sum of all credits in one certain `coin` equivalent
     * @param coin Equivalent token address
     * @param dexType Exchange type
     * @return totalSum Total sum of all credits
     */
    function getCreditsTotalEquivalent(
        address coin,
        IExchange.DEX dexType
    ) external view returns (uint256 totalSum) {
        IExchange exchange = _factory.getExchange(dexType);

        require(address(exchange) != address(0), "ZERO_ADDRESS");

        for (uint256 i = 0; i < _coins.length; i++) {
            uint256 balance = _creditCoins[_coins[i]];

            uint256 amountOut = _coins[i] == coin
                ? balance
                : exchange.getAmountOut(coin, _coins[i], balance);

            totalSum += amountOut;
        }
    }

    // LINE_SALT_PROD_{1687522099}

    // LINE_SALT_PROD_{1687522208}
}

