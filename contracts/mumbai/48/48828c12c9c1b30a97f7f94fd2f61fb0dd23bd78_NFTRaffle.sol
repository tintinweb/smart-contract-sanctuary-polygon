contract NFTRaffle is Ownable, ReentrancyGuard, Pausable, IERC721Receiver, VRFConsumerBaseV2 {
    // Testnet REKT dogs contract address
    address constant REKT_DOGS_CONTRACT_ADDRESS = address(0xAce8187B113a38F83Bd9C896C6878B175c234dCc);
    address constant REKT_STAKING_CONTRACT_ADDRESS = address(0xEED7870BBbb6aCE5C38b3CC8b23Eee2a6aCBC7aF);
    address constant REKT_CONTRACT_ADDRESS = address(0x5336359d618dd0Fe5f154EFe63F0df2e3751E618);
    address constant ETH_CONTRACT_ADDRESS = address(0x7ceB23fD6bC0adD59E62ac25578270cFf1b9f619);

    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    struct RaffleInfo {
        /// IERC-20 interface for this raffle, only this token can be used to buy tickets, pay for base fee & sales fee.
        IERC20 token;
        /// ERC-721 contract for NFTs.
        address nftContract;
        /// The token Id for the NFT which is the reward of the raffle.
        uint256 nftTokenId;
        /// The total number of tickets that the creator want to sell in the raffle.
        uint256 totalTickets;
        /// The number of tokens that is required for each ticket.
        uint256 tokensPerTicket;
        /// The max percentage of total tickets that one address can buy in the raffle.
        uint256 maxPercentPerAddrCanBuy;
        /// The raffle start time in timestamp
        uint256 startTime;
        /// The raffle end time in timestamp.
        uint256 endTime;
        /// The admin of the raffle.
        address raffleAdmin;
        /// The winner of the raffle.
        address winner;
        /// Has the raffle NFT been withdraw yet?
        bool isNFTWithdrew;
        /// Has the revenue been claimed yet?
        bool isRevenueClaimed;
        /// The participants addresses. We record each ticket as one element,
        /// so if the same address bought multiple tickets, it will be recorded
        /// multiple times. This is not the most efficient, we could have use a
        /// mapping to store it but it help when choosing the winner at the end
        /// since we don't need to loop through the entire array.
        address[] participants;
        /// In 'holder only mode', the user which has the NFT in this list can
        /// buy tickets.
        address[] holderOnlyContractList;
    }

    /// Information related to Chainlink Oracle.

    /// The keyhash, which is network dependent.
    bytes32 internal immutable keyHash;

    /// Subscription Id, need to get from the Chainlink UI.
    uint64 internal immutable subId;

    /// Chainlink VRF Coordinator.
    VRFCoordinatorV2Interface COORDINATOR;

    /// A mapping from VRF request Id to raffle.
    mapping(uint256 => uint256) internal randomnessRequestToRaffle;

    /// Base fee, unit is wei.
    uint256 public BASE_FEE_AMOUNT;

    /// Sales fee percentage, charge for the raffle admin after the end of raffle.
    uint256 public SALES_FEE_PCT;

    /// Percentage denominator.
    uint256 public immutable PERCENTAGE_DENOMINATOR = 100;

    /// RaffleInfo.
    RaffleInfo[] public raffles;

    /// This is to avoid RaffleInfo to contain nested mapping that will prevent it to be returned in public function.
    mapping(address => uint256)[] public participantsToTicketsCounts;

    /// Only NFTs in the whitelist are allowed to start a raffle.
    mapping(address => bool) public NFTWhiteList;

    event RaffleStarted(uint256 indexed raffleId, uint256 tokenId, uint256 ticketPrice, uint256 endTime);
    event TicketsPurchased(uint256 indexed raffleId, address indexed participant, uint256 numTickets);
    event RaffleEnded(uint256 indexed raffleId, address indexed winner);
    event NFTWithrew(uint256 indexed raffleId, address indexed admin);
    event NFTClaimed(uint256 indexed raffleId, address indexed winner);
    event RevenueClaimed(uint256 indexed raffleId, address indexed admin, uint256 amount);

    modifier hasRaffle(uint256 raffleId) {
        require(raffleId < raffles.length, "Raffle not exist");
        _;
    }

    modifier raffleHasEnded(uint256 raffleId) {
        RaffleInfo storage raffle = raffles[raffleId];
        require(
            block.timestamp >= raffle.endTime || raffle.participants.length == raffle.totalTickets,
            "Raffle has not ended yet"
        );
        _;
    }

    constructor(
        bytes32 _keyHash,
        uint64 _subId,
        address _vrfCoordinator,
        uint256 _baseFeeAmount,
        uint256 _salesFeePercentage
    ) VRFConsumerBaseV2(_vrfCoordinator) {
        require(_baseFeeAmount > 0, "Base fee cannot be zero.");
        require(0 < _salesFeePercentage && _salesFeePercentage <= 100, "Sales fee percentage out of bound.");

        BASE_FEE_AMOUNT = _baseFeeAmount;
        SALES_FEE_PCT = _salesFeePercentage;
        keyHash = _keyHash;
        subId = _subId;
        COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
    }

    function getRafflesCount() public view returns (uint256) {
        return raffles.length;
    }

    function getRaffle(uint256 raffleId) public view returns (RaffleInfo memory) {
        return raffles[raffleId];
    }

    function getRaffles(uint256[] calldata raffleIds) external view returns (RaffleInfo[] memory) {
        RaffleInfo[] memory _raffles = new RaffleInfo[](raffleIds.length);
        for (uint256 i = 0; i < raffleIds.length; i++) {
            _raffles[i] = raffles[raffleIds[i]];
        }
        return _raffles;
    }

    function ticketPrice(uint256 raffleId) external view hasRaffle(raffleId) returns (uint256) {
        return raffles[raffleId].tokensPerTicket;
    }

    function getTotalTokenCollected(uint256 raffleId) external view hasRaffle(raffleId) returns (uint256) {
        return raffles[raffleId].participants.length.mul(raffles[raffleId].tokensPerTicket);
    }

    function getTotalTicketSold(uint256 raffleId) external view hasRaffle(raffleId) returns (uint256) {
        return raffles[raffleId].participants.length;
    }

    function getWinner(uint256 raffleId) external view hasRaffle(raffleId) raffleHasEnded(raffleId) returns (address) {
        RaffleInfo storage raffle = raffles[raffleId];
        require(raffle.winner != address(0), "No winner yet");

        return raffle.winner;
    }

    function modifyNFTWhitelist(address[] calldata addrs, bool isAdd) external onlyOwner {
        uint256 len = addrs.length;
        if (isAdd) {
            for (uint256 i = 0; i < len; i++) {
                NFTWhiteList[addrs[i]] = true;
            }
        } else {
            for (uint256 i = 0; i < len; i++) {
                NFTWhiteList[addrs[i]] = false;
            }
        }
    }

    function modifyBaseFeeAmount(uint256 _baseFeeAmount) external onlyOwner {
        require(_baseFeeAmount > 0, "Base fee cannot be zero.");
        BASE_FEE_AMOUNT = _baseFeeAmount;
    }

    function modifySellFeePercentage(uint256 _sellFeePercentage) external onlyOwner {
        require(0 < _sellFeePercentage && _sellFeePercentage <= 100, "Sell fee percentage out of bound");
        SALES_FEE_PCT = _sellFeePercentage;
    }

    /// Creates a new raffle, `_nftContract` approve should be called before calling this.
    function startRaffle(
        address _token,
        address _nftContract,
        uint256 _nftId,
        uint256 _tokensPerTicket,
        uint256 _startTime,
        uint256 _duration,
        uint256 _totalTickets,
        uint256 _maxPercentPerAddrCanBuy,
        address[] calldata _holderOnlyContractList
    ) external payable {
        // Zero address for MATIC
        require(
            _token == REKT_CONTRACT_ADDRESS || _token == ETH_CONTRACT_ADDRESS || _token == address(0),
            "Only support REKT, ETH or MATIC."
        );
        require(NFTWhiteList[_nftContract], "Only NFTs in the whitelist are allowed to start a raffle");
        require(_tokensPerTicket > 0 && _duration > 0 && _totalTickets > 0, "Invalid input parameters");
        require(
            _maxPercentPerAddrCanBuy == 5 || _maxPercentPerAddrCanBuy == 10 || _maxPercentPerAddrCanBuy == 20,
            "Max percentage per address can buy only can be 5, 10 or 20"
        );
        require(
            _totalTickets * _maxPercentPerAddrCanBuy / PERCENTAGE_DENOMINATOR >= 1,
            "Need to make sure people can buy at least 1 ticket."
        );
        require(raffles.length == participantsToTicketsCounts.length, "Raffle Length Mismatch");
        require(msg.value == BASE_FEE_AMOUNT, "Send wrong amount of base fee.");

        for (uint256 i = 0; i < _holderOnlyContractList.length; i++) {
            require(NFTWhiteList[_holderOnlyContractList[i]], "holder only contract is not in NFT whitelist.");
        }

        uint256 endTime;
        if (_startTime <= block.timestamp) {
            _startTime = block.timestamp;
            endTime = block.timestamp.add(_duration);
        } else {
            endTime = _startTime.add(_duration);
        }

        address[] memory tickets;
        IERC20 token = IERC20(_token);
        RaffleInfo memory _raffle = RaffleInfo(
            token,
            _nftContract,
            _nftId,
            _totalTickets,
            _tokensPerTicket,
            _maxPercentPerAddrCanBuy,
            _startTime,
            endTime,
            msg.sender,
            address(0),
            false,
            false,
            tickets,
            _holderOnlyContractList
        );
        raffles.push(_raffle);
        participantsToTicketsCounts.push();

        IERC721(_nftContract).safeTransferFrom(msg.sender, address(this), _nftId);

        emit RaffleStarted(raffles.length, _nftId, _tokensPerTicket, endTime);
    }

    /// Create Raffle only when ERC721 token is received.
    function onERC721Received(address, address, uint256, bytes calldata) public pure returns (bytes4) {
        return bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"));
    }

    function buyTickets(uint256 _raffleId, uint256 _numTickets) external hasRaffle(_raffleId) payable {
        require(_numTickets > 0, "Number of tickets must be greater than zero.");

        RaffleInfo storage raffle = raffles[_raffleId];
        require(block.timestamp >= raffle.startTime, "Raffle has not started.");
        require(block.timestamp <= raffle.endTime, "Raffle has ended.");
        require(
            raffle.participants.length + _numTickets <= raffle.totalTickets,
            "Cannot buy more than allowed ticket counts."
        );
        require(
            participantsToTicketsCounts[_raffleId][msg.sender] + _numTickets
                <= raffle.totalTickets * raffle.maxPercentPerAddrCanBuy / PERCENTAGE_DENOMINATOR,
            "Cannot buy more than allowed percentage."
        );

        if (raffle.holderOnlyContractList.length > 0) {
            bool isHolder;
            for (uint256 i = 0; i < raffle.holderOnlyContractList.length; i++) {
                if (IERC721(raffle.holderOnlyContractList[i]).balanceOf(msg.sender) > 0) {
                    isHolder = true;
                    break;
                }
            }
            require(isHolder, "This raffle is in `Holder Only Mode`, only holders can buy tickets.");
        }

        uint256 totalPrice = raffle.tokensPerTicket.mul(_numTickets);

        if (raffle.token == IERC20(address(0))) {
            require(msg.value == totalPrice, "Send wrong amount of total price.");
        } else {
            raffle.token.transferFrom(msg.sender, address(this), totalPrice);
        }

        // Add tickets, this is not space efficient. But it helps us in choosing winner quickly.
        for (uint256 i = 0; i < _numTickets; i++) {
            raffle.participants.push(msg.sender);
        }
        participantsToTicketsCounts[_raffleId][msg.sender] =
            participantsToTicketsCounts[_raffleId][msg.sender].add(_numTickets);

        // Choose the winner after all the tickets have been sold.
        if (raffle.participants.length + _numTickets == raffle.totalTickets) {
            requestRandomWords(_raffleId);
        }

        emit TicketsPurchased(_raffleId, msg.sender, _numTickets);
    }

    /// End Raffle either when the time has passed.
    function endRaffle(uint256 _raffleId) external hasRaffle(_raffleId) returns (uint256) {
        RaffleInfo storage raffle = raffles[_raffleId];
        require(raffle.participants.length > 0, "No tickets were sold");
        require(raffle.winner == address(0), "Winner already selected");
        require(block.timestamp >= raffle.endTime, "It's not the end time of raffle yet");

        return requestRandomWords(_raffleId);
    }

    function requestRandomWords(uint256 _raffleId) internal returns (uint256) {
        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            subId,
            3, // Default confirmation of 3 blocks.
            200000, // Remaining gas for the `fulfillRandomWords`.
            1 // Number of random words that we request.
        );
        randomnessRequestToRaffle[requestId] = _raffleId;
        return requestId;
    }

    /// After receiving the random result, we transfer the NFT to winner and transfer the tokens to admin.
    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        uint256 raffleId = randomnessRequestToRaffle[requestId];

        RaffleInfo storage raffle = raffles[raffleId];
        require(raffle.winner == address(0), "Winner already selected");
        require(randomWords.length == 1, "Random word length incorrect");

        uint256 winnerIndex = randomWords[0] % raffle.participants.length;

        address winner = raffle.participants[winnerIndex];
        raffle.winner = winner;

        emit RaffleEnded(raffleId, winner);

        return;
    }

    /// Admin of the Raffle can withdraw the NFT if not a single ticket was sold after the end of raffle.
    function adminWithdrawNFT(uint256 _raffleId) external hasRaffle(_raffleId) nonReentrant {
        RaffleInfo storage raffle = raffles[_raffleId];

        require(block.timestamp >= raffle.endTime, "Raffle has not ended yet.");
        require(raffle.participants.length == 0, "Some tickets were sold, you should call `endRaffle`.");
        require(!raffle.isNFTWithdrew, "NFT has been withdrawn.");
        require(msg.sender == raffle.raffleAdmin, "Not raffle admin.");

        raffle.isNFTWithdrew = true;
        IERC721(raffle.nftContract).transferFrom(address(this), msg.sender, raffle.nftTokenId);

        emit NFTWithrew(_raffleId, raffle.raffleAdmin);
    }

    /// Winner can claim his reward NFT.
    function winnerClaimNFT(uint256 _raffleId) external hasRaffle(_raffleId) nonReentrant {
        RaffleInfo storage raffle = raffles[_raffleId];

        require(!raffle.isNFTWithdrew, "NFT has been withdrawn.");
        require(raffle.winner != address(0), "Raffle has not ended yet.");
        require(msg.sender == raffle.winner, "Not raffle winner.");

        raffle.isNFTWithdrew = true;
        IERC721(raffle.nftContract).transferFrom(address(this), msg.sender, raffle.nftTokenId);

        emit NFTClaimed(_raffleId, raffle.winner);
    }

    /// Raffle admin claim his tickets salses revenue
    /// And charged for a sales fee of SALES_FEE_PCT
    /// Rekt Dogs NFT holder can save half of sales fee
    function adminClaimTicketSalesRevenue(uint256 _raffleId) external hasRaffle(_raffleId) payable nonReentrant {
        RaffleInfo storage raffle = raffles[_raffleId];

        require(!raffle.isRevenueClaimed, "Revenue has been claimed.");
        require(raffle.winner != address(0), "Raffle has not ended yet.");
        require(msg.sender == raffle.raffleAdmin, "Not raffle admin.");

        uint256 totalTicketSalesRevenue = raffle.participants.length.mul(raffle.tokensPerTicket);
        uint256 salesFeeAmount = totalTicketSalesRevenue.mul(SALES_FEE_PCT).div(PERCENTAGE_DENOMINATOR);
        // If admin has REKT dogs in their wallet or use it for raffle, or staking them in the staking contract, the sales fee will have a 50% discount.
        bool hasStaking;
        (bool success, bytes memory result) = REKT_STAKING_CONTRACT_ADDRESS.call(abi.encodeWithSignature("balanceOf(address)", msg.sender));
        if (success && uint256(bytes32(result)) > 0) {
            hasStaking = true;
        }
        if (
            IERC721(REKT_DOGS_CONTRACT_ADDRESS).balanceOf(raffle.raffleAdmin) > 0
                || raffle.nftContract == REKT_DOGS_CONTRACT_ADDRESS || hasStaking
        ) {
            salesFeeAmount = salesFeeAmount.div(2);
        }
        uint256 adminRevenueAfterFee = totalTicketSalesRevenue.sub(salesFeeAmount);

        raffle.isRevenueClaimed = true;

        if (raffle.token == IERC20(address(0))) {
            _transferMatic(payable(owner()), salesFeeAmount);
            _transferMatic(payable(raffle.raffleAdmin), adminRevenueAfterFee);
        } else {
            raffle.token.transfer(owner(), salesFeeAmount);
            raffle.token.transfer(raffle.raffleAdmin, adminRevenueAfterFee);
        }

        emit RevenueClaimed(_raffleId, raffle.raffleAdmin, adminRevenueAfterFee);
    }

    function _transferMatic(address payable _to, uint256 amount) internal {
        if (amount == 0) {
            return;
        }
        require(_to != address(0), "Cannot transfer to zero address.");

        (bool sent, ) = _to.call{value: amount}("");
        require(sent, "Error, failed to send MATIC");
    }
}