contract GreetUser is BaseRelayRecipient {
	//Contract state
	address public admin;
	//uint256 MAX_INT = 100000000000000;

	//User ID counter
	using Counters for Counters.Counter;
	Counters.Counter private _userIdCounter;
	Counters.Counter private _userCounter;

	//Modifier
	modifier onlyAdmin() {
		require(admin == msg.sender, 'Only admin can call this function');
		_;
	}
	modifier onlyRegisteredUser(address _userAddress) {
		require(
			isRegistered(_userAddress),
			'Only registered user can call this function'
		);
		_;
	}

	//Mapping
	mapping(address => userDetail) public users;
	mapping(string => address) public uniqueUserName;
	mapping(address => uint256) public userIndex;

	userDetail[] public usersList;

	//Event
	event userRegistered(
		uint256 indexed id,
		address indexed userAddress,
		string indexed userName
	);

	//Struct
	struct userDetail {
		address userAddress;
		uint256 id;
		string userName;
	}

	// struct following {
	// 	address userAddress;
	// 	string userName;
	// 	uint256 timeStamp;
	// }

	// struct follower {
	// 	address userAddress;
	// 	string userName;
	// 	uint256 timestamp;
	// }

	// constructor(address _trustedForwarder) {
	// 	_setTrustedForwarder(_trustedForwarder);
	// 	admin = msg.sender;
	// }

	constructor() {
		admin = msg.sender;
		//admin = _address;
		registerNewUser("Greet");
	}

	//Function

	function setTrustForwarder(address _trustedForwarder) public onlyAdmin {
		_setTrustedForwarder(_trustedForwarder);
	}

	function versionRecipient() external pure override returns (string memory) {
		return '1';
	}

	function registerNewUser(string memory _userName) public {
		require(bytes(_userName).length > 0, 'User name cannot be empty');
		require(
			!isRegistered(msg.sender),
			'This address already registered as a user'
		);
		require(!isUserNameTaken(_userName), 'This user name is taken');
		

		//Increment
		_userIdCounter.increment();
		_userCounter.increment();
		uint256 currentId = _userIdCounter.current();

		userDetail memory newUser = userDetail(
			msg.sender,
			currentId,
			_userName
		);
		users[msg.sender] = newUser;

		//Add the genesis user
		if (currentId == 1) {
			usersList.push(userDetail(address(0), 0, 'Greet_genesis'));
		}
		usersList.push(newUser);

		userIndex[msg.sender] = currentId;
		uniqueUserName[_userName] = msg.sender;
		emit userRegistered(currentId, msg.sender, _userName);
	}

	// function registerNewUser(string memory _userName) public {
	// 	// require(
	// 	// 	isRegistered(msg.sender),
	// 	// 	'This address already registered as a user'
	// 	// );
	// 	require(!isUserNameTaken(_userName), 'This user name is taken');
	// 	require(bytes(_userName).length > 0, 'User name cannot be empty');

	// 	_userIdCounter.increment();
	// 	uint256 currentId = _userIdCounter.current();
	// 	console.log("%s",currentId);
	// 	//usersList.length++;

	// 	usersList[currentId] = userDetail(msg.sender,currentId, _userName,0,0);
	// 	userIndex[msg.sender] = currentId;
	// 	users[msg.sender] = userDetail(msg.sender,currentId,_userName,0,0);

	// 	uniqueUserName[_userName] = msg.sender;
	// 	emit userRegistered(currentId, msg.sender, _userName);
	// }

	function isRegistered(address _userAddress) public view returns (bool) {
		return bytes(users[_userAddress].userName).length != 0;
	}

	// function isRegistered(address _userAddress) public view returns (bool) {
	// 	uint256 _userIndex = userIndex[_userAddress];
	// 	return true;
	// }

	function isUserNameTaken(string memory _userName)
		internal
		view
		returns (bool)
	{
		//console.log("%s",_userName);
		return uniqueUserName[_userName] != address(0);
	}

	function getUserPublicDetailsByAddress(address _userAddress)
		public
		view
		returns (uint256 _id, string memory _userName)
	{
		require(users[_userAddress].id != 0, 'This address is not registered');
		_id = users[_userAddress].id;
		_userName = users[_userAddress].userName;
	}

	// function getUserPublicDetailsByAddress(address _userAddress)
	// 	public
	// 	view
	// 	returns (uint256 _id, string memory _userName)
	// {
	// 	require(isRegistered(_userAddress), 'This address is not registered');
	// 	uint256 _userIndex = userIndex[_userAddress];
	// 	_id = usersList[_userIndex].id;
	// 	_userName = usersList[_userIndex].userName;
	//}

	// function getUserPublicDetailsById(uint256 _id)
	// 	public
	// 	view
	// 	returns (uint256 id, string memory _userName)
	// {
	// 	require(_id != 0 || _id < MAX_INT-1,"Invalid ID");
	// 	uint256 userId = _id;
	// 	id = userDetail[userId].id;
	// 	_userName = userDetail[_id].userName;
	// }

	function getUserCount() public view returns (uint256) {
		return _userCounter.current();
	}
}

