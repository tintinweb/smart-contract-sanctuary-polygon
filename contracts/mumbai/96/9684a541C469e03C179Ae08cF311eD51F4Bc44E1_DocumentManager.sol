// SPDX-License-Identifier: MIT
pragma solidity =0.8.17;

import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";

import "./interfaces/IDocumentManager.sol";

import "./interfaces/IDocument.sol";

contract Document is IDocument, AccessControlEnumerable {
    using EnumerableSet for EnumerableSet.AddressSet;

    bytes32 public constant OWNER_ROLE =
        bytes32(keccak256(bytes("OWNER_ROLE")));
    bytes32 public constant WRITER_ROLE =
        bytes32(keccak256(bytes("WRITER_ROLE")));
    bytes32 public constant READER_ROLE =
        bytes32(keccak256(bytes("READER_ROLE")));

    string public fileId;
    string public fileHash;
    uint256 public timestamp;

    address public immutable manager;

    constructor(
        string memory fileId_,
        string memory fileHash_,
        address manager_,
        uint256 timestamp_,
        address[] memory owners,
        address[] memory writers,
        address[] memory readers
    ) {
        require(manager_ == msg.sender, "Caller is not manager");
        require(
            IDocumentManager(manager_).supportsInterface(type(IDocumentManager).interfaceId),
            "Manager is not DocumentManager"
        );
        // require(!compare(fileId, ""), "Invalid id");
        // require(!compare(fileHash, ""), "Invalid hash");
        require(owners.length != 0, "No owners");
        require(owners.length < 1000, "Too many owners");
        require(writers.length < 1000, "Too many writers");
        require(readers.length < 1000,  "Too many readers");

        fileId = fileId_;
        fileHash = fileHash_;
        manager = manager_;
        timestamp = timestamp_;

        for (uint256 i; i < owners.length; ++i) {
            require(owners[i] != address(0), "Zero owner address");
            _grantRole(OWNER_ROLE, owners[i]);
        }

        for (uint256 i; i < writers.length; ++i) {
            require(writers[i] != address(0), "Zero writer address");
            _grantRole(WRITER_ROLE, writers[i]);
        }

        for (uint256 i; i < readers.length; ++i) {
            require(readers[i] != address(0), "Zero reader address");
            _grantRole(READER_ROLE, readers[i]);
        }
    }

    function addRoleForUsers(bytes32 role, address[] calldata users) external {
        require(msg.sender == manager, "Caler is not manager");
        //require(users.length != 0, "No users");
        require(users.length < 1000, "Too many users");
        require(role == OWNER_ROLE || role == WRITER_ROLE || role == READER_ROLE, "Unknown role");

        for (uint256 i; i < users.length; ++i) {
            require(users[i] != address(0), "Zero user address");
            _grantRole(role, users[i]);
        }
    }

    function deleteRoleForUsers(bytes32 role, address[] calldata users) external {
        require(msg.sender == manager, "Caler is not manager");
        //require(users.length != 0, "No users");
        require(users.length < 1000, "Too many users");
        require(role == OWNER_ROLE || role == WRITER_ROLE || role == READER_ROLE, "Unknown role");

        for (uint256 i; i < users.length; ++i) {
            require(users[i] != address(0), "Zero user address");
            _revokeRole(role, users[i]);
        }
    }

    function setFileHash(string memory fileHash_) external {
        require(msg.sender == manager, "Caler is not manager");
        require(!compare(fileHash, fileHash_), "Hash duplicate");
        fileHash = fileHash_;
    }

    function compare(string memory str1, string memory str2) private pure returns (bool) {
        return keccak256(abi.encodePacked(str1)) == keccak256(abi.encodePacked(str2));
    }
}

// SPDX-License-Identifier: MIT
pragma solidity =0.8.17;

import '@openzeppelin/contracts/access/AccessControlEnumerable.sol';
import '@openzeppelin/contracts/utils/structs/EnumerableSet.sol';

import './Whitelist.sol';
import './Document.sol';

import './interfaces/IDocumentManager.sol';
import './interfaces/IDocument.sol';

contract DocumentManager is IDocumentManager, AccessControlEnumerable, Whitelist {
    using EnumerableSet for EnumerableSet.UintSet;
    bytes32 public constant ADMIN_ROLE = bytes32(keccak256(bytes('ADMIN_ROLE')));

    mapping(string => Document) public fileById;
    mapping(string => string) public idByHash;

    mapping(uint256 => string) public fileIdMap;
    mapping(address => EnumerableSet.UintSet) private userFileIds;

    uint256 public globalId;

    constructor(address admin) {
        require(admin != address(0), 'Not admin');
        _grantRole(DEFAULT_ADMIN_ROLE, admin);
        _grantRole(ADMIN_ROLE, admin);
    }

    function addDocument(
        string memory fileId,
        string memory fileContentHash,
        address[] calldata owners,
        address[] calldata writers,
        address[] calldata readers
    ) external {
        Document newDoc = new Document(
            fileId,
            fileContentHash,
            address(this),
            block.timestamp,
            owners,
            writers,
            readers
        );

        fileById[fileId] = newDoc;
        idByHash[fileContentHash] = fileId;

        bytes32 OWNER_ROLE = newDoc.OWNER_ROLE();
        newDoc.addRoleForUsers(OWNER_ROLE, owners);

        bytes32 WRITER_ROLE = newDoc.OWNER_ROLE();
        newDoc.addRoleForUsers(WRITER_ROLE, writers);

        bytes32 READER_ROLE = newDoc.OWNER_ROLE();
        newDoc.addRoleForUsers(READER_ROLE, readers);

        fileIdMap[globalId] = fileId;

        for (uint256 i; i < owners.length; ++i) {
            (userFileIds[owners[i]]).add(globalId);
        }

        ++globalId;
    }

    function getUserFiles(address user) external view returns(string[] memory, string[] memory, uint256[] memory) {
        uint256[] memory ids = userFileIds[user].values();
        string[] memory fileIds = new string[](ids.length);
        string[] memory hashes = new string[](ids.length);
        uint256[] memory timestamps = new uint256[](ids.length);
        for (uint256 i; i < ids.length; ++i) {
            fileIds[i] = fileIdMap[ids[i]];
            hashes[i] = fileById[fileIds[i]].fileHash();
            timestamps[i] = fileById[fileIds[i]].timestamp();
        }
        
        return (fileIds, hashes, timestamps);
    }

    function deleteDocument(string memory fileId) external {
        delete idByHash[fileById[fileId].fileHash()];
        delete fileById[fileId];
    }

    function addOwnersForDocument(
        string memory fileId,
        address[] calldata owners
    ) external {
        Document doc = fileById[fileId];
        bytes32 OWNER_ROLE = doc.OWNER_ROLE();
        doc.addRoleForUsers(OWNER_ROLE, owners);
    }

    function deleteOwnersForDocument(
        string memory fileId,
        address[] calldata owners
    ) external {
        Document doc = fileById[fileId];
        bytes32 OWNER_ROLE = doc.OWNER_ROLE();
        doc.deleteRoleForUsers(OWNER_ROLE, owners);
    }

    function proveAccessToFile(
        string memory fileId,
        address user,
        bytes32 role
    ) external view returns (bool) {}

    function addWritersForDocument(
        string memory fileId,
        address[] calldata writers
    ) external {
        Document doc = fileById[fileId];
        bytes32 WRITER_ROLE = doc.WRITER_ROLE();
        doc.addRoleForUsers(WRITER_ROLE, writers);
    }

    function deleteWritersForDocument(
        string memory fileId,
        address[] calldata writers
    ) external {
        Document doc = fileById[fileId];
        bytes32 WRITER_ROLE = doc.WRITER_ROLE();
        doc.deleteRoleForUsers(WRITER_ROLE, writers);
    }

    function addReadersForDocument(
        string memory fileId,
        address[] calldata readers
    ) external {
        Document doc = fileById[fileId];
        bytes32 READER_ROLE = doc.READER_ROLE();
        doc.addRoleForUsers(READER_ROLE, readers);
    }

    function deleteReadersForDocument(
        string memory fileId,
        address[] calldata readers
    ) external {
        Document doc = fileById[fileId];
        bytes32 READER_ROLE = doc.READER_ROLE();
        doc.deleteRoleForUsers(READER_ROLE, readers);
    }

    function addWhitelistedUsers(
        address[] memory users,
        string[] memory names
    ) external onlyRole(ADMIN_ROLE) {
        uint256 len = users.length;
        require(len != 0, 'Invalid length');
        require(names.length == len, 'Invalid names length');
        for (uint256 i; i < len; ++i) {
            require(_addWhitelistedUser(users[i], names[i]), 'Can not add a user');
        }
    }

    function removeWhitelistedUsers(
        address[] memory users
    ) external onlyRole(ADMIN_ROLE) {
        uint256 len = users.length;
        require(len != 0, 'Invalid length');
        for (uint256 i; i < len; ++i) {
            require(_removeWhitelistedUser(users[i]), 'Can not remove a user');
        }
    }

    function addAdmins(address[] memory admins) external onlyRole(ADMIN_ROLE) {
        uint256 len = admins.length;
        require(len != 0, 'Invalid length');
        for (uint256 i; i < len; ++i) {
            require(admins[i] != address(0), 'Invalid admin');
            _grantRole(ADMIN_ROLE, admins[i]);
        }
    }

    function removeAdmins(address[] memory admins) external onlyRole(ADMIN_ROLE) {
        uint256 len = admins.length;
        require(len != 0, 'Invalid length');
        for (uint256 i; i < len; ++i) {
            require(admins[i] != address(0) && admins[i] != msg.sender, 'Invalid admin');
            _grantRole(ADMIN_ROLE, admins[i]);
        }
    }

    function supportsInterface(
        bytes4 interfaceId
    ) public view override(AccessControlEnumerable, IDocumentManager) returns (bool) {
        return (interfaceId == type(IDocumentManager).interfaceId) || super.supportsInterface(interfaceId);
    }
}

// SPDX-License-Identifier: MIT
pragma solidity =0.8.17;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

abstract contract Whitelist {
    using EnumerableSet for EnumerableSet.AddressSet;

    EnumerableSet.AddressSet private whitelistedUsers;
    mapping(address => string) public userNames;

    function _addWhitelistedUser(address user, string memory name) internal returns(bool) {
        require(user != address(0), 'Invalid user');
        if (whitelistedUsers.add(user)) {
            userNames[user] = name;
            return true;
        }
        return false;
    }

    function _removeWhitelistedUser(address user) internal returns(bool) {
        require(user != address(0), 'Invalid user');
        if (whitelistedUsers.remove(user)) {
            delete userNames[user];
            return true;
        }
        return false;
    }

    function isWhitelisted(address user) public view returns(bool) {
        return whitelistedUsers.contains(user);
    } 

    function getUsers() public view returns(address[] memory) {
        return whitelistedUsers.values();
    }
}

