// File: Arbitrum Projects/NFT Lending/Main.sol


pragma solidity ^0.8.0;




contract Main is Pausable{
    using EnumerableSet for EnumerableSet.AddressSet;
    address public admin = msg.sender; // marketplace controller
    
    // An address set of all supported collections
    EnumerableSet.AddressSet private collections;
    
    event CollectionAdded(address indexed _collection);
    event CollectionRemoved(address indexed _collection);
    event ProtocolCreated(address indexed protocol, string name, uint8 protocolFee, uint8 securityFee); 

    function addProtocol(address _protocol, string calldata _name, uint8 _protocolFee, uint8 _securityFee) external isAdmin{
        emit ProtocolCreated(_protocol, _name, _protocolFee, _securityFee);
    }

    /**
     * @notice Get all collections supported by the marketplace
     */
    function getSupportedCollections()external view returns(address[] memory availableCollections){
        uint256 length = collections.length();
        availableCollections = new address[](length);
        for (uint256 i = 0; i < length; i++) {
            availableCollections[i] = collections.at(i);
        }
        return availableCollections;
    }

    
    // modifier to check that only admin can call the function
    modifier isAdmin() {
        require(_msgSender() == admin, "Caller != Admin");
        _;
    }
    // modifier to check if a collection is supported
    modifier isCollection(address _collection) {
        require(
            collections.contains(_collection),
            "Collection not supported"
        );
        _;
    }

    /**
     * @notice a public getter function to get if a collection exist
     * @param _collection address to check offer from
     */
    function getCollection(address _collection) public view returns (bool) {
        return collections.contains(_collection);
    }

    /** 
        @notice Add a collection to the marketplace
        @param _collection address of the collection
    */
    function addCollection(address _collection) external whenNotPaused isAdmin{
        require(!collections.contains(_collection),"Collection exists");
        require(IERC721(_collection).supportsInterface(0x80ac58cd),"not supported");
        collections.add(_collection);
        emit CollectionAdded(_collection);
    }

    /** 
        @notice Remove a collection from the marketplace
        @param _collection address of the collection
    */
    function removeCollection(address _collection)external whenNotPaused isAdmin isCollection(_collection)
    {
        collections.remove(_collection);
        emit CollectionRemoved(_collection);
    }
}