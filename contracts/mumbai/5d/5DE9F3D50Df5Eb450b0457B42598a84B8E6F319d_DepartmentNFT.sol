pragma solidity ^0.8.0;

import { EnumerableSet} from "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import { Strings} from "@openzeppelin/contracts/utils/Strings.sol";
import { Counters} from "@openzeppelin/contracts/utils/Counters.sol";
import { IERC721 } from "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import { ERC721Upgradeable } from "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import { MerkleProofUpgradeable } from "@openzeppelin/contracts-upgradeable/utils/cryptography/MerkleProofUpgradeable.sol";
import { ERC721Holder } from "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import { OwnableUpgradeable } from "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import { ReentrancyGuardUpgradeable } from "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "hardhat/console.sol";

contract LibraryLockDataLayout {
  bool public initializedFlag;
}

contract DepartmentNFTWrapper is LibraryLockDataLayout {
    bool internal revealed;
    uint16 internal totalSupply000;
    uint16 internal totalSupply001;
    uint8 internal currentStep;
    uint256 mintPrice;
    mapping(uint256 => string) internal tokenURIs;

    function initialized(uint16 _totalSupply000, uint16 _totalSupply001, uint8 _currentStep, bool _revealed, uint256 _mintPrice) internal {
        totalSupply000 = _totalSupply000;
        totalSupply001 = _totalSupply001;
        currentStep = _currentStep;
        revealed = _revealed;
        mintPrice = _mintPrice;
    }
}

contract DepartmentNFT is DepartmentNFTWrapper, OwnableUpgradeable, ReentrancyGuardUpgradeable, ERC721Upgradeable {
    using Strings for uint256;
    using Counters for Counters.Counter;
    
    struct RoyaltyInfo {
        address receiver;
        uint96 royaltyFraction;
    }
    
    address private signer;
    Counters.Counter private _registered; 

    uint16 private mintCount;
    uint16 [] private hoodies;
    uint16 [] private freeMints;
    uint16 [] private sunglasses;
    bytes32 public merkleRoot;
    bytes32 [] private merkleProof;
    RoyaltyInfo [] private royalties;

    uint256 revealtime; 

    function initialize(
        string memory _name,
        string memory _symbol,
        uint16 _totalSupply000,
        uint16 _totalSupply001,
        uint16 _hoodieCount,
        uint16 _freeMintCount,
        uint16 _sunglassCount,
        uint16 [] memory _hoodies,
        uint16 [] memory _freeMints,
        uint16 [] memory _sunglasses,
        uint256 _mintPrice,
        uint256 _revealtime
    ) public initializer {
        require(!initializedFlag, "Contract is already initialized");
        ERC721Upgradeable.__ERC721_init(_name, _symbol);
        DepartmentNFTWrapper.initialized(_totalSupply000, _totalSupply001, 0, false, _mintPrice);
        signer = _msgSender();
        initializedFlag = true;
        mintCount = 0;
        
        hoodies = new uint16 [](_hoodieCount);
        freeMints = new uint16 [](_freeMintCount);
        sunglasses = new uint16 [](_sunglassCount);

        uint16 i;
        for (i = 0; i <= _hoodieCount - 1; i++)
            hoodies[i] = _hoodies[i];
        for (i = 0; i <= _freeMintCount - 1; i++)
            freeMints[i] = _freeMints[i];
        for (i = 0; i <= _sunglassCount - 1; i++)
            sunglasses[i] = _sunglasses[i];
        
        revealtime = _revealtime;
    }

    function setRoyalties(
        uint16 _royaltiesCount,
        address [] memory _receivers,
        uint96 [] memory _royaltyFractions) external {
        uint16 i;
        for (i = 0; i <= _royaltiesCount - 1; i++) {
            RoyaltyInfo storage ri = royalties[i];
            ri.receiver = _receivers[i];
            ri.royaltyFraction = _royaltyFractions[i];
        }
    }

    function setTokenUri(uint256 tokenId, string memory _tokenURI) private {
        tokenURIs[tokenId] = _tokenURI;
    }

    /**
     * @dev See {IERC721Metadata-tokenURI}.
     */
    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        require(revealed, "NFTs are not revealed yet");
        
        return string(
            abi.encodePacked(
            "https://ipfs.io/ipfs/",
            tokenURIs[tokenId]
           )
        );
    }

    function isMinted(address sender) private view returns (bool) {
        uint16 i;

        for (i = 0; i <= totalSupply000 - 1; i++) {
            if (ownerOf(i) == sender)
                return true;
        }

        return false;
    }

    function isSunglass(address sender) private view returns (bool) {
        uint16 i;

        for (i = 0; i <= sunglasses.length - 1; i++) {
            if (ownerOf(sunglasses[i]) == sender)
                return true;
        }

        return false;
    }

    function isHoodie(address sender) private view returns (bool) {
        uint16 i;

        for (i = 0; i <= hoodies.length - 1; i++) {
            if (ownerOf(hoodies[i]) == sender)
                return true;
        }

        return false;
    }

    function isFreeMint(address sender) private view returns (bool) {
        uint16 i;

        for (i = 0; i <= freeMints.length - 1; i++) {
            if (ownerOf(freeMints[i]) == sender)
                return true;
        }

        return false;
    }

    function _feeDenominator() internal pure virtual returns (uint96) {
        return 10000;
    }

    function mint(uint256 _tokenId, uint256 _price, string memory _tokenURI) external payable {
        require((currentStep == 0 && mintCount < totalSupply000) || (currentStep == 1 && mintCount < totalSupply001), "No more available to mint");
        require(msg.value == _price);
        if (currentStep == 0) {
            require(isMinted(msg.sender) == false, "Sender has already minted the NFT");
            require(_price == 0, "Current step is free mint");
            bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
            require(MerkleProofUpgradeable.verify(merkleProof, merkleRoot, leaf), "Invalid proof.");
        } else if (isFreeMint(msg.sender)) {
            require(_price == 0, "You can mint as free");
        } else {
            uint16 i;
            uint256 sendAmt;
            for (i = 0; i < royalties.length; i++) {
                sendAmt = (_price * royalties[i].royaltyFraction) / _feeDenominator();
                require(payable(royalties[i].receiver).send(sendAmt), "Failed to send Ether");
            }
        }
        setTokenUri(_tokenId, _tokenURI);
        _mint(msg.sender, _tokenId);
        mintCount ++;
    }

    function reveal(bool _revealed) external isSigner {
        if (_revealed == true) {
            require(revealtime < block.timestamp, "It's not available to reveal yet.");
        }
        revealed = _revealed;
    }

    function setMerkleProof(bytes32 _merkleRoot, bytes32 [] memory _merkleProof) external isSigner {
        merkleRoot = _merkleRoot;
        merkleProof = _merkleProof;
    }

    function verify(
        bytes memory _input,
        bytes memory _signature
    ) private view returns (bool) {
        bytes32 messageHash = getMessageHash(_input);
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);

        return recoverSigner(ethSignedMessageHash, _signature) == signer;
    }

    function getMessageHash(bytes memory _input) private pure returns (bytes32) {
        return keccak256(abi.encodePacked(_input));
    }

    function getEthSignedMessageHash(bytes32 _messageHash)
        private
        pure
        returns (bytes32)
    {
        /*
        Signature is produced by signing a keccak256 hash with the following format:
        "\x19Ethereum Signed Message\n" + len(msg) + msg
        */
        return
            keccak256(
                abi.encodePacked("\x19Ethereum Signed Message:\n32", _messageHash)
            );
    }

    function recoverSigner(bytes32 _ethSignedMessageHash, bytes memory _signature)
        private
        pure
        returns (address)
    {
        (bytes32 r, bytes32 s, uint8 v) = splitSignature(_signature);

        return ecrecover(_ethSignedMessageHash, v, r, s);
    }

    function splitSignature(bytes memory sig)
        private
        pure
        returns (
            bytes32 r,
            bytes32 s,
            uint8 v
        )
    {
        require(sig.length == 65, "invalid signature length");

        assembly {
            /*
            First 32 bytes stores the length of the signature

            add(sig, 32) = pointer of sig + 32
            effectively, skips first 32 bytes of signature

            mload(p) loads next 32 bytes starting at the memory address p into memory
            */

            // first 32 bytes, after the length prefix
            r := mload(add(sig, 32))
            // second 32 bytes
            s := mload(add(sig, 64))
            // final byte (first byte of the next 32 bytes)
            v := byte(0, mload(add(sig, 96)))
        }

        // implicitly return (r, s, v)
    }

    function getTotalSupply000() public view returns (uint16) {
        return totalSupply000;
    }

    function updateTotalSupply000(uint16 _totalSupply000) external isSigner {
        totalSupply000 = _totalSupply000;
    }

    function getTotalSupply001() public view returns (uint16) {
        return totalSupply001;
    }

    function updateTotalSupply001(uint16 _totalSupply001) external isSigner {
        totalSupply001 = _totalSupply001;
    }

    function setCurrentStep(uint8 _currentStep) external isSigner {
        currentStep = _currentStep;
    }

    function getSigner() public view returns (address) {
        return signer;
    }

    function updateSigner(address _signer) external isSigner {
        signer = _signer;
    }

    function getTimestamp() public view virtual returns (uint256) {
        return block.timestamp;
    }

    modifier isSigner {
        require(_msgSender() == signer, "This function can only be called by an signer");
        _;
    }

    modifier delegatedOnly() {
        require(initializedFlag, "The library is locked. No direct 'call' is allowed");
        _;
    }
}

