contract NFTRedemption is Ownable, Pausable{

    using ECDSA for bytes32;


    ICDHNFTInventory public cdhNFT;
    ITowerInventory public towerInventory;
    ICardPool public equipmentPool;
    ICardPool public heroPool;
    ICardPool public spellPool;
    ICardPool public towerPool;
    address public blackHoleContract;
    address public randomNumberSender;

    bytes1 rarity;


    event SetRandomNumberSenderAddress(address oldSender, address newSender);
    event SetBlackHoleContract(address blackHoleContract);
    event TicketRedemption(address indexed sender, uint256 quantity, uint256[] cards, uint NUMBER_OF_OPENER_CARDS, uint256 ticket_type);




    /// @notice NFTRedemption has multiple parameters while deployment
    /// @param _randomNumberSender Wallet address used for generating random number and signature from backend
    /// @param _cdhNft Contract Address for inventory contract
    /// @param _towerInventory Contract Address for tower inventory  
    /// @param _equipmentPool Contract Address for Equipment Pool
    /// @param _heroPool Contract Address for Hero Pool
    /// @param _spellPool Contract Address for Spell Pool
    /// @param _towerPool Contract Address for Tower Pool
    /// @param _blackHoleContract Contract Address for Blackhole contract
    constructor(
        address _cdhNft,
        address _towerInventory,
        address _equipmentPool,
        address _heroPool,
        address _spellPool,
        address _towerPool,
        address _blackHoleContract,
        address _randomNumberSender) 
        {
        equipmentPool = ICardPool(_equipmentPool);
        heroPool = ICardPool(_heroPool);
        spellPool = ICardPool(_spellPool);
        towerPool = ICardPool(_towerPool);
        cdhNFT = ICDHNFTInventory(_cdhNft);
        towerInventory = ITowerInventory(_towerInventory);
        blackHoleContract = _blackHoleContract;
        randomNumberSender = _randomNumberSender;
    }

    /// @notice Number of cards to open for each chest
    uint8 public NUMBER_OF_OPENER_CARDS = 3;
    /// @notice Total number of pools being handled while chests are opened
    uint8 constant public NUMBER_OF_POOLS = 4;
    /// @notice Number of various Rarities of CDH Cards
    uint8 constant public NUMBER_OF_RARITY = 4;
    /// @notice Decimal multiplier for each token
    uint256 constant public DECIMALS = 10 ** 18;

        /// @notice Seed Nonce value for a address
    mapping(address => uint256) public seedNonce;



     /// @notice Set wallet address used for generating random number and signature from backend
    /// @dev Backed uses the same wallet address for generating random number and signing the value through the wallet.
    ///      Validation of the signed data, makes sure of the correct wallet used.
    /// @param _newSender New Random Number sender wallet, used for signing.
    function setRandomNumberSender(address _newSender) external onlyOwner {
        require(_newSender != address(0), "Invalid Random Number Sender");

        emit SetRandomNumberSenderAddress(randomNumberSender, _newSender);
        randomNumberSender = _newSender;
    }

    /// @notice Contract Address that acts as BlackHole for tokens that will be burnt in polygon network
    /// @dev BlackHole contract should not be 0x00
    /// @param _blackHoleContract Contract Address of new BlackHole Contract
    function setBlackHoleContract(address _blackHoleContract) external onlyOwner {
        require(_blackHoleContract != address(0), "Invalid Token Lock Contract");

        emit SetBlackHoleContract(_blackHoleContract);
        blackHoleContract = _blackHoleContract;
    }

    /// @notice Generate Base Seed for the sender while opening chest, generated using random number signed value from backend
    /// @dev Nonce value for a sender is stored for maintaining uniqueness in contract itself as well and is used in backend
    ///      while generating random number for the user
    /// @param sender Address that requested for random number and chest opens
    /// @param _quantity Number of tower chest tokens to open
    /// @param _sig Wallet signed random number and signature data
    /// @return Returns bytes32 value encoded with generated random signature and block timestamp
    function _generateBaseSeed(address sender, uint256 _quantity, bytes memory _sig) internal returns (bytes32) {
        uint256 nonce = seedNonce[sender];
        bytes32 hash = keccak256(abi.encodePacked(sender, address(this), _quantity, nonce));
        require(hash.toEthSignedMessageHash().recover(_sig) == randomNumberSender, "NFTRedemption: invalid signature");
        seedNonce[sender]++;
        return keccak256(abi.encodePacked(_sig));
    }



    /// @notice Address of pool contracts based on identifier of pool
    /// @dev Constants are defined for each Pool Identifiers
    /// @param category Category is the Identifier of pools
    /// @return Returns Address of respective pool contracts
    function _getPool(bytes1 category) internal view returns (address) {
        if (category == Constants.EQUIPMENT_POOL_IDENTIFIER) return address(equipmentPool);
        else if (category == Constants.HERO_POOL_IDENTIFIER) return address(heroPool);
        else if (category == Constants.SPELL_POOL_IDENTIFIER) return address(spellPool);
        else if (category == Constants.TOWER_POOL_IDENTIFIER) return address(towerPool);
        else return address(0);
    }



    /// @notice Main method for opening chest, this has provision of handling multiple chests at once. Random number signature
    ///         is passed for base random seed.
    /// @dev Meta Transactions are processed for this open() method.
    ///      On opening chest/s, transfer the chest token from owner's wallet to blackhole contract for locking the token.
    ///      `BaseSeed` is genrated with `_generateBaseSeed()` method and other random seed for pool and rarity selection
    ///      is also generated based on that base seed, to ensure high accuracy random values and signature.
    ///      Methods in pools are called through this method for getting Card Ids from each data pool of each category.
    /// @param _quantity Number of tower chest tokens
    /// @param _sig Random Number signature generated from backend for random seed
    /// @param ticket_type this defines how many number of opener card will be there

    function redeemTicket(uint256 _quantity, bytes memory _sig, uint256 ticket_type) external whenNotPaused{
        require(_quantity > 0, "Quantity must be greater than zero");
        address sender = msg.sender;

        require(sender != address(0), "Invalid Sender");
        ITowerInventory(towerInventory).safeTransferFrom(sender, blackHoleContract, ticket_type, _quantity, '0x00');

        if (ticket_type == 1){
            NUMBER_OF_OPENER_CARDS =5;
        }else if (ticket_type == 2){
            NUMBER_OF_OPENER_CARDS =4;
        }
        else {
            NUMBER_OF_OPENER_CARDS =3;
        }


        bytes32 baseSeed = _generateBaseSeed(sender, _quantity, _sig);
        uint256[] memory idBatch = new uint256[](_quantity * NUMBER_OF_OPENER_CARDS);
        uint256[] memory amountBatch = new uint256[](_quantity * NUMBER_OF_OPENER_CARDS);
        uint256 inner = 0;
        //inner loop counter
        uint256 outer = 0;
        // outer loop counter

        for (uint256 i = 1; i <= _quantity; i++) {
            for (uint256 counter = 1; counter <= NUMBER_OF_OPENER_CARDS; counter++) {
                bytes32 poolSeed = keccak256(abi.encodePacked("POOL", baseSeed, i, counter));
                bytes1 poolId = bytes1(bytes32((uint256(poolSeed) % NUMBER_OF_POOLS + 1) << 248));
                console.logBytes1(poolId);
                address pooladdress = _getPool(poolId);
                console.log("pooladdress: ", pooladdress);
                ICardPool pool = ICardPool(pooladdress);
            

                bytes32 raritySeed = keccak256(abi.encodePacked("RARITY", poolSeed));
                uint256 rarityProb = uint256(raritySeed) % 100 + 1;
                if (NUMBER_OF_OPENER_CARDS == 5){
                    rarity = _getProbabilisticRarityGold(rarityProb, counter);
                }else if (NUMBER_OF_OPENER_CARDS == 4){
                     rarity = _getProbabilisticRaritySilver(rarityProb, counter);
                }
                else {
                     rarity = _getProbabilisticRarityBronze(rarityProb, counter);
                }

                uint256 totalCardsByRarity = pool.numberOfCardsByRarity(rarity);
                uint256 cardIndex = uint256(keccak256(abi.encodePacked("CARD", raritySeed))) % totalCardsByRarity;
                uint256 cardId = pool.getCardIdByRarity(rarity, cardIndex);
                console.log("cardId: ", cardId);

                idBatch[inner + outer] = cardId;
                // console.log("idBatch: ", idBatch[inner + outer]);
                amountBatch[inner + outer] = 1;
                inner++;
            }
            inner--;
            outer++;
        }
 
 
                ICDHNFTInventory(cdhNFT).mintBatch(sender, idBatch, amountBatch, "0x00");


        console.log("idBatch after minitng");
        emit TicketRedemption(sender, _quantity, idBatch, NUMBER_OF_OPENER_CARDS, ticket_type);
    }




    /// @notice Drop rate probability of rarity of cards generated for each card positions
    ///         when chest is opened
    /// @dev Probabilistic Rarity value randomly generated when chest is opened and passed in parameter for Rarity
    /// @param rarityProb Rarity value randomly generated for each card position and pool seed
    /// @param counter Card Position of the card when a chest is opened
    /// @return
    function _getProbabilisticRarityBronze(uint256 rarityProb, uint256 counter) public pure returns (bytes1) {
        if (counter == 1) {
             return Constants.RARITY_RARE;
        } else if (counter == 2 || counter ==3) {
            if (rarityProb <= 50) {
                return Constants.RARITY_COMMON;
            } else if (rarityProb > 50 && rarityProb <= 85) {
                return Constants.RARITY_RARE;
            } else if (rarityProb > 85 && rarityProb <= 100) {
                return Constants.RARITY_EPIC;
            }
        }
        return Constants.RARITY_UNKNOWN;
    }

    function _getProbabilisticRaritySilver(uint256 rarityProb, uint256 counter) public pure returns (bytes1) {
        if (counter == 1) {
            return Constants.RARITY_EPIC;
        } 
        else if (counter == 2) {
            if (rarityProb <= 50) {
                return Constants.RARITY_RARE;
            } else if (rarityProb > 50 && rarityProb <= 95) {
                return Constants.RARITY_EPIC;
            } else if (rarityProb > 95 && rarityProb <= 100) {
                return Constants.RARITY_LEGENDARY;
            }
        } else if (counter == 3) {
            if (rarityProb <= 50) {
                return Constants.RARITY_RARE;
            } else if (rarityProb > 50 && rarityProb <= 90) {
                return Constants.RARITY_EPIC;
            } else if (rarityProb > 90 && rarityProb <= 100) {
                return Constants.RARITY_LEGENDARY;
            }
        }
        else if (counter == 4) {
            if (rarityProb <= 50) {
                return Constants.RARITY_RARE;
            } else if (rarityProb > 50 && rarityProb <= 85) {
                return Constants.RARITY_EPIC;
            } else if (rarityProb > 85 && rarityProb <= 100) {
                return Constants.RARITY_LEGENDARY;
            }
        }
        return Constants.RARITY_UNKNOWN;
    }

    function _getProbabilisticRarityGold(uint256 rarityProb, uint256 counter) public pure returns (bytes1) {
if (counter == 1 || counter==2) {
    return Constants.RARITY_EPIC;
} 
else if (counter == 3 || counter==4 || counter ==5 ) {
    if (rarityProb <= 50) {
        return Constants.RARITY_RARE;
    } else if (rarityProb > 50 && rarityProb <= 85) {
        return Constants.RARITY_EPIC;
    } else if (rarityProb > 85 && rarityProb <= 100) {
        return Constants.RARITY_LEGENDARY;
    }
}
        return Constants.RARITY_UNKNOWN;
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

        function uint2str(uint256 num) public pure returns (string memory _uintAsString) {    
        if (num == 0) {
            return "0";
        }

        uint256 j = num;
        uint256 len;
        while (j != 0) {
            len++;
            j /= 10;
        }

        bytes memory bstr = new bytes(len);
        uint256 k = len - 1;
        while (num != 0) {
            bstr[k--] = bytes1(uint8(48 + (num % 10)));
            num /= 10;
        }
        return string(bstr);
    }

  

}