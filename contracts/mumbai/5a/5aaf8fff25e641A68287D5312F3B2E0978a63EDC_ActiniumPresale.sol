pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";

contract ActiniumPresaleManagement is AccessControl, Ownable, Pausable {
    // for oda token
    address public ODA;
    // for POP
    address public POP;


    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function setODAAddress(address _oda) public onlyOwner {
        ODA = _oda;
    }

    function oda() internal view returns (IERC20) {
        return IERC20(ODA);
    }

    function setPOPAddress(address _pop) public onlyOwner {
        POP = _pop;
    }

    function pop() internal view returns (IERC1155) {
        return IERC1155(POP);
    }

    function getODABalance() public view returns (uint256) {
        return oda().balanceOf(address(this));
    }

    function withdrawODA(uint256 _amount) public onlyOwner {
        require(_amount > 0, "Invalid withdraw amount");
        IERC20(ODA).transferFrom(address(this), owner(), _amount);
    }
}

