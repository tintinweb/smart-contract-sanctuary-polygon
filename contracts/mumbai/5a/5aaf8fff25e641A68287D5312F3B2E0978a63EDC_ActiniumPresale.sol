pragma solidity ^0.8.0;

import "./lib/ActiniumPresaleProduct.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";


contract ActiniumPresale is ActiniumPresaleProduct {
    using SafeERC20 for IERC20;

    event PresalePurchased(uint256 indexed id, uint256 indexed scheduleId, address indexed walletAddress, uint256 purchaseQuantity, uint256 scheduleSoldQuantity, string metadataURI);
    // abi encoded erc721 call function signature - buyer address, quantity, metadata uri
    string internal constant ERC721_BATCH_MINT_SIGNATURE = "batchMint(address,uint256,string)";
    // abi encoded erc1155 call function signature - buyer address, token id(arr), quantity(arr), data
    string internal constant ERC1155_MINT_BATCH_SIGNATURE = "mintBatch(address,uint256[],uint256[],bytes)";

    /**
    * @dev buy presaleProduct by oda
    */
    function purchase(uint256 _id, uint256 _scheduleId, uint256 _quantity) external payable whenNotPaused {
        address buyer = msg.sender;
        /*
        *   presale validations
        */
        // get presale
        PresaleProduct storage presale = _getPresaleProduct(_id);
        // check presale is paused
        require(presale.isPaused == false, "ActiniumPresale: paused");
        // check schedule exists
        uint256 scheduleIdx = _getPresaleScheduleIdx(presale.schedules, _scheduleId);
        PresaleSchedule storage schedule = presale.schedules[scheduleIdx];
        // check buy quantity valid
        _validateSchedulePurchase(_id, schedule, _quantity, buyer);
        // check pop whitelist
        _validateScheduleWhitelist(schedule, buyer);
        /*
        * oda validations
        */
        uint256 totalPrice = schedule.price * _quantity;
        if (totalPrice != 0) {
            IERC20 oda = oda();

            /*
            * ODA processing
            */
            // take ODA to this contract
            oda.transferFrom(buyer, address(this), totalPrice);
        }

        /*
        * bought data processing
        */
        // update sold quantity
        schedule.soldQuantity += _quantity;
        // update purchase quantity
        purchases[_id][_scheduleId][buyer] += _quantity;
        // call presale mint function using erc721 mint function signature
        if (presale.productType == ProductType.ERC721) {
            (bool success,) = presale.productAddress.call(
                abi.encodeWithSignature(ERC721_BATCH_MINT_SIGNATURE, buyer, _quantity, presale.metadataURI)
            );
            require(success, "ActiniumPresale: ERC721 mint failed");
        } else if (presale.productType == ProductType.ERC1155) {
            // call presale mint function using erc1155 mint function signature
            uint256[] memory ids = new uint256[](1);
            uint256[] memory amounts = new uint256[](1);
            ids[0] = presale.tokenId;
            amounts[0] = _quantity;
            (bool success,) = presale.productAddress.call(
                abi.encodeWithSignature(ERC1155_MINT_BATCH_SIGNATURE, buyer, ids, amounts, bytes(presale.metadataURI))
            );
            require(success, "ActiniumPresale: ERC1155 mint failed");
        }


        emit PresalePurchased(_id, _scheduleId, buyer, _quantity, schedule.soldQuantity, presale.metadataURI);
    }

    function _validateSchedulePurchase(uint256 _id, PresaleSchedule memory _schedule, uint256 _quantity, address _address) internal view {
        // check buy quantity valid
        require(_quantity > 0, "ActiniumPresale: invalid quantity");
        // check schedule remain quantity
        require(_schedule.targetQuantity == 0 || _schedule.soldQuantity + _quantity <= _schedule.targetQuantity, "ActiniumPresale: insufficient quantity");
        // check address quantity affordable
        require(_schedule.maxBuyPerWallet == 0 || purchases[_id][_schedule.presaleScheduleId][_address] + _quantity <= _schedule.maxBuyPerWallet, "ActiniumPresale: exceed max buy per wallet");
        // check schedule time
        require(block.timestamp >= _schedule.startTime && block.timestamp <= _schedule.endTime, "ActiniumPresale: not in sale period");
    }

    function _validateScheduleWhitelist(PresaleSchedule memory _schedule, address _buyer) internal view {
        // check pop whitelist
        if (_schedule.whitelistPopIds.length > 0) {
            // check pop using balanceOfBatch
            address[] memory senderAddresses = new address[](_schedule.whitelistPopIds.length);
            uint256[] memory popIds = new uint256[](_schedule.whitelistPopIds.length);
            for (uint256 i = 0; i < _schedule.whitelistPopIds.length; i++) {
                senderAddresses[i] = _buyer;
                popIds[i] = _schedule.whitelistPopIds[i];
            }
            uint256[] memory balances = pop().balanceOfBatch(senderAddresses, popIds);
            // check each pop
            for (uint256 i = 0; i < balances.length; i++) {
                require(balances[i] > 0, "ActiniumPresale: not in whitelist");
            }
        }
    }
}

