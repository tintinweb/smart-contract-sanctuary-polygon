pragma solidity ^0.8.0;

import "./ActiniumPresaleManagement.sol";
import "./ActiniumPresaleStruct.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/introspection/IERC165.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";

contract ActiniumPresaleProduct is ActiniumPresaleManagement {
    using Strings for uint256;

    event PresaleAdded(uint256 indexed id);
    event PresalePaused(uint256 indexed id);
    event PresaleResumed(uint256 indexed id);

    mapping(uint256 => PresaleProduct) public presaleProducts;
    mapping(uint256 => mapping(uint256 => mapping(address => uint256))) public purchases;
    // minimum price
    uint256 private constant MIN_PRICE = 100000000000000;


    function addPresaleProduct(uint256 _id, PresaleProduct calldata _presale) public onlyOwner {
        require(_id > 0, "ActiniumPresaleProduct: invalid product id");
        require(presaleProducts[_id].productType == ProductType.NONE, "ActiniumPresaleProduct: already registered product");
        require(_presale.schedules.length > 0, "ActiniumPresaleProduct: no schedule");
        // validate product contract
        _validateProductContract(_presale.productType, _presale.productAddress);
        // check for every schedules
        _validateSchedules(_presale.schedules);

        // make presale
        presaleProducts[_id] = _presale;

        emit PresaleAdded(_id);
    }

    function addPresaleProductBulk(uint256[] calldata _ids, PresaleProduct[] calldata _presales) external onlyOwner {
        require(_ids.length == _presales.length, "ActiniumPresaleProduct: invalid length");
        for (uint256 i = 0; i < _ids.length; i++) {
            addPresaleProduct(_ids[i], _presales[i]);
        }
    }

    function getPurchasesQuantity(uint256 _presaleId, uint256[] calldata _scheduleIds, address _address) public view returns (uint256[] memory){
        uint256[] memory result = new uint256[](_scheduleIds.length);
        for (uint256 i = 0; i < _scheduleIds.length; i++) {
            result[i] = purchases[_presaleId][_scheduleIds[i]][_address];
        }
        return result;
    }

    function pausePresaleProduct(uint256 _id) external onlyOwner {
        PresaleProduct storage presale = _getPresaleProduct(_id);
        presale.isPaused = true;
        emit PresalePaused(_id);
    }

    function resumePresaleProduct(uint256 _id) external onlyOwner {
        PresaleProduct storage presale = _getPresaleProduct(_id);
        presale.isPaused = false;
        emit PresaleResumed(_id);
    }

    function getPresaleProduct(uint256 _id) external view returns (PresaleProduct memory){
        return _getPresaleProduct(_id);
    }

    function _getPresaleProduct(uint256 _id) internal view returns (PresaleProduct storage) {
        // check presale exists
        require(presaleProducts[_id].productType != ProductType.NONE, "ActiniumPresaleProduct: presale not exists");
        return presaleProducts[_id];
    }

    function getPresaleSchedule(uint256 _id, uint256 _scheduleId) external view returns (PresaleSchedule memory){
        PresaleProduct memory presale = _getPresaleProduct(_id);
        uint256 scheduleIdx = _getPresaleScheduleIdx(presale.schedules, _scheduleId);
        return presale.schedules[scheduleIdx];
    }

    function _getPresaleScheduleIdx(PresaleSchedule[] memory _schedules, uint256 _scheduleId) internal view returns (uint256){
        for (uint256 i = 0; i < _schedules.length; i++) {
            if (_schedules[i].presaleScheduleId == _scheduleId) {
                return i;
            }
        }
        revert("ActiniumPresaleProduct: invalid schedule id");
    }

    function _validateProductContract(ProductType _productType, address _productAddress) internal view {
        if (_productType == ProductType.ERC721) {
            require(IERC165(_productAddress).supportsInterface(type(IERC721).interfaceId), "ActiniumPresaleProduct: invalid product contract");
        } else if (_productType == ProductType.ERC1155) {
            require(IERC165(_productAddress).supportsInterface(type(IERC1155).interfaceId), "ActiniumPresaleProduct: invalid product contract");
        } else {
            revert("ActiniumPresaleProduct: invalid product type");
        }
    }

    /*
    * @dev check presale schedules
    */
    function _validateSchedules(PresaleSchedule[] memory _schedules) internal view {
        // check for every schedules
        for (uint256 i = 0; i < _schedules.length; i++) {
            PresaleSchedule memory schedule = _schedules[i];
            require(schedule.endTime >= schedule.startTime, "ActiniumPresaleProduct: invalid schedule end time");
            require(schedule.soldQuantity == 0, "ActiniumPresaleProduct: invalid sold quantity");
            // free mint or greater than minimum price
            require(schedule.price >= MIN_PRICE || schedule.price == 0, "ActiniumPresaleProduct: invalid price");
        }
    }
}

