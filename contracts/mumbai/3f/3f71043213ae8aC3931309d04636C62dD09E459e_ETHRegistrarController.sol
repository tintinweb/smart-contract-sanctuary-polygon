import {IRebateRegistrar} from "./IRebateRegistrar.sol";

import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {IERC165} from "@openzeppelin/contracts/utils/introspection/IERC165.sol";
import {Address} from "@openzeppelin/contracts/utils/Address.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {SafeERC20} from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import {INameWrapper} from "../wrapper/INameWrapper.sol";
import {ERC20Recoverable} from "../utils/ERC20Recoverable.sol";

error CommitmentTooNew(bytes32 commitment);
error CommitmentTooOld(bytes32 commitment);
error NameNotAvailable(string name);
error DurationTooShort(uint256 duration);
error ResolverRequiredWhenDataSupplied();
error UnexpiredCommitmentExists(bytes32 commitment);
error InsufficientValue();
error Unauthorised(bytes32 node);
error MaxCommitmentAgeTooLow();
error MaxCommitmentAgeTooHigh();

/**
 * @dev A registrar controller for registering and renewing names at fixed cost.
 */
contract ETHRegistrarController is
    Ownable,
    IETHRegistrarController,
    IERC165,
    ERC20Recoverable,
    ReverseClaimer
{
    using StringUtils for *;
    using Address for address;
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    uint256 public constant MIN_REGISTRATION_DURATION = type(uint256).max;
    bytes32 private constant BIU_NODE =
        0xe90f581e17e2ccc47f963f9b0a784a87e3b279e6b1fac46765376ed8be536d83;
    uint256 private constant MAX_EXPIRY = type(uint256).max;
    BaseRegistrarImplementation immutable base;
    address public immutable usdtAddress;
    IPriceOracle public prices;
    IRebateRegistrar public rebateRegistrar;
    uint256 public immutable minCommitmentAge;
    uint256 public immutable maxCommitmentAge;
    ReverseRegistrar public immutable reverseRegistrar;
    INameWrapper public immutable nameWrapper;
    uint256[] public steps = [9, 8, 7, 6, 5, 4, 3];

    mapping(bytes32 => uint256) public commitments;

    event NameRegistered(RegisterInfo info, ReabateInfo rebateInfo);
 
    event NewPriceOracle(address indexed oracle);

    constructor(
        BaseRegistrarImplementation _base,
        IPriceOracle _prices,
        uint256 _minCommitmentAge,
        uint256 _maxCommitmentAge,
        ReverseRegistrar _reverseRegistrar,
        INameWrapper _nameWrapper,
        ENS _ens,
        IRebateRegistrar _rebateRegistrar,
        address _usdtAddress
    ) ReverseClaimer(_ens, msg.sender) {
        if (_maxCommitmentAge <= _minCommitmentAge) {
            revert MaxCommitmentAgeTooLow();
        }

        if (_maxCommitmentAge > block.timestamp) {
            revert MaxCommitmentAgeTooHigh();
        }

        base = _base;
        prices = _prices;
        minCommitmentAge = _minCommitmentAge;
        maxCommitmentAge = _maxCommitmentAge;
        reverseRegistrar = _reverseRegistrar;
        nameWrapper = _nameWrapper;
        rebateRegistrar = _rebateRegistrar;
        usdtAddress = _usdtAddress;
    }

    function setPriceOracle(IPriceOracle _prices) public onlyOwner {
        prices = _prices;
        emit NewPriceOracle(address(prices));
    }

    function setRebateRegistrar(
        IRebateRegistrar _rebateRegistrar
    ) public onlyOwner {
        rebateRegistrar = _rebateRegistrar;
    }

    function setSteps(uint256[] memory _steps) public onlyOwner {
        steps = _steps;
    }

    function rentPrice(
        string memory name,
        uint256 duration
    ) public view override returns (IPriceOracle.Price memory price) {
        bytes32 label = keccak256(bytes(name));
        price = prices.price(
            name,
            base.nameExpires(uint256(label)),
            duration,
            false
        );
        return price;
    }

    function rentUSDPrice(
        string memory name,
        uint256 duration
    ) public view returns (IPriceOracle.Price memory price) {
        bytes32 label = keccak256(bytes(name));
        price = prices.price(
            name,
            base.nameExpires(uint256(label)),
            duration,
            true
        );
        return price;
    }

    function valid(string memory name) public view returns (bool) {
        return
            name.strlen() >= 3 &&
            (_contains(name.strlen()) || name.strlen() >= 9);
    }

    function available(string memory name) public view override returns (bool) {
        bytes32 label = keccak256(bytes(name));
        return valid(name) && base.available(uint256(label));
    }

    function register(
        RegisterData calldata data,
        bool isERC20
    ) public payable override {
        IPriceOracle.Price memory price;
        IERC20 usdt = IERC20(usdtAddress);
        bool isRebate = false;
        address rebateAddress = address(0);
        uint256 rebateAmount = 0;
        if (!available(data.name)) {
            revert NameNotAvailable(data.name);
        }
        if (!isERC20) {
            price = rentPrice(data.name, data.duration);
        } else {
            price = rentUSDPrice(data.name, data.duration);
        }
        if (data.rebateName.strlen() >= 3) {
            (isRebate, rebateAddress) = rebateRegistrar.checkRegistar(
                data.rebateName,
                data.resolver
            );
        }
       
        if ((!available(data.rebateName) || isRebate) && price.base > 0 && data.rebateName.strlen() >= 3) {
            price.base = price.base.mul(95).div(100);
            if (isRebate) {
                rebateAmount = price.base.mul(15).div(100);
            } else if (!available(data.rebateName)) {
                rebateAmount = price.base.mul(5).div(100);
            }
        }
        if (
            !isERC20 &&
            ((msg.value < price.base + price.premium) && price.base > 0)
        ) {
            revert InsufficientValue();
        } else if (price.base > 0 && isERC20) {
            uint256 allowance = usdt.allowance(msg.sender, address(this));
            require(
                allowance >= price.base + price.premium,
                "Insufficient ERC20 token allowance"
            );
            usdt.safeTransferFrom(
                msg.sender,
                address(this),
                price.base + price.premium
            );
        }
        uint256 expires = nameWrapper.registerAndWrapETH2LD(
            data.name,
            data.owner,
            data.duration,
            data.resolver,
            data.ownerControlledFuses
        );

        if (data.data.length > 0) {
            _setRecords(data.resolver, keccak256(bytes(data.name)), data.data);
        }

        if (data.reverseRecord) {
            _setReverseRecord(data.name, data.resolver, msg.sender);
        }
        if (
            !isERC20 && rebateAmount > 0 && address(this).balance > rebateAmount
        ) {
            payable(rebateAddress).transfer(rebateAmount);
        } else if (isERC20 && rebateAmount > 0) {
            usdt.safeTransfer(rebateAddress, rebateAmount);
        }
        emit NameRegistered(
            RegisterInfo({
                name: data.name,
                label: keccak256(bytes(data.name)),
                owner: data.owner,
                baseCost: price.base,
                premium: price.premium,
                expires: expires,
                isERC20: isERC20
            }),
            ReabateInfo({
                rebateAddress: rebateAddress,
                rebateAmount: rebateAmount,
                rebateName: data.rebateName
            })
        );

        if (!isERC20 && msg.value > (price.base + price.premium)) {
            payable(msg.sender).transfer(
                msg.value - (price.base + price.premium)
            );
        }
    }

    function withdraw() public onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    function supportsInterface(
        bytes4 interfaceID
    ) external pure returns (bool) {
        return
            interfaceID == type(IERC165).interfaceId ||
            interfaceID == type(IETHRegistrarController).interfaceId;
    }
    
    function _setRecords(
        address resolverAddress,
        bytes32 label,
        bytes[] calldata data
    ) internal {
        // use hardcoded .biu namehash
        bytes32 nodehash = keccak256(abi.encodePacked(BIU_NODE, label));
        Resolver resolver = Resolver(resolverAddress);
        resolver.multicallWithNodeCheck(nodehash, data);
    }

    function _setReverseRecord(
        string memory name,
        address resolver,
        address owner
    ) internal {
        reverseRegistrar.setNameForAddr(
            msg.sender,
            owner,
            resolver,
            string.concat(name, ".biu")
        );
    }

    function _contains(uint256 number) internal view returns (bool) {
        for (uint i = 0; i < steps.length; i++) {
            if (steps[i] == number) {
                return true;
            }
        }
        return false;
    }
}

