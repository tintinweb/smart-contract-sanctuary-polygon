pragma solidity >=0.8.0 <0.9.0;

import "./openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721RoyaltyUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/MathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/Base64Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

interface INever {
    function isMinter(address _minter) external view returns (bool);

    function mintAmount(address _minter) external view returns (uint256);

    function totalMinter() external view returns (uint256);

    function totalSupply() external view returns (uint256);

    function maximumSupply() external view returns (uint256);

    function totalBridgedToken() external view returns (uint256);
}

contract MinterShare is
    Initializable,
    ERC721RoyaltyUpgradeable,
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable,
    UUPSUpgradeable,
    EIP712Upgradeable
{
    event PaymentReceived(address from, uint256 amount);
    event PayeeAdded(address indexed account, uint256 tokenId, uint256 shares);
    event ShareReleased(
        address indexed payee,
        uint256 amount,
        uint256 indexed tokenId
    );

    using StringsUpgradeable for uint256;
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;

    CountersUpgradeable.Counter private supply;
    EnumerableSetUpgradeable.UintSet private bridgedTokenId;

    mapping(uint256 => address) public bridgeContract;
    mapping(bytes32 => bool) public usedHash;
    mapping(address => bool) public minterClaimed;
    mapping(address => bool) private relayer;
    mapping(uint256 => uint256) private _shares;
    mapping(uint256 => uint256) private _totalReleased;
    mapping(uint256 => mapping(uint256 => uint256)) private _released;
    mapping(IERC20Upgradeable => mapping(uint256 => uint256))
        private _erc20TotalReleased;
    mapping(IERC20Upgradeable => mapping(uint256 => mapping(uint256 => uint256)))
        private _erc20Released;
    address public interfaceAddr;
    address public erc20;

    string public uriPrefix;
    string public uriSuffix;
    string private _contractURI;
    string public hiddenMetadata;

    uint256 public contractStart;
    uint256[] private blockMined;
    bytes32 public constant _CLAIM_TYPEHASH =
        keccak256(
            "Claim(uint256 blocks,address holder,uint256 tokenId,uint256 chainFrom,address contract)"
        );
    bytes32 public constant _CLAIMERC20_TYPEHASH =
        keccak256(
            "ClaimERC20(address erc20,uint256 blocks,address holder,uint256 tokenId,uint256 chainFrom,address contract)"
        );

    bool private isHidden;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(address _erc20) public initializer {
        __ERC721_init("test NFT Minter", "MVFT");
        __EIP712_init("test NFT Minter", "Polygon");
        __ERC721Royalty_init();
        __Ownable_init();
        __ReentrancyGuard_init();
        __UUPSUpgradeable_init();
        blockMined.push(block.number - 1);
        contractStart = block.timestamp;
        _setDefaultRoyalty(owner(), 1000);
        setContractURI(
            "https://bafybeibze73yfrrgkmsqndzua4ze3qpxq32jzgd55gts5fiyapbm2irt3u.ipfs.nftstorage.link/testContractURIMinter.json"
        );
        uriSuffix = ".png";
        uriPrefix = "https://ikzttp.mypinata.cloud/ipfs/QmYDvPAXtiJg7s8JdRBSLWdgSphQdac8j1YuQNNxcGE1hg/";
        hiddenMetadata = "https://bafybeidtrwv5ckcpbys7nv5d6b3zjm4zx6524ukn54kmjk7rktiod4fuxm.ipfs.dweb.link/";
        erc20 = _erc20;
        isHidden = true;
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyOwner
    {}

    receive() external payable {
        emit PaymentReceived(_msgSender(), msg.value);
    }

    function setRelayer(address _relay) public onlyOwner {
        relayer[_relay] = true;
    }

    function setBridgeContract(uint256 chain, address _contract)
        public
        onlyOwner
    {
        bridgeContract[chain] = _contract;
    }

    function totalShares() public view returns (uint256) {
        INever token = INever(interfaceAddr);
        return token.maximumSupply();
    }

    function getBlock() public view returns (uint256) {
        uint256 last = blockMined.length - 1;
        return blockMined[last];
    }

    function queryBlock() public view returns (uint256[] memory) {
        return blockMined;
    }

    function totalReleased(uint256 _b) public view returns (uint256) {
        return _totalReleased[_b];
    }

    function totalReleased(IERC20Upgradeable token, uint256 _b)
        public
        view
        returns (uint256)
    {
        return _erc20TotalReleased[token][_b];
    }

    function shares(uint256 _tokenId) public view returns (uint256) {
        return _shares[_tokenId];
    }

    function released(uint256 _b, uint256 _tokenId)
        public
        view
        returns (uint256)
    {
        return _released[_b][_tokenId];
    }

    function released(
        IERC20Upgradeable token,
        uint256 _b,
        uint256 _tokenId
    ) public view returns (uint256) {
        return _erc20Released[token][_b][_tokenId];
    }

    function _pendingPayment(
        uint256 _tokenId,
        uint256 totalReceived,
        uint256 alreadyReleased
    ) private view returns (uint256) {
        uint256 total = totalShares();
        return (totalReceived * _shares[_tokenId]) / total - alreadyReleased;
    }

    function IERC20pendingPaymentPerToken(
        IERC20Upgradeable _token,
        uint256 _tokenId
    ) public view returns (uint256) {
        require(_exists(_tokenId), "NFT not exist yet");

        uint256 blocks = getBlock();

        uint256 totalReceived = _token.balanceOf(address(this)) +
            totalReleased(_token, blocks);
        return
            _pendingPayment(
                _tokenId,
                totalReceived,
                released(_token, blocks, _tokenId)
            );
    }

    function nativePendingPaymentPerToken(uint256 _tokenId)
        public
        view
        returns (uint256)
    {
        require(_exists(_tokenId), "NFT not exist yet");

        uint256 blocks = getBlock();

        uint256 totalReceived = address(this).balance + totalReleased(blocks);
        return
            _pendingPayment(
                _tokenId,
                totalReceived,
                released(blocks, _tokenId)
            );
    }

    function _addPayee(address account, uint256 _tokenId) private {
        require(
            account != address(0),
            "PaymentSplitter: account is the zero address"
        );

        INever never = INever(interfaceAddr);
        uint256 shares_ = never.mintAmount(account);

        _shares[_tokenId] = shares_;
        emit PayeeAdded(account, _tokenId, shares_);
    }

    function release(uint256 _tokenId) external nonReentrant {
        require(_exists(_tokenId), "either NFT not exist or bridged");
        address account = ownerOf(_tokenId);

        uint256 blocks = getBlock();

        uint256 totalReceived = address(this).balance + totalReleased(blocks);
        uint256 payment = _pendingPayment(
            _tokenId,
            totalReceived,
            released(blocks, _tokenId)
        );

        require(payment != 0, "PaymentSplitter: account is not due payment");

        _released[blocks][_tokenId] += payment;
        _totalReleased[blocks] += payment;

        AddressUpgradeable.sendValue(payable(account), payment);
        // emit PaymentReleased(account, payment);
    }

    //if delta pointer are big, the transaction will likely to fail due to blocksize limit
    function releaseAllRoyalty(
        IERC20Upgradeable _token,
        address _holder,
        uint256 _startPoint,
        uint256 _endPoint
    ) external nonReentrant {
        require(_startPoint < _endPoint);

        uint256 blocks = getBlock();

        uint256 total;

        uint256 totalReceived = _token.balanceOf(address(this)) +
            totalReleased(_token, blocks);
        for (_startPoint; _startPoint <= _endPoint; _startPoint++) {
            if (ownerOf(_startPoint) == _holder) {
                uint256 pay = _pendingPayment(
                    _startPoint,
                    totalReceived,
                    released(_token, blocks, _startPoint)
                );
                _erc20Released[_token][blocks][_startPoint] += pay;
                total += pay;
            }
        }
        _erc20TotalReleased[_token][blocks] += total;
        SafeERC20Upgradeable.safeTransfer(_token, _holder, total);
    }

    function erc20Release(IERC20Upgradeable token, uint256 _tokenId)
        external
        nonReentrant
    {
        require(_exists(_tokenId), "either NFT not exist or bridged");
        address account = ownerOf(_tokenId);

        uint256 blocks = getBlock();

        uint256 totalReceived = token.balanceOf(address(this)) +
            totalReleased(token, blocks);
        uint256 payment = _pendingPayment(
            _tokenId,
            totalReceived,
            released(token, blocks, _tokenId)
        );

        require(payment != 0, "PaymentSplitter: account is not due payment");

        _erc20Released[token][blocks][_tokenId] += payment;
        _erc20TotalReleased[token][blocks] += payment;

        SafeERC20Upgradeable.safeTransfer(token, account, payment);
        emit ShareReleased(account, payment, _tokenId);
    }

    function claimMint() public nonReentrant {
        // Verify mint requirements
        require(mintEnabled(), "The mint is not enabled!");
        require(!minterClaimed[_msgSender()], "already claimed!");

        INever never = INever(interfaceAddr);
        require(
            never.isMinter(_msgSender()),
            "You are not #NEVERFORGET minter!"
        );

        minterClaimed[_msgSender()] = true;
        supply.increment();
        _safeMint(_msgSender(), supply.current());
        _addPayee(_msgSender(), supply.current());
    }

    function maxSupply() public view returns (uint256) {
        INever never = INever(interfaceAddr);
        return never.totalMinter();
    }

    function tokensOfOwner(address _holder)
        public
        view
        returns (uint256[] memory)
    {
        uint256 balance = balanceOf(_holder);
        uint256 len = totalSupply();
        uint256[] memory ids = new uint256[](balance);
        uint256 index;
        for (uint256 i = 0; i < len; i++) {
            (address addr, uint256 id) = tokenIdOwner(i);
            if (addr == _holder) {
                ids[index] = id;
                index++;
            }
        }
        return ids;
    }

    function setUriPrefix(string memory _uriPrefix) public onlyOwner {
        uriPrefix = _uriPrefix;
    }

    function setWETHAddr(address _erc20) public onlyOwner {
        erc20 = _erc20;
    }

    function setHiddenMetadata(string memory _hiddenMetadata) public onlyOwner {
        hiddenMetadata = _hiddenMetadata;
    }

    function revealOrConceal(bool _bool) public onlyOwner {
        isHidden = _bool;
    }

    //after 2 years
    function emergencyRelease(IERC20Upgradeable _token) public onlyOwner {
        require(block.timestamp > contractStart + 720);

        SafeERC20Upgradeable.safeTransfer(
            _token,
            owner(),
            _token.balanceOf(address(this))
        );
    }

    //after 2 years
    function emergencyWithdraw() public payable onlyOwner {
        require(block.timestamp > contractStart + 720);

        (bool os, ) = payable(owner()).call{value: address(this).balance}("");
        require(os);
    }

    //reset after emergency withdraw
    function nextBlock() public onlyOwner {
        blockMined.push(block.number - 1);
    }

    function setUriSuffix(string memory _uriSuffix) public onlyOwner {
        uriSuffix = _uriSuffix;
    }

    function mintEnabled() public view returns (bool) {
        INever token = INever(interfaceAddr);

        uint256 supplie = token.totalSupply() + token.totalBridgedToken();
        uint256 max = token.maximumSupply();

        return _msgSender() == owner() || supplie >= max;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return uriPrefix;
    }

    function setInterfaceAddress(address _nft) public onlyOwner {
        interfaceAddr = _nft;
    }

    function interfaceAddress() external view returns (address) {
        return interfaceAddr;
    }

    function contractURI() public view returns (string memory) {
        return _contractURI;
    }

    function setContractURI(string memory contractURI_) public onlyOwner {
        _contractURI = contractURI_;
    }

    function setDefaultRoyalty(address _new, uint96 _value) public onlyOwner {
        _setDefaultRoyalty(_new, _value);
    }

    function _bridgeToken(uint256 _tokenId) private {
        require(_msgSender() == ownerOf(_tokenId));
        bridgedTokenId.add(_tokenId);
        _burn(_tokenId);
    }

    function _bridgeBack(address to, uint256 _tokenId) private {
        bridgedTokenId.remove(_tokenId);
        _safeMint(to, _tokenId);
    }

    function queryTokenBridge() public view returns (uint256[] memory) {
        return bridgedTokenId.values();
    }

    function totalTokenBridge() public view returns (uint256) {
        return bridgedTokenId.length();
    }

    function tokenIdName(uint256 _tokenId) public pure returns (string memory) {
        return
            string(
                abi.encodePacked(
                    "Minter Share Never Forget NFT #",
                    _tokenId.toString()
                )
            );
    }

    function imageTokenId(uint256 _tokenId)
        public
        view
        returns (string memory)
    {
        if (isHidden) {
            return hiddenMetadata;
        }
        return
            string(abi.encodePacked(uriPrefix, _tokenId.toString(), uriSuffix));
    }

    function descriptionTokenId(uint256 _tokenId)
        public
        view
        returns (string memory)
    {
        if (_tokenId != 1) {
            return
                string(
                    abi.encodePacked(
                        "Never Forget 24.FEB.2022 Minter Share NFT are use for claim royalty in the Dapp. To see updated royalty this token hold, click refresh metadata button in your respective marketplace. To be whitelisted to mint this NFT you need to mint at least 1 #NeverForget24.Feb.2022 NFT. The amount you mint represent share owned per token Id. Token Id number #",
                        uintToString(_tokenId),
                        "Share Owned = ",
                        uintToString(shares(_tokenId)),
                        "."
                    )
                );
        } else {
            return
                string(
                    abi.encodePacked(
                        "Never Forget 24.FEB.2022 Minter Share NFT are use for claim royalty in the Dapp. To see updated royalty this token hold, click refresh metadata button in your respective marketplace. To be whitelisted to mint this NFT you need to mint at least 1 #NeverForget24.Feb.2022 NFT. The amount you mint represent share owned per token Id. Token Id number #",
                        uintToString(_tokenId),
                        "Share Owned = ",
                        uintToString(shares(_tokenId)),
                        ". Token Id number #1 are reserved for whoever who can mint SS Rare Never Forget 24.FEB.2022 NFT"
                    )
                );
        }
    }

    function uintToString(uint256 _value) public pure returns (string memory) {
        return _value.toString();
    }

    function cut(uint256 _value) public pure returns (uint256) {
        uint256 denom = 1 ether;
        return _value / denom;
    }

    function cutGwei(uint256 _value) public pure returns (uint256) {
        uint256 denom = 10000 gwei;
        return _value / denom;
    }

    function padZero(uint256 _value) public pure returns (string memory zero) {
        if (_value == 1) {
            zero = "0000";
        }
        if (_value == 2) {
            zero = "000";
        }
        if (_value == 3) {
            zero = "00";
        }
        if (_value == 4) {
            zero = "0";
        }
    }

    function format(uint256 _value)
        public
        pure
        returns (string memory formated)
    {
        uint256 beforeComa = cut(_value);
        uint256 afterComa = cutGwei(_value);
        if (beforeComa != 0) {
            uint256 result = afterComa - (beforeComa * 100000);
            string memory value = result.toString();
            uint256 len = bytes(value).length;
            if (len == 5) {
                formated = string(
                    abi.encodePacked(beforeComa.toString(), ".", value)
                );
            } else {
                formated = string(
                    abi.encodePacked(
                        beforeComa.toString(),
                        ".",
                        padZero(len),
                        value
                    )
                );
            }
        } else if (afterComa != 0) {
            string memory value = afterComa.toString();
            uint256 len = bytes(value).length;
            if (len == 5) {
                formated = string(abi.encodePacked("0.", value));
            } else {
                formated = string(abi.encodePacked("0.", padZero(len), value));
            }
        } else {
            formated = "0";
        }
    }

    function traitTokenId(uint256 _tokenId)
        public
        view
        returns (string memory)
    {
        IERC20Upgradeable wethToken = IERC20Upgradeable(erc20);
        uint256 blocks = getBlock();
        uint256 totalWETH = released(wethToken, blocks, _tokenId);
        uint256 totalMatic = released(blocks, _tokenId);
        uint256 accumWETH = IERC20pendingPaymentPerToken(wethToken, _tokenId);
        uint256 accumMatic = nativePendingPaymentPerToken(_tokenId);
        uint256 shareOwned = shares(_tokenId);

        return
            string(
                abi.encodePacked(
                    '{"trait_type" : "Share Owned Per Token Id", "value" : ',
                    uintToString(shareOwned),
                    ', "max_value" : 10000}, {"trait_type" : "Total WETH Received", "value" : ',
                    format(totalWETH),
                    ', "max_value" : 100}, {"trait_type" : "Total Matic Received", "value" : ',
                    format(totalMatic),
                    ', "max_value" : 1000}, {"trait_type" : "Royalty WETH Accumulated", "value" : ',
                    format(accumWETH),
                    ', "max_value" : 100}, {"trait_type" : "Royalty Matic Accumulated", "value" : ',
                    format(accumMatic),
                    ', "max_value" : 1000}, {"display_type": "date", "trait_type": "last updated", "value" : ',
                    uintToString(block.timestamp),
                    "}"
                )
            );
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(_tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64Upgradeable.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"',
                                tokenIdName(_tokenId),
                                '", "description":"',
                                descriptionTokenId(_tokenId),
                                '", "image": "',
                                imageTokenId(_tokenId),
                                '", "external_url": "neverforget24FEB.com", "attributes": [',
                                traitTokenId(_tokenId),
                                "]}"
                            )
                        )
                    )
                )
            );
    }

    function version() public pure returns (string memory) {
        return "V1";
    }
}

