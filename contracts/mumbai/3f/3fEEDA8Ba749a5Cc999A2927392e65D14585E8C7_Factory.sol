pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";

import "./interfaces/IFactory.sol";
import "./interfaces/exchange/IExchange.sol";
import "./alp/AlpDeployer.sol";
import "./interfaces/IAlp.sol";
import "hardhat/console.sol";

/**
 * @title D4X Factory
 */
contract Factory is IFactory, AlpDeployer, Ownable {
    address public resolver;

    /// @dev Holds a mapping of tokens to alps.
    mapping(address => address) private alpsMap;

    /// @dev Holds a mapping of exchange types to exchanges.
    mapping(IExchange.DEX => IExchange) private exchanges;

    /// @dev Array of all alps
    address[] public alps;

    /// @dev Array of all tokens
    address[] public _tokens;

    mapping(address => uint) private _tokensMap;

    /// @dev Method can only be called by the ALP contract or this.
    modifier isAlpOrThis() {
        if (msg.sender != address(this)) {
            address baseToken = IAlp(msg.sender).getBaseToken();

            require(
                alpsMap[baseToken] == msg.sender,
                "Factory: Caller is not ALP or this"
            );
        }
        _;
    }

    /**
     * @notice changes resolver address
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setResolver(address resolver_) external onlyOwner {
        resolver = resolver_;
    }

    /// @notice Get number of alps
    function countAlp() external view returns (uint256) {
        return alps.length;
    }

    /// @notice Get alp by `token`
    function getAlp(address token) external view returns (address alp) {
        return alpsMap[token];
    }

    function getTokens() external view returns (address[] memory) {
        return _tokens;
    }

    /// @notice Get exchange by `type_`
    function getExchange(
        IExchange.DEX type_
    ) external view returns (IExchange) {
        return exchanges[type_];
    }

    /**
     * @notice Register `exchange` by `type_`
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function registerExchange(
        IExchange.DEX type_,
        IExchange exchange
    ) external onlyOwner {
        exchanges[type_] = exchange;
    }

    function addToken(address token) external isAlpOrThis {
        if (_tokensMap[token] == 0) {
            _tokens.push(token);
            _tokensMap[token] = _tokens.length;
        }
    }

    function removeToken(address token) external isAlpOrThis {
        uint256 index = _tokensMap[token];

        if (index > 0) {
            index--;
            _tokensMap[_tokens[index]] = 0;
            _tokens[index] = _tokens[_tokens.length - 1];
            _tokensMap[_tokens[index]] = index + 1;
            _tokens.pop();
        }
    }

    /**
     * @notice Creating the new alp
     *
     * @param token Token to be assign to the alp
     * @param name Name of the alp
     * @return alp The address of the new alp
     * @dev this function calls method deploy from AlpDeployer contract
     *
     * Emits a `AlpCreated` event.
     *
     * Requirements:
     *
     * - `token` cannot be the zero address.
     * - alp with `token` must not exist.
     *
     */
    function createAlp(
        address token,
        string memory name,
        uint8 decimals
    ) external returns (address alp) {
        require(token != address(0), "Factory: ZERO_ADDRESS");
        require(alpsMap[token] == address(0), "Factory: ALP_EXIST");

        alp = deploy(address(this), token, name, decimals);

        alpsMap[token] = alp;
        alps.push(alp);

        this.addToken(token);

        emit AlpCreated(token, alp, alps.length);
    }
}

