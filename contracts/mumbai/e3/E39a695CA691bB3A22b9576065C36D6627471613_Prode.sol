        uint256 pot;
        uint256 finishedMatches;
        TournamentStatus status;
        bool allowMultipleBets;
        bytes32 league;
        bytes32 code;
        uint256 dateFinished;
    }

    enum TournamentStatus {
        PENDING,
        STARTED,
        FINISHED
    }

    enum MatchStatus {
        PENDING,
        FINISHED,
        SUSPENDED
    }
    enum MatchResult {
        LOCAL,
        VISITOR,
        DRAW
    }

    struct Match {
        uint256 id;
        bytes32 group;
        bytes32 team1;
        bytes32 team2;
        uint256 goalsTeam1;
        uint256 goalsTeam2;
        uint256 date;
        MatchStatus status;
    }

    struct Forecast {
        uint256 matchId;
        bytes32 team1;
        bytes32 team2;
        uint256 goalsTeam1;
        uint256 goalsTeam2;
    }

    struct Bet {
        uint256 id;
        uint256 tournamentId;
        address owner;
        uint256 userId;
        uint256 amountPayed;
        Forecast[] forecasts;
        uint256 timestamp;
    }

    struct BetDTO {
        // Bet
        uint256 id;
        address owner;
        uint256 userId;
        uint256 timestamp;
        Score[] forecastScores;
        Forecast[] forecasts;
        // Score
        uint256 resultPoints;
        uint256 goalsPoints;
        uint256 bonusPoints;
        uint256 score;
    }

    struct Score {
        uint256 resultPoints;
        uint256 goalsPoints;
        uint256 bonusPoints;
        uint256 score;
    }

    struct WinnerBet {
        uint256 id;
        address owner;
        uint256 userId;
        uint256 timestamp;
        uint256 score;
        uint256 prize;
        bool claimed;
    }

    struct WinnerBetDTO {
        // Bet
        uint256 id;
        address owner;
        uint256 userId;
        uint256 timestamp;
        // Score
        uint256 resultPoints;
        uint256 goalsPoints;
        uint256 bonusPoints;
        uint256 score;
        // WinnerBet
        uint256 prize;
        bool claimed;
    }

    struct ProdeStorage {
        uint256 numTournaments;
        mapping(uint256 => Tournament) tournaments;
        mapping(uint256 => Bet[]) tournamentBets;
        mapping(uint256 => Score[]) tournamentBetScores;
        // address => tournamentId => betId[]
        mapping(address => mapping(uint256 => uint256[])) bets;
        // Para los que pagan con fiat: userId => tournamentId => betId[]
        mapping(uint256 => mapping(uint256 => uint256[])) betsByUserId;
        // Our unclaimed fees
        uint256 unclaimedFees;
        // Players unclaimed prizes
        uint256 unclaimedPrizes;
        uint256 lastTournamendIdWithPrizesExpired;
    }

    function prodeStorage() internal pure returns (ProdeStorage storage ds) {
        bytes32 position = PRODE_STORAGE_POSITION;
        assembly {
            ds.slot := position
        }
    }

    function getStableAddress(
        address testErc20Address
    ) internal view returns (address erc20address) {
        if (block.chainid == 137)
            // mainnet
            erc20address = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174;
        else if (block.chainid == 80001)
            // mumbai
            erc20address = 0xE097d6B3100777DC31B34dC2c58fB524C2e76921;
            // test
        else erc20address = testErc20Address;
    }
}

