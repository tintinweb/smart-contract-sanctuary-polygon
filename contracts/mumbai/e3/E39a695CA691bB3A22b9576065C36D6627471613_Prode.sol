pragma solidity >=0.8.0;

import {LibProde} from "../libraries/LibProde.sol";
import {LibDiamond} from "../libraries/LibDiamond.sol";
import {LibReentrancy} from "../libraries/LibReentrancy.sol";
import {ERC20} from "../openzeppelin/token/ERC20/ERC20.sol";
import "hardhat/console.sol";

contract Prode {
    uint256 public constant MATCH_RESULT_POINTS = 3;
    uint256 public constant GOALS_POINTS = 1;
    uint256 public constant BONUS_POINTS = 5;
    uint256 public constant WINNERS = 3;
    uint256 public constant TOURNAMENT_FEE = 10;

    modifier onlyOwner() {
        LibDiamond.enforceIsContractOwner();
        _;
    }

    modifier nonReentrancy() {
        LibReentrancy.nonReentrantBefore();
        _;
        LibReentrancy.nonReentrantAfter();
    }

    event TournamentCreated(LibProde.Tournament tournament);
    event WinnerBetsCalculated(uint256 tournamentId);

    // event Log(string message);
    // event LogBytes(bytes data);

    function _getTournament(
        uint256 tournamentId
    ) internal view returns (LibProde.Tournament storage) {
        LibProde.ProdeStorage storage ps = LibProde.prodeStorage();
        mapping(uint256 => LibProde.Tournament) storage tournaments = ps
            .tournaments;
        require(
            tournamentId > 0 && tournamentId <= ps.numTournaments,
            "The tournament doesn't exist"
        );
        return tournaments[tournamentId];
    }

    function createTournament(
        string memory name,
        LibProde.Match[] memory matches,
        uint256 betPrice,
        uint256 pot,
        bool allowMultipleBets,
        bytes32 league,
        bytes32 code
    )
        external
        onlyOwner
        returns (LibProde.Tournament memory createdTournament)
    {
        require(bytes(name).length > 0, "Name can't be empty");
        require(betPrice >= 0, "Bet price must be >= 0");
        require(
            matches.length > 0,
            "The tournament must have at least one match"
        );
        require(
            matches[0].date > block.timestamp * 1000,
            "First match must start in a future date"
        );

        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();

        // Tournament creation
        bs.numTournaments++;

        LibProde.Tournament storage newTournament = bs.tournaments[
            bs.numTournaments
        ];
        newTournament.id = bs.numTournaments;
        newTournament.name = name;
        newTournament.status = LibProde.TournamentStatus.PENDING;
        for (uint256 i = 0; i < matches.length; i++) {
            if (i > 0)
                require(
                    matches[i - 1].date <= matches[i].date,
                    "Every match should start at the same time or after the previous match"
                );
            matches[i].id = i + 1;
            newTournament.matches.push(matches[i]);
        }
        newTournament.betPrice = betPrice;
        newTournament.pot = pot;
        newTournament.allowMultipleBets = allowMultipleBets;
        newTournament.league = league;
        newTournament.code = code;

        createdTournament = bs.tournaments[bs.numTournaments];
        emit TournamentCreated(createdTournament);
        // End transaction
    }

    function getAllTournaments()
        external
        view
        returns (LibProde.TournamentDTO[] memory result)
    {
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        mapping(uint256 => LibProde.Tournament) storage currentTournaments = bs
            .tournaments;
        result = new LibProde.TournamentDTO[](bs.numTournaments);
        for (uint256 i = 1; i <= bs.numTournaments; i++) {
            LibProde.TournamentDTO memory tournament;
            tournament.id = currentTournaments[i].id;
            tournament.name = currentTournaments[i].name;
            tournament.betPrice = currentTournaments[i].betPrice;
            tournament.betsQuantity = bs.tournamentBets[tournament.id].length;
            tournament.pot = currentTournaments[i].pot;
            tournament.finishedMatches = currentTournaments[i].finishedMatches;
            tournament.status = currentTournaments[i].status;
            tournament.allowMultipleBets = currentTournaments[i]
                .allowMultipleBets;
            tournament.league = currentTournaments[i].league;
            tournament.code = currentTournaments[i].code;
            tournament.dateFinished = currentTournaments[i].dateFinished;
            result[i - 1] = tournament;
        }
    }

    function getTournament(
        uint256 tournamentId
    ) external view returns (LibProde.Tournament memory tournament) {
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        require(
            tournamentId > 0 && tournamentId <= bs.numTournaments,
            "Tournament doesn't exist"
        );
        mapping(uint256 => LibProde.Tournament) storage currentTournaments = bs
            .tournaments;
        tournament = currentTournaments[tournamentId];
        tournament.betsQuantity = bs.tournamentBets[tournamentId].length;
    }

    function placeBet(
        uint256 tournamentId,
        uint256 userId,
        LibProde.Forecast[] memory forecasts,
        address erc20address
    ) external payable nonReentrancy returns (LibProde.Bet memory newBet) {
        erc20address = LibProde.getStableAddress(erc20address);
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        require(
            tournamentId > 0 && tournamentId <= bs.numTournaments,
            "Tournament id doesn't exist"
        );
        LibProde.Tournament storage tournament = bs.tournaments[tournamentId];
        require(
            tournament.status != LibProde.TournamentStatus.FINISHED,
            "Tournament has already finished"
        );
        // Date is in ms and block.timestamp in seconds
        require(
            tournament.matches[0].date > block.timestamp * 1000,
            "Tournament has already started. Wait for the next one ;)"
        );
        ERC20 ierc20 = ERC20(erc20address);
        uint256 amountToPay = tournament.betPrice;
        if (amountToPay > 0) {
            uint256 balance = ierc20.balanceOf(msg.sender);
            require(
                balance >= amountToPay,
                "Insufficient balance to place your bet"
            );
        }
        require(
            tournament.matches.length == forecasts.length,
            "Your bet doesn't have the exact quantity of matches the tournament has"
        );
        require(
            tournament.allowMultipleBets ||
                bs.bets[msg.sender][tournamentId].length == 0,
            "You have already made your bet in this tournament."
        );

        // Save new bet
        LibProde.Bet[] storage tournamentBets = bs.tournamentBets[tournamentId];
        LibProde.Bet storage _bet = tournamentBets.push();
        _bet.id = tournamentBets.length;
        _bet.tournamentId = tournamentId;
        _bet.owner = msg.sender;
        _bet.userId = userId;
        _bet.amountPayed = tournament.betPrice;
        _bet.timestamp = block.timestamp;
        for (uint256 i = 0; i < forecasts.length; i++) {
            require(
                forecasts[i].matchId == tournament.matches[i].id,
                "Tournament match id is different from match id provided in forecast"
            );
            require(
                forecasts[i].team1 == tournament.matches[i].team1 &&
                    forecasts[i].team2 == tournament.matches[i].team2,
                "Forecast teams doesn't match tournament teams"
            );
            require(
                forecasts[i].goalsTeam1 >= 0 && forecasts[i].goalsTeam2 >= 0,
                "Scores should be positive numbers or zero"
            );
            _bet.forecasts.push(forecasts[i]);
        }
        // Transfer the bet price to the contract
        if (amountToPay > 0) {
            console.log(
                "Sending ",
                amountToPay,
                ierc20.symbol(),
                address(this)
            );
            ierc20.transferFrom(msg.sender, address(this), amountToPay);
        }

        // Add payed bet to tournament's pot
        tournament.pot += tournament.betPrice;
        newBet = tournamentBets[tournamentBets.length - 1];

        // Add bet to tournament's bets
        mapping(uint256 => uint256[]) storage gamblerTournamentBets = bs.bets[
            msg.sender
        ];
        gamblerTournamentBets[tournamentId].push(_bet.id);
        // If userId is > 0 it's a fiat payer
        if (userId > 0) {
            mapping(uint256 => uint256[]) storage betsByUserId = bs
                .betsByUserId[userId];
            betsByUserId[tournamentId].push(_bet.id);
        }
    }

    function setResults(
        uint256 tournamentId,
        LibProde.Match[] memory matchesToUpdate
    ) external onlyOwner {
        require(matchesToUpdate.length > 0, "Provide at least one match");
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        mapping(uint256 => LibProde.Tournament) storage tournaments = bs
            .tournaments;
        require(
            tournamentId > 0 && tournamentId <= bs.numTournaments,
            "The tournament doesn't exist"
        );
        LibProde.Tournament storage tournament = tournaments[tournamentId];
        LibProde.Match[] storage matches = tournament.matches;
        for (uint256 i = 0; i < matchesToUpdate.length; i++) {
            LibProde.Match storage matchToUpdate = matches[
                matchesToUpdate[i].id - 1
            ];
            require(
                matchToUpdate.id == matchesToUpdate[i].id,
                "Tournament match id is different from match id provided. Can't update"
            );
            require(
                matchToUpdate.team1 == matchesToUpdate[i].team1 &&
                    matchToUpdate.team2 == matchesToUpdate[i].team2,
                "Tournament match teams are different from match teams provided. Can't update"
            );

            require(
                matchToUpdate.status != LibProde.MatchStatus.FINISHED,
                "You can't set the result of a finished match"
            );
            matchToUpdate.goalsTeam1 = matchesToUpdate[i].goalsTeam1;
            matchToUpdate.goalsTeam2 = matchesToUpdate[i].goalsTeam2;
            matchToUpdate.status = LibProde.MatchStatus.FINISHED;
            tournament.finishedMatches++;
        }
        //  updateScore(tournament, matchesToUpdate);
        // TODO estoy arrancando el torneo cuando se actualiza el resultado del primer partido, esta mal :)
        if (tournament.status == LibProde.TournamentStatus.PENDING)
            tournament.status = LibProde.TournamentStatus.STARTED;
        // if (tournament.finishedMatches == tournament.matches.length) endTournament(tournament);
    }

    function endTournament(uint256 tournamentId) external onlyOwner {
        LibProde.Tournament storage tournament = _getTournament(tournamentId);
        require(
            tournament.finishedMatches == tournament.matches.length,
            "There are matches still not finalized"
        );
        tournament.status = LibProde.TournamentStatus.FINISHED;
        tournament.dateFinished = block.timestamp;
    }

    // Actualiza el arreglo de apuestas ganadoras a partir de una apuesta
    function updateWinnerBets(
        LibProde.WinnerBet[] storage winnerBets,
        uint256 id,
        address owner,
        uint256 userId,
        uint256 betTimestamp,
        uint256 betScore
    ) internal onlyOwner {
        for (uint256 j = 0; j < winnerBets.length; j++) {
            if (
                betScore > winnerBets[j].score ||
                (betScore == winnerBets[j].score &&
                    betTimestamp < winnerBets[j].timestamp)
            ) {
                // current bet replaces current winner bet. Shift the array and replace
                for (uint256 k = winnerBets.length - 1; k > j; k--) {
                    winnerBets[k] = winnerBets[k - 1];
                }
                winnerBets[j].id = id;
                winnerBets[j].owner = owner;
                winnerBets[j].userId = userId;
                winnerBets[j].timestamp = betTimestamp;
                winnerBets[j].score = betScore;
                break;
            }
        }
    }

    function calculateWinnerBets(
        uint256 tournamentId
    ) external onlyOwner returns (bool finished) {
        uint256 initialGas = gasleft();
        uint256 currentGas = gasleft();
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        LibProde.Bet[] storage bets = bs.tournamentBets[tournamentId];
        LibProde.Score[] storage tournamentBetScores = bs.tournamentBetScores[
            tournamentId
        ];
        require(
            bets.length > tournamentBetScores.length,
            "Winner bets already calculated for this tournament"
        );
        LibProde.Tournament storage tournament = bs.tournaments[tournamentId];
        LibProde.Match[] storage matches = tournament.matches;
        LibProde.WinnerBet[] storage winnerBets = tournament.winnerBets;
        // Si es la primera vez que ejecuto inicializo el array de winners
        if (tournamentBetScores.length == 0)
            for (uint256 i = 0; i < WINNERS; i++)
                tournament.winnerBets.push(
                    LibProde.WinnerBet(
                        0,
                        0x0000000000000000000000000000000000000000,
                        0,
                        0,
                        0,
                        0,
                        false
                    )
                );

        console.log("getWinnerBets.1 gas usado: ", currentGas - gasleft());
        currentGas = gasleft();
        // Calculo el puntaje de cada apuesta mientras haya gas
        for (uint256 i = tournamentBetScores.length; i < bets.length; i++) {
            currentGas = gasleft();
            // Si queda menos del 20% del gas inicial no sigo. TODO: puse el 20% a ojo, seguro se puede definir mejor.
            if (currentGas < (initialGas * 20) / 100) break;
            (
                LibProde.Score memory betScore,
                LibProde.Score[] memory forecastScores
            ) = getBetScore(matches, bets[i], false);
            console.log(
                "getWinnerBets.4 gas usado para calcular score del bet: ",
                i,
                " => ",
                currentGas - gasleft()
            );
            updateWinnerBets(
                winnerBets,
                i + 1,
                bets[i].owner,
                bets[i].userId,
                bets[i].timestamp,
                betScore.score
            );
            currentGas = gasleft();
            tournamentBetScores.push(betScore);
            console.log("Gas usado para escribir =>", currentGas - gasleft());
        }
        console.log(
            "Bets totales =>",
            bets.length,
            "Bets procesadas =>",
            tournamentBetScores.length
        );
        finished = tournamentBetScores.length == bets.length;
        if (finished) {
            // Calculamos nuestro fee
            uint256 tournamentFee = (tournament.pot * TOURNAMENT_FEE) / 100;
            uint256 realPot = tournament.pot - tournamentFee;
            // TODO configurable x torneo?
            uint8[3] memory prizes = [50, 30, 20];
            for (uint256 i = 0; i < WINNERS; i++) {
                uint256 prize = (realPot * prizes[i]) / 100;
                winnerBets[i].prize = prize;
                winnerBets[i].claimed = false;
            }
            bs.unclaimedFees += tournamentFee;
        }
        emit WinnerBetsCalculated(tournamentId);
    }

    function getBetScore(
        LibProde.Match[] storage matches,
        LibProde.Bet storage bet,
        bool includeForecasts
    )
        internal
        view
        returns (
            LibProde.Score memory betScore,
            LibProde.Score[] memory forecastScores
        )
    {
        if (includeForecasts)
            forecastScores = new LibProde.Score[](bet.forecasts.length);
        for (uint256 j = 0; j < matches.length; j++) {
            LibProde.Match storage _match = matches[j];
            if (_match.status != LibProde.MatchStatus.FINISHED) break;
            LibProde.Forecast storage forecast = bet.forecasts[_match.id - 1];
            LibProde.MatchResult matchResult = getResult(
                _match.goalsTeam1,
                _match.goalsTeam2
            );
            LibProde.MatchResult forecastResult = getResult(
                forecast.goalsTeam1,
                forecast.goalsTeam2
            );

            bool perfect = true;
            // L/D/V?
            if (matchResult == forecastResult) {
                betScore.resultPoints += MATCH_RESULT_POINTS;
                if (includeForecasts)
                    forecastScores[j].resultPoints += MATCH_RESULT_POINTS;
            } else perfect = false;

            // Goals?
            if (_match.goalsTeam1 == forecast.goalsTeam1) {
                betScore.goalsPoints += GOALS_POINTS;
                if (includeForecasts)
                    forecastScores[j].goalsPoints += GOALS_POINTS;
            } else perfect = false;

            if (_match.goalsTeam2 == forecast.goalsTeam2) {
                betScore.goalsPoints += GOALS_POINTS;
                if (includeForecasts)
                    forecastScores[j].goalsPoints += GOALS_POINTS;
            } else perfect = false;

            if (perfect) {
                betScore.bonusPoints += BONUS_POINTS;
                if (includeForecasts)
                    forecastScores[j].bonusPoints += BONUS_POINTS;
            }
            if (includeForecasts)
                forecastScores[j].score =
                    forecastScores[j].resultPoints +
                    forecastScores[j].goalsPoints +
                    forecastScores[j].bonusPoints;
        }
        betScore.score =
            betScore.resultPoints +
            betScore.goalsPoints +
            betScore.bonusPoints;
    }

    function getResult(
        uint256 goalsTeam1,
        uint256 goalsTeam2
    ) internal pure returns (LibProde.MatchResult) {
        if (goalsTeam1 > goalsTeam2) return LibProde.MatchResult.LOCAL;
        if (goalsTeam2 > goalsTeam1) return LibProde.MatchResult.VISITOR;
        return LibProde.MatchResult.DRAW;
    }

    function getBet(
        uint256 tournamentId,
        uint256 betId
    ) external view returns (LibProde.BetDTO memory _bet) {
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        require(
            tournamentId > 0 && tournamentId <= bs.numTournaments,
            "Tournament doesn't exist"
        );
        LibProde.Bet[] storage bets = bs.tournamentBets[tournamentId];
        require(betId > 0 && betId <= bets.length, "Bet doesn't exist");
        LibProde.Bet storage bet = bets[betId - 1];
        LibProde.Tournament storage tournament = bs.tournaments[tournamentId];
        LibProde.Match[] storage matches = tournament.matches;
        (
            LibProde.Score memory betScore,
            LibProde.Score[] memory forecastScores
        ) = getBetScore(matches, bet, true);

        // Bet
        _bet.id = bet.id;
        _bet.owner = bet.owner;
        _bet.userId = bet.userId;
        _bet.timestamp = bet.timestamp;
        _bet.forecastScores = forecastScores;
        _bet.forecasts = bet.forecasts;
        // Score
        _bet.resultPoints = betScore.resultPoints;
        _bet.goalsPoints = betScore.goalsPoints;
        _bet.bonusPoints = betScore.bonusPoints;
        _bet.score = betScore.score;
    }
}

