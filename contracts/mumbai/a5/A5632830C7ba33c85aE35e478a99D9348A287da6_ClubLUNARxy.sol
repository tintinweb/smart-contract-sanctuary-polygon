        require(_newAddress != address(0), "CAN'T PUT 0 ADDRESS");
        tokenAddresses[token] = _newAddress;
    }

    function setMintAddressToken(address _newAddress) external onlyOwner {
        require(_newAddress != address(0), "CAN'T PUT 0 ADDRESS");
