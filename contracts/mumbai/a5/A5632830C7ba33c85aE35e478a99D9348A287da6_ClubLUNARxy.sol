contract ClubLUNARxy is ERC721AQueryable, Ownable, ReentrancyGuard {
    using Strings for uint256;
    using ECDSA for bytes32;

    //@dev Consts
    uint256 public constant NORMAL_CREW = 1;
    uint256 public constant CO_FOUNDER_CREW = 2;

    //@dev adjust these values for your collection
    uint public maxSupply = 100000000000000000;

    mapping(address => uint) public nonces;

    mapping(address => bool) approvedReceiver;
    bool tradingEnabled = false;

    /*@dev Reference Address to Compare ECDSA Signature
    Fill this in with your own WL Address
    To learn more about signatures check out
    https://docs.ethers.io/v5/api/signer/#:~:text=A%20Signer%20in%20ethers%20is,on%20the%20sub%2Dclass%20used.*/
    // TODO - Change this to your PK addresses
    address private mintAddressToken = 0x342496937C3c0879A54415165C13ee59A3868066;
    address private mintAddressFiat = 0x342496937C3c0879A54415165C13ee59A3868066;

    // TODO - Change this to your payout address
    address public payoutAddress = 0xA6ac35A957E363Bcea584c3C17a508C8B5BB2A21;
    address public productAddress = 0xA6ac35A957E363Bcea584c3C17a508C8B5BB2A21;

    // TODO -
    address public spaceExplorersAddress = 0x342496937C3c0879A54415165C13ee59A3868066;
    address public cosmicCrewAddress = 0x342496937C3c0879A54415165C13ee59A3868066;
    address public swissEaglesAddress = 0x342496937C3c0879A54415165C13ee59A3868066;

    mapping(address => uint) public membershipDate;

    mapping(string => address) public tokenAddresses;

    mapping(string => uint) public referalRewards;
    mapping(string => uint) public referalRewardsClaimed;

    /* @dev Used in TokenURI Function for exchanges.
        For more information about this standard check out
        https://docs.opensea.io/docs/metadata-standards
    */
    string public baseURI = "https://api.lunarxy.io/v1/nft/metadata/";
    string public uriSuffix = ".json";

    uint public projectWalletPercent = 6000;
    uint public productPercent = 2500;
    uint public referal1Percent = 1000;
    uint public referal2Percent = 500;

    uint public tier1Amount = 1;
    uint public tier2Amount = 5;
    uint public tier3Amount = 100;

    mapping(address => uint) public tierAddress;

    bool saleStarted = true;

    // EVENTS
    event MINT(string token, address indexed wallet, uint amount, uint payout);
    event MINT_WITHOUT_ETH(address indexed wallet, uint amount);

    constructor()
    ERC721A("ClubLUNARxy", "CLXY")
    {
        tokenAddresses["USDT"] = 0xE60b4600626acb6DF5d279d575ffB44156356599;
        tokenAddresses["USDC"] = 0xE60b4600626acb6DF5d279d575ffB44156356599;
        tokenAddresses["DAI"] = 0xE60b4600626acb6DF5d279d575ffB44156356599;
        tokenAddresses["BUSD"] = 0xE60b4600626acb6DF5d279d575ffB44156356599;
    }

    function _startTokenId() internal view override returns (uint256) {
        return 1;
    }

    //SIGNATURE VERIFICATION

    /*@dev helper function for WL sale
        returns true if reference address and signature match
        false otherwise
        Read more about ECDSA @openzeppelin https://docs.openzeppelin.com/contracts/2.x/utilities
    */
    function verifyAddressSigner(
        address referenceAddress,
        bytes32 messageHash,
        bytes memory signature
    ) internal pure returns (bool) {
        return
            referenceAddress ==
            messageHash.toEthSignedMessageHash().recover(signature);
    }

    // @dev, helper hash function for WL Mint
    function hashMessage(uint number, uint nonce, address sender) private pure returns (bytes32)
    {
        return keccak256(abi.encodePacked(number, nonce, sender));
    }
    // @dev, helper hash function for WL Mint
    function hashMessagePrice(uint number, string calldata token, uint price, uint nonce, address referal1, address referal2, string calldata key1, string calldata key2, address sender) private pure returns (bytes32)
    {
        return keccak256(abi.encodePacked(number, token, price, nonce, referal1, referal2, key1, key2, sender));
    }

    //END SIGNATURE VERIFICATION

    /* MINTING */
    //@dev The Max Someone Can Mint is Encoded In The Signature. Be careful
    function clubMintToken(string calldata token, uint amount, uint price, address referal1, address referal2, string calldata key_referal1, string calldata key_referal2, uint nonce, bytes memory signature) external {
        if (!saleStarted) revert SaleNotStarted();
        if (nonce < nonces[msg.sender] + 1) revert NonceTooLow();
        if (totalSupply() + amount > maxSupply) revert SoldOut();
        if (!verifyAddressSigner(mintAddressToken, hashMessagePrice(amount, token, price, nonce, referal1, referal2, key_referal1, key_referal2, msg.sender), signature)) revert NotAuth();

        uint total_price = price;
        {
            IERC20 _tokenContract = IERC20(tokenAddresses[token]);
            uint tokens_allowance = _tokenContract.allowance(msg.sender, address(this));
            if (tokens_allowance < total_price) revert ValueTooLow();


            _tokenContract.transferFrom(msg.sender, address(this), total_price);

            // transfer to project wallet
            uint projectAmount = total_price * projectWalletPercent / 10000;
            _tokenContract.transfer(payoutAddress, projectAmount);

            // transfer to product wallet
            uint productAmount = total_price * productPercent / 10000;
            _tokenContract.transfer(productAddress, productAmount);
        }

        // transfer to referal wallet
        _transferToReferalWallet(token, key_referal1, key_referal2, referal1, referal2, total_price);

        _mintAmountTiers(amount);

        nonces[msg.sender] = nonce;
        emit MINT(token, msg.sender, amount, total_price);

    }

    function _mintAmountTiers(uint amount) private {
        CosmicCrew cc = CosmicCrew(cosmicCrewAddress);
        SpaceExplorers se = SpaceExplorers(spaceExplorersAddress);

        if (amount == tier1Amount) {
            tierAddress[msg.sender] = 1;

            se.mint(msg.sender, amount);
            _mint(msg.sender, amount);

        } else if (amount == tier2Amount) {
            tierAddress[msg.sender] = 2;

            se.mint(msg.sender, amount);
            cc.mint(msg.sender, 1, NORMAL_CREW);

            _mint(msg.sender, amount);

        } else if (amount == tier3Amount) {
            tierAddress[msg.sender] = 3;

            se.mint(msg.sender, amount);
            cc.mint(msg.sender, 1, CO_FOUNDER_CREW);

            _mint(msg.sender, amount);
        }

        membershipDate[msg.sender] = block.timestamp;

        SwissEagles swe = SwissEagles(swissEaglesAddress);
        swe.addNftsToClaimToWallet(msg.sender, amount);
    }

    function _transferToReferalWallet(string calldata token, string calldata key1, string calldata key2, address referal1, address referal2, uint total_price) private {
        if (referal1 != address(0) && referal2 == address(0)) {
            uint referal1Amount = total_price * (referal1Percent + referal2Percent) / 10000;
            referalRewards[key1] += referal1Amount;
        }
        else if (referal1 != address(0) && referal2 != address(0)) {

            uint referal1Amount = total_price * referal1Percent / 10000;
            referalRewards[key1] += referal1Amount;

            uint referal2Amount = total_price * referal2Percent / 10000;
            referalRewards[key2] += referal2Amount;
        }


    }

    //@dev minting pay offchain
    function clubMintWithoutEth(uint amount, uint nonce, bytes memory signature) external {
        if (!saleStarted) revert SaleNotStarted();
        if (nonce < nonces[msg.sender] + 1) revert NonceTooLow();
        if (totalSupply() + amount > maxSupply) revert SoldOut();
        if (!verifyAddressSigner(mintAddressFiat, hashMessage(amount, nonce, msg.sender), signature)) revert NotAuth();


        if (amount == tier1Amount) {
            tierAddress[msg.sender] = 1;
        } else if (amount == tier2Amount) {
            tierAddress[msg.sender] = 2;
        } else if (amount == tier3Amount) {
            tierAddress[msg.sender] = 3;
        }
        _mint(msg.sender, amount);

        nonces[msg.sender] = nonce;

        emit MINT_WITHOUT_ETH(msg.sender, amount);
    }

    function mint(address to, uint256 amount) public onlyOwner {
        _mint(to, amount);
    }

    /* END MINTING */

    // FACTORY

    function tokenURI(uint256 tokenId)
    public
    view
    override(IERC721A, ERC721A)
    returns (string memory)
    {

        string memory currentBaseURI = baseURI;
        return
            bytes(currentBaseURI).length > 0
                ? string(abi.encodePacked(currentBaseURI, tokenId.toString(), uriSuffix))
                : "";
    }

    //Override the transferFrom function
    function transferFrom(address from, address to, uint256 tokenId) public payable override (IERC721A, ERC721A) {
        require(approvedReceiver[msg.sender] || tradingEnabled, "Trading is not live");
        super.transferFrom(from, to, tokenId);
    }

    function withdraw(string[] calldata keys, string[] calldata tokens) public onlyOwner {
        uint256 balance = address(this).balance;
        (bool r1,) = payable(owner()).call{value: balance}("");
        require(r1);

        for (uint256 i; i < keys.length; i++) {
            IERC20 _tokenContract = IERC20(tokenAddresses[tokens[i]]);
            _tokenContract.transfer(owner(), referalRewards[keys[i]]);

            referalRewardsClaimed[keys[i]] += referalRewards[keys[i]];
            referalRewards[keys[i]] = 0;
        }


    }

    // BEGIN SETTERS
    function setReceiverStatus(address receiver, bool status) public onlyOwner {
        require(receiver != address(0), "CAN'T PUT 0 ADDRESS");
        approvedReceiver[receiver] = status;
    }

