        mintAddressFiat = _newAddress;
    }

    function contractURI() public view returns (string memory) {
        return "https://club.lunarxy.com/contract-metadata.json";
    }

    function setTradingEnabled(bool status) external onlyOwner {
        tradingEnabled = status;
    }

    function setProjectWalletPercent(uint _new_percent) external onlyOwner {
        require(_new_percent <= 100, "Cant be more than 100%");
        require(_new_percent >= 0, "Cant be less than 0%");
        projectWalletPercent = _new_percent;
    }

    function setProductPercent(uint _new_percent) external onlyOwner {
        require(_new_percent <= 100, "Cant be more than 100%");
        require(_new_percent >= 0, "Cant be less than 0%");
        productPercent = _new_percent;
    }

    function setReferal1Percent(uint _new_percent) external onlyOwner {
        require(_new_percent <= 100, "Cant be more than 100%");
        require(_new_percent >= 0, "Cant be less than 0%");
        referal1Percent = _new_percent;
    }

    function setReferal2Percent(uint _new_percent) external onlyOwner {
        require(_new_percent <= 100, "Cant be more than 100%");
        require(_new_percent >= 0, "Cant be less than 0%");
        referal2Percent = _new_percent;
    }

    function setTier1Amount(uint _new_amount) external onlyOwner {
        require(_new_amount >= 0, "Cant be less than 0");
        tier1Amount = _new_amount;
    }

    function setTier2Amount(uint _new_amount) external onlyOwner {
        require(_new_amount >= 0, "Cant be less than 0");
        require(_new_amount >= tier1Amount, "Cant be less than tier1Amount");
        tier2Amount = _new_amount;
    }

    function setTier3Amount(uint _new_amount) external onlyOwner {
        require(_new_amount >= 0, "Cant be less than 0");
        require(_new_amount >= tier2Amount, "Cant be less than tier2Amount");
        tier3Amount = _new_amount;
    }

    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        baseURI = _newBaseURI;
    }

    function setPayoutAddress(address _newAddress) external onlyOwner {
        require(_newAddress != address(0), "CAN'T PUT 0 ADDRESS");
        payoutAddress = _newAddress;
    }

    function setProductAddress(address _newAddress) external onlyOwner {
        require(_newAddress != address(0), "CAN'T PUT 0 ADDRESS");
        productAddress = _newAddress;
    }

    function setSpaceExplorersAddress(address _newAddress) external onlyOwner {
        require(_newAddress != address(0), "CAN'T PUT 0 ADDRESS");
        spaceExplorersAddress = _newAddress;
    }

    function setCosmicCrewAddress(address _newAddress) external onlyOwner {
        require(_newAddress != address(0), "CAN'T PUT 0 ADDRESS");
        cosmicCrewAddress = _newAddress;
    }

    function setSwissEaglesAddress(address _newAddress) external onlyOwner {
        require(_newAddress != address(0), "CAN'T PUT 0 ADDRESS");
        swissEaglesAddress = _newAddress;
    }

    function setUriSuffix(string memory _newSuffix) external onlyOwner {
        uriSuffix = _newSuffix;
    }

    function setMaxSupply(uint newSupply) external onlyOwner {
        require(newSupply > totalSupply(), "Cant Increase Size Of Collection");
        maxSupply = newSupply;
    }

    function setSaleStatus(bool status) external onlyOwner {
        saleStarted = status;
    }
    //END SETTERS


}

