enum MinerType {
    SKIP_0,
    SKIP_1,
    StandardMiner,          // 2
    RemoteMiner             // 3
}

enum MinerStatus {
    Pending,            // 0
    Normal,             // 1
    Locked,             // 2
    Terminated          // 3
