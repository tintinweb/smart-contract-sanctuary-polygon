pragma abicoder v2;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@uniswap/lib/contracts/libraries/TransferHelper.sol";

import "./interfaces/IFactory.sol";
import "./interfaces/IExtraProtocol.sol";
import "./interfaces/IProtocol.sol";

import "./MixinResolver.sol";

import "hardhat/console.sol";

/**
 * @title This contact provides stability even if external protocols fail or are not available
 * at all.
 **/
contract ExtraProtocol is IExtraProtocol, MixinResolver, Ownable {
    bytes32 private constant FACTORY_CONTRACT = "Factory";
    bytes32 private constant D4X_CONTRACT = "D4X";

    // TODO: replace to interface selector using e.g: IQuoterV2.quoteExactInputSingle.selector

    /// @dev Hash of the deposit function
    bytes4 public constant DEPOSIT =
        bytes4(keccak256(bytes("deposit(uint256,address)")));

    /// @dev Hash of the withdraw function
    bytes4 public constant WITHDRAW =
        bytes4(keccak256(bytes("withdraw(uint256,address,address)")));

    /// @dev Holds a mapping of protocols to indexes of protocols in _protocols array + 1
    mapping(address => uint256) internal _protocolIndexes;

    /// @dev Holds a mapping of tokens to the total amount of shares.
    mapping(address => uint256) private _totalShares;

    /// @dev Holds a mapping of positions to the amount of shares.
    mapping(uint256 => uint256) private _sharesPosition;

    /// @dev Holds a mapping of tokens to the amount of shares.
    mapping(address => uint256) private _sharesAddress;

    /// @dev Holds a mapping of positionIds to positions
    mapping(uint256 => PositionParams) internal _positions;

    // /// @dev Holds a mapping of address balances
    // mapping(address => uint256) _alpBalances;

    /// @dev Array of protocols, where index equals priority
    address[] _protocols;

    IFactory public _factory;
    address public _d4x;

    struct PositionParams {
        address protocol;
        address token;
        uint256 amount;
    }

    constructor(address resolver) MixinResolver(resolver) {}

    function resolverAddressesRequired()
        public
        pure
        override
        returns (bytes32[] memory addresses)
    {
        addresses = new bytes32[](2);
        addresses[0] = D4X_CONTRACT;
        addresses[1] = FACTORY_CONTRACT;
    }

    function _resolveAddressesFinish() internal override {
        _factory = IFactory(requireAndGetAddress(FACTORY_CONTRACT));
        _d4x = requireAndGetAddress(D4X_CONTRACT);
    }

    /**
     * @notice Adding external protocols that implements IProtocol interface
     * @param protocols Array of external protocols address
     */

    function setProtocols(address[] calldata protocols) external onlyOwner {
        _protocols = new address[](protocols.length);

        for (uint8 i = 0; i < protocols.length; i++) {
            _protocols[i] = protocols[i];
            _protocolIndexes[protocols[i]] = i + 1;
        }
    }

    /**
     * @notice Move `amount` of tokens to extra protocol by position.
     * @param token Token address to be sent
     * @param amount Amount of `token`
     * @param positionId Position ID
     * @dev In the loop, an attempt is made to make a deposit on one of the available
     * protocols. If the deposit is not made, the tokens will remain on this contract
     *
     * Requirements:
     *
     * - `amount` cannot be less then msg.sender balance.
     * - the caller must approve `token` for at least `amount`.
     *
     */
    function deposit(
        uint256 amount,
        uint256 positionId,
        address token
    ) external {
        uint256 sharesAmount = getSharesByPooled(amount, token);

        TransferHelper.safeTransferFrom(
            token,
            msg.sender,
            address(this),
            amount
        );

        address protocol;
        uint256 size = _protocols.length;

        for (uint256 i = 0; i < size; i++) {
            if (IProtocol(_protocols[i]).checkValidToken(token)) {
                TransferHelper.safeApprove(token, _protocols[i], amount);
                (bool success, ) = address(_protocols[i]).call(
                    abi.encodeWithSelector(DEPOSIT, amount, token)
                );

                if (success) {
                    protocol = _protocols[i];
                    break;
                }
            }
        }

        console.log("sharesAmount calc", sharesAmount);

        if (sharesAmount == 0) {
            // totalControlledEther is 0: either the first-ever deposit or complete slashing
            // assume that shares correspond to Ether 1-to-1
            sharesAmount = amount;
        }

        console.log("sharesAmount fact", amount, sharesAmount);

        _mintSharesPosition(positionId, token, sharesAmount);

        _positions[positionId] = PositionParams({
            protocol: protocol,
            token: token,
            amount: amount
        });
    }

    /**
     * @notice Move `amount` of tokens to extra.
     * @param token Token address to be sent
     * @param amount Amount of tokens
     * @dev In the loop, an attempt is made to make a deposit on one of the available
     * protocols. If the deposit is not made, the tokens will remain on this contract
     *
     * Requirements:
     *
     * - `amount` cannot be less then msg.sender balance.
     * - the caller must approve `token` for at least `amount`.
     *
     */
    function deposit(uint256 amount, address token) external {
        uint256 sharesAmount = getSharesByPooled(amount, token);

        TransferHelper.safeTransferFrom(
            token,
            msg.sender,
            address(this),
            amount
        );

        uint256 size = _protocols.length;

        for (uint256 i = 0; i < size; i++) {
            if (IProtocol(_protocols[i]).checkValidToken(token)) {
                TransferHelper.safeApprove(token, _protocols[i], amount);
                (bool success, ) = address(_protocols[i]).call(
                    abi.encodeWithSelector(DEPOSIT, amount, token)
                );

                if (success) break;
            }
        }

        console.log("sharesAmount calc", sharesAmount);

        if (sharesAmount == 0) {
            // totalControlledEther is 0: either the first-ever deposit or complete slashing
            // assume that shares correspond to Ether 1-to-1
            sharesAmount = amount;
        }

        console.log("sharesAmount fact", amount, sharesAmount);

        _mintSharesAddress(msg.sender, token, sharesAmount);
    }

    /**
     * @notice Withdraw tokens to `recipient` by `positionId`
     * @param positionId Position ID
     * @dev In the loop, an attempt is made to make a withdraw on one of the available
     * protocols. If the withdraw is not made, the tokens will be transfered from this contract
     *
     * Requirements:
     *
     * - `positionId` must exist.
     *
     */
    function withdraw(uint256 positionId) external {
        PositionParams memory position = _positions[positionId];
        require(_d4x == msg.sender, "ExtraProtocol: Caller haven't access");

        require(
            position.token != address(0),
            "ExtraProtocol: incorrect positionId"
        );

        uint256 balance = this.balanceOf(positionId);
        bool success;

        if (position.protocol != address(0)) {
            IProtocol(position.protocol).withdraw(
                balance,
                position.token,
                _d4x
            );
        }

        uint256 sharesAmount = getSharesByWithdrow(balance, position.token);

        _burnSharesPosition(positionId, position.token, sharesAmount);

        if (!success) {
            TransferHelper.safeTransfer(position.token, msg.sender, balance);
        }
    }

    /**
     * @notice Withdraw tokens to `recipient`
     * @param token Token address to withdraw
     * @param amount Amount of `token`
     * @dev In the loop, an attempt is made to make a withdraw on one of the available
     * protocols. If the withdraw is not made, the tokens will be transfered from this contract
     */
    function withdraw(
        uint256 amount,
        address token,
        address recipient
    ) external {
        require(
            msg.sender == _factory.getAlp(token),
            "ExtraProtocol: Caller haven't access"
        );

        uint256 balance = this.balanceOf(token, msg.sender);
        uint256 size = _protocols.length;
        bool needTransfer = true;

        for (uint256 i = 0; i < size; i++) {
            if (IProtocol(_protocols[i]).checkValidToken(token)) {
                (bool success, ) = address(_protocols[i]).call(
                    abi.encodeWithSelector(WITHDRAW, amount, token, recipient)
                );

                if (success) {
                    needTransfer = false;
                    break;
                }
            }
        }

        uint256 sharesAmount = getSharesByWithdrow(amount, token);

        _burnSharesAddress(msg.sender, token, sharesAmount);

        if (needTransfer) {
            require(amount <= balance, "ExtraProtocol: not enogth balance");

            TransferHelper.safeTransfer(token, recipient, amount);
        }
    }

    /**
     * @return balance of the token by `positionId`
     *
     * Requirements:
     *
     * - `positionId` must exist.
     *
     */
    function balanceOf(uint256 positionId) external view returns (uint256) {
        PositionParams memory position = _positions[positionId];

        if (position.token == address(0)) return 0;

        return getPooledByShares(_sharesPosition[positionId], position.token);
    }

    /**
     * @return balance The sum of balances of the `token` of all protocols
     */
    function balanceOf(address token) external view returns (uint256 balance) {
        return _getTotalPooled(token);
    }

    /**
     * @return balance The sum of balances of the `token` of all protocols
     */
    function balanceOf(
        address token,
        address alp
    ) external view returns (uint256 balance) {
        return getPooledByShares(_sharesAddress[alp], token);
    }

    /***************************SHARES ***************************/

    /**
     * @return shares of `token` that corresponds to `_amount` token.
     */
    function getSharesByPooled(
        uint256 _amount,
        address token
    ) public view returns (uint256) {
        uint256 totalPooled = _getTotalPooled(token);

        console.log("totalPooled", totalPooled);

        if (totalPooled == 0) {
            return 0;
        } else {
            return (_amount * _totalShares[token]) / totalPooled;
        }
    }

    /**
     * @return amount of `token` that corresponds to `value` token shares.
     */
    function getPooledByShares(
        uint256 value,
        address token
    ) public view returns (uint256 amount) {
        uint256 totalShares = _totalShares[token];
        if (totalShares == 0) {
            return 0;
        } else {
            return (value * _getTotalPooled(token)) / totalShares;
        }
    }

    /**
     * @return shares of `token` that corresponds to `_amount` token.
     */
    function getSharesByWithdrow(
        uint256 _amount,
        address token
    ) public view returns (uint256) {
        uint256 totalPooled = _getTotalPooled(token);
        if (totalPooled == 0) {
            return 0;
        } else {
            uint256 m = _amount * _totalShares[token];
            uint256 res = m / totalPooled;

            return res + ((totalPooled * res) == m ? 0 : 1);
        }
    }

    /**
     * @return the entire amount of `token` controlled by the protocol.
     */
    function _getTotalPooled(address token) internal view returns (uint256) {
        uint256 balance = IERC20(token).balanceOf(address(this));
        uint256 size = _protocols.length;

        for (uint256 i = 0; i < size; i++) {
            balance += IProtocol(_protocols[i]).balanceOf(token);
        }

        return balance;
    }

    /**
     * @notice Assigns `sharesAmount` to `token`, increasing the total amount of shares by `id`
     */
    function _mintSharesPosition(
        uint256 id,
        address token,
        uint256 sharesAmount
    ) internal {
        _totalShares[token] += sharesAmount;
        _sharesPosition[id] += sharesAmount;
    }

    /**
     * @notice Assigns `sharesAmount` to `token`, increasing the total amount of shares
     */
    function _mintSharesAddress(
        address recipient,
        address token,
        uint256 sharesAmount
    ) internal {
        _totalShares[token] += sharesAmount;
        _sharesAddress[recipient] += sharesAmount;
    }

    /**
     * @notice Destroys `_sharesAmount` shares from `token`'s holdings,
     * decreasing the total amount of shares by `id`
     * @dev This doesn't decrease the token total supply.
     */
    function _burnSharesPosition(
        uint256 id,
        address token,
        uint256 sharesAmount
    ) internal {
        _totalShares[token] -= sharesAmount;
        _sharesPosition[id] -= sharesAmount;
    }

    /**
     * @notice Destroys `sharesAmount` shares from `token`'s holdings,
     * decreasing the total amount of shares.
     * @dev This doesn't decrease the token total supply.
     */
    function _burnSharesAddress(
        address recipient,
        address token,
        uint256 sharesAmount
    ) internal {
        _totalShares[token] -= sharesAmount;
        _sharesAddress[recipient] -= sharesAmount;
    }
}

