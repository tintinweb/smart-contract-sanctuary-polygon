contract ClaimRewardFeature is FixinAccessControl {

    enum RewardType {
        NATIVE, // 0, native currency
        ERC20,
        MB // 2, mystery box
    }

    address public treasury;

    constructor(address _treasury) {
        treasury = _treasury;
        transferOwnership(_treasury);
    }

    function updateTreasury(address _treasury) external onlyRole(GOVERNOR_ROLE) {
         treasury = _treasury;
    }

    // solhint-disable-next-line no-empty-blocks
    function distribute(address to, RewardType[] memory tokenTypes, address[] memory rewardTokens, uint256[] memory rewardAmounts) external onlyRole(GOVERNOR_ROLE) {
        require(tokenTypes.length == rewardTokens.length, "TOKEN_TYPE_ADDRESS_MISMATCH");
        require(rewardAmounts.length == rewardTokens.length, "TOKEN_ADDRESS_AMOUNT_MISMATCH");
        for (uint i = 0; i < tokenTypes.length; i += 1) {
            require(rewardTokens[i] != address(0), "INVALID_REWARD_ADDRESS");
            if  (tokenTypes[i] == RewardType.NATIVE) {
                (bool success, ) = to.call{value: rewardAmounts[i]}("");
                require(success, "FAILED_TO_TRANFER_NATIVE_CURRENCY");
            } else if (tokenTypes[i] == RewardType.ERC20) {
                IERC20(rewardTokens[i]).transferFrom(treasury, to, rewardAmounts[i]);
            } else if (tokenTypes[i] == RewardType.MB ) {
                IMysteryBox(rewardTokens[i]).mintBatch(to, rewardAmounts[i]);
            }
        }
    }
}