pragma solidity 0.8.17;

import "./Governed.sol";
import "./interfaces/IRegistry.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "hardhat-deploy/solc_0.8/proxy/Proxied.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

error ForceNFTRegistryThisNameIsNotEmpty(address project);
error ForceNFTRegistryAddressIsZero();
error ForceNFTRegistryProjectRemovalFailed();
error ForceNFTRegistryProjectAdditionFailed();

contract Registry is Initializable, Proxied, Governed, IRegistry {
    using EnumerableSet for EnumerableSet.UintSet;

    bytes32 public constant MINTER_ROLE = 0x9f2df0fed2c77648de5860a4cc508cd0818c85b8b8a1ab4ceeef8d981c8956a6;
    bytes32 public constant BURNER_ROLE = 0x3c11d16cbaffd01df69ce1c404f6340ee057498f5f00246190ea54220576a848;
    address public override getMetaCore;
    address public override getForcePool;
    mapping(uint256 => address) public override getForceNFT;
    address public override getControlContract;
    address public override getForceRewardCoin;
    address public override getInternalEnergyCoin;
    mapping(string => uint256) public override metaProjectId;
    mapping(uint256 => string) public override metaProjectName;
    mapping(uint256 => address) public metaProject;
    mapping(uint256 => address) public energyToken;
    uint256 public index;
    uint256 public override getMaxLvlNFT;
    EnumerableSet.UintSet internal activeProjects;

    modifier notZeroAddress(address setAddress) {
        if (setAddress == address(0)) {
            revert ForceNFTRegistryAddressIsZero();
        }
        _;
    }

    constructor() {}

    function setMetaCore(address _metaCore) external override onlyGovernor notZeroAddress(_metaCore) {
        getMetaCore = _metaCore;
    }

    function setForceNFT(uint256 level, address _forceNFT) external override onlyGovernor notZeroAddress(_forceNFT) {
        getForceNFT[level] = _forceNFT;
    }

    function setForcePool(address _forcePool) external override onlyGovernor notZeroAddress(_forcePool) {
        getForcePool = _forcePool;
    }

    function setForceRewardCoin(address _forceCoin) external override onlyGovernor notZeroAddress(_forceCoin) {
        getForceRewardCoin = _forceCoin;
    }

    function setInternalEnergyCoin(address _internalEnergyCoin)
        external
        override
        onlyGovernor
        notZeroAddress(_internalEnergyCoin)
    {
        getInternalEnergyCoin = _internalEnergyCoin;
    }

    function setMetaProject(
        string memory _name,
        address _metaProject,
        address _energyCoin
    ) external override onlyGovernor {
        if (checkName(_name)) {
            revert ForceNFTRegistryThisNameIsNotEmpty(metaProject[metaProjectId[_name]]);
        }
        if (_metaProject == address(0x0) || _energyCoin == address(0x0)) {
            revert ForceNFTRegistryAddressIsZero();
        }
        metaProjectId[_name] = index;
        metaProjectName[index] = _name;
        metaProject[index] = _metaProject;
        energyToken[index] = _energyCoin;
        if (!activeProjects.add(index)) {
            revert ForceNFTRegistryProjectAdditionFailed();
        }
        unchecked {
            ++index;
        }
    }

    function setControlContract(address _control) external override onlyGovernor notZeroAddress(_control) {
        uint256 maxLvlNFT = getMaxLvlNFT;
        for (uint256 i; i <= maxLvlNFT; ) {
            AccessControl(getForceNFT[i]).revokeRole(MINTER_ROLE, getControlContract);
            AccessControl(getForceNFT[i]).revokeRole(BURNER_ROLE, getControlContract);
            AccessControl(getForceNFT[i]).grantRole(MINTER_ROLE, _control);
            AccessControl(getForceNFT[i]).grantRole(BURNER_ROLE, _control);
            unchecked {
                ++i;
            }
        }
        getControlContract = _control;
    }

    function deleteMetaProject(string memory _name) external override onlyGovernor {
        metaProject[metaProjectId[_name]] = address(0x0);
        energyToken[metaProjectId[_name]] = address(0x0);
        if (!activeProjects.remove(metaProjectId[_name])) {
            revert ForceNFTRegistryProjectRemovalFailed();
        }
    }

    function deleteMetaProject(uint256 id) external override onlyGovernor {
        metaProject[id] = address(0x0);
        energyToken[id] = address(0x0);
        if (!activeProjects.remove(id)) {
            revert ForceNFTRegistryProjectRemovalFailed();
        }
    }

    function setMaxLevelNFT(uint256 lvl) external override onlyGovernor {
        getMaxLvlNFT = lvl;
    }

    function getMetaProject(string calldata _nameMetaProject) external view override returns (address) {
        return metaProject[metaProjectId[_nameMetaProject]];
    }

    function getEnergyToken(string calldata _nameMetaProject) external view override returns (address) {
        return energyToken[metaProjectId[_nameMetaProject]];
    }

    function getMetaProject(uint256 id) external view override returns (address) {
        return metaProject[activeProjects.at(id)];
    }

    function getEnergyToken(uint256 id) external view override returns (address) {
        return energyToken[activeProjects.at(id)];
    }

    function getNameProject(uint256 id) external view override returns (string memory name) {
        return metaProjectName[activeProjects.at(id)];
    }

    function getAmountMetaProject() external view override returns (uint256) {
        return activeProjects.length();
    }

    function getIndexProject(uint256 num) external view override returns (uint256) {
        return activeProjects.at(num);
    }

    function getAllIndexes() external view override returns (uint256[] memory) {
        return activeProjects.values();
    }

    function initialize() public proxied initializer {
        setGovernor(msg.sender);
        index = 1;
        getMaxLvlNFT = 7;
    }

    function checkName(string memory _name) public view returns (bool) {
        return activeProjects.contains(metaProjectId[_name]);
    }
}

