        uint256 tokenId,
        bytes calldata data
    ) external returns (bytes4) {
        from;
        data;
        ITokenBundle.Token[] memory token;
        uint256[] memory numOfRewardUnits;
        token[0] = ITokenBundle.Token(
            operator,
            ITokenBundle.TokenType.ERC721,
            tokenId,
            1
        );
        numOfRewardUnits[0] = 1;

        createOrAddToPack(token, numOfRewardUnits);
        return IERC721ReceiverUpgradeable.onERC721Received.selector;
    }

    function onERC1155Received(
        address operator,
        address from,
        uint256 id,
        uint256 value,
        bytes calldata data
    ) external returns (bytes4) {
        operator;
        from;
        bool hasData = (data.length != 0);
        ITokenBundle.Token[] memory token = new ITokenBundle.Token[](1);
        uint256[] memory numOfRewardUnits = new uint256[](1);
        numOfRewardUnits[0] = hasData ? abi.decode(data, (uint256)) : value;

        require(numOfRewardUnits[0] != 0, "!unit");
        require(value % numOfRewardUnits[0] == 0, "!mod");
        token[0] = ITokenBundle.Token(
            msg.sender,
            ITokenBundle.TokenType.ERC1155,
            id,
            value
        );


        createOrAddToPack(token, numOfRewardUnits);
        return IERC1155ReceiverUpgradeable.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(
        address operator,
        address from,
        uint256[] calldata ids,
        uint256[] calldata values,
        bytes calldata data
    ) external returns (bytes4) {
        operator;
        from;
        bool hasData = (data.length != 0);

        ITokenBundle.Token[] memory token = new ITokenBundle.Token[](ids.length);
        uint256[] memory numOfRewardUnits = hasData ? abi.decode(data, (uint256[])) : values;
        require(numOfRewardUnits.length == token.length, "!len");

        for (uint256 i = 0; i < ids.length; i++) {
            require(numOfRewardUnits[i] != 0, "!unit");
            require(values[i] % numOfRewardUnits[i] == 0, "!mod");

            token[i] = ITokenBundle.Token(
                msg.sender,
                ITokenBundle.TokenType.ERC1155,
                ids[i],
                values[i]
            );
        }

        createOrAddToPack(token, numOfRewardUnits);
        return IERC1155ReceiverUpgradeable.onERC1155BatchReceived.selector;

    }

    function approveToken(ITokenBundle.Token memory t) internal {
        if (t.tokenType == ITokenBundle.TokenType.ERC1155) {
            IERC1155Upgradeable(t.assetContract).setApprovalForAll(address(packContract), true);
            return;
        }
        if (t.tokenType == ITokenBundle.TokenType.ERC721) {
            IERC721Upgradeable(t.assetContract).approve(address(packContract), t.tokenId);
            return;
        }
    }

    function createOrAddToPack(
        ITokenBundle.Token[] memory token,
        uint256[] memory numOfRewardUnits
    ) internal {
        for (uint256 i=0; i < token.length; i++) {
            approveToken(token[i]);
        }
        if (!_isPackCreated()) {
            packContract.createPack(
                poolId,
                token,
                numOfRewardUnits,
                openStartTimestamp,
                amountDistributedPerOpen,
                address(factory)
            );
            return;
        }
        packContract.addPackContents(
            poolId,
            token,
            numOfRewardUnits,
            address(factory)
        );

    }

    function openPack(uint256 amount, address recipient) external onlyRole(OPENER_ROLE) {
        packContract.openPack(poolId, amount, recipient);
    }

}

