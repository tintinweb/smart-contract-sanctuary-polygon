contract FutabaLightClient is ILightClient, Ownable {
    struct RootData {
        bytes32 currentRoot;
        bytes result;
    }

    address public oracle;
    address public gateway;

    RootData public currentData;

    function verifyLog(
        MPTValidatorV2.MerkleProof memory receiptdata,
        uint256 logIndex,
        bytes memory logData,
        bytes memory result
    ) external {
        // TODO when implement oracle, comment out
        // require(
        //     receiptdata.root == currentData.currentRoot,
        //     "Futaba: invalid root"
        // );
        bytes memory serializedReceipt = verifyTrieProof(receiptdata);
        // EthereumDecoder.TransactionReceiptTrie memory receipt = EthereumDecoder
        //     .toReceipt(serializedReceipt);

        // EthereumDecoder.Log memory log = receipt.logs[logIndex];
        bytes memory packet = abi.decode(logData, (bytes));
        (
            address callBack,
            bytes[] memory decodedQueries,
            bytes memory message
        ) = abi.decode(packet, (address, bytes[], bytes));
        console.log(callBack);
        IGateway.Query[] memory queries = new IGateway.Query[](
            decodedQueries.length
        );
        for (uint256 i = 0; i < decodedQueries.length; i++) {
            (
                address sender,
                address receiver,
                IGateway.Query memory query
            ) = abi.decode(
                    decodedQueries[i],
                    (address, address, IGateway.Query)
                );
            queries[i] = query;
        }
        bytes memory returnedData = _verifyData(result);

        IGateway(gateway).receiveQuery(
            returnedData,
            callBack,
            queries,
            message,
            msg.sender
        );
    }

    function updateHeader(
        bytes32 rootHash,
        bytes memory result
    ) external override onlyOracle {
        currentData.currentRoot = rootHash;
        currentData.result = result;
        emit UpdateHeader(oracle, rootHash, result);
    }

    function setOracle(address _oracle) public onlyOwner {
        oracle = _oracle;
    }

    function getOracle() public view returns (address) {
        return oracle;
    }

    function setGateway(address _gateway) public onlyOwner {
        gateway = _gateway;
    }

    function getGateway() public view returns (address) {
        return gateway;
    }

    function _verifyData(
        bytes memory result
    ) internal view returns (bytes memory) {
        require(
            keccak256(result) == keccak256(currentData.result),
            "Futaba: invalid data"
        );
        return result;
    }

    function toBlockHeader(
        bytes memory data
    ) public view returns (EthereumDecoder.BlockHeader memory header) {
        return EthereumDecoder.toBlockHeader(data);
    }

    function toReceipt(
        bytes memory data
    )
        public
        view
        returns (EthereumDecoder.TransactionReceiptTrie memory receipt)
    {
        return EthereumDecoder.toReceipt(data);
    }

    function toReceiptLog(
        bytes memory data
    ) public view returns (EthereumDecoder.Log memory) {
        return EthereumDecoder.toReceiptLog(data);
    }

    function getReceiptRlpData(
        EthereumDecoder.TransactionReceiptTrie memory receipt
    ) public view returns (bytes memory data) {
        return EthereumDecoder.getReceiptRlpData(receipt);
    }

    function verifyTrieProof(
        MPTValidatorV2.MerkleProof memory data
    ) internal view returns (bytes memory) {
        return MPTValidatorV2.validateProof(data.root, data.paths, data.proof);
    }

    modifier onlyOracle() {
        require(address(oracle) == msg.sender, "Futaba: Only oracle");
        _;
    }
}

