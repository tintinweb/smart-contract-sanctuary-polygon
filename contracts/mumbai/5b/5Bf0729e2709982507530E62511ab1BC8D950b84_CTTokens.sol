pragma solidity 0.8.19;

import 'hardhat/console.sol';
import '@openzeppelin/contracts/token/ERC1155/ERC1155.sol';
import '@openzeppelin/contracts/utils/Counters.sol';
import {Proposal} from './Proposal.sol';
import {Manager} from './Manager.sol';
import {ManagerFactory} from './ManagerFactory.sol';

contract CTTokens is ERC1155 {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    address private _managerFactory;

    // Mapping from proposal address to nft token id
    mapping(address => uint) private _proposalNFTs;

    // Mapping from manager address to ft token id
    mapping(address => uint) private _managerFTs;

    event NftMinted(uint id, address proposal, address admin);
    event FtMinted(uint id, address manager, address admin, uint amount);

    constructor(address managerFactory, string memory uri_) ERC1155(uri_) {
        _managerFactory = managerFactory;
    }

    function mintProposalNFT(address pAddress) public returns (uint) {
        require(pAddress != address(0), 'Zero address');
        require(_proposalNFTs[pAddress] == 0, 'Already minted for this proposal');

        Proposal proposal = Proposal(pAddress);
        require(proposal.status() == Proposal.Status.Finished, 'Proposal has not been finished');
        require(proposal.manager().admin() == msg.sender, 'Only proposal manager admin');

        _tokenIds.increment();
        uint256 newTokenId = _tokenIds.current();
        _mint(proposal.admin(), newTokenId, 1, '');
        _proposalNFTs[pAddress] = newTokenId;

        emit NftMinted(newTokenId, pAddress, msg.sender);

        return newTokenId;
    }

    function mintFT(address manager, uint256 amount) public returns (uint) {
        address admin = msg.sender;
        require(_managerFTs[manager] == 0, 'Already minted for this manager');
        require(Manager(manager).admin() == admin, 'Only for manager admin');
        require(ManagerFactory(_managerFactory).isManager(manager), 'Only for managers');

        _tokenIds.increment();
        uint256 newTokenId = _tokenIds.current();
        _mint(admin, newTokenId, amount, '');
        _managerFTs[manager] = newTokenId;

        emit FtMinted(newTokenId, manager, admin, amount);

        return newTokenId;
    }
}

