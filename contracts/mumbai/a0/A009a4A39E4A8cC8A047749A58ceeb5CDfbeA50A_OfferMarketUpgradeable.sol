abstract contract BaseMarketUpgradeable is IMarket, MarketHubRegistrarUpgradeable, SignatureValidator, UUPSUpgradeable {
    function initialize(string memory name) public virtual initializer {
        __UUPSUpgradeable_init();
        __SignatureValidator_init(name, "v1.0");
        __MarketHubRegistrar_init();
    }

    function _authorizeUpgrade(address newImplementation) internal override {
        require(
            marketHub.getUpgradeGatekeeper().getUpgradeTarget(address(this)) == newImplementation,
            "BaseMarket: newImplementation must be set as the target in the UpgradeGatekeeper contract"
        );
        marketHub.getUpgradeGatekeeper().resetUpgradeTarget();
    }

    function _createSale(
        address _seller,
        address _buyer,
        address _spender,
        address _collection,
        uint256 _tokenId,
        address _erc20,
        uint256 _price,
        bool _payNow
    ) internal {
        require(marketHub.isERC20Registered(_erc20), "Marketplace: Currency not registered.");
        require(marketHub.isERC721Registered(_collection), "Marketplace: Collection not registered.");
        require(marketHub.isAccountActive(_seller), "Marketplace: Seller not registered.");
        require(marketHub.isAccountActive(_buyer), "Marketplace: Buyer not registered.");
        require(_seller != _buyer, "Marketplace: Cannot buy your own product.");

        Account memory seller = marketHub.getAccount(_seller);
        require(marketHub.isRoyaltyRegistered(seller.tier), "Marketplace: Royalty not registered.");

        IEscrow escrow = marketHub.getEscrow();
        escrow.createSale(_buyer, _spender, _erc20, _price, _seller, _collection, _tokenId, _payNow);
    }

    function handleTokenIdNoLongerAvailable(address _seller, address _erc721, uint256 _tokenId) external virtual;
}

