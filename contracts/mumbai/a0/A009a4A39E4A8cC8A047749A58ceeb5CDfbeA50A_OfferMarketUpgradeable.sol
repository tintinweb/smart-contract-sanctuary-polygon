pragma solidity ^0.8.17;

import "./BaseMarketUpgradeable.sol";
import "../collections/IColleCollection.sol";

contract OfferMarketUpgradeable is BaseMarketUpgradeable {
    struct Offer {
        uint256 price;
        address spender;
        bool payNow;
    }

    // collection => tokenId => buyer => erc20 => price
    mapping(address => mapping(uint256 => mapping(address => mapping(address => Offer)))) offers;
    // collection => tokenId => erc20 => buyers
    mapping(address => mapping(uint256 => mapping(address => address[]))) pendingBuyers;

    event OfferPlaced(
        address indexed buyer,
        address spender,
        address indexed collection,
        uint256 indexed tokenId,
        address erc20,
        uint256 price,
        bool payNow
    );
    event OfferRevoked(address indexed buyer, address indexed collection, uint256 indexed tokenId, address erc20);
    event OffersInvalidated(
        address indexed collection,
        uint256 indexed tokenId,
        address erc20,
        address[] pendingBuyers
    );

    function offer(address _collection, uint256 _tokenId, address _erc20, uint256 _price, bool _payNow) public {
        _offer(msg.sender, msg.sender, _collection, _tokenId, _erc20, _price, _payNow);
    }

    function permitOffer(
        address _buyer,
        address _collection,
        uint256 _tokenId,
        address _erc20,
        uint256 _price,
        bool _payNow,
        uint256 _deadline,
        bytes memory _signature
    )
        public
        onlyColle
        isValidSignature(
            _buyer,
            _getPermitOfferHash(_buyer, _collection, _tokenId, _erc20, _price, _payNow, _deadline),
            _deadline,
            _signature
        )
    {
        _offer(_buyer, _buyer, _collection, _tokenId, _erc20, _price, _payNow);
    }

    function permitOffChainPaymentOffer(
        address _buyer,
        address _collection,
        uint256 _tokenId,
        address _erc20,
        uint256 _price,
        uint256 _deadline,
        bytes memory _signature
    )
        public
        onlyColle
        isValidSignature(
            _buyer,
            _getPermitOffChainPaymentOfferHash(_buyer, _collection, _tokenId, _erc20, _price, _deadline),
            _deadline,
            _signature
        )
    {
        _offer(_buyer, msg.sender, _collection, _tokenId, _erc20, _price, false);
    }

    function revokeOffer(address _collection, uint256 _tokenId, address _erc20) public {
        _revokeOffer(msg.sender, _collection, _tokenId, _erc20);
    }

    function permitRevokeOffer(
        address _buyer,
        address _collection,
        uint256 _tokenId,
        address _erc20,
        uint256 _deadline,
        bytes memory _signature
    )
        public
        onlyColle
        isValidSignature(
            _buyer,
            _getPermitRevokeOfferHash(_buyer, _collection, _tokenId, _erc20, _deadline),
            _deadline,
            _signature
        )
    {
        _revokeOffer(_buyer, _collection, _tokenId, _erc20);
    }

    function acceptOffer(address _buyer, address _erc20, address _collection, uint256 _tokenId) public {
        _acceptOffer(msg.sender, _buyer, _erc20, _collection, _tokenId);
    }

    function permitAcceptOffer(
        address _seller,
        address _buyer,
        address _erc20,
        address _collection,
        uint256 _tokenId,
        uint256 _deadline,
        bytes memory _signature
    )
        public
        onlyColle
        isValidSignature(
            _seller,
            _getPermitAcceptOfferHash(_seller, _buyer, _erc20, _collection, _tokenId, _deadline),
            _deadline,
            _signature
        )
    {
        _acceptOffer(_seller, _buyer, _erc20, _collection, _tokenId);
    }

    function handleTokenIdNoLongerAvailable(
        address /*_seller*/,
        address _erc721,
        uint256 _tokenId
    ) external virtual override onlyMarketHub {
        address[] memory erc20s = marketHub.getERC20s();
        for (uint i = 0; i < erc20s.length; ++i) {
            address erc20 = erc20s[i];

            address[] memory buyers = pendingBuyers[_erc721][_tokenId][erc20];
            if (buyers.length > 0) {
                for (uint256 j = 0; j < buyers.length; ++j) {
                    delete offers[_erc721][_tokenId][buyers[j]][erc20];
                }

                emit OffersInvalidated(_erc721, _tokenId, erc20, buyers);
                delete pendingBuyers[_erc721][_tokenId][erc20];
            }
        }
    }

    function _offer(
        address _buyer,
        address _spender,
        address _collection,
        uint256 _tokenId,
        address _erc20,
        uint256 _price,
        bool _payNow
    ) private {
        require(marketHub.allowNewSales(), "Marketplace is not accepting new sales");
        IColleCollection collection = marketHub.getCollection(_collection);
        address seller = collection.ownerOf(_tokenId);
        require(seller != address(0), "Marketplace: Token does not exist.");
        require(seller != _buyer, "Marketplace: Cannot buy your own product.");
        uint256 usdcPrice = marketHub.getCurrencyByERC20(_erc20).getEstimatedUSDCValue(_price);
        require(usdcPrice >= marketHub.getMinUSDCPrice(), "Marketplace: Price must be above minimum price.");

        Offer memory newOffer = Offer({price: _price, payNow: _payNow, spender: _spender});

        offers[_collection][_tokenId][_buyer][_erc20] = newOffer;

        pendingBuyers[_collection][_tokenId][_erc20].push(_buyer);

        emit OfferPlaced(_buyer, _spender, _collection, _tokenId, _erc20, _price, _payNow);
    }

    function _revokeOffer(address _buyer, address _collection, uint256 _tokenId, address _erc20) private {
        require(offers[_collection][_tokenId][_buyer][_erc20].price > 0, "Marketplace: Offer does not exist.");

        delete offers[_collection][_tokenId][_buyer][_erc20];

        uint256 offerLength = pendingBuyers[_collection][_tokenId][_erc20].length;
        for (uint256 i = 0; i < offerLength; i++) {
            if (pendingBuyers[_collection][_tokenId][_erc20][i] == _buyer) {
                pendingBuyers[_collection][_tokenId][_erc20][i] = pendingBuyers[_collection][_tokenId][_erc20][
                    offerLength - 1
                ];
                pendingBuyers[_collection][_tokenId][_erc20].pop();
                break;
            }
        }

        emit OfferRevoked(_buyer, _collection, _tokenId, _erc20);
    }

    function _acceptOffer(
        address _seller,
        address _buyer,
        address _erc20,
        address _collection,
        uint256 _tokenId
    ) private {
        require(
            !marketHub.getEscrow().hasActiveSale(_collection, _tokenId),
            "Marketplace: Sale already in progress for this item."
        );
        IColleCollection collection = marketHub.getCollection(_collection);
        Offer memory currentOffer = offers[_collection][_tokenId][_buyer][_erc20];
        require(currentOffer.price > 0, "Marketplace: Offer does not exist.");
        require(_seller == collection.ownerOf(_tokenId), "Marketplace: Only the token owner can call this function.");
        _createSale(
            _seller,
            _buyer,
            currentOffer.spender,
            _collection,
            _tokenId,
            _erc20,
            currentOffer.price,
            currentOffer.payNow
        );
    }

    function getOffer(
        address _collection,
        uint256 _tokenId,
        address _buyer,
        address _erc20
    ) public view returns (Offer memory) {
        return offers[_collection][_tokenId][_buyer][_erc20];
    }

    function _getPermitOfferHash(
        address _buyer,
        address _collection,
        uint256 _tokenId,
        address _erc20,
        uint256 _price,
        bool _payNow,
        uint256 _deadline
    ) private view returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    "\x19\x01", // EIP191: Indicates EIP712
                    DOMAIN_SEPARATOR,
                    keccak256(
                        abi.encode(
                            keccak256(
                                "Offer(address collection,uint256 tokenId,address buyer,address erc20,uint256 price,bool payNow,uint256 deadline,uint256 nonce)"
                            ),
                            _collection,
                            _tokenId,
                            _buyer,
                            _erc20,
                            _price,
                            _payNow,
                            _deadline,
                            nonces(_buyer)
                        )
                    )
                )
            );
    }

    function _getPermitOffChainPaymentOfferHash(
        address _buyer,
        address _collection,
        uint256 _tokenId,
        address _erc20,
        uint256 _price,
        uint256 _deadline
    ) private view returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    "\x19\x01", // EIP191: Indicates EIP712
                    DOMAIN_SEPARATOR,
                    keccak256(
                        abi.encode(
                            keccak256(
                                "OffChainPaymentBuy(address collection,uint256 tokenId,address buyer,address erc20,uint256 price,uint256 deadline,uint256 nonce)"
                            ),
                            _collection,
                            _tokenId,
                            _buyer,
                            _erc20,
                            _price,
                            _deadline,
                            nonces(_buyer)
                        )
                    )
                )
            );
    }

    function _getPermitRevokeOfferHash(
        address _buyer,
        address _collection,
        uint256 _tokenId,
        address _erc20,
        uint256 _deadline
    ) private view returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    "\x19\x01", // EIP191: Indicates EIP712
                    DOMAIN_SEPARATOR,
                    keccak256(
                        abi.encode(
                            keccak256(
                                "RevokeOffer(address collection,uint256 tokenId,address buyer,address erc20,uint256 deadline,uint256 nonce)"
                            ),
                            _collection,
                            _tokenId,
                            _buyer,
                            _erc20,
                            _deadline,
                            nonces(_buyer)
                        )
                    )
                )
            );
    }

    function _getPermitAcceptOfferHash(
        address _seller,
        address _buyer,
        address _erc20,
        address _collection,
        uint256 _tokenId,
        uint256 _deadline
    ) private view returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    "\x19\x01", // EIP191: Indicates EIP712
                    DOMAIN_SEPARATOR,
                    keccak256(
                        abi.encode(
                            keccak256(
                                "AcceptOffer(address collection,uint256 tokenId,address seller,address buyer,address erc20,uint256 deadline,uint256 nonce)"
                            ),
                            _collection,
                            _tokenId,
                            _seller,
                            _buyer,
                            _erc20,
                            _deadline,
                            nonces(_seller)
                        )
                    )
                )
            );
    }
}

