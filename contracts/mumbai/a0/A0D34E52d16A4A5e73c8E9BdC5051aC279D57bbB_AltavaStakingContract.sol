pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract StakingCore is OwnableUpgradeable {
    address public altavaLandNFT;
    address public assetNFT;
    address public lpToken;
    address public tavaToken;
    /**
     * ==================
     * Land Storage
     * ==================
     */
    enum LandStakingStatus {
        NOT_STAKING,
        UPGRADING,
        STAKING,
        FARMING
    }

    uint256 landMaxLevel;

    // type => claimAmount
    mapping(uint256 => uint256) landClaimAmountPerStaking;
    // type => level => upgrade period, cost
    mapping(uint256 => mapping(uint256 => LandUpgradeInitialInfo)) landUpgradeInitialInfo;
    // level => staking Period
    mapping(uint256 => uint256) landStakingPeriod;
    // land Id => building type => max available number
    mapping(uint256 => mapping(uint256 => uint256)) maxAvailableBuildingInstallNumber;

    // owner => staked land ids
    mapping(address => uint256[]) stakedLandIds;
    // landId => landInfo
    mapping(uint256 => LandInfo) landInfo;
    // landId => stakingInfo
    mapping(uint256 => LandStakingInfo) landStakingInfo;
    // landId => upgradeFinishTimestamp
    mapping(uint256 => uint256) landUpgradeFinishTimestamp;
    // landId => buildingIds
    mapping(uint256 => uint256[]) installedBuildingIds;
    // landId => bulidingId => building Number
    mapping(uint256 => mapping(uint256 => uint256)) installedBuildingNumber;
    // landId => building type => building type Count
    mapping(uint256 => mapping(uint256 => uint256)) installedBuildingTypeCount;

    struct LandInfo {
        // 160bit
        address owner;
        // 8bit
        // 실제 레벨 : level + 1
        uint8 level;
    }

    struct LandStakingInfo {
        // staking 시작한 시간
        uint256 startTimestamp;
        // 도중에 claim한 양
        uint256 claimedLPAmount;
    }

    struct LandUpgradeInitialInfo {
        uint96 period;
        uint80 tavaAmount;
        uint80 instantUpgradeTavaAmount;
    }

    /**
     * ==================
     * Buiding Storage
     * ==================
     */

    // building type => level => farming initial info
    mapping(uint256 => mapping(uint256 => BuildingFarmingInitialInfo)) buildingFarmingInitialInfo;
    // building type => level => upgrade info
    mapping(uint256 => mapping(uint256 => BuildingUpgradeInfo)) buildingUpgradeInfo;

    // owner => buildingId => staked amount
    mapping(address => mapping(uint256 => uint256)) stakedBuildingAmount;
    // owner => buildingId => LP amount
    mapping(address => mapping(uint256 => uint256))
        public chargedLPAmountToBuilding;
    // owner => buildingId => farming info
    mapping(address => mapping(uint256 => BuildingFarmingInfo))
        public buildingFarmingInfo;
    // owner => buildingId => upgrade FinishTimestamp
    mapping(address => mapping(uint256 => uint256)) buildingUpgradeFinishTimestamp;

    struct BuildingTokenUnit {
        uint256 tokenId;
        uint256 amount;
    }

    struct BuildingFarmingInitialInfo {
        uint256 period;
        uint256 farmingAmount;
        uint256 usingLPAmount;
    }

    struct BuildingFarmingInfo {
        uint256 startTime;
        uint256 claimedNumber;
    }

    struct BuildingUpgradeInfo {
        uint96 period;
        uint80 lpAmount;
        uint80 instantUpgradeTavaAmount;
    }

    /**
     * ==================
     * Input Struct
     * ==================
     */

    struct LandClaimAmountInput {
        uint256 landType;
        uint256 lpAmount;
    }

    struct LandUpgradingInitialInfoInput {
        uint256 landType;
        uint256 level;
        LandUpgradeInitialInfo landUpgradeInfo;
    }

    struct LandStakingPeriodInput {
        uint256 level;
        uint256 period;
    }

    struct MaxAvailableBuildingInstallNumberInput {
        uint256 landType;
        uint256 buildingType;
        uint256 maxAvailableNumber;
    }

    struct BuildingFarmingInitialInfoInput {
        uint256 buildingType;
        uint256 buildingLevel;
        BuildingFarmingInitialInfo buildingFarmingInitialInfo;
    }

    struct BuildingUpgradeInfoInput {
        uint256 buildingType;
        uint256 buildingLevel;
        BuildingUpgradeInfo buildingUpgradeInfo;
    }

    /**
     * ==================
     * Modifier
     * ==================
     */

    modifier onlyStakingLandOwner(uint256 landId) {
        require(
            IERC721(altavaLandNFT).ownerOf(landId) == address(this) &&
                landInfo[landId].owner == msg.sender,
            "StakingCore: not owner"
        );
        _;
    }

    modifier whenNotLandUpgrading(uint256 landId) {
        require(
            landUpgradeFinishTimestamp[landId] <= block.timestamp,
            "StakingCore: land upgrading"
        );
        _;
    }

    modifier onlyProperStakeAmount(uint256 buildingId) {
        require(
            stakedBuildingAmount[msg.sender][buildingId] > 0,
            "StakingCore: not staker"
        );
        _;
    }

    modifier whenNotBuildingUpgrading(uint256 buildingId) {
        require(
            buildingUpgradeFinishTimestamp[msg.sender][buildingId] <=
                block.timestamp,
            "StakingCore: building upgrading"
        );
        _;
    }

    /**
     * ==================
     * Initialize
     * ==================
     */

    function __StakingCore_init(
        address _altavaLandNFT,
        address _assetNFT,
        address _lpToken,
        address _tavaToken
    ) internal initializer {
        __StakingCore_init_unchained(
            _altavaLandNFT,
            _assetNFT,
            _lpToken,
            _tavaToken
        );
    }

    function __StakingCore_init_unchained(
        address _altavaLandNFT,
        address _assetNFT,
        address _lpToken,
        address _tavaToken
    ) internal initializer {
        __Ownable_init();
        altavaLandNFT = _altavaLandNFT;
        assetNFT = _assetNFT;
        lpToken = _lpToken;
        tavaToken = _tavaToken;
    }

    /**
     * ==================
     * Land Getter
     * ==================
     */

    function getMaxAvailableBuildingNumber(
        uint256 landId,
        uint256 buildingType
    ) public view returns (uint256) {
        return
            maxAvailableBuildingInstallNumber[getLandType(landId)][
                buildingType
            ];
    }

    // 0 : ONE_ONE,
    // 1 : ONE_TWO,
    // 2 : ONE_THREE,
    // 3 : TWO_TWO,
    // 4 : TWO_THREE,
    // 5 : FOUR_FOUR,
    // 6 : EIGHT_EIGHT
    function getLandType(uint256 tokenId) public pure returns (uint256) {
        if (tokenId <= 900) {
            return 0;
        } else if (tokenId <= 1035) {
            return 1;
        } else if (tokenId <= 1120) {
            return 2;
        } else if (tokenId <= 1320) {
            return 3;
        } else if (tokenId <= 1394) {
            return 4;
        } else if (tokenId <= 1437) {
            return 5;
        } else if (tokenId <= 1440) {
            return 6;
        } else {
            revert("AltavaLandNFT: invalid tokenId");
        }
    }

    /**
     * ==================
     * Land Staking Setting
     * ==================
     */

    function setLandMaxLevel(uint256 _landMaxLevel) public onlyOwner {
        landMaxLevel = _landMaxLevel;
    }

    function setMaxAvailableBuildingNumber(
        MaxAvailableBuildingInstallNumberInput[] memory inputs
    ) public onlyOwner {
        for (uint256 i = 0; i < inputs.length; i++) {
            maxAvailableBuildingInstallNumber[inputs[i].landType][
                inputs[i].buildingType
            ] = inputs[i].maxAvailableNumber;
        }
    }

    function setLandClaimAmountPerStaking(
        LandClaimAmountInput[] calldata inputs
    ) public onlyOwner {
        for (uint256 i = 0; i < inputs.length; i++) {
            landClaimAmountPerStaking[inputs[i].landType] = inputs[i].lpAmount;
        }
    }

    function setLandUpgradingInfo(
        LandUpgradingInitialInfoInput[] calldata inputs
    ) public onlyOwner {
        for (uint256 i = 0; i < inputs.length; i++) {
            landUpgradeInitialInfo[inputs[i].landType][
                inputs[i].level
            ] = inputs[i].landUpgradeInfo;
        }
    }

    function setLandStakingPeriod(
        LandStakingPeriodInput[] calldata inputs
    ) public onlyOwner {
        for (uint256 i = 0; i < inputs.length; i++) {
            landStakingPeriod[inputs[i].level] = inputs[i].period;
        }
    }

    /**
     * ==================
     * Building Staking Setting
     * ==================
     */

    function setBuildingFarmingInitialInfo(
        BuildingFarmingInitialInfoInput[] calldata inputs
    ) public onlyOwner {
        for (uint256 i = 0; i < inputs.length; i++) {
            buildingFarmingInitialInfo[inputs[i].buildingType][
                inputs[i].buildingLevel
            ] = inputs[i].buildingFarmingInitialInfo;
        }
    }

    function setBuildingUpgradeInfo(
        BuildingUpgradeInfoInput[] calldata inputs
    ) public onlyOwner {
        for (uint256 i = 0; i < inputs.length; i++) {
            buildingUpgradeInfo[inputs[i].buildingType][
                inputs[i].buildingLevel
            ] = inputs[i].buildingUpgradeInfo;
        }
    }

    /**
     * ========================
     * Owner Functions
     * ========================
     */
    function tavaWithdraw(uint256 amount) public onlyOwner {
        IERC20(tavaToken).transfer(msg.sender, amount);
    }

    uint256[27] private __gap;
}

