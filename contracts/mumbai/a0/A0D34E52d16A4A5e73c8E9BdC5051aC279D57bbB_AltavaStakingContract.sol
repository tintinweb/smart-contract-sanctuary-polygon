pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";

import "../interfaces/IMaterialNFT.sol";
import "../interfaces/ILandLPToken.sol";
import "../interfaces/IBuildingNFT.sol";
import "./StakingCore.sol";

contract BuildingStaking is StakingCore {
    function installBuildings(
        uint256 landId,
        BuildingTokenUnit[] memory buildingTokenUnits
    ) public onlyStakingLandOwner(landId) {
        for (uint256 i = 0; i < buildingTokenUnits.length; i++) {
            uint256 buildingType = getBuildingType(
                buildingTokenUnits[i].tokenId
            );
            uint256 buildingId = buildingTokenUnits[i].tokenId;
            uint256 currentAvailableNum = getMaxAvailableBuildingNumber(
                landId,
                buildingType
            ) - installedBuildingTypeCount[landId][buildingType];

            require(
                currentAvailableNum > 0,
                "BuildingStaking: no available building slot"
            );
            require(
                IERC1155(assetNFT).balanceOf(msg.sender, buildingId) >=
                    buildingTokenUnits[i].amount,
                "BuildingStaking: The token balance is insufficient"
            );
            installedBuildingIds[landId].push(buildingId);

            installedBuildingNumber[landId][buildingId] += buildingTokenUnits[i]
                .amount;

            installedBuildingTypeCount[landId][
                buildingType
            ] += buildingTokenUnits[i].amount;

            IERC1155(assetNFT).safeTransferFrom(
                msg.sender,
                address(this),
                buildingId,
                buildingTokenUnits[i].amount,
                ""
            );
            //TODO : building id의 채굴 속도 높이는 작업
        }
    }

    function chargeLPTokenToBuiding(
        uint256 buildingId,
        uint256 lpAmount
    )
        public
        onlyProperStakeAmount(buildingId)
        whenNotBuildingUpgrading(buildingId)
    {
        require(
            lpAmount > 0,
            "BuildingStaking: amount should be greater than 0"
        );
        // TODO : max 충전량 제한
        require(
            IERC20(lpToken).balanceOf(msg.sender) >= lpAmount,
            "BuildingStaking: not enough lp token"
        );
        ILandLPToken(lpToken).burn(msg.sender, lpAmount);
        chargedLPAmountToBuilding[msg.sender][buildingId] += lpAmount;

        buildingFarmingInfo[msg.sender][buildingId] = BuildingFarmingInfo(
            block.timestamp,
            0
        );
    }

    function claimMaterial(
        uint256 buildingId
    )
        public
        onlyProperStakeAmount(buildingId)
        whenNotBuildingUpgrading(buildingId)
    {
        uint256 availableNumber = getClaimAvailableMaterialNumber(buildingId);
        require(availableNumber > 0, "BuildingStaking: no available material");

        chargedLPAmountToBuilding[msg.sender][
            buildingId
        ] = getRemainChargedLPTokenAmount(buildingId);

        uint256 claimAvailableAmount = availableNumber *
            buildingFarmingInitialInfo[getBuildingType(buildingId)][
                getBuildingLevel(buildingId)
            ].farmingAmount;

        IMaterialNFT(assetNFT).mint(
            msg.sender,
            getMaterialTokenId(buildingId),
            claimAvailableAmount
        );

        buildingFarmingInfo[msg.sender][buildingId]
            .claimedNumber += availableNumber;
    }

    function upgradeBuilding(
        uint256 buildingId,
        uint256 amount
    ) public onlyProperStakeAmount(buildingId) {
        require(
            getBuildingLevel(buildingId) < 24,
            "BuildingStaking: already max level"
        );
        claimMaterial(buildingId);
        // 남아있는 LP 보내준 후 초기화
        // TODO: burn
        if (chargedLPAmountToBuilding[msg.sender][buildingId] > 0) {
            //TODO: amount만큼 곱해야함
            IERC20(lpToken).transfer(
                msg.sender,
                chargedLPAmountToBuilding[msg.sender][buildingId]
            );
            delete chargedLPAmountToBuilding[msg.sender][buildingId];
        }
        delete buildingFarmingInfo[msg.sender][buildingId];
        // TODO: burn
        IERC20(lpToken).transferFrom(
            msg.sender,
            address(this),
            buildingUpgradeInfo[getBuildingType(buildingId)][
                getBuildingLevel(buildingId)
            ].lpAmount
        );

        IBuildingNFT(assetNFT).upgrade(msg.sender, buildingId, amount);

        uint256 upgradePeriod = getUpgradePeriod(buildingId);
        buildingUpgradeFinishTimestamp[msg.sender][buildingId] =
            block.timestamp +
            upgradePeriod;

        delete buildingFarmingInfo[msg.sender][buildingId];
    }

    function upgradeBuildingInstantly(
        uint256 buildingId
    ) public onlyProperStakeAmount(buildingId) {
        require(
            buildingUpgradeFinishTimestamp[msg.sender][buildingId] >
                block.timestamp,
            "BuildingStaking: not upgrading"
        );
        uint256 upgradeTavaAmount = buildingUpgradeInfo[
            getBuildingType(buildingId)
        ][getBuildingLevel(buildingId)].instantUpgradeTavaAmount;

        IERC20(tavaToken).transferFrom(
            msg.sender,
            address(this),
            upgradeTavaAmount
        );

        buildingUpgradeFinishTimestamp[msg.sender][buildingId] = block
            .timestamp;
    }

    function removeBuildings(
        uint256 landId,
        BuildingTokenUnit[] memory buildingTokenUnits
    ) public onlyStakingLandOwner(landId) {
        for (uint256 i = 0; i < buildingTokenUnits.length; i++) {
            uint256 buildingId = buildingTokenUnits[i].tokenId;
            require(
                stakedBuildingAmount[msg.sender][buildingId] >=
                    buildingTokenUnits[i].amount,
                "StakingCore: not staker"
            );
            require(
                buildingUpgradeFinishTimestamp[msg.sender][buildingId] <=
                    block.timestamp,
                "BuildingStaking: upgrading"
            );
            for (uint256 j = 0; j < installedBuildingIds[landId].length; j++) {
                if (installedBuildingIds[landId][j] == buildingId) {
                    installedBuildingIds[landId][j] = installedBuildingIds[
                        landId
                    ][installedBuildingIds[landId].length - 1];
                    installedBuildingIds[landId].pop();
                    break;
                }
            }
            if (getClaimAvailableMaterialNumber(buildingId) > 0) {
                claimMaterial(buildingId);
            }
            delete buildingFarmingInfo[msg.sender][buildingId];
            // 남아있는 LP 보내준 후 초기화
            // TODO: burn
            if (chargedLPAmountToBuilding[msg.sender][buildingId] > 0) {
                IERC20(lpToken).transfer(
                    msg.sender,
                    chargedLPAmountToBuilding[msg.sender][buildingId]
                );
                delete chargedLPAmountToBuilding[msg.sender][buildingId];
            }
            IERC721(assetNFT).transferFrom(
                address(this),
                msg.sender,
                buildingId
            );

            stakedBuildingAmount[msg.sender][buildingId] -= buildingTokenUnits[
                i
            ].amount;

            installedBuildingTypeCount[landId][
                getBuildingType(buildingId)
            ] -= buildingTokenUnits[i].amount;

            installedBuildingNumber[landId][(buildingId)] -= buildingTokenUnits[
                i
            ].amount;

            if (installedBuildingNumber[landId][(buildingId)] == 0) {
                delete installedBuildingIds[landId][(buildingId)];
            }
        }
        //TODO: builing id에 해당하는 building number를 빼줘야함
        //TODO: 남아있는 해당하는  building id가 없으면 지우기
    }

    function getChargedLPTokenAmount(
        uint256 buildingId
    ) public view returns (uint256) {
        return chargedLPAmountToBuilding[msg.sender][buildingId];
    }

    function getRemainChargedLPTokenAmount(
        uint256 buildingId
    ) public view returns (uint256) {
        uint256 availableNumber = getClaimAvailableMaterialNumber(buildingId);
        uint256 currentLevel = getBuildingLevel(buildingId);
        uint256 buildingType = getBuildingType(buildingId);
        uint256 usedLPAmount = buildingFarmingInitialInfo[buildingType][
            currentLevel
        ].usingLPAmount * availableNumber;
        return chargedLPAmountToBuilding[msg.sender][buildingId] - usedLPAmount;
    }

    function getInstalledBuildingIds(
        uint256 landId
    ) public view returns (uint256[] memory) {
        return installedBuildingIds[landId];
    }

    function getUpgradePeriod(
        uint256 buildingId
    ) public view returns (uint256) {
        return
            buildingUpgradeInfo[getBuildingType(buildingId)][
                getBuildingLevel(buildingId)
            ].period;
    }

    function getClaimAvailableMaterialNumber(
        uint256 buildingId
    ) public view returns (uint256) {
        uint256 farmingTime = block.timestamp -
            buildingFarmingInfo[msg.sender][buildingId].startTime;
        uint256 totalAvailableNum = (farmingTime /
            getMaterialFarmingPeriod(buildingId));
        return
            totalAvailableNum -
            buildingFarmingInfo[msg.sender][buildingId].claimedNumber;
    }

    function getClaimAvailableMaterialAmount(
        uint256 buildingId
    ) public view returns (uint256) {
        uint256 availableNumber = getClaimAvailableMaterialNumber(buildingId);
        return
            availableNumber *
            buildingFarmingInitialInfo[getBuildingType(buildingId)][
                getBuildingLevel(buildingId)
            ].farmingAmount;
    }

    function getMaterialTokenId(
        uint256 buildingId
    ) public pure returns (uint256) {
        return (buildingId / 25) + 1;
    }

    // 0 : Toolkit,
    // 1 : Natural Fiber,
    // 2 : Synthetic Fiber,
    // 3 : Supplies,
    function getBuildingType(uint256 buildingId) public pure returns (uint256) {
        return (buildingId / 25) + 1;
    }

    function getBuildingLevel(
        uint256 buildingId
    ) public pure returns (uint256) {
        return (buildingId % 25) + 1;
    }

    function getMaterialFarmingPeriod(
        uint256 buildingId
    ) public view returns (uint256) {
        uint256 buildingLevel = getBuildingLevel(buildingId);
        return
            buildingFarmingInitialInfo[getBuildingType(buildingId)][
                buildingLevel
            ].period;
    }

    uint256[50] private __gap;
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";

import "./BuildingStaking.sol";
import "../interfaces/ILandLPToken.sol";
import "hardhat/console.sol";

contract LandStaking is BuildingStaking {
    function landStake(uint256 landId) public whenNotLandUpgrading(landId) {
        if (IERC721(altavaLandNFT).ownerOf(landId) == msg.sender) {
            //스테이킹이 안되어 있는 경우
            IERC721(altavaLandNFT).transferFrom(
                msg.sender,
                address(this),
                landId
            );
        } else if (IERC721(altavaLandNFT).ownerOf(landId) == address(this)) {
            // 이미 스테이킹이 되어 있는 경우(재스테이킹)
            require(
                getLandStakingRemainTimestamp(landId) == 0,
                "LandStaking: staking not finished"
            );
            //claim 안됐으면 나머지 클레임
            if (
                landStakingInfo[landId].claimedLPAmount !=
                landClaimAmountPerStaking[getLandType(landId)]
            ) {
                landClaim(landId);
            }
            delete landStakingInfo[landId];
        } else {
            revert("LandStaking: not owner");
        }
        landInfo[landId].owner = msg.sender;
        landStakingInfo[landId].startTimestamp = block.timestamp;
        stakedLandIds[msg.sender].push(landId);
    }

    function landBatchStake(uint256[] memory landIds) public {
        for (uint256 i = 0; i < landIds.length; i++) {
            require(
                landUpgradeFinishTimestamp[landIds[i]] <= block.timestamp,
                "LandStaking: land upgrading"
            );
            landStake(landIds[i]);
        }
    }

    function landUnstake(
        uint256 landId
    ) public onlyStakingLandOwner(landId) whenNotLandUpgrading(landId) {
        //staking 기간 끝나면 unstake 가능
        require(
            getLandStakingRemainTimestamp(landId) == 0,
            "LandStaking: land staking not finished"
        );
        //claim 안됐으면 나머지 클레임
        if (
            landStakingInfo[landId].claimedLPAmount !=
            landClaimAmountPerStaking[getLandType(landId)]
        ) {
            landClaim(landId);
        }
        IERC721(altavaLandNFT).transferFrom(address(this), msg.sender, landId);
        landInfo[landId].owner = address(0);

        //설치되어 있는 building 제거
        uint256[] memory buildingIds = installedBuildingIds[landId];
        if (buildingIds.length > 0) {
            BuildingTokenUnit[]
                memory buildingTokenUnits = new BuildingTokenUnit[](
                    buildingIds.length
                );
            for (uint256 i = 0; i < buildingIds.length; i++) {
                buildingTokenUnits[i] = BuildingTokenUnit(
                    buildingIds[i],
                    installedBuildingNumber[landId][buildingIds[i]]
                );
            }
            removeBuildings(landId, buildingTokenUnits);
        }
        delete landStakingInfo[landId];
        for (uint256 i = 0; i < stakedLandIds[msg.sender].length; i++) {
            if (stakedLandIds[msg.sender][i] == landId) {
                stakedLandIds[msg.sender][i] = stakedLandIds[msg.sender][
                    stakedLandIds[msg.sender].length - 1
                ];
                stakedLandIds[msg.sender].pop();
                break;
            }
        }
    }

    function landBatchUnstake(uint256[] calldata landIds) public {
        for (uint256 i = 0; i < landIds.length; i++) {
            require(
                landUpgradeFinishTimestamp[landIds[i]] <= block.timestamp,
                "StakingCore: land upgrading"
            );
            require(
                IERC721(altavaLandNFT).ownerOf(landIds[i]) == address(this) &&
                    landInfo[landIds[i]].owner == msg.sender,
                "StakingCore: not owner"
            );
            landUnstake(landIds[i]);
        }
    }

    function landClaim(
        uint256 landId
    ) public onlyStakingLandOwner(landId) whenNotLandUpgrading(landId) {
        uint256 availableClaimAmount = getAvailableLPToken(landId);

        require(
            availableClaimAmount > 0,
            "LandStaking: no available claim amount"
        );

        ILandLPToken(lpToken).mint(msg.sender, availableClaimAmount);

        landStakingInfo[landId].claimedLPAmount =
            availableClaimAmount +
            landStakingInfo[landId].claimedLPAmount;
    }

    function landBatchClaim(uint256[] calldata landIds) public {
        for (uint256 i = 0; i < landIds.length; i++) {
            require(
                landUpgradeFinishTimestamp[landIds[i]] <= block.timestamp,
                "StakingCore: land upgrading"
            );
            require(
                IERC721(altavaLandNFT).ownerOf(landIds[i]) == address(this) &&
                    landInfo[landIds[i]].owner == msg.sender,
                "StakingCore: not owner"
            );
            landClaim(landIds[i]);
        }
    }

    function landUpgrade(
        uint256 landId
    ) public onlyStakingLandOwner(landId) whenNotLandUpgrading(landId) {
        require(
            landInfo[landId].level < landMaxLevel,
            "LandStaking: already max level"
        );
        //claim 안됐으면 나머지 클레임
        if (getAvailableLPToken(landId) > 0) {
            landClaim(landId);
        }

        require(
            IERC20(tavaToken).balanceOf(msg.sender) >=
                landUpgradeInitialInfo[getLandType(landId)][
                    landInfo[landId].level
                ].tavaAmount,
            "LandStaking: not enough tava"
        );
        IERC20(tavaToken).transferFrom(
            msg.sender,
            address(this),
            landUpgradeInitialInfo[getLandType(landId)][landInfo[landId].level]
                .tavaAmount
        );

        landInfo[landId].level += 1;

        landUpgradeFinishTimestamp[landId] =
            block.timestamp +
            landUpgradeInitialInfo[getLandType(landId)][landInfo[landId].level]
                .period;

        delete landStakingInfo[landId];
    }

    function landUpgradeInstantly(
        uint256 landId
    ) public onlyStakingLandOwner(landId) {
        // 업그레이드 중이어야 함
        require(
            landUpgradeFinishTimestamp[landId] > block.timestamp,
            "LandStaking: not upgrading"
        );

        require(
            IERC20(tavaToken).balanceOf(msg.sender) >=
                landUpgradeInitialInfo[getLandType(landId)][
                    landInfo[landId].level
                ].instantUpgradeTavaAmount,
            "LandStaking: not enough tava"
        );
        IERC20(tavaToken).transferFrom(
            msg.sender,
            address(this),
            landUpgradeInitialInfo[getLandType(landId)][landInfo[landId].level]
                .instantUpgradeTavaAmount
        );

        landUpgradeFinishTimestamp[landId] = block.timestamp;
    }

    function getLandStakingStatus(
        uint256 landId
    ) public view returns (LandStakingStatus) {
        if (IERC721(altavaLandNFT).ownerOf(landId) == address(this)) {
            if (isUpgrading(landId)) {
                return LandStakingStatus.UPGRADING;
            } else if (
                landStakingInfo[landId].startTimestamp +
                    landStakingPeriod[landInfo[landId].level] >
                block.timestamp
            ) {
                return LandStakingStatus.FARMING;
            } else {
                // farming은 끝나고 restake해야하는 상태
                return LandStakingStatus.STAKING;
            }
        } else {
            return LandStakingStatus.NOT_STAKING;
        }
    }

    function isUpgrading(uint256 landId) public view returns (bool) {
        return landUpgradeFinishTimestamp[landId] > block.timestamp;
    }

    function getAvailableLPToken(uint256 landId) public view returns (uint256) {
        if (
            landStakingInfo[landId].claimedLPAmount ==
            landClaimAmountPerStaking[getLandType(landId)]
        ) {
            return 0;
        } else {
            return
                (((landStakingPeriod[landInfo[landId].level] -
                    getLandStakingRemainTimestamp(landId)) *
                    landClaimAmountPerStaking[getLandType(landId)]) /
                    landStakingPeriod[landInfo[landId].level]) -
                landStakingInfo[landId].claimedLPAmount;
        }
    }

    function getLandLevel(uint256 landId) external view returns (uint256) {
        return landInfo[landId].level + 1;
    }

    function getLandStakingRemainTimestamp(
        uint256 landId
    ) public view returns (uint256) {
        if (
            block.timestamp - landStakingInfo[landId].startTimestamp >
            landStakingPeriod[landInfo[landId].level]
        ) {
            return 0;
        } else {
            return
                landStakingPeriod[landInfo[landId].level] -
                (block.timestamp - landStakingInfo[landId].startTimestamp);
        }
    }

    function getLandStakingPeriod(
        uint256 landId
    ) public view returns (uint256) {
        return landStakingPeriod[landInfo[landId].level];
    }

    function getUpgradeReaminTimestamp(
        uint256 landId
    ) public view returns (uint256) {
        if (landUpgradeFinishTimestamp[landId] > block.timestamp) {
            return landUpgradeFinishTimestamp[landId] - block.timestamp;
        } else {
            return 0;
        }
    }

    function getLandStakingFinishTimestamp(
        uint256 landId
    ) public view returns (uint256) {
        return
            landStakingInfo[landId].startTimestamp +
            landStakingPeriod[landInfo[landId].level];
    }

    function getLandInfo(uint256 landId) public view returns (LandInfo memory) {
        return landInfo[landId];
    }

    function getLandStakingInfo(
        uint256 landId
    ) public view returns (LandStakingInfo memory) {
        return landStakingInfo[landId];
    }

    function getLandUpgradeFinishTime(
        uint256 landId
    ) public view returns (uint256) {
        return landUpgradeFinishTimestamp[landId];
    }

    function getLandUpgradeInitialInfo(
        uint256 landId
    ) public view returns (LandUpgradeInitialInfo memory) {
        return
            landUpgradeInitialInfo[getLandType(landId)][landInfo[landId].level];
    }

    uint256[50] private __gap;
}

