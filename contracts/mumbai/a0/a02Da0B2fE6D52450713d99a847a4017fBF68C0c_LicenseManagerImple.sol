pragma solidity >=0.8.13;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/interfaces/IERC2981.sol";
// import "@openzeppelin/contracts/proxy/beacon/BeaconProxy.sol";
import "./ILicense.sol";
import "../zeppelin/BeaconProxy.sol";
import {SpaceLibrary} from "../space/ISpaceManager.sol";

contract LicenseManagerImple is Initializable, OwnableUpgradeable, ILicense {
    address public collectionBeacon;
    ISpaceManager public spaceManager;

    uint256 public registryCount;
    uint256 public maxLicenseId;

    // collection's address => license_id
    mapping(address => uint256) public collections;

    // collection's address => registry id
    mapping(address => uint256[]) public collectionRegistries;

    mapping(uint256 => License.AuthorizedLicense) public licenses;

    mapping(uint256 => License.Registry) public registries;

    constructor() {
        _disableInitializers();
    }

    function license_initialize(
        address _collectionBeacon
    ) external initializer {
        collectionBeacon = _collectionBeacon;
        __Ownable_init();
    }

    function GetCollectionLicense(
        address collection
    ) external view returns (License.AuthorizedLicense memory) {
        return licenses[collections[collection]];
    }

    function GetRegisty(
        uint256 registryId
    ) external view returns (License.Registry memory) {
        return registries[registryId];
    }

    function CreateRegistry(
        License.Registry memory _registry
    ) public returns (uint256) {
        // check licensor.type
        // a. Creator: verify signature from data is signed by verifier on registry
        // b. Holder: check tx.origin == IERC721(registry.licensor.addr).ownerOf(registry.licensor.id)
        // c. Licensee: check tx.origin == licenses[registry.licensor.id] && licenses[registry.licensor.id].sublicensable
        // _registryId  prevents replay attack
        // write new registry and update status
        // emit event

        SpaceLibrary.Collection memory c = spaceManager.getCollection(
            _registry.licensor.collection
        );

        if (c.spaceId == 0) {
            revert ErrInvalidSpace();
        }
        if (_registry.split > 1e9) revert ErrInvalidSplit();
        if (c.cbeLicense == CBE.LicenseVersion.PUBLIC && _registry.split > 0)
            revert ErrInvalidSplit();
        if (_registry.licensor.licenseType == License.LicensorType.Holder) {
            if (
                c.cbeLicense == CBE.LicenseVersion.PERSONAL ||
                c.cbeLicense == CBE.LicenseVersion.PERSONAL_NO_HATE
            ) {
                revert ErrInvalidCBELicense();
            }
            if (
                IERC721(_registry.licensor.collection).ownerOf(
                    _registry.licensor.id
                ) != msg.sender
            ) revert ErrNoPermission();
        } else {
            if (c.cbeLicense == CBE.LicenseVersion.EXCLUSIVE) {
                revert ErrInvalidCBELicense();
            }
            if (!c.verified) {
                revert ErrNotVerified();
            }
            if (
                spaceManager.getCollectionOwner(
                    _registry.licensor.collection
                ) != msg.sender
            ) revert ErrNoPermission();
        }

        registryCount++;
        registries[registryCount] = _registry;
        collectionRegistries[_registry.licensor.collection].push(registryCount);

        emit RegistryCreated(registryCount, _registry.licensor.collection);
        return registryCount;
    }

    function CreateLicensedDerivative(
        uint256 _registryId,
        uint256 _spaceId,
        Collection.InitParams memory _params,
        string memory _metadataURI
    ) public payable returns (address) {
        // check registry.approver, if not null, check signature
        // create new license with a revenue collector and deploy a derivative collection
        // _licenseId which allocated by centralized backend server prevents replay attack

        License.Registry storage registry = registries[_registryId];

        // get licensor collection
        SpaceLibrary.Collection memory licensorCollection = spaceManager
            .getCollection(registry.licensor.collection);

        // if derivative collection creator choose to create new space, check if caller is the owner of the space
        if (_spaceId != licensorCollection.spaceId) {
            (address spaceOwner, , bool verified) = spaceManager.spaces(
                _spaceId
            );

            if (!verified) {
                revert ErrNotVerified();
            }
            if (msg.sender != spaceOwner) {
                revert ErrNotSpaceOwner();
            }
        }

        if (!registry.available) {
            revert ErrRegistryRevoked();
        }
        if (registry.licensor.collection == address(0)) {
            revert ErrRegistryNotExist();
        }
        if (msg.value != registry.price) {
            revert ErrInvalidPrice();
        }
        if (registry.maxSupply != 0) {
            if (registry.issued >= registry.maxSupply)
                revert ErrMaxSupplyExceeded();
        }

        // record issued amount
        registry.issued++;

        // create a new collection from BeaconProxy factory
        BeaconProxy collection = new BeaconProxy(collectionBeacon, "");
        uint256 expiredAt;
        if (registry.duration > 0) {
            expiredAt = block.timestamp + registry.duration;
        }
        License.AuthorizedLicense memory license = License.AuthorizedLicense({
            owner: msg.sender,
            registryId: _registryId,
            expiredAt: expiredAt,
            split: registry.split,
            active: true,
            sublicensable: registry.sublicensable,
            authorizedContract: address(collection)
        });

        // record derivative collection on space contract
        spaceManager.addDerivativeCollection(
            address(collection),
            _spaceId,
            _params.owner,
            _metadataURI,
            _params.cbeLicense
        );
        maxLicenseId++;
        licenses[maxLicenseId] = license;
        collections[address(collection)] = maxLicenseId;

        _params.spaceManager = address(spaceManager);
        _params.licenseManger = address(this);
        _params.owner = msg.sender;
        _params.paymentRecipient = address(0);
        ICollection(address(collection)).initialize(_params);
        emit DerivativeLicensed(maxLicenseId, address(collection));

        //transfer value
        if (registry.price > 0) {
            address valueReceiver;
            if (registry.licensor.licenseType == License.LicensorType.Holder) {
                valueReceiver = IERC721(registry.licensor.collection).ownerOf(
                    registry.licensor.id
                );
            } else {
                // if external collection not support ERC2981,
                // license paid fee will be sent to collection owner
                // otherwise, paid fee will be sent to royalty receiver
                address collectionOwner = spaceManager.getCollectionOwner(
                    registry.licensor.collection
                );
                if (
                    !IERC165(registry.licensor.collection).supportsInterface(
                        type(IERC2981).interfaceId
                    )
                ) {
                    valueReceiver = collectionOwner;
                } else {
                    (address receiver, ) = IERC2981(
                        registry.licensor.collection
                    ).royaltyInfo(0, 0);

                    valueReceiver = receiver != address(0)
                        ? receiver
                        : collectionOwner;
                }
            }

            payable(valueReceiver).transfer(registry.price);

            emit LicensePaid(
                maxLicenseId,
                msg.sender,
                valueReceiver,
                registry.price
            );
        }

        return address(collection);
    }

    function UpdateRegistry(
        uint256 registryId,
        License.Registry memory registry
    ) public {
        if (registryId > registryCount) revert ErrRegistryNotExist();
        License.Registry storage _registry = registries[registryId];

        // check current license registry owner
        address RegistryOwner = _registry.licensor.licenseType ==
            License.LicensorType.Holder
            ? IERC721(_registry.licensor.collection).ownerOf(
                _registry.licensor.id
            )
            : spaceManager.getCollectionOwner(_registry.licensor.collection);

        if (RegistryOwner != msg.sender) revert ErrNoPermission();
        if (!_registry.available) revert ErrRegistryRevoked();
        if (_registry.issued != 0) revert ErrRegistryIssued(); // only allow calling before the registry used

        // update registry
        _registry.duration = registry.duration;
        _registry.price = registry.price;
        _registry.split = registry.split;
        _registry.maxSupply = registry.maxSupply;
        _registry.revoker = registry.revoker;
        _registry.approver = registry.approver;
        _registry.allowdedCBELicense = registry.allowdedCBELicense;
        _registry.metadataURI = registry.metadataURI;
        _registry.sublicensable = registry.sublicensable;

        emit RegistryUpdated(registryId, registry);
    }

    function RevokeRegistry(uint256 registryId) public {
        License.Registry storage _registry = registries[registryId];

        address RegistryOwner = _registry.licensor.licenseType ==
            License.LicensorType.Holder
            ? IERC721(_registry.licensor.collection).ownerOf(
                _registry.licensor.id
            )
            : spaceManager.getCollectionOwner(_registry.licensor.collection);

        if (registryId > registryCount) revert ErrRegistryNotExist();
        if (!_registry.available) revert ErrRegistryRevoked();
        if (_registry.revoker == address(0)) revert ErrRevokerNotSet();
        if (_registry.revoker != msg.sender && RegistryOwner != msg.sender)
            revert ErrNoPermission();

        _registry.available = false;

        emit RegistryRevoked(registryId, _registry.revoker);
    }

    /** ========== admin functions ========== */
    function SetSpaceManager(address _space) external onlyOwner {
        spaceManager = ISpaceManager(_space);
    }

    function SetCollectionBeacon(address _collectionBeacon) external onlyOwner {
        collectionBeacon = _collectionBeacon;
    }

    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
     */
    uint256[50] private __gap;
}

