        address collection; // collection contract address
        uint256 id; // null, token_id , license_id
    }

    struct Registry {
        Licensor licensor;
        uint256 duration;
        uint256 price; // in wei
        // decimal is 9
        // i.e. 1,000,000,000 stands for 100% profit distributed to the Authorizer
        uint64 split;
        uint256 maxSupply; // 0 means unlimited
        uint256 issued; // issued license count
        address revoker;
        bool available; // default is true
        address approver;
        uint256 allowdedCBELicense; //supported cbe license types for derivatives which are recorded in bits
        // metadataURI is ipfs url
        // json format:
        // {
        // "name": "license registry name",
        // "image": "File | Blob",
        // "description": "descr",
        //  }
        string metadataURI;
        bool sublicensable;
    }
}

