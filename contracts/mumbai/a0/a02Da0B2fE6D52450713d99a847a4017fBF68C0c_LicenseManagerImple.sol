pragma solidity ^0.8.13;

import "../cbe/ICantBeEvil.sol";

interface ISpaceManager {
    struct Space {
        address spaceOwner;
        string details;
        bool verified;
    }

    // view interfaces
    function spaces(
        uint256 spaceId
    ) external view returns (address, string memory, bool);

    function verifiedSpace(uint256 spaceId) external view returns (bool);

    function isSpaceOwner(
        uint256 spaceId,
        address account
    ) external view returns (bool);

    function getCollectionOwner(
        address collection
    ) external view returns (address);

    function getCollection(
        address collection
    ) external view returns (SpaceLibrary.Collection memory);

    function getUserSpaces(
        address spaceOwner
    ) external view returns (uint256[] memory createdSpaces);

    /** ========== main function ========== */
    function addDerivativeCollection(
        address _collection,
        uint256 _spaceId,
        address _collectionOwner,
        string memory _metadataURI,
        CBE.LicenseVersion cbeLicense
    ) external;

    // space provider interfaces
    function registerNewSpace(string memory spaceMetadata) external;

    // update space detail
    function updateSpace(uint256 spaceId, string memory newDetail) external;

    /** ========== event ========== */
    event NewSpaceRegistered(
        uint256 indexed newSpaceId,
        address indexed registerAddress,
        string detail
    );

    event UpdatedSpace(uint256 indexed spaceId, string newDetail);

    event SpaceApproved(uint256 indexed spaceId);

    event CollectionAdded(
        uint256 indexed spaceId,
        address indexed collectionOwner,
        address indexed collection
    );

    event CollectionVerified(address indexed collection);

    event CollectionBeaconUpdated(address indexed newBeacon);

    // error
    error ErrNoPermission();

    error ErrCollectionAlreadyAdded();

    error ErrInvalidSpace();

    error ErrSpaceNotExist();

    error ErrSpaceApproved();

    error ErrOnlyInternalCall();

    error ErrInvalidCollection();
}

library SpaceLibrary {
    struct Collection {
        uint256 spaceId;
        // _metadataURI is ipfs url of space
        // json format:
        // {
        // "name": "collecion name",
        // "image": "File | Blob",
        //  "description": "collection descr",
        //  "cbe_license": 0,// 0: PUBLIC,1: EXCLUSIVE,2: COMMERCIAL,3: COMMERCIAL_NO_HATE, 4: PERSONAL 5:PERSONAL_NO_HATE,
        //  "category":"art"
        //  }
        string metadataURI;
        bool verified;
        bool isOriginal;
        bool isImported;
        CBE.LicenseVersion cbeLicense;
        uint256 chainId;
    }
}

