        Holder
    }

    struct AuthorizedLicense {
        address owner;
        uint256 registryId; // for information like licensor, parentLicenses, revoker
        uint256 expiredAt; // epoch timestamp in millisecond
        // decimal is 9
        // i.e. 1,000,000,000 stands for 100% profit distributed to the Authorizer
        uint64 split;
