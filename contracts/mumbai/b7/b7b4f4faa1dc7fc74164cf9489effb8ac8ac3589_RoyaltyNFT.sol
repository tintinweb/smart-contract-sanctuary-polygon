pragma solidity ^0.8.13;
import "./Counter.sol";
import "./RoyaltyNFT.sol";
import "./GambleToken.sol";
import "forge-std/Test.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract Odds is Ownable{
    struct BetInfo
    {
        uint256 odds; //オッズ
        uint256 value; //BETの合計
        uint256 betUserCount; //BETしている総人数計算
        string name; //項目の名称
    }

    struct GameInfo
    {
        uint256 num; //項目数
        uint256 totalValue; //全項目のBET合計値
        uint256 status; //投票可能フラグ
        uint256 deadLine; //締切時間
        string GameName; //BET名称
    }

    struct tokenBetInfo
    {
        uint256 value; //BET学
        uint256 entry; //エントリー済みフラグ
    }

    struct resultInfo
    {
        uint256 value; //払出トークン
        uint256 paidOut; //払い出し済みフラグ
    }

    mapping (uint256=>GameInfo) GameMap;
    mapping (uint256=>mapping(uint256=>BetInfo)) BetMap;
    mapping (uint256=>mapping(uint256=>mapping(uint256=>uint256))) BetUserTokenIdMap;
    mapping (uint256=>mapping(uint256=>mapping(uint256=>tokenBetInfo))) BetValueMap;
    mapping (uint256=>mapping(uint256=>resultInfo)) resultValueMap;

    uint256 private gameCounter;
    RoyaltyNFT nft;

    GambleToken token;

    constructor()
    {
    }

    //NFTコントラクトのアドレスをセット onlyOwner
    function setNFT(address _nft) public onlyOwner
    {
        nft = RoyaltyNFT(_nft);
    }

    //トークンコントラクトのアドレスをセット onlyOwner
    function setToken(address _token) public onlyOwner
    {
        token = GambleToken(_token);
    }

    //新しいBETを作製する関数
    function newRace(string memory name, BetInfo[] memory info,uint256 deadLine) public onlyOwner returns(uint256)
    {
        require(block.timestamp < deadLine,"Ileegal Deadline");

        //ゲームのIDを被らないようにするためgameCounterを加算する
        gameCounter++;
        //gameIdにgameCounterをセット
        uint256 gameId = gameCounter;
        //GameMap構造体に引数の情報をセット
        GameMap[gameId].GameName = name;
        GameMap[gameId].num = info.length;
        GameMap[gameId].status = 1;
        //締切時間設定
        GameMap[gameId].deadLine = deadLine;
        for(uint i = 0; i < GameMap[gameId].num; i++ )
        {
            BetMap[gameId][i].odds = 0;
            BetMap[gameId][i].value = 0;
            BetMap[gameId][i].name = info[i].name;
        }
        //gameIdをリターンする
        return gameId;
    }

    //Bet可能状態を変更する。
    function chgBetStatus(uint256 gameId, uint256 canBet) public onlyOwner
    {
        //作成済みのゲームでnumが0のことはないためリバート
        require(GameMap[gameId].num != 0 ,"don't exist this game");
        //status=2は終了済みであるため変更不可
        require(GameMap[gameId].status < 2 ,"this game alredady finish");
        //canBetは２以上を設定することはできない。
        require(canBet < 2 ,"Invalid value for canBet");
        //canBetをセット
        GameMap[gameId].status = canBet;
    }

    //現在のオッズ情報を取得
    function getNowOdds(uint256 gameId) public view returns(BetInfo[] memory)
    {
        //BetInfo構造体を作製
        BetInfo[] memory info = new BetInfo[](GameMap[gameId].num);
        //現在の情報をセット
        for(uint i = 0; i < GameMap[gameId].num; i++ )
        {
            info[i].name = BetMap[gameId][i].name;
            info[i].value = BetMap[gameId][i].value;
            info[i].odds = BetMap[gameId][i].odds;
        }
        //BetInfo構造体を返す
        return info;
    }

    //別途後の状態を取得
    function getOddsAfterBet(uint256 gameId, uint256 oddsId,uint256 tokenId) public view returns(BetInfo[] memory)
    {
        //トークンIDに紐づいたrarytyをValueとしてセット
        uint256 value = nft.getRarity(tokenId);
        //BET後のBET合計値を変数にセット
        uint256 afterTotalValue = GameMap[gameId].totalValue + value;
        //BetInfo構造体を作製
        BetInfo[] memory info = new BetInfo[](GameMap[gameId].num);
        for(uint i = 0; i < GameMap[gameId].num; i++ )
        {
            info[i].name = BetMap[gameId][i].name;
            info[i].value = BetMap[gameId][i].value;
            if(i == oddsId)
            {
                info[oddsId].value += value;
            }
            if(info[i].value != 0)
            {
                info[i].odds =  1e10*afterTotalValue/info[i].value;
            }
            else
            {
                info[i].odds = 0;
            }
        }
        return info;
    }

    //BET処理、オッズ情報のアップデート
    function newBet(uint256 gameId, uint256 oddsId, uint256 tokenId) public returns(bool)
    {
        require(GameMap[gameId].deadLine >= block.timestamp,"time expired");
        //トークンのステータスがBET済みになっていた場合はリバート
        require(nft.getTokenBetStatus(tokenId) == 0 ,"you already bet this token");
        //指定されたトークンのオーナーが所有者でない場合はリバート
        require(nft.ownerOf(tokenId) == msg.sender ,"you are not this token owner");
        //ゲームの状態がBET可能かどうかをチェックし、不可能な場合はリバート
        require(GameMap[gameId].status == 1 ,"This Game Already Finish");
        //トークンのバリューを取得
        uint256 value = nft.getRarity(tokenId);
        //gameIdのトータルのBET額の更新
        GameMap[gameId].totalValue += value;
        //gameIdのoddsIdのトータルのBET額の更新
        BetMap[gameId][oddsId].value += value;
        //トークンが未エントリーの場合
        if(BetValueMap[gameId][oddsId][tokenId].entry == 0)
        {
            //odd Idの参加者IDのトークンIDをセット
            //結果が出た際にエントリー済みのトークンIdを洗い出すために使用
            BetUserTokenIdMap[gameId][oddsId][BetMap[gameId][oddsId].betUserCount] = tokenId;
            //トークンのエントリーフラグを立てる
            BetValueMap[gameId][oddsId][tokenId].entry = 1;
            //ユーザーIDをインクリメント
            BetMap[gameId][oddsId].betUserCount++;
        }
        //tokenIdに紐づいたValueにBET学を足し込み（複数ベットを不可にしたため+=の必要はない）
        BetValueMap[gameId][oddsId][tokenId].value += value;
        //各項目のオッズの算出
        for(uint i = 0; i < GameMap[gameId].num; i++ )
        {
            if(BetMap[gameId][i].value != 0)
            {
                BetMap[gameId][i].odds =  1e10*GameMap[gameId].totalValue/BetMap[gameId][i].value;
            }
        }
        //NFTをBET済みのステータスに変更
        nft.chgTokenBetStatus(tokenId,1);
        //NFTをアプルーブする
        nft.approveItem(address(this), tokenId);
        return true;
    }

    //BET取消後の情報取得（状態変更はしない）
    function getOddsAfterReset(uint256 gameId ,uint256 tokenId) public view returns(BetInfo[] memory)
    {
        uint256 afterTotalValue;
        BetInfo[] memory info = new BetInfo[](GameMap[gameId].num);
        for(uint i = 0; i < GameMap[gameId].num; i++ )
        {
            info[i].name = BetMap[gameId][i].name;
            info[i].value = BetMap[gameId][i].value;
            if(BetValueMap[gameId][i][tokenId].value > 0)
            {
                //トータルからBET額を原産
                afterTotalValue = GameMap[gameId].totalValue - BetValueMap[gameId][i][tokenId].value;
                //項目ごとのトータルからBET額を減産
                info[i].value -= BetValueMap[gameId][i][tokenId].value;
            }
            if(info[i].value != 0)
            {
                info[i].odds =  1e10*afterTotalValue/info[i].value;
            }
            else
            {
                info[i].odds = 0;
            }
        }
        return info;
    }


    //トークンIDのBETをリセットする
    function oddsResetUpdate(uint256 gameId,uint256 tokenId) public payable returns(bool)
    {
        for(uint i = 0; i < GameMap[gameId].num; i++ )
        {
            //指定したtokenIdのBET情報をトータルから削除
            if(BetValueMap[gameId][i][tokenId].value > 0)
            {
                GameMap[gameId].totalValue -= BetValueMap[gameId][i][tokenId].value;
                BetMap[gameId][i].value -= BetValueMap[gameId][i][tokenId].value;
                BetValueMap[gameId][i][tokenId].value = 0;
            }
            if(BetMap[gameId][i].value != 0)
            {
                BetMap[gameId][i].odds =  1e10*GameMap[gameId].totalValue/BetMap[gameId][i].value;
            }
            else
            {
                BetMap[gameId][i].odds = 0;
            }
        }
        nft.chgTokenBetStatus(tokenId,0);
        return true;
    }

    //結果のセット
    function setResults(uint256 gameId, uint256 oddsId) public onlyOwner
    {
        //払い出しが走った後のため変更不可
        require(GameMap[gameId].status != 3," don't change status");
        uint256 tokenId;
        for(uint i = 0; i < BetMap[gameId][oddsId].betUserCount;i++)
        {
            //当選したトークンIdを取得
            tokenId = BetUserTokenIdMap[gameId][oddsId][i];
            //払出額を計算
            uint256 pay = BetValueMap[gameId][oddsId][tokenId].value*BetMap[gameId][oddsId].odds/1e10;
            //払出額にセット
            resultValueMap[gameId][tokenId].value = pay;
        }
        //結果が出た後のためこれ以降BETY不可
        GameMap[gameId].status = 2;
    }

    //払出処理
    function withDraw(uint256 gameId, uint256 tokenId) public onlyOwner
    {
        //valueが0の場合は払い出しがないためリバート
        require(resultValueMap[gameId][tokenId].value != 0 ,"not withDraw");
        require(GameMap[gameId].status >= 2 ,"not withDraw");
        require(GameMap[gameId].status >= 2 ,"not withDraw");
        require(resultValueMap[gameId][tokenId].paidOut == 0 ,"this result already paidOut");

        //value取得
        uint256 value = resultValueMap[gameId][tokenId].value;
        //valueを０に
        resultValueMap[gameId][tokenId].value = 0;
        //払出済みフラグ
        resultValueMap[gameId][tokenId].paidOut = 1;
        //払出開始済みフラグ
        GameMap[gameId].status = 3;
        //nftのオーナーに対しトークン発行
        token.mintToken(nft.ownerOf(tokenId), value);
        //NFTのバーン
        nft.burnNFT(tokenId);
    }

    //抽選結果をキャンセルする
    function notWithDraw(uint256 gameId, uint256 tokenId) public onlyOwner
    {
        require(resultValueMap[gameId][tokenId].value != 0 ,"not withDraw");
        resultValueMap[gameId][tokenId].value = 0;
    }
}

