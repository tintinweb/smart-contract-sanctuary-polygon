// SPDX-License-Identifier: MIT
pragma solidity ^0.8.17;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./Odds.sol";

contract RoyaltyNFT is ERC721, Ownable, ERC2981  {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    Odds odds;

    //NFT構造体
    struct nftStr
    {
        uint256 rarity; //レアリティ
        uint256 alreadyBet; //BET済みかどうかのチェック
    }

    //mapping
    //tokenIdとNFT構造体のマッピング
    mapping(uint256=>nftStr) _nftStr;

    //Oddsコントラクトからの実行に限定する
    modifier onlyOdds()
    {
        require(msg.sender == address(odds),"only use odds this function");
        _; 
    }

    //コンストラクター
    constructor() ERC721("RoyaltyNFT", "RNFT") 
    {
        //デフォルトのロイヤリティ設定（使用しないけど）
        _setDefaultRoyalty( owner(), 1000 );
    }

    function supportsInterface(bytes4 interfaceId) public view override( ERC721, ERC2981 ) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    //Oddsアドレスをセットする関数 onlyOwner
    function setOdds(address _odds) public onlyOwner
    {
        //Oddsコントラクトのアドレスをセット
        odds = Odds(_odds);
    }

    //NFTの出品の際に呼び出すメソッド
    function approveItem(address spender,uint256 tokenId) public
    {
        _approve(spender,tokenId);
    }

    //NFT購入用関数
    function buyNFT() payable public returns (uint256)
    {
        //トークンが1000単位でなければリバート
        require( msg.value % 1000 == 0 ,"Please put the number in units of 1000");
        uint256 kind;
        //種別判別
        kind = msg.value / 1000;
        //1,3,5の種別は存在し、それ以外は存在しないためリバート
        if(kind == 1 || kind == 3 || kind == 5)
        {
            //レアリティを設定してNFTを発行
            return mintItem(msg.sender, kind);
        }
        else
        {
            revert("value is not suitable");
        }
    }

    //レアリティの設定とNFTのミント
    function mintItem(address to, uint256 kind) internal
        returns (uint256)
    {
        _tokenIds.increment();

        uint256 id = _tokenIds.current();
        _mint(to, id);
        _nftStr[id].rarity = kind;
        return id;
    }

    //レアリティの設定とNFTのミント
    function burnNFT(uint256 tokenId) public
    {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: caller is not token owner or approved");
        _burn(tokenId);
    }

    //トークンのレアリティを取得
    function getRarity(uint256 tokenId) public view returns (uint256)
    {
        return _nftStr[tokenId].rarity; 
    }

    //トークンのBET状態を確認する関数
    function getTokenBetStatus(uint256 tokenId) public view returns(uint256)
    {
        return _nftStr[tokenId].alreadyBet;
    }

    //トークンのBET状態を設定する関数
    function chgTokenBetStatus(uint256 tokenId, uint256 status) public onlyOdds
    {
        _nftStr[tokenId].alreadyBet = status;
    }

    //トークンのロイヤリティを設定する関数
    function SetTokenRoyalty(uint256 tokenId, address receiver, uint96 feeNumerator)  public
    {
        _setTokenRoyalty(tokenId, receiver, feeNumerator);
    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://ikzttp.mypinata.cloud/ipfs/QmQFkLSQysj94s5GvTHPyzTxrawwtjgiiYS2TBLgrvw8CW/";
    }
}

