// SPDX-License-Identifier: MIT
pragma solidity ^0.8.14;
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "@openzeppelin/contracts/access/Ownable2Step.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract SSNFTMintContractPolygon is ERC721URIStorage, Ownable2Step, EIP712, AccessControl, ReentrancyGuard{
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE"); 
    string private constant SIGNING_DOMAIN = "SSNFT-Voucher";
    string private constant SIGNATURE_VERSION = "1";

    uint256 m_royalty = 1000; // 10%
    uint256 m_quantity = 100;// max quantity
    uint256 public firstSaleFee = 800;//0.08 = 8%

    uint256 public units = 10000;
    uint256 public decimal = 10 ** 18;

    address public manager; // developers
    address public minter;

    mapping(uint256 => TokenInfo) public nftList;
    mapping(bytes32 => bool) usedSignatures;

    struct TokenInfo {
        uint256 tokenId;
        uint256 royalty;
        address creatorAddress;
        address minter;
        string tokenUrl;
    }

    struct NFTVoucher {
        address creator;
        address recipient;
        uint256 tokenId;
        uint256 royalty;
        uint256 price;
        string uri;
        bytes signature;
    }

    event SetOwnersEvent(address owner, bool active);
    event SetArtistsEvent(
        address creator,
        bool status
    );
    event NFTMintEvent(
        uint256 mintedTokenId,
        uint256 royalty,
        address creatorAddress,
        string tokenUrl
    );
    event UpdateRoyaltyEvent(uint256 tokenId, uint256 royalty);

    constructor() ERC721("SS-NFT", "SeibuSOGO")EIP712(SIGNING_DOMAIN, SIGNATURE_VERSION){
        require(msg.sender != address(0));
        manager = msg.sender;
    }
    
    /* Mints a token */
    function mintToken(NFTVoucher calldata voucher) external payable  nonReentrant{
        bytes32 messageHash = keccak256(abi.encodePacked(voucher.signature, msg.sender));
        require(!usedSignatures[messageHash], "Signature already used");
        require(msg.sender != address(0),'Invalid address');
        require(voucher.creator != address(0),'Invalid address');
        require(voucher.recipient != address(0),'Invalid address');
        require(m_royalty >= voucher.royalty,"Overflow max royalty");
        require(!_exists(voucher.tokenId), "tokenId already exists");
        // make sure signature is valid and get the address of the signer
        address signer = _verify(voucher);
        // make sure that the signer is authorized to mint NFTs
        require(hasRole(MINTER_ROLE, signer), "Signature invalid or unauthorized");
        //check Minter Address
        require(compareAddresses(voucher.recipient, msg.sender),'You are not signed Minter'); 
        uint256 price = voucher.price * decimal / units;
        require(msg.value >= price, "Insufficient funds.");
        _safeMint(voucher.recipient, voucher.tokenId);
        _setTokenURI(voucher.tokenId, voucher.uri);
        //Calculate Fees
        uint256 platformFee = msg.value * firstSaleFee / units;
        uint256 creatorAmount = msg.value - platformFee;
        //Send to Creator
        payable(voucher.creator).transfer(creatorAmount);
        nftList[voucher.tokenId] = TokenInfo(
            voucher.tokenId,
            voucher.royalty,
            voucher.creator,
            msg.sender,
            voucher.uri
        );        
        usedSignatures[messageHash] = true;
        emit NFTMintEvent(
            voucher.tokenId,
            voucher.royalty,
            voucher.recipient,
            voucher.uri
        );
    }

    function setMinterRole(address _addr) external onlyOwner{
        require(_addr != address(0));
        if(minter != address(0)){
            revokeRole(MINTER_ROLE, minter);
        }
        _setupRole(MINTER_ROLE, _addr);
        minter = _addr;
    }   

    function updateRoyalty(uint256 _tokenId, uint256 _royalty) external{
        TokenInfo memory tokenInfo = nftList[_tokenId];
        require(tokenInfo.creatorAddress == msg.sender, "UnAuthorized Owner");
        require(m_royalty >= _royalty,"Overflow max royalty");
        nftList[tokenInfo.tokenId].royalty = _royalty;
        emit UpdateRoyaltyEvent(_tokenId, _royalty);
    }

    // get Info functions
    function getManager() external view returns (address){
        return manager;
    }      

    function getRoyalty(uint256 _tokenId) external view returns(uint256){
        return nftList[_tokenId].royalty;
    }

    function getCreator(uint256 _tokenId) external view returns(address){
        return nftList[_tokenId].creatorAddress;
    }

    function getMinter(uint256 _tokenId) external view returns(address){
        return nftList[_tokenId].minter;
    }    

    function supportsInterface(bytes4 interfaceId) public view virtual override (AccessControl, ERC721) returns (bool) {
        return ERC721.supportsInterface(interfaceId) || AccessControl.supportsInterface(interfaceId);
    }

    function _hash(NFTVoucher calldata voucher) internal view returns (bytes32) {
        return _hashTypedDataV4(keccak256(abi.encode(
        keccak256("NFTVoucher(address creator,address recipient,uint256 tokenId,uint256 royalty,uint256 price,string uri)"),
        voucher.creator,
        voucher.recipient,
        voucher.tokenId,
        voucher.royalty,
        voucher.price,
        keccak256(bytes(voucher.uri))
        )));
    }

    function _verify(NFTVoucher calldata voucher) internal view returns (address) {
        bytes32 digest = _hash(voucher);
         return ECDSA.recover(digest, voucher.signature);
    }

    function getChainID() external view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }

    function acceptManager() external {
        address sender = _msgSender();
        require(pendingOwner() == sender, "Ownable2Step: caller is not the new owner");
        _transferOwnership(sender);
        manager = sender;
    }    

         //withdraw ether 
     function withdrawPlatFormFee() payable external onlyOwner{
        address payable to = payable(msg.sender);
        uint256 amount = address(this).balance;
        require(amount != 0, "Insufficient Error");
        to.transfer(amount);
     }

    function compareAddresses(address addr1, address addr2) internal pure returns(bool) {
        bytes20 b1 = bytes20(addr1);
        bytes20 b2 = bytes20(addr2);
        bytes memory b1hash = abi.encodePacked(ripemd160(abi.encodePacked(b1)));
        bytes memory b2hash = abi.encodePacked(ripemd160(abi.encodePacked(b2)));
        return keccak256(b1hash) == keccak256(b2hash);
    }  
}

// SPDX-License-Identifier: GPL-3.0
pragma solidity ^0.8.14;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./polygonNFTContract.sol";

contract PolygonSSNFTMarketplace is ReentrancyGuard, SSNFTMintContractPolygon {
    using Counters for Counters.Counter;
    Counters.Counter private _itemCounter; // start from 1

    struct MarketItem {
        uint256 tokenId;
        uint256 price;
        address owner;
        bool onSale;
    }

    event ItemListedEvent(
        uint256 tokenId,
        uint256 price,
        address owner,
        bool onSale
    );

    event ItemCanceled(
        uint256 tokenId,
        address owner,
        bool onSale
    );

    event ItemBoughtEvent(
        uint256 tokenId,
        uint256 price,
        address owner,
        address oldOwner,
        bool onSale
    );

    mapping(uint256 => MarketItem) public marketItems; // tokenId --> NFT Info
    mapping(uint256 => bool) public exists; // tokenId --> bool; 
    address payable public marketowner;

    uint256 public secondSaleFee = 500;//0.05 = 5%
    uint256 public maxPrice = 10000000; // 10000ETH

    SSNFTMintContractPolygon NFTContract;

    modifier isExisted(uint256 _tokenId) {
    require(exists[_tokenId], "token not existed");
        _;
    }

    constructor(address _NFTaddr) {
        require(_NFTaddr != address(0));
        marketowner = payable(msg.sender);
        NFTContract = SSNFTMintContractPolygon(_NFTaddr);
    }

    function listItem(
        uint256 _tokenId,
        uint256 _price
    ) external{
        // check msg.sender && price != 0;        
        require(msg.sender != address(0));
        require(_price > 0,'Price is not zero');
        require(_price < maxPrice, 'Overflow 1000 ETH');
        //check token onwer
        address tokenOwner = NFTContract.ownerOf(_tokenId);
        require( tokenOwner == msg.sender, "Not Owner!");
        
        if(!exists[_tokenId]){
        // total amount ++
        _itemCounter.increment();
        NFTContract.transferFrom(msg.sender, address(this), _tokenId);
        marketItems[_tokenId] = MarketItem(
            _tokenId,
            _price * decimal / units,
            msg.sender,
            true
        );
        exists[_tokenId] = true;
        emit ItemListedEvent(        
            _tokenId,
            _price,
            msg.sender,
            true
            );
        }else {
            // total amount ++
            _itemCounter.increment();
            /** already registered NFT */
            marketItems[_tokenId].price =_price * decimal / units;
            marketItems[_tokenId].owner = msg.sender;
            marketItems[_tokenId].onSale = true;
            NFTContract.transferFrom(msg.sender, address(this), _tokenId);
            exists[_tokenId] = true;
            emit ItemListedEvent(        
                _tokenId,
                _price,
                msg.sender,
                true
                );
        }
    }
    /* Deletes the sale of a marketplace item*/
    function cancelListing(uint256 _tokenId) external isExisted(_tokenId){
        require(marketItems[_tokenId].owner == msg.sender, "UnAuthorized Access!");
        NFTContract.transferFrom(address(this), msg.sender, _tokenId);
        // total amount --
        _itemCounter.decrement();
        marketItems[_tokenId].onSale = false;
        emit ItemCanceled(
            _tokenId,
            msg.sender,
            false
            );
    }

    /* Creates the sale of a marketplace item */
     function buyItem(uint256 _tokenId) external payable isExisted(_tokenId){
        // check if NFT is on sale
        require(marketItems[_tokenId].onSale, "NFT is not on sale.");
        //check caller && market item exist
        require(msg.sender != address(0));

        // get info creatir, price, owner, royalty
        address creator = NFTContract.getCreator(_tokenId);
        uint256 price = marketItems[_tokenId].price; 
        address owner = marketItems[_tokenId].owner;
        uint256 royalty = NFTContract.getRoyalty(_tokenId);

        //check price and owner is correct
        require(msg.value >= price, "Insufficient funds.");
        require(msg.sender != owner, "Owner can not buy");

        NFTContract.transferFrom(address(this), msg.sender, _tokenId);

        //Calculate Fees
        uint256 platformFee = msg.value * secondSaleFee / units;
        uint256 royaltyFee = msg.value * royalty / units; 
        uint256 sellerAmount = msg.value - platformFee - royaltyFee;
        
        //Send to Creator
        payable(creator).transfer(royaltyFee);

        //Send to Seller
        address sendTo = marketItems[_tokenId].owner;
        payable(sendTo).transfer(sellerAmount);


        // get old owner
        address oldOwner=  marketItems[_tokenId].owner;
        // Market Item State
        marketItems[_tokenId].onSale = false;
        marketItems[_tokenId].owner = msg.sender;

        //Toal amount --
        _itemCounter.decrement();

        emit ItemBoughtEvent(
            _tokenId,
            price,
            msg.sender,
            oldOwner,
            false
        );
     }
     //Set platform Fee
     function setPlatformFee(uint256 _secondSaleFee) external onlyOwner{
        require(_secondSaleFee != 0);
        secondSaleFee = _secondSaleFee;
     }

     //withdraw ether 
     function withdrawProceeds() payable external onlyOwner{

        address payable to = payable(msg.sender);
        uint256 amount = address(this).balance;
        require(amount != 0, "Insufficient Error");
        to.transfer(amount);
     }

     //Get Total Amount
     function getMarketTotalAmount () external view returns(uint256){
        return _itemCounter.current();
     }
}