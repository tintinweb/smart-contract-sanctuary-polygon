        // APPROVE OFF-CHAIN
        //Project Deposit with assigment profit holders
        function projectProfitDeposit(uint256 _id, uint256 _amount)
        public
        nonReentrant
        onlyRole(CAPITAL_MANAGER)
        {   
            
            require(projectStructs[_id].projectActive,
             "Project is not activated");
            (,,uint256 feePS, uint256 feeU) = calculate(_amount);
            uint256 length = projectStructs[_id].projectHolders.length;
            if(projectStructs[_id].isProjectPassive){
            cUsd.safeTransferFrom(msg.sender,address(this),_amount + feePS);
            projectStructs[_id].FeeRecipientOfPlatform += feePS;
            for (uint i=0;i<length;i++){
                address payable addr = projectStructs[_id].projectHolders[i];
                (uint256 ownerShares,,,,) = UsersContract.getInfoInvestorByAddress(_id,addr);
                uint256 ownerPercentage = _amount*ownerShares/projectStructs[_id].projectTotalSupply;
                UsersContract.increaseGains(_id,addr,ownerPercentage);
                cUsd.safeIncreaseAllowance(addr,ownerPercentage);
                }
            }
            else{
            cUsd.safeTransferFrom(msg.sender,address(this),_amount + feeU);
            projectStructs[_id].FeeRecipientOfPlatform += feeU;
            for (uint i=0;i<length;i++){
                address payable addr = projectStructs[_id].projectHolders[i];
                (uint256 ownerShares,,,,) = UsersContract.getInfoInvestorByAddress(_id,addr);
                uint256 ownerPercentage = _amount*ownerShares/projectStructs[_id].projectTotalSupply;
                UsersContract.increaseGains(_id,addr,ownerPercentage);
                cUsd.safeIncreaseAllowance(addr,ownerPercentage);
                }
            }
        }

        function projectProfitDistributionByOwnerCall
        (uint256 _id)
        public
        {
            (,uint256 _profit,,bool _proof,) = UsersContract.getInfoInvestorByAddress(_id,msg.sender);
            require(_proof,"You are not holder project");
            cUsd.safeTransfer(msg.sender,_profit);
            cUsd.safeDecreaseAllowance(msg.sender,_profit);
            UsersContract.callToClaimProfit(_id,msg.sender,_profit);
            
        }
        


