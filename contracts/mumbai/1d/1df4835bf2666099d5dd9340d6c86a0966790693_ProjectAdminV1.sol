                    (uint256 ownerShares,,,,) = UsersContract.getInfoInvestorByAddress(_id,addr);
                    uint256 ownerPercentage = _amount*ownerShares/projectStructs[_id].projectTotalSupply;
                    cUsd.safeTransfer(addr,ownerPercentage);
                    UsersContract.increaseGains(_id,addr,ownerPercentage);
                    UsersContract.callToClaimProfit(_id,addr,ownerPercentage);
                }
                }
                else {

                cUsd.safeTransferFrom(msg.sender,address(this),_amount + feeU);
