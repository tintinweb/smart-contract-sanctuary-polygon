pragma solidity ^0.8.13;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

// Interfaces
import "./interfaces/pharo/IPharoCover.sol";
import "./interfaces/pharo/IPHROToken.sol";
import "./interfaces/pharo/IPharoMarket.sol";
// import "./interfaces/pharo/IPharoPhinance.sol";

// Libraries
import "./utility/PharoConstants.sol";
import "./utility/PharoEmits.sol";
import "./utility/PharoRewards.sol";

contract PharoMarket is IPharoMarket, PharoEmits, PharoRewards, Ownable, AccessControl
{
    using Counters for Counters.Counter;

    address public constant pharoHolder = 0x3f15B8c6F9939879Cb030D6dd935348E57109637;
    IPharoCover public pharoCover;

    Counters.Counter private pharoIdsCount;
    uint256[] pharoIds;
    mapping(uint256 => Pharo) public pharos;

    uint256[] public trueEventTimes;
    Counters.Counter public _eventIds;
    mapping(uint256 => PharoConstants.SignedObelisk) public obelisk;
    mapping(uint256 => PharoConstants.SignedWoC) private WoC;

    // { 0: MUMMY, 1: PHARO, 2: IMHOTEP, 3: ANUBIS, 4: OBELISK }
    enum PharoState
    {
        MUMMY,
        PHARO,
        IMHOTEP,
        ANUBIS,
        OBELISK
    }

    struct Pharo
    {
        uint256 id; // the id of the Pharo
        //uint256 nftId; // id of the NFT representing the Pharo
        string name; // the name of the Pharo
        string description; // a description for the Pharo
        uint256 lifetime; // Pharo lifetime in seconds
        uint256 birthday; // epoch date the Pharo was created
        PharoState state; // the state of the Pharo
        //uint256 liqBalance; // the balance of the Pharo's liquidity
    }

    /// @dev Require Pharo to be in a certain state to call function
    modifier inPharoStateObelisk(PharoState _state) {
        require(_state == PharoState.OBELISK);
        _;
    }

    /// @dev Require Pharo to be in a certain state to call function
    modifier inPharoStateAnubis(PharoState _state) {
        require(_state == PharoState.ANUBIS);
        _;
    }

    /// @dev Require Pharo to be in a certain state to call function
    modifier inPharoStatePharo(PharoState _state) {
        require(_state == PharoState.PHARO);
        _;
    }

    /// @dev Require Pharo to be in a certain state to call function
    modifier inPharoStateImhotep(PharoState _state) {
        require(_state == PharoState.IMHOTEP);
        _;
    }

    /// @dev Require Pharo to be in a certain state to call function
    modifier inPharoStateMummy(PharoState _state) {
        require(_state == PharoState.MUMMY);
        _;
    }

    mapping(bytes32 => mapping(uint256 => Pharo))
        public eventHashToPharoIdToPharo;

    constructor(address _PharoCoverAddress) {
        pharoCover = IPharoCover(_PharoCoverAddress);
        _setupRole(PharoConstants.OPERATOR_ROLE, msg.sender);
    }

    function grantOperatorRole(address newOperator) public onlyOwner {
        _setupRole(PharoConstants.OPERATOR_ROLE, newOperator);
    }

    function confirmPharo(uint256 pharo_id) external view returns (bool confirmed)
    {
        for (uint i = 0; i < pharoIdsCount.current(); i++)
        {
            if (i == pharo_id)
            {
                PharoState state = pharos[pharo_id].state;
                confirmed = (state == PharoState.MUMMY || state == PharoState.PHARO);
                break;
            }
        }
    }

    function getPharoIds() public view returns(uint256[] memory)
    {
        return pharoIds;
    }

    function _verifyObeliskVsPolicies(PharoConstants.SignedObelisk memory tObelisk) internal view returns(bool verified)
    {
        for (uint32 bx = 0; bx < tObelisk.cb_policy_id.length; bx++)
        {
            verified = pharoCover.verifyPolicy(tObelisk.pharo_id,
                                               tObelisk.cb_policy_id[bx],
                                               tObelisk.cb_premium[bx],    // aka stake
                                               tObelisk.cb_rate_estimate[bx],
                                               tObelisk.cb_min_cover[bx]);
            if (!verified)
                return false;
        }

        return true;
    }

    function createPharo(uint256 lifetime,
                         string memory name,
                         string memory description/*,
                         string memory tokenAddress*/) external returns(bool success, uint256 pharoId)
    {
        if (!hasRole(PharoConstants.OPERATOR_ROLE, msg.sender)) revert WrongRole();

        pharoId = pharoIdsCount.current();
        pharos[pharoId] = Pharo(pharoId, name, description, lifetime, block.timestamp, PharoState.MUMMY);
        
        pharoIds.push(pharoId);
        pharoIdsCount.increment();
        
        success = true;
        //todo: also, implement the destruction/invalidation of Pharos in the collection
    }

    // PharoConstants.SignedPolicy memory sPolicy
    /// @notice To be called by the Pharo Anubis API only.
    /// Usually called in response to the Pharo Event happening,
    /// or to account for premiums collected by each LP for display or withdraw.
    function updateMarkets(uint256 pharo_id,
                           PharoConstants.SignedObelisk memory sObelisk,
                           PharoConstants.SignedWoC memory sWoC) external returns(bool success)
    {
        if(!_verifyObeliskVsPolicies(sObelisk)) emit APIOutOfSync();

        obelisk[pharo_id] = sObelisk;
        WoC[pharo_id] = sWoC;

        for (uint256 px=0; px < sObelisk.cb_cover_bought.length; px++)
        {
            success = pharoCover.updatePolicy(pharo_id, 
                                              sObelisk.cb_policy_id[px], 
                                              sObelisk.cb_cover_bought[px], 
                                              sObelisk.cb_reward[px]);
            if (!success) emit PolicyUpdateError();
        }

        // for (uint256 px=0; px < sObelisk.cb_cover_bought.length; px++){
        //     success = pharoLiquidity.updateProducts();
        //     if (!success) emit ProductUpdateError();
        // }

        success = actOnUpdatedMarket(pharo_id);
        if (!success) emit MarketUpdateError();

        return true;
    }

    // todo: verify API results by checking with this algorithm.
    function _sumCoverBought(uint256[] memory cbxx_srvd_value)
        private
        returns (uint256 coverAwarded)
    {
        coverAwarded = 0;
        for (uint32 mx = 0; mx < cbxx_srvd_value.length; mx++) {
            unchecked {
                coverAwarded = coverAwarded + cbxx_srvd_value[mx];
            }
        }
        emit CoverCalculation(coverAwarded);

        return coverAwarded;
    }

    function calcPolicyRewards(uint256 pharo_id, uint32 rate_truth)
        internal returns (bool success)
    {
        uint256 total_reward;

        _eventIds.increment();
        trueEventTimes[_eventIds.current()] = rate_truth;

        // for (uint32 bx = 0; bx < obelisk[pharo_id].buyerList.length; bx++) {
        for (uint32 bx = 0; bx < obelisk[pharo_id].cb_policy_id.length; bx++) {
            total_reward += calcReward(
                obelisk[pharo_id].cb_rate_estimate[bx],
                obelisk[pharo_id].cb_premium[bx],
                rate_truth,
                WoC[pharo_id].gof,
                WoC[pharo_id].alpha,
                WoC[pharo_id].beta,
                WoC[pharo_id].gamma_x,
                WoC[pharo_id].gamma_y);

            obelisk[pharo_id].cb_reward[bx] = total_reward;
        }

        return true;
    }

    function _calcPremium(uint256[] memory cb_srvd_value) internal pure returns (uint256 funds_needed)
    {
        for (uint256 px=0; px < cb_srvd_value.length; px++)
        {
            funds_needed += cb_srvd_value[px];
        }
    }

    function actOnUpdatedMarket(uint256 pharo_id) internal returns (bool)
    {
        bool success; //iaro: this flag isn't properly used throughout the method
        uint256 premium2pay;

        bool rebalance_market = pharoCover.verifyPolicyFunds(pharo_id);
        if (rebalance_market) { return false; } 
        else {
            for (uint256 cx = 0; cx < obelisk[pharo_id].cb_policy_id.length; cx++)
            {       
                premium2pay = _calcPremium(obelisk[pharo_id].cb_srvd_value[cx]);
                success = pharoCover.updatePolicyPremiumPaid(pharo_id, 
                                                             obelisk[pharo_id].cb_policy_id[cx], // policy_id
                                                             premium2pay); // record how much they've paid.
                if (!success) {
                    emit UpdatePolicyPremiumPaidError(
                        pharo_id, 
                        obelisk[pharo_id].cb_policy_id[cx], 
                        premium2pay, 
                        "Execution Failed");
                }

                // for (uint256 px = 0; px < obelisk[pharo_id].cb_srvd_product_id[cx].length; px++) {
                //     success = pharoLiquidity.updateProductPremiumReceived(
                //         pharo_id, 
                //         obelisk[pharo_id].cb_srvd_product_id[cx][px], // product id
                //         obelisk[pharo_id].cb_srvd_value[cx][px]); // record how much they've collected.

                //     if (!success) {
                //         emit UpdateProductPremiumReceivedError(
                //             pharo_id, 
                //             obelisk[pharo_id].cb_srvd_product_id[cx][px], 
                //             obelisk[pharo_id].cb_srvd_value[cx][px], 
                //             "Execution Failed");
                //     }
                // }
            }
            success = pharoCover.payPremiums(pharo_id);
        }
        return true;
    }

    function pharoEventTriggered(uint256 pharo_id, uint32 trueEventTime) 
        external returns(bool success)
    {
        // todo: add access control
        success = calcPolicyRewards(pharo_id, trueEventTime);
        if (!success) { emit RewardCalculationError(pharo_id, trueEventTime); }
        else {
            for (uint256 cx=0; cx < obelisk[pharo_id].cb_reward.length; cx++)
            {
                success = pharoCover.updatePolicyEvent(pharo_id,
                                                       obelisk[pharo_id].cb_policy_id[cx], 
                                                       obelisk[pharo_id].cb_reward[cx], 
                                                       obelisk[pharo_id].cb_cover_bought[cx], 
                                                       trueEventTime);
                if (!success)
                { 
                  emit PolicyUpdateError(pharo_id,
                                         obelisk[pharo_id].cb_policy_id[cx], 
                                         obelisk[pharo_id].cb_reward[cx], 
                                         obelisk[pharo_id].cb_cover_bought[cx], 
                                         trueEventTime);
                }
            }
        }

        success = pharoCover.collectCover(pharo_id);
    }
}

