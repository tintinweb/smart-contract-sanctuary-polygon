/// @title Pharo Rewards Contract
/// @author Aphilos, Jaxcoder
/// @notice Calculates user reward from position and event truth
/// @dev Full cover calculations will live here after the IDO...
abstract contract PharoRewards { // is PharoConstants {
  // https://www.pnw.edu/wp-content/uploads/2020/03/Lecture-Notes-7.pdf
  using SafeERC20 for IERC20;
  using SafeMath for uint256;

  event UserReward (uint256 reward);
 
  struct PctlDelta {
    uint32 est_mean_pctl_delta;
    uint32 est_truth_pctl_delta;
    uint32 truth_mean_pctl_delta;
  }
  struct PctlActors {
    uint32 estimate_pctl;
    uint32 rate_mean_pctl;
    uint32 truth_pctl;
  }
  
  /// @notice logic and calculations

  /// @notice Calculate Percentile
  /// @param rate the rate
  /// @param gamma_x array of gamma x
  /// @param gamma_y array of gamma y
  /// @return ratePercentile 
  function calcPercentile (
    uint32 rate,
    uint32[] memory gamma_x,
    uint32[] memory gamma_y
  )
    internal
    pure
    returns(uint32 ratePercentile)
  {

    uint16 rx;
    uint32 rateDeltaY;
    // estimate_pctl = gamma.cdf(rate_est, alpha, scale=beta)
    // Get the index where this rate estimate lies between.
    for (uint16 xx = 1 ; xx < gamma_x.length; xx++) {
        if ((gamma_x[xx - 1] <= rate) && (gamma_x[xx] >= rate)) {
            rx = xx;
            break;
        }
    }
    uint32 rateDeltaXPcnt = (rate - gamma_x[rx-1]) / (gamma_x[rx] - gamma_x[rx-1]);
    rateDeltaY = rateDeltaXPcnt * (gamma_y[rx] - gamma_y[rx-1]);

    // Calculate the sum to that index.
    uint32 rateSum = 0;
    uint32 totalSum = 0;
    uint32 remainder = 0;
    bool continueRateSum = true;
    for (uint16 xx = 0 ; xx < gamma_x.length; xx++) {
        if (continueRateSum) {
            if ((xx == rx) && (xx > 0)) {
                rateSum = rateSum + rateDeltaY;
                remainder = gamma_y[xx] - (gamma_y[xx - 1] + rateDeltaY);
                continueRateSum = false;
            } else {
                rateSum = rateSum + gamma_y[xx];
            }
        } 
        totalSum = totalSum + gamma_y[xx];
    }
    ratePercentile = rateSum / totalSum;
    
    return ratePercentile;
  }
  
  /// @notice Calculate Reward
  /// @param rateEstimate Rate estimate
  /// @param stakeAtRisk The stake at risk
  /// @param rateTruth The rate of truth
  /// @param gof Goodness of fit
  /// @param alpha Alpha
  /// @param beta Beta
  /// @param gamma_x Array of gamma x
  /// @param gamma_y Array of gamma y
  /// @return reward The reward amount in real time
  function calcReward
  (
    uint32 rateEstimate,
    uint256 stakeAtRisk,
    uint32 rateTruth,
    uint32 gof,
    uint32 alpha,
    uint32 beta,
    uint32[] memory gamma_x,
    uint32[] memory gamma_y
  ) 
    public
    returns(uint256 reward) 
  {
      uint256 score = calcScore( rateEstimate, rateTruth, uint16(gof), uint16(alpha), uint16(beta), gamma_x, gamma_y );
      reward = stakeAtRisk * score; // GoF

      console.log(" ");
      console.log("    calcReward()");
      console.log("    rateEstimate: ",rateEstimate);
      console.log("    rateTruth:    ",rateTruth);
      console.log("    gof:          ",gof);
      console.log("    alpha:        ",alpha);
      console.log("    beta:         ",beta);
      // for (uint256 xx = 0; xx < gamma_x.length; xx++) {
      // console.log("    gamma_x:      ",gamma_x[xx]); }
      // for (uint256 xx = 0; xx < gamma_y.length; xx++) {
      // console.log("    gamma_y:      ",gamma_y[xx]);
      console.log("    Score:        ",score);
      console.log("    Stake:        ",stakeAtRisk);
      console.log(" ");

      emit UserReward(reward);

      return reward;
  }

  /// @notice Calculate Score
  /// @param rateEstimate Rate estimate
  /// @param rateTruth The rate of truth
  /// @param gof Goodness of fit
  /// @param alpha Alpha
  /// @param beta Beta
  /// @param gamma_x Array of gamma x
  /// @param gamma_y Array of gamma y
  /// @return score The score in real time
  function calcScore
  (
    uint32 rateEstimate,
    uint32 rateTruth,
    uint16 gof,
    uint16 alpha,
    uint16 beta,
    uint32[] memory gamma_x,
    uint32[] memory gamma_y
  ) 
    public view
    returns(uint32 score)
  {
    PctlDelta memory pctlDelta;
    PctlActors memory pctlActors;

    console.log("Calculating Percentiles");
    
    pctlActors.estimate_pctl  = calcPercentile( rateEstimate, gamma_x, gamma_y );
    console.log("Estimate Percentile: ",pctlActors.estimate_pctl);
    pctlActors.rate_mean_pctl = calcPercentile( alpha * beta, gamma_x, gamma_y );
    console.log("Estimate Percentile: ",pctlActors.rate_mean_pctl);
    pctlActors.truth_pctl     = calcPercentile( rateTruth   , gamma_x, gamma_y );
    console.log("Estimate Percentile: ",pctlActors.truth_pctl);

    pctlDelta = calcPctlDeltas(pctlActors);
    console.log("Deltas: ",pctlDelta.est_mean_pctl_delta);
    console.log("Deltas: ",pctlDelta.est_truth_pctl_delta);
    console.log("Deltas: ",pctlDelta.truth_mean_pctl_delta);

    score = calcRiskScore( pctlDelta);
    console.log("calcRiskScore: ", score);
    return score;
  }

  /// @dev uint32 estimate_pctl, uint32 rate_mean_pctl, uint32 truth_pctl
  /// @param pctlActors Actors
  /// @return pctlDelta The percentile delta
  function calcPctlDeltas
  (
    PctlActors memory pctlActors
  )
    private
    pure
    returns(PctlDelta memory pctlDelta)
  {
    pctlDelta.est_mean_pctl_delta   = calcAbsDiff32( pctlActors.estimate_pctl, pctlActors.rate_mean_pctl );
    pctlDelta.est_truth_pctl_delta  = calcAbsDiff32( pctlActors.estimate_pctl, pctlActors.truth_pctl     );
    pctlDelta.truth_mean_pctl_delta = calcAbsDiff32( pctlActors.truth_pctl,    pctlActors.rate_mean_pctl );
    return pctlDelta;
  }

  /// @dev uint32 est_mean_pctl_delta, uint32 est_truth_pctl_delta, uint32 truth_mean_pctl_delta
  /// @param pctlDelta Percent delta
  /// @return score Returns the risk score in real time
  function calcRiskScore
  (
    PctlDelta memory pctlDelta
  )
    private
    pure
    returns(uint32 score)
  {

    uint32 user_risk;
    uint32 truth_risk;
    uint32 user_score;
    uint32 crowd_score;

    user_risk = 2 * pctlDelta.est_mean_pctl_delta;
    // if (user_risk > (90 / 100)) { user_risk = 1; }
    
    truth_risk = 2 * pctlDelta.est_truth_pctl_delta;
    // if (truth_risk > (90 / 100)) { truth_risk = 1; }

    user_score = 1 - truth_risk;

    crowd_score = 2 * pctlDelta.truth_mean_pctl_delta;
    // if (crowd_score > (90 / 100)) { crowd_score = 1; }

    // uint256 gof100 = (100 - gof);
    score = user_risk * user_score * crowd_score; // gof100 * 
    return score;
  }
  
  /// @notice calulate AbsDiff32
  /// @param val1 the first value
  /// @param val2 the second value
  /// @return delta Returns the delta of the two values
  function calcAbsDiff32(uint32 val1, uint32 val2) public pure returns(uint32 delta) {

      if (val1 > val2) { 
          delta = val1 - val2;
      } else {
          delta = val2 - val1;
      }
      return delta;
  }
}

