contract MinimalForwarder is EIP712 {
    using ECDSA for bytes32;
    using StringStorage for StringStorage.Set;

    struct ForwardRequest {
        string txType;
        string description;
        address from;
        address to;
        address contractAddr;
        uint256[] tokenID;
        uint256[] amount;
        uint256 gas;
        string onetimestr;
    }

    string public constant TYPETRANSFERERC721 = "transferERC721";
    string public constant TYPEMINTERC721 = "batchMintERC721";
    string public constant TYPEMINTERC1155 = "batchMintERC1155";
    string public constant TYPETRANSFERERC1155 = "transferERC1155";
    string public constant TYPEAPPROVEERC1155 = "approveERC1155";
    string public constant TYPEAPPROVEERC721 = "approveERC721";
    string public constant TYPEAPPROVEBATCHERC721 = "batchApproveERC721";
    string public constant TYPEAPPROVEBATCHERC1155 = "batchApproveERC1155";
    string public constant TYPEREVOKEERC721 = "revokeERC721";
    string public constant TYPEREVOKEERC1155 = "revokeERC1155";
    string public constant TYPEBURNERC1155 = "burnERC1155";
    string public constant TYPEBURNERC721 = "burnERC721";


    bytes32 private constant _TYPEHASH =
    keccak256("ForwardRequest(string txType,string description,address from,address to,address contractAddr,uint256[] tokenID,uint256[] amount,uint256 gas,string onetimestr)");

    mapping(address => StringStorage.Set) private _onetimestr;

    constructor() EIP712("Pixchange Transactions", "0.0.1") {}

    function verify(ForwardRequest calldata req, bytes calldata signature) public view returns (bool) {
        if (_hashTypedDataV4(
            keccak256(bytes.concat(
                abi.encode(
                    _TYPEHASH,
                    keccak256(abi.encodePacked(req.txType)),
                    keccak256(abi.encodePacked(req.description)),
                    req.from,
                    req.to,
                    req.contractAddr
                ), abi.encode(
                    keccak256(abi.encodePacked(req.tokenID)),
                    keccak256(abi.encodePacked(req.amount)),
                    req.gas,
                    keccak256(abi.encodePacked(req.onetimestr))
                ))
            )
        ).recover(signature) != req.from) {
            return false;
        }
        return !_onetimestr[req.from].contains(req.onetimestr);
    }

    function _isTypeExists(string memory txType) public returns (bool){
        return (
        _compareStr(txType, TYPEMINTERC721) ||
        _compareStr(txType, TYPETRANSFERERC721) ||
        _compareStr(txType, TYPEMINTERC1155) ||
        _compareStr(txType, TYPETRANSFERERC1155) ||
        _compareStr(txType, TYPEBURNERC1155) ||
        _compareStr(txType, TYPEBURNERC721) ||
        _compareStr(txType, TYPEAPPROVEERC1155) ||
        _compareStr(txType, TYPEAPPROVEERC721) ||
        _compareStr(txType, TYPEREVOKEERC1155) ||
        _compareStr(txType, TYPEREVOKEERC721) ||
        _compareStr(txType, TYPEAPPROVEBATCHERC721) ||
        _compareStr(txType, TYPEAPPROVEBATCHERC1155)
        );
    }

    function _compareStr(string memory str1, string memory str2) private pure returns (bool){
        return keccak256(abi.encodePacked(str1)) == keccak256(abi.encodePacked(str2));
    }

    function _packRequest(ForwardRequest memory req) view internal returns (bytes memory){
        if (_compareStr(req.txType, TYPEMINTERC721)) {
            return abi.encodeWithSelector(
                bytes4(keccak256("batchSafeMint(address,uint256[])")),
                req.to,
                req.tokenID
            );
        }

        if (_compareStr(req.txType, TYPEMINTERC1155)) {
            return abi.encodeWithSelector(
                bytes4(keccak256("batchMint(address,uint256[],uint256[],bytes)")),
                req.to,
                req.tokenID,
                req.amount,
                ""
            );
        }

        if (_compareStr(req.txType, TYPEAPPROVEERC721)) {
            return abi.encodeWithSelector(
                bytes4(keccak256("approve(address,uint256)")),
                address(req.to),
                req.tokenID[0]
            );
        }

        if (_compareStr(req.txType, TYPEAPPROVEERC1155)) {
            return abi.encodeWithSelector(
                bytes4(keccak256("approve(address,address,uint256)")),
                address(req.from),
                address(req.to),
                req.tokenID[0]
            );
        }

        if (_compareStr(req.txType, TYPEAPPROVEBATCHERC721)) {
            return abi.encodeWithSelector(
                bytes4(keccak256("batchApprove(address,uint256[])")),
                address(req.to),
                req.tokenID
            );
        }

        if (_compareStr(req.txType, TYPEAPPROVEBATCHERC1155)) {
            return abi.encodeWithSelector(
                bytes4(keccak256("batchApprove(address,address,uint256[])")),
                address(req.from),
                address(req.to),
                req.tokenID
            );
        }

        if (_compareStr(req.txType, TYPEREVOKEERC721)) {
            return abi.encodeWithSelector(
                bytes4(keccak256("batchRevoke(uint256[])")),
                req.tokenID,
                false
            );
        }

        if (_compareStr(req.txType, TYPEREVOKEERC1155)) {
            return abi.encodeWithSelector(
                bytes4(keccak256("batchRevoke(address,address,uint256[])")),
                address(req.from),
                address(req.to),
                req.tokenID
            );
        }

        if (_compareStr(req.txType, TYPETRANSFERERC721)) {
            return abi.encodeWithSelector(
                bytes4(keccak256("transferFrom(address,address,uint256)")),
                req.from,
                req.to,
                req.tokenID[0]
            );
        }

        if (_compareStr(req.txType, TYPETRANSFERERC1155)) {
            return abi.encodeWithSelector(
                bytes4(keccak256("safeTransferFrom(address,address,uint256,uint256,bytes)")),
                req.from,
                req.to,
                req.tokenID[0],
                req.amount[0],
                ""
            );
        }

        if (_compareStr(req.txType, TYPEBURNERC1155)) {
            return abi.encodeWithSelector(
                bytes4(keccak256("burn(address,uint256,uint256)")),
                req.from,
                req.tokenID[0],
                req.amount[0]
            );
        }

        if (_compareStr(req.txType, TYPEBURNERC721)) {
            return abi.encodeWithSelector(
                bytes4(keccak256("burn(uint256)")),
                req.tokenID[0]
            );
        }

        revert("bad request txType");
    }

    function execute(ForwardRequest calldata req, bytes calldata signature)
    public
    payable
    returns (bool, bytes memory)
    {
        require(_isTypeExists(req.txType), "such type is not exists");
        require(verify(req, signature), "signature does not match request");

        (bool success, bytes memory returndata) = req.contractAddr.call{gas : req.gas, value : 0}(
            abi.encodePacked(_packRequest(req), req.from)
        );
        _onetimestr[req.from].mustAdd(req.onetimestr);

        // Validate that the relayer has sent enough gas for the call.
        // See https://ronan.eth.link/blog/ethereum-gas-dangers/
        if (gasleft() <= req.gas / 63) {
            // We explicitly trigger invalid opcode to consume all gas and bubble-up the effects, since
            // neither revert or assert consume all gas since Solidity 0.8.0
            // https://docs.soliditylang.org/en/v0.8.0/control-structures.html#panic-via-assert-and-error-via-require
            assembly {
                invalid()
            }
        }

        return (success, returndata);
    }
}

contract Pixchange is ERC1155Holder, ERC721Holder {
    IFactory factory;
    MinimalForwarder forwarder;

    Roles.Role private _managers;

    event ERC721Swapped(address seller, address buyer, uint256 tokenID, string price);
    event ERC1155Swapped(address seller, address buyer, uint256 tokenID, uint256 amount, string price);
    event ManagerAdded(address manager);
    event ManagerDeleted(address manager);

    struct ExchangeERC721 {
        address _token;
        address _seller;
        address _buyer;
        uint256 _tokenID;
        uint256 _gas;
        string _price;
        uint256[] _approvedList;
        uint256[] _approvedAmounts;
        string _description;
        string _onetimestr;
        bytes _signature;
    }

    struct ExchangeERC1155 {
        address _token;
        address _seller;
        address _buyer;
        uint256 _tokenID;
        uint256 _amount;
        uint256 _gas;
        string _price;
        uint256[] _approvedList;
        uint256[] _approvedAmounts;
        string _description;
        string _onetimestr;
        bytes _signature;
    }

    modifier onlyFactoryOwner {
        require(msg.sender == factory.owner());
        _;
    }

    modifier onlyManager() {
        require(isManager(msg.sender), "Pixchange: sender must be manager");
        _;
    }

    constructor(IFactory _factory, address _forwarder, address[] memory managers_){
        factory = _factory;
        for (uint256 i = 0; i < managers_.length; ++i) {
            Roles.add(_managers, managers_[i]);
        }
        forwarder = MinimalForwarder(_forwarder);
    }

    function isManager(address _manager) public returns (bool){
        return Roles.has(_managers, _manager);
    }

    function addManager(address _manager) external onlyFactoryOwner {
        Roles.add(_managers, _manager);
        emit ManagerAdded(_manager);
    }

    function deleteManager(address _manager) external onlyFactoryOwner {
        Roles.remove(_managers, _manager);
        emit ManagerDeleted(_manager);
    }

    function exchange(
        ExchangeERC721 calldata req
    ) external onlyManager {
        emit ERC721Swapped(req._seller, req._buyer, req._tokenID, req._price);
        if (req._signature.length != 0) {
            string memory txType = "approveERC721";
            if (req._approvedList.length > 1) {
                txType = "batchApproveERC721";
            }

            MinimalForwarder.ForwardRequest memory fReq = MinimalForwarder.ForwardRequest(
                txType,
                req._description,
                req._seller,
                address(this),
                req._token,
                req._approvedList,
                req._approvedAmounts,
                req._gas,
                req._onetimestr
            );

            (bool ok,) = forwarder.execute(fReq, req._signature);
            require(ok, "failed to execute");
        }

        IERC721(req._token).safeTransferFrom(req._seller, req._buyer, req._tokenID);
    }

    function exchange(
        ExchangeERC1155 calldata req
    ) external onlyManager {
        emit ERC1155Swapped(req._seller, req._buyer, req._tokenID, req._amount, req._price);
        if (req._signature.length != 0) {
            string memory txType = "approveERC1155";
            if (req._approvedList.length > 1) {
                txType = "batchApproveERC1155";
            }
            MinimalForwarder.ForwardRequest memory fReq = MinimalForwarder.ForwardRequest(
                txType,
                req._description,
                req._seller,
                address(this),
                req._token,
                req._approvedList,
                req._approvedAmounts,
                req._gas,
                req._onetimestr
            );

            (bool ok,) = forwarder.execute(fReq, req._signature);
            require(ok, "failed to execute");
        }

        IERC1155(req._token).safeTransferFrom(req._seller, req._buyer, req._tokenID, req._amount, "");
    }


    function _makeArray(uint256 _num) private view returns (uint256[] memory){
        uint256[] memory _arr = new uint256[](1);
        _arr[0] = _num;
        return _arr;
    }
}