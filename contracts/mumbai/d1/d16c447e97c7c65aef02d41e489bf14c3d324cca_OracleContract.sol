// File contracts/oracle.sol

pragma solidity 0.8.15;
contract OracleContract {

    event Request(uint256 requestID,bytes32 data,uint256 DAOVoteID);
    
    error NullAddress();
    error NotAdmin();
    error AlreadyLinked();
    error HasNotStakedEnough();
    error NotApprovedByAdminWallet();
    error NotAuthorised();
    error NotApproved();
    
    ICoin private COT;
    IStake private Staking;
    
    uint256 public cost;

    uint256 private currentRequestID;
    
    address private feeReceiver;
    
    address private walletToAddNewOracleNodeWallets;
    
    address[] public feeEarningNodeProviders;

    mapping(address => bool) private isAuthorisedNodeWallet;
    
    mapping(address => address) private linkedWallets;
    
    struct RequestDetails {
        address caller;
        uint256 DAOVoteID;
        address[] nodesResponded;
        mapping(address => bool) respondedValues;
    }
    
    mapping(uint256 => RequestDetails) private requestDetails;
    
    mapping(address => uint256) private nodeWalletNonces;
    
    constructor(address _COT,address _Staking){
        if(_COT == address(0) || _Staking == address(0)) revert NullAddress();
        COT = ICoin(_COT);
        Staking = IStake(_Staking);
        walletToAddNewOracleNodeWallets = msg.sender;
        cost = 1 * 10 ** 18;
        feeReceiver = msg.sender;
    }    
    
    modifier NotNullAddress(address _addr){
        if(_addr == address(0)) revert NullAddress();
        _;
    }

    modifier OnlyApproved{
        if(msg.sender != walletToAddNewOracleNodeWallets) revert NotAdmin();
        _;
    }
    
    function changeAdminWallet(address _new) external NotNullAddress(_new) OnlyApproved {
        walletToAddNewOracleNodeWallets = _new;
    }
    
    function addNodeWallet(address _new) external NotNullAddress(_new) OnlyApproved {
        isAuthorisedNodeWallet[_new] = true;
    }
    
    function removeNodeWallet(address _remove) external NotNullAddress(_remove) OnlyApproved {
        delete isAuthorisedNodeWallet[_remove];
    }
    
    //This function is called internally
    //This function is called to verify if a signature was used to sign a given message
    //Params
    //_hashedMessage => A message that has been passed through the keccak256 algorithm
    //_v, _r & _s are pieces of the signature
    //returns(bool) => Returns true or false whether the signature is verified or not
    function VerifyMessage(
        bytes32 _hashedMessage,
        uint8 _v,
        bytes32 _r,
        bytes32 _s
    ) internal view returns (address) {

        //Define a string to be prefixed to the hashed message
        bytes memory prefix = "\x19Ethereum Signed Message:\n32";

        //Combine the prefix & hashedMessage
        bytes32 prefixedHashMessage = keccak256(
            abi.encodePacked(prefix, _hashedMessage)
        );

        // console.logBytes32(prefixedHashMessage);

        //Retrieve the signing address of the message
        address signer = ecrecover(prefixedHashMessage, _v, _r, _s);

        //Require that the signer is the admin of the contract, return bool accordingly
        return signer;
    }
    
    //This signature is from the wallet on the node oracle
    function linkWallet(
        uint8 _v,
        bytes32 _r,
        bytes32 _s
    ) external  {
        require(Staking.hasPermission(msg.sender),"HasNotStakedEnough");
        address signer = VerifyMessage(keccak256(abi.encode("NodeStart",msg.sender)),_v,_r,_s);
        require(linkedWallets[signer] == address(0),"AlreadyLinked");
        console.log(signer);
        require(isAuthorisedNodeWallet[signer],"NotAuthorised");
        linkedWallets[signer] = msg.sender;
    }
    
    function RequestVerification(bytes32 data,uint256 DAOVoteID) external returns(uint256){
        //Check contract has approved this contract to spend COT
        uint256 AA = COT.allowance(msg.sender,address(this));
        if(AA < cost) revert NotApproved();
        COT.transferFrom(msg.sender,feeReceiver,cost);
        
        //Register request details
        uint256 requestID = ++currentRequestID;
        requestDetails[requestID].caller = msg.sender;
        requestDetails[requestID].DAOVoteID = DAOVoteID;
        
        //Emit event
        emit Request(requestID, data, DAOVoteID);
        
        return requestID;
    }
    
    function submitVerification(
        uint8 _v,
        bytes32 _r,
        bytes32 _s,
        bool result,
        uint256 requestID
    ) external{
        if(!isAuthorisedNodeWallet[msg.sender]) revert NotAuthorised();
        
        if(!Staking.hasPermission(linkedWallets[msg.sender])) revert HasNotStakedEnough();
        
        RequestDetails storage details = requestDetails[requestID];

        address signer = VerifyMessage(keccak256(abi.encode("NodeJob",msg.sender,details.caller,details.DAOVoteID)),_v,_r,_s);

        if(signer != walletToAddNewOracleNodeWallets) revert NotApprovedByAdminWallet();
        nodeWalletNonces[msg.sender]++;
        
        
        details.nodesResponded.push(msg.sender);
        details.respondedValues[msg.sender] = result;
        
        if(details.nodesResponded.length == 4){
            bool last = false;
            for(uint256 i = 0; i < 4; ){
                if(i == 0){
                    last = details.respondedValues[details.nodesResponded[i]]; 
                }else{
                    if(last != details.respondedValues[details.nodesResponded[i]]){
                        IDAO(details.caller).acceptResult(false);            
                    }
                }   
                unchecked{ 
                    i++;
                }
            }
            IDAO(details.caller).acceptResult(true);
        }
    }
    
    function getNonce(address query) external view returns(uint256){
        return nodeWalletNonces[query];
    }
    
}