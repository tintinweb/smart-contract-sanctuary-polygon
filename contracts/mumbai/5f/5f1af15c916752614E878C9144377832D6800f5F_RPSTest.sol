contract RPSTest is ERC721A, ReentrancyGuard, VRFV2WrapperConsumerBase, Ownable {

    uint32 callbackGasLimit = 100000;
    // The default is 3, but you can set this higher.
    uint16 requestConfirmations = 3;
    // Cannot exceed VRFV2Wrapper.getConfig().maxNumWords.
    uint32 numWords = 1;

    uint256 public constant MAX_SUPPLY = 100;
    uint256 public MAX_MINT = 3;
    uint256 private constant MAX_RESERVED = 0;
    uint256 public giftCount = 0;
    uint256 public PRESALE_PRICE = 0;
    uint256 public PRICE = 0;

    // Address LINK and WRAPPER
    address linkAddress;
    address wrapperAddress;
    address private treasury;

    string private _contractURI;
    string private _metadataBaseURI;

    Match[] public matches;

    enum PlayerClass {
        rock,
        paper,
        scissors,
        none
    }

    struct TokenData {
        uint256 tokenID;
        uint32 wins;
        uint32 kills;
        uint32 losses;
        uint32 maxSpeed;
        PlayerClass tokenType;
    }

    struct Match {
        bool matchOpen;
        PlayerClass winningType;
        uint256 matchId;
        uint256 matchSeed;
    }

    struct RequestStatus {
        uint256 paid; // amount paid in link
        bool fulfilled; // whether the request has been successfully fulfilled
        uint256[] randomWords;
    }

    mapping (uint256 => TokenData) public playerTokenData; // relates an NFT token ID to its on-chain data
    // mapping (uint256 => uint256) public gameSeeds; // each match will require an on-chain verifiably random seed to determine outcome // REDUNDANT
    // mapping (uint256 => bool) public activeMatches; // tracks how many matches exist // REDUNDANT
    mapping (uint256 => uint256[]) public matchPlayers; // track tokens enrolled in a match
    mapping (uint256 => RequestStatus) public s_requests; // requestId --> requestStatus for verafiable randomness

    // Events
    event PlayerOptedIn(uint256 playerId, uint256 matchId); // broadcast when a player joins a match
    event PlayerOptedOut(uint256 playerId, uint256 matchId); // broadcast if a player opts-out of a match after opting-in
    event RequestSent(uint256 requestId, uint32 numWords);
    event RequestFulfilled(
        uint256 requestId,
        uint256[] randomWords,
        uint256 payment
    );
    event matchUpdates(uint256 matchId, bool matchStatus);

    error MintAllowanceExceeded();
    error NotOwnerToken(); // the token selected is not owned by the caller
    error MatchLocked(); // if a match's seed has already been generated, no more players may enter or leave
    error NoMatchExists(uint256 matchID); // throws error if a match seed is requested for a non-existent match
    error NoRequestFound(); // if requestID does not match an existing random request

    /**
     * @dev modifiers for requirements consistent accorss multiple functions
     */
    modifier checkTokenSupplyAvailable(uint256 mintNum) {
        require(totalSupply() + mintNum < (MAX_SUPPLY - (MAX_RESERVED - giftCount) + 1), "Error: this would exceed max supply");
        _;
    }

    // modifier validPayment(uint256 mintNum, uint256 mintPrice) {
    //     require(mintPrice * mintNum == msg.value, "Error, incorrect amount");
    //     _;
    // }

    // modifier addressWhitelisted(bytes32[] calldata merkleProof, bytes32 listRoot) {
    //     require(MerkleProof.verifyCalldata(merkleProof, listRoot, keccak256(abi.encodePacked(msg.sender))), "You are not on the whitelist");
    //     _;
    // }

    modifier onlyHolder(uint256 tokenID) {
        if(ownerOf(tokenID) != msg.sender) revert NotOwnerToken();
        _;
    } 

    constructor(string memory _cURI, string memory _metaBURI, address _treasury, address _linkAdd, address _wrapAdd) public ERC721A("RPC", "RPCTest") VRFV2WrapperConsumerBase(_linkAdd, _wrapAdd) {
        _contractURI = _cURI;
        _metadataBaseURI = _metaBURI;
        treasury = _treasury;
        linkAddress = _linkAdd; // for randomseed gen via Chainlink
        wrapperAddress = _wrapAdd; // for randomseed gen via Chainlink
    }

    /**
     * @dev mint functions: saleMint is open to anyone
     * presaleMint requires whitelist
     * gift can only be called by owner
     */
    function saleMint(uint256 qty) checkTokenSupplyAvailable(qty) nonReentrant external  {
        if(qty > MAX_MINT) revert MintAllowanceExceeded();

        uint256 count = 0;
        uint256 _supply = totalSupply();

        for(uint256 i = _supply + 1; i <= (_supply + qty); i++) {
            playerTokenData[i] = TokenData(i, 0, 0, 0, 4, PlayerClass(i % 3));
            count++;
        }
        _mint(msg.sender, qty);
    }

    function playerEnterGame(uint256 playerTokenId, uint256 matchId) public onlyHolder(playerTokenId) {
        if(matchId < 1 || matchId > matches.length) revert NoMatchExists(matchId);
        if(matches[matchId - 1].matchSeed != 0 || matches[matchId - 1].matchOpen == false) revert MatchLocked();

        matchPlayers[matchId -1].push(playerTokenId);

        emit PlayerOptedIn(playerTokenId, matchId);
    }

    function playerRemoveGame(uint256 playerTokenId, uint256 matchId) public onlyHolder(playerTokenId) {
        if(matchId < 1 || matchId > matches.length) revert NoMatchExists(matchId);
        if(matches[matchId - 1].matchSeed != 0 || matches[matchId - 1].matchOpen == false) revert MatchLocked();

        bool playerFound = false;
        for (uint256 i = 0; i < matchPlayers[matchId - 1].length; i++) {
            if (matchPlayers[matchId - 1][i] == playerTokenId) {
                matchPlayers[matchId - 1][i] = matchPlayers[matchId - 1][matchPlayers[matchId - 1].length - 1]; // overwrite with last element from array to 'erase' it
                matchPlayers[matchId - 1].pop(); // delete the now duplicate element
                playerFound = true;
                break;
            }
        }

        require(playerFound, "Player has not opted into this match");

        emit PlayerOptedOut(playerTokenId, matchId);
    }


    // function updatePlayerEntry(uint256 playerTokenId, uint256 matchId) public onlyHolder(playerTokenId) {
        // if(matchId < 1 || matchId > matches.length) revert NoMatchExists(matchId);
        // if(matches[matchId - 1].matchSeed != 0 || matches[matchId - 1].matchOpen == false) revert MatchLocked();

    //     uint256 playerIndex = matches[matchId - 1].matchPlayers.length; // initialize to an invalid index
    //     for (uint256 i = 0; i < matches[matchId - 1].matchPlayers.length; i++) {
    //         if (matches[matchId - 1].matchPlayers[i] == playerTokenId) {
    //             playerIndex = i;
    //             break;
    //         }
    //     }
    

    //     if (playerIndex < matches[matchId - 1].matchPlayers.length) {
    //         // Player is already in the match, remove them
    //         matches[matchId - 1].matchPlayers[playerIndex] = matches[matchId - 1].matchPlayers[matches[matchId - 1].matchPlayers.length - 1];
    //         matches[matchId - 1].matchPlayers.pop();
    //         emit PlayerOptedOut(playerTokenId, matchId);
    //     } else {
    //         // Player is not in the match, add them
    //         matches[matchId - 1].matchPlayers.push(playerTokenId);
    //         emit PlayerOptedIn(playerTokenId, matchId);
    //     }
    // }

    function burnToken(uint256 tokenID) public onlyHolder(tokenID) {
        _burn(tokenID);
    }

    /**
     * @dev gameplay functions
     */

    function getWalletTokenData(address _wallet) external view returns(TokenData[] memory) {
        require(balanceOf(_wallet) > 0, "Wallet contains no Assassin tokens");
        uint256 tokenCount = balanceOf(_wallet);
        TokenData[] memory tokenData = new TokenData[](tokenCount);

        uint256 j = 0;
        for (uint256 i = _startTokenId(); i <= totalSupply(); i++) {
            if (ownerOf(i) == _wallet) {
                tokenData[j] = playerTokenData[i];
                j++;
            }
            if(j == tokenCount) {
                break;
            }
        }
        return tokenData;
    }

    function updateGameData(
        uint256[] calldata playerIDs, 
        uint32[] calldata wins, 
        uint32[] calldata kills, 
        uint32[] calldata losses, 
        uint32[] calldata maxSpeed
    ) public onlyOwner {

        for(uint256 i = 0; i < playerIDs.length; i++) {
            playerTokenData[playerIDs[i]].wins = wins[i];
            playerTokenData[playerIDs[i]].kills = kills[i];
            playerTokenData[playerIDs[i]].losses = losses[i];
            playerTokenData[playerIDs[i]].maxSpeed = maxSpeed[i];    
        }
    }

    /**
     * @dev Admin functions to create matches
     */
    function createMatch(uint256 numMatches) public onlyOwner {
        uint256 matchArrLeng = matches.length;
        for(uint i = 1; i <= numMatches; i++) {
            matches.push(Match({
                matchOpen: true,
                winningType: PlayerClass.none,
                matchId: matchArrLeng + i,
                matchSeed: 0
            }));
        }
    }

    function updateMatch(uint256 matchId, bool status) public onlyOwner {
        if(matchId < 1 || matchId > matches.length) revert NoMatchExists(matchId);

        matches[matchId - 1].matchOpen = status;
    }

    function getMatch(uint256 matchId) public view returns(Match memory, uint256[] memory) {
        return (matches[matchId - 1], matchPlayers[matchId - 1]);
    }

    function getAllMatches() public view returns(Match[] memory) {
        return matches;
    } 

    /**
     * @dev getOwnersTokens will return an array of all the tokens belonging to input address
     */
    function getOwnersTokens(address _owner)
        external
        view
        returns (uint256[] memory)
    {
        require(balanceOf(_owner) > 0, "Wallet contains no Assassin tokens");
        uint256 tokenCount = balanceOf(_owner);
        uint256 foundTokens = 0;
        uint256[] memory tokenIdsArr = new uint256[](tokenCount);

        for (uint256 i = _startTokenId(); i <= totalSupply(); i++) {
            if (ownerOf(i) == _owner) {
                tokenIdsArr[foundTokens] = i;
                foundTokens++;
            }
        }
        return tokenIdsArr;
    }

    /**
     * @dev returnAddressesOfHolder will return an array of addresses belonging to the input tokenIDs
     */
    function returnAddressesOfHolder(uint256[] calldata tokenIds) external view onlyOwner returns (address[] memory) {
        
        address[] memory fetchedAdd = new address[](tokenIds.length);

        for(uint256 i = 0; i < tokenIds.length; i++) {
            fetchedAdd[i] = ownerOf(tokenIds[i]);
        }
        return fetchedAdd;
    }

    /**
     * @dev isHolder is a simple function that returns true if address holder any tokens
     */
    function isHolder(address _owner) external view returns (bool) {
        bool holderStatus = false;
        uint256 bal = balanceOf(_owner);
        if(bal > 0) {
            holderStatus = true;
        }
        return holderStatus;
    }

    function withdrawFunds() public onlyOwner {
        (bool success, ) = payable(treasury).call{value: address(this).balance}("");
        require(success, "Failed to send payment.");
    }

    /**
     * @dev functions to update variables such as baseURI
     */

    function updateMetadataURI(string calldata _URI) external onlyOwner {
        _metadataBaseURI = _URI;
    }

    function updateContractURI(string calldata _URI) external onlyOwner {
        _contractURI = _URI;
    }

    
    function updatePrice(uint256 _price) external onlyOwner {
        PRICE = _price;
    }

    function updatePrePrice(uint256 _price) external onlyOwner {
        PRESALE_PRICE = _price;
    }
    
    function updateMaxMint(uint256 _amt) external onlyOwner {
        MAX_MINT = _amt;
    }

    function updateTreasury(address _trsy) external onlyOwner {
        treasury = _trsy;
    }


    /**
     * @dev read only functions
     */

    function contractURI() external view returns (string memory) {
        return _contractURI;
    }

    function _baseURI() internal view override returns (string memory) {
        return _metadataBaseURI;
    }

    // ERC721A change startTokenID to 1
    function _startTokenId() internal view virtual override returns (uint256) {
        return 1;
    }


    /*
     * Verifiable randomness code below appropriated from ChainLink's Documentation
     * Source: https://docs.chain.link/vrf/v2/direct-funding/examples/get-a-random-number#analyzing-the-contract
     */
    function requestRandomWords(uint256 matchId)
        external
        onlyOwner
        returns (uint256 requestId)
    {
        if(matchId < 1 || matchId > matches.length) revert NoMatchExists(matchId);
        requestId = requestRandomness(
            callbackGasLimit,
            requestConfirmations,
            numWords
        );
        s_requests[requestId] = RequestStatus({
            paid: VRF_V2_WRAPPER.calculateRequestPrice(callbackGasLimit),
            randomWords: new uint256[](0),
            fulfilled: false
        });
        matches[matchId - 1].matchSeed = requestId;
        emit RequestSent(requestId, numWords);
        return requestId;
    }

    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory _randomWords
    ) internal override {
        require(s_requests[_requestId].paid > 0, "request not found");
        // Limit the range of the random number to 1 - 4000
        _randomWords[0] = _randomWords[0] % 4000 + 1;

        s_requests[_requestId].fulfilled = true;
        s_requests[_requestId].randomWords = _randomWords;
        emit RequestFulfilled(
            _requestId,
            _randomWords,
            s_requests[_requestId].paid
        );
    }

    function getRequestStatus(
        uint256 _requestId
    )
        external
        view
        returns (uint256 paid, bool fulfilled, uint256[] memory randomWords)
    {
        require(s_requests[_requestId].paid > 0, "request not found");
        RequestStatus memory request = s_requests[_requestId];
        return (request.paid, request.fulfilled, request.randomWords);
    }

    /**
     * When called this returns the random seed for the match
     */
    function getMatchSeed(uint256 matchID) public view returns(uint256) {
        uint256 reqId = matches[matchID - 1].matchSeed;
        if(reqId == 0) revert NoMatchExists(matchID);
        if(s_requests[reqId].paid == 0) revert NoRequestFound();

        RequestStatus memory request = s_requests[reqId];

        return request.randomWords[0];
    } // UNTESTED

    /**
     * Allow withdraw of Link tokens from the contract
     */
    function withdrawLink() public onlyOwner {
        LinkTokenInterface link = LinkTokenInterface(linkAddress);
        require(
            link.transfer(msg.sender, link.balanceOf(address(this))),
            "Unable to transfer"
        );
    }
}

