interface IShikkuboStaking {
  error OwnerCheckFailed();
  error NoOwnerOrApproved();
  error WithdrawalOverflow();
  error StakeZeroAmount();
  error DirectTokenTransfer();

  struct Staker {
    uint32 amountStaked;
    uint256 weight;
    uint256 updatedAt;
  }

  struct Pool {
    uint256 totalWeight;
    uint256 rewardSupply;
    uint256 updatedAt;
  }

