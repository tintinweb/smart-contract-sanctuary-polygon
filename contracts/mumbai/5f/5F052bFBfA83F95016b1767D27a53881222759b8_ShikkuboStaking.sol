pragma solidity ^0.8.18;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/Address.sol";

import "./interfaces/IShikkuboERC721.sol";
import "./interfaces/IShikkuboStaking.sol";

contract ShikkuboStaking is IShikkuboStaking, ReentrancyGuard, ERC165, IERC721Receiver {
  bool private _isStaking = false;

  Pool private _pool;

  mapping(uint256 => address) private _owners;
  mapping(address => Staker) private _stakers;

  IShikkuboERC721 public immutable stakingToken;

  constructor(IShikkuboERC721 _stakingToken) {
    stakingToken = _stakingToken;
    _pool.updatedAt = block.timestamp;
  }

  receive() external payable {}

  /*///////////////////////////////////////////////////////////////
                         Public getters
  //////////////////////////////////////////////////////////////*/

  function estimatedShare(address _staker) external view returns (uint256) {
    uint256 _rewards = _calculateRewards(_staker);
    uint256 _supply = _calculateRewardSupply();

    return _calculateShare(_rewards, _supply);
  }

  function tokensOfOwner(address _staker) external view returns (uint256[] memory) {
    unchecked {
      uint32 _tokenIdsLength = _stakers[_staker].amountStaked;
      uint256 _tokenIdsIdx;
      uint256[] memory _tokenIds = new uint256[](_tokenIdsLength);

      for (uint256 i = 1; _tokenIdsIdx != _tokenIdsLength; ++i) {
        if (_owners[i] == _staker) {
          _tokenIds[_tokenIdsIdx] = i;
          ++_tokenIdsIdx;
        }
      }

      return _tokenIds;
    }
  }

  function ownerOf(uint256 _tokenId) external view returns (address) {
    return _owners[_tokenId];
  }

  /*///////////////////////////////////////////////////////////////
                      ERC 165 / 721 logic
  //////////////////////////////////////////////////////////////*/

  function onERC721Received(address, address, uint256, bytes calldata) external view returns (bytes4) {
    if (!_isStaking) {
      revert DirectTokenTransfer();
    }

    return this.onERC721Received.selector;
  }

  function supportsInterface(bytes4 interfaceId) public view override returns (bool) {
    return interfaceId == type(IERC721Receiver).interfaceId || super.supportsInterface(interfaceId);
  }

  /*//////////////////////////////////////////////////////////////
                         Staking functions
  //////////////////////////////////////////////////////////////*/

  function stake(uint256[] calldata _tokenIds) external nonReentrant {
    uint256 _length = _tokenIds.length;

    if (_length == 0) {
      revert StakeZeroAmount();
    }

    address _staker = msg.sender;

    _safeClaim(_staker);

    _isStaking = true;

    for (uint256 i; i < _length; ) {
      uint256 _tokenId = _tokenIds[i];
      address _owner = stakingToken.ownerOf(_tokenId);

      if (
        _owner != _staker &&
        (stakingToken.getApproved(_tokenId) != address(this) || stakingToken.isApprovedForAll(_owner, address(this)))
      ) {
        revert NoOwnerOrApproved();
      }

      stakingToken.safeTransferFrom(_staker, address(this), _tokenId);

      _increaseWeight(_staker, _tokenId);

      _owners[_tokenId] = _staker;

      unchecked {
        ++i;
      }
    }
    _isStaking = false;

    _stakers[_staker].amountStaked += uint32(_length);
  }

  function withdraw(uint256[] calldata _tokenIds) external nonReentrant {
    address _staker = msg.sender;
    uint256 _length = _tokenIds.length;

    if (_stakers[_staker].amountStaked < _length) {
      revert WithdrawalOverflow();
    }

    _safeClaim(_staker);

    for (uint256 i; i < _length; ) {
      uint256 _tokenId = _tokenIds[i];

      if (_owners[_tokenId] != _staker) {
        revert OwnerCheckFailed();
      }

      stakingToken.safeTransferFrom(address(this), _staker, _tokenId);

      _decreaseWeight(_staker, _tokenId);

      _owners[_tokenId] = address(0);

      unchecked {
        ++i;
      }
    }

    _stakers[_staker].amountStaked -= uint32(_length);
  }

  function claim() external nonReentrant {
    _safeClaim(msg.sender);
  }

  /*//////////////////////////////////////////////////////////////
                         Internal functions
  //////////////////////////////////////////////////////////////*/

  function _safeClaim(address _staker) internal {
    _pool.rewardSupply = _calculateRewardSupply();
    _pool.updatedAt = block.timestamp;

    uint256 _rewards = _calculateRewards(_staker);
    _stakers[_staker].updatedAt = block.timestamp;

    if (_rewards == 0 || _pool.rewardSupply == 0) return;

    uint256 _share = _calculateShare(_rewards, _pool.rewardSupply);

    Address.sendValue(payable(_staker), _share);

    _pool.rewardSupply -= _rewards;
  }

  function _calculateRewardSupply() internal view returns (uint256) {
    uint256 _timeSinceLastUpdate = block.timestamp - _pool.updatedAt;

    return _pool.rewardSupply + (_timeSinceLastUpdate * _pool.totalWeight);
  }

  function _calculateRewards(address _staker) internal view returns (uint256) {
    uint256 _timeSinceLastUpdate = block.timestamp - _stakers[_staker].updatedAt;

    return _timeSinceLastUpdate * _stakers[_staker].weight;
  }

  function _calculateShare(uint256 _rewards, uint256 _supply) internal view returns (uint256) {
    uint256 _balance = address(this).balance;

    return (_balance * _rewards) / _supply;
  }

  function _tokenWeight(uint256 _tokenId) internal view returns (uint64 _weight) {
    IShikkuboERC721.Rarity _rarity = stakingToken.rarityOf(_tokenId);

    if (_rarity == IShikkuboERC721.Rarity.COMMON) {
      _weight = 1;
    } else if (_rarity == IShikkuboERC721.Rarity.UNCOMMON) {
      _weight = 2;
    } else if (_rarity == IShikkuboERC721.Rarity.RARE) {
      _weight = 3;
    } else if (_rarity == IShikkuboERC721.Rarity.EPIC) {
      _weight = 5;
    } else {
      _weight = 8;
    }

    _weight = _weight * 10 ** 18;
  }

  function _increaseWeight(address _staker, uint256 _tokenId) internal {
    uint64 _weight = _tokenWeight(_tokenId);

    _stakers[_staker].weight += _weight;
    _pool.totalWeight += _weight;
  }

  function _decreaseWeight(address _staker, uint256 _tokenId) internal {
    uint64 _weight = _tokenWeight(_tokenId);

    _stakers[_staker].weight -= _weight;
    _pool.totalWeight -= _weight;
  }
}

