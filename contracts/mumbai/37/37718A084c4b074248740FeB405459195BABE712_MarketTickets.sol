pragma solidity ^0.8.9;







contract MarketTickets is Pausable, Ownable, ReentrancyGuard {
    address private _nftContract;
    address private _tokenAccepted;
    event nftContractSetted(address _nft);
    event tokenContractSetted(address _tokenAccepted);
    event successfulPurchase(address user, TICKETS_TYPE ticket, uint amount);
    event ticketPriceSetted(uint _id, uint _price);
    event increasedTickets(uint _id, uint _amount);

    mapping(address=>mapping(uint=>uint[])) nftOwners;
    enum TICKETS_TYPE {Common,Rare,Exclusive,Unique}

    struct Ticket{
        TICKETS_TYPE ticket_type;
        uint price;
        uint remaining;
    }
    mapping(uint=>Ticket) public ticketsData;
    constructor(address _nft, address _token){
        setNftContract(_nft);
        setTokenContract(_token);
        ticketsData[1] = Ticket(TICKETS_TYPE.Common,10 * (10 ** ERC20(_tokenAccepted).decimals()),10000);
        ticketsData[2] = Ticket(TICKETS_TYPE.Rare,20 * (10 ** ERC20(_tokenAccepted).decimals()),1000);
        ticketsData[3] = Ticket(TICKETS_TYPE.Exclusive,50 * (10 ** ERC20(_tokenAccepted).decimals()),100);
        ticketsData[4] = Ticket(TICKETS_TYPE.Unique,100 * (10 ** ERC20(_tokenAccepted).decimals()),10);
    }


    
    modifier existsTicket(uint _id){
        require(_id >= 1 && _id <= 4 , "Ticket doesn't exists");
        _;
    }

    function setNftContract(address _nft) public onlyOwner{
        _nftContract = _nft;
        emit nftContractSetted(_nftContract);
    }

    function setTokenContract(address _token) public onlyOwner{
        _tokenAccepted = _token;
        emit tokenContractSetted(_tokenAccepted);
    }

    function buyTicket(uint _id, uint _amount)  public existsTicket(_id) nonReentrant{
        require(ticketsData[_id].remaining >= _amount, "There is not enought tickets to buy");
        uint total = _amount * ticketsData[_id].price;
        uint balance = IERC20(_tokenAccepted).balanceOf(_msgSender());
        require(balance >= total, "user doesn't have enought funds");
        IERC20(_tokenAccepted).transferFrom(_msgSender(),owner(),total);
        IERC1155(_nftContract).safeTransferFrom(owner(),_msgSender(),_id,_amount,"0x");

        emit successfulPurchase(_msgSender(),ticketsData[_id].ticket_type,_amount);
    }

    function setTicketsPrice (uint _id, uint _price) public onlyOwner existsTicket(_id){
        require(_price > 0, "price must be greater than 0");
        ticketsData[_id].price = _price * (10 ** ERC20(_tokenAccepted).decimals());
        emit ticketPriceSetted(_id, ticketsData[_id].price);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }
}