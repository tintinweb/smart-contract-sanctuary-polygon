pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./interfaces/INFT721.sol";
import "../../collection/interfaces/ICollectionMaster.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";

contract NFT721 is INFT721, Ownable, ERC721Burnable, ERC721Enumerable {
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using Strings for uint256;

    string public baseURI;
    string public newBaseClaimURI;
    mapping(uint64 => uint256) public currentIndex;
    // Info address receive money
    address payable RECEIVER;
    mapping(address => mapping(address => bool)) public isOperators;
    mapping(uint256 => bool) public isClaimed;
    event NFTClaimed(address owner, uint256 tokenId);
 

    modifier onlyOperator(address _from) {
        require(
            isOperators[_from][msg.sender] || owner() == msg.sender,
            "caller not operator"
        );
        _;
    }

    constructor(
        string memory _name,
        string memory _symbol,
        uint256[] memory _index
    ) ERC721(_name, _symbol) {
        for (uint64 i = 0; i < _index.length; i++) {
            currentIndex[i] = _index[i];
        }
    }

    function mint(
        address _from,
        address _to,
        uint64 _typeMint
    ) public override onlyOperator(_from) {
        uint256 tokenId = currentIndex[_typeMint];
        require(tokenId != 0, "type mint is invalid");

        require(
            (currentIndex[_typeMint + 1] == 0 ||
                tokenId < currentIndex[_typeMint + 1]),
            "type mint is exceed"
        );

        currentIndex[_typeMint]++;
        // // Check operator's account balance
        // uint256 operatorBalance = IERC20Upgradeable(token).balanceOf(
        //     msg.sender
        // );

        // Set the mint fee to 100
        // uint256 mintFee = 100;
        // require(
        //     operatorBalance >= mintFee,
        //     "Insufficient balance for mint fee"
        // );

        // Deduct mint fee from operator's account
        // SafeERC20Upgradeable.safeTransferFrom(
        //     IERC20Upgradeable(token),
        //     msg.sender,
        //     RECEIVER,
        //     0
        // );
        _safeMint(_to, tokenId);
    }

    function tokenURI(
        uint256 tokenId
    ) public view virtual override returns (string memory) {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        string memory currentBaseURI = _baseURI();
         string memory currentBaseClaimURI = _baseClaimURI();

        if (bytes(currentBaseClaimURI).length > 0) {
            if (bytes(currentBaseURI).length > 0) {
                return
                    string(
                        abi.encodePacked(
                            currentBaseURI,
                            tokenId.toString(),
                            currentBaseClaimURI
                        )
                    );
            } else {
                return
                    string(
                        abi.encodePacked(
                            tokenId.toString(),
                            currentBaseClaimURI
                        )
                    );
            }
        } else {
            return currentBaseURI;
        }
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(
        bytes4 interfaceId
    ) public view virtual override(ERC721, ERC721Enumerable) returns (bool) {
        return
            interfaceId == type(IERC721Enumerable).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    /**
     * @dev See {ERC721-_beforeTokenTransfer}.
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 firstTokenId,
        uint256 batchSize
    ) internal virtual override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, firstTokenId, batchSize);
    }

    // internal
    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        baseURI = _newBaseURI;
    }

    // internal
    function _baseClaimURI() internal view virtual returns (string memory) {
        return newBaseClaimURI;
    }

    function setBaseClaimURI(string memory _newBaseClaimURI) public onlyOwner {
        newBaseClaimURI = _newBaseClaimURI;
    }

    // set address operator
    function setOperator(
        address _spender,
        address _operator,
        bool _status
    ) external onlyOwner {
        isOperators[_spender][_operator] = _status;
    }

    // claim NFT
    function claim(uint256 tokenId, uint256 claimFee, address token) external {
        require(_exists(tokenId), "Token does not exist");
        require(
            !_isApprovedOrOwner(msg.sender, tokenId),
            "Caller is not the owner of the token"
        );

        // Check operator's account balance
        uint256 operatorBalance = IERC20Upgradeable(token).balanceOf(
            msg.sender
        );
        require(
            operatorBalance >= claimFee,
            "Insufficient balance for claim fee"
        );

        // Deduct claim fee from operator's account
        IERC20Upgradeable(token).safeTransferFrom(
            msg.sender,
            address(this),
            claimFee
        );

        isClaimed[tokenId] = true;

        // Mint NFT to the caller's wallet
        _safeMint(msg.sender, tokenId);
    }

    // unclaim NFT
    function unclaim(uint256 tokenId) external {
        require(_exists(tokenId), "Token does not exist");
        require(
            ownerOf(tokenId) == msg.sender,
            "Caller is not the owner of the token"
        );

        isClaimed[tokenId] = false;
    }

    // Check NFT isClaim
    function isTokenClaimed(uint256 tokenId) external view returns (bool) {
        require(_exists(tokenId), "Token does not exist");
        return isClaimed[tokenId];
    }
}

