pragma solidity ^0.8.0;
pragma abicoder v2;

import "./interfaces/ICollectionMaster.sol";
import "./CollectionBase.sol";
import "./SignatureVerifier.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "hardhat/console.sol";
contract CollectionMaster is
    Initializable,
    OwnableUpgradeable,
    ICollectionMaster,
    CollectionBase,
    SignatureVerifier,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable
{
    using SafeERC20Upgradeable for IERC20Upgradeable;

    // Info address receive money
    address payable RECEIVER;

    // Info list operator
    mapping(address => bool) public isOperators;

    event BuyPackage(address sender, uint256 typePackage, uint256 nonce);


    modifier onlyOperator() {
        require(isOperators[msg.sender] || owner() == msg.sender, "caller not operator");
        _;
    }

    function initialize() public initializer {
        __Ownable_init();
        TRUSTED_PARTY = 0x8e5299F2A59F5372EC428c70565d1ACb54cF4e1d;
        RECEIVER = payable(0x562aa82193f1AB34ddeA2D3F3C1433ebD8a5022C);

    }

    /**
        @notice verify signature and send token
        @param typePackage type package user choose
        @param token address token payment
        @param value total amount payable
        @param nonce random number
        @param signature signature to verify data input
     */
    function buyPackage(
        uint256 typePackage,
        address token,
        uint256 value,
        uint256 nonce,
        bytes memory signature
    ) public payable override nonReentrant{
        verifySignature(msg.sender, typePackage, token, value, nonce, signature);

        if (token == address(0)) {
            require(
                msg.value >= value,
                "total amount to be paid is not correct"
            );
            (bool success, ) = RECEIVER.call{value: value}("");
            require(success, "transfer failed");
        } else {
            SafeERC20Upgradeable.safeTransferFrom(
                IERC20Upgradeable(token),
                msg.sender,
                RECEIVER,
                value
            );
        }

        // re-send
        if (msg.value > value) {
            require(msg.value > msg.value - value, "error safe math");
            (bool rs, ) = payable(msg.sender).call{value: msg.value - value}("");
            require(rs, "re-send transfer failed");
        }

        emit BuyPackage(msg.sender, typePackage, nonce);
    }

    /**
        @notice create collection NFT onchain
        @param _owner address owner of collection NFT
        @param _operator address for access mint in collection NFT
        @param _tokenName name token
        @param _tokenSymbol symbol token
        @param _index start index type for each package user choose 
        @param _salt is a random number used to create an address
     */
    function createCollection(
        address _owner,
        address _operator,
        address _token,
        address _receiver,
        string memory _tokenName,
        string memory _tokenSymbol,
        string memory _newBaseURI,
        string memory _newBaseClaimURI,
        uint256[] memory _index,
        uint256 _salt
        // uint256 _tokenId
    ) public override onlyOperator whenNotPaused {
        bytes memory bytecode = getBytecode(
            _tokenName,
            _tokenSymbol,
            _index,
            _token,
            _receiver
        );
     
        _deploy(bytecode, _salt);
     
        address collectionAddress = getAddress(bytecode, _salt);

        NFT721(collectionAddress).setBaseURI(_newBaseURI);
        NFT721(collectionAddress).setOperator(_owner, _operator, true);
        NFT721(collectionAddress).transferOwnership(_owner);
        NFT721(collectionAddress).setBaseClaimURI(_newBaseClaimURI);
     

        emit CollectionCreated(collectionAddress, _owner);
    }


    function setnewReceiver(address _newReceiver) external onlyOwner {
        RECEIVER = payable(_newReceiver);
    }

    // set address operator
    function setOperator(address _operator, bool _status) external onlyOwner {
        isOperators[_operator] = _status;
    }
}

