pragma solidity ^0.8.9;
import "hardhat/console.sol";

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import {DefaultOperatorFilterer} from "./DefaultOperatorFilterer.sol";

contract PunksWearable1155 is ERC1155, AccessControl, Pausable, ERC1155Supply, ERC2981, DefaultOperatorFilterer {
    using Counters for Counters.Counter;
    Counters.Counter private idCounter;
    mapping(uint256=>uint256) public idToAmount;
    mapping(uint256=>uint256) public idToPrice;
    bool public preSaleLive;   
    bool public publicSaleLive;    
    bytes32 public root;     

    bytes32 public constant URI_SETTER_ROLE = keccak256("URI_SETTER_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    constructor(uint96 _feeNumerator) ERC1155("http://punks1155/api/") {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(URI_SETTER_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        _setDefaultRoyalty(_msgSender(), _feeNumerator);
    }

    function setURI(string memory newuri) external onlyRole(URI_SETTER_ROLE) {
        _setURI(newuri);
    } 
 
    function publishSale(uint256 amount,uint256 pricePerQuantity) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(preSaleLive==false,"Presale for the last round should be closed");
        require(publicSaleLive==false,"Public sale for the last round should be closed");
        uint256 tokenId = idCounter.current();
        idCounter.increment();
        idToAmount[tokenId]= amount; 
        idToPrice[tokenId]= pricePerQuantity; 
    }

    function startPreSale() external onlyRole(DEFAULT_ADMIN_ROLE) {
        preSaleLive=true;
    }

    function stopPreSale() external onlyRole(DEFAULT_ADMIN_ROLE) {
        preSaleLive=false;
    }

    function startPublicSale() external onlyRole(DEFAULT_ADMIN_ROLE) {
        publicSaleLive=true;
    }

    function stopPublicSale() external onlyRole(DEFAULT_ADMIN_ROLE) {
        publicSaleLive=false;
    }

    function airdrop(address[] memory accounts,uint256[] memory quantities, bytes memory data) external onlyRole(DEFAULT_ADMIN_ROLE){
        require(accounts.length==quantities.length,"Length of both the array should be same");   
        for (uint256 i = 0; i < accounts.length; ) {
        _mint(accounts[i], getCurrentId(), quantities[i], data); 
            unchecked {
                i++;
            } 
        }  
    } 

    function mintPreSale(address account, uint256 quantity, bytes32[] memory proof, bytes memory data) external payable whenNotPaused{
        require(preSaleLive==true,"Presale is not live for this round");
        require(publicSaleLive==false,"Public sale is live for this round");
        require(msg.value>=(idToPrice[getCurrentId()]*quantity),"Not enough ethers");
        require(isValid(proof, keccak256(abi.encodePacked(msg.sender))), "Not a part of Whitelist"); 
        uint maxAmount= idToAmount[getCurrentId()];
        require(quantity+totalSupply(getCurrentId())<=maxAmount,"Quantity exceeded the max limit for this round");
        _mint(account, getCurrentId(), quantity, data); 
    }

    function mintPublicSale(address account, uint256 quantity, bytes memory data) external payable whenNotPaused{
        require(publicSaleLive==true,"Public sale is not live for this round");
        require(preSaleLive==false,"Presale is live for this round");
        require(msg.value>=(idToPrice[getCurrentId()]*quantity),"Not enough ethers");
        uint maxAmount= idToAmount[getCurrentId()];
        require(quantity+totalSupply(getCurrentId())<=maxAmount,"Quantity exceeded the max limit for this round");
        // console.log(msg.value); 
        // console.log((idToPrice[getCurrentId()]*quantity)); 
        // console.log(totalSupply(getCurrentId())); 
        // console.log(maxAmount); 
        _mint(account, getCurrentId(), quantity, data); 
    }

    function pause() external onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() external onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function isValid(bytes32[] memory proof, bytes32 leaf) public view returns (bool) {
        return MerkleProof.verify(proof, root, leaf);
    }

    function setMerkleRoot(bytes32 _newRoot) external onlyRole(DEFAULT_ADMIN_ROLE) {
        root=_newRoot;
    }


    function getCurrentId() public view returns(uint256){
        return idCounter.current()==0 ? 0 :idCounter.current()-1;
    }
    
    function setDefaultRoyalty(address _receiver, uint96 _feeNumerator)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _setDefaultRoyalty(_receiver, _feeNumerator);
    }
    function setTokenRoyalty(
        uint256 _tokenId,
        address _receiver,
        uint96 _feeNumerator
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _setTokenRoyalty(_tokenId, _receiver, _feeNumerator);
    }

    /// @dev Allow owner to delete the default royalty for all collection
    function deleteDefaultRoyalty() external onlyRole(DEFAULT_ADMIN_ROLE) {
        _deleteDefaultRoyalty();
    }

    /// @dev Reset specific royalty
    /// @param tokenId The token id where to reset the royalty
    function resetTokenRoyalty(uint256 tokenId) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _resetTokenRoyalty(tokenId);
    }

    //=======================================================================
    // [public/override/onlyAllowedOperatorApproval] for OperatorFilter
    //=======================================================================

    function setApprovalForAll(address operator, bool approved) public override onlyAllowedOperatorApproval(operator) { 
        super.setApprovalForAll(operator, approved); 
    }
    
    function safeTransferFrom(address from, address to, uint256 tokenId, uint256 amount, bytes memory data) public override onlyAllowedOperator(from) { 
        super.safeTransferFrom(from, to, tokenId, amount, data); 
    }

    function safeBatchTransferFrom( address from, address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data ) public virtual override onlyAllowedOperator(from) { 
        super.safeBatchTransferFrom(from, to, ids, amounts, data); 
    }

    function _beforeTokenTransfer(address operator, address from, address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)
        internal
        whenNotPaused
        override(ERC1155, ERC1155Supply)
    {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    //=======================================================================
    // [public/override] 
    //=======================================================================

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155, AccessControl, ERC2981)
        returns (bool)
    {
        return interfaceId == type(IERC2981).interfaceId || super.supportsInterface(interfaceId);
    }
}

