    ERC721URIStorageUpgradeable
} from "./internal-upgradeable/ERC721URIStorageUpgradeable.sol";
import { ERC721BurnableUpgradeable } from "./internal-upgradeable/ERC721BurnableUpgradeable.sol";
import {
    EIP712Upgradeable,
    ERC721WithPermitUpgradable
} from "./internal-upgradeable/ERC721WithPermitUpgradable.sol";

import { NonFungibleType } from "./internal-upgradeable/NonFungibleType.sol";
import { AccountRegistryUpgradeable } from "./internal-upgradeable/AccountRegistryUpgradeable.sol";
import { Helper } from "./libraries/Helper.sol";
import { CurrencyTransferLib } from "./libraries/CurrencyTransferLib.sol";
import { ReentrancyGuard } from "./utils/ReentrancyGuard.sol";
import { IERC6551Registry } from "./interfaces/IERC6551Registry.sol";
import { IAccount } from "./interfaces/IAccount.sol";

contract PNFT is
    AccountRegistryUpgradeable,
    NonFungibleType,
    ReentrancyGuard,
    UUPSUpgradeable,
    OwnableUpgradeable,
    ERC721BurnableUpgradeable,
    ERC721EnumerableUpgradeable,
    ERC721URIStorageUpgradeable,
    ERC721WithPermitUpgradable
{
    error NFT__InvalidType();

    event Registered(
        address indexed recipient,
        uint256 indexed typeNFT,
        uint256 tokenId,
        uint256 quantity,
        uint256 soldByType
    );

    using Helper for *;

    address public upgrader;
    uint256 private _idCounter;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
        string calldata name_,
        string calldata symbol_,
        IERC6551Registry registry_,
        address implementation_,
        Operation calldata operation_
    ) external initializer {
        address sender = _msgSender();
        upgrader = sender;

        __Ownable_init_unchained();
        __ReentrancyGuard_init_unchained();
        __ERC721WithPermitUpgradable_init(name_, symbol_);
        __AccountRegistryUpgradeable_init_unchained(registry_, implementation_);
        _setType(1, address(0), 10 ether, 100, operation_);
        _setType(2, address(0), 8 ether, 100, operation_);
        _setType(3, address(0), 5 ether, 100, operation_);
        _setType(4, address(0), 2 ether, 100, operation_);

        _idCounter = 1;
    }

    function setBaseTokenURI(string calldata baseTokenURI_) external onlyOwner {
        _setBaseURI(baseTokenURI_);
    }

    function setType(
        uint256 type_,
        address paymentToken_,
        uint256 price_,
        uint256 limit_,
        Operation calldata operation_
    ) external onlyOwner {
        _setType(type_, paymentToken_, price_, limit_, operation_);
    }

    function buy(uint256 typeNFT_, uint256 quantity_) external payable nonReentrant {
        TypeInfo memory typeInfo = _typeInfo[typeNFT_];
        uint256 total = typeInfo.price * quantity_;
        if (total == 0) revert NFT__InvalidType();
        address sender = _msgSender();
        {
            uint256 refund;
            uint256 value = msg.value;
            if (typeInfo.paymentToken == address(0)) {
                refund = value - total; // will throw underflow error if value < total -> skip refund
                if (refund > 0.1 ether) {
                    CurrencyTransferLib.safeTransferNativeToken(sender, refund);
                    CurrencyTransferLib.safeTransferNativeToken(owner(), total);
                } else {
                    CurrencyTransferLib.safeTransferNativeToken(owner(), value);
                }
            } else {
                CurrencyTransferLib.safeTransferERC20(
                    typeInfo.paymentToken,
                    sender,
                    owner(),
                    total
                );
            }
        }
        _setSold(typeNFT_, quantity_);
        uint256 tokenId = _idCounter;
        emit Registered(sender, typeNFT_, tokenId, quantity_, _sold[typeNFT_]);
        _batchProcess(sender, tokenId, quantity_, typeInfo.operation);
    }

    function recoverToken(address currency_, uint256 amount_) external onlyOwner {
        CurrencyTransferLib.transferCurrency(currency_, address(this), owner(), amount_);
    }

    function getTypeNFT(
        uint256 typeNFT
    ) external view returns (address, uint256, uint256, Operation memory) {
        return (
            _typeInfo[typeNFT].paymentToken,
            _typeInfo[typeNFT].price,
            _typeInfo[typeNFT].limit,
            _typeInfo[typeNFT].operation
        );
    }

    function _batchProcess(
        address account_,
        uint256 tokenId_,
        uint256 totalMint_,
        Operation memory operation_
    ) internal {
        uint256 chainId = block.chainid;
        IERC6551Registry registry = _registry;
        address implementation = _implementation;
        address tokenCollection = address(this);
        address tba;
        bytes memory callData = operation_.data;

        for (uint256 i; i < totalMint_; ) {
            tba = registry.account(implementation, chainId, tokenCollection, tokenId_, 0);
            assembly {
                mstore(add(callData, 0x24), tba)
            }
            operation_.data = callData;

            _safeMint(account_, tokenId_);
            _call(operation_);
            registry.createAccount(
                implementation,
                chainId,
                tokenCollection,
                tokenId_,
                0,
                "0x8129fc1c"
            );
            IAccount(tba).initialize();
            unchecked {
                ++tokenId_;
                ++i;
            }
        }
        _idCounter = tokenId_;
    }

    // The following functions are overrides required by Solidity.

    function _baseURI()
        internal
        view
        override(ERC721URIStorageUpgradeable, ERC721Upgradeable)
        returns (string memory)
    {
        return _baseUri;
    }

    function exists(uint256 tokenId) external view returns (bool) {
        return _exists(tokenId);
    }

    function tokenURI(
        uint256 tokenId
    )
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function _transfer(
        address from_,
        address to_,
        uint256 tokenId_
    ) internal override(ERC721Upgradeable, ERC721WithPermitUpgradable) {
        super._transfer(from_, to_, tokenId_);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal override(ERC721Upgradeable, ERC721EnumerableUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable, ERC721WithPermitUpgradable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function _authorizeUpgrade(address implement_) internal virtual override onlyOwner {}
}

