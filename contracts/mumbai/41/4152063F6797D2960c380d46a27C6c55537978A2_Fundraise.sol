pragma solidity ^0.8.9;

import { Initializable } from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

abstract contract FundToken is Initializable {
  mapping(address => uint256) internal _iouBalances;

  uint256 internal _iouTotalSupply;

  string private _name;
  string private _symbol;

  error BalanceExceeded();
  error AddressZero();

  event FundTokenMinted(address _account, uint256 _amount);
  event FundTokenBurned(address _account, uint256 _amount);

  /**
   * @dev Sets the values for {name} and {symbol}.
   *
   * The default value of {decimals} is 18. To select a different value for
   * {decimals} you should overload it.
   *
   * All two of these values are immutable: they can only be set once during
   * construction.
   */
  function __FundToken_init(string memory name_, string memory symbol_) internal onlyInitializing {
    __FundToken_init_unchained(name_, symbol_);
  }

  function __FundToken_init_unchained(string memory name_, string memory symbol_) internal onlyInitializing {
    _name = name_;
    _symbol = symbol_;
  }

  function name() external view virtual returns (string memory) {
    return _name;
  }

  function symbol() external view virtual returns (string memory) {
    return _symbol;
  }

  function decimals() external view virtual returns (uint8) {
    return 18;
  }

  function fundTokenTotalSupply() external view virtual returns (uint256) {
    return _iouTotalSupply;
  }

  function fundTokenBalanceOf(address account) external view virtual returns (uint256) {
    return _iouBalances[account];
  }

  function _mintFundToken(address account, uint256 amount) internal virtual {
    if (account == address(0)) revert AddressZero();

    _iouTotalSupply += amount;
    unchecked {
      // Overflow not possible: balance + amount is at most totalSupply + amount, which is checked above.
      _iouBalances[account] += amount;
    }
    emit FundTokenMinted(account, amount);
  }

  function _burn(address account, uint256 amount) internal virtual {
    if (account == address(0)) revert AddressZero();

    uint256 accountBalance = _iouBalances[account];
    if (accountBalance < amount) revert BalanceExceeded();
    unchecked {
      _iouBalances[account] = accountBalance - amount;
      // Overflow not possible: amount <= accountBalance <= totalSupply.
      _iouTotalSupply -= amount;
    }

    emit FundTokenBurned(account, amount);
  }
}

