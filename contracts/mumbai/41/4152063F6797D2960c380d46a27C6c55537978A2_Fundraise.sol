pragma solidity ^0.8.9;

import { FundToken } from "./FundToken.sol";
import { FundraiseBase } from "./FundraiseBase.sol";
import { IProjectToken } from "../projectToken/IProjectToken.sol";
import { ITokenManager } from "../tokenManager/ITokenManager.sol";
import { IERC20 } from "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import { SafeERC20 } from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import { Initializable } from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import { ECDSAUpgradeable } from "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";
import { EIP712Upgradeable } from "@openzeppelin/contracts-upgradeable/utils/cryptography/EIP712Upgradeable.sol";

contract Fundraise is Initializable, FundraiseBase, EIP712Upgradeable, FundToken {
  // allows to use safe safeTransfer
  using SafeERC20 for IERC20;
  // x% fundraise fee (range: 0-10000)
  uint16 public fundraiseFee;
  // address to safeTransfer fee
  address public feeWallet;
  // project's emitent address where funds are transferred
  address public projectWallet;
  // project's token used e.g. for rewards
  address public projectToken;
  // usdc token used for fundraising
  address public stablecoin;
  // address of the Token Manager
  address public tokenManager;

  // fee denominator related to fundraiseFee
  uint16 private constant FEE_DENOMINATOR = 10 ** 4;

  // signature data
  bytes32 private constant _INVEST_TYPEHASH = keccak256("InvestmentData(address investor,uint256 amount,bytes32 salt)");
  string private constant _DOMAIN_NAME = "Mosaico";
  string private constant _DOMAIN_VERSION = "0.1.0";

  // signature used for funding
  mapping(bytes => bool) private _signaturesUsed;

  /// @custom:oz-upgrades-unsafe-allow constructor
  constructor() {
    _disableInitializers();
  }

  /**  
    @notice Add new fundraise phase
    @param _fundraiseOwner Fundraise owner's address
    @param _name Fundraise name
    @param _softCap Fundraise soft cap in $. Must have decimals equal to the stablecoin's decimals
    @param _hardCap Fundraise hard cap in $. Must have decimals equal to the stablecoin's decimals
    @param _projectWallet Wallet address to which the fundraise funds will be transferred
    @param _stableCoin Stablecoin address
    @param _projectToken Project Token address
    @param _feePercent Fundraise fee (range: 0-10000), e.g. number 100 equals 1% of fee
    @param _feeWallet Wallet address to which the fee will be transferred
    @param _expiryDate End date of the fundraise in seconds since Unix epoch
  */
  function initialize(
    address _fundraiseOwner,
    string memory _name,
    uint256 _softCap,
    uint256 _hardCap,
    address _projectWallet,
    address _stableCoin,
    address _projectToken,
    uint16 _feePercent,
    address _feeWallet,
    uint256 _expiryDate,
    address _tokenManager
  ) public initializer {
    __AccessControl_init();
    __EIP712_init(_DOMAIN_NAME, _DOMAIN_VERSION);
    __FundToken_init(_name, "IOU");
    __Fundraise_init(_stableCoin, _projectToken, _feePercent, _expiryDate);

    _grantRole(DEFAULT_ADMIN_ROLE, _fundraiseOwner);
    _grantRole(OWNER_ROLE, _fundraiseOwner);

    _setFundraiseCap(_softCap, _hardCap);
    _setProjectWallet(_projectWallet);
    _setFeeWallet(_feeWallet);
    _setTokenManager(_tokenManager);

    _addInitPhase(_expiryDate, hardCap);
  }

  /** 
    @notice Invest in project
    @param investor Investor's address
    @param amount Project token amount 
    @param salt Unique value used for signature
    @param signature Transaction signature
    */
  function invest(
    address investor,
    uint256 amount,
    bytes32 salt,
    bytes memory signature
  ) external checkMilestone atMilestoneExcept(Milestone.fundraiseClosed) checkPhase(activePhaseId) atPhaseExcept(SALE_SUSPENDED) {
    FundraiseData memory fundraiseData = FundraiseData(investor, amount, salt);
    _verifyInvestor(fundraiseData, signature, _msgSender());
    uint256 stablecoinAmount = _transferFunds(investor, amount);
    emit FundsInvested(investor, amount, stablecoinAmount, totalFundsRaised);
  }

  function swapFundTokensToProjectTokens() external atMilestoneExcept(Milestone.softCapNotReached) {
    _burnFundMintProjectTokens(_msgSender());
  }

  function swapFundTokensToProjectTokensFrom(address investor) external atMilestoneExcept(Milestone.softCapNotReached) {
    _burnFundMintProjectTokens(investor);
  }

  function returnFunds() external atMilestone(Milestone.fundraiseClosed) {
    _burnFundReturnStablecoin(_msgSender());
  }

  function returnFundsTo(address investor) external atMilestone(Milestone.fundraiseClosed) onlyRole(OWNER_ROLE) {
    _burnFundReturnStablecoin(investor);
  }

  function setProjectWallet(address newProjectWallet) external onlyRole(OWNER_ROLE) {
    _setProjectWallet(newProjectWallet);
  }

  function setFeeWallet(address newFeeWallet) external onlyRole(OWNER_ROLE) {
    _setFeeWallet(newFeeWallet);
  }

  function setTokenManager(address newTokenManager) external onlyRole(OWNER_ROLE) {
    _setTokenManager(newTokenManager);
  }

  function version() external view virtual returns (string memory) {
    return _DOMAIN_VERSION;
  }

  /**
   * @dev Sets non-zero fundraise values
   * Some of these values are immutable: they can only be set once during construction.
   */
  function __Fundraise_init(address _stableCoin, address _projectToken, uint16 _feePercent, uint256 _expiryDate) internal onlyInitializing {
    if (_stableCoin == address(0) || _projectToken == address(0)) revert AddressZero();
    if (_feePercent == 0 || _feePercent > FEE_DENOMINATOR) revert InvalidFeePercent(_feePercent);
    stablecoin = _stableCoin;
    projectToken = _projectToken;
    fundraiseFee = _feePercent;
    _setExpiryDate(_expiryDate);
  }

  function _setProjectWallet(address newProjectWallet) internal {
    if (newProjectWallet == address(0)) revert AddressZero();
    address oldAddress = projectWallet;
    projectWallet = newProjectWallet;
    emit ProjectWalletChanged(oldAddress, newProjectWallet);
  }

  function _setFeeWallet(address newFeeWallet) internal {
    if (newFeeWallet == address(0)) revert AddressZero();
    address oldAddress = projectWallet;
    feeWallet = newFeeWallet;
    emit FeeWalletChanged(oldAddress, newFeeWallet);
  }

  function _setTokenManager(address newTokenManager) internal {
    if (newTokenManager == address(0)) revert AddressZero();
    address oldAddress = newTokenManager;
    tokenManager = newTokenManager;
    emit TokenManagerSet(oldAddress, newTokenManager);
  }

  function _verifyInvestor(FundraiseData memory fundraiseData, bytes memory signature, address signer) internal {
    if (_signaturesUsed[signature] == true) revert SignatureAlreadyUsed();
    bytes32 digest = _hashTypedDataV4(
      keccak256(abi.encode(_INVEST_TYPEHASH, fundraiseData.investor, fundraiseData.amount, fundraiseData.salt))
    );
    address recoveredAddress = ECDSAUpgradeable.recover(digest, signature);
    if (recoveredAddress != signer) revert InvalidSignature();
    _signaturesUsed[signature] = true;
  }

  function _burnFundMintProjectTokens(address investor) internal {
    uint256 _iouBalance = _iouBalances[investor];
    if (_iouBalance > 0) {
      _burn(investor, _iouBalance);
      _mintProjectTokens(investor, _iouBalance);
    }
  }

  function _mintProjectTokens(address investor, uint256 amount) internal {
    ITokenManager(tokenManager).delegateMint(investor, amount);
  }

  function _transferFunds(address investor, uint256 amount) internal returns (uint256 stablecoinAmount) {
    if (allPhases[activePhaseId].soldTokens + amount > allPhases[activePhaseId].maxTokensAmount)
      revert MaxTokensAtThisPhase(allPhases[activePhaseId].maxTokensAmount);

    /* RATE_DENOMINATOR is used to correctly calculate stablecoin amount which depends on 
    1) tokenRate and 2) decimals difference. So the formula:
    RATE_DENOMINATOR = 10^(stablecoin.decimals) * 10^(projectToken.decimals-stablecoin.decimals)
    can be simplified as below.
    */
    uint256 RATE_DENOMINATOR = 10 ** IProjectToken(projectToken).decimals();

    // calculate stablecoin amount which needs to be transferred
    stablecoinAmount = (amount * allPhases[activePhaseId].tokenRate) / RATE_DENOMINATOR;
    if (stablecoinAmount < allPhases[activePhaseId].minInvest || stablecoinAmount > allPhases[activePhaseId].maxInvest)
      revert InvalidInvestValue(stablecoinAmount);

    totalFundsRaised += stablecoinAmount;
    _fundsBalances[investor] += stablecoinAmount;
    allPhases[activePhaseId].soldTokens += amount;

    // check if safeTransfer project tokens (if softCap reached) or mint IOU
    if (activeMilestone == Milestone.softCapNotReached) {
      IERC20(stablecoin).safeTransferFrom(investor, address(this), stablecoinAmount);
      _mintFundToken(investor, amount);
    } else if (activeMilestone == Milestone.softCapReached) {
      uint256 feeAmount = (stablecoinAmount * fundraiseFee) / FEE_DENOMINATOR;
      uint256 projectFundsAmount = stablecoinAmount - feeAmount;

      IERC20(stablecoin).safeTransferFrom(investor, feeWallet, feeAmount);
      IERC20(stablecoin).safeTransferFrom(investor, projectWallet, projectFundsAmount);

      _mintProjectTokens(investor, amount);
    } else revert InvalidAtThisMilestone(activeMilestone);
  }

  function _releaseFundsLockedOnFundraise() internal override {
    uint256 fundsLocked = IERC20(stablecoin).balanceOf(address(this));
    // split stablecoin funds locked untill soft cap is reached (% for fundraising platform)
    uint256 feeAmount = (fundsLocked * fundraiseFee) / FEE_DENOMINATOR;
    IERC20(stablecoin).safeTransfer(feeWallet, feeAmount);
    unchecked {
      fundsLocked -= feeAmount;
    }
    IERC20(stablecoin).safeTransfer(projectWallet, fundsLocked);
  }

  function _burnFundReturnStablecoin(address investor) internal {
    if (!_isSoftCapReached() && _iouBalances[investor] > 0 && _fundsBalances[investor] > 0) {
      uint256 fundTokenAmount = _iouBalances[investor];
      uint256 investmentAmount = _fundsBalances[investor];
      _burn(investor, fundTokenAmount);
      IERC20(stablecoin).safeTransfer(investor, investmentAmount);
      _fundsBalances[investor] -= investmentAmount;
      emit FundsReturned(investor, fundTokenAmount, investmentAmount);
    }
  }
}

