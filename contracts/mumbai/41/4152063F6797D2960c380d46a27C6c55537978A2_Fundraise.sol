pragma solidity ^0.8.9;

import { IFundraise } from "./IFundraise.sol";
import { AccessControlUpgradeable } from "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";

abstract contract FundraiseBase is IFundraise, AccessControlUpgradeable {
  // contract roles
  bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");

  // current fundraise phase (private / pre / public / public 2 ...)
  uint8 public activePhaseId;
  // counter of total phases defined so far
  uint8 public phasesCounter;
  // latest milestone reached by fundraise (soft- / hard-cap)
  Milestone public activeMilestone;

  // total funds raised by project from the beginning till now (in $)
  // need it because funds from wallet can be used by emitent before hard cap is reached
  uint256 public totalFundsRaised;
  // fundraise capitalization milestones (in $)
  uint256 public hardCap;
  uint256 public softCap;
  // fundraise final date
  uint256 public expiryDate;
  // defined phases (can be more than 3, eg. ICO2, ICO3 etc.): phaseId => Phase
  mapping(uint8 => Phase) public allPhases;
  // investment amount of each investor (in $) used if softcap not reached and funds need to be return
  mapping(address => uint256) internal _fundsBalances;
  // default phaseId: 0 as sale suspended
  uint8 internal constant SALE_SUSPENDED = 0;

  // phases modifiers
  modifier atMilestone(Milestone _milestone) {
    if (activeMilestone != _milestone) revert InvalidAtThisMilestone(activeMilestone);
    _;
  }

  modifier atMilestoneExcept(Milestone _milestone) {
    // always check if milestone is not upcoming
    if (activeMilestone == Milestone.upcoming || activeMilestone == _milestone) revert InvalidAtThisMilestone(activeMilestone);
    _;
  }

  modifier atPhase(uint8 _phase) {
    if (activePhaseId != _phase) revert InvalidAtThisPhase(activePhaseId);
    _;
  }

  modifier atPhaseExcept(uint8 _phase) {
    if (activePhaseId == _phase) revert InvalidAtThisPhase(activePhaseId);
    _;
  }

  // check fundraise expiry time and caps
  modifier checkMilestone() {
    if (!_isFundraiseExpired()) {
      _;
      _isSoftCapReached();
      _isHardCapReached();
    }
  }

  // check if phase is completed
  modifier checkPhase(uint8 _phaseId) {
    if (!_isTimeExpired(_phaseId)) {
      _;
      _isTokenLimit(_phaseId);
    }
  }

  function setupFundraise() external onlyRole(OWNER_ROLE) atMilestone(Milestone.upcoming) {
    // check all fundraise params and if ok start fundraise
    if (phasesCounter == 0) revert InvalidAtThisPhase(activePhaseId);
    _setMilestone(Milestone.softCapNotReached);
  }

  /**  
    @notice Add new fundraise phase
    @param _phaseTypeId Type (private-/ pre-/ public sale) represented by number
    @param _startDate Start date in seconds since Unix epoch
    @param _endDate End date in seconds since Unix epoch
    @param _tokenRate Project token rate in relation to the stablecoin. Must have decimals equal to the stablecoin's decimals
    @param _minInvest Minimum investment value (in $). Must have decimals equal to the stablecoin's decimals
    @param _maxInvest Maximum investment value (in $). Must have decimals equal to the stablecoin's decimals
    @param _maxTokensAmount Max Project Token amount available for sale. Must have decimals equal to the Project Token's decimals
    */
  function addNewPhase(
    uint8 _phaseTypeId,
    uint256 _startDate,
    uint256 _endDate,
    uint256 _tokenRate,
    uint256 _minInvest,
    uint256 _maxInvest,
    uint256 _maxTokensAmount
  ) external onlyRole(OWNER_ROLE) {
    if (_phaseTypeId == 0 || _phaseTypeId > uint8(type(PhaseType).max)) revert InvalidPhaseType(_phaseTypeId);
    Phase memory _phase = Phase(
      PhaseType(_phaseTypeId),
      PhaseStatus.inactive,
      _startDate,
      _endDate,
      _tokenRate,
      _minInvest,
      _maxInvest,
      _maxTokensAmount,
      0
    );
    _addNewPhase(_phase);
  }

  function editPhaseTimeLimits(uint8 _phaseId, uint256 _startDate, uint256 _endDate) external onlyRole(OWNER_ROLE) {
    _editPhaseTimeLimits(_phaseId, _startDate, _endDate);
  }

  function editPhaseTokenDetails(uint8 _phaseId, uint256 _tokenRate, uint256 _maxTokensAmount) external onlyRole(OWNER_ROLE) {
    _editPhaseTokenDetails(_phaseId, _tokenRate, _maxTokensAmount);
  }

  function editPhaseInvestLimits(uint8 _phaseId, uint256 _minInvest, uint256 _maxInvest) external onlyRole(OWNER_ROLE) {
    _editPhaseInvestLimits(_phaseId, _minInvest, _maxInvest);
  }

  function setSoftCap(uint256 newSoftCap) external onlyRole(OWNER_ROLE) atMilestone(Milestone.softCapNotReached) {
    _setFundraiseCap(newSoftCap, hardCap);
  }

  function setHardCap(uint256 newHardCap) external onlyRole(OWNER_ROLE) atMilestoneExcept(Milestone.fundraiseClosed) {
    _setFundraiseCap(softCap, newHardCap);
  }

  function setMilestone(Milestone newMilestone) external onlyRole(OWNER_ROLE) atMilestoneExcept(Milestone.upcoming) {
    _setMilestone(newMilestone);
  }

  function setExpiryDate(uint256 newExpiryDate) external onlyRole(OWNER_ROLE) {
    _setExpiryDate(newExpiryDate);
  }

  function startPhase(uint8 newPhaseId) external onlyRole(OWNER_ROLE) checkMilestone checkPhase(activePhaseId) {
    _startPhase(newPhaseId);
  }

  function investmentBalance(address investor) external view virtual returns (uint256) {
    return _fundsBalances[investor];
  }

  function _addInitPhase(uint256 _endDate, uint256 _maxTokensAmount) internal {
    // default fundraise phase 0: sale suspended
    Phase memory _phase = Phase(PhaseType.suspendedSale, PhaseStatus.active, block.timestamp, _endDate, 0, 0, 0, _maxTokensAmount, 0);
    allPhases[phasesCounter] = _phase;
    emit PhaseAdded(phasesCounter);
    phasesCounter++;
  }

  function _addNewPhase(Phase memory _phase) internal {
    if (_phase.tokenRate == 0 || _phase.maxTokensAmount == 0) revert InvalidPhaseTokenParams();
    if (_phase.maxInvest == 0 || _phase.minInvest >= _phase.maxInvest) revert InvalidPhaseInvestParams();
    if (
      (phasesCounter > 1 && _phase.startDate < allPhases[phasesCounter - 1].endDate) ||
      _phase.startDate <= block.timestamp ||
      _phase.startDate >= _phase.endDate ||
      _phase.endDate > expiryDate
    ) revert InvalidTimeframe();

    allPhases[phasesCounter] = _phase;
    emit PhaseAdded(phasesCounter);
    phasesCounter++;
  }

  function _editPhaseTimeLimits(uint8 _phaseId, uint256 _startDate, uint256 _endDate) internal {
    if (allPhases[_phaseId].startDate <= block.timestamp || allPhases[_phaseId].status != PhaseStatus.inactive)
      revert PhaseExpired(_phaseId);
    if (
      (allPhases[phasesCounter - 1].phaseType != PhaseType.suspendedSale && _startDate < allPhases[_phaseId - 1].endDate) ||
      _startDate >= _endDate ||
      _endDate > expiryDate
    ) revert InvalidTimeframe();

    allPhases[_phaseId].startDate = _startDate;
    allPhases[_phaseId].endDate = _endDate;
    emit PhaseDetailsChanged(_phaseId);
  }

  function _editPhaseTokenDetails(uint8 _phaseId, uint256 _tokenRate, uint256 _maxTokensAmount) internal {
    if (allPhases[_phaseId].status == PhaseStatus.expired) revert PhaseExpired(_phaseId);
    if (_tokenRate == 0 || _maxTokensAmount == 0) revert InvalidPhaseTokenParams();
    allPhases[_phaseId].tokenRate = _tokenRate;
    allPhases[_phaseId].maxTokensAmount = _maxTokensAmount;
    emit PhaseDetailsChanged(_phaseId);
  }

  function _editPhaseInvestLimits(uint8 _phaseId, uint256 _minInvest, uint256 _maxInvest) internal {
    if (allPhases[_phaseId].status == PhaseStatus.expired) revert PhaseExpired(_phaseId);
    if (_maxInvest == 0 || _minInvest >= _maxInvest) revert InvalidPhaseInvestParams();
    allPhases[_phaseId].minInvest = _minInvest;
    allPhases[_phaseId].maxInvest = _maxInvest;
    emit PhaseDetailsChanged(_phaseId);
  }

  function _setFundraiseCap(uint256 _softCap, uint256 _hardCap) internal checkMilestone {
    if (_softCap >= _hardCap) revert InvalidValue();
    if (softCap != _softCap) softCap = _softCap;
    if (hardCap != _hardCap) hardCap = _hardCap;
    emit FundraiseCapChanged(_softCap, _hardCap);
  }

  function _setMilestone(Milestone _newMilestone) internal {
    if (_newMilestone == Milestone.upcoming) revert InvalidValue();
    activeMilestone = _newMilestone;
    emit MilestoneChanged(uint8(_newMilestone));
  }

  function _setExpiryDate(uint256 _expiryDate) internal {
    if (_expiryDate <= block.timestamp) revert InvalidTimeframe();
    expiryDate = _expiryDate;
    emit ExpiryDateChanged(_expiryDate);
  }

  function _startPhase(uint8 _phaseId) internal {
    if (_phaseId >= phasesCounter) revert InvalidPhaseId(_phaseId);
    if (allPhases[_phaseId].status != PhaseStatus.active) {
      if (allPhases[_phaseId].startDate > block.timestamp || allPhases[_phaseId].status == PhaseStatus.expired)
        revert PhaseCantBeStarted(_phaseId);
      if (allPhases[activePhaseId].status != PhaseStatus.expired) allPhases[activePhaseId].status = PhaseStatus.inactive;
      activePhaseId = _phaseId;
      allPhases[_phaseId].status = PhaseStatus.active;
      emit PhaseChanged(_phaseId);
    }
  }

  function _isSoftCapReached() internal returns (bool _isReached) {
    _isReached = totalFundsRaised >= softCap;
    if (_isReached && activeMilestone == Milestone.softCapNotReached) {
      _setMilestone(Milestone.softCapReached);
      _releaseFundsLockedOnFundraise();
    }
  }

  function _isHardCapReached() internal returns (bool _isReached) {
    _isReached = totalFundsRaised >= hardCap;
    if (_isReached && activeMilestone != Milestone.fundraiseClosed) {
      _setMilestone(Milestone.fundraiseClosed);
      _startPhase(SALE_SUSPENDED);
    }
  }

  function _isTokenLimit(uint8 _phaseId) internal returns (bool _isLimitReached) {
    _isLimitReached = allPhases[_phaseId].soldTokens >= allPhases[_phaseId].maxTokensAmount;

    if (_isLimitReached) {
      allPhases[_phaseId].status = PhaseStatus.expired;
      _startPhase(SALE_SUSPENDED);
    }
  }

  function _isTimeExpired(uint8 _phaseId) internal returns (bool _isExpired) {
    _isExpired = block.timestamp >= allPhases[_phaseId].endDate;

    if (_isExpired) {
      allPhases[_phaseId].status = PhaseStatus.expired;
      _startPhase(SALE_SUSPENDED);
    }
  }

  function _isFundraiseExpired() internal returns (bool _isExpired) {
    if (activeMilestone != Milestone.fundraiseClosed) {
      _isExpired = block.timestamp >= expiryDate;

      if (_isExpired) {
        _setMilestone(Milestone.fundraiseClosed);
      }
    }
  }

  function _releaseFundsLockedOnFundraise() internal virtual;
}

