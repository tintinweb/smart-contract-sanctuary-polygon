pragma solidity 0.8.19;

import {Initializable} from "openzeppelin-contracts-upgradeable/contracts/proxy/utils/Initializable.sol";
import {Ownable2StepUpgradeable} from "openzeppelin-contracts-upgradeable/contracts/access/Ownable2StepUpgradeable.sol";
import {UUPSUpgradeable} from "openzeppelin-contracts-upgradeable/contracts/proxy/utils/UUPSUpgradeable.sol";
import {PausableUpgradeable} from "openzeppelin-contracts-upgradeable/contracts/security/PausableUpgradeable.sol";
import {IUniswapV2Factory} from "src/interfaces/IUniswapV2.sol";
import {ITPVaultFactory} from "src/interfaces/ITPVaultFactory.sol";
import {ITPTokenFactory} from "src/interfaces/ITPTokenFactory.sol";
import {ITPVault} from "src/interfaces/ITPVault.sol";
import {WhitelistAndBlacklistUpgradeable} from "src/utils/whitelist/WhitelistAndBlacklist.sol";

contract TPManager is
    Initializable,
    WhitelistAndBlacklistUpgradeable,
    Ownable2StepUpgradeable,
    PausableUpgradeable,
    UUPSUpgradeable
{
    address public admin;
    address public liquidityManager;
    address public tokenFactory;
    address public vaultFactory;
    address public humanFactory;
    address public swapManager;
    address public uv2Factory;
    address public usdt;

    event InitManager(
        address owner,
        address admin,
        address liquidityManager,
        address tokenFactory,
        address vaultFactory,
        address humanFactory,
        address swapManager,
        address usdt,
        address uv2Factory
    );
    event UpdateConfig(
        address admin,
        address liquidityManager,
        address tokenFactory,
        address vaultFactory,
        address humanFactory,
        address swapManager
    );
    event CreateVaultAndToken(address indexed vault, address indexed token, bytes32 indexed symbolHash);

    modifier onlyAdmin() {
        require(msg.sender == admin, "TPE001");
        _;
    }

    constructor() {
        _disableInitializers();
    }

    function initialize(
        address newOwner,
        address newAdmin,
        address liquidityManager_,
        address tokenFactory_,
        address vaultFactory_,
        address humanFactory_,
        address swapManager_,
        address usdt_,
        address uv2Factory_,
        address signer_
    ) public initializer {
        __WhitelistAndBlacklistUpgradeable_init(signer_, bytes32(0), "");
        __Ownable2Step_init();
        __UUPSUpgradeable_init();

        _transferOwnership(newOwner);

        admin = newAdmin;
        liquidityManager = liquidityManager_;
        tokenFactory = tokenFactory_;
        vaultFactory = vaultFactory_;
        humanFactory = humanFactory_;
        swapManager = swapManager_;
        usdt = usdt_;
        uv2Factory = uv2Factory_;

        emit InitManager(
            newOwner, admin, liquidityManager, tokenFactory, vaultFactory, humanFactory, swapManager, usdt, uv2Factory
        );
    }

    function checkTransferAddresses(address token, address from, address to) public view returns (bool) {
        if (paused()) return false;
        if (_isPair(token, from)) {
            if (!(to == liquidityManager || to == swapManager)) return false;
        } else {
            if (!(whitelisted(from) || from == swapManager || from == liquidityManager || from == address(0))) {
                return false;
            }
        }

        if (_isPair(token, to)) {
            if (!(from == liquidityManager || from == swapManager)) return false;
        } else {
            if (!(whitelisted(to) || to == swapManager || to == liquidityManager || to == address(0))) return false;
        }

        return true;
    }

    function pause() public onlyAdmin {
        _pause();
    }

    function unpause() public onlyAdmin {
        _unpause();
    }

    function blacklist(address account, bool status) public override onlyAdmin whenNotPaused {
        _blacklist(account, status);
    }

    function whitelist(address account) public override onlyAdmin whenNotPaused {
        _whitelist(account);
    }

    function humanFactoryWhitelist(address account) public whenNotPaused {
        require(msg.sender == humanFactory, "TPE002");
        _whitelist(account);
    }

    function updateConfig(
        address admin_,
        address liquidityManager_,
        address swapManager_,
        address tokenFactory_,
        address vaultFactory_,
        address humanFactory_
    ) public onlyOwner {
        admin = admin_;
        liquidityManager = liquidityManager_;
        swapManager = swapManager_;
        tokenFactory = tokenFactory_;
        vaultFactory = vaultFactory_;
        humanFactory = humanFactory_;

        emit UpdateConfig(admin, liquidityManager, tokenFactory, vaultFactory, humanFactory, swapManager);
    }

    function createVaultAndToken(
        address companyOwner,
        uint256 liquidityUnlockTime,
        uint256 liquidityMinAmount,
        uint256 maxSupply,
        bytes32 merkleRoot,
        string memory uri,
        string calldata name,
        string calldata symbol
    ) public onlyAdmin whenNotPaused returns (address vault, address token) {
        vault = ITPVaultFactory(vaultFactory).createVault(
            companyOwner, liquidityUnlockTime, liquidityMinAmount, merkleRoot, uri
        );
        _whitelist(vault);
        token = ITPTokenFactory(tokenFactory).createToken(companyOwner, vault, maxSupply, name, symbol);
        ITPVault(vault).setToken(token);

        emit CreateVaultAndToken(vault, token, keccak256(abi.encodePacked(symbol)));
    }

    // solhint-disable-next-line no-empty-blocks
    function _authorizeUpgrade(address) internal override onlyOwner {}

    // solhint-disable-next-line no-empty-blocks
    function _authSetSigner(address) internal virtual override onlyAdmin {}

    // solhint-disable-next-line no-empty-blocks
    function _authUpdateMerkleRoot(bytes32, string memory) internal virtual override onlyAdmin {}

    function _isPair(address token, address account) internal view returns (bool) {
        if (account == address(0)) return false;
        return account == IUniswapV2Factory(uv2Factory).getPair(token, usdt);
    }
}

