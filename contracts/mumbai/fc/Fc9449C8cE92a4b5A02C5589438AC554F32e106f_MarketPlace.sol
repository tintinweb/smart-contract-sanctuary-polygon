/// @title ERC-20 Token template
contract MarketPlace {
    string private myName;
    string private mySymbol;

    address public wMetaex = 0x404043a64Cfeeb51d0010475e75732fa98128eCA;
    uint64 private pMetaex = 20;
    address public wMobiup = 0x2919771f951fd264445d4D4c682EE5D8326B93ec;
    uint64 private pMobiup = 10;

    constructor() {
        myName = "MarketPlace";
        mySymbol = "MKTP";
    }

    function name() public view returns(string memory) {
        return myName;
    }

    function symbol() public view returns(string memory) {
        return mySymbol;
    }
    
    function trade(address _addressCollection, address _addressWalletFrom, address _addressWalletTo, uint256 _tokenId) public payable returns (bool) {
        IERC721(_addressCollection).transferFrom(_addressWalletFrom, _addressWalletTo, _tokenId);

        uint256 amountMobiup = msg.value * pMobiup / 100;
        payable(wMobiup).transfer(amountMobiup);

        uint256 amountMetaexp = msg.value * pMetaex / 100;
        payable(wMetaex).transfer(amountMetaexp);

        payable(_addressWalletFrom).transfer(address(this).balance);

        return true;
    }

    fallback() external payable {
        revert();
    }

    receive() external payable {
        payable(wMetaex).transfer(address(this).balance);
    }
}