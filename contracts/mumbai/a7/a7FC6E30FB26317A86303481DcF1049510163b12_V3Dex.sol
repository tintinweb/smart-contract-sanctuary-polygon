library Path {
    using BytesLib for bytes;
    uint256 private constant ADDR_SIZE = 20;
    uint256 private constant FEE_SIZE = 3;
    uint256 private constant NEXT_OFFSET = ADDR_SIZE + FEE_SIZE;
    uint256 private constant POP_OFFSET = NEXT_OFFSET + ADDR_SIZE;
    uint256 private constant MULTIPLE_POOLS_MIN_LENGTH = POP_OFFSET + NEXT_OFFSET;

    function hasMultiplePools(bytes memory path) internal pure returns (bool) {
        return path.length >= MULTIPLE_POOLS_MIN_LENGTH;
    }

    function numPools(bytes memory path) internal pure returns (uint256) {
        return ((path.length - ADDR_SIZE) / NEXT_OFFSET);
    }

    function decodeFirstPool(bytes memory path)
    internal
    pure
    returns (
        address tokenA,
        address tokenB,
        uint24 fee
    )
    {
        tokenA = path.toAddress(0);
        fee = path.toUint24(ADDR_SIZE);
        tokenB = path.toAddress(NEXT_OFFSET);
    }

    function getFirstPool(bytes memory path) internal pure returns (bytes memory) {
        return path.slice(0, POP_OFFSET);
    }

    function skipToken(bytes memory path) internal pure returns (bytes memory) {
        return path.slice(NEXT_OFFSET, path.length - NEXT_OFFSET);
    }
}

library TransferHelper {
    function addressToString(address _addr) internal pure returns (string memory) {
        bytes32 value = bytes32(uint256(uint160(_addr)));
        bytes memory alphabet = "0123456789abcdef";

        bytes memory str = new bytes(42);
        str[0] = '0';
        str[1] = 'x';
        for (uint256 i = 0; i < 20; i++) {
            str[2 + i * 2] = alphabet[uint8(value[i + 12] >> 4)];
            str[3 + i * 2] = alphabet[uint8(value[i + 12] & 0x0f)];
        }
        return string(str);
    }

    function uintToString(uint256 _value) internal pure returns (string memory) {
        if (_value == 0) {
            return "0";
        }
        uint256 temp = _value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (_value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + _value % 10));
            _value /= 10;
        }
        return string(buffer);
    }

    function safeTransferFrom(
        address token,
        address from,
        address to,
        uint256 value,
        uint256 balance
    ) internal {
        (bool success, bytes memory data) =
        token.call(abi.encodeWithSelector(IERC20.transferFrom.selector, from, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), string(abi.encodePacked("safeTransferFrom|token|", addressToString(token), "|from|", addressToString(from), "|to|", addressToString(to), "|value|", uintToString(value), "|balance|", uintToString(balance))));
    }

    function safeTransfer(
        address token,
        address to,
        uint256 value
    ) internal {
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(IERC20.transfer.selector, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), string(abi.encodePacked("safeTransfer|token|", addressToString(token), "|to|", addressToString(to), "|value|", uintToString(value))));
    }

    function safeTransferETH(address to, uint256 value) internal {
        (bool success,) = to.call{value : value}(new bytes(0));
        require(success, string(abi.encodePacked("safeTransferETH|to|", addressToString(to), "|value|", uintToString(value))));
    }
}

interface IWETH is IERC20 {
    function deposit() external payable;

    function withdraw(uint256) external;
}

contract V3Dex is Ownable, ReentrancyGuard {
    struct ExactInputParams {
        IV3Factory factory;
        address[] tokenList;
        uint24[] feeList;
        address recipient;
        uint256 amountIn;
        uint256 amountOutMinimum;
        uint160 sqrtPriceLimitX96;
        bool forceUseWETH;
    }

    struct ExactOutputParams {
        IV3Factory factory;
        address[] tokenList;
        uint24[] feeList;
        address recipient;
        uint256 amountOut;
        uint256 amountInMaximum;
        uint160 sqrtPriceLimitX96;
        bool forceUseWETH;
    }

    struct swapItem {
        uint256 balance0;
        bool hasAlreadyPaid;
        bool hasMultiplePools;
        address recipient;
        address payer;
        bytes path;
        string[6] error;
        uint256 balance1;
    }

    struct SwapCallbackData {
        IV3Factory factory;
        bytes path;
        address payer;
        bool forceUseWETH;
    }

    struct FactoryItem {
        string factoryName;
        IV3Factory factory;
    }

    using Path for bytes;
    using SafeCast for uint256;
    using Strings for uint256;
    uint160 internal constant MIN_SQRT_RATIO = 4295128739;
    uint160 internal constant MAX_SQRT_RATIO = 1461446703485210103287273052203988822378723970342;
    uint256 private constant DEFAULT_AMOUNT_IN_CACHED = type(uint256).max;
    uint256 private amountInCached = DEFAULT_AMOUNT_IN_CACHED;
    address public WETH;
    mapping(IV3Factory => bool) public factoryList;
    mapping(IV3Factory => FactoryItem) public factoryInfoList;

    modifier isAddressOneOrTwo(address _address) {
        require(_address != address(1) && _address != address(2), "Invalid address");
        _;
    }

    function setWETH(address _WETH) external onlyOwner {
        WETH = _WETH;
    }

    function addFactoryList(IV3Factory[] memory _factoryList, string[] memory _factoryNameList, bool _status) external onlyOwner {
        uint256 _num = _factoryList.length;
        for (uint256 i = 0; i < _num; i++) {
            factoryList[_factoryList[i]] = _status;
            factoryInfoList[_factoryList[i]] = FactoryItem({
            factoryName : _factoryNameList[i],
            factory : _factoryList[i]
            });
        }
    }

    function _refundETH() internal {
        if (address(this).balance > 0) {
            TransferHelper.safeTransferETH(msg.sender, address(this).balance);
        }
    }

    function _concatStringList(string[6] memory _strList) internal pure returns (string memory) {
        uint256 _num = _strList.length;
        bytes memory encodedData;
        for (uint i = 0; i < _num; i++) {
            encodedData = abi.encodePacked(encodedData, _strList[i]);
        }
        return string(encodedData);
    }

    function _encodeExactInputPool(address[] memory A, uint24[] memory B) internal pure returns (bytes memory) {
        bytes memory encodedData = abi.encodePacked(A[0], B[0]);
        for (uint i = 1; i < B.length; i++) {
            encodedData = abi.encodePacked(encodedData, A[i], B[i]);
        }
        return abi.encodePacked(encodedData, A[A.length - 1]);
    }

    function encodeExactInputPool(address[] memory A, uint24[] memory B) external pure returns (bytes memory) {
        return _encodeExactInputPool(A, B);
    }

    function _pay(
        address token,
        address payer,
        address recipient,
        uint256 value,
        bool forceUseWETH
    ) internal {
        if (!forceUseWETH && token == WETH && address(this).balance >= value) {
            IWETH(WETH).deposit{value : value}();
            IWETH(WETH).transfer(recipient, value);
        } else if (payer == address(this)) {
            TransferHelper.safeTransfer(token, recipient, value);
        } else {
            TransferHelper.safeTransferFrom(token, payer, recipient, value, address(this).balance);
        }
    }

    function _V3SwapCallback(
        int256 amount0Delta,
        int256 amount1Delta,
        bytes calldata _data
    ) internal {
        require(amount0Delta > 0 || amount1Delta > 0);
        SwapCallbackData memory data = abi.decode(_data, (SwapCallbackData));
        (address tokenIn, address tokenOut, uint24 fee) = data.path.decodeFirstPool();
        require(factoryList[data.factory], "Unsupported factory contract");
        require(data.factory.getPool(tokenIn, tokenOut, fee) == msg.sender, "Invalid pool contract");
        (bool isExactInput, uint256 amountToPay) =
        amount0Delta > 0
        ? (tokenIn < tokenOut, uint256(amount0Delta))
        : (tokenOut < tokenIn, uint256(amount1Delta));

        if (isExactInput) {
            _pay(tokenIn, data.payer, msg.sender, amountToPay, data.forceUseWETH);
        } else {
            if (data.path.hasMultiplePools()) {
                data.path = data.path.skipToken();
                _exactOutputInternal(amountToPay, msg.sender, 0, data);
            } else {
                amountInCached = amountToPay;
                _pay(tokenOut, data.payer, msg.sender, amountToPay, data.forceUseWETH);
            }
        }
    }

    function pancakeV3SwapCallback(
        int256 amount0Delta,
        int256 amount1Delta,
        bytes calldata _data
    ) external {
        _V3SwapCallback(amount0Delta, amount1Delta, _data);
    }

    function uniswapV3SwapCallback(int256 amount0Delta,
        int256 amount1Delta,
        bytes calldata _data) external {
        _V3SwapCallback(amount0Delta, amount1Delta, _data);
    }

    function _exactInputInternal(
        uint256 amountIn,
        address recipient,
        uint160 sqrtPriceLimitX96,
        SwapCallbackData memory data
    ) private returns (uint256 amountOut) {
        (address tokenIn, address tokenOut, uint24 fee) = data.path.decodeFirstPool();
        bool zeroForOne = tokenIn < tokenOut;
        (int256 amount0, int256 amount1) =
        IV3Pool(data.factory.getPool(tokenIn, tokenOut, fee)).swap(
            recipient,
            zeroForOne,
            amountIn.toInt256(),
            sqrtPriceLimitX96 == 0
            ? (zeroForOne ? MIN_SQRT_RATIO + 1 : MAX_SQRT_RATIO - 1)
            : sqrtPriceLimitX96,
            abi.encode(data)
        );
        return uint256(- (zeroForOne ? amount1 : amount0));
    }

    function exactInputSingle(
        ExactInputParams memory params
    ) internal returns (uint256 amountOut)
    {
        swapItem memory x = new swapItem[](1)[0];
        x.balance0 = IERC20(WETH).balanceOf(address(this));
        require(factoryList[params.factory], "Unsupported factory contract");
        x.recipient = params.forceUseWETH ? params.recipient : ((params.tokenList[params.tokenList.length - 1] == WETH) ? address(this) : params.recipient);
        x.path = _encodeExactInputPool(params.tokenList, params.feeList);
        x.payer = x.hasAlreadyPaid ? address(this) : msg.sender;
        amountOut = _exactInputInternal(
            params.amountIn,
            x.recipient,
            params.sqrtPriceLimitX96,
            SwapCallbackData({
        factory : params.factory,
        path : x.path,
        payer : x.payer,
        forceUseWETH : params.forceUseWETH
        })
        );
        x.error = ["amountIn|", (params.amountIn).toString(), "|amountOut|", (amountOut).toString(), "|amountOutMinimum|", (params.amountOutMinimum).toString()];
        require(amountOut >= params.amountOutMinimum, _concatStringList(x.error));
        _refundETH();
        x.balance1 = IERC20(WETH).balanceOf(address(this));
        if (x.balance1 - x.balance0 > 0) {
            IWETH(WETH).withdraw(x.balance1 - x.balance0);
            TransferHelper.safeTransferETH(params.recipient, x.balance1 - x.balance0);
        }
    }

    function exactInput(
        ExactInputParams memory params
    ) internal returns (uint256 amountOut) {
        swapItem memory x = new swapItem[](1)[0];
        x.balance0 = IERC20(WETH).balanceOf(address(this));
        require(factoryList[params.factory], "Unsupported factory contract");
        x.recipient = params.forceUseWETH ? params.recipient : ((params.tokenList[params.tokenList.length - 1] == WETH) ? address(this) : params.recipient);
        x.path = _encodeExactInputPool(params.tokenList, params.feeList);
        x.payer = x.hasAlreadyPaid ? address(this) : msg.sender;
        while (true) {
            x.hasMultiplePools = x.path.hasMultiplePools();
            params.amountIn = _exactInputInternal(
                params.amountIn,
                x.hasMultiplePools ? address(this) : x.recipient,
                0,
                SwapCallbackData({
            factory : params.factory,
            path : x.path.getFirstPool(),
            payer : x.payer,
            forceUseWETH : params.forceUseWETH
            })
            );

            if (x.hasMultiplePools) {
                x.payer = address(this);
                x.path = x.path.skipToken();
            } else {
                amountOut = params.amountIn;
                break;
            }
        }
        x.error = ["amountIn|", (params.amountIn).toString(), "|amountOut|", (amountOut).toString(), "|amountOutMinimum|", (params.amountOutMinimum).toString()];
        require(amountOut >= params.amountOutMinimum, _concatStringList(x.error));
        _refundETH();
        x.balance1 = IERC20(WETH).balanceOf(address(this));
        if (x.balance1 - x.balance0 > 0) {
            IWETH(WETH).withdraw(x.balance1 - x.balance0);
            TransferHelper.safeTransferETH(params.recipient, x.balance1 - x.balance0);
        }
    }

    function _exactOutputInternal(
        uint256 amountOut,
        address recipient,
        uint160 sqrtPriceLimitX96,
        SwapCallbackData memory data
    ) private returns (uint256 amountIn) {
        (address tokenOut, address tokenIn, uint24 fee) = data.path.decodeFirstPool();
        bool zeroForOne = tokenIn < tokenOut;
        (int256 amount0Delta, int256 amount1Delta) =
        IV3Pool(data.factory.getPool(tokenIn, tokenOut, fee)).swap(
            recipient,
            zeroForOne,
            - amountOut.toInt256(),
            sqrtPriceLimitX96 == 0
            ? (zeroForOne ? MIN_SQRT_RATIO + 1 : MAX_SQRT_RATIO - 1)
            : sqrtPriceLimitX96,
            abi.encode(data)
        );

        uint256 amountOutReceived;
        (amountIn, amountOutReceived) = zeroForOne
        ? (uint256(amount0Delta), uint256(- amount1Delta))
        : (uint256(amount1Delta), uint256(- amount0Delta));
        if (sqrtPriceLimitX96 == 0) require(amountOutReceived == amountOut);
    }

    function exactOutputSingle(
        ExactOutputParams memory params
    ) internal returns (uint256 amountIn) {
        swapItem memory x = new swapItem[](1)[0];
        x.balance0 = IERC20(WETH).balanceOf(address(this));
        require(factoryList[params.factory], "Unsupported factory contract");
        x.recipient = params.forceUseWETH ? params.recipient : ((params.tokenList[0] == WETH) ? address(this) : params.recipient);
        x.path = _encodeExactInputPool(params.tokenList, params.feeList);
        amountIn = _exactOutputInternal(
            params.amountOut,
            x.recipient,
            params.sqrtPriceLimitX96,
            SwapCallbackData({
        factory : params.factory,
        path : x.path,
        payer : msg.sender,
        forceUseWETH : params.forceUseWETH
        })
        );
        x.error = ["amountOut|", (params.amountOut).toString(), "|amountIn|", (amountIn).toString(), "|amountInMaximum|", (params.amountInMaximum).toString()];
        require(amountIn <= params.amountInMaximum, _concatStringList(x.error));
        amountInCached = DEFAULT_AMOUNT_IN_CACHED;
        _refundETH();
        x.balance1 = IERC20(WETH).balanceOf(address(this));
        if (x.balance1 - x.balance0 > 0) {
            IWETH(WETH).withdraw(x.balance1 - x.balance0);
            TransferHelper.safeTransferETH(params.recipient, x.balance1 - x.balance0);
        }
    }

    function exactOutput(
        ExactOutputParams memory params
    ) internal returns (uint256 amountIn) {
        swapItem memory x = new swapItem[](1)[0];
        x.balance0 = IERC20(WETH).balanceOf(address(this));
        require(factoryList[params.factory], "Unsupported factory contract");
        x.recipient = params.forceUseWETH ? params.recipient : ((params.tokenList[0] == WETH) ? address(this) : params.recipient);
        x.path = _encodeExactInputPool(params.tokenList, params.feeList);
        _exactOutputInternal(
            params.amountOut,
            x.recipient,
            0,
            SwapCallbackData({
        factory : params.factory,
        path : x.path,
        payer : msg.sender,
        forceUseWETH : params.forceUseWETH
        })
        );

        amountIn = amountInCached;
        x.error = ["amountOut|", (params.amountOut).toString(), "|amountIn|", (amountIn).toString(), "|amountInMaximum|", (params.amountInMaximum).toString()];
        require(amountIn <= params.amountInMaximum, _concatStringList(x.error));
        amountInCached = DEFAULT_AMOUNT_IN_CACHED;
        _refundETH();
        x.balance1 = IERC20(WETH).balanceOf(address(this));
        if (x.balance1 - x.balance0 > 0) {
            IWETH(WETH).withdraw(x.balance1 - x.balance0);
            TransferHelper.safeTransferETH(params.recipient, x.balance1 - x.balance0);
        }
    }

    function _formatOutParams(ExactOutputParams memory params) internal pure returns (ExactOutputParams memory) {
        uint256 _num = params.tokenList.length;
        address[] memory tokenList = new address[](_num);
        for (uint256 i = 0; i < _num; i++) {
            tokenList[i] = params.tokenList[params.tokenList.length - 1 - i];
        }
        uint24[] memory feeList = new  uint24[](_num - 1);
        for (uint256 i = 0; i < _num - 1; i++) {
            feeList[i] = params.feeList[params.feeList.length - 1 - i];
        }
        params.tokenList = tokenList;
        params.feeList = feeList;
        return params;
    }

    function swapExactTokensForToken(ExactInputParams memory params) external payable nonReentrant isAddressOneOrTwo(params.recipient) returns (uint256 amountOut) {
        if (params.forceUseWETH) {
            require(msg.value == 0, "Does not consume ETH");
        } else {
            //            if (params.tokenList[0] == WETH) {
            //                require(msg.value > 0, "The consumed ETH must be greater than 0");
            //            }
        }
        require(params.tokenList.length == params.feeList.length + 1, "Invalid Params");
        if (params.tokenList.length == 2) {
            return exactInputSingle(params);
        } else {
            return exactInput(params);
        }
    }

    function swapTokensForExactToken(ExactOutputParams memory params) external payable nonReentrant isAddressOneOrTwo(params.recipient) returns (uint256 amountOut) {
        if (params.forceUseWETH) {
            require(msg.value == 0, "Does not consume ETH");
        } else {
            //            if (params.tokenList[0] == WETH) {
            //                require(msg.value > 0, "The consumed ETH must be greater than 0");
            //            }
        }
        params = _formatOutParams(params);
        require(params.tokenList.length == params.feeList.length + 1, "Invalid Params");
        if (params.tokenList.length == 2) {
            return exactOutputSingle(params);
        } else {
            return exactOutput(params);
        }
    }

    receive() external payable {
        require(msg.sender == WETH, "Unsupported ETH acceptance");
    }
}