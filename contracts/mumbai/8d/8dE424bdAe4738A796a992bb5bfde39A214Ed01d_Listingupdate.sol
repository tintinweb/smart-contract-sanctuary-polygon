pragma solidity ^0.8.9;
pragma abicoder v2;


import '@openzeppelin/contracts/utils/structs/EnumerableSet.sol';

interface IBitNFT {

    enum NFTState {nftListed, nftDelisted, amountDeposited, canceledBeforeDelivery, deliveryInitiated, delivered}


    struct Listing {
        uint256 tokenId;
        uint256 value;
        address seller;
        NFTState status;
    }

    struct ERC721Market {
        EnumerableSet.UintSet tokenIdWithListing;
        mapping(uint256 => Listing) listings;
    }

    event TokenListed(
        NFTState indexed nftListed,
        uint256 indexed tokenId,
        address indexed seller,
        string listingId
    );
    event TokenDelisted(
        NFTState indexed nftDelisted,
        uint256 indexed tokenId,
        Listing listing,
        address indexed seller
    );

    event TokenBought(
        uint256 indexed tokenId,
        address indexed buyer,
        Listing listing
    );


     event TokenEscrowed(
        uint256 indexed tokenId,
        address indexed buyer,
        Listing listing
    );

    /**
     * List token for sale
     * @param tokenId erc721 token Id
     * @param value min price to sell the token
     */
    function listToken(
        uint256 tokenId,
        uint256 value,
        string memory listingId
    ) external;

    /**
     *  Delist token for sale
     * @param tokenId erc721 token Id
     */
    function delistToken( uint256 tokenId) external;

    /**
     *  get current listing of a token
     * @param tokenId erc721 token Id
     * @return current valid listing or empty listing struct
     */
    function getTokenListing(uint256 tokenId)
        external
        view
        returns (Listing memory);

    /**
     *  get listings Count total Number of NFTs Listed
     */
    function numTokenListings()
        external
        view
        returns (uint256);

    /**
     *  get current valid listings by size
     * @param from index to start
     * @param size size to query
     * @return current valid listings
     * This to help batch query when list gets big
     */
    function getTokenListings(
        uint256 from,
        uint256 size
    ) external view returns (Listing[] memory);
}

