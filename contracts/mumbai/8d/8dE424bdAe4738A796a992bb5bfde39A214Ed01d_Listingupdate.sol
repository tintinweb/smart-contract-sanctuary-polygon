pragma solidity ^0.8.9;
pragma abicoder v2;

import '@openzeppelin/contracts/utils/Address.sol';
import '@openzeppelin/contracts/token/ERC721/IERC721.sol';
import './BitNFTV2.sol';
import './interface/IBITNFTListing_bkup.sol';

contract Listingupdate is BitNFTV2, IBitNFT {
    using Address for address;
    using EnumerableSet for EnumerableSet.UintSet;
    using EnumerableSet for EnumerableSet.AddressSet;

    address private _owner;
    bool public _istransferAllowed;
    address public _adminWallet;

    mapping(address => ERC721Market) private _erc721Market;
    mapping(uint256 => bool) public _tokenListed;
    mapping(address => uint256) private _balances;
    mapping(uint256 => address) private _owners;

    
    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override(ERC721Upgradeable, IERC721Upgradeable) {
        require(
            _isApprovedOrOwner(_msgSender(), tokenId),
            'ERC721: caller is not token owner or approved'
        );

        require(_istransferAllowed,"Transfer Not Allowed");

        _transfer(from, to, tokenId);
    }

    function _isApprovedOrOwner(address spender, uint256 tokenId)
        internal
        view
        virtual
        override(ERC721Upgradeable)
        returns (bool)
    {
        require(
            _exists(tokenId),
            'ERC721: operator query for nonexistent token'
        );
        address tokenOwner = ownerOf(tokenId);
        return (spender == tokenOwner ||
            getApproved(tokenId) == spender ||
            isApprovedForAll(tokenOwner, spender) ||
            spender == owner());
    }

      function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override(ERC721Upgradeable, IERC721Upgradeable) {
        require(_istransferAllowed,"Transfer Not Allowed");
        safeTransferFrom(from, to, tokenId, "");
    }


    function listToken(
        uint256 tokenId,
        uint256 value,
        string memory listingId
    ) external override {
        address _seller = ownerOf(tokenId);
        Listing memory listing = Listing({
            tokenId: tokenId,
            value: value,
            seller: _seller,
            status: NFTState.nftListed
        });
        require(_isListingValid(listing), 'NOT VALID');
        require(_tokenListed[tokenId] == false, 'ALREADY LISTED');
        _erc721Market[address(this)].listings[tokenId] = listing;
        _erc721Market[address(this)].tokenIdWithListing.add(tokenId);
        _tokenListed[tokenId] = true;
        emit TokenListed(
            NFTState(listing.status),
            tokenId,
            listing.seller,
            listingId
        );
        transferFrom(listing.seller, address(this), tokenId);
    }

    function delistToken(uint256 tokenId) external override {
        require(
            _erc721Market[address(this)].listings[tokenId].seller ==
                msg.sender ||
                msg.sender == owner(),
            'UNAUTHORIZED'
        );
        Listing memory listing = _erc721Market[address(this)].listings[tokenId];
        listing.status = NFTState.nftDelisted;
        _tokenListed[tokenId] = false;
        emit TokenDelisted(
            listing.status,
            tokenId,
            _erc721Market[address(this)].listings[tokenId],
            listing.seller
        );
        safeTransferFrom(address(this), listing.seller, tokenId);
        _delistToken(tokenId);
    }

    function getTokenListing(uint256 tokenId)
        public
        view
        override
        returns (Listing memory validListing)
    {
        Listing memory listing = _erc721Market[address(this)].listings[tokenId];
        if (_isListingValid(listing)) {
            validListing = listing;
        }
    }

    function numTokenListings() public view override returns (uint256) {
        return _erc721Market[address(this)].tokenIdWithListing.length();
    }

    function getTokenListings(uint256 from, uint256 size)
        public
        view
        override
        returns (Listing[] memory listings)
    {
        uint256 listingsCount = numTokenListings();

        if (from < listingsCount && size > 0) {
            uint256 querySize = size;
            if ((from + size) > listingsCount) {
                querySize = listingsCount - from;
            }
            listings = new Listing[](querySize);
            for (uint256 i = 0; i < querySize; i++) {
                uint256 tokenId = _erc721Market[address(this)]
                    .tokenIdWithListing
                    .at(i + from);
                Listing memory listing = _erc721Market[address(this)].listings[
                    tokenId
                ];
                if (_isListingValid(listing)) {
                    listings[i] = listing;
                }
            }
        }
    }

    function _isTokenOwner(uint256 tokenId, address account)
        private
        view
        returns (bool)
    {
        IERC721 _erc721 = IERC721(address(this));
        try _erc721.ownerOf(tokenId) returns (address tokenOwner) {
            return tokenOwner == account;
        } catch {
            return false;
        }
    }

    function _isListingValid(Listing memory listing)
        private
        view
        returns (bool isValid)
    {
        if (
            _isTokenOwner(listing.tokenId, listing.seller) ||
            msg.sender == owner() ||
            (msg.sender == ownerOf(listing.tokenId) && listing.value > 0)
        ) {
            isValid = true;
        }
    }

    function _delistToken(uint256 tokenId) private {
        if (_erc721Market[address(this)].tokenIdWithListing.contains(tokenId)) {
            delete _erc721Market[address(this)].listings[tokenId];
            _erc721Market[address(this)].tokenIdWithListing.remove(tokenId);
        }
    }

    //   function changetransferableStatus(uint256[] memory tokenIds,bool allowed) external onlyOwner {
    //             for (uint256 i = 0; i < tokenIds.length; i++) {
    //             _trasnferAllowed[tokenIds[i]] = allowed;
    //             }
    //     }

    function changetransferableStatus(bool allowed) external onlyOwner {
        _istransferAllowed = allowed;
    }

    function changeAdminWallet(address _newAdminWallet) external onlyOwner {
        _adminWallet = _newAdminWallet;
    }

    function confirmDelivery(address _buyer, uint256 _tokenId)
        public
        payable
        onlyOwner
    {
        Listing memory listing = _erc721Market[address(this)].listings[
            _tokenId
        ];
        require(msg.value >= listing.value, 'INSUFFICIENT BALANCE');
        Address.sendValue(payable(listing.seller), msg.value);
        listing.status = NFTState.delivered;
        emit TokenBought({tokenId: _tokenId, buyer: _buyer, listing: listing});
        safeTransferFrom(address(this), _buyer, _tokenId);
        _delistToken(_tokenId);
    }


}

