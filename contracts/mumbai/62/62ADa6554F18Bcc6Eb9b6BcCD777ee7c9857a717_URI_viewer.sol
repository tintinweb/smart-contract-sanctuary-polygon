/**
  @title LokhatBoxes

  @notice 

  @author 
 */
contract LokhatBoxes is
    ERC1155,
    // AccessControl,
    ERC1155Burnable,
    ERC1155Supply,
    // ERC1155URIStorage,
    // ContextMixin,
    // NativeMetaTransaction,
    Cards
{
    using SafeMath for uint256;
    uint256 public MintedBoxes = 0;
    string public constant name = "LokhateBoxes";
    URI_viewer private uri_delegator;

    uint256 box_number=0;
    mapping(uint256 => Box) public boxes;
    // Box[] public boxes;
    string public __contractURI;
    // ========== Errors ==========
    error Not_enough_boxes();
    error Max_boxes_reached();
    error Select_one();
    error Invalid_Id();
    error Sold_out();
    error Different_length();
    error Select_less();

    function contractURI() public view returns (string memory) {
        return __contractURI;
    }

    constructor(
        address vrfCoordinatorV2,
        uint64 subscriptionId,
        bytes32 gasLane, // keyHash
        uint32 callbackGasLimit,
        string memory _contractURI
    ) Cards(vrfCoordinatorV2, subscriptionId, gasLane, callbackGasLimit) ERC1155(name) {
        // _initializeEIP712(name);
        __contractURI=_contractURI;
    }

    function getBoxes() external view returns (Box[] memory) {
        Box[] memory _boxes = new Box[](box_number);
        for (uint256 i = 0; i < box_number; i++) {
            _boxes[i] = boxes[i];
        }
        return _boxes;
    }

    function getBox(uint256 id) external view returns (Box memory) {
        if(id >= box_number) revert Invalid_Id();
        return boxes[id];
    }

    function addBoxes(
        bytes32[] memory _name,
        uint256[] memory _supply,
        uint256[] memory _price,
        string[] memory _uri,
        uint16[] memory probability_rare,
        uint16[] memory probability_epic,
        uint16[] memory probability_legendary
    ) public OnlyOwner {
        for (uint256 i = 0; i < _name.length; i++) {
            if(box_number >= MAXIMUM_CASE) revert Max_boxes_reached();
            boxes[box_number]=Box( _name[i], 0, _supply[i], _price[i], _uri[i],probability_rare[i], probability_epic[i], probability_legendary[i]);
            box_number++;
        }
    }

    function updateBox(
        uint256 _id,
        bytes32 _name,
        uint256 _supply,
        uint256 _price,
        string memory _uri,
        uint16  _probability_rare,
        uint16  _probability_epic,
        uint16  _probability_legendary
    ) external OnlyOwner {
        if(_id > box_number) revert Invalid_Id();
        // Box memory box = boxes[_id];
        // box.name = _name;
        // box.supply = _supply;
        // box.price = _price;
        // box.probability_rare = _probability_rare;
        // box.probability_epic = _probability_epic;
        // box.probability_legendary = _probability_legendary;
        boxes[_id] = Box(_name, boxes[_id].mintedSupply, _supply, _price, _uri, _probability_rare, _probability_epic, _probability_legendary);
    }

    function buyBoxes(uint256[] memory _ids,uint256[] memory _amounts) external payable { //whenNotPaused
        if(_ids.length == 0||_amounts.length == 0) revert Select_one();
        if(msg.value == 0) revert Not_enough_money();
        if(_ids.length != _amounts.length) revert Different_length();
        // Calculate the total cost
        uint256 mintCostTotal = 0;

        for (uint256 i = 0; i < _ids.length; i++) {
            uint256 boxId = _ids[i];
            if(boxId >= box_number) revert Invalid_Id();
            if(boxes[boxId].mintedSupply + _amounts[i] > boxes[boxId].supply) revert Sold_out();
            mintCostTotal = mintCostTotal.add(boxes[boxId].price.mul(_amounts[i]));
        }

        if(msg.value < mintCostTotal) revert Not_enough_money();
        // if(mintCostTotal==0) revert Select_one();// give free box

        _mintBatch(msg.sender, _ids, _amounts, "");
        Redistribute(mintCostTotal);
        for (uint256 i = 0; i < _ids.length; i++) {
            boxes[_ids[i]].mintedSupply = boxes[_ids[i]].mintedSupply.add(_amounts[i]);
            MintedBoxes = MintedBoxes.add(_amounts[i]);
        }
        // Refund any excess payment
        if (msg.value > mintCostTotal) {
            payable(msg.sender).transfer(msg.value.sub(mintCostTotal));
        }
        deleteExpiredGenesis();
    }

    function openBox(uint256 _boxid) external { //whenNotPaused payable
        if(balanceOf(msg.sender, _boxid) == 0) revert Not_enough_boxes();
        _burn(msg.sender, _boxid, 1);
        generateRandomCard(_boxid,boxes[_boxid].probability_rare,boxes[_boxid].probability_epic,boxes[_boxid].probability_legendary);
    }
    function openBoxes(uint256 _boxid,uint256 number) external { //whenNotPaused payable
        // if(number > 20) revert Select_less();
        if(balanceOf(msg.sender, _boxid) < number) revert Not_enough_boxes();
        _burn(msg.sender, _boxid, number);
        for(uint256 i = 0; i < number; i++) {
            generateRandomCard(_boxid,boxes[_boxid].probability_rare,boxes[_boxid].probability_epic,boxes[_boxid].probability_legendary);
        }
    }

    function mint(address wallet, Card memory cardToMint) internal override(Cards) {
        _mint(wallet, cardToMint.id, 1, "");
        // _setURI(cardToMint.id, cardToMint.uri);
        _setURI(cardToMint);
        emit CardGenerated(cardToMint.name, cardToMint.uri, cardToMint.cardType);
    }

    function getBoxesByAddress(address wallet) internal view returns (uint256[] memory) {
        uint256[] memory amounts = new uint256[](box_number);

        for (uint256 i = 0; i < box_number; i++) {
            amounts[i]=balanceOf(wallet, i);
        }
        return amounts;
    }

    function getBoxesBought(address wallet) external view returns (uint256[] memory) {
        return getBoxesByAddress(wallet);
    }

    function getBoxesBought() external view returns (uint256[] memory) {
        return getBoxesByAddress(msg.sender);
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155, ERC1155Supply)  { //whenNotPaused
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
        if(from==address(0)) return;
        for(uint256 i=0;i<ids.length;i++){
            if(ids[i] <= MAXIMUM_CASE) continue;
            transferGenesis(from,to,Tools.pow(uint256(_tokenCard[ids[i]].cardType)+1,2));
        }
    }
    // function supportsInterface(
    //     bytes4 interfaceId
    // ) public view override(ERC1155, AccessControl) returns (bool) {
    //     return super.supportsInterface(interfaceId);
    // }

    // function pause() public OnlyOwner {
    //     _pause();
    // }

    // function unpause() public OnlyOwner {
    //     _unpause();
    // }

    //==================================================================================================================================================================

    function totalSupply(uint256 id) public view override returns (uint256) {
        if(id >= box_number) revert Invalid_Id();
        return boxes[id].supply;
    }
    // useless
    function totalSupply() external view returns (uint256) {//0.112kib
        uint256 total = 0;
        for (uint i = 0; i < box_number; i++) {
            total += boxes[i].supply;
        }
        return total;
    }

    
    function _setURI(Card memory _card) internal {
        _tokenCard[_card.id] = _card;
        emit URI(_card.uri,_card.id);
    }
    function upgradeDelegate(address new_uri_address) public OnlyOwner {
        uri_delegator = URI_viewer(new_uri_address);
    }
    function getCard(uint256 tokenId) external view returns (Card memory) {
        return _tokenCard[tokenId];
    }
    function uri(uint256 tokenId) public view override returns (string memory) {
        return uri_delegator.generate_uri(tokenId);
    }
}

