library GenesisRemover {
    struct Deletion {
        address addr;
        uint256 time;
    }

    struct FIFO {
        mapping(uint256 => Deletion) queue;
        uint256 first;
        uint256 last;
    }

    function addDeletion(FIFO storage self, address _addr) internal {
        self.last++;
        self.queue[self.last] = Deletion(_addr, block.timestamp + 1 weeks); // solhint-disable-line not-rely-on-time
    }

    function removeExpired(FIFO storage self) internal returns (address _addr) {
        if (self.last >= self.first && block.timestamp >= self.queue[self.first].time) { // solhint-disable-line not-rely-on-time
            _addr = self.queue[self.first].addr;
            delete self.queue[self.first];
            self.first++;
        }
    }

    function getQueueLength(FIFO storage self) internal view returns (uint256) {
        return self.last - self.first + 1;
    }
}

