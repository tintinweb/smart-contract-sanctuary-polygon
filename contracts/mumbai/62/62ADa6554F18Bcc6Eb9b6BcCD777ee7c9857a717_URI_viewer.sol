        for(uint256 i=0;i<(uint256(Tools.CardType.LEGENDARY)+1);i++){
            if(MintableCards[Tools.CardType(i)].length ==0) revert Add_card_first();
        }
        uint256 requestId = i_vrfCoordinator.requestRandomWords(
            i_gasLane,
            i_subscriptionId,
            REQUEST_CONFIRMATIONS,
            i_callbackGasLimit,
            NUM_WORDS
        );
        // uint256 requestId = VRF_COORDINATOR.requestRandomWords(keyHash,VRF_subscriptionId,requestConfirmations,callbackGasLimit,numWords);
        requestToSender[requestId] = msg.sender;
        requestToRands[requestId] = Tools.concatenateUint48(probability_rare,probability_epic,probability_legendary);
        emit BoxOpeningStarted(requestId, boxid, msg.sender);
    }

    function fulfillRandomWords(uint256 requestId,uint256[] memory randomWords) internal override{

        (uint256 cardTypeIndex,uint256 cardIndex) = Tools.extractBits(randomWords[0]);//TODO: inline the 2 functions
        Tools.CardType cardType = Tools.getRarity(cardTypeIndex,requestToRands[requestId]);

        uint256 cards_of_type_length=MintableCards[cardType].length;
        if(cards_of_type_length==0) revert Card_not_found();
        cardIndex %= cards_of_type_length;

        MintableCard memory selectedCard = getMintableCard(cardType,cardIndex);
        bool is_genesis = block.timestamp < TIME_END_GENESIS;
        Card memory card_to_send = Card(
            selectedCard.name,
            selectedCard.uri,
            cardType,
            is_genesis,
            block.timestamp,
            MintCardId++
        );

        if(is_genesis){
            addGenesis(requestToSender[requestId],Tools.pow(uint256(cardType)+1,2));
        }
        mint(requestToSender[requestId],card_to_send);
    }
