pragma solidity ^0.8.0;
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./utils/GenesisRemover.sol";
contract Earnings{
    using GenesisRemover for GenesisRemover.FIFO;
    GenesisRemover.FIFO private fifo;
    using SafeMath for uint256;
    address private immutable owner=msg.sender;
    uint256 public totalGenesisValue = 0;
    struct earning {
        uint256 value;
        uint256 earned;
    }
    mapping(address => earning) internal EarnedTokens;
    function getearning(address _address) public view returns (uint256,uint256) {
        return (EarnedTokens[_address].value,EarnedTokens[_address].earned);
    }
    address[] private holders;
    uint256 constant public owner_percent = 80;
    error Unauthorized();
    // ========== Errors ==========
    error No_earnings();
    function _totalHolders() external view returns (uint256) {
        return holders.length;
    }
    function deleteExpiredGenesis() public {//TODO: optimiser
        address _address = fifo.removeExpired();
        if(_address == address(0)) return;
        for (uint256 i = 0; i < holders.length; i++) { 
            if (holders[i] == _address) {
                holders[i] = holders[holders.length - 1];
                delete holders[holders.length - 1];
                EarnedTokens[owner].earned=EarnedTokens[owner].earned.add(EarnedTokens[_address].earned);
                EarnedTokens[_address].earned=0;
                return;
            }
        }
    }
    function addGenesis(address _address,uint256 value) internal {
        if(value == 0 || _address==address(0) || _address==address(this)) return;
        if(EarnedTokens[_address].value == 0){
            holders.push(_address);
        }
        EarnedTokens[_address].value = EarnedTokens[_address].value.add(value);
        totalGenesisValue = totalGenesisValue.add(value);
    }
    function removeGenesis(address _address,uint256 value) internal {
        if(value == 0 || _address==address(0) || _address==address(this)) return;
        EarnedTokens[_address].value = EarnedTokens[_address].value.sub(value,"You have not enought genesis");
        totalGenesisValue = totalGenesisValue.sub(value);
        if(EarnedTokens[_address].value == 0){
            // delete this genesis owner and let him 1week to get is money
            fifo.addDeletion(_address);
        }
    }
    function transferGenesis(address _from,address _to,uint256 value) internal {
        removeGenesis(_from,value);
        if(_to != address(0)){
            addGenesis(_to,value);
        }
    }
    function Redistribute(uint256 value) internal {
        uint256 owner_value = value.mul(owner_percent).div(100);
        EarnedTokens[owner].earned = EarnedTokens[owner].earned.add(owner_value);
        uint256 user_staying_value = value - owner_value;
        for (uint256 i = 0; i < holders.length; i++) {
            uint256 user_current_value = user_staying_value.mul(EarnedTokens[holders[i]].value).div(totalGenesisValue);
            EarnedTokens[holders[i]].earned = EarnedTokens[holders[i]].earned.add(user_current_value);
            user_staying_value = user_staying_value.sub(user_current_value);
        }
        EarnedTokens[owner].earned = EarnedTokens[owner].earned.add(user_staying_value);
    }

    function getEarned() external view returns (uint256) {
        return EarnedTokens[msg.sender].earned;
    }
    function getGenesis() external view returns (uint256) {
        return EarnedTokens[msg.sender].value;
    }
    function withdraw() external {
        if(EarnedTokens[msg.sender].earned == 0) revert No_earnings();
        uint256 value = EarnedTokens[msg.sender].earned;
        EarnedTokens[msg.sender].earned = 0;
        payable(msg.sender).transfer(value);
    }
    
    modifier OnlyOwner() {
		_checkOwner();
		_;
    }

    function _checkOwner() internal view virtual {
        if(msg.sender != owner) revert Unauthorized();
    }

}

