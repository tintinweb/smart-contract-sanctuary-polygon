import "@chainlink/contracts/src/v0.8/interfaces/AutomationCompatibleInterface.sol";

import "@uniswap/lib/contracts/libraries/TransferHelper.sol";

import "./../interfaces/IFactory.sol";
import "./../interfaces/IInsuranceFund.sol";
import "./../interfaces/exchange/IExchange.sol";
import "./../interfaces/ICreditSystem.sol";

import "./../lib/Utils.sol";
import "./../MixinResolver.sol";

import "hardhat/console.sol";

/**
 * @title Credit Keeper
 *
 * This contract gets all coins to be covered and then
 * cover them automatically using Chainlink Upkeep (see https://docs.chain.link/
 * chainlink-automation/introduction/)
 * This contract contains logic allow Chainlink Automation to
 * determine when to execute smart contract functions.
 * This contract is compatible with the
 * AutomationCompatibleInterface contract and its functions.
 *
 **/
contract CreditKeeper is AutomationCompatibleInterface, MixinResolver, Ownable {
    using Utils for uint256[];

    bytes32 private constant FACTORY_CONTRACT = "Factory";
    bytes32 private constant INSURANCE_FUND_CONTRACT = "InsuranceFund";
    bytes32 private constant LINK_TOKEN = "LinkToken";

    IFactory _factory;
    ICreditSystem _creditSystem;
    IExchange _exchange;

    address LINK;

    /// @dev Holds a mapping of coins to min swap amountsIn. To all plan amountsIn
    mapping(address => uint) _coinsMinAmountsIn;

    /// @dev Holds a mapping of coins to min swap amountsOut. By each fact swap amountOut
    mapping(address => uint) _coinsMinAmountsOut;

    /// @dev Holds a mapping of coins to `_coins` array indexes (+1).+
    mapping(address => uint) _coinsIndexes;

    /// @dev Array of all coins allowded to be covered by other assets
    address[] _coins;

    constructor(address resolver) MixinResolver(resolver) {}

    /**
     * @notice Set new `coin` address with `minAmount` and
     * `minSwapAmount` which allowded to be covered by other assets
     *
     * @param coin Token address
     * @param minAmountIn Minimum amount of used amount of `coin`. This param
     * is needed for checkUpkeep function. To cover the coins, it is necessary
     * that the used `coin` amount be greater then `minAmount`
     * @param minAmountOut Minimum amount of each swap amount of `coin`. This param
     * is needed for performUpkeep function. To cover the coins, it is necessary
     * that the swap `coin` amount be greater then `minSwapAmount`
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setCoin(
        address coin,
        uint minAmountIn,
        uint minAmountOut
    ) external onlyOwner {
        _coinsMinAmountsIn[coin] = minAmountIn;
        _coinsMinAmountsOut[coin] = minAmountOut;

        if (_coinsIndexes[coin] == 0) {
            _coins.push(coin);
            _coinsIndexes[coin] = _coins.length;
        }
    }

    /**
     * @notice Delete `coin` from all maps and arrays.
     * After deleting coin will not be allowded to be covered by other assets
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function deleteCoin(address coin) external onlyOwner {
        _coinsMinAmountsIn[coin] = 0;
        _coinsMinAmountsOut[coin] = 0;

        uint256 coinIndex = _coinsIndexes[coin] - 1;
        address lastCoin = _coins[_coins.length - 1];

        _coinsIndexes[lastCoin] = coinIndex + 1;
        _coinsIndexes[coin] = 0;

        _coins[coinIndex] = lastCoin;

        _coins.pop();
    }

    /**
     * @notice to set the addresses required for the contract
     *
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function resolverAddressesRequired()
        public
        pure
        override
        returns (bytes32[] memory addresses)
    {
        addresses = new bytes32[](3);
        addresses[0] = FACTORY_CONTRACT;
        addresses[1] = INSURANCE_FUND_CONTRACT;
        addresses[2] = LINK_TOKEN;
    }

    /**
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function _resolveAddressesFinish() internal override {
        _factory = IFactory(requireAndGetAddress(FACTORY_CONTRACT));
        _creditSystem = ICreditSystem(
            requireAndGetAddress(INSURANCE_FUND_CONTRACT)
        );

        _exchange = _factory.getExchange(IExchange.DEX.UNISWAP);

        LINK = requireAndGetAddress(LINK_TOKEN);
    }

    /**
     * @notice This function retrieves all coins that need to be
     * covered, executed by the keeper
     *
     * @return upkeepNeeded boolean to indicate whether the keeper should call
     * performUpkeep or not.
     * @return performData Array of coins that need to be refunded and array of amounts
     * in bytes that the keeper should call performUpkeep with
     *
     * @dev This function checks all `_coins' for a condition: if used amount of coin
     * is greater then _coinsMinAmountsIn[coin] this coin must be refunded.
     * This function contains the logic that will be executed
     * off-chain by Chainlink Upkeep to see if performUpkeep should be executed.
     */
    function checkUpkeep(
        bytes calldata checkData
    )
        external
        view
        override
        returns (bool upkeepNeeded, bytes memory performData)
    {
        uint size = 0;

        // If used amount of coin is greater then _coinsMinAmountsIn[coin]
        // this coin must be refunded.
        for (uint256 i = 0; i < _coins.length; i++) {
            uint256 usedAmount = _creditSystem.getUsedCreditBalance(_coins[i]);

            if (usedAmount > _coinsMinAmountsIn[_coins[i]]) size++;
        }

        address[] memory coins = new address[](size);
        uint[] memory amounts = new uint[](size);
        uint j = 0;

        // Filling coins array and amounts array with values
        for (uint256 i = 0; i < _coins.length; i++) {
            uint256 usedAmount = _creditSystem.getUsedCreditBalance(_coins[i]);

            if (usedAmount > _coinsMinAmountsIn[_coins[i]]) {
                coins[j] = _coins[i];
                amounts[j] = usedAmount;
                j++;
            }
        }

        return (coins.length > 0, abi.encode(coins, amounts));
    }

    /**
     * @notice Execution covering
     *
     * @param performData is the data which was passed back from the checkUpkeep
     *
     * @dev This function calculates balances for each coin and then tries to cover.
     * This function will be executed by Chainlink Upkeep
     * when checkUpkeep function retunrs true.
     */
    function performUpkeep(bytes calldata performData) external override {
        (address[] memory coins, uint256[] memory amounts) = abi.decode(
            performData,
            (address[], uint256[])
        );

        for (uint256 k = 0; k < coins.length; k++) {
            // Get all available tokens by which the coin[k] could be covered
            address[] memory tokens = _factory.getTokens();

            // Calculate balances for all tokens
            uint256[] memory balances = _calculateBalances(
                coins[k],
                tokens,
                amounts[k]
            );

            uint256 sum = 0;
            uint i = 0;

            // Covering coin[k] using tokens
            while (sum < amounts[k] && i < balances.length && balances[i] > 0) {
                _creditSystem.transferFreeCoins(tokens[i], balances[i]);

                uint256 balance = IERC20(tokens[i]).balanceOf(address(this));

                if (tokens[i] == coins[k]) {
                    sum += balances[i];
                } else {
                    TransferHelper.safeApprove(
                        tokens[i],
                        address(_exchange),
                        balances[i]
                    );

                    (, uint256 amountOut) = _exchange.swap(
                        IExchange.SwapParams({
                            amountIn: balances[i],
                            amountOut: 0,
                            tokenIn: tokens[i],
                            tokenOut: coins[k],
                            timestamp: block.timestamp,
                            path: "0x",
                            dexType: IExchange.DEX.UNISWAP,
                            recipient: address(_creditSystem)
                        })
                    );

                    sum += amountOut;
                }

                i++;
            }
        }
    }

    /**
     * @notice Calculating `tokens` balances. Receives a structured sorted
     * list of coins and balances which could be used for covering
     *
     * @param coin Token address
     * @param tokens Tokens with which the `coin` can be refunded
     * @param amount Amount of `coin` to refund
     * @return array `tokens` balances
     *
     * @dev This function calculates balances `token`.
     */
    function _calculateBalances(
        address coin,
        address[] memory tokens,
        uint256 amount
    ) internal view returns (uint256[] memory) {
        uint256[] memory rates = new uint256[](tokens.length);
        uint256[] memory balances = new uint256[](tokens.length);

        uint256 sum = 0;

        for (uint256 i = 0; i < tokens.length; i++) {
            // Get credit balance of token
            balances[i] = _creditSystem.getFreeBalance(tokens[i]);

            // Get amountIn of token in coin equivalent with amountOut = amount
