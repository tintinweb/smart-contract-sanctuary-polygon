pragma solidity 0.8.15;


import {INFT} from "../interfaces/INFT.sol";
import {AccessControl} from "@openzeppelin/contracts/access/AccessControl.sol";
import {NFTMetadataViews} from "./NFTMetadataViews.sol";
import {
    IERC721Upgradeable
} from "@openzeppelin-upgradeable/contracts/token/ERC721/IERC721Upgradeable.sol";
import {IMutagenNFT} from "./interfaces/IMutagenNFT.sol";
import {IResolver} from "./interfaces/IResolver.sol";

contract Mutagenise is AccessControl {

    //----------- Errors ------------//
    error MutationNotAllowed();
    error ResolverDoesNotExists();
    error ZeroMutagenAddress();
    error ZeroAddressNotAllowed();
    error NftLengthMismatch();

    // Mapping to keep the resolvers for mutagens
    mapping(address => IResolver) resolvers;

    // Emitted when the mutation has happened on an NFT
    event Mutagenise(address projectNFT, address mutagenNFT, uint256 projectNftId, uint256 mutagenNftId);

    // Emitted when the resolver gets added/replaced for a mutagen
    event ResolverAdded(address mutagen, IResolver resolver);

    /// @notice Initializer of the contract.
    /// @param _admin Address of the admin.
    constructor(address _admin) {
        if (_admin == address(0)) {
            revert ZeroAddressNotAllowed();
        }
        _setupRole(DEFAULT_ADMIN_ROLE, _admin);
    }

    /// @notice Function to Add or replace the resolver for the given mutagen NFT.
    /// @param mutagen Address of the mutagen contract.
    /// @param resolver Address of the resolver contract.
    function addResolver(address mutagen, IResolver resolver) external onlyRole(DEFAULT_ADMIN_ROLE) {
        if (address(mutagen) == address(0)) {
            revert ZeroMutagenAddress();
        }
        resolvers[mutagen] = resolver;
        emit ResolverAdded(mutagen, resolver);
    }

    /// @notice Function would perform the mutation on the given NFT and burn the mutagenNFT.
    /// @param projectNFT Address of the NFT contract.
    /// @param projectNftId Id of the NFT.
    /// @param mutagenNFT Address of the mutagen contract.
    /// @param mutagenNFTId Id of the mutagen NFT.
    /// @param expiry Expiry of the signature.
    /// @param signatureNonce Nonce that is used to create the signature.
    /// @param signature Offchain signature of the authorised address.
    function mutagenise(
        address projectNFT,
        uint256 projectNftId,
        address mutagenNFT,
        uint256 mutagenNFTId,
        uint256 expiry,
        uint256 signatureNonce,
        bytes memory signature,
        NFTMetadataViews.NFTView memory metadata, 
        string memory salt
    ) 
    external 
    {   
        if(!isMutationAllowed(mutagenNFT, projectNFT, projectNftId)) {
            revert MutationNotAllowed();
        }
        INFT(projectNFT).revealMetadata(projectNftId, metadata, salt);
        IERC721Upgradeable(mutagenNFT).transferFrom(msg.sender, address(this), mutagenNFTId);
        INFT(projectNFT).mutate(projectNFT, projectNftId, mutagenNFT, mutagenNFTId, expiry, signatureNonce, signature);
        IMutagenNFT(mutagenNFT).burn(mutagenNFTId);
        emit Mutagenise(projectNFT, mutagenNFT, projectNftId, mutagenNFTId);
    }

    /// @notice Function to check that mutation has been allowed for project NFT with mutagen NFT.
    /// @param mutagenNFT Address of the mutagen contract.
    /// @param projectNFT Address of the NFT contract.
    /// @param projectNftId Id of the NFT.
    function isMutationAllowed(
        address mutagenNFT, 
        address projectNFT, 
        uint256 projectNftId
    ) 
        public
        returns(bool) 
    {
        return resolvers[mutagenNFT].isMutationAllowed(projectNFT, projectNftId);
    }

    /// @notice Function to get the multiplier for mutagen that would get mutiplied into the basePrice of mutagen.
    /// @param mutagenNFT Address of the mutagen contract.
    /// @param metadata Metadata of the mutagen NFT.
    function getBaseMultiplier(address mutagenNFT, NFTMetadataViews.NFTView memory metadata) external view returns(uint256) {
        IResolver resolver = resolvers[mutagenNFT];
        if (address(resolver) == address(0)) {
            revert ResolverDoesNotExists();
        }
        return resolver.getBaseMultiplier(metadata);
    }

    // /// @notice Function to get the preview of the mutation that gonna happen on project NFT.
    // /// @param projectNFT Address of the NFT contract.
    // /// @param projectNftId Id of the NFT.
    // /// @param mutagenNFT Address of the mutagen contract.
    // /// @param metadata Metadata of the mutagen NFT.
    // function previewMutagensis(
    //     address projectNFT,
    //     uint256 projectNftId,
    //     address mutagenNFT, 
    //     NFTMetadataViews.NFTView calldata metadata
    // ) 
    //     external 
    //     view
    //     returns (NFTMetadataViews.NFTView memory newMetadata) 
    // {   
    //     IResolver resolver = resolvers[mutagenNFT];
    //     if (address(resolver) == address(0)) {
    //         revert ResolverDoesNotExists();
    //     }
    //     // return resolver.Mutagensis(projectNFT, projectNftId, metadata);
    // }

    // /// @notice Function to get the preview of the mutation that gonna happen on batch project NFT.
    // /// @param projectNFTs Addresses of the NFT contract.
    // /// @param projectNftIds Id's of the NFT.
    // /// @param mutagenNFT Address of the mutagen contract.
    // /// @param metadata Metadata of the mutagen NFT.
    // function batchPreviewMutagensis(
    //     address[] calldata projectNFTs,
    //     uint256[] calldata projectNftIds,
    //     address mutagenNFT, 
    //     NFTMetadataViews.NFTView calldata metadata
    // ) 
    //     external 
    //     view
    //     returns (NFTMetadataViews.NFTView[] memory newMetadata) 
    // {
    //     if (projectNFTs.length != projectNftIds.length) {
    //         revert NftLengthMismatch();
    //     }
    //     IResolver resolver = resolvers[mutagenNFT];
    //     if (address(resolver) == address(0)) {
    //         revert ResolverDoesNotExists();
    //     }
    //     // return resolver.BatchMutagensis(projectNFTs, projectNftIds, metadata);
    // }

}

// SPDX-License-Identifier: MIT
pragma solidity 0.8.15;

import {IERC721} from "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "../mutagens/NFTMetadataViews.sol";

interface INFT is IERC721 {
    /// @notice Initialize the NFT collection.
    /// @param _maxSupply maximum supply of a collection.
    /// @param baseUri base Url of the nft's metadata.
    /// @param _name name of the collection.
    /// @param _symbol symbol of the collection.
    /// @param _owner owner of the collection.
    /// @param _minter Address of the minter allowed to mint tokenIds.
    /// @param _royaltyReceiver Beneficary of the royalty.
    /// @param _feeNumerator Percentage of fee charged as royalty.
    /// @param _maxRoyaltyPercentage Percentage of maximum fee charged as royalty.
    function initialize(
        uint256 _maxSupply,
        string calldata baseUri,
        string calldata _name,
        string calldata _symbol,
        address _owner,
        address _minter,
        address _royaltyReceiver,
        uint96 _feeNumerator,
        uint96 _maxRoyaltyPercentage
    )
        external;

    /// @notice Mint a token and assign it to an address.
    /// @param _to NFT transferred to the given address.
    /// @param metadata of the NFT.
    function mint(address _to, NFTMetadataViews.NFTView memory metadata) external;

    /// @notice Mint a token and assign it to an address.
    /// @param _to NFT transferred to the given address.
    /// @param metadataHash Hash of the metadata of the NFT.
    function commitMint(address _to, bytes32 metadataHash, string calldata _tokenUri) external;

    /// @notice Sets the royalty information that all ids in this contract will default to.
    /// Requirements:
    /// `receiver` cannot be the zero address.
    /// `feeNumerator` cannot be greater than the fee denominator.
    function setDefaultRoyalty(address receiver, uint96 feeNumerator) external;

    /// @notice Sets the royalty information for a specific token id, overriding the global default.
    /// Requirements:
    /// `receiver` cannot be the zero address.
    /// `feeNumerator` cannot be greater than the fee denominator.
    /// @param tokenId Token identitifer whom royalty information gonna set.
    /// @param receiver Beneficiary of the royalty.
    /// @param feeNumerator Percentage of fee gonna charge as royalty.
    function setTokenRoyalty(
        uint256 tokenId,
        address receiver,
        uint96 feeNumerator
    )
        external;

    /// @notice Deletes the default royalty information.
    function deleteDefaultRoyalty() external;

    /// @notice Global royalty would not be in use after this.
    function closeGlobalRoyalty() external;

    /// @notice Global royalty would be in use after this.
    function openGlobalRoyalty() external;

    /// @notice Resets royalty information for the token id back to the global default.
    function resetTokenRoyalty(uint256 tokenId) external;

    /// @notice Returns the URI that provides the details of royalty for OpenSea support.
    /// Ref - https://docs.opensea.io/v2.0/docs/contract-level-metadata
    function contractURI() external view returns (string memory);

    /// @notice Returns the Uniform Resource Identifier (URI) for `tokenId` token.
    /// @param tokenId Identifier for the token
    function tokenURI(uint256 tokenId)
        external
        view
        returns (string memory);
    
    /// @notice Returns the base URI for the contract.
    function baseURI() external view returns (string memory);

    /// @notice Set the base URI, Only ADMIN can call it.
    /// @param newBaseUri New base uri for the metadata.
    function setBaseUri(string memory newBaseUri) external;

    /// @notice Set the token URI for the given tokenId.
    /// @param tokenId Identifier for the token
    /// @param tokenUri URI for the given tokenId.
    function setTokenUri(uint256 tokenId, string memory tokenUri) external;

    /// @notice Perform the mutation on a tokenID. Only tokenId owner allowed to perform mutation.
    /// @param tokenId Identifier for the token.
    /// @param mutagen Address of the mutagen.
    /// @param expiry Expiry of the signature.
    /// @param signatureNonce Nonce that is used to create the signature.
    /// @param signature Offchain signature of the authorised address.
    function mutate(
        address projectNFT, 
        uint256 tokenId, 
        address mutagen, 
        uint256 mutagenNFTId,
        uint256 expiry, 
        uint256 signatureNonce,
        bytes memory signature
    ) external;

    /// @notice Reveal the metadata of the already minted tokenId.
    /// @param tokenId Identifier of the NFT whose metadata is going to set.
    /// @param metadata Metadata of the given NFT.
    /// @param salt Unique identifier use to reveal the metadata.
    function revealMetadata(uint256 tokenId, NFTMetadataViews.NFTView memory metadata, string memory salt) external;

    function nextTokenId() external view returns (uint256);

    function maximumSupply() external view returns (uint256);

    function globalRoyaltyInEffect() external view returns (bool);

    function owner() external view returns (address);
}

