    }

    function formatTokenURI(uint256 tokenId, string memory imageURI) public view returns (string memory) {
        string memory description = "The P proves that you are a member of thePIXEL.The P is proof of thePIXEL and a symbol of PIXEL ART community where artists, critics, collectors, and others are gently connected and resonate with each other to spread the artistic and cultural value of 'pixel art' and pass its value and context on to future generations.";
        string memory baseURL = "data:application/json;base64,";
        string memory json = string(
        abi.encodePacked(
            '{"name": "',
            string(abi.encodePacked(name(), '_#', formatTokenId(tokenId))),
            '", "description":"',
            description,
            '", "image_data":"',
            imageURI,
            '"}'
        )
        );
        string memory jsonBase64Encoded = Base64.encode(bytes(json));
        return string(abi.encodePacked(baseURL, jsonBase64Encoded));
    }

    function formatTokenId(uint256 tokenId) public view returns (string memory) {
        string memory result = Base64.toString(tokenId);
        if (tokenId < 10) {
        result = string(abi.encodePacked('000', result));
        } else if (tokenId < 100) {
        result = string(abi.encodePacked('00', result));
        } else if (tokenId < 1000) {
        result = string(abi.encodePacked('0', result));
        }
        return result;
    }
}