// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "./IPirateItemMerchant.sol";
import "./IPirateItemContract.sol";

import "hardhat/console.sol";

contract PirateItemMerchant is 
    Ownable,
    Pausable,
    ReentrancyGuard,
    IPirateItemMerchant, 
    ERC1155Holder
{
    using SafeMath for uint256;

    IERC20 internal doubloons;

    address internal pirateItemContractAddress;
    IPirateItemContract internal pirateItemContract;

    uint256 private profitPercentage = 5;

    event Bought(uint256 tokenId, uint256 amount);
    event Sold(uint256 tokenId, uint256 amount);
    event Received(address, uint256);

    fallback() external payable { }  

    constructor(
        address _pirateItemContractAddress,
        address _doubloonsContractAddress
    )
        payable 
    {
        doubloons = IERC20(_doubloonsContractAddress);

        setPirateItemContract(_pirateItemContractAddress);
    }

    receive() external payable {
        emit Received(msg.sender, msg.value);
    }

    function setPirateItemContract(address _pirateItemContractAddress) public onlyOwner {
        pirateItemContractAddress = _pirateItemContractAddress;
        pirateItemContract = IPirateItemContract(pirateItemContractAddress);
    }

    function getProfitPercentage() public view returns (uint256) {
        return profitPercentage;
    }

    function setProfitPercentage(uint256 _profitPercentage) public onlyOwner {
        profitPercentage = _profitPercentage;
    }

    function withdrawDoubloons(uint256 amount) public onlyOwner {
        require(doubloons.transfer(_msgSender(), amount), "Withdraw error");
    }

    function buyPirateItemFromMerchant(uint256 tokenId, uint256 amount) 
        public
        payable 
        nonReentrant  
        whenNotPaused 
    {
        require(pirateItemContract.balanceOf(address(this), tokenId) >= amount, "Insufficient stock");

        bytes32 symbol = pirateItemContract.getSymbol(tokenId);

        uint256 cost = pirateItemContract.getCost(tokenId).mul(amount);

        uint256 totalCost = cost.add(profitPercentage.mul(cost).div(100));

        if (symbol == "DOUBLOONS") {
            uint256 balance = doubloons.balanceOf(msg.sender);

            require (balance >= totalCost, "Insufficient funds");

            require(doubloons.transferFrom(msg.sender, address(this), totalCost), "Transaction error");
        }
        else {
            require(msg.value >= totalCost, "Insufficient funds");
        }

        pirateItemContract.transfer(address(this), msg.sender, tokenId, amount, "");

        emit Bought(tokenId, amount);
    }

    function sellPirateItemToMerchant(uint256 tokenId, uint256 amount) 
        public 
        whenNotPaused 
        nonReentrant 
    {
        require(pirateItemContract.balanceOf(msg.sender, tokenId) >= amount, "Insufficient items");

        uint256 cost = pirateItemContract.getCost(tokenId).mul(amount);

        uint256 totalCost = cost.sub(profitPercentage.mul(cost).div(100)); 

        bytes32 symbol = pirateItemContract.getSymbol(tokenId);

        if (symbol == "DOUBLOONS") {
            uint256 allowance = doubloons.allowance(msg.sender, address(this));
            
            require(allowance >= totalCost, "Insufficient allowance");

            uint256 balance = doubloons.balanceOf(address(this));

            require (balance >= totalCost, "Insufficient funds");

            require(doubloons.transfer(msg.sender, totalCost), "Transaction error");
        }
        else {
            require(address(this).balance >= totalCost, "Insufficient merchant balance");

            (bool success, ) = payable(msg.sender).call{value: totalCost}("");
            require(success, "Payment error");
        }

        pirateItemContract.transfer(msg.sender, address(this), tokenId, amount, "");

        emit Sold(tokenId, amount);
    }

    function withdraw(uint256 amount) public payable nonReentrant onlyOwner {
        require(address(this).balance >= amount, "Amount exceeds balance");

        (bool success, ) = payable(msg.sender).call{value: amount}("");
        require(success, "Withdraw error");
    }
}

