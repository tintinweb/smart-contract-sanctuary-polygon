        uint256 allowedQuantity_,
        uint256 quantity_,
        bytes32[] calldata proof_
    ) external {
        // Check input parameters
        if (allowedQuantity_ == 0) {
            revert Utils.AllowedQuantityIsZero();
        }

        if (quantity_ == 0) {
            revert Utils.QuantityIsZero();
        }

        // to address must not be zero address condition is checked in the Eve contract

        // Check already minted NFTs for the to address
        uint256 alreadyMinted = _merkleMintedNFTs[msg.sender].length();
        if (alreadyMinted + quantity_ > allowedQuantity_) {
            revert Utils.ExceedsAllowedQuantity();
        }

        if (!_isWhitelisted(msg.sender, allowedQuantity_, proof_)) {
            revert Utils.NotWhitelisted();
        }

        // Mint NFTs
        uint256[] memory tokenIds = eveContract.mintNormal(
            msg.sender,
            quantity_
        );

        // Record and Emit events for each of the tokenIds
        for (uint256 i = 0; i < tokenIds.length; ) {
            _merkleMintedNFTs[msg.sender].add(tokenIds[i]);
            emit EveMerkleMinted(tokenIds[i]);
            unchecked {
                ++i;
            }
        }
    }

    /// @notice Returns the tokenIds minted for the given user.
    /// @param user_ The address of the user.
    /// @return tokenIds The tokenIds minted for the given user.
    function getMintedNFTs(
        address user_
    ) external view returns (uint256[] memory) {
        EnumerableSet.UintSet storage userNFTs = _merkleMintedNFTs[user_];
        uint256 length = userNFTs.length();
        uint256[] memory tokenIds = new uint256[](length);

        for (uint256 i = 0; i < length; ) {
            tokenIds[i] = userNFTs.at(i);
            unchecked {
                ++i;
            }
        }

        return tokenIds;
    }
}

