pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./structs/IssuanceStructs.sol";
import "../RupeeDexifStableCoin.sol";

contract IssuanceERC1155Wrapper is ERC1155, Ownable {
    mapping(address => bool) public whitelistedUser;
    // ISIN => IssuanceStoreData
    mapping(string => IssuanceStoreData) public issuances;
    // minted token id => tokenName
    mapping(uint256 => string) public tokenIsinIds;
    // batch id => minted token ids
    mapping(uint256 => uint256[]) public batches;
    // raise id => primary asset raise
    mapping(uint256 => PrimaryAsset) public primaryRaises;
    // investors addr => raise id => investors lots funded
    mapping(address => mapping(uint256 => uint256)) public investors;
    // raise id => investors
    mapping(uint256 => address[]) public raiseInvestors;

    uint256 public raiseId = 0;
    uint256 public lastTokenId = 0;
    uint256 public lastBatchId = 0;

    address public rsc;

    event Minted(
        uint256 id,
        uint256 batchId,
        string tokenName,
        address indexed mintTo
    );
    event RaiseCreated(
        string issuanceIsin,
        uint256 raiseId,
        uint256 numLots,
        uint256 pricePerLot,
        uint256 raiseGoal,
        uint256 raisedAmount,
        bool isEnded,
        bool isMinAmountRaised,
        address founder,
        address escrowWallet
    );
    event Funded(
        uint256 raiseId,
        uint256 numLots,
        uint256 pricePerLot,
        address investor
    );

    event ReFunded(
        uint256 raiseId,
        uint256 refundedAmount,
        address claimer,
        RefundType refundType
    );
    event Closed(uint256 raiseId);
    event RaisedAssetMinted(uint256 raiseId, BatchData[] batches);

    constructor(address rscContact) ERC1155("") {
        rsc = rscContact;
    }

    function mintBatch(
        BatchType batchType,
        IssuanceInput[] memory issuancesToMint
    ) public {
        require(whitelistedUser[msg.sender], "IssuanceContract: !whitelisted");

        for (uint256 i = 0; i < issuancesToMint.length; i++) {
            IssuanceStoreData memory existed = issuances[
                issuancesToMint[i].tokenName
            ];
            uint256 idToMint = existed.exists
                ? existed.mintedTokenId
                : lastTokenId;

            if (batchType == BatchType.STRIPPED) {
                issuances[issuancesToMint[i].tokenName] = IssuanceStoreData(
                    true,
                    issuancesToMint[i].isin,
                    issuancesToMint[i].tokenName,
                    lastBatchId,
                    idToMint,
                    batchType,
                    issuancesToMint[i].bondType,
                    true,
                    issuancesToMint[i].cfId,
                    issuancesToMint[i].payStatus,
                    0
                );
            }
            if (batchType == BatchType.WHOLE) {
                issuances[issuancesToMint[i].tokenName] = IssuanceStoreData(
                    true,
                    issuancesToMint[i].isin,
                    issuancesToMint[i].tokenName,
                    lastBatchId,
                    issuancesToMint[i].bondType == BondType.COUPON
                        ? idToMint
                        : existed.exists
                        ? existed.mintedTokenId
                        : lastTokenId,
                    batchType,
                    issuancesToMint[i].bondType,
                    issuancesToMint[i].bondType == BondType.COUPON
                        ? false
                        : true,
                    issuancesToMint[i].cfId,
                    issuancesToMint[i].payStatus,
                    0
                );
            }
            batches[lastBatchId].push(idToMint);
            tokenIsinIds[lastTokenId] = issuancesToMint[i].tokenName;
            _mint(
                issuancesToMint[i].mintTo,
                idToMint,
                issuancesToMint[i].numFrac,
                "0x00"
            );
            emit Minted(
                idToMint,
                lastBatchId,
                issuancesToMint[i].tokenName,
                issuancesToMint[i].mintTo
            );
            if (!existed.exists) {
                lastTokenId = lastTokenId + 1;
            }
        }
        lastBatchId = lastBatchId + 1;
    }

    function whitelistMinter(address minter) public onlyOwner {
        whitelistedUser[minter] = true;
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 mintedTokenId,
        uint256 amount,
        bytes memory data
    ) public virtual override {
        IssuanceStoreData memory issuance = getIssuanceById(mintedTokenId);

        require(
            from == _msgSender() || isApprovedForAll(from, _msgSender()),
            "IssuanceContract: !owner or !approved"
        );
        require(issuance.tradable, "IssuanceContract: !tradable");

        if (
            issuance.batchType == BatchType.WHOLE &&
            issuance.bondType == BondType.PRINCIPAL
        ) {
            uint256[] memory mintedTokenIds = batches[issuance.batchId];

            for (uint256 i = 0; i < mintedTokenIds.length; i++) {
                IssuanceStoreData memory currentIssuance = getIssuanceById(
                    mintedTokenIds[i]
                );

                if (currentIssuance.relamountFrac == 0) {
                    _safeTransferFrom(
                        from,
                        to,
                        mintedTokenIds[i],
                        amount,
                        data
                    );
                }
            }
        } else {
            require(issuance.relamountFrac == 0, "IssuanceContract: rel token");
            _safeTransferFrom(from, to, issuance.mintedTokenId, amount, data);
        }
    }

    function releasePayout(
        uint256 mintedTokenId,
        uint256[] calldata amount,
        address[] calldata to
    ) public {
        require(
            amount.length == to.length,
            "IssuanceContract: !matching length"
        );

        for (uint256 i = 0; i < amount.length; i++) {
            string memory isin = tokenIsinIds[mintedTokenId];
            IssuanceStoreData storage issuance = issuances[isin];
            issuance.relamountFrac += amount[i];

            RupeeDexifStableCoin(rsc).transferFrom(
                msg.sender,
                to[i],
                amount[i]
            );
        }
    }

    function createPrimaryAssetRaise(
        string memory isin,
        uint256 minNumLots,
        uint256 pricePerLot,
        address escrowAddr
    ) external {
        require(whitelistedUser[msg.sender], "IssuanceContract: !whitelisted");
        require(
            RupeeDexifStableCoin(rsc).allowance(escrowAddr, address(this)) ==
                0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff,
            "IssuanceContract: !max uint approved from escrow wallet"
        );

        primaryRaises[raiseId] = PrimaryAsset(
            isin,
            minNumLots,
            pricePerLot,
            minNumLots * pricePerLot,
            0,
            false,
            false,
            false,
            false,
            msg.sender,
            escrowAddr
        );
        raiseId += 1;

        emit RaiseCreated(
            isin,
            raiseId,
            minNumLots,
            pricePerLot,
            minNumLots * pricePerLot,
            0,
            false,
            false,
            msg.sender,
            escrowAddr
        );
    }

    function closePrimaryAssetRaise(uint256 raiseId) external {
        require(whitelistedUser[msg.sender], "IssuanceContract: !whitelisted");

        primaryRaises[raiseId].isEnded = true;

        emit Closed(raiseId);
    }

    function fundAsset(uint256 raisingId, uint256 numLots) external {
        require(
            !primaryRaises[raisingId].isEnded,
            "IssuanceContract: funding ended"
        );
        require(
            investors[msg.sender][raisingId] == 0,
            "IssuanceContract: already funded"
        );

        uint256 chargeAmount = numLots * primaryRaises[raisingId].pricePerLot;
        raiseInvestors[raisingId].push(msg.sender);

        if (
            primaryRaises[raisingId].raisedAmount + chargeAmount >=
            primaryRaises[raisingId].minRaiseGoal
        ) {
            primaryRaises[raisingId].isMinAmountRaised = true;
        }

        investors[msg.sender][raisingId] = numLots;
        primaryRaises[raisingId].raisedAmount += chargeAmount;

        RupeeDexifStableCoin(rsc).transferFrom(
            msg.sender,
            primaryRaises[raisingId].escrowWallet,
            chargeAmount
        );

        emit Funded(
            raisingId,
            numLots,
            primaryRaises[raisingId].pricePerLot,
            msg.sender
        );
    }

    function refundIfNotRaised(uint256 raisingId) external {
        require(
            !primaryRaises[raisingId].isMinAmountRaised &&
                primaryRaises[raisingId].isEnded,
            "IssuanceContract: !refund conditions"
        );
        require(
            !primaryRaises[raisingId].isRefunded,
            "IssuanceContract: already refunded"
        );
        require(whitelistedUser[msg.sender], "IssuanceContract: !whitelisted");

        address[] memory investorsAddrs = raiseInvestors[raisingId];

        for (uint256 i = 0; i < investorsAddrs.length; i++) {
            uint256 investedInLots = investors[investorsAddrs[i]][raisingId];

            RupeeDexifStableCoin(rsc).transferFrom(
                primaryRaises[raisingId].escrowWallet,
                investorsAddrs[i],
                investedInLots * primaryRaises[raisingId].pricePerLot
            );
            investors[investorsAddrs[i]][raisingId] = 0;

            emit ReFunded(
                raisingId,
                investedInLots * primaryRaises[raisingId].pricePerLot,
                investorsAddrs[i],
                RefundType.NOT_RAISED
            );
        }
        primaryRaises[raisingId].isRefunded = true;
    }

    function refundIfNotApprovedLots(
        uint256 raisingId,
        address[] memory receivers,
        uint256[] memory approvedLotsPerReceiver
    ) external {
        require(
            receivers.length == approvedLotsPerReceiver.length,
            "IssuanceContract: Unmatching length"
        );
        require(
            receivers.length == approvedLotsPerReceiver.length,
            "IssuanceContract: Unmatching length"
        );
        require(primaryRaises[raisingId].isMinted, "IssuanceContract: !minted");
        require(
            !primaryRaises[raisingId].isRefunded,
            "IssuanceContract: already refunded"
        );
        require(whitelistedUser[msg.sender], "IssuanceContract: !whitelisted");

        for (uint256 i = 0; i < receivers.length; i++) {
            uint256 investedInLots = investors[receivers[i]][raisingId];

            RupeeDexifStableCoin(rsc).transferFrom(
                primaryRaises[raisingId].escrowWallet,
                receivers[i],
                (investedInLots - approvedLotsPerReceiver[i]) *
                    primaryRaises[raisingId].pricePerLot
            );

            emit ReFunded(
                raisingId,
                (investedInLots - approvedLotsPerReceiver[i]) *
                    primaryRaises[raisingId].pricePerLot,
                receivers[i],
                RefundType.NOT_ENOUGHT_APPROVED_LOTS
            );
        }
        primaryRaises[raisingId].isRefunded = true;
    }

    function mintRaisedAsset(
        BatchData[] memory batchesToMint,
        uint256 raisingId
    ) external {
        address[] memory assetInvestors = raiseInvestors[raisingId];

        require(
            primaryRaises[raisingId].isMinAmountRaised,
            "IssuanceContract: !min amount raised"
        );
        require(
            !primaryRaises[raisingId].isMinted,
            "IssuanceContract: already minted"
        );
        require(
            assetInvestors.length == batchesToMint.length,
            "IssuanceContract: Unmatching length"
        );

        primaryRaises[raisingId].isMinted = true;

        for (uint256 i = 0; i < batchesToMint.length; i++) {
            mintBatch(
                batchesToMint[i].batchType,
                batchesToMint[i].issuancesToMint
            );
        }

        emit RaisedAssetMinted(raisingId, batchesToMint);
    }

    function getBatchData(uint256 issuanceBatchId)
        public
        view
        returns (uint256[] memory)
    {
        return batches[issuanceBatchId];
    }

    function getIssuanceById(uint256 mintedTokenId)
        public
        view
        returns (IssuanceStoreData memory issuance)
    {
        string memory isin = tokenIsinIds[mintedTokenId];
        issuance = issuances[isin];
        return issuance;
    }

    function getIssuances(uint256[] memory mintedTokenIds)
        public
        view
        returns (IssuanceStoreData[] memory)
    {
        IssuanceStoreData[] memory storedIssuances = new IssuanceStoreData[](
            mintedTokenIds.length
        );

        for (uint256 i = 0; i < mintedTokenIds.length; i++) {
            string memory tokenIsin = tokenIsinIds[mintedTokenIds[i]];

            storedIssuances[i] = issuances[tokenIsin];
        }

        return storedIssuances;
    }
}

