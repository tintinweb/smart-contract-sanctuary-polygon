pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

contract RupeeDexifStableCoin is ERC20, ERC20Burnable, Ownable, Pausable {
    mapping(address => bool) public whitelistedMinter;
    // bankTxId => amount
    mapping(string => uint256) public depositData;
    // bankTxId => amount
    mapping(string => uint256) public withdrawData;

    constructor(string memory tokenName, string memory tokenSymbol)
        ERC20(tokenName, tokenSymbol)
    {
        whitelistedMinter[msg.sender] = true;
        mint(msg.sender, 10000000 * 10**18, "initial_mint_tx");
    }

    function whitelistMinter(address minter) public onlyOwner {
        whitelistedMinter[minter] = true;
    }

    function mint(address to, uint256 amount, string memory bankTxId) public {        
        depositData[bankTxId] = amount;

        require(
            whitelistedMinter[tx.origin],
            "RSC: address not whitelisted for mint"
        );
        _mint(to, amount);
    }

    function burnFromUser(address account, uint256 amount, string memory bankTxId) public  {
        withdrawData[bankTxId] = amount;

        require(
            whitelistedMinter[tx.origin],
            "RSC: address not whitelisted for burn"
        );

        _burn(account, amount);
    }
    
    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override whenNotPaused {
        super._beforeTokenTransfer(from, to, amount);
    }
}

pragma solidity ^0.8.0;
import "../enums/IssuanceEnums.sol";

struct IssuanceInput {
    string isin; // ISIN1
    string tokenName; // ISIN1_Date2_C_S0
    uint256 numFrac; // 0
    address mintTo; // 0x00
    BondType bondType; // principal
    uint256 cfId; // 0
    string payStatus; // pending
}

struct IssuanceStoreData {
    bool exists; // true
    string isin; // ISIN1
    string tokenName; // ISIN1_Date2_C_S
    uint256 batchId; // 0
    uint256 mintedTokenId;
    BatchType batchType;
    BondType bondType; // principal
    bool tradable; // true
    uint256 cfId; // 0
    string payStatus; // pending
    uint256 relamountFrac; // 0
}

struct PrimaryAsset {
    string issuanceIsin;
    uint256 minNumLots;
    uint256 pricePerLot;
    uint256 minRaiseGoal;
    uint256 raisedAmount;
    bool isEnded;
    bool isMinAmountRaised;
    bool isMinted;
    bool isRefunded;
    address creator;
    address escrowWallet;
}

struct BatchData {
    BatchType batchType;
    IssuanceInput[] issuancesToMint;
}

