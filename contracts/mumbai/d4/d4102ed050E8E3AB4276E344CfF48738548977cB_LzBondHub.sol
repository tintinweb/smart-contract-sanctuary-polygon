    function initializeFollowModule(
        uint256 profileId,
        bytes calldata data
    ) external override onlyHub returns (bytes memory) {
        console.log('ApplicationFollowModule linitialize %s ', profileId);
        // address owner = IERC721(HUB).ownerOf(profileId);
        if (data.length > 0) {
