    function removeElement(
        uint256[] storage _array,
        uint256 _element
    ) internal returns (uint256[] memory) {
        for (uint256 i; i < _array.length; i++) {
            if (_array[i] == _element) {
                _array[i] = _array[_array.length - 1];
                _array.pop();
                break;
            }
        }
        return _array;
    }

    function decideWinner(
        uint256 _gameId,
        uint256 _player1Score,
        uint256 _player2Score,
        uint256 _player1Wins,
        uint256 _player2Wins
    ) internal returns (uint256) {
        Game memory currentGame = allGames[_gameId];
        // At the end, the player with the maximum point wins.
        uint256 winner;
        if (_player1Score != _player2Score) {
            winner = _player1Score > _player2Score
                ? currentGame.player1
                : currentGame.player2;
        }
        // In case of tie with points, one winning more rounds wins. In case of tie here also, the starting player wins.
        else {
            winner = _player1Wins >= _player2Wins
                ? currentGame.player1
                : currentGame.player2;
        }
        transferRewards(_gameId, userAddresses[winner]);
        return winner;
    }

    function updateUserScore(
        uint256 _gameId,
        uint256 _winnerId,
        uint256 _loserId,
        uint256 _cardIndex
    ) internal {
        Game storage currentGame = allGames[_gameId];
        uint256 winnerCardLevel = 0;
        uint256 loserCardLevel = 0;
        uint256 winnerCardId = 0;
        uint256 loserCardId = 0;
        if (
            (_winnerId == currentGame.player1 &&
                _loserId == currentGame.player2) ||
            (_winnerId == currentGame.player2 &&
                _loserId == currentGame.player1)
        ) {
            winnerCardLevel = _winnerId == currentGame.player1
                ? getCurrentCardStat(currentGame.player1Cards[_cardIndex], 9)
                : getCurrentCardStat(currentGame.player2Cards[_cardIndex], 9);
            loserCardLevel = _winnerId == currentGame.player1
                ? getCurrentCardStat(currentGame.player2Cards[_cardIndex], 9)
                : getCurrentCardStat(currentGame.player1Cards[_cardIndex], 9);
            winnerCardId = _winnerId == currentGame.player1
                ? currentGame.player1Cards[_cardIndex]
                : currentGame.player2Cards[_cardIndex];
            loserCardId = _winnerId == currentGame.player1
                ? currentGame.player2Cards[_cardIndex]
                : currentGame.player1Cards[_cardIndex];
        }
        if (winnerCardLevel < loserCardLevel) {
            gameCardContract.incrementCardWinsAgainstBetterPlayer(winnerCardId);
            gameCardContract.incrementCardLossesAgainstWorsePlayer(loserCardId);
            allPlayers[_winnerId].score += (loserCardLevel - winnerCardLevel);
            allPlayers[_loserId].negativeScore += (loserCardLevel -
                winnerCardLevel);
        }
        updateUserLevel(_winnerId);
        updateUserLevel(_loserId);
        gameCardContract.updateCardLevel(winnerCardId);
        gameCardContract.updateCardLevel(loserCardId);
    }

    function updateUserLevel(uint256 _playerId) internal {
        Player storage currentPlayer = allPlayers[_playerId];

        if (currentPlayer.score > currentPlayer.negativeScore) {
            if (
                currentPlayer.score - currentPlayer.negativeScore >= 200 &&
                currentPlayer.level < 5
            ) {
                currentPlayer.score = 0;
                currentPlayer.negativeScore = 0;
                currentPlayer.level += 1;
            } else if (
                currentPlayer.score - currentPlayer.negativeScore >= 500 &&
                currentPlayer.level > 4 &&
                currentPlayer.level < 10
            ) {
                currentPlayer.score = 0;
                currentPlayer.negativeScore = 0;
                currentPlayer.level += 1;
            } else if (
                currentPlayer.score - currentPlayer.negativeScore >= 750 &&
                currentPlayer.level > 4 &&
                currentPlayer.level < 15
            ) {
                currentPlayer.score = 0;
                currentPlayer.negativeScore = 0;
                currentPlayer.level += 1;
            } else if (
                currentPlayer.score - currentPlayer.negativeScore >= 1000 &&
                currentPlayer.level > 4 &&
                currentPlayer.level < 20
            ) {
                currentPlayer.score = 0;
                currentPlayer.negativeScore = 0;
                currentPlayer.level += 1;
            }
        }
    }

    function updateCurrentPlayerscore(
        uint256 _gameId,
        uint256 _playerId,
        uint256 _currentScore,
        uint256 _cardIndex
    ) internal view returns (uint256) {
        Game storage currentGame = allGames[_gameId];
        // reading currentPlayer's responses
        uint256[] memory currentPlayerResonse = _playerId == currentGame.player1
            ? currentGame.player1Response
            : currentGame.player2Response;

        // if the chosen card is the captain Card
        if (_cardIndex == currentPlayerResonse[11]) {
            _currentScore += 100;
        }
        // if the chosen card is the vice captain card
        else if (_cardIndex == currentPlayerResonse[12]) {
            _currentScore += 50;
        }
        // remaining cards
        else {
            _currentScore += 10;
        }
        // for each win player's overall score gets updated
        return _currentScore;
    }

    function exportNFT() public isEligible(0) {
        uint256 currentPlayerId = addressToPlayerId[msg.sender];
        Player storage currentPlayer = allPlayers[currentPlayerId];
        // Check if the caller owns an NFT
        require(
            nftToken.balanceOf(msg.sender) <= getNoOfSpecialMoves(),
            "user already claimed NFT"
        );
        NFTMetadata memory metaData;
        metaData.userLevel = currentPlayer.level;
        metaData.userScore = currentPlayer.score;
        metaData.currentOwner = msg.sender;
        metaData.timestamp = block.timestamp;
        // Mint a new NFT and assign it to the recipient

        uint256 tokenId = nftToken.mint(msg.sender);
        allNftMetadatas[tokenId] = metaData;

        allPlayers[addressToPlayerId[msg.sender]].level = 0;
        allPlayers[addressToPlayerId[msg.sender]].score = 0;
        allPlayers[addressToPlayerId[msg.sender]].negativeScore = 0;
        emit NFTClaimed(
            currentPlayerId,
            currentPlayer.level,
            tokenId,
            block.timestamp
        );
    }

    function importNFT(uint256 _tokenId) external {
        require(isTokenOwner(msg.sender, _tokenId), "Caller not the owner");
        Player storage currentPlayer = allPlayers[
            addressToPlayerId[msg.sender]
        ];
        if (
            allPlayers[addressToPlayerId[msg.sender]].level >=
            minimumLevelForRewards
        ) {
            exportNFT();
        }
        NFTMetadata storage metadata = allNftMetadatas[_tokenId];
        currentPlayer.level = metadata.userLevel;
        currentPlayer.score = metadata.userScore;
        metadata.currentOwner = address(this);
        nftToken.transfer(msg.sender, address(this), _tokenId);
    }

