pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

//imported contracts
interface GameCardsContractInterface {
    function getCardStat(
        uint256 _cardId,
        uint256 _responseId
    ) external view returns (uint256);

    function getPreferredPlayer() external view returns (uint256);

    function getUniquePreferredPlayer(
        uint256[] memory cards
    ) external view returns (uint256);

    function getUniquePlayer(
        uint256[] memory cards
    ) external view returns (uint256);

    function incremetCardMatches(uint256 _cardId) external;

    function incrementCardWinsAgainstBetterPlayer(uint256 _cardId) external;

    function incrementCardLossesAgainstWorsePlayer(uint256 _cardId) external;

    function updateCardLevel(uint256 _cardId) external;
}

interface MyNFTInterface {
    function mint(address recipient) external returns (uint256);

    function transfer(address from, address to, uint256 tokenId) external;

    function ownerOf(uint256 tokenId) external view returns (address);

    function balanceOf(address owner) external view returns (uint256);

    function approve(address to, uint256 tokenId) external;
}

contract GamePlay is Ownable {
    using SafeMath for uint256;
    struct Game {
        uint256 player1;
        uint256 player2;
        uint256[] player1Cards;
        uint256[] player1Response; //last two indexes store captain and vice captain's cardId [index of player1Cards]
        uint256[] player2Cards;
        uint256[] player2Response; ////last two indexes store captain and vice captain's cardId [index of player2Cards]
        uint256[] player1SuperPowerCards;
        uint256[] player2SuperPowerCards;
        uint256 winner;
        uint256 startTime;
        bool matchInComplete;
        bool rewardsClaimed;
    }

    struct Player {
        uint256 level;
        uint256 score;
        uint256 negativeScore;
        uint256 matches; //no of matches joined by the player till now
        uint256[] currentGameIds; //keeps track of all the active games the player is a part of
        mapping(uint256 => uint256) joiningBlockNumbers;
    }

    struct NFTMetadata {
        uint256 userLevel;
        uint256 userScore;
        address currentOwner;
        uint256 timestamp;
    }

    uint256 private maxNoOfCards = 1000;
    uint256 private cardsAssignedToEachUser = 11;
    uint256 private responseArraySize = 13;
    uint256 private joiningTokens = 1000 wei;
    uint256 public rewardTokens = 1800 wei;
    uint256 public gameDuration = 30 minutes;
    uint256 public blockConfirmations = 3;
    uint256 private minimumLevelForRewards = 6;
    uint256 private levelDifference = 5;
    uint256 private userTiers = 5;

    uint256 private lastGameId; // keeps track of the most recent game any player has joined
    uint256 private playerCount; // last playerId in the game
    uint256 private lastUnMatchedGameId = 1; //keeps track of the last gameId having one participant
    uint256[] private activeGameIds; //currently active gameIds

    mapping(address => uint256) public addressToPlayerId;
    mapping(uint256 => Player) public allPlayers; //playerId to playerDetails
    mapping(uint256 => NFTMetadata) public allNftMetadatas; //mapping from tokenId
    mapping(uint256 => Game) private allGames; //gameId to gameDetails
    mapping(uint256 => uint256) private canCheckRivalCards; // mapping from gameId to playerId
    mapping(uint256 => address) private userAddresses;

    event PlayerJoined(
        address playerAddress,
        uint256 gameId,
        uint256 timeOfJoin
    );
    event ResponseSubmitted(
        uint256 playerId,
        uint256 gameId,
        uint256 timeOfSubmission
    );
    event winnerDeclared(uint256 gameId, uint256 winnerId);
    event RewardsTransferred(
        uint256 gameId,
        uint256 playerId,
        uint256 amount,
        uint256 timeOfRewardTransfer
    );
    event NFTClaimed(
        uint256 playerId,
        uint256 playerLevel,
        uint256 tokenId,
        uint256 timeOfNftClaim
    );
    event CardsShuffled(
        uint256 _gameId,
        uint256 _playerId,
        uint256[] _shuffledCards,
        uint256 timeOfCardShuffle
    );
    event CardsReplaced(
        uint256 _gameId,
        uint256 _playerId,
        uint256[] _replacedCards,
        uint256 timeOfCardReplace
    );
    event PreferredCardsSet(
        uint256 _gameId,
        uint256 _playerId,
        uint256[] _preferredCards,
        uint256 timeOfCardUpdate
    );
    event RivalCardsChecked(
        uint256 _gameId,
        uint256 _playerId,
        uint256 timeOfCardCheck
    );
    event SuperPowerUsed(
        uint256 _gameId,
        uint256 _playerId,
        uint256 _superpower,
        uint256 timeOfMoveUsed
    );

    GameCardsContractInterface private gameCardContract;
    IERC20 private gameToken;
    MyNFTInterface private nftToken;

    constructor(
        address _gameCardContractAddress,
        address _erc20TokenAddress,
        address _nftTokenAddress
    ) {
        gameCardContract = GameCardsContractInterface(_gameCardContractAddress);
        gameToken = IERC20(_erc20TokenAddress);
        nftToken = MyNFTInterface(_nftTokenAddress);
    }

    //MODIFIERS
    modifier blocksConfirmed(uint256 _blockNumber) {
        require(
            block.number >= _blockNumber + blockConfirmations,
            "try again after sometime"
        );
        _;
    }

    modifier playerInCurrentGame(uint256 _gameId) {
        Game memory currentGame = allGames[_gameId];
        require(
            currentGame.player1 == addressToPlayerId[msg.sender] ||
                currentGame.player2 == addressToPlayerId[msg.sender],
            "player not in current game"
        );
        _;
    }

    modifier gameTimeNotOver(uint256 _gameId) {
        Game memory currentGame = allGames[_gameId];
        if (
            (currentGame.player1 == addressToPlayerId[msg.sender] &&
                currentGame.startTime != 0) ||
            addressToPlayerId[msg.sender] == currentGame.player2
        ) {
            require(
                block.timestamp < currentGame.startTime + gameDuration,
                "time limit exceeded"
            );
        }
        _;
    }

    modifier responseNotSubmitted(uint256 _gameId) {
        Game memory currentGame = allGames[_gameId];
        uint256 response = addressToPlayerId[msg.sender] == currentGame.player1
            ? currentGame.player1Response[0]
            : currentGame.player2Response[0];
        require(response == 0, "response already submitted");

        _;
    }

    modifier playerInActiveGames() {
        require(
            allPlayers[addressToPlayerId[msg.sender]].currentGameIds.length !=
                0,
            "player not in any active games"
        );
        _;
    }

    modifier playerIdExists() {
        require(addressToPlayerId[msg.sender] != 0, "player doesn't exist");
        _;
    }

    modifier gameIdExists(uint256 _gameId) {
        require(_gameId <= lastGameId, "invalid GameID");
        _;
    }

    modifier rewardNotClaimed(uint256 _gameId) {
        Game memory currentGame = allGames[_gameId];
        require(currentGame.rewardsClaimed == false, "already claimed");
        _;
    }

    modifier isEligible(uint256 _powerIndex) {
        require(
            allPlayers[addressToPlayerId[msg.sender]].level >=
                minimumLevelForRewards,
            "user not eligible"
        );
        _;
    }

    //FUNCTIONS

    function getNoOfSpecialMoves() internal view returns (uint256) {
        uint256 currentPlayerLevel = allPlayers[addressToPlayerId[msg.sender]]
            .level;
        for (uint256 i = 1; i < userTiers; i++) {
            if (
                currentPlayerLevel > i * levelDifference &&
                currentPlayerLevel <
                minimumLevelForRewards + i * levelDifference
            ) {
                return i;
            }
        }
        return 0;
    }

    function assignRandomCards(
        uint256 _gameId
    ) internal view returns (uint256[] memory) {
        uint256[] memory cardNos = new uint256[](cardsAssignedToEachUser);
        Game memory currentGame = allGames[_gameId];
        uint256 currentPlayerLevel = allPlayers[addressToPlayerId[msg.sender]]
            .level;
        uint256 firstindex = (uint256(
            keccak256(abi.encodePacked(block.timestamp, lastGameId))
        ) % maxNoOfCards) + 1;
        uint256 offset = (uint256(
            keccak256(abi.encodePacked(block.prevrandao, block.number))
        ) % maxNoOfCards) + 1;
        if (addressToPlayerId[msg.sender] == currentGame.player2) {
            uint256[] memory exclude = currentGame.player1Cards;
            uint256 count = 0;
            while (count < cardsAssignedToEachUser) {
                uint256 random = (((firstindex + (count + 1) * offset) %
                    maxNoOfCards) + 1);
                bool found = false;
                for (uint256 i = 0; i < exclude.length; i++) {
                    if (exclude[i] == random) {
                        found = true;
                        break;
                    }
                }
                if (!found) {
                    cardNos[count] = random;
                    count++;
                }
            }
        } else {
            for (uint256 i = 0; i < cardsAssignedToEachUser; i++) {
                cardNos[i] = (((firstindex + (i + 1) * offset) % maxNoOfCards) +
                    1);
            }
        }
        if (currentPlayerLevel >= minimumLevelForRewards) {
            setPreferredPlayers(
                _gameId,
                addressToPlayerId[msg.sender],
                cardNos,
                currentPlayerLevel
            );
        }
        return cardNos;
    }

    function setPreferredPlayers(
        uint256 _gameId,
        uint256 _playerId,
        uint256[] memory cards,
        uint256 currentPlayerLevel
    ) internal view returns (uint256[] memory) {
        if (_playerId == allGames[_gameId].player2) {
            for (uint i = 0; i < cards.length; i++) {
                if (currentPlayerLevel >= (i + 1) * levelDifference + 1) {
                    cards[i] = gameCardContract.getUniquePreferredPlayer(
                        allGames[_gameId].player1Cards
                    );
                }
            }
        } else {
            for (uint i = 0; i < cards.length; i++) {
                if (currentPlayerLevel >= (i + 1) * levelDifference + 1) {
                    cards[i] = gameCardContract.getPreferredPlayer();
                }
            }
        }

        return cards;
    }

    function joinGame(uint256 _tokenAmount) external {
        require(_tokenAmount >= joiningTokens, "joining fee not sufficient");
        bool success = acceptPayment(_tokenAmount);
        require(success, "failed to transfer tokens");
        uint256 currentPlayerId;
        // if player not already in the DB add it to DB
        if (addressToPlayerId[msg.sender] == 0) {
            playerCount++;
            currentPlayerId = playerCount;
            addressToPlayerId[msg.sender] = playerCount;
            userAddresses[playerCount] = msg.sender;
        }
        // if player already in the db
        else {
            currentPlayerId = addressToPlayerId[msg.sender];
        }
        allPlayers[playerCount].matches++;
        // if last gameId in db doesn't have a matching player add the new player to last game
        if (
            allGames[lastUnMatchedGameId].matchInComplete &&
            allGames[lastUnMatchedGameId].player1 != currentPlayerId
        ) {
            Game storage lastUnmatchedGame = allGames[lastUnMatchedGameId];
            lastUnmatchedGame.player2 = addressToPlayerId[msg.sender];
            lastUnmatchedGame.player2Cards = assignRandomCards(
                lastUnMatchedGameId
            );
            lastUnmatchedGame.matchInComplete = false;
            lastUnmatchedGame.rewardsClaimed = false;
            lastUnmatchedGame.winner = 0;
            lastUnmatchedGame.startTime = block.timestamp;
            lastUnmatchedGame.player1Response = new uint256[](
                responseArraySize
            );
            lastUnmatchedGame.player2Response = new uint256[](
                responseArraySize
            );
            activeGameIds = removeElement(activeGameIds, lastUnMatchedGameId);
            allPlayers[currentPlayerId].currentGameIds.push(
                lastUnMatchedGameId
            );
            allPlayers[currentPlayerId].joiningBlockNumbers[
                lastUnMatchedGameId
            ] = block.number;
            emit PlayerJoined(msg.sender, lastUnMatchedGameId, block.timestamp);
            lastUnMatchedGameId++;
        }
        // If the last game's team is complete create a new game
        else {
            Game memory game = allGames[lastGameId];
            game.player1 = addressToPlayerId[msg.sender];
            game.player1Cards = assignRandomCards(lastGameId);
            game.matchInComplete = true;
            lastGameId++;
            allGames[lastGameId] = game;
            activeGameIds.push(lastGameId);
            allPlayers[currentPlayerId].currentGameIds.push(lastGameId);
            allPlayers[currentPlayerId].joiningBlockNumbers[lastGameId] = block
                .number;
            emit PlayerJoined(msg.sender, lastUnMatchedGameId, block.timestamp);
        }
    }

    function submitResponse(
        uint256 _gameId,
        uint256[] memory _stats, //preferred card stat indexes(11) , captain, vice Captain cardIds
        bool _executeSuperPower,
        uint256[] memory _superPowerCards //cardIndexes
    )
        external
        gameIdExists(_gameId)
        playerInCurrentGame(_gameId)
        gameTimeNotOver(_gameId)
        responseNotSubmitted(_gameId)
    {
        require(_stats.length == responseArraySize, "Invalid array size.");
        if (_executeSuperPower) {
            require(
                _superPowerCards.length <= getNoOfSpecialMoves(),
                "array size exceeds allowedCardMoves."
            );
        }

        Game storage currentGame = allGames[_gameId];
        uint256 currentPlayerId = addressToPlayerId[msg.sender];

        if (currentPlayerId == currentGame.player1) {
            currentGame.player1Response = _stats;
            if (_executeSuperPower) {
                currentGame.player1SuperPowerCards = _superPowerCards;
            }
        } else if (currentPlayerId == currentGame.player2) {
            currentGame.player2Response = _stats;
            if (_executeSuperPower) {
                currentGame.player2SuperPowerCards = _superPowerCards;
            }
        }

        // if both  the players submitted resposnses, declare results
        if (
            currentGame.player1Response[0] != 0 &&
            currentGame.player2Response[0] != 0
        ) {
            declareResults(_gameId);
        }
        emit ResponseSubmitted(currentPlayerId, _gameId, block.timestamp);
    }

    function declareResults(uint256 _gameId) internal {
        Game storage currentGame = allGames[_gameId];
        uint256 player1Id = currentGame.player1;
        uint256 player2Id = currentGame.player2;
        uint256 player1Score; //current game's scores
        uint256 player2Score;
        uint256 currentStatChoice; //which stat to play on 1, 2, 3, 4, 5, 6, 7 or 8
        uint256 player1CurrentCardStat; //player's current card's stat
        uint256 player2CurrentCardStat;
        uint256 player1Wins; //no of rounds won by a player in the current game
        uint256 player2Wins;
        uint256 currentCardStatChooser = 1; //whether player 1 or player 2 gets to choose the card stat(currentStatChoice), initially set to player 1
        bool isPlayer1SuperCard;
        bool isPlayer2SuperCard;
        for (uint256 i = 0; i < cardsAssignedToEachUser; i++) {
            currentStatChoice = currentCardStatChooser == 1
                ? currentGame.player1Response[i]
                : currentGame.player2Response[i];
            isPlayer1SuperCard = (currentGame.player1SuperPowerCards.length !=
                0 &&
                (currentGame.player1SuperPowerCards[0] == i ||
                    currentGame.player1SuperPowerCards[1] == i ||
                    currentGame.player1SuperPowerCards[2] == i));
            isPlayer2SuperCard = (currentGame.player2SuperPowerCards.length !=
                0 &&
                (currentGame.player2SuperPowerCards[0] == i ||
                    currentGame.player2SuperPowerCards[1] == i ||
                    currentGame.player2SuperPowerCards[2] == i));
            player1CurrentCardStat =
                getCurrentCardStat(
                    currentGame.player1Cards[i],
                    isPlayer1SuperCard ? 2 : currentStatChoice
                ) +
                (
                    isPlayer1SuperCard
                        ? getCurrentCardStat(currentGame.player1Cards[i], 8)
                        : 0
                );
            player2CurrentCardStat =
                getCurrentCardStat(
                    currentGame.player2Cards[i],
                    isPlayer2SuperCard ? 2 : currentStatChoice
                ) +
                (
                    isPlayer2SuperCard
                        ? getCurrentCardStat(currentGame.player2Cards[i], 8)
                        : 0
                );
            if (player1CurrentCardStat > player2CurrentCardStat) {
                player1Score = updateCurrentPlayerscore(
                    _gameId,
                    player1Id,
                    player1Score,
                    i
                );
                updateUserScore(_gameId, player1Id, player2Id, i);
                player1Wins++;
                currentCardStatChooser = 1;
            }
            // in case of tie both players score increases and next stat is choosed according to previous game
            else if (player1CurrentCardStat == player2CurrentCardStat) {
                player1Score = updateCurrentPlayerscore(
                    _gameId,
                    player1Id,
                    player1Score,
                    i
                );
                player2Score = updateCurrentPlayerscore(
                    _gameId,
                    player2Id,
                    player2Score,
                    i
                );
                player1Wins++;
                player2Wins++;
            } else {
                player2Score = updateCurrentPlayerscore(
                    _gameId,
                    player2Id,
                    player2Score,
                    i
                );
                updateUserScore(_gameId, player2Id, player1Id, i);
                player2Wins++;
                currentCardStatChooser = 2;
            }
            gameCardContract.incremetCardMatches(currentGame.player1Cards[i]);
            gameCardContract.incremetCardMatches(currentGame.player2Cards[i]);
        }
        currentGame.winner = decideWinner(
            _gameId,
            player1Score,
            player2Score,
            player1Wins,
            player2Wins
        );
        allPlayers[player1Id].currentGameIds = removeElement(
            allPlayers[player1Id].currentGameIds,
            _gameId
        );
        allPlayers[player2Id].currentGameIds = removeElement(
            allPlayers[player2Id].currentGameIds,
            _gameId
        );
        allPlayers[currentGame.winner].level++;

        emit winnerDeclared(_gameId, currentGame.winner);
    }

    function transferRewards(uint256 _gameId, address winnerAddress) internal {
        Game storage currentGame = allGames[_gameId];
        currentGame.rewardsClaimed = true;
        gameToken.transfer(winnerAddress, rewardTokens);
        emit RewardsTransferred(
            _gameId,
            addressToPlayerId[winnerAddress],
            rewardTokens,
            block.timestamp
        );
    }

    function retrieveGameResults(
        uint256 _gameId
    ) external view returns (uint256) {
        uint256 winner = allGames[_gameId].winner;
        require(winner != 0, "game not over yet");
        return winner;
    }

    function transferRewardTokensForAbandonedGames(
        uint256 _gameId
    ) external onlyOwner gameIdExists(_gameId) {
        Game storage currentGame = allGames[_gameId];

        if (
            currentGame.winner == 0 &&
            block.timestamp > currentGame.startTime + gameDuration
        ) {
            if (
                currentGame.player1Response[0] == 0 &&
                currentGame.player2Response[0] != 0
            ) {
                currentGame.winner = currentGame.player2;
                transferRewards(_gameId, userAddresses[currentGame.player2]);
            } else if (
                currentGame.player1Response[0] != 0 &&
                currentGame.player2Response[0] == 0
            ) {
                currentGame.winner = currentGame.player1;
                transferRewards(_gameId, userAddresses[currentGame.player1]);
            }
        }
    }

