    function shuffleCards(
        uint256 _gameId,
        uint256 _playerId,
        uint256[] memory _cardIndexes,
        uint256[] memory _newCardIndexes
    ) external isEligible(1) {
        uint256 allowedCardMoves = getNoOfSpecialMoves();
        uint256 arrayLength = _cardIndexes.length;
        require(
            arrayLength <= allowedCardMoves,
            "array size exceeds allowedCardMoves"
        );
        require(
            arrayLength == _newCardIndexes.length,
            "array sizes don't match"
        );
        Game storage currentGame = allGames[_gameId];
        uint256[] memory playerCards = getMyCards(_gameId);
        uint256 temp;
        for (uint256 i = 0; i < arrayLength; i++) {
            temp = playerCards[_cardIndexes[i]];
            playerCards[_cardIndexes[i]] = playerCards[_newCardIndexes[i]];
            playerCards[_newCardIndexes[i]] = temp;
        }
        if (_playerId == currentGame.player1) {
            currentGame.player1Cards = playerCards;
        } else if (_playerId == currentGame.player2) {
            currentGame.player1Cards = playerCards;
        }
        emit CardsShuffled(_gameId, _playerId, playerCards, block.timestamp);
    }

    function replaceCards(
        uint256 _gameId,
        uint256[] memory _cardIndexesToReplace
    ) external isEligible(2) {
        uint256 allowedCardMoves = getNoOfSpecialMoves();
        uint256 arrayLength = _cardIndexesToReplace.length;
        require(
            arrayLength <= allowedCardMoves,
            "array size exceeds allowedCardMoves"
        );
        Game storage currentGame = allGames[_gameId];
        uint256[] memory playerCards = getMyCards(_gameId);
        uint256 playerId = addressToPlayerId[msg.sender];
        uint256 randomCard;
        for (uint256 i = 0; i < arrayLength; i++) {
            randomCard = gameCardContract.getUniquePlayer(playerCards);
            playerCards[_cardIndexesToReplace[i]] = randomCard;
        }
        if (playerId == currentGame.player1) {
            currentGame.player1Cards = playerCards;
        } else if (playerId == currentGame.player2) {
            currentGame.player2Cards = playerCards;
        }
        emit CardsReplaced(_gameId, playerId, playerCards, block.timestamp);
    }

    function checkRivalCards(uint256 _gameId) external isEligible(4) {
        uint256 currentPlayerId = addressToPlayerId[msg.sender];
        canCheckRivalCards[_gameId] = currentPlayerId;
        emit RivalCardsChecked(_gameId, currentPlayerId, block.timestamp);
    }

    function viewRivalCards(
        uint256 _gameId
    ) external view returns (uint256[] memory) {
        Game storage currentGame = allGames[_gameId];
        uint256 currentPlayerId = addressToPlayerId[msg.sender];
        require(
            currentGame.matchInComplete == false,
            "Match incomplete, please try after sometime"
        );
        require(
            canCheckRivalCards[_gameId] == currentPlayerId,
            "restricted operation"
        );
        uint256[] memory rivalCards = currentGame.player1 == currentPlayerId
            ? currentGame.player2Cards
            : currentGame.player1Cards;
        return rivalCards;
    }
