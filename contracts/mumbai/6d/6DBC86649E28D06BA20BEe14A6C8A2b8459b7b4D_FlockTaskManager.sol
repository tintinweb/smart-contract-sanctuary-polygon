// SPDX-License-Identifier: UNLICENSED
pragma solidity >=0.8.13;

import "FlockToken.sol";
import "FlockTask.sol";

contract FlockTaskManager {
    FlockToken public flockToken;
    VRFCoordinatorV2Interface COORDINATOR;
    address public coordinatorAddress;
    uint64 public subscriptionId;

    address public flockTokenAddress;

    address[] public tasks;

    constructor(address _flockTokenAddress, address _coordinatorAddress) {
        flockTokenAddress = _flockTokenAddress;
        coordinatorAddress = _coordinatorAddress;
        COORDINATOR = VRFCoordinatorV2Interface(coordinatorAddress);
        flockToken = FlockToken(_flockTokenAddress);

        createNewSubscription();
    }

    // Create a new subscription when the contract is initially deployed.
    function createNewSubscription() private {
        subscriptionId = COORDINATOR.createSubscription();
    }

    // Add a consumer contract to the subscription.
    function addConsumer(address consumerAddress) private {
        COORDINATOR.addConsumer(subscriptionId, consumerAddress);
    }

    function createTask(
        string memory _metadata,
        uint256 _secondsPerRound,
        string memory _modelDefinitionHash,
        uint256 _totalNumberOfRounds,
        uint256 _minNumberOfParticipants,
        uint256 _maxNumberOfParticipants,
        uint256 _minStakeThreshold,
        uint256 _initialRewardPoolSize
    ) public returns (address newContract) {
        FlockTask flock = new FlockTask(
            flockTokenAddress,
            subscriptionId,
            coordinatorAddress,
            _metadata,
            _secondsPerRound,
            _modelDefinitionHash,
            _totalNumberOfRounds,
            _minNumberOfParticipants,
            _maxNumberOfParticipants,
            _minStakeThreshold
        );
        flockToken.transferFrom(
            msg.sender,
            address(this),
            _initialRewardPoolSize
        );

        flockToken.approve(address(flock), _initialRewardPoolSize);

        flock.initializeRewardPool(_initialRewardPoolSize);
        address deployedContractAddress = address(flock);
        addConsumer(deployedContractAddress);

        tasks.push(deployedContractAddress);
        return deployedContractAddress;
    }

    function getTasks() public view returns (address[] memory) {
        return tasks;
    }
}

// SPDX-License-Identifier: UNLICENSED
pragma solidity >=0.8.13;

import "ERC20.sol";
import "Ownable.sol";

/**
 * @author Murat Saglam, Rordigo Pavezi, Ben Dumoulin
 * @title The FLOCK Token used to incentivise contributions to the FL Models
 *        and to pay for FL Model preidctions.
 */

contract FlockToken is ERC20, Ownable {
    uint256 private maxSupply = 11000000 * 1e18;

    constructor() ERC20("FLOCK", "FLO") {
        _mint(msg.sender, 1000000 * 1e18);
    }

    function mint(address _account, uint256 _amount) public onlyOwner {
        require(totalSupply() + _amount <= maxSupply, "Minting would exceed maxSupply.");
        _mint(_account, _amount);
    }

    function burn(address _account, uint256 _amount) public onlyOwner {
        _burn(_account, _amount);
    }
}

