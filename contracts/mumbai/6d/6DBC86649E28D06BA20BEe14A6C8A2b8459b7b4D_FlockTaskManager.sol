import "VRFCoordinatorV2Interface.sol";

/**
 * @author Murat Saglam, Rordigo Pavezi, Ben Dumoulin
 * @title FlockTask implements a more decentralised and scalable Tokenomics
 * model to incentivize all roles to call the necessary functions, without
 * relying solely on an Admin.
 */

contract FlockTask is VRFConsumerBaseV2, Ownable {
    VRFCoordinatorV2Interface COORDINATOR;
    bytes32 keyHash =
        0x474e34a077df58807dbe9c96d3c009b23b3c6d0cce433e59bbf5b34f823bc56c;
    uint32 callbackGasLimit = 10000000;
    uint16 requestConfirmations = 3;

    struct RequestStatus {
        bool fulfilled; // whether the request has been successfully fulfilled
        bool exists; // whether a requestId exists
        uint256[] randomWords;
    }
    mapping(uint256 => mapping(uint256 => RequestStatus))
        public s_requests; /* round --> requestId --> requestStatus */

    // Your subscription ID.
    uint64 subscriptionId;

    string public metadata;

    // past requests Id.
    uint256[] public requestIds;
    uint256 public lastRequestId;

    FlockToken public flockToken;
    uint256 public secondsPerRound = 180 seconds;

    struct Vote {
        address voterAddress;
        string voteHash;
        int256 voteScore;
    }

    struct RoundResult {
        string popularHash;
        int256 voteScore;
        string nextHash;
    }

    uint256 public maxNumberOfParticipants = 30;
    uint256 public minNumberOfParticipants = 4;
    uint256 public participantsDistributionRate = 5000; // 50%

    uint256 public minStakeThreshold = 1000 * 10 ** 18;

    uint256 public rewardPool = 0;
    uint256 public totalStakedTokens = 0;
    mapping(uint256 => uint256) public roundTotalStakedTokens;

    uint256 public rewardPerStake = 5 * 10 ** 16; // default reward is 5% of staked amount
    uint256 public slashedPerStake = 50 * 10 ** 16; // default slashing is 50% of staked amount

    uint256 public totalNumberOfRounds = 0;
    uint256 public currentRound = 0;
    uint256 public alreadySlashedRounds = 0;

    string public modelDefinitionHash = "";
    mapping(address => uint256) public alreadyRewardedRounds;
    mapping(uint256 => uint256) public roundDeadline;

    mapping(uint256 => mapping(address => Vote)) public votes;
    mapping(uint256 => address[]) public voters;
    mapping(uint256 => mapping(string => uint256)) public votesHashCount;
    mapping(uint256 => mapping(string => int256)) public hashScore;
    mapping(uint256 => int256) public totalScorePerRound;

    //An array to store proposers and voters for current round
    mapping(uint256 => address[]) public participants;
    mapping(uint256 => mapping(address => bool)) public addedParticipants;

    // Define a mapping to store the balances of each address
    mapping(address => uint256) public balances;

    //Mapping of contirbutions from proposers for an round
    mapping(uint256 => mapping(address => string)) public contributions;

    //Mapping of the global model for each epoch
    mapping(uint256 => string) public globalModelHash;

    // Mapping of staked tokens to proposer addresses
    mapping(address => uint256) public stakedTokens;
    mapping(uint256 => mapping(address => uint256)) public roundStakedTokens;

    mapping(uint256 => RoundResult) public roundResult; // log winning votes

    mapping(uint256 => address[]) public selectedProposers;

    mapping(uint256 => address[]) public selectedVoters;

    mapping(uint256 => mapping(address => uint)) public participantRoles;

    enum Role {
        NONE,
        VOTER,
        PROPOSER
    }

    enum Status {
        STAKE_REQUIRED,
        CLAIM_AVAILABLE,
        CONTRIBUTE,
        VOTE,
        WAIT
    }

    // Event for staking tokens
    event Staked(address proposer, uint256 amount);

    // Event for rewarding tokens for voters and proposers
    event Rewarded(
        address contributor,
        uint256 amount,
        uint256 round,
        string contributorType
    );

    // Event for slashing tokens for voters and proposers
    event Slashed(
        address slasher,
        address contributor,
        uint256 amount,
        uint256 round,
        string contributorType
    );

    event ChangeInStake(address[] participant, uint256 amount, string reason);

    event RoundFinished(
        uint256 resultRound,
        address[] resultProposers,
        address[] resultVoters
    );

    event RequestSent(uint256 requestId, uint32 numWords);
    event RequestFulfilled(uint256 requestId, uint256[] randomWords);

    constructor(
        address _flockTokenAddress,
        uint64 _subscriptionId,
        address _coordinatorAddress,
        string memory _metadata,
        uint256 _secondsPerRound,
        string memory _modelDefinitionHash,
        uint256 _totalNumberOfRounds,
        uint256 _minNumberOfParticipants,
        uint256 _maxNumberOfParticipants,
        uint256 _minStakeThreshold
    ) VRFConsumerBaseV2(_coordinatorAddress) {
        flockToken = FlockToken(_flockTokenAddress);
        COORDINATOR = VRFCoordinatorV2Interface(_coordinatorAddress);
        subscriptionId = _subscriptionId;

        secondsPerRound = _secondsPerRound;
        modelDefinitionHash = _modelDefinitionHash;

        totalNumberOfRounds = _totalNumberOfRounds;

        metadata = _metadata;
        minNumberOfParticipants = _minNumberOfParticipants;
        maxNumberOfParticipants = _maxNumberOfParticipants;
        minStakeThreshold = _minStakeThreshold;
    }

    function initializeRewardPool(uint256 _initialRewardPoolSize) public {
        require(rewardPool == 0, "Reward pool has been already initialized");

        rewardPool = _initialRewardPoolSize;
        flockToken.transferFrom(
            msg.sender,
            address(this),
            _initialRewardPoolSize
        );
    }

    function allContributed(uint256 _round) external returns (bool) {
        require(!hasRoundFinished(_round), "Round has already finished!");
        for (uint256 i = 0; i < selectedProposers[currentRound].length; i++) {
            address proposer = selectedProposers[currentRound][i];
            bool contributed = stringsAreDiff(
                contributions[currentRound][proposer],
                ""
            );
            if (!contributed) {
                return false;
            }
        }
        return true;
    }

    function enoughParticipantsJoined() public view returns (bool) {
        return participants[currentRound].length >= minNumberOfParticipants;
    }

    function getNumberOfProposers() public view returns (uint256) {
        return selectedProposers[currentRound].length;
    }

    function getNumberOfParticipants() public view returns (uint256) {
        return participants[currentRound].length;
    }

    function hasRoundFinished(uint _round) public view returns (bool) {
        if (roundDeadline[_round] == 0) {
            return false;
        }
        if (block.timestamp > roundDeadline[_round]) {
            return true;
        }
        if (
            selectedVoters[_round].length > 0 &&
            voters[_round].length == selectedVoters[_round].length
        ) {
            return true;
        }
        return false;
    }

    function finishRound() internal {
        string memory popularHash = mostPopularHash(currentRound);
        string memory nextHash = globalModelHash[currentRound];
        int256 popularHashScore = hashScore[currentRound][popularHash];

        if (popularHashScore > 0) {
            nextHash = popularHash;
        }
        roundResult[currentRound] = RoundResult({
            popularHash: popularHash,
            voteScore: popularHashScore,
            nextHash: nextHash
        });
        emit RoundFinished(
            currentRound,
            selectedProposers[currentRound],
            selectedVoters[currentRound]
        );
        currentRound++;
        globalModelHash[currentRound] = nextHash;
    }

    modifier updateRound() {
        if (hasRoundFinished(currentRound)) {
            finishRound();
        }
        _;
    }

    // Depends on the number of requested values that you want sent to the
    // fulfillRandomWords() function. Storing each word costs about 20,000 gas,
    // so 100,000 is a safe default for numWords = 2. Test and adjust
    // this limit based on the network that you select, the size of the request,
    // and the processing of the callback request in the fulfillRandomWords()
    // function.
    function setGasLimit(uint32 _callbackGasLimit) external onlyOwner {
        callbackGasLimit = _callbackGasLimit;
    }

    function setStakeThreshold(uint256 _stakeThreshold) public onlyOwner {
        minStakeThreshold = _stakeThreshold;
    }

    function setSecondsPerRound(uint256 _secondsPerRound) public onlyOwner {
        secondsPerRound = _secondsPerRound;
    }

    function setMaxNumberOfParticipants(
        uint256 _numberOfParticipants
    ) public onlyOwner {
        require(
            _numberOfParticipants > 0,
            "Number of participants must be greater than 0"
        );
        maxNumberOfParticipants = _numberOfParticipants;
    }

    function setMinNumberOfParticipants(
        uint256 _numberOfParticipants
    ) public onlyOwner {
        require(
            _numberOfParticipants > 0,
            "Number of proposers must be greater than 0"
        );
        minNumberOfParticipants = _numberOfParticipants;
    }

    function joinRound() public updateRound {
        require(
            stakedTokens[msg.sender] >= minStakeThreshold,
            "You don't have enough tokens staked."
        );
        require(roundDeadline[currentRound] == 0, "Round has already started!");
        require(
            participants[currentRound].length <= maxNumberOfParticipants,
            "Max number of participants has been reached"
        );
        require(
            roundStakedTokens[currentRound][msg.sender] == 0,
            "Participant already added"
        );
        participants[currentRound].push(msg.sender);
        roundStakedTokens[currentRound][msg.sender] = stakedTokens[msg.sender];
        roundTotalStakedTokens[currentRound] += stakedTokens[msg.sender];
    }

    // Used for testing only
    function seedRoundParticipants(uint256 _random) external updateRound {
        require(
            participants[currentRound].length >= minNumberOfParticipants,
            "Minimum number of participants is required"
        );
        require(roundDeadline[currentRound] == 0, "Round has already started!");
        // Shuffle Array of participants
        for (uint256 i = 0; i < participants[currentRound].length; i++) {
            uint256 n = i + (_random % (participants[currentRound].length - i));
            address temp = participants[currentRound][n];
            participants[currentRound][n] = participants[currentRound][i];
            participants[currentRound][i] = temp;
        }
        // calculates the number of voters based on percentage
        uint256 numberOfVoters = (participants[currentRound].length *
            participantsDistributionRate) / 10_000;

        uint256 pickedVoters = 0;
        for (uint256 j = 0; j < participants[currentRound].length; j++) {
            address participant = participants[currentRound][j];

            if (pickedVoters < numberOfVoters) {
                participantRoles[currentRound][participant] = uint(Role.VOTER);
                selectedVoters[currentRound].push(participant);
                pickedVoters++;
            } else {
                participantRoles[currentRound][participant] = uint(
                    Role.PROPOSER
                );
                selectedProposers[currentRound].push(participant);
            }
        }

        roundDeadline[currentRound] = secondsPerRound + block.timestamp;
    }

    function selectRoundParticipants()
        external
        updateRound
        returns (uint256 requestId)
    {
        require(
            !s_requests[currentRound][lastRequestId].exists,
            "Last request has not been fufilled"
        );

        require(
            participants[currentRound].length >= minNumberOfParticipants,
            "Minimum number of participants is required"
        );

        uint32 numWords = 1;

        // Will revert if subscription is not set and funded.
        requestId = COORDINATOR.requestRandomWords(
            keyHash,
            subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );
        s_requests[currentRound][requestId] = RequestStatus({
            randomWords: new uint256[](0),
            exists: true,
            fulfilled: false
        });
        requestIds.push(requestId);
        lastRequestId = requestId;
        emit RequestSent(requestId, numWords);
        return requestId;
        roundDeadline[currentRound] = secondsPerRound + block.timestamp;
    }

    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory _randomWords
    ) internal override updateRound {
        require(
            s_requests[currentRound][_requestId].exists,
            "request not found"
        );
        s_requests[currentRound][_requestId].fulfilled = true;

        // Shuffle Array of participants
        for (uint256 i = 0; i < participants[currentRound].length; i++) {
            uint256 n = i +
                (_randomWords[0] % (participants[currentRound].length - i));
            address temp = participants[currentRound][n];
            participants[currentRound][n] = participants[currentRound][i];
            participants[currentRound][i] = temp;
        }

        // calculates the number of voters based on percentage
        uint256 numberOfVoters = (participants[currentRound].length *
            participantsDistributionRate) / 10_000;

        uint256 pickedVoters = 0;
        for (uint256 j = 0; j < participants[currentRound].length; j++) {
            address participant = participants[currentRound][j];

            if (pickedVoters < numberOfVoters) {
                participantRoles[currentRound][participant] = uint(Role.VOTER);
                selectedVoters[currentRound].push(participant);
                pickedVoters++;
            } else {
                participantRoles[currentRound][participant] = uint(
                    Role.PROPOSER
                );
                selectedProposers[currentRound].push(participant);
            }
        }

        s_requests[currentRound][_requestId].randomWords = _randomWords;

        roundDeadline[currentRound] = secondsPerRound + block.timestamp;

        emit RequestFulfilled(_requestId, _randomWords);
    }

    /// @notice function to stake tokens
    /// @dev has a unique modifier allowing both roles to stake tokens
    /// @param amount The amount of tokens to be staked
    function stake(uint256 amount) external {
        require(
            amount >= minStakeThreshold,
            "Amount is below staking threshold."
        );
        require(
            flockToken.balanceOf(msg.sender) >= amount,
            "you have insufficient funds"
        );

        // msg.sender needs to have apprved this transfer via the ERC20 approve() function
        totalStakedTokens += amount;
        stakedTokens[msg.sender] += amount;
        flockToken.transferFrom(msg.sender, address(this), amount);

        emit Staked(msg.sender, amount);
    }

    /// @notice function to call when a proposer will be contributing to the proposal
    /// @param _hash Hash to the contribution stored on IPFS
    function contribute(string memory _hash) public updateRound {
        require(
            participantRoles[currentRound][msg.sender] == uint(Role.PROPOSER),
            "You are not a proposer"
        );
        contributions[currentRound][msg.sender] = _hash;
    }

    function canBeSlashed(
        address _address,
        uint256 _round
    ) public returns (bool) {
        bool votedIncorrectHash = stringsAreDiff(
            votes[_round][_address].voteHash,
            roundResult[_round].popularHash
        );

        // If they did not vote (score) on the side of majority, votedIncorrectScore is true and they can be slashed
        int256 votedScore = votes[_round][_address].voteScore;
        int256 majorityScore = totalScorePerRound[_round];
        bool votedIncorrectScore = (majorityScore > 0 && votedScore < 0) ||
            (majorityScore < 0 && votedScore > 0);

        return
            (stakedTokens[_address] > 0) &&
            (votedIncorrectHash || votedIncorrectScore);
    }

    function stringsAreDiff(
        string memory a,
        string memory b
    ) public pure returns (bool) {
        return keccak256(abi.encodePacked(a)) != keccak256(abi.encodePacked(b));
    }

    /// @notice a function to slash a proposer's stake
    /// @dev caller of slash function gets 10% of stake as reward. 90% of stake is burned.
    function slash() external updateRound {
        require(currentRound > 0, "No previous round");

        for (; alreadySlashedRounds < currentRound; alreadySlashedRounds++) {
            bool totalScorePerRoundNegative = totalScorePerRound[
                alreadySlashedRounds
            ] < 0;

            uint256 totalSlashedAmount;
            for (
                uint256 j = 0;
                j < selectedProposers[alreadySlashedRounds].length;
                j++
            ) {
                address proposerAddress = selectedProposers[
                    alreadySlashedRounds
                ][j];
                bool proposerContributed = stringsAreDiff(
                    contributions[alreadySlashedRounds][proposerAddress],
                    ""
                );

                if (!proposerContributed || totalScorePerRoundNegative) {
                    uint256 slashedAmount = (roundStakedTokens[
                        alreadySlashedRounds
                    ][proposerAddress] * slashedPerStake) / 10 ** 18;
                    stakedTokens[proposerAddress] -= slashedAmount;
                    totalStakedTokens -= slashedAmount;

                    emit Slashed(
                        msg.sender,
                        proposerAddress,
                        slashedAmount,
                        alreadySlashedRounds,
                        "proposer"
                    );

                    totalSlashedAmount += slashedAmount;
                }
            }

            for (
                uint256 w = 0;
                w < selectedVoters[alreadySlashedRounds].length;
                w++
            ) {
                address voterAddress = selectedVoters[alreadySlashedRounds][w];
                bool voterCanBeSlashed = canBeSlashed(
                    voterAddress,
                    alreadySlashedRounds
                );

                if (voterCanBeSlashed) {
                    uint256 slashedAmount = (roundStakedTokens[
                        alreadySlashedRounds
                    ][voterAddress] * slashedPerStake) / 10 ** 18;
                    stakedTokens[voterAddress] -= slashedAmount;
                    totalStakedTokens -= slashedAmount;

                    emit Slashed(
                        msg.sender,
                        voterAddress,
                        slashedAmount,
                        alreadySlashedRounds,
                        "voter"
                    );
                    totalSlashedAmount += slashedAmount;
                }
            }

            // send 10% of slashed tokens to the caller as reward
            // they can withdraw it from staked tokens
            uint256 slashReward = totalSlashedAmount / 10;
            stakedTokens[msg.sender] += slashReward;

            rewardPool += (totalSlashedAmount - slashReward);
            totalStakedTokens += slashReward;

            emit Rewarded(msg.sender, slashReward, currentRound, "slasher");
        }
    }

    function mulDiv(uint x, uint y, uint z) public pure returns (uint) {
        return
            ABDKMathQuad.toUInt(
                ABDKMathQuad.div(
                    ABDKMathQuad.mul(
                        ABDKMathQuad.fromUInt(x),
                        ABDKMathQuad.fromUInt(y)
                    ),
                    ABDKMathQuad.fromUInt(z)
                )
            );
    }

    /// @notice Allows a proposer or voter claim reward for their contribution
    function claimReward() external updateRound {
        require(stakedTokens[msg.sender] > 0, "You have no stake");
        require(currentRound > 0, "No previous round");
        require(
            alreadySlashedRounds > currentRound - 1,
            "Slash for the rounds have not happened"
        );

        for (
            uint256 i = alreadyRewardedRounds[msg.sender];
            i < currentRound;
            i++
        ) {
            // reward available if they contributed to i
            bool proposerContributed = stringsAreDiff(
                contributions[i][msg.sender],
                ""
            );
            bool totalScorePerRoundPositive = totalScorePerRound[i] > 0;

            // reward available if they voted correctly in _prevRound
            bool voterCanBeSlashed = canBeSlashed(msg.sender, i);

            bool isVoter = votes[i][msg.sender].voterAddress != address(0);

            string memory contributorType;
            if (isVoter) {
                contributorType = "voter";

                if (voterCanBeSlashed || !totalScorePerRoundPositive) {
                    continue;
                }
            } else {
                contributorType = "proposer";

                if (!proposerContributed || !totalScorePerRoundPositive) {
                    continue;
                }
            }

            uint256 scale = 10 ** 18;
            uint256 roundsLeft = totalNumberOfRounds - i;
            uint256 roundRewardAmount = rewardPool / roundsLeft;
            uint256 proportion = mulDiv(
                roundStakedTokens[i][msg.sender] * scale,
                100,
                roundTotalStakedTokens[i]
            );
            uint256 rewardAmount = mulDiv(
                roundRewardAmount,
                proportion,
                100 * scale
            );

            stakedTokens[msg.sender] += rewardAmount;
            totalStakedTokens += rewardAmount;

            emit Rewarded(msg.sender, rewardAmount, i, contributorType);
        }

        alreadyRewardedRounds[msg.sender] = currentRound;
    }

    /// @notice a function for voters to vote on the global model
    /// @dev requires their stake to be greater than 0 and saves it to a mapping
    function vote(string memory _hash, int8 _score) external updateRound {
        require(
            participantRoles[currentRound][msg.sender] == uint(Role.VOTER),
            "You are not a voter"
        );
        require(_score >= -1 && _score <= 1, "Invalid score");
        require(
            votes[currentRound][msg.sender].voterAddress == address(0),
            "You have already voted this round."
        );

        Vote memory newVote = Vote({
            voterAddress: msg.sender,
            voteHash: _hash,
            voteScore: _score
        });

        votes[currentRound][msg.sender] = newVote;
        voters[currentRound].push(msg.sender);

        votesHashCount[currentRound][_hash]++;
        hashScore[currentRound][_hash] += _score;
        totalScorePerRound[currentRound] += _score;
    }

    function mostPopularHash(
        uint256 _round
    ) public view returns (string memory) {
        string memory popularHash = "";
        uint256 maxVoteCount = 0;

        // loop through votes and find the highest vote count
        for (uint256 i = 0; i < voters[_round].length; i++) {
            address _voter = voters[_round][i];
            string memory _voteHash = votes[_round][_voter].voteHash;
            uint256 voteCount = votesHashCount[_round][_voteHash];

            if (voteCount > maxVoteCount) {
                maxVoteCount = voteCount;
                popularHash = _voteHash;
            }
        }
        return popularHash;
    }
}

