        _;
    }

    function editionContractInit(string calldata uri_) external initializer {
        name = "Sample Contract Name";
        _contractURI = "https://bafybeidy3766zmyaz5szqr7q5mzeivlq5af4ve5gtrvi7o2ep2slu5o4ce.ipfs.w3s.link/sample_json.json";
        __UUPSOwnable_init();
        __ERC1155PermitUpgradeable_init(uri_);
