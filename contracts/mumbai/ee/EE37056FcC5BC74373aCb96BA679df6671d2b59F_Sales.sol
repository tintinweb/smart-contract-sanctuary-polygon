pragma solidity ^0.8.17;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import { ECDSA } from "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

contract Sales is Ownable {
    IERC20 public token;
    mapping(address => bool) public isAllowedToWithdraw;
    uint256 private constant SIGNATURE_LENGTH = 65;
    address private _signer;

    // events
    event Purchase(address indexed from, address indexed to, uint256 payment_amount, uint256 purchase_amount);
    event PurchaseWithdraw(address indexed from, address indexed to, uint256 amount);
    event SignerUpdate(address indexed oldSigner, address indexed newSigner);

    constructor(address initialSigner, address _token) {
        token = IERC20(_token);
        require(initialSigner != address(0), "Signer is zero address.");
        _signer = initialSigner;
    }
    
    function purchase(
        uint256 paymentAmount,
        uint256 purchaseAmount,
        uint256 expiry,
        bytes memory signature
    ) external {
        // veryfy signature
        require(verifySignature(paymentAmount, purchaseAmount, expiry, signature), "Invalid signature");
        
        // Check the expiry time
        require(block.timestamp <= expiry, "Signature has expired");

        // Transfer tokens
        token.transferFrom(msg.sender, address(this), paymentAmount);

        emit Purchase(msg.sender, address(this), paymentAmount, purchaseAmount);
    }

    function allowWithdrawal(address _addr) external onlyOwner {
        isAllowedToWithdraw[_addr] = true;
    }

    function disallowWithdrawal(address _addr) external onlyOwner {
        isAllowedToWithdraw[_addr] = false;
    }
    
    function purchaseWithdraw(address to, uint amount) external {
        require(isAllowedToWithdraw[msg.sender], "you are not allowed to withdraw");
        token.transfer(to, amount);
        
        emit PurchaseWithdraw(address(this), to, amount);
    }

    function verifySignature(
        uint256 paymentAmount,
        uint256 purchaseAmount,
        uint256 expiry,
        bytes memory signature
    ) public view returns (bool) {
        require(signature.length % SIGNATURE_LENGTH == 0, "Invalid signature length");

        uint256 chainid = block.chainid;
        address contractAddress = address(this);
        address fromAddress = msg.sender;

        address recovered = _recoverSigner(
            chainid,
            contractAddress,
            fromAddress,
            paymentAmount,
            purchaseAmount,
            expiry,
            signature
        );
        // console.log("recovered: %s", recovered);
        return (recovered == _signer);
    }

    function _recoverSigner(
        uint256 chainid,
        address contractAddress,
        address fromAddress,
        uint256 paymentAmount,
        uint256 purchaseAmount,
        uint256 expiry,
        bytes memory signature
    ) private pure returns (address) {
        bytes32 hash = keccak256(
            abi.encodePacked(
                "\x19Ethereum Signed Message:\n32",
                keccak256(abi.encodePacked(chainid, contractAddress, fromAddress, paymentAmount, purchaseAmount, expiry))
            )
        );

        (address recovered, ) = ECDSA.tryRecover(
            hash,
            signature
        );
        return recovered;
    }

    function updateSigner(address newSigner) external onlyOwner {
        require(newSigner != address(0), "New signer is zero address.");
        require(_signer != newSigner, "New signer is the same as the current signer.");

        address oldSigner = _signer;
        _signer = newSigner;

        emit SignerUpdate(oldSigner, newSigner);
    }

    function bytes32ToBytes(bytes32 b_) private pure returns (bytes memory){
        return abi.encodePacked(b_);
    }
}

