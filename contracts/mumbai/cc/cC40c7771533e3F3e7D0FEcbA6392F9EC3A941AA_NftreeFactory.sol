        // Adding the nftree to our mapping of nftree addresses
        treesBatch[batchSupply].nftree = nftree;
        treesBatch[batchSupply].staking = staking;
        treesBatch[batchSupply].ecoEmpire = ecoEmpire;
        treesBatch[batchSupply].backup = backup;
    }

    function getNftreeClones() public view returns (BatchTrees[] memory) {
        BatchTrees[] memory nftreebatches = new BatchTrees[](batchSupply);
        for(uint256 i = 0; i < batchSupply; i++) {
            nftreebatches[i] = ( treesBatch[i+1]);
        }

