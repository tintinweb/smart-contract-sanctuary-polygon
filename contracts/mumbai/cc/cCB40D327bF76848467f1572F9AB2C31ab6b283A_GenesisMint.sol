pragma solidity ^0.8.4;

import "./utils/Types.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";

contract Horse is
    Initializable,
    ERC721Upgradeable,
    ERC721EnumerableUpgradeable,
    ERC721URIStorageUpgradeable,
    PausableUpgradeable,
    AccessControlUpgradeable,
    ERC721BurnableUpgradeable
{
    using CountersUpgradeable for CountersUpgradeable.Counter;

    uint256 public genesisHorseCounter;
    CountersUpgradeable.Counter private _tokenIdCounter;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant ORACLE_ROLE = keccak256("ORACLE_ROLE");

    error GenesisHorseLimitReached();
    error InvalidOwnerAddress();

    event CreateHorse(string tempId, uint256 horseId);

    mapping(uint256 => Types.Horse) public horses;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize() public initializer {
        __ERC721_init("Mythera Horse", "MYTHERA_HORSE");
        __ERC721Enumerable_init();
        __ERC721URIStorage_init();
        __Pausable_init();
        __AccessControl_init();
        __ERC721Burnable_init();

        genesisHorseCounter = 32000;

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://mythic.run/horse/";
    }

    /**
     * @notice Mint a new genesis horse
     * @param owner The wallet that will receive the minted horse
     * @param tempId A temporary horseId, used by the oracle
     * @return horseId
     */
    function mintGenesisHorse(
        address owner,
        string memory tempId,
        Types.Horse memory horseData
    ) public onlyRole(ORACLE_ROLE) whenNotPaused returns (uint256) {
        // Maximum amount of genesis horses reached
        if (genesisHorseCounter <= 0) revert GenesisHorseLimitReached();

        // Reduce genesis horse count
        genesisHorseCounter--;

        // Mint new horse token
        uint256 horseId = mintHorse(owner, tempId);

        // Record the horse data
        storeHorse(horseId, horseData);

        return horseId;
    }

    /**
     * @notice Mint a new offspring horse
     * @param owner The wallet that will receive the minted horse
     * @param tempId A temporary horseId, used by the oracle
     */
    function mintOffspringHorse(
        address owner,
        string memory tempId,
        Types.Horse memory horseData
    ) public onlyRole(ORACLE_ROLE) whenNotPaused returns (uint256) {
        uint256 horseId = mintHorse(owner, tempId);

        // Record the horse data
        storeHorse(horseId, horseData);

        return horseId;
    }

    /**
     * @notice Mint a new tokenId that's assigned to a horse
     * @param owner The wallet that will receive the minted horse token
     * @param tempId The temporary ID assigned to the horse by oracle
     * @return horseId
     */
    function mintHorse(address owner, string memory tempId)
        public
        onlyRole(ORACLE_ROLE)
        whenNotPaused
        returns (uint256)
    {
        _tokenIdCounter.increment();

        uint256 tokenId = _tokenIdCounter.current();

        _safeMint(owner, tokenId);
        _setTokenURI(
            tokenId,
            string(abi.encodePacked(Strings.toString(tokenId), ".json"))
        );

        // Horse is ready, minting oracle will update the horse's tempId the tokenId
        emit CreateHorse(tempId, tokenId);

        return tokenId;
    }

    /**
     * @notice Write horse data to the blockchain.
     * @param horseId The unique horseId
     * @param horseData The horse data to be stored on chain
     */
    function storeHorse(uint256 horseId, Types.Horse memory horseData)
        public
        onlyRole(ORACLE_ROLE)
    {
        horses[horseId] = horseData;
    }

    /**
     * @notice Retrieve information on a horse using the horseId
     * @param horseId The horseId to fetch information about
     * @return Types.Horse
     */
    function getHorse(uint256 horseId)
        public
        view
        returns (Types.Horse memory)
    {
        return horses[horseId];
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    )
        internal
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
        whenNotPaused
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _burn(uint256 tokenId)
        internal
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
    {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(
            ERC721Upgradeable,
            ERC721EnumerableUpgradeable,
            AccessControlUpgradeable
        )
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Capped.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Snapshot.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

contract Coin is
    ERC20,
    ERC20Capped,
    ERC20Burnable,
    ERC20Snapshot,
    AccessControl,
    Pausable
{
    address private bank;
    uint256 private bankMintingShare;

    bytes32 public constant SNAPSHOT_ROLE = keccak256("SNAPSHOT_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    error BankAddressRequired();
    error BankMintingShareOutOfBounds(); // Bank minting share must be between 0 and 100

    event BankAddressUpdated(address bankAddress);
    event BankMintingShareUpdated(uint256 bankMintingShare);

    constructor()
        ERC20("Horsie Coin", "HRSE")
        ERC20Capped(1000000000 * 10**decimals())
    {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(SNAPSHOT_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);

        setBankMintingShare(50);
    }

    /**
     * @notice Fetch the current address of the bank
     */
    function getBank() external view returns (address) {
        return bank;
    }

    /**
     * @notice Change the current bank address
     * @param _bank The new address for the bank
     */
    function setBank(address _bank) external onlyRole(MINTER_ROLE) {
        if (_bank == address(0)) revert BankAddressRequired();
        bank = _bank;
        emit BankAddressUpdated(_bank);
    }

    /**
     * @notice Fetch the percent of a mint the bank will receive
     */
    function getBankMintingShare() public view returns (uint256) {
        return bankMintingShare;
    }

    /**
     * @notice Change the percent of a mint the bank will receive
     * @param _bankMintingShare Percent of mint to be sent to bank
     */
    function setBankMintingShare(uint256 _bankMintingShare)
        public
        onlyRole(MINTER_ROLE)
    {
        if (_bankMintingShare > 100) revert BankMintingShareOutOfBounds();
        bankMintingShare = _bankMintingShare;
        emit BankMintingShareUpdated(_bankMintingShare);
    }

    function snapshot() public onlyRole(SNAPSHOT_ROLE) {
        _snapshot();
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    /**
     * @notice Mint new tokens and distribute amount to account and the bank
     * @param account The account that will receive the tokens
     * @param amount The amount of tokens to mint
     * @dev Bank minting share is subtracted from amount of tokens minted
     */
    function mint(address account, uint256 amount)
        external
        onlyRole(MINTER_ROLE)
    {
        if (bank == address(0)) revert BankAddressRequired();

        uint256 bankAmount = (amount * getBankMintingShare()) / 100;
        uint256 accountAmount = amount - bankAmount;

        _mint(bank, bankAmount);
        _mint(account, accountAmount);
    }

    function _mint(address account, uint256 amount)
        internal
        override(ERC20, ERC20Capped)
    {
        super._mint(account, amount);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override(ERC20, ERC20Snapshot) whenNotPaused {
        super._beforeTokenTransfer(from, to, amount);
    }
}

