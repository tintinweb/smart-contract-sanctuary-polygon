pragma solidity ^0.8.4;

import "../Coin.sol";
import "../Horse.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

contract GenesisMint is Pausable, AccessControl {
    Coin public coinContract;
    Horse public horseContract;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

    error GenesisMintSoldOut();
    error InsufficientFunds();
    error InsufficientFundsApproved();

    event CreateGenesisHorse(address owner, uint256 price);

    uint256 public price = 100 * 10**18;
    uint256 public inventory = 0;

    /**
     * Construct new genesis mint instance.
     *
     * @param _coinContract - Address of the ERC20 token accepted
     * @param _horseContract - Address of the ERC721 Horse contract
     */
    constructor(Coin _coinContract, Horse _horseContract) {
        coinContract = _coinContract;
        horseContract = _horseContract;

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
    }

    /**
     * @notice Return current inventory value.
     * @return inventory
     */
    function getInventory() public view returns (uint256) {
        return inventory;
    }

    /**
     * @notice Update the current available inventory value.
     * @param _inventory - New inventory amount
     */
    function setInventory(uint256 _inventory)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        inventory = _inventory;
    }

    /**
     * @notice Purchase the minting of a new horse paid for with HRSE.
     */
    function buy() public whenNotPaused {
        require(
            coinContract.balanceOf(msg.sender) >= price,
            "Insufficient HRSE funds"
        );

        require(
            coinContract.allowance(msg.sender, address(this)) >= price,
            "The contract is not authorized to use the accepted token on sender behalf"
        );

        // Inventory empty, no new genesis horses are available
        if (inventory <= 0) revert GenesisMintSoldOut();

        // Transfer funds
        coinContract.transferFrom(msg.sender, coinContract.getBank(), price);

        // Update remaining inventory total
        inventory--;

        // Trigger horse mint
        emit CreateGenesisHorse(msg.sender, price);
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }
}

