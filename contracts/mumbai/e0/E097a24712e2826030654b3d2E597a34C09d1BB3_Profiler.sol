pragma solidity ^0.8.9;

// Uncomment this line to use console.log
import "hardhat/console.sol";
import "./RLNFT.sol";

contract Profiler {
    uint256 internal s_instructorId;
    uint256 s_mintfee = 0.00005 ether;
    struct userData {
        string firstname;
        string lastname;
        string username;
        string imageURI;
        bool exist;
    }

    struct Course {
        string courseName;
        string courseFile;
        string encryptedKey;
    }
    mapping(address => userData) _registeredStudent;

    string[] usernames;
    event newInstructor(
        string name,
        string profession,
        string briefIntro,
        string[3] links,
        string imageLink,
        uint256 instructorId
    );

    event instructionEdited(uint256 instructorId);
    event instructorDeleted(uint256 instructorId);
    event new_User(address user, string name, string username);
    event user_Edited(address user);
    event user_Deleted(address user);

    function add_Instructor(
        string memory name,
        string memory userId,
        string memory profession,
        string memory briefIntro,
        string[3] memory links,
        string memory imageURI,
        RLNFT _RLNFT
    ) public payable {
        require(msg.value == s_mintfee, "Incorrect amount sent sent");
        _RLNFT.MintNft(msg.sender, name, userId, profession, briefIntro, links, imageURI);
        emit newInstructor(name, profession, briefIntro, links, imageURI, s_instructorId);
    }

    function edit_InstructorProfile(
        string memory name,
        string memory userId,
        string memory profession,
        string memory briefIntro,
        string[3] memory links,
        string memory imageURI,
        uint256 tokenId,
        RLNFT _RLNFT
    ) public {
        _RLNFT.editNft(msg.sender, name, userId, profession, briefIntro, links, imageURI, tokenId);
    }

    function delete_InstructorProfile(uint256 tokenId, RLNFT _RLNFT) public {
        _RLNFT.deleteNft(msg.sender, tokenId);
    }

    function add_User(
        string memory firstname,
        string memory lastname,
        string memory username,
        string memory imageURI
    ) public {
        require(_registeredStudent[msg.sender].exist == false, "Address Registered");
        require(!isUsernameAvailable(username), "username already used");
        _registeredStudent[msg.sender] = userData({
            firstname: firstname,
            lastname: lastname,
            username: username,
            imageURI: imageURI,
            exist: true
        });

        usernames.push(username);
        emit new_User(msg.sender, string(abi.encodePacked(firstname, lastname)), username);
    }

    function edit_UserProfile(
        string memory firstname,
        string memory lastname,
        string memory username,
        string memory imageURI
    ) public {
        require(_registeredStudent[msg.sender].exist == true, "Address not registered");
        if (
            keccak256(abi.encodePacked(username)) !=
            keccak256(abi.encodePacked(_registeredStudent[msg.sender].username))
        ) {
            require(!isUsernameAvailable(username), "username already used");
            for (uint i = 0; i < usernames.length; i++) {
                if (
                    keccak256(abi.encodePacked(usernames[i])) ==
                    keccak256(abi.encodePacked(_registeredStudent[msg.sender].username))
                ) {
                    usernames[i] = username;

                    break;
                }
            }
        }
        _registeredStudent[msg.sender] = userData({
            firstname: firstname,
            lastname: lastname,
            username: username,
            imageURI: imageURI,
            exist: true
        });
    }

    function delete_UserProfile() public {
        require(_registeredStudent[msg.sender].exist == true, "Address not registered");
        delete _registeredStudent[msg.sender];
        for (uint i = 0; i < usernames.length; i++) {
            if (
                keccak256(abi.encodePacked(usernames[i])) ==
                keccak256(abi.encodePacked(_registeredStudent[msg.sender].username))
            ) {
                for (uint l = i; l < usernames.length - 1; l++) {
                    usernames[l] = usernames[l + 1];
                }
                usernames.pop();
                break;
            }
        }
        _registeredStudent[msg.sender].exist = false;
    }

    function AddCourse(string memory encryptedCourse, uint256 tokenId) public {}

    function registeredStudent(address _student) public view returns (userData memory) {
        return _registeredStudent[_student];
    }

    function isStudentRegistered(address _student) public view returns (bool) {
        return _registeredStudent[_student].exist;
    }

    function isUsernameAvailable(string memory username) public view returns (bool) {
        bool found = false;
        for (uint i = 0; i < usernames.length; i++) {
            if (
                keccak256(abi.encodePacked(usernames[i])) == keccak256(abi.encodePacked(username))
            ) {
                found = true;
                break;
            }
        }
        return found;
    }

    function sendSomfn(address receiver) public payable {
        (bool Success, ) = payable(receiver).call{value: msg.value}("");
        require(Success, "failed");
    }
}

