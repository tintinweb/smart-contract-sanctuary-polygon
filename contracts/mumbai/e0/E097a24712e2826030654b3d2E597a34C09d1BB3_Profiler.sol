pragma solidity ^0.8.7;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "base64-sol/base64.sol";

contract RLNFT is ERC721 {
    uint256 public s_tokenCounter;
    mapping(uint256 => string) IdtoURI;

    //mapping of addresses to Ids
    mapping(address => uint256[]) addressToIds;

    address internal immutable caller;

    constructor(address _caller) ERC721("RidLearnNFT", "RLN") {
        caller = _caller;
    }

    function MintNft(
        address newInstructor,
        string memory _name,
        string memory _userId,
        string memory profession,
        string memory briefIntro,
        string[3] memory links,
        string memory imageLink
    ) public returns (uint256) {
        require(msg.sender == caller, "cannot be called");
        uint256 currentId = s_tokenCounter;
        s_tokenCounter = s_tokenCounter + 1;
        addressToIds[newInstructor].push(currentId);
        _safeMint(newInstructor, currentId);
        URI(_name, _userId, profession, briefIntro, links, imageLink, currentId);
        return currentId;
    }

    function editNft(
        address newInstructor,
        string memory _name,
        string memory _userId,
        string memory profession,
        string memory briefIntro,
        string[3] memory links,
        string memory imageLink,
        uint256 tokenId
    ) public {
        require(msg.sender == caller, "cannot be called");
        require(_exists(tokenId), "Token does Not Exist");
        require(_ownerOf(tokenId) == newInstructor, "Not Authorized");
        URI(_name, _userId, profession, briefIntro, links, imageLink, tokenId);
    }

    function deleteNft(address _Instructor, uint256 tokenId) public {
        require(msg.sender == caller, "cannot be called");
        require(_exists(tokenId), "Token does Not Exist");
        require(_ownerOf(tokenId) == _Instructor, "Not Authorized");
        delete IdtoURI[tokenId];
        for (uint i = 0; i < addressToIds[_Instructor].length; i++) {
            if (addressToIds[_Instructor][i] == tokenId) {
                for (uint l = i; l < addressToIds[_Instructor].length - 1; l++) {
                    addressToIds[_Instructor][l] = addressToIds[_Instructor][l + 1];
                }
                addressToIds[_Instructor].pop();
                return;
            }
        }
        _burn(tokenId);
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        return string(Base64.decode(IdtoURI[tokenId]));
    }

    function URI(
        string memory _name,
        string memory _userId,
        string memory profession,
        string memory briefIntro,
        string[3] memory links,
        string memory imageURI,
        uint256 tokenId
    ) private {
        string memory Uri = string(
            abi.encodePacked(
                _baseURI(),
                Base64.encode(
                    bytes(
                        abi.encodePacked(
                            '{"name":"',
                            name(),
                            '", "description":"An NFT that represent that you are an instructor on the Ridlearn platform", ',
                            '"Owner":[{"name": "',
                            _name,
                            '", "userId": "',
                            _userId,
                            "_",
                            Strings.toString(tokenId),
                            '", "tokenId":"',
                            Strings.toString(tokenId),
                            '", "profession":"',
                            profession,
                            '", "links":[{"faceBook":"',
                            links[0],
                            '", "youtube":"',
                            links[1],
                            '", "email":"',
                            links[2],
                            '"}], "briefIntro":"',
                            briefIntro,
                            '"}], "imageURI":"',
                            imageURI,
                            '"}'
                        )
                    )
                )
            )
        );
        IdtoURI[tokenId] = Uri;
    }

    function getTokensOwned(address _address) public view returns (uint256[] memory) {
        return addressToIds[_address];
    }
}

