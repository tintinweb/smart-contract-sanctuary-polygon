pragma solidity ^0.8.9;

import "hardhat/console.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

contract Receiver is Pausable {
    address public owner;
    address public addressFactory;
    bytes32 public immutable signerHash;

    constructor(address _owner, address _addressFactory, bytes32 _signerHash) {
        owner = _owner;
        addressFactory = _addressFactory;
        signerHash = _signerHash;
    }

    function withdrawFromContract(
        address _owner
    ) external payable whenNotPaused {
        console.log("owner : ", msg.sender);

        require(
            msg.sender == addressFactory,
            "Only the factory can execute this contract"
        );

        bytes32 _hashMsg = keccak256(abi.encode(_owner, addressFactory));

        require(signerHash == _hashMsg, "Not the owner that signed the hash");

        uint256 _balance = address(this).balance;

        (bool ok, ) = _owner.call{value: _balance}("");
        require(ok, "Failed to withdraw");
        _pause();
        //selfdestruct(payable(owner));
    }

    receive() external payable whenNotPaused {
        //
    }

    fallback() external payable whenNotPaused {
        //
    }
}

