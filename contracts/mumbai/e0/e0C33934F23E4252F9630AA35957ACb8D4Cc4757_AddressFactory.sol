pragma solidity ^0.8.9;

import "hardhat/console.sol";

import "@openzeppelin/contracts/utils/Counters.sol";
import "./Receiver.sol";

contract AddressFactory {
    Receiver receiver;

    using Counters for Counters.Counter;

    mapping(address => Receiver[]) private ownContracts;
    mapping(address => Counters.Counter) private contractCounter;
    mapping(address => bool) private contractUsed;

    function createNewContracts() external returns (bool) {
        console.log("creating new contract");
        _createNewAddress();
        return true;
    }

    function withdrawFromContract(uint256 index) external returns (bool) {
        require(
            index < ownContracts[msg.sender].length,
            "Invalid contract index"
        );

        Receiver contractToExecute = ownContracts[msg.sender][index];
        console.log("The contract to execute: ", address(contractToExecute));
        console.log("The index", index);
        bool _contractused = contractUsed[address(contractToExecute)];

        if (_contractused == false) {
            contractUsed[address(contractToExecute)] = true;
            // delete ownContracts[msg.sender][index];
            remove(index);
        } else {
            revert("Address already used");
        }

        contractToExecute.withdrawFromContract(msg.sender);
        contractCounter[msg.sender].decrement();
        return (true);
    }

    function getAddressContracts(
        address _address
    ) external view returns (address[] memory) {
        require(msg.sender == _address, "Not the owner");

        require(
            contractCounter[msg.sender].current() > 0,
            "There are no current contracts."
        );
        uint256 lenght = contractCounter[msg.sender].current();
        address[] memory _contracts = new address[](lenght);

        require(lenght > 0, "There are no contracts.");

        for (uint256 i; i < lenght; i++) {
            address _ownedContracts = address(ownContracts[msg.sender][i]);
            _contracts[i] = (_ownedContracts);
        }
        return _contracts;
    }

    function getContractsUsed(
        address _addressContract
    ) external view returns (bool) {
        return (contractUsed[address(_addressContract)]);
    }

    function getOwnContracts(
        address _address
    ) external view returns (address[] memory) {
        require(msg.sender == _address, "Not the owner");
        require(
            contractCounter[_address].current() > 0,
            "There are no current contracts."
        );

        uint256 lenght = contractCounter[_address].current();
        address[] memory _ownContracts = new address[](lenght);

        for (uint256 i; i < lenght; i++) {
            _ownContracts[i] = address(ownContracts[_address][i]);
        }
        return (_ownContracts);
    }

    function getContractCounter(
        address _address
    ) external view returns (uint256) {
        console.log("The sender : ", msg.sender);
        console.log("The owner : ", _address);
        console.log(
            "The returned param :",
            contractCounter[_address].current()
        );
        require(msg.sender == _address, "Not the owner");
        return (contractCounter[_address].current());
    }

    function _createNewAddress() private returns (Receiver) {
        console.log("creating new address");
        bytes32 _signerHash = keccak256(abi.encode(msg.sender, address(this)));

        Receiver newContract = new Receiver(
            msg.sender,
            address(this),
            _signerHash
        );

        console.log("owner : ", msg.sender);
        console.log("this address : ", address(this));
        console.log("contract address : ", address(newContract));

        ownContracts[msg.sender].push(newContract);
        /*
        address checkThis = address(ownContracts[msg.sender][0]);
        console.log("Contract Address: ", checkThis);
        */
        contractCounter[msg.sender].increment();
        return (newContract);
    }

    function remove(uint _index) private {
        require(_index < ownContracts[msg.sender].length, "index out of bound");

        for (uint i = _index; i < ownContracts[msg.sender].length - 1; i++) {
            ownContracts[msg.sender][i] = ownContracts[msg.sender][i + 1];
        }
        ownContracts[msg.sender].pop();
    }
}

