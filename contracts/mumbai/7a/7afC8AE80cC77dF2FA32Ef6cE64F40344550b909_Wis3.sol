        newCourse.courseCreator = msg.sender;
        newCourse.courseFee = courseFee;

        Generated newNFTaddress = new Generated(courseName,courseCode);
        tokens.push(newNFTaddress);

        newCourse.NFT = (newNFTaddress);

        courseToken_Details[courseCode] = newCourse;
    }

    function buyCourse(string memory courseCode) public payable {
        if(msg.value < courseToken_Details[courseCode].courseFee){
            revert Wis3__NOT_ENOUGH_ETH_TO_BUY_THE_COURSE();
        }
        proceeds[courseToken_Details[courseCode].courseCreator]+= (((courseToken_Details[courseCode].courseFee) * 80 )/100);
        Generated NFT = courseToken_Details[courseCode].NFT;
        NFT.safeMint(msg.sender);
        courseToken_Details[courseCode].student_count+=1;
    }

    function createProposal(string memory courseCode, string memory description, string memory composeStream)public  {
        // Check sender is student or not
        Generated Nft = courseToken_Details[courseCode].NFT;
        if(Nft.balanceOf(msg.sender)<1){
            revert Wis3__NOT_AUTHORISED();
        }
        Proposal memory newProps;
        newProps.creator = msg.sender; 
        newProps.courseCode = courseCode;
        newProps.description = description;
        newProps.ComposeDBstream = composeStream;

        add_proposal[msg.sender] = newProps;
    }

    function performVoting(address addr,uint256 vote) public {
        
        Generated Nft = courseToken_Details[add_proposal[addr].courseCode].NFT;
        if(Nft.balanceOf(msg.sender)<1){
            revert Wis3__NOT_AUTHORISED();
        }
        if(hasVoted[msg.sender][addr] == true){
            revert Wis3__ALREADY_VOTED();
        }
        
        Proposal memory props = add_proposal[addr] ;
        hasVoted[msg.sender][addr] = true;
        if(hasCred[msg.sender][add_proposal[addr].courseCode] == true){
            if(vote==1){
                props.votesInFavour+=2;
            }
            else{
                props.votesAgainst+=2;
            }
        }
        else{
            if(vote==1){
                props.votesInFavour+=1;
            }
            else{
                props.votesAgainst+=1;
            }
        }
        props.peoplevotedbynow+=1;

        add_proposal[addr] = props;
    }

    function handleRefund() public {
        if(add_proposal[msg.sender].votesInFavour < add_proposal[msg.sender].votesAgainst ){
            revert();
        }
        if(add_proposal[msg.sender].status ==0){
            revert();
        }
        (bool sent, ) = msg.sender.call{value: proceeds[msg.sender]}("");
        add_proposal[msg.sender].status = 1;
        require(sent);
    }

    function fundContract() payable public {

    } 

    function withdrawProceeds()public {
        if(proceeds[msg.sender]==0){
            revert Wise3__No_Balance();
        }
        proceeds[msg.sender] = 0;
        (bool sent, ) = msg.sender.call{value: proceeds[msg.sender]}("");
        require(sent, "Failed to send Ether");
    }

    function addCredentials (string memory courseCode, string memory composeDB)public{
        credentials[msg.sender][courseCode] = composeDB;
        hasCred[msg.sender][courseCode]=true;
    }

    function getCredential(address addr, string memory courseCode) public view returns (string memory){
        return credentials[addr][courseCode];
    }

    function getCredentialBool(address addr, string memory courseCode) public view returns (bool){
        return hasCred[addr][courseCode];
    }

    function getCourseCreator (string memory courseCode) public  view returns (address){
