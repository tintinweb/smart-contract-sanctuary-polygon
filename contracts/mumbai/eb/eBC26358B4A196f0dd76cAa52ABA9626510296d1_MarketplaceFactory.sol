pragma solidity ^0.8.0;
pragma abicoder v2;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/proxy/beacon/UpgradeableBeacon.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./ERC777RecipientBeaconProxy.sol";
import "./Marketplace.sol";
import "./@galaxis/IRegistry.sol";
import "./@galaxis/CommunityList.sol";
import "./@galaxis/CommunityRegistry.sol";

contract MarketplaceFactory is Ownable {
    uint256 public constant VERSION = 20230515;

    address public immutable marketplaceBeaconAddress;
    UpgradeableBeacon private _marketplaceBeacon;

    address public immutable splitterBeaconAddress;
    UpgradeableBeacon private _splitterBeacon;

    address public constant REGISTRY_ADDRESS = 0x1e8150050A7a4715aad42b905C08df76883f396F;
    IRegistry private _registry;

    string public constant REGISTRY_KEY_MARKETPLACE_FACTORY_CONTRACT = "MARKETPLACE_FACTORY";
    string public constant REGISTRY_KEY_COMMUNITY_LIST = "COMMUNITY_LIST";
    bytes32 public constant COMMUNITY_REGISTRY_ADMIN = keccak256("COMMUNITY_REGISTRY_ADMIN");

    event MarketplaceProxyDeployed(address _address);

    constructor(address _marketplaceImplementation, address _splitterImplementation) {
        _marketplaceBeacon = new UpgradeableBeacon(_marketplaceImplementation);
        marketplaceBeaconAddress = address(_marketplaceBeacon);

        _splitterBeacon = new UpgradeableBeacon(_splitterImplementation);
        splitterBeaconAddress = address(_splitterBeacon);

        _registry = IRegistry(REGISTRY_ADDRESS);
    }

    function deploy(
        uint32 _communityId,
        uint32 _tokenNum,
        uint16 _royalty,
        address[] memory _addresses, // royalty splitting recipients
        uint256[] memory _shares // royalty splitting shares
    ) external {
        // validate royalty is less than 10%
        require(_royalty <= 1000, "MarketplaceFactory: Royalty limit is 10%");
        // validate this contract is the current version to be used, else fail
        address factoryAddress = _registry.getRegistryAddress(REGISTRY_KEY_MARKETPLACE_FACTORY_CONTRACT);
        require(factoryAddress == address(this), "MarketplaceFactory: Not current project factory");

        CommunityList communityList = CommunityList(_registry.getRegistryAddress(REGISTRY_KEY_COMMUNITY_LIST));
        (, address crAddr, ) = communityList.communities(_communityId);
        require(crAddr != address(0), "MarketplaceFactory: Invalid community ID");
        CommunityRegistry communityRegistry = CommunityRegistry(crAddr);
        require(
            communityRegistry.isUserCommunityAdmin(COMMUNITY_REGISTRY_ADMIN, msg.sender),
            "MarketplaceFactory: Community not owned by sender"
        );
        address tokenContractAddress = communityRegistry.getRegistryAddress(
            string(abi.encodePacked("TOKEN_", Strings.toString(_tokenNum)))
        );

        require(tokenContractAddress != address(0), "MarketplaceFactory: Invalid token number to link with");
        // instantiate marketplace proxy
        ERC777RecipientBeaconProxy splitterProxy = new ERC777RecipientBeaconProxy(
            splitterBeaconAddress,
            abi.encodeWithSignature(
                "initialize(address,address[],uint16[])",
                msg.sender, // deployer account becomes owner of splitter
                _addresses,
                _shares
            )
        );
        // instantiate marketplace proxy
        ERC777RecipientBeaconProxy marketplaceProxy = new ERC777RecipientBeaconProxy(
            marketplaceBeaconAddress,
            abi.encodeWithSignature(
                "initialize(address,address,address,uint16)",
                tokenContractAddress,
                address(splitterProxy),
                msg.sender, // deployer account becomes owner  of marketplace
                _royalty
            )
        );
        emit MarketplaceProxyDeployed(address(marketplaceProxy));

        // set marketplace address in community's registry
        communityRegistry.setRegistryAddress(
            string(abi.encodePacked("MARKETPLACE_", Strings.toString(_tokenNum), "_ADDRESS")),
            address(marketplaceProxy)
        );
    }

    function updateMarketplaceImplementation(address _address) external onlyOwner {
        _marketplaceBeacon.upgradeTo(_address);
    }

    function updateSplitterImplementation(address _address) external onlyOwner {
        _splitterBeacon.upgradeTo(_address);
    }
}

