// SPDX-License-Identifier: MIT

pragma solidity ^0.8.0;
pragma abicoder v2;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/interfaces/IERC2981.sol";
import "@openzeppelin/contracts/token/ERC777/IERC777.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC777/IERC777Recipient.sol";
import "@openzeppelin/contracts/utils/introspection/IERC1820Registry.sol";
import "@openzeppelin/contracts/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./recovery/BlackHolePrevention.sol";
import "./@galaxis/IRegistry.sol";

contract Marketplace is ReentrancyGuard, IERC777Recipient, Initializable, IERC2981, BlackHolePrevention {
    uint256 public constant VERSION = 20230607;

    uint16 public royalty; // royalty percentage expressed in tenthousandths
    address public tokenAddress; // ERC721 NFT contract address
    ERC721 private _tokenContract; // ERC721 NFT token contract instance
    address payable public splitterAddress; //  The royalty splitter contract address

    address public constant REGISTRY_ADDRESS = 0x1e8150050A7a4715aad42b905C08df76883f396F;
    IRegistry private _galaxisRegistryContract; // Galaxis Registry contract

    struct Offer {
        bool isForSale; // flag to check sale status
        address seller;
        uint256 value;
        address sellOnlyTo; // specify to sell only to a specific address
        address erc777Address;
        uint256 indexInOfferArray; // index where the offer is at in the all-offers array
    }

    struct Bid {
        bool hasBid; // flag to check bid status
        address bidder;
        uint256 value;
        address erc777Address;
        uint256 indexInBidArray; // index where the offbider is at in the all-bids array
    }

    // map offers and bids for each token
    mapping(uint256 => Offer) public cardsForSaleInETH; // map of card id to offer struct of cards for sale in ETH
    mapping(uint256 => Offer) public cardsForSaleInERC777; // map of card id to offer struct of cards for sale in ERC777
    mapping(uint256 => Bid) public bidsInEther; // map of card id to bid struct of cards for sale in ETH
    mapping(uint256 => Bid) public bidsInERC777; // map of card id to bid struct of cards for sale in ERC777

    uint256[] private _allCardsForSaleInETH; // dynamic array of all token ids on sale in ETH
    uint256[] private _allCardsForSaleInERC777; // dynamic array of all token ids on sale in ERC777
    uint256[] private _allCardsWithBidsInETH; // dynamic array of all token ids with bids in ETH
    uint256[] private _allCardsWithBidsInERC777; // dynamic array of all token ids with bids in ERC777

    event OfferForSale(address _from, address _to, uint256 _tokenId, uint256 _value, bool _useERC777);
    event OfferExecuted(address _from, address _to, uint256 _tokenId, uint256 _value, bool _useERC777);
    event OfferRevoked(address _from, address _to, uint256 _tokenId, uint256 _value, bool _useERC777);
    event OfferModified(address _from, uint256 _tokenId, uint256 _value, address _sellOnlyTo, bool _useERC777);

    event BidReceived(
        address _from,
        address _to,
        uint256 _tokenId,
        uint256 _newValue,
        uint256 _prevValue,
        bool _useERC777
    );
    event BidAccepted(address _from, address _to, uint256 _tokenId, uint256 _value, bool _useERC777);
    event BidRevoked(address _from, uint256 _tokenId, uint256 _value, bool _useERC777);
    event BidDenied(address _from, uint256 _tokenId, bool _useERC777);
    event RoyaltyChanged(address _from, uint16 _royalty);

    modifier onlyCardOwner(uint256 _tokenId) {
        require(_tokenContract.ownerOf(_tokenId) == msg.sender, "Marketplace: Unauthorized");
        _;
    }

    /**
    @param _tokenAddress address of the IERC721 NFT contract
    which is to be traded on this marketplace instance
    @param _owner address of the owner of thei smarketplace instance.
    Ownership will be transferred to this address after the deployment
    succeeds.
    @param _splitterAddress address of royalty splitter
    @dev this function is called when a proxy is deployed and the state
    modified in it is unique to the proxy instance.
     */
    function initialize(
        address _tokenAddress,
        address _splitterAddress,
        address _owner,
        uint16 _royalty
    ) external initializer {
        require(_tokenAddress != address(0), "Marketplace: Null address not accepted");
        require(_splitterAddress != address(0), "Marketplace: Null address not accepted");
        require(_owner != address(0), "Marketplace: Null address not accepted");
        require(_royalty <= 1000, "Marketplace: Royalty limit is 10%");

        tokenAddress = _tokenAddress;
        _tokenContract = ERC721(_tokenAddress);
        splitterAddress = payable(_splitterAddress);
        royalty = _royalty;
        _galaxisRegistryContract = IRegistry(REGISTRY_ADDRESS);
        _transferOwnership(_owner);
    }

    /**
    @inheritdoc IERC777Recipient
     */
    function tokensReceived(
        address,
        address,
        address,
        uint256,
        bytes calldata,
        bytes calldata
    ) external view override {
        // handle incoming ERC777 when bids are made
        require(msg.sender == getAcceptedERC777Address(), "Marketplace: Invalid token type");
    }

    //________________________________Offers______________________________

    function _offerCardForSale(
        uint256 _tokenId,
        uint256 _minPrice,
        address _sellOnlyTo,
        bool _useERC777
    ) private onlyCardOwner(_tokenId) {
        // check if the contract is approved by token owner
        require(_tokenContract.isApprovedForAll(msg.sender, address(this)), "Marketplace: Contract not authorized");

        // check if price is set to higher than 0
        require(_minPrice > 0, "Marketplace: Offer price must be > 0");

        require(_sellOnlyTo != msg.sender, "Marketplace: Sell only to address cannot be seller's address");

        // initialize offer
        if (_useERC777) {
            // create id => offer mapping
            cardsForSaleInERC777[_tokenId] = Offer(
                true,
                msg.sender,
                _minPrice,
                _sellOnlyTo,
                getAcceptedERC777Address(),
                _allCardsForSaleInERC777.length
            );
            // save card id to list of cards on sale
            _allCardsForSaleInERC777.push(_tokenId);
        } else {
            // create id => offer mapping
            cardsForSaleInETH[_tokenId] = Offer(
                true,
                msg.sender,
                _minPrice,
                _sellOnlyTo,
                address(0),
                _allCardsForSaleInETH.length
            );
            // save card id to list of cards on sale
            _allCardsForSaleInETH.push(_tokenId);
        }

        // emit sale event
        emit OfferForSale(msg.sender, address(0), _tokenId, _minPrice, _useERC777);
    }

    function offerCardForSaleInETH(
        uint256 _tokenId,
        uint256 _minPrice,
        address _sellOnlyTo
    ) external {
        _offerCardForSale(_tokenId, _minPrice, _sellOnlyTo, false);
    }

    function offerCardForSaleInERC777(
        uint256 _tokenId,
        uint256 _minPrice,
        address _sellOnlyTo
    ) external {
        _offerCardForSale(_tokenId, _minPrice, _sellOnlyTo, true);
    }

    function _modifyOffer(
        uint256 _tokenId,
        uint256 _value,
        address _sellOnlyTo,
        bool _useERC777
    ) private onlyCardOwner(_tokenId) {
        Offer memory offer = _useERC777 ? cardsForSaleInERC777[_tokenId] : cardsForSaleInETH[_tokenId];

        require(offer.isForSale, "Marketplace: This token is not for sale");
        require(_value > 0, "Marketplace: Offer price must be > 0");
        require(_sellOnlyTo != msg.sender, "Marketplace: Sell only to address cannot be seller's address");

        Offer memory newOffer = Offer(
            offer.isForSale,
            offer.seller,
            _value,
            _sellOnlyTo,
            offer.erc777Address,
            offer.indexInOfferArray
        );
        // modify offer
        if (_useERC777) {
            cardsForSaleInERC777[_tokenId] = newOffer;
        } else {
            cardsForSaleInETH[_tokenId] = newOffer;
        }
        emit OfferModified(msg.sender, _tokenId, _value, _sellOnlyTo, _useERC777);
    }

    function modifyEtherOffer(
        uint256 _tokenId,
        uint256 _value,
        address _sellOnlyTo
    ) external {
        _modifyOffer(_tokenId, _value, _sellOnlyTo, false);
    }

    function modifyERC777Offer(
        uint256 _tokenId,
        uint256 _value,
        address _sellOnlyTo
    ) external {
        _modifyOffer(_tokenId, _value, _sellOnlyTo, true);
    }

    function _revokeOffer(uint256 _tokenId, bool _useERC777) private onlyCardOwner(_tokenId) {
        Offer memory offer = _useERC777 ? cardsForSaleInERC777[_tokenId] : cardsForSaleInETH[_tokenId];
        require(offer.isForSale, "Marketplace: This token is not for sale");

        _deleteOffer(_tokenId, _useERC777);

        Offer memory newOffer = Offer(false, address(0), 0, address(0), address(0), 0);
        if (_useERC777) {
            cardsForSaleInERC777[_tokenId] = newOffer;
        } else {
            cardsForSaleInETH[_tokenId] = newOffer;
        }
        emit OfferRevoked(offer.seller, offer.sellOnlyTo, _tokenId, offer.value, _useERC777);
    }

    function revokeEtherOffer(uint256 _tokenId) external {
        _revokeOffer(_tokenId, false);
    }

    function revokeERC777Offer(uint256 _tokenId) external {
        _revokeOffer(_tokenId, true);
    }

    function _buyItNow(uint256 _tokenId, bool _useERC777) private nonReentrant {
        Offer memory offer = _useERC777 ? cardsForSaleInERC777[_tokenId] : cardsForSaleInETH[_tokenId];
        // check if the offer is valid
        require(offer.isForSale, "Marketplace: This token is not for sale");
        require(offer.seller != address(0), "Marketplace: This token is not for sale");
        require(offer.value > 0, "Marketplace: This token is not for sale");

        // check if it is for sale for someone specific
        if (offer.sellOnlyTo != address(0)) {
            // only sell to someone specific
            require(offer.sellOnlyTo == msg.sender, "Marketplace: This token is not for sale for buyer");
        }

        // make sure buyer is not the owner
        require(msg.sender != _tokenContract.ownerOf(_tokenId), "Marketplace: Token already owned");

        // check approval status, user may have modified transfer approval
        require(_tokenContract.isApprovedForAll(offer.seller, address(this)), "Marketplace: Contract not authorized");

        if (_useERC777) {
            // check if buyer has enough ERC777 to purchase
            require(_getAcceptedERC777().balanceOf(msg.sender) >= offer.value, "Marketplace: Not enough ERC777");
        } else {
            // check if offer value and sent values match
            require(offer.value == msg.value, "Marketplace: Not enough ETH sent");
        }

        // make sure the seller is the owner
        require(offer.seller == _tokenContract.ownerOf(_tokenId), "Marketplace: Unauthorized");

        // save the seller variable
        address seller = offer.seller;

        // delete offers for this card
        _deleteOffer(_tokenId, true);
        _deleteOffer(_tokenId, false);

        // check if there were any ether bids on this card
        Bid memory bid = bidsInEther[_tokenId];
        if (bid.hasBid) {
            // save bid values and bidder variables
            address bidder = bid.bidder;
            uint256 amount = bid.value;
            // reset bid
            bidsInEther[_tokenId] = Bid(false, address(0), 0, address(0), 0);
            // send back bid value to bidder
            bool sent;
            (sent, ) = bidder.call{value: amount}("");
            require(sent, "Marketplace: Failed to send back ETH");
        }

        // check if there were any ERC777 bids on this card
        Bid memory ERC777Bid = bidsInERC777[_tokenId]; //solhint-disable-line var-name-mixedcase
        if (ERC777Bid.hasBid) {
            // save bid values and bidder variables
            address bidder = ERC777Bid.bidder;
            uint256 amount = ERC777Bid.value;
            // reset bid
            bidsInERC777[_tokenId] = Bid(false, address(0), 0, address(0), 0);
            // send back bid value to bidder
            _getAcceptedERC777().operatorSend(address(this), bidder, amount, "", "");
        }

        // first send the token to the buyer
        _tokenContract.safeTransferFrom(seller, msg.sender, _tokenId);

        // transfer ether to acceptor and pay royalty to the community owner
        if (_useERC777) {
            _splitERC777(msg.sender, seller, offer.value);
        } else {
            _split(seller, offer.value);
        }

        // check if the user recieved the item
        require(_tokenContract.ownerOf(_tokenId) == msg.sender);

        // emit event
        emit OfferExecuted(offer.seller, msg.sender, _tokenId, offer.value, _useERC777);
    }

    function buyItNowForEther(uint256 _tokenId) external payable {
        _buyItNow(_tokenId, false);
    }

    function buyItNowForERC777(uint256 _tokenId) external {
        _buyItNow(_tokenId, true);
    }

    //_______________________Bids_________________________________

    function _bidOnCard(
        uint256 _tokenId,
        uint256 _bidValue,
        address _bidder,
        bool _useERC777
    ) private nonReentrant {
        // check if bid value is valid
        require(_bidValue > 0, "Marketplace: Bid value must be > 0");

        // check that not bidding on owned card
        require(_bidder != _tokenContract.ownerOf(_tokenId), "Marketplace: Token already owned");

        Bid memory lastBid = _useERC777 ? bidsInERC777[_tokenId] : bidsInEther[_tokenId];

        require(lastBid.value < _bidValue, "Marketplace: Bid must exceed current bid");

        // initialize the bid with the new values
        Bid memory newBid = Bid(
            true,
            msg.sender,
            _bidValue,
            _useERC777 ? getAcceptedERC777Address() : address(0),
            _useERC777 ? _allCardsWithBidsInERC777.length : _allCardsWithBidsInETH.length
        );

        if (_useERC777) {
            require(_getAcceptedERC777().balanceOf(_bidder) >= _bidValue, "Marketplace: Not enough ERC777");

            // save bid for token id mapping
            bidsInERC777[_tokenId] = newBid;

            // save the token id in the all-cards-with-bid array
            _allCardsWithBidsInERC777.push(_tokenId);

            // move ERC777 into marketplace contract
            _getAcceptedERC777().operatorSend(_bidder, address(this), _bidValue, "", "");
        } else {
            // save bid for token id mapping
            bidsInEther[_tokenId] = newBid;

            // save the token id in the all-cards-with-bid array
            _allCardsWithBidsInETH.push(_tokenId);
        }

        // emit event
        emit BidReceived(
            msg.sender,
            _tokenContract.ownerOf(_tokenId),
            _tokenId,
            newBid.value,
            lastBid.value,
            _useERC777
        );

        // refund previous bidder if exists
        if (lastBid.hasBid) {
            if (_useERC777) {
                _getAcceptedERC777().operatorSend(address(this), lastBid.bidder, lastBid.value, "", "");
            } else {
                bool sent;
                (sent, ) = lastBid.bidder.call{value: lastBid.value}("");
                require(sent, "Marketplace: Failed to send back ETH");
            }
        }
    }

    function bidOnCardWithEther(uint256 _tokenId) external payable {
        _bidOnCard(_tokenId, msg.value, msg.sender, false);
    }

    function bidOnCardWithERC777(uint256 _tokenId, uint256 _bidValue) external {
        _bidOnCard(_tokenId, _bidValue, msg.sender, true);
    }

    function _acceptBid(uint256 _tokenId, bool _useERC777) private nonReentrant {
        Bid memory bid = _useERC777 ? bidsInERC777[_tokenId] : bidsInEther[_tokenId];

        // make sure there is a valid bid on the card
        require(bid.hasBid, "Marketplace: No bid found for token");
        // check if the contract is still approved for transfer
        require(_tokenContract.isApprovedForAll(msg.sender, address(this)), "Marketplace: Contract not authorized");

        // reset offers for this token
        cardsForSaleInETH[_tokenId] = Offer(false, address(0), 0, address(0), address(0), 0);
        cardsForSaleInERC777[_tokenId] = Offer(false, address(0), 0, address(0), address(0), 0);

        address buyer = bid.bidder;
        uint256 amount = bid.value;

        Bid memory otherBid = _useERC777 ? bidsInEther[_tokenId] : bidsInERC777[_tokenId];

        // delete bids
        _deleteBid(_tokenId, true); // ERC777 bid
        _deleteBid(_tokenId, false); // ETH bid

        if (_useERC777) {
            if (otherBid.hasBid) {
                // Refund existing ETH bid
                (bool success, ) = otherBid.bidder.call{value: otherBid.value}("");
                require(success, "Marketplace: Transfer failed");
            }
            // send ERC777
            _splitERC777(address(this), msg.sender, amount);
        } else {
            if (otherBid.hasBid) {
                // Refund  existing ERC777 bid
                _getAcceptedERC777().send(otherBid.bidder, otherBid.value, "");
            }
            // send ETH
            _split(msg.sender, amount);
        }

        // send token from acceptor to the bidder
        _tokenContract.safeTransferFrom(msg.sender, buyer, _tokenId);

        // check if the user received the token
        require(_tokenContract.ownerOf(_tokenId) == buyer);

        // emit event
        emit BidAccepted(msg.sender, bid.bidder, _tokenId, amount, _useERC777);
    }

    function acceptEtherBid(uint256 _tokenId) external onlyCardOwner(_tokenId) {
        _acceptBid(_tokenId, false);
    }

    function acceptERC777Bid(uint256 _tokenId) external onlyCardOwner(_tokenId) {
        _acceptBid(_tokenId, true);
    }

    function _revokeBid(uint256 _tokenId, bool _useERC777) private nonReentrant {
        Bid memory bid = _useERC777 ? bidsInERC777[_tokenId] : bidsInEther[_tokenId];
        // check if the bid exists
        require(bid.hasBid, "Marketplace: No bid found for token");
        // check if the bidder is the sender of the message
        require(bid.bidder == msg.sender, "Marketplace: Unauthorized");
        // save bid value into a variable
        uint256 amount = bid.value;

        // remove bid
        _deleteBid(_tokenId, _useERC777);
        if (_useERC777) {
            // refund ERC777
            _getAcceptedERC777().send(msg.sender, amount, "");
        } else {
            // refund ETH
            (bool sent, ) = msg.sender.call{value: amount}("");
            require(sent, "Marketplace: Failed to send back ETH");
        }

        // emit event
        emit BidRevoked(msg.sender, _tokenId, amount, _useERC777);
    }

    function revokeEtherBid(uint256 _tokenId) external {
        _revokeBid(_tokenId, false);
    }

    function revokeERC777Bid(uint256 _tokenId) external {
        _revokeBid(_tokenId, true);
    }

    function _denyBid(uint256 _tokenId, bool _useERC777) private onlyCardOwner(_tokenId) {
        Bid memory bid = _useERC777 ? bidsInERC777[_tokenId] : bidsInEther[_tokenId];
        // check if the bid exists
        require(bid.hasBid, "Marketplace: No bid found for token");

        uint256 amount = bid.value;
        address bidder = bid.bidder;

        // delete bid
        _deleteBid(_tokenId, _useERC777);

        if (_useERC777) {
            // refund ERC777
            _getAcceptedERC777().send(bidder, amount, "");
        } else {
            // refund ETH
            (bool sent, ) = bidder.call{value: amount}("");
            require(sent, "Marketplace: Failed to send back ETH");
        }

        // emit event
        emit BidRevoked(msg.sender, _tokenId, amount, _useERC777);
    }

    function denyEtherBid(uint256 _tokenId) external {
        _denyBid(_tokenId, false);
    }

    function denyERC777Bid(uint256 _tokenId) external {
        _denyBid(_tokenId, true);
    }

    function getAllERC777Offers() external view returns (uint256[] memory) {
        return _allCardsForSaleInERC777;
    }

    function getAllEtherOffers() external view returns (uint256[] memory) {
        return _allCardsForSaleInETH;
    }

    function getAllERC777Bids() external view returns (uint256[] memory) {
        return _allCardsWithBidsInERC777;
    }

    function getAllEtherBids() external view returns (uint256[] memory) {
        return _allCardsWithBidsInETH;
    }

    function setRoyalty(uint16 _royalty) public onlyOwner {
        require(_royalty <= 1000, "Marketplace: Royalty limit is 10%");
        royalty = _royalty;
        emit RoyaltyChanged(msg.sender, _royalty);
    }

    function _split(address _seller, uint256 _amount) private {
        uint256 royaltyAmount = (_amount * royalty) / 10000;

        bool success = false;

        // send to splitter
        (bool _success, bytes memory returndata) = splitterAddress.call{value: royaltyAmount}("");
        require(_success, string(returndata));

        // transfer rest to the seller
        (success, ) = _seller.call{value: _amount - royaltyAmount}("");
        require(success, "Marketplace: Transfer failed");
    }

    function _splitERC777(
        address _buyer,
        address _seller,
        uint256 _amount
    ) private {
        uint256 royaltyAmount = (_amount * royalty) / 10000;

        // send royalty to splitter
        _getAcceptedERC777().operatorSend(_buyer, splitterAddress, royaltyAmount, "", "");

        // send rest to seller
        _getAcceptedERC777().operatorSend(_buyer, _seller, _amount - royaltyAmount, "", "");
    }

    function getAcceptedERC777Address() public view returns (address) {
        return _galaxisRegistryContract.getRegistryAddress("MARKETPLACE_ACCEPTED_ERC777");
    }

    function _getAcceptedERC777() private view returns (IERC777) {
        address acceptedERC777Address = getAcceptedERC777Address();
        return IERC777(acceptedERC777Address);
    }

    function _deleteOffer(uint256 _tokenId, bool _useERC777) private {
        uint256[] storage arrayToRemoveFrom = _useERC777 ? _allCardsForSaleInERC777 : _allCardsForSaleInETH;
        mapping(uint256 => Offer) storage mapOfOffersToUse = _useERC777 ? cardsForSaleInERC777 : cardsForSaleInETH;

        // get offer to remove
        Offer memory currentOffer = mapOfOffersToUse[_tokenId];
        if (!currentOffer.isForSale) {
            return;
        }

        if (arrayToRemoveFrom.length > 1) {
            // get last token id in list
            uint256 lastOfferIdInArray = arrayToRemoveFrom[arrayToRemoveFrom.length - 1];
            // get corresponding offer
            Offer memory lastOfferInArray = mapOfOffersToUse[lastOfferIdInArray];

            // save new location for the moved offer
            lastOfferInArray.indexInOfferArray = currentOffer.indexInOfferArray;

            // move the last offer in place of deleted one
            arrayToRemoveFrom[currentOffer.indexInOfferArray] = lastOfferIdInArray;
        }

        // remove last element
        arrayToRemoveFrom.pop();

        // reset offer in mapping
        mapOfOffersToUse[_tokenId] = Offer(false, address(0), 0, address(0), address(0), 0);
    }

    function _deleteBid(uint256 _tokenId, bool _useERC777) private {
        uint256[] storage arrayToRemoveFrom = _useERC777 ? _allCardsWithBidsInERC777 : _allCardsWithBidsInETH;
        mapping(uint256 => Bid) storage mapOfBidsToUse = _useERC777 ? bidsInERC777 : bidsInEther;

        // get bid to remove
        Bid memory currentBid = mapOfBidsToUse[_tokenId];
        if (!currentBid.hasBid) {
            return;
        }

        if (arrayToRemoveFrom.length > 1) {
            // get last token id in list
            uint256 lastBidIdInArray = arrayToRemoveFrom[arrayToRemoveFrom.length - 1];
            // get corresponding bid
            Bid memory lastBidInArray = mapOfBidsToUse[lastBidIdInArray];

            // save new location for the moved bid
            lastBidInArray.indexInBidArray = currentBid.indexInBidArray;

            // move the last bid in place of deleted one
            arrayToRemoveFrom[currentBid.indexInBidArray] = lastBidIdInArray;
        }

        // remove last element
        arrayToRemoveFrom.pop();

        // reset bid in mapping
        mapOfBidsToUse[_tokenId] = Bid(false, address(0), 0, address(0), 0);
    }

    /**
    @inheritdoc IERC2981
     */
    function royaltyInfo(uint256, uint256 salePrice)
        external
        view
        override(IERC2981)
        returns (address receiver, uint256 royaltyAmount)
    {
        return (splitterAddress, (salePrice * royalty) / 10000);
    }

    /**
    @inheritdoc IERC165
     */
    function supportsInterface(bytes4 interfaceId) external pure override(IERC165) returns (bool) {
        return interfaceId == type(IERC2981).interfaceId || interfaceId == type(IERC777Recipient).interfaceId;
    }
}

