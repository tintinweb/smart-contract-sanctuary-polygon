/**
 * @title ERC721Rollup
 * ERC721Rollup - ERC721 contract that has a Merkle based rollup of addition facts about each token which can be added to over time
 */
abstract contract ERC721Rollup is ERC721, Ownable, Pausable, AccessControl, ERC2771ContextUpdateable {
    using SafeMath for uint256;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant TRANSFER_ROLE = keccak256("TRANSFER_ROLE");

    uint256 public totalSupply;

    bool public mintAutoIncrement = false;

    bool public trackLegacyRoots = false;

    bool public transferRoleEnabled = true;

   // url data for the meta data
    string public tokenURIPrefix = "";

    // each token has a root for it's merkle tree that can be used to see if a hash exists in that tree
    mapping(uint256 => bytes32) public tokenRoots;
    // we optionally keep a list of legacy roots for extra security - this means  that we can prove that a specific root
    // belonged to a specific token in the past. This extra security comes at the cost of extra storage
    mapping(bytes32 => uint256) public legacyRoots;

    event ChangeTokenRoot(address indexed _address, uint256 indexed _tokenId, bytes32 _oldRoot, bytes32 indexed _newRoot);

    constructor(
        string memory _name,
        string memory _symbol,
        bool _mintAutoIncrement
    ) ERC721(_name, _symbol) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(MINTER_ROLE, msg.sender);
        _setupRole(PAUSER_ROLE, msg.sender);
        _setupRole(TRANSFER_ROLE, msg.sender);
        mintAutoIncrement = _mintAutoIncrement;
    }


    //////////////////////////////////////////////////
    // enable meta transactions from a trusted forwarder

    function _msgSender()
        internal
        override(Context, ERC2771ContextUpdateable)
        view
        returns (address sender)
    {
        return ERC2771ContextUpdateable._msgSender();
    }


    function _msgData()
        internal
        override(Context, ERC2771ContextUpdateable)
        view
        returns (bytes calldata)
    {
        return ERC2771ContextUpdateable._msgData();
    }

    //set the forwarder
    function setForwarder(address forwarder) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _setTrustedForwarder(forwarder);
    }

    //////////////////////////////////////////////////////
    // allow approval overrides for transfers by admins - which is a one time diablable feature - for security

    function isApprovedForAll(
        address _owner,
        address _operator
    ) public override view returns (bool isOperator) {
      if (transferRoleEnabled && hasRole(DEFAULT_ADMIN_ROLE, _operator)){
        return true;
      }
        
        // otherwise, use the default ERC721.isApprovedForAll()
        return ERC721.isApprovedForAll(_owner, _operator);
    }

    // one time disabling of the transfer role  the we want to secure the contract and only allow owners to commit their own transfers
    function disableTransferRole() public onlyRole(DEFAULT_ADMIN_ROLE) {
        transferRoleEnabled = false;
    }


    ////////////////////////////////////////////////////////////////////////////////
    // token meta data url functions
    function baseTokenURI() virtual public view returns (string memory) {
        return tokenURIPrefix;
    }

    function tokenURI(uint256 _tokenId) override public view returns (string memory) {
        return string(abi.encodePacked(baseTokenURI(), Strings.toString(_tokenId), ".json"));
    }

    // pausable functions

    /**
     * @dev Before the token is transferred - ensure it's not paused
     *
     */
    function _beforeTokenTransfer(address from, address to, uint256 amount)
        internal
        whenNotPaused
        override(ERC721)
    {
        super._beforeTokenTransfer(from, to, amount);
    }

    /**
     * @dev Pauses all token transfers.
     *
     * See {ERC721Pausable} and {Pausable-_pause}.
     *
     */
    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    /**
     * @dev Unpauses all token transfers.
     *
     * See {ERC721Pausable} and {Pausable-_unpause}.
     *
     */
    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }


    // get a list of tokens owned by someone
    function tokensOfOwner(address _owner) external view returns(uint256[] memory ownerTokens) {
        uint256 tokenCount = balanceOf(_owner);

        if (tokenCount == 0) {
            // Return an empty array
            return new uint256[](0);
        } else {
            uint256[] memory result = new uint256[](tokenCount);
            uint256 totalItems = totalSupply;
            uint256 resultIndex = 0;

            // We count on the fact that all items have IDs starting at 1 and increasing
            // sequentially up to the totalItems count.
            uint256 itemId;

            for (itemId = 1; itemId <= totalItems; itemId++) {
                if (ownerOf(itemId) == _owner) {
                    result[resultIndex] = itemId;
                    resultIndex++;
                }
            }

            return result;
        }
    }
        
   // The following functions are overrides required by Solidity.

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }        

    // change if we are tracking legacy roots
    function trackLegacyRootsActive( bool _active) public onlyRole(DEFAULT_ADMIN_ROLE) {
        trackLegacyRoots = _active;
    }

    // update the token root for a specific token - only the token owner can do this
    function updateTokenRoot(uint256 _tokenId, bytes32 _tokenRoot) public{
        require(_isApprovedOrOwner(_msgSender(), _tokenId),  "You must own the token or be approved to change the token root");

        bytes32 oldTokenRoot = tokenRoots[_tokenId];
        // change the token root
        tokenRoots[_tokenId] = _tokenRoot;

        // track the old root if enabled
        if (trackLegacyRoots)
            legacyRoots[oldTokenRoot] = _tokenId;

        // emit the change
        emit ChangeTokenRoot(_msgSender(), _tokenId, oldTokenRoot, _tokenRoot);
    }    

    // validate that an entry is in the current root for a token
    function validateEntry(uint256 _tokenId, bytes32[] calldata proof, bytes32 _entry) public view{
        require(MerkleProof.verify(proof, tokenRoots[_tokenId], keccak256(abi.encodePacked(_entry))), "BAD_MERKLE_PROOF");
       
    }

    // a helper function used to check legacy entry - if legacy tracking is enabled - it will also check that this root was once part of this history
    function validatePreviousEntry(uint256 _tokenId, bytes32[] calldata proof, bytes32 _entry, bytes32 _legacyRoot) public view{
        if (trackLegacyRoots)
            require(
                legacyRoots[_legacyRoot] == _tokenId,
                "This root did not belong to this token"
            );   
        require(MerkleProof.verify(proof, _legacyRoot, keccak256(abi.encodePacked(_entry))), "BAD_MERKLE_PROOF");
    }     

    

    function updateTokenURIPrefix(string memory newPrefix) external onlyRole(DEFAULT_ADMIN_ROLE) {
        tokenURIPrefix = newPrefix;
    }        
 
    // you should either mint the tokens by an auto incrementing ID - OR by passing in a specific ID - but NOT both
    // this mint is design to support a custodial wallet that doesn't handle meta transactions yet - and as such allows the minter to then transfer it to another wallet
    // if this is minted to a non-custodial wallet - then the new owner can remove the approval

    // mint according to autoincrementing id
    // allow the owner to pre-mint or save a number of tokens
    function mintTokens(uint _amount, address _receiver, bool _addApproval) virtual public onlyRole(MINTER_ROLE) {       

        require(mintAutoIncrement == true, "This contract is not setup to autoincrement tokens Ids");

        uint256 newSupply = totalSupply + _amount;

        for (uint256 i = 0; i < _amount; i++) {
            _mint(_receiver, totalSupply + i);
            // set the caller as approver - so they can move it or burn it
            if (_addApproval)
                _approve(_msgSender(), totalSupply + i);
        }    
        // update the total supply
        totalSupply = newSupply;    
    }

    function mintTokens(uint _amount, address _receiver) virtual public onlyRole(MINTER_ROLE) {      
        return  mintTokens(_amount, _receiver, false);
    }

    // bulk mint to a list of users
    function mintBulkTokens(address[] calldata _receivers,  bool _addApproval) virtual public onlyRole(MINTER_ROLE) {      
        require(mintAutoIncrement == true, "This contract is not setup to allow mint by auto increment");      

        uint256 newSupply = totalSupply + _receivers.length;

        for (uint256 i = 0; i < _receivers.length; i++) {
            _mint(_receivers[i], totalSupply + i);
            if (_addApproval)
              _approve(_msgSender(),totalSupply + i);
        }    
        // update the total supply
        totalSupply = newSupply;    
    }

    // bulk mint to a list of users
    function mintBulkTokens(address[] calldata _receivers) virtual public onlyRole(MINTER_ROLE) {      
        return  mintBulkTokens(_receivers, false);
    }

    // mint according to assigned id
    // allow the owner to pre-mint or save a number of tokens

    function mintTokenById(uint256 _id, address _receiver, bool _addApproval) virtual public onlyRole(MINTER_ROLE) {        
        require(mintAutoIncrement == false, "This contract is not setup to allow mint by custom token Id"); 

        uint256 newSupply = totalSupply + 1;

        // mint the token
        _mint(_receiver, _id);

        // set the caller as approver - so they can move it or burn it
        if (_addApproval)
            _approve(_msgSender(), _id);
  
        // update the total supply
        totalSupply = newSupply;    
    }

    function mintTokenById(uint256 _id, address _receiver) virtual public onlyRole(MINTER_ROLE) {  
        return mintTokenById(_id, _receiver, false);  
    }

    // allow the owner to pre-mint or save a number of tokens
    function mintTokensById(uint256[] calldata _ids, address _receiver, bool _addApproval) virtual public onlyRole(MINTER_ROLE) {     
        require(mintAutoIncrement == false, "This contract is not setup to allow mint by custom token Id");      

        uint256 newSupply = totalSupply + _ids.length;

        for (uint256 i = 0; i < _ids.length; i++) {
            _mint(_receiver, _ids[i]);
            if (_addApproval)
                _approve(_msgSender(), _ids[i]);
        }    
        // update the total supply
        totalSupply = newSupply;    
    }    
    function mintTokensById(uint256[] calldata _ids, address _receiver) virtual public onlyRole(MINTER_ROLE) {     
        return mintTokensById(_ids, _receiver, false);
    }
    // allow the owner to pre-mint or save a number of tokens
    function mintBulkTokensById(uint256[] calldata _ids, address[] calldata _receivers, bool _addApproval) virtual public onlyRole(MINTER_ROLE) {     
        require(mintAutoIncrement == false, "This contract is not setup to allow mint by custom token Id");      

        uint256 newSupply = totalSupply + _ids.length;

        for (uint256 i = 0; i < _ids.length; i++) {
            _mint(_receivers[i], _ids[i]);
            if (_addApproval)
              _approve(_msgSender(), _ids[i]);
        }    
        // update the total supply
        totalSupply = newSupply;    
    }  
    function mintBulkTokensById(uint256[] calldata _ids, address[] calldata _receivers) virtual public onlyRole(MINTER_ROLE) {     
        return mintBulkTokensById(_ids, _receivers, false);
    }

    // a version of safe transfer that keeps the approval of the sender
    // so that if we are transferring from one custodial wallet to another we can continue to transfer and add rullup stories
    function safeTransferFromWithApproval(address from, address to, uint256 tokenId) public virtual {
        // do the transfer
        safeTransferFrom(from, to, tokenId, "");
        // readd the approval - this will onl yget here if the transfer was a success
        _approve(_msgSender(), tokenId);
    }


}

