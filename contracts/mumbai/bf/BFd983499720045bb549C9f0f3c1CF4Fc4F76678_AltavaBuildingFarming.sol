// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

import "./buildingFarming/BuildingFarmingCore.sol";
import "./interfaces/IAssetNFT.sol";
import "hardhat/console.sol";

contract AltavaBuildingFarming is Initializable, BuildingFarmingCore {
    function initialize(
        address _altavaLandNFT,
        address _assetNFT,
        address _lpToken,
        address _tavaToken,
        address _landStakingContract
    ) public initializer {
        __BuildingFarmingCore_init(
            _altavaLandNFT,
            _assetNFT,
            _lpToken,
            _tavaToken,
            _landStakingContract
        );
    }

    function installBuildings(
        uint256 landId,
        BuildingTokenUnit[] memory buildingTokenUnits
    ) public onlyStakingLandOwner(landId) {
        require(
            buildingTokenUnits.length > 0,
            "AltavaBuildingFarming: buildingTokenUnits length should be greater than 0"
        );
        for (uint256 i = 0; i < buildingTokenUnits.length; i++) {
            // 설치 가능한지 체크
            uint256 buildingId = buildingTokenUnits[i].tokenId;
            uint256 buildingType = getBuildingType(buildingId);
            uint256 currentAvailableNum = getAvailableBuildingSlot(
                landId,
                IAltavaLandStaking(landStakingContract).getLandType(landId),
                buildingType
            );

            require(
                currentAvailableNum > 0,
                "AltavaBuildingFarming: no available building slot"
            );
            require(
                IERC1155(assetNFT).balanceOf(msg.sender, buildingId) >=
                    buildingTokenUnits[i].amount,
                "AltavaBuildingFarming: The building balance is insufficient"
            );
            _installBuilding(landId, buildingId, buildingTokenUnits[i].amount);
        }
    }

    function _installBuilding(
        uint256 landId,
        uint256 buildingId,
        uint256 amount
    ) internal {
        // 스테이킹
        IERC1155(assetNFT).safeTransferFrom(
            msg.sender,
            address(this),
            buildingId,
            amount,
            ""
        );
        // 건물 추가
        bool isBuildingIdExist = false;
        for (uint256 j = 0; j < installedBuildingIds[landId].length; j++) {
            if (installedBuildingIds[landId][j] == buildingId) {
                isBuildingIdExist = true;
                break;
            }
        }
        if (!isBuildingIdExist) {
            installedBuildingIds[landId].push(buildingId);
        }
        uint256 buildingType = getBuildingType(buildingId);

        installedBuildingIdNumber[landId][buildingId] += amount;

        installedBuildingTypeCount[landId][buildingType] += amount;

        if (buildingFarmingInfo[landId][buildingType].materialId != 0) {
            // farming 중이었으면 farming 정보 갱신
            buildingFarmingInfo[landId][buildingType] = BuildingFarmingInfo({
                materialId: buildingFarmingInfo[landId][buildingType]
                    .materialId,
                claimableMaterialAmount: getClaimAvailableMaterialAmount(
                    landId,
                    buildingType
                ),
                startTimestamp: block.timestamp
            });
        }

        _installedAmountMap[msg.sender] += amount;
    }

    function removeBuildings(
        uint256 landId,
        BuildingTokenUnit[] memory buildingTokenUnits
    ) public onlyStakingLandOwner(landId) {
        require(
            buildingTokenUnits.length > 0,
            "AltavaBuildingFarming: buildingTokenUnits length should be greater than 0"
        );
        for (uint256 i = 0; i < buildingTokenUnits.length; i++) {
            _removeBuilding(
                landId,
                buildingTokenUnits[i].tokenId,
                msg.sender,
                buildingTokenUnits[i].amount
            );
        }
    }

    function _removeBuilding(
        uint256 landId,
        uint256 buildingId,
        address to,
        uint256 amount
    ) internal {
        require(
            installedBuildingIdNumber[landId][buildingId] >= amount,
            "AltavaBuildingFarming: exceed max available number"
        );
        uint256 buildingType = getBuildingType(buildingId);

        bool isBuildingDisappeared = _checkBuildingAndSettle(
            landId,
            buildingType,
            amount
        );

        _decreaseBuildingIdNumber(landId, buildingId, amount);

        IERC1155(assetNFT).safeTransferFrom(
            address(this),
            to,
            buildingId,
            amount,
            ""
        );

        if (!isBuildingDisappeared) {
            _refreshFarmingInfo(landId, buildingType);
        }

        _installedAmountMap[to] -= amount;
    }

    function chargeFullLPTokenToBuiding(
        uint256 landId,
        uint256 buildingType
    )
        public
        onlyStakingLandOwner(landId)
        onlyBuildingTypeExists(landId, buildingType)
    {
        uint256 lpAmount = getCharableLPAmount(landId, buildingType);
        // lp 소각 후 충전
        ILandLPToken(lpToken).burn(msg.sender, lpAmount);
        // * using lp token amount도 포함되어 있음(claim할때 정산될 예정)
        chargedLPAmountToBuildingType[landId][buildingType] += lpAmount;
    }

    function chargeLPTokenToBuiding(
        uint256 landId,
        uint256 buildingType,
        uint256 lpAmount
    )
        public
        onlyStakingLandOwner(landId)
        onlyBuildingTypeExists(landId, buildingType)
    {
        // 충전 가능한지 체크
        require(
            lpAmount > 0,
            "AltavaBuildingFarming: amount should be greater than 0"
        );
        require(
            getRemainChargedLPTokenAmount(landId, buildingType) + lpAmount <=
                getMaxCharableLPAmount(landId, buildingType),
            "AltavaBuildingFarming: exceed max charge amount"
        );
        // lp 소각 후 충전
        ILandLPToken(lpToken).burn(msg.sender, lpAmount);
        // * using lp token amount도 포함되어 있음(claim할때 정산될 예정)
        chargedLPAmountToBuildingType[landId][buildingType] += lpAmount;
    }

    function activateFarming(
        uint256 landId,
        uint256 buildingType,
        uint256 materialId
    )
        public
        onlyStakingLandOwner(landId)
        onlyBuildingTypeExists(landId, buildingType)
        onlyAvailableMaterial(buildingType, materialId)
    {
        require(
            buildingFarmingInfo[landId][buildingType].materialId == 0,
            "AltavaBuildingFarming: farming is already activated"
        );
        // farming 시작
        buildingFarmingInfo[landId][buildingType].materialId = materialId;
        buildingFarmingInfo[landId][buildingType].startTimestamp = block
            .timestamp;
    }

    function deactivateFarming(
        uint256 landId,
        uint256 buildingType
    )
        public
        onlyStakingLandOwner(landId)
        onlyBuildingTypeExists(landId, buildingType)
    {
        require(
            buildingFarmingInfo[landId][buildingType].materialId != 0,
            "AltavaBuildingFarming: farming is not activated"
        );
        if (getClaimAvailableMaterialAmount(landId, buildingType) > 0) {
            claimMaterial(landId, buildingType);
        }
        buildingFarmingInfo[landId][buildingType] = BuildingFarmingInfo({
            materialId: 0,
            startTimestamp: 0,
            claimableMaterialAmount: 0
        });
    }

    function claimMaterial(
        uint256 landId,
        uint256 buildingType
    )
        public
        onlyStakingLandOwner(landId)
        onlyBuildingTypeExists(landId, buildingType)
    {
        uint256 materialId = buildingFarmingInfo[landId][buildingType]
            .materialId;
        uint256 claimAvailableAmount = getClaimAvailableMaterialAmount(
            landId,
            buildingType
        );

        require(
            claimAvailableAmount > 0,
            "AltavaBuildingFarming: no available claimable material"
        );

        // LP 갱신
        chargedLPAmountToBuildingType[landId][
            buildingType
        ] = getRemainChargedLPTokenAmount(landId, buildingType);

        buildingFarmingInfo[landId][buildingType].startTimestamp = block
            .timestamp;

        // material claim
        IAssetNFT(assetNFT).mintByFarmingContract(
            msg.sender,
            materialId,
            claimAvailableAmount
        );
    }

    function upgradeBuildings(
        uint256 landId,
        BuildingTokenUnit[] memory buildingTokenUnits
    ) public onlyStakingLandOwner(landId) {
        require(
            buildingTokenUnits.length > 0,
            "AltavaBuildingFarming: buildingTokenUnits length should be greater than 0"
        );
        for (uint256 i = 0; i < buildingTokenUnits.length; i++) {
            _upgradeBuilding(
                landId,
                buildingTokenUnits[i].tokenId,
                buildingTokenUnits[i].amount
            );
        }
    }

    function _upgradeBuilding(
        uint256 landId,
        uint256 buildingId,
        uint256 amount
    ) internal {
        require(
            amount > 0,
            "AltavaBuildingFarming: amount should be greater than 0"
        );
        // upgrade 가능한지 체크
        require(
            getBuildingLevel(buildingId) < 24,
            "AltavaBuildingFarming: already max level"
        );
        require(
            installedBuildingIdNumber[landId][buildingId] >= amount,
            "AltavaBuildingFarming: exceed max available number"
        );
        // LP 소진
        uint256 upgradeLPAmount = buildingUpgradeInfo[buildingId].lpAmount *
            amount;
        require(
            IERC20(lpToken).balanceOf(msg.sender) >= upgradeLPAmount,
            "AltavaBuildingFarming: not enough lp token"
        );
        ILandLPToken(lpToken).burn(msg.sender, upgradeLPAmount);

        uint256 buildingType = getBuildingType(buildingId);

        bool isBuildingDisappeared = _checkBuildingAndSettle(
            landId,
            buildingType,
            amount
        );

        _decreaseBuildingIdNumber(landId, buildingId, amount);

        // 해당 레벨 건물 소각
        IAssetNFT(assetNFT).burnByFarmingContract(buildingId, amount);

        if (!isBuildingDisappeared) {
            _refreshFarmingInfo(landId, buildingType);
        }

        // 다음 레벨 민팅 정보 입력
        uint256 upgradePeriod = getUpgradePeriod(buildingId);

        BuildingUpgradeMintingInfo[]
            storage upgradeMintingInfos = buildingUpgradeMintingInfo[landId][
                buildingType
            ];
        uint256 upgradeTargetId = buildingId + 1;
        for (uint256 i = 0; i < upgradeMintingInfos.length; i++) {
            if (upgradeMintingInfos[i].buildingId == upgradeTargetId) {
                buildingUpgradeMintingInfo[landId][buildingType][
                    i
                ] = BuildingUpgradeMintingInfo({
                    buildingId: upgradeTargetId,
                    amount: upgradeMintingInfos[i].amount + amount,
                    upgradeFinishTimestamp: block.timestamp + upgradePeriod
                });
                return;
            }
        }
        buildingUpgradeMintingInfo[landId][buildingType].push(
            BuildingUpgradeMintingInfo({
                buildingId: upgradeTargetId,
                amount: amount,
                upgradeFinishTimestamp: block.timestamp + upgradePeriod
            })
        );
    }

    function mintUpgradedBuildings(
        uint256 landId,
        uint256 buildingType,
        uint256[] memory indexes
    ) public onlyStakingLandOwner(landId) {
        for (uint256 i = 0; i < indexes.length; i++) {
            _mintUpgradedBuilding(landId, buildingType, indexes[i]);
        }
    }

    function _mintUpgradedBuilding(
        uint256 landId,
        uint256 buildingType,
        uint256 index
    ) internal {
        // 가능한 index인지 체크
        require(
            buildingUpgradeMintingInfo[landId][buildingType].length >= index,
            "AltavaBuildingFarming: invalid index"
        );
        BuildingUpgradeMintingInfo
            memory upgradeMintingInfo = buildingUpgradeMintingInfo[landId][
                buildingType
            ][index];

        // 민팅 가능한지 체크
        require(
            upgradeMintingInfo.upgradeFinishTimestamp <= block.timestamp,
            "AltavaBuildingFarming: minting is not available"
        );
        // minting info 갱신
        buildingUpgradeMintingInfo[buildingType][
            index
        ] = buildingUpgradeMintingInfo[buildingType][
            buildingUpgradeMintingInfo[landId][buildingType].length - 1
        ];
        buildingUpgradeMintingInfo[landId][buildingType].pop();

        // 민팅
        IAssetNFT(assetNFT).mintByFarmingContract(
            msg.sender,
            upgradeMintingInfo.buildingId,
            upgradeMintingInfo.amount
        );
    }

    function mintUpgradingBuildingInstantly(
        uint256 landId,
        uint256 buildingType,
        uint256 index
    ) public onlyStakingLandOwner(landId) {
        // 가능한 index인지 체크
        require(
            buildingUpgradeMintingInfo[landId][buildingType].length > index,
            "AltavaBuildingFarming: invalid index"
        );

        BuildingUpgradeMintingInfo
            memory upgradeMintingInfo = buildingUpgradeMintingInfo[landId][
                buildingType
            ][index];
        // 이미 업그레이드가 끝났는지 체크
        require(
            upgradeMintingInfo.upgradeFinishTimestamp > block.timestamp,
            "AltavaBuildingFarming: minting is available"
        );
        // 부스트에 필요한 타바 지불
        uint256 upgradeTavaAmount = buildingUpgradeInfo[
            upgradeMintingInfo.buildingId - 1
        ].instantUpgradeTavaAmount * upgradeMintingInfo.amount;

        require(
            IERC20(tavaToken).balanceOf(msg.sender) >= upgradeTavaAmount,
            "AltavaBuildingFarming: not enough tava token"
        );

        IERC20(tavaToken).transferFrom(
            msg.sender,
            address(this),
            upgradeTavaAmount
        );

        // minting info 갱신
        buildingUpgradeMintingInfo[landId][buildingType][
            index
        ] = buildingUpgradeMintingInfo[landId][buildingType][
            buildingUpgradeMintingInfo[landId][buildingType].length - 1
        ];
        buildingUpgradeMintingInfo[landId][buildingType].pop();

        // 민팅
        IAssetNFT(assetNFT).mintByFarmingContract(
            msg.sender,
            upgradeMintingInfo.buildingId,
            upgradeMintingInfo.amount
        );
    }

    function _retrieveLPToken(uint256 landId, uint256 buildingId) internal {
        uint256 lpAmount = getRemainChargedLPTokenAmount(landId, buildingId);
        if (lpAmount > 0) {
            ILandLPToken(lpToken).mint(msg.sender, lpAmount);
        }
    }

    function _decreaseBuildingIdNumber(
        uint256 landId,
        uint256 buildingId,
        uint256 amount
    ) internal {
        installedBuildingTypeCount[landId][
            getBuildingType(buildingId)
        ] -= amount;
        installedBuildingIdNumber[landId][buildingId] -= amount;
        if (installedBuildingIdNumber[landId][buildingId] == 0) {
            for (uint256 i = 0; i < installedBuildingIds[landId].length; i++) {
                if (installedBuildingIds[landId][i] == buildingId) {
                    installedBuildingIds[landId][i] = installedBuildingIds[
                        landId
                    ][installedBuildingIds[landId].length - 1];
                    installedBuildingIds[landId].pop();
                    break;
                }
            }
        }
    }

    /**
     * @return isBuildingDisappeared amount만큼 건물을 감소시킬때 building이 사라지는지 여부
     */
    function _checkBuildingAndSettle(
        uint256 landId,
        uint256 buildingType,
        uint256 removeAmount
    ) internal returns (bool isBuildingDisappeared) {
        // 해당 레벨 건물이 없어질 경우 claim하고 lp token 반환
        if (installedBuildingTypeCount[landId][buildingType] == removeAmount) {
            require(
                isBuildingUpgradeListEmpty(landId, buildingType),
                "AltavaBuildingFarming: building upgrading list is not empty"
            );
            if (getClaimAvailableMaterialAmount(landId, buildingType) > 0) {
                claimMaterial(landId, buildingType);
            }
            // upgrade list가 없어야지만 모든 건물을 제거할 수 있음
            _retrieveLPToken(landId, buildingType);
            delete buildingFarmingInfo[landId][buildingType];
            isBuildingDisappeared = true;
        } else {
            isBuildingDisappeared = false;
        }
    }

    function _refreshFarmingInfo(
        uint256 landId,
        uint256 buildingType
    ) internal {
        // 건물이 없어지지 않을 경우
        // LP 갱신
        if (
            getRemainChargedLPTokenAmount(landId, buildingType) >
            getMaxCharableLPAmount(landId, buildingType)
        ) {
            //최대치 초과한 경우
            uint256 retrieveAmount = getRemainChargedLPTokenAmount(
                landId,
                buildingType
            ) - getMaxCharableLPAmount(landId, buildingType);
            ILandLPToken(lpToken).mint(msg.sender, retrieveAmount);
            chargedLPAmountToBuildingType[landId][buildingType] =
                getRemainChargedLPTokenAmount(landId, buildingType) -
                retrieveAmount;
        } else {
            //최대치를 초과하지 않은 경우
            chargedLPAmountToBuildingType[landId][
                buildingType
            ] = getRemainChargedLPTokenAmount(landId, buildingType);
        }
        // farming 정보 갱신
        buildingFarmingInfo[landId][buildingType] = BuildingFarmingInfo({
            materialId: buildingFarmingInfo[landId][buildingType].materialId,
            claimableMaterialAmount: getClaimAvailableMaterialAmount(
                landId,
                buildingType
            ),
            startTimestamp: block.timestamp
        });
    }

    //land staking contract 용
    function removeAllBuildings(
        uint256 landId,
        address to
    ) public onlyStakingContract {
        uint256[] memory buildingIds = installedBuildingIds[landId];
        for (uint256 i = 0; i < buildingIds.length; i++) {
            uint256 buildingId = buildingIds[i];
            _removeBuilding(
                landId,
                buildingId,
                to,
                installedBuildingIdNumber[landId][buildingId]
            );
        }
    }

    uint256[50] private __gap;
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155HolderUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "../interfaces/ILandLPToken.sol";
import "../interfaces/IAltavaLandStaking.sol";
import "../interfaces/IAltavaBuildingFarming.sol";
import "hardhat/console.sol";

abstract contract BuildingFarmingCore is
    OwnableUpgradeable,
    ERC1155HolderUpgradeable,
    IAltavaBuildingFarming
{
    address public altavaLandNFT;
    address public assetNFT;
    address public lpToken;
    address public tavaToken;
    address public landStakingContract;

    /**
     * ==================
     * Buiding Storage
     * ==================
     */

    // material id => level =>  farmingAmountPerMillionSecond
    mapping(uint256 => mapping(uint256 => uint256)) materialFarmingAmountPerMillionSecond;
    // building id => farming LP initial info
    mapping(uint256 => BuildingFarmingLPInitialInfo) buildingFarmingLPInitialInfo;
    // building id => upgrade info
    mapping(uint256 => BuildingUpgradeInfo) buildingUpgradeInfo;
    // building type => availableMaterialIds
    mapping(uint256 => uint256[]) availableMaterialIds;
    // land Id => building type => max available number
    mapping(uint256 => mapping(uint256 => uint256)) maxAvailableBuildingInstallNumber;

    // landId =>  building type => LP amount
    mapping(uint256 => mapping(uint256 => uint256))
        public chargedLPAmountToBuildingType;
    // landId => building type => farming info
    mapping(uint256 => mapping(uint256 => BuildingFarmingInfo))
        public buildingFarmingInfo;
    // landId => building type => BuildingUpgradeMintingInfo[]
    mapping(uint256 => mapping(uint256 => BuildingUpgradeMintingInfo[])) buildingUpgradeMintingInfo;

    // landId => buildingIds
    mapping(uint256 => uint256[]) installedBuildingIds;
    // landId => bulidingId => building Number
    mapping(uint256 => mapping(uint256 => uint256)) installedBuildingIdNumber;
    // landId => building type => building type Count
    mapping(uint256 => mapping(uint256 => uint256)) installedBuildingTypeCount;
    //address => installed amount
    mapping(address => uint256) internal _installedAmountMap;

    struct BuildingFarmingLPInitialInfo {
        uint256 lpUsageAmountPerMillionSecond;
        uint256 maxChargeAvailableLPAmount;
    }

    struct BuildingFarmingInfo {
        uint256 materialId;
        uint256 startTimestamp;
        uint256 claimableMaterialAmount; //install, uinstall 시 임시 저장 => 할때 lp amount 소진, claim 하면 이거 claim하고 초기화
    }

    struct BuildingUpgradeInfo {
        uint96 period;
        uint256 lpAmount;
        uint256 instantUpgradeTavaAmount;
    }

    struct BuildingUpgradeMintingInfo {
        uint256 buildingId;
        uint256 amount;
        uint256 upgradeFinishTimestamp;
    }

    struct BuildingTokenUnit {
        uint256 tokenId;
        uint256 amount;
    }

    struct InstalledBuildingTypeInfo {
        uint256 installedCount;
        uint256 currentLPAmount;
        uint256 maxChargableLPAmount;
        uint256 farmingMaterialId;
        uint256 claimableMaterialAmount;
        uint256 lpUsageAmountPerMillionSecond;
        uint256[] availableMaterialIds;
        uint256[] materialFarmingAmountPerDay;
    }

    /**
     * ==================
     * Input Struct
     * ==================
     */

    struct BuildingFarmingAmountPerMillionSecondInput {
        uint256 materialId;
        uint256 buildingLevel;
        uint256 farmingAmountPerMillionSecond;
    }

    struct BuildingFarmingLPInitialInfoInput {
        uint256 buildingType;
        uint256 buildingLevel;
        BuildingFarmingLPInitialInfo buildingFarmingLPInitialInfo;
    }

    struct BuildingUpgradeInfoInput {
        uint256 buildingType;
        uint256 buildingLevel;
        BuildingUpgradeInfo buildingUpgradeInfo;
    }

    struct BuildingMaterialIdsInput {
        uint256 buildingType;
        uint256[] materialIds;
    }

    struct MaxAvailableBuildingInstallNumberInput {
        uint256 landType;
        uint256 buildingType;
        uint256 maxAvailableNumber;
    }

    /**
     * ==================
     * Output Struct
     * ==================
     */
    struct BuildingTypeInfoCount {
        uint256 farmingCount;
        uint256 upgradingCount;
    }

    /**
     * ==================
     * Modifier
     * ==================
     */

    modifier onlyStakingLandOwner(uint256 landId) {
        require(
            IERC721(altavaLandNFT).ownerOf(landId) == landStakingContract &&
                IAltavaLandStaking(landStakingContract).getLandOwner(landId) ==
                msg.sender,
            "BuildingFarmingCore: not land owner"
        );
        _;
    }

    modifier onlyBuildingTypeExists(uint256 landId, uint256 buildingType) {
        bool isExists = false;
        for (uint256 i = 0; i < installedBuildingIds[landId].length; i++) {
            uint256 buildingId = installedBuildingIds[landId][i];
            if (getBuildingType(buildingId) == buildingType) {
                isExists = true;
                break;
            }
        }
        require(isExists, "BuildingFarmingCore: building type not exists");
        _;
    }

    modifier onlyAvailableMaterial(uint256 buildingType, uint256 materialId) {
        uint256[] memory materialIds = availableMaterialIds[buildingType];
        bool isAvailable = false;
        for (uint256 i = 0; i < materialIds.length; i++) {
            if (materialIds[i] == materialId) {
                isAvailable = true;
                break;
            }
        }
        require(isAvailable, "BuildingFarmingCore: not available material");
        _;
    }

    modifier onlyStakingContract() {
        require(
            msg.sender == landStakingContract,
            "BuildingFarmingCore: not staking contract"
        );
        _;
    }

    /**
     * ==================
     * Initialize
     * ==================
     */

    function __BuildingFarmingCore_init(
        address _altavaLandNFT,
        address _assetNFT,
        address _lpToken,
        address _tavaToken,
        address _landStakingContract
    ) internal initializer {
        __BuildingFarmingCore_init_unchained(
            _altavaLandNFT,
            _assetNFT,
            _lpToken,
            _tavaToken,
            _landStakingContract
        );
    }

    function __BuildingFarmingCore_init_unchained(
        address _altavaLandNFT,
        address _assetNFT,
        address _lpToken,
        address _tavaToken,
        address _landStakingContract
    ) internal initializer {
        __Ownable_init();
        altavaLandNFT = _altavaLandNFT;
        assetNFT = _assetNFT;
        lpToken = _lpToken;
        tavaToken = _tavaToken;
        landStakingContract = _landStakingContract;
    }

    /**
     * ===================
     * External Functions
     * ==================
     */
    function getMaxAvailableBuildingNumber(
        uint256 landType,
        uint256 buildingType
    ) external view returns (uint256) {
        return maxAvailableBuildingInstallNumber[landType][buildingType];
    }

    function getInstalledBuildingIds(
        uint256 landId
    ) external view returns (uint256[] memory) {
        return installedBuildingIds[landId];
    }

    function getInstalledBuildingTypeCount(
        uint256 landId,
        uint256 buildingType
    ) external view returns (BuildingTypeInfoCount memory) {
        return
            BuildingTypeInfoCount(
                installedBuildingTypeCount[landId][buildingType],
                buildingUpgradeMintingInfo[landId][buildingType].length
            );
    }

    function getInstalledBuildingAmount(
        address account
    ) external view returns (uint256) {
        return _installedAmountMap[account];
    }

    function getInstalledBuildingListByType(
        uint256 landId,
        uint256 buildingType
    ) external view returns (BuildingTokenUnit[] memory) {
        uint256[] memory buildingIds = installedBuildingIds[landId];

        BuildingTokenUnit[] memory result = new BuildingTokenUnit[](
            installedBuildingTypeCount[landId][buildingType]
        );
        uint256 resultIndex = 0;
        for (uint256 i = 0; i < buildingIds.length; i++) {
            uint256 buildingId = buildingIds[i];
            if (getBuildingType(buildingId) == buildingType) {
                result[resultIndex].tokenId = buildingId;
                result[resultIndex].amount = installedBuildingIdNumber[landId][
                    buildingId
                ];
                resultIndex++;
            }
        }
        return result;
    }

    function getInstalledBuildingTypeInfo(
        uint256 landId,
        uint256 buildingType
    ) external view returns (InstalledBuildingTypeInfo memory) {
        uint256[] memory _availableMaterialIds = availableMaterialIds[
            buildingType
        ];
        uint256[] memory materialFarmingAmountPerDay = new uint256[](
            _availableMaterialIds.length
        );
        for (uint256 i = 0; i < _availableMaterialIds.length; i++) {
            uint256 totalFarmingAmountPerMillionSecond = getTotalFarmingAmountPerMillionSecond(
                    landId,
                    buildingType,
                    _availableMaterialIds[i]
                );
            materialFarmingAmountPerDay[i] =
                (totalFarmingAmountPerMillionSecond * 60 * 60 * 24) /
                1000000;
        }
        return
            InstalledBuildingTypeInfo(
                installedBuildingTypeCount[landId][buildingType],
                getRemainChargedLPTokenAmount(landId, buildingType),
                getMaxCharableLPAmount(landId, buildingType),
                buildingFarmingInfo[landId][buildingType].materialId,
                getClaimAvailableMaterialAmount(landId, buildingType),
                getTotalLPUsageAmountPerMillionSecond(landId, buildingType),
                _availableMaterialIds,
                materialFarmingAmountPerDay
            );
    }

    /**
     * ==================
     * Building Getter
     * ==================
     */

    // 0 : Toolkit,
    // 1 : Natural Fiber,
    // 2 : Synthetic Fiber,
    // 3 : Supplies,
    function getBuildingType(uint256 buildingId) public pure returns (uint256) {
        require(
            buildingId != 0 && buildingId < 101,
            "BuildingFarmingCore: invalid buildingId"
        );
        return ((buildingId - 1) / 25);
    }

    function getBuildingLevel(
        uint256 buildingId
    ) public pure returns (uint256) {
        require(
            buildingId != 0 && buildingId < 101,
            "BuildingFarmingCore: invalid buildingId"
        );
        return ((buildingId - 1) % 25) + 1;
    }

    function getCharableLPAmount(
        uint256 landId,
        uint256 buildingType
    ) public view returns (uint256) {
        return
            getMaxCharableLPAmount(landId, buildingType) -
            getRemainChargedLPTokenAmount(landId, buildingType);
    }

    function getMaxFarmingAvailableMaterialAmount(
        uint256 landId,
        uint256 buildingType
    ) public view returns (uint256) {
        uint256 finishTimestamp = getFarmingFinishTimestamp(
            landId,
            buildingType
        );

        if (finishTimestamp == 0) {
            return 0;
        }

        uint256 farmingDuration = finishTimestamp -
            buildingFarmingInfo[landId][buildingType].startTimestamp;

        uint256 materialId = buildingFarmingInfo[landId][buildingType]
            .materialId;
        if (materialId == 0) {
            return 0;
        }
        return
            (farmingDuration *
                getTotalFarmingAmountPerMillionSecond(
                    landId,
                    buildingType,
                    materialId
                )) / 1000000;
    }

    function getFarmingFinishTimestamp(
        uint256 landId,
        uint256 buildingType
    ) public view returns (uint256) {
        BuildingFarmingInfo memory farmingInfo = buildingFarmingInfo[landId][
            buildingType
        ];
        if (farmingInfo.startTimestamp == 0) {
            return 0;
        }
        uint256 materialId = farmingInfo.materialId;
        require(
            materialId != 0,
            "BuildingFarmingCore: farming is not activated"
        );
        uint256 totalLPUsageAmountPerMillionSecond = getTotalLPUsageAmountPerMillionSecond(
                landId,
                buildingType
            );
        uint256 availableFarmingTime = (chargedLPAmountToBuildingType[landId][
            buildingType
        ] * 1000000) / totalLPUsageAmountPerMillionSecond;

        return farmingInfo.startTimestamp + availableFarmingTime;
    }

    function getUsingCharedLPTokenAmount(
        uint256 landId,
        uint256 buildingType
    ) public view returns (uint256) {
        uint256 finishTimestamp;
        BuildingFarmingInfo memory farmingInfo = buildingFarmingInfo[landId][
            buildingType
        ];

        // farming이 끝났으면 farming이 끝난 시간으로 계산
        if (getFarmingFinishTimestamp(landId, buildingType) < block.timestamp) {
            finishTimestamp = getFarmingFinishTimestamp(landId, buildingType);
        } else {
            finishTimestamp = block.timestamp;
        }
        // 아직 시작 안했거나 finishtimestamp가 0인 경우 0 리턴
        if (
            finishTimestamp < farmingInfo.startTimestamp ||
            farmingInfo.startTimestamp == 0
        ) {
            return 0;
        }

        uint256 totalLPUsageAmountPerMillionSecond = getTotalLPUsageAmountPerMillionSecond(
                landId,
                buildingType
            );

        return
            ((finishTimestamp - farmingInfo.startTimestamp) *
                totalLPUsageAmountPerMillionSecond) / 1000000;
    }

    function getClaimAvailableMaterialAmount(
        uint256 landId,
        uint256 buildingType
    ) public view returns (uint256) {
        uint256 usingLPAmount = getUsingCharedLPTokenAmount(
            landId,
            buildingType
        );
        // 사용한 LP가 없으면 0리턴
        if (usingLPAmount == 0) {
            return 0;
        }
        uint256 materialId = buildingFarmingInfo[landId][buildingType]
            .materialId;
        if (materialId == 0) {
            return 0;
        }
        uint256 totalFarmingAmountPerMillionSecond = getTotalFarmingAmountPerMillionSecond(
                landId,
                buildingType,
                materialId
            );
        // 파밍중인 양이 없으면 0리턴
        if (totalFarmingAmountPerMillionSecond == 0) {
            return 0;
        }
        uint256 totalLPUsageAmountPerMillionSecond = getTotalLPUsageAmountPerMillionSecond(
                landId,
                buildingType
            );
        uint256 farmingAmount = (totalFarmingAmountPerMillionSecond *
            usingLPAmount) / totalLPUsageAmountPerMillionSecond;
        return
            farmingAmount +
            buildingFarmingInfo[landId][buildingType].claimableMaterialAmount;
    }

    function getRemainChargedLPTokenAmount(
        uint256 landId,
        uint256 buildingType
    ) public view returns (uint256) {
        return
            chargedLPAmountToBuildingType[landId][buildingType] -
            getUsingCharedLPTokenAmount(landId, buildingType);
    }

    function getFarmingInfo(
        uint256 landId,
        uint256 buildingType
    ) public view returns (BuildingFarmingInfo memory) {
        return buildingFarmingInfo[landId][buildingType];
    }

    function getAvailableBuildingSlot(
        uint256 landId,
        uint256 landType,
        uint256 buildingType
    ) public view returns (uint256) {
        return (maxAvailableBuildingInstallNumber[landType][buildingType] -
            installedBuildingTypeCount[landId][buildingType]);
    }

    function getUpgradePeriod(
        uint256 buildingId
    ) public view returns (uint256) {
        return buildingUpgradeInfo[buildingId].period;
    }

    function getUpgradeAvailableIndexs(
        uint256 landId,
        uint256 buildingType
    ) public view returns (uint256[] memory) {
        BuildingUpgradeMintingInfo[]
            memory upgradeMintingInfos = buildingUpgradeMintingInfo[landId][
                buildingType
            ];
        uint256[] memory availableIndexs = new uint256[](
            upgradeMintingInfos.length
        );
        uint256 availableIndexCount = 0;
        for (uint256 i = 0; i < upgradeMintingInfos.length; i++) {
            if (
                upgradeMintingInfos[i].upgradeFinishTimestamp <= block.timestamp
            ) {
                availableIndexs[availableIndexCount] = i;
                availableIndexCount++;
            }
        }
        uint256[] memory result = new uint256[](availableIndexCount);
        for (uint256 i = 0; i < availableIndexCount; i++) {
            result[i] = availableIndexs[i];
        }
        return result;
    }

    function getBuildingUpgradeInfos(
        uint256 landId,
        uint256 buildingType
    ) public view returns (BuildingUpgradeMintingInfo[] memory) {
        return buildingUpgradeMintingInfo[landId][buildingType];
    }

    function getMaxCharableLPAmount(
        uint256 landId,
        uint256 buildingType
    ) public view returns (uint256) {
        uint256 maxChargeAvailableLPAmount = 0;
        for (uint256 i = 0; i < installedBuildingIds[landId].length; i++) {
            uint256 buildingId = installedBuildingIds[landId][i];
            if (getBuildingType(buildingId) != buildingType) continue;
            maxChargeAvailableLPAmount += buildingFarmingLPInitialInfo[
                buildingId
            ].maxChargeAvailableLPAmount;
        }
        return maxChargeAvailableLPAmount;
    }

    function isBuildingUpgradeListEmpty(
        uint256 landId,
        uint256 buildingType
    ) public view returns (bool) {
        return buildingUpgradeMintingInfo[landId][buildingType].length == 0;
    }

    function getAvailableMaterials(
        uint256 buildingId
    ) external view returns (uint256[] memory) {
        return availableMaterialIds[getBuildingType(buildingId)];
    }

    function getTotalFarmingAmountPerMillionSecond(
        uint256 landId,
        uint256 buildingType,
        uint256 materialId
    ) public view returns (uint256) {
        //farming is not activated
        if (materialId == 0) {
            return 0;
        }
        uint256 totalFarmingAmountPerMillionSecond = 0;
        for (uint256 i = 0; i < installedBuildingIds[landId].length; i++) {
            uint256 buildingId = installedBuildingIds[landId][i];
            if (getBuildingType(buildingId) != buildingType) continue;
            totalFarmingAmountPerMillionSecond += (installedBuildingIdNumber[
                landId
            ][buildingId] *
                materialFarmingAmountPerMillionSecond[materialId][
                    getBuildingLevel(buildingId)
                ]);
        }
        return totalFarmingAmountPerMillionSecond;
    }

    function getTotalLPUsageAmountPerMillionSecond(
        uint256 landId,
        uint256 buildingType
    ) public view returns (uint256) {
        uint256 totalLPUsageAmountPerMillionSecond = 0;
        for (uint256 i = 0; i < installedBuildingIds[landId].length; i++) {
            uint256 buildingId = installedBuildingIds[landId][i];
            if (getBuildingType(buildingId) != buildingType) continue;
            totalLPUsageAmountPerMillionSecond += (installedBuildingIdNumber[
                landId
            ][buildingId] *
                buildingFarmingLPInitialInfo[buildingId]
                    .lpUsageAmountPerMillionSecond);
        }
        return totalLPUsageAmountPerMillionSecond;
    }

    /**
     * ==================
     * Building Staking Setting
     * ==================
     */

    function setBuildingFarmingAmountPerMillionSecond(
        BuildingFarmingAmountPerMillionSecondInput[] calldata inputs
    ) public onlyOwner {
        for (uint256 i = 0; i < inputs.length; i++) {
            materialFarmingAmountPerMillionSecond[inputs[i].materialId][
                inputs[i].buildingLevel
            ] = inputs[i].farmingAmountPerMillionSecond;
        }
    }

    function setBuildingFarmingLPInitialInfo(
        BuildingFarmingLPInitialInfoInput[] calldata inputs
    ) public onlyOwner {
        for (uint256 i = 0; i < inputs.length; i++) {
            buildingFarmingLPInitialInfo[
                inputs[i].buildingType * 25 + inputs[i].buildingLevel + 1
            ] = inputs[i].buildingFarmingLPInitialInfo;
        }
    }

    function setBuildingUpgradeInfo(
        BuildingUpgradeInfoInput[] calldata inputs
    ) public onlyOwner {
        for (uint256 i = 0; i < inputs.length; i++) {
            buildingUpgradeInfo[
                inputs[i].buildingType * 25 + inputs[i].buildingLevel + 1
            ] = inputs[i].buildingUpgradeInfo;
        }
    }

    function setBulidingMaterialIds(
        BuildingMaterialIdsInput[] calldata inputs
    ) public onlyOwner {
        for (uint256 i = 0; i < inputs.length; i++) {
            availableMaterialIds[inputs[i].buildingType] = inputs[i]
                .materialIds;
        }
    }

    function setMaxAvailableBuildingNumber(
        MaxAvailableBuildingInstallNumberInput[] memory inputs
    ) public onlyOwner {
        for (uint256 i = 0; i < inputs.length; i++) {
            maxAvailableBuildingInstallNumber[inputs[i].landType][
                inputs[i].buildingType
            ] = inputs[i].maxAvailableNumber;
        }
    }

    // function setLandStakingContract(
    //     address _landStakingContract
    // ) public onlyOwner {
    //     landStakingContract = _landStakingContract;
    // }

    function ownerFunction() public onlyOwner {
        for (uint256 i = 0; i < installedBuildingIds[15].length; i++) {
            uint256 buildingId = installedBuildingIds[15][i];
            if (buildingId == 0) {
                installedBuildingIds[15][i] = installedBuildingIds[15][
                    installedBuildingIds[15].length - 1
                ];
                installedBuildingIds[15].pop();
                i--;
            }
        }
    }

    receive() external payable {}

    /**
     * ========================
     * Owner Functions
     * ========================
     */
    function tavaWithdraw(uint256 amount) public onlyOwner {
        IERC20(tavaToken).transfer(msg.sender, amount);
    }

    uint256[33] private __gap;
}

