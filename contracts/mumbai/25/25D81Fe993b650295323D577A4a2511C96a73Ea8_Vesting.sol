pragma solidity ^0.8.18;

import "lib/openzeppelin-contracts/contracts/token/ERC20/ERC20.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC20/utils/SafeERC20.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC20/IERC20.sol";
import "lib/openzeppelin-contracts/contracts/security/ReentrancyGuard.sol";
import "lib/openzeppelin-contracts/contracts/access/Ownable.sol";
import "lib/openzeppelin-contracts/contracts/utils/math/Math.sol";

import "../TilttVault.sol";

contract Vesting is Ownable, ReentrancyGuard {
    using SafeERC20 for ERC20;
    using Math for uint256;

    error InvalidOptIn(
        address user,
        uint256 tierId,
        uint256 totalAmount,
        uint256 requiredAssetsFromMembership
    );

    uint256 public constant PRESEED = 0;
    uint256 public constant SEED = 1;
    uint256 public constant PRIVATESALE = 2;
    uint256 public constant PRESALE = 3;
    uint256 public constant PUBLIC = 4;
    uint256 public constant TEAM = 5;
    uint256 public constant SPONSORSHIP = 6;

    struct SaleRound{
        uint256 atTGE;
        uint256 cliff;
        uint256 vestingDuration;
    }

    struct Beneficiary {
        SaleRound saleRound;
        uint256 amountTotal;
        uint256 released;
    }

    mapping (address => Beneficiary) private beneficiaries;
    uint256 public start;
    mapping(uint256 => SaleRound) saleRounds;

    // average 1 month duration
    uint256 private constant ONE_MONTH = 2629744;
    // 7889232
    uint256 private constant THREE_MONTHS = 3 * ONE_MONTH;
    // 15778464
    uint256 private constant SIX_MONTHS = 6 * ONE_MONTH;
    // 23667696
    uint256 private constant NINE_MONTHS = 9 * ONE_MONTH;
    // 31556928
    uint256 private constant TWELVE_MONTHS = 12 * ONE_MONTH;
    // 63113856
    uint256 private constant TWENTYFOUR_MONTHS = 24 * ONE_MONTH;

    IERC20 private immutable _token;

    TilttVault private immutable _vault;

    bool private initFlag;

    constructor(address token, address vault, uint256 _start){

        require(token != address(0), "ZERO_TOKEN_ADDRESS");
        require(vault != address(0), "ZERO_VAULT_ADDRESS");
        _token = ERC20(token);
        _vault = TilttVault(vault);

        start = _start == 0 ? block.timestamp : _start;
        
        // Sale Round ID 0
        saleRounds[PRESEED].atTGE = 5;
        saleRounds[PRESEED].cliff = SIX_MONTHS;
        saleRounds[PRESEED].vestingDuration = TWENTYFOUR_MONTHS;

        // Sale Round ID 1
        saleRounds[SEED].atTGE = 5;
        saleRounds[SEED].cliff = SIX_MONTHS;
        saleRounds[SEED].vestingDuration = TWENTYFOUR_MONTHS;

        // Sale Round ID 2
        saleRounds[PRIVATESALE].atTGE = 5;
        saleRounds[PRIVATESALE].cliff = THREE_MONTHS;
        saleRounds[PRIVATESALE].vestingDuration = TWELVE_MONTHS;

        // Sale Round ID 3
        saleRounds[PRESALE].atTGE = 5;
        saleRounds[PRESALE].cliff = THREE_MONTHS;
        saleRounds[PRESALE].vestingDuration = NINE_MONTHS;

        // Sale Round ID 4
        saleRounds[PUBLIC].atTGE = 100;
        saleRounds[PUBLIC].cliff = 0;
        saleRounds[PUBLIC].vestingDuration = 0;

        // Sale Round ID 5
        saleRounds[TEAM].atTGE = 0;
        saleRounds[TEAM].cliff = SIX_MONTHS;
        saleRounds[TEAM].vestingDuration = TWENTYFOUR_MONTHS;

        // Sale Round ID 6
        saleRounds[SPONSORSHIP].atTGE = 5;
        saleRounds[SPONSORSHIP].cliff = 0;
        saleRounds[SPONSORSHIP].vestingDuration = TWELVE_MONTHS;

    }

    // GETTERS
    function getToken() external view returns (address) {
        return address(_token);
    }

    function getVestingSchedule(address user) external view returns (Beneficiary memory) {
        return beneficiaries[user];
    }

    function getSaleRound(uint256 id) external view returns (SaleRound memory) {
        return saleRounds[id];
    }

    function getInitFlag() external view returns (bool) {
        return initFlag;
    }

    // SETTERS
    function setStart(uint256 _start) external onlyOwner {
        require(_start > start, "NEW_VALUE_HAS_TO_BE_GREATER");
        start = _start;
    }

    function createVestingSchedule(address[] calldata addresses, 
                                   uint256[] calldata _saleRoundIds, 
                                   uint256[] calldata totalAmounts, 
                                   uint256[] calldata tierIds,
                                   uint256 amount
                                   ) external onlyOwner {
        require(initFlag == false, "VESTING_SCHEDULE_HAS_BEEN_CREATED_BEFORE");
        uint256 numberOfUsers = addresses.length;
        
        require(numberOfUsers == _saleRoundIds.length && 
                numberOfUsers == totalAmounts.length && 
                numberOfUsers == tierIds.length , "ARRAY_LENGTH_MISMATCH");
        
        uint256 totalAmountsSum;
        for (uint i = 0; i < numberOfUsers; i++) {
            totalAmountsSum += totalAmounts[i];
        }
        require(amount >= totalAmountsSum, "NOT_ENOUGH_FUNDS");
        
        SafeERC20.safeTransferFrom(_token, msg.sender, address(this), amount);

        for (uint i = 0; i < numberOfUsers; i++) {
            require(addresses[i] != address(0), "ZERO_ADDRESS");
            require(_saleRoundIds[i] <= 6, "INVALID_SALEROUNDID");
            require(totalAmounts[i] > 0, "ZERO_AMOUNT");
            require(tierIds[i] < 6 || tierIds[i] == 93, "INVALID_TIERID");


            SaleRound memory saleRound = saleRounds[_saleRoundIds[i]];
            beneficiaries[addresses[i]] = Beneficiary(saleRound, totalAmounts[i], 0);

            // Users who do not opt in have the tier ID 93
            if(tierIds[i] != 93){
                _optin(addresses[i], tierIds[i], saleRound, totalAmounts[i]);
            }

        }
        initFlag = true;
    }

    function _optin(address userAddress, uint256 tierId, SaleRound memory saleRound, uint256 totalAmount) internal {
        // the tier fees are 200 TLT, 1000 TLT and 5000 TLT
        // 200 = 200*5^0
        // 1000 = 200*5^1
        // 5000 = 200*5^2
        uint256 requiredAssetsForMembership = (5 ** ((5 - tierId) / 2)) * 200 * 1e18;
        // users can only opt in if they will have sufficient funds for their selected membership
        if(totalAmount >= requiredAssetsForMembership){
            uint256 _duration;
            // The time required for a user to have their membership covered from their vested tokens can be described by the following equation
            // membership_fee = tokens_from_TGE + tokens_from_linear_vesting
            // more precisely: requiredAssetsForMembership = (totalAmount * TGE / 100) + [totalAmount * (100 - TGE) / 100 * _duration / vestingDuration]
            // if we solve this equation for _duration, we get the equation that can be seen below
            // it is possible that the TGE covers the membership fee, in this case the added lockup is 0
            if(100 * requiredAssetsForMembership > totalAmount * saleRound.atTGE){
                _duration = (100 * saleRound.vestingDuration * requiredAssetsForMembership - saleRound.vestingDuration * totalAmount * saleRound.atTGE) / (totalAmount * (100 - saleRound.atTGE));
            }
            else{
                _duration = 0;
            }
            beneficiaries[userAddress].released = requiredAssetsForMembership;
            _token.approve(address(_vault), requiredAssetsForMembership);
            _vault.vestingDeposit(userAddress, tierId, requiredAssetsForMembership, _duration);
        }
        // If they don't have sufficient fund the opt in is skipped and en event is emitted
        else {
            revert InvalidOptIn(userAddress, tierId, totalAmount, requiredAssetsForMembership);
        }

    }

    function release() public nonReentrant {
        uint256 releasableAmount = computeReleasableAmount(msg.sender);
        require(releasableAmount > 0, "NO_RELEASABLE_AMOUNT");

        beneficiaries[msg.sender].released += releasableAmount;

        SafeERC20.safeTransfer(_token, msg.sender, releasableAmount);
    }

    function computeReleasableAmount(address user) public view returns (uint256) {
        uint256 currentTime = block.timestamp;
        Beneficiary memory beneficiary = beneficiaries[user];

        // Before start the user should't be able to redeem anything
        if (currentTime < start) {
            return 0;
        // after start but before cliff the user can only redeem the amount from TGE minus the amount used for their membership if they opted in
        } else if (currentTime <= start + beneficiary.saleRound.cliff) {
            if(beneficiary.released > beneficiary.amountTotal * beneficiary.saleRound.atTGE / 100){
                return 0;
            } 
            return beneficiary.amountTotal * beneficiary.saleRound.atTGE / 100 - beneficiary.released;
        // after the end of the vesting duration the user can redeem the whole amount minus the already redeemed amounts
        } else if (currentTime >= start + beneficiary.saleRound.cliff + beneficiary.saleRound.vestingDuration) {
            return beneficiary.amountTotal - beneficiary.released;
        // After the cliff but before the end of the vesting duration 
        // the user should be able to redeem the amount from TGE plus the proportional amount from the linear vesting minus the already redeemed amounts
        } else {
            uint256 timeSinceVestingStarted = currentTime - (start + beneficiary.saleRound.cliff) + 1;
            uint256 releasedAtTGE = beneficiary.amountTotal * beneficiary.saleRound.atTGE / 100;
            // If the user opted in it is possible that the released amount is higher than their vested amount so we have to return 0
            if(releasedAtTGE + (beneficiary.amountTotal - releasedAtTGE) * timeSinceVestingStarted / beneficiary.saleRound.vestingDuration < beneficiary.released){
                return 0;
            } 
            return releasedAtTGE + (beneficiary.amountTotal - releasedAtTGE) * timeSinceVestingStarted / beneficiary.saleRound.vestingDuration - beneficiary.released;
        }
    }
}