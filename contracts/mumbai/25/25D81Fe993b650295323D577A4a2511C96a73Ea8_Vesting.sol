pragma solidity ^0.8.18;

import "lib/openzeppelin-contracts-upgradeable/contracts/access/AccessControlUpgradeable.sol";
import "lib/openzeppelin-contracts-upgradeable/contracts/proxy/utils/UUPSUpgradeable.sol";
import "lib/openzeppelin-contracts/contracts/utils/math/Math.sol";
import "lib/openzeppelin-contracts/contracts/utils/cryptography/ECDSA.sol";
import "./MembershipHandler.sol";
import "./interfaces/ITLTPriceOracle.sol";
import "./TilttVault.sol";

contract VaultHelpers is Initializable, AccessControlUpgradeable, UUPSUpgradeable {
    using ECDSA for bytes32;
    using Math for uint256;

    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");

    TilttVault public tilttVault;

    MembershipHandler public membershipHandler;

    address private _whitelistSignerAddress;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function _authorizeUpgrade(address newImplementation) internal onlyRole(UPGRADER_ROLE) override{}

    function initialize(address _vaultAddress, address whitelistSignerAddress, address membershipHandlerAddress) initializer public {
        require(_vaultAddress != address(0), "NULL_ADDRESS");
        require(whitelistSignerAddress != address(0), "NULL_ADDRESS");
        require(membershipHandlerAddress != address(0), "NULL_ADDRESS");
        tilttVault = TilttVault(_vaultAddress); 
        _whitelistSignerAddress = whitelistSignerAddress;
        membershipHandler = MembershipHandler(membershipHandlerAddress);

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);

    }

    // SETTERS
    function setVault(address vaultAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(vaultAddress != address(0), "ZERO_VAULT_ADDRESS");
        tilttVault = TilttVault(vaultAddress);    
    }

    function setWhiteListerAddress(address whitelistSignerAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(whitelistSignerAddress != address(0), "ZERO_WHITELISTER_ADDRESS");
        _whitelistSignerAddress = whitelistSignerAddress;  
    }

    // MODIFIERS

    function checkWhitelist(bytes calldata signature, address user) external view virtual {
        bytes32 messageHash = keccak256(abi.encodePacked(user));
        require(_whitelistSignerAddress == messageHash.toEthSignedMessageHash()
                .recover(signature), "Invalid whitelist");       
    }

    function checkFreeSpaces(uint256 tierId) external view virtual {
        Tiers tier = getCommonIdentifier(tierId);
        require(tilttVault.freeSpaces(tier) > 0 || tier == Tiers.Bronze, "No free spaces for the selected tier");
    }

    // GETTERS

    function getCommonIdentifier(uint256 tierId) public pure returns(Tiers){
        if (tierId == PLATINUM_SHORT || tierId == PLATINUM_LONG){
            return Tiers.Platinum;
        } else if (tierId == GOLD_SHORT || tierId == GOLD_LONG){
            return Tiers.Gold;
        } else if (tierId == SILVER_SHORT || tierId == SILVER_LONG){
            return Tiers.Silver;
        }
        return Tiers.Bronze;
    }

    function getTierFreeSpaces(uint256 tierId) external view returns(uint256){
        return tilttVault.freeSpaces(getCommonIdentifier(tierId));
    }

    // ACCOUNTING LOGIC

    function totalAssets(uint256 tierId) public view virtual returns (uint256) {
        return tilttVault.getTier(tierId).assetBalance;
    }

    function totalSupply(uint256 tierId) public view virtual returns (uint256) {
        return tilttVault.getTier(tierId).totalSupply;
    }

    function convertToShares(uint256 tierId, uint256 assets) external view virtual returns (uint256) {
        return _convertToShares(tierId, assets, Math.Rounding.Down);
    }

    function convertToAssets(uint256 tierId, uint256 shares) public view virtual returns (uint256) {
        return _convertToAssets(tierId, shares, Math.Rounding.Down);
    }

    function previewDeposit(uint256 tierId, uint256 assets) public view virtual returns (uint256) {
        return _convertToShares(tierId, assets, Math.Rounding.Down);
    }

    function previewMint(uint256 tierId, uint256 shares) external view virtual returns (uint256) {
        return _convertToAssets(tierId, shares, Math.Rounding.Up);
    }

    function previewWithdraw(uint256 tierId, uint256 assets) external view virtual returns (uint256) {
        return _convertToShares(tierId, assets, Math.Rounding.Up);    
    }

    function previewRedeem(uint256 tierId, uint256 shares) external view virtual returns (uint256) {
        return _convertToAssets(tierId, shares, Math.Rounding.Down);
    }

    function maxWithdraw(address account, uint256 tierId) external view returns (uint256){
        uint256 shares = tilttVault.balanceOf(account, tierId);
        uint256 userTotalAssets = convertToAssets(tierId, shares);
        uint256 userTotalAssetsUsdcValue = tilttVault.tltOracle().estimateAmountOut(tilttVault.asset(), uint128(userTotalAssets), tilttVault.oracleTwapDuration());
        uint256 tierCost = tilttVault.getTier(tierId).tierCost;

        if(userTotalAssetsUsdcValue > tierCost){
            uint256 withdrawableUsdcvalue = userTotalAssetsUsdcValue - tierCost;
            uint256 withdrawableTlt = tilttVault.tltOracle().estimateAmountOut(address(membershipHandler._usdc()), uint128(withdrawableUsdcvalue), tilttVault.oracleTwapDuration());
            return withdrawableTlt;
        }
        return 0;
    }

    function previewYield(address account) external view returns (uint256) {
        if (membershipHandler.hasMembership(account)){
            Membership memory membership = membershipHandler.getMembership(account);    
            uint256 userAssets = convertToAssets(membership.tier, tilttVault.balanceOf(account, membership.tier));
            if(userAssets <= membership.balance) return 0;
            uint256 yield = convertToAssets(membership.tier, tilttVault.balanceOf(account, membership.tier)) - membership.balance;
            return yield;
        }
        revert("Account not member");
    }

    function estimateTier(uint128 tltAmount, bool isShortLockup) external view returns (uint256) {
        uint256 assetsUsdcValue = tilttVault.tltOracle().estimateAmountOut(tilttVault.asset(), uint128(tltAmount), tilttVault.oracleTwapDuration());
        if (assetsUsdcValue >= tilttVault.platinumCost()){
            return isShortLockup ? PLATINUM_SHORT : PLATINUM_LONG;
        } else if(assetsUsdcValue >= tilttVault.goldCost()){
            return isShortLockup ? GOLD_SHORT : GOLD_LONG;
        } else if(assetsUsdcValue >= tilttVault.silverCost()){
            return isShortLockup ? SILVER_SHORT : SILVER_LONG;
        }
        return BRONZE;
    }

    // Function used to simulate reward distribution
    function checkRewards(uint256 reward) public view returns(uint256[] memory) {
        uint weightedSumOfAssets;
        uint256 numberOfTiers = 7;
        uint256[] memory rewardForAllTiers = new uint256[](numberOfTiers);
        for (uint256 i = 0; i < numberOfTiers; i++) {
            weightedSumOfAssets += totalAssets(i) * tilttVault.getTier(i).rewardMultiplier;
        }
        for (uint256 i = 0; i < numberOfTiers; i++) {
            uint256 rewardForTier;
            rewardForTier = reward * (totalAssets(i) * tilttVault.getTier(i).rewardMultiplier) / weightedSumOfAssets;
            rewardForAllTiers[i] = rewardForTier;
        }
        return rewardForAllTiers;
    }

    function checkRewardsWithMultiplier(uint256 reward, uint256[7] memory rewardMultipliers) public view returns(uint256[] memory) {
        uint weightedSumOfAssets;
        uint256 numberOfTiers = 7;
        uint256[] memory rewardForAllTiers = new uint256[](numberOfTiers);
        for (uint256 i = 0; i < numberOfTiers; i++) {
            weightedSumOfAssets += totalAssets(i) * rewardMultipliers[i];
        }
        for (uint256 i = 0; i < numberOfTiers; i++) {
            uint256 rewardForTier;
            rewardForTier = reward * (totalAssets(i) * rewardMultipliers[i]) / weightedSumOfAssets;
            rewardForAllTiers[i] = rewardForTier;
        }
        return rewardForAllTiers;
    }
    

    // INTERNAL FUNCTIONS

    function _convertToShares(uint256 tierId, uint256 assets, Math.Rounding rounding) internal view virtual returns (uint256) {
        uint256 supply = totalSupply(tierId);
        return
            (assets == 0 || supply == 0)
                ? assets
                : assets.mulDiv(supply, totalAssets(tierId), rounding);
    }

    function _convertToAssets(uint256 tierId, uint256 shares, Math.Rounding rounding) internal view virtual returns (uint256) {
        uint256 supply = totalSupply(tierId);
        return
            (supply == 0) 
                ? shares 
                : shares.mulDiv(totalAssets(tierId), supply, rounding);
    }
}

