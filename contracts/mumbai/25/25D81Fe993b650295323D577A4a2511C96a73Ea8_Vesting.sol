pragma solidity ^0.8.18;

import "lib/openzeppelin-contracts-upgradeable/contracts/access/AccessControlUpgradeable.sol";
import "lib/openzeppelin-contracts-upgradeable/contracts/proxy/utils/UUPSUpgradeable.sol";
import "./tokens/OGSoulboundToken.sol";
import "./VaultHelpers.sol";
import "./TilttVault.sol";

struct Membership {
    uint256 tier;
    uint256 balance; // the sum of user's deposited assets (in case of partial withdraw, the balance will be the shares value in asset)
    uint256 withdrawableBalance; // used when membership gets inactive and we have to store the withdrawabale balance at expiration time
    uint256 lockupEndTs; // the end of the  membership lockup (blokc.timestamp at staking time + lockupPeriod)
    uint256 vestingEnd;
    uint256 expirationBufferEndTs; // lockupEndTs + bufferTime (1 week)
    bool isShortLockup;
    bool isOG;
}

contract MembershipHandler is Initializable, AccessControlUpgradeable, UUPSUpgradeable {

    event MakeOG(
        address indexed caller,
        address indexed userAddress
    );

    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
    bytes32 public constant YEET_ROLE = keccak256("YEET_ROLE");
    bytes32 public constant OG_ADMIN_ROLE = keccak256("OG_ADMIN_ROLE");
    uint256 public constant ONE_WEEK = 604800;

    mapping(address => Membership) private _memberships;

    TilttVault public vault;

    OGSoulboundToken public OgToken;
    VaultHelpers public vaultHelpers;

    uint256 bufferTime;

    address public _usdc;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function _authorizeUpgrade(address newImplementation) internal onlyRole(UPGRADER_ROLE) override{}

    function initialize(address vaultAddress, address usdc) initializer public {
        require(vaultAddress != address(0), "NULL_ADDRESS");
        require(usdc != address(0), "NULL_ADDRESS");

        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);

        vault = TilttVault(vaultAddress);
        _usdc = usdc;

        bufferTime = ONE_WEEK;
    }

    // SETTERS

    function setVaultHelpers(address _vaultHelpers) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_vaultHelpers != address(0), "ZERO_VAULT_HELPERS_ADDRESS");
        vaultHelpers = VaultHelpers(_vaultHelpers);
    }

    function setOgToken(address _ogTokenAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_ogTokenAddress != address(0), "ZERO_ADDRESS");
        OgToken = OGSoulboundToken(_ogTokenAddress);
    }

    // ACCESS CONTROL

    modifier onlyVault() {
        _checkVault();
        _;
    }

    modifier onlyIfFreeSpaces(uint256 tierId){
        vaultHelpers.checkFreeSpaces(tierId);
        _;
    }

    function _checkVault() internal view {
        require(msg.sender == address(vault), "Caller is not the Vault");
    }

    // CREATE

    function createMembership(address newMember, uint256 _tier, uint256 _balance, uint256 _lockupEndTs, bool _isShortLockup, uint256 _vestingEnd) external onlyVault {
        require(_memberships[newMember].balance == 0, "User already member");
        require(!_memberships[newMember].isOG, "OG User already exists");
        _memberships[newMember] = Membership({
            tier: _tier, 
            balance: _balance, 
            withdrawableBalance: 0, 
            lockupEndTs: _lockupEndTs, 
            expirationBufferEndTs: _lockupEndTs + bufferTime, 
            vestingEnd: _vestingEnd,
            isShortLockup: _isShortLockup, 
            isOG: false});
    }

    // TODO if someone becomes an OG later on we have to set the expirationBufferEndTs in the db as well
    function createOGMembership(address newMember) external onlyRole(OG_ADMIN_ROLE) {
        require(_memberships[newMember].balance == 0 && !_memberships[newMember].isOG, "User already member");
        _memberships[newMember] = Membership({
            tier: 1, 
            balance: 0, 
            withdrawableBalance: 0, 
            lockupEndTs: type(uint256).max, 
            expirationBufferEndTs: type(uint256).max, 
            vestingEnd: 0,
            isShortLockup: false, 
            isOG: true});
        OgToken.safeMint(newMember);
    }

    // GETTERS

    function getMembership(address member) external view returns (Membership memory) {
        return _memberships[member];
    }

    function getMembershipTier(address member) external view returns (uint256) {
        return _memberships[member].tier;
    }

    function getMembershipLockupEnd(address member) external view returns (uint256) {
        return _memberships[member].lockupEndTs;
    }
    
    function getMembershipBalance(address member) external view returns(uint256) {
        return _memberships[member].balance;
    }

    function hasMembership(address member) external view returns (bool) {
        require(_memberships[member].withdrawableBalance == 0, "Membership expired");
        return _memberships[member].balance > 0 || _memberships[member].isOG;
    }

    function getVestingEnd(address member) external view returns (uint256) {
        return _memberships[member].vestingEnd;
    }

    // MUTATORS

    function setExpirationBufferTime(uint256 timeInSeconds) external onlyRole(DEFAULT_ADMIN_ROLE) {
        bufferTime = timeInSeconds;
    }

    function setNewMembershipBalance(address member, uint256 amount) external onlyVault {
        _memberships[member].balance = amount;
    }

    function addAssetsToMembershipBalance(address member, uint256 amount) external onlyVault {
        _memberships[member].balance += amount;
    }

    function removeAssetsFromMembershipBalance(address member, uint256 amount) external onlyVault {
        _memberships[member].balance -= amount;  
    }

    // INTERNAL MUTATORS

    function _setExpiredMembershipBalance(address member, uint256 amount) internal {
        _memberships[member].balance = 0;
        _memberships[member].withdrawableBalance = amount;
    }

    function _deleteMembership(address member) internal {
        delete _memberships[member];
    }

    function _extendMembership(address member, uint256 tierId, uint256 _lockupEndTs, bool _isShortLockup) internal {
        _memberships[member].tier = tierId;
        _memberships[member].lockupEndTs = _lockupEndTs;
        _memberships[member].expirationBufferEndTs = _lockupEndTs + bufferTime;
        _memberships[member].isShortLockup = _isShortLockup;
    }

    function _extendMembershipAndSetBalance(address member, uint256 tierId, uint256 amount, uint256 _lockupEndTs, bool _isShortLockup) internal {
        _memberships[member].tier = tierId;
        _memberships[member].balance = amount;
        _memberships[member].lockupEndTs = _lockupEndTs;
        _memberships[member].expirationBufferEndTs = _lockupEndTs + bufferTime;
        _memberships[member].isShortLockup = _isShortLockup;
    }

    function _updateMembershipTierAndBalance(address member, uint256 newTier, uint256 amount) internal {
        _memberships[member].tier = newTier;
        _memberships[member].balance = amount;
    }

    function _makeMembershipOG(address member) internal {
        _memberships[member].isOG = true;
        _memberships[member].isShortLockup = false;
        _memberships[member].lockupEndTs = type(uint256).max;
        OgToken.safeMint(member);
    }

    // MEMBERSHIP HANDLING LOGIC

    function cancelMembership() public returns (uint256) {
        Membership memory membership = _memberships[msg.sender];
        require(membership.balance > 0 || membership.withdrawableBalance > 0, "Not member");
        require(membership.lockupEndTs <= block.timestamp, "Tokens locked");
        
        uint256 tierId = membership.tier;
        uint256 shares = vault.balanceOf(msg.sender, tierId);
        uint256 assets = vaultHelpers.previewRedeem(tierId, shares);
        require(assets != 0, "ZERO_ASSETS");

        // if user is an expired member their withdrawable balance is used
        assets = membership.withdrawableBalance > 0 ? membership.withdrawableBalance : assets;

        vault.withdraw(msg.sender, tierId, assets, shares);

        // if user is an expired memebr his/her space already freed up
        if(membership.withdrawableBalance == 0){
            vault.addOneFreeSpace(vaultHelpers.getCommonIdentifier(tierId));
        }

        _deleteMembership(msg.sender);

        return assets;
    }

    function extendMembership(bool isShortLockup) public  {
        Membership memory membership = _memberships[msg.sender];
        require(!membership.isOG, "OG members cannot extend"); // Redundant but may help with debugging
        require(membership.balance > 0, "Not member");
        require(membership.lockupEndTs <= block.timestamp, "Tokens locked");
        require(membership.tier != BRONZE, "Bronze tier, use upgrade");

        uint256 duration = isShortLockup ? vault.shortLockupDuration() : vault.longLockupDuration();
        uint256 tierId = membership.tier;
        // if user picks a different duration than the previous, we have to move the user to the other one
        if (isShortLockup != membership.isShortLockup){
            uint256 fromShares = vault.balanceOf(msg.sender, tierId);
            uint256 assets = vaultHelpers.previewRedeem(tierId, fromShares);
            uint256 newTierId = membership.isShortLockup ? tierId + 1 : tierId - 1;
            uint256 toShares = vaultHelpers.previewDeposit(newTierId, assets);
            
            vault.moveBetweenTiers(msg.sender, tierId, newTierId, assets, fromShares, toShares);
            tierId = newTierId;
        }

        _extendMembership(msg.sender, tierId, block.timestamp + duration, isShortLockup);
    }

    function extendAndDowngradeMembership(uint256 newTierId) public onlyIfFreeSpaces(newTierId) {
        Membership memory membership = _memberships[msg.sender];
        require(!membership.isOG, "OG members cannot extend and downgrade");
        require(membership.balance > 0, "Not member");
        require(newTierId != BRONZE, "Can't downgrade to Bronze tier, please cancel your membership and re-apply for bronze tier");
        require(membership.tier != BRONZE, "Bronze tier can't downgrade");
        require(membership.tier != newTierId, "Same Tier downgrade");
        require(membership.tier < newTierId, "New tier must be lower"); // Smaller tiers has increasing id
        require(membership.tier != SILVER_SHORT && membership.tier != SILVER_LONG, "You can't downgrade lower from Silver tier, cancel your membership and apply for bronze tier");
        require(membership.lockupEndTs <= block.timestamp, "Tokens locked");

        uint256 currentTierId = membership.tier;

        Tier memory tier = vault.getTier(newTierId);

        bool isShortLockup = tier.isShortLockup;
        uint256 duration = isShortLockup ? vault.shortLockupDuration() : vault.longLockupDuration();

        uint256 fromShares = vault.balanceOf(msg.sender, currentTierId);
        uint256 userTotalAssets = vaultHelpers.previewRedeem(currentTierId, fromShares);
        uint256 toShares = vaultHelpers.previewDeposit(newTierId, userTotalAssets);

        // move the tokens to the new tier
        vault.moveBetweenTiers(msg.sender, currentTierId, newTierId, userTotalAssets, fromShares, toShares);

        uint256 userTotalAssetsUsdcValue = vault.tltOracle().estimateAmountOut(vault.asset(), uint128(userTotalAssets), vault.oracleTwapDuration());
        
        // if member's staked TLT worth more than the downgraded tier fee send it back
        if(userTotalAssetsUsdcValue > tier.tierCost){
            uint256 withdrawableUsdcvalue = userTotalAssetsUsdcValue - tier.tierCost;
            uint256 withdrawableTlt = vault.tltOracle().estimateAmountOut(address(_usdc), uint128(withdrawableUsdcvalue), vault.oracleTwapDuration());

            userTotalAssets = userTotalAssets - withdrawableTlt;
            toShares = vaultHelpers.previewDeposit(newTierId, withdrawableTlt);
            vault.withdraw(msg.sender, newTierId, withdrawableTlt, toShares);
        }
        
        _extendMembershipAndSetBalance(msg.sender, newTierId, userTotalAssets, block.timestamp + duration, isShortLockup);
        
        // adjust free spaces in the tiers
        vault.addOneFreeSpace(vaultHelpers.getCommonIdentifier(currentTierId));
        vault.removeOneFreeSpace(vaultHelpers.getCommonIdentifier(newTierId));

        // mint membership NFT for the new tier
        vault.mintNFT(msg.sender, newTierId);
    }

    function upgradeMembership(uint256 newTierId) public onlyIfFreeSpaces(newTierId) {
        Membership memory membership = _memberships[msg.sender];
        require(!membership.isOG, "OG members cannot upgrade");
        require(membership.balance > 0, "Not member");
        require(membership.tier != PLATINUM_SHORT && membership.tier != PLATINUM_LONG, "You can't upgrade higher from Platinum tier");
        require(membership.tier != newTierId, "Same Tier upgrade");
        require(membership.lockupEndTs > block.timestamp || membership.tier == BRONZE, "Please extend your membership before upgrade");

        uint256 fromShares = vault.balanceOf(msg.sender, membership.tier);
        uint256 assets = vaultHelpers.previewRedeem(membership.tier, fromShares);
        uint256 toShares = vaultHelpers.previewDeposit(newTierId, assets);

        uint256 estimatedTier = vaultHelpers.estimateTier(uint128(assets), membership.isShortLockup);
        if (!(estimatedTier == newTierId)){
            if (membership.isShortLockup && membership.tier != BRONZE){ // bronze tier has default false for isShortLockup so we have to exclude check explicitly
                require(newTierId == SILVER_SHORT || newTierId == GOLD_SHORT || newTierId == PLATINUM_SHORT, "Tier mismatch (wrong duration)");
            } else if (!membership.isShortLockup && membership.tier != BRONZE) {
                require(newTierId == SILVER_LONG || newTierId == GOLD_LONG || newTierId == PLATINUM_LONG, "Tier mismatch (wrong duration)");
            }
            revert("Not enough TLT staked");
        }

        vault.moveBetweenTiers(msg.sender, membership.tier, newTierId, assets, fromShares, toShares);
        
        if (membership.tier == BRONZE){
            bool isShortLockup = vault.getTier(newTierId).isShortLockup;
            uint256 duration = isShortLockup ? vault.shortLockupDuration() : vault.longLockupDuration();
            _extendMembershipAndSetBalance(msg.sender, newTierId, assets, block.timestamp + duration, isShortLockup);
        } else {
            _updateMembershipTierAndBalance(msg.sender, newTierId, assets);
            // we increase the original tier here since we know it's not bronze tier
            vault.addOneFreeSpace(vaultHelpers.getCommonIdentifier(membership.tier));
        }

        vault.removeOneFreeSpace(vaultHelpers.getCommonIdentifier(newTierId));

        // mint membership NFT for the new tier
        vault.mintNFT(msg.sender, newTierId);
    }

    function downgradeMembership(uint256 newTierId) public onlyIfFreeSpaces(newTierId) {
        Membership memory membership = _memberships[msg.sender];
        require(!membership.isOG, "OG members cannot downgrade");
        require(membership.balance > 0, "Not member");
        require(newTierId != BRONZE, "Can't downgrade to Bronze tier, please cancel your membership and re-apply for bronze tier");
        require(membership.tier != BRONZE, "You can't downgrade from Bronze tier");
        require(membership.tier != SILVER_SHORT && membership.tier != SILVER_LONG, "You can't downgrade lower from Silver tier");
        require(membership.tier != newTierId, "Same Tier downgrade");
        require(membership.tier < newTierId, "New tier must be lower"); // Smaller tiers has increasing id
        require(membership.lockupEndTs > block.timestamp, "Please extend your membership or use extend and downgrade");

        uint256 fromShares = vault.balanceOf(msg.sender, membership.tier);
        uint256 assets = vaultHelpers.previewRedeem(membership.tier, fromShares);
        uint256 toShares = vaultHelpers.previewDeposit(newTierId, assets);

        if (membership.isShortLockup){
            require(newTierId == SILVER_SHORT || newTierId == GOLD_SHORT || newTierId == PLATINUM_SHORT, "Tier mismatch (wrong duration)");
        } else {
            require(newTierId == SILVER_LONG || newTierId == GOLD_LONG || newTierId == PLATINUM_LONG, "Tier mismatch (wrong duration)");
        }

        vault.moveBetweenTiers(msg.sender, membership.tier, newTierId, assets, fromShares, toShares);
        _updateMembershipTierAndBalance(msg.sender, newTierId, assets);

        vault.addOneFreeSpace(vaultHelpers.getCommonIdentifier(membership.tier));
        vault.removeOneFreeSpace(vaultHelpers.getCommonIdentifier(newTierId));
        
        // mint membership NFT for the new tier
        vault.mintNFT(msg.sender, newTierId);
    }

    function proposeOG(address memberAddress) public onlyRole(DEFAULT_ADMIN_ROLE) {
        Membership memory membership = _memberships[memberAddress];
        require(!membership.isOG, "User is already OG");

        // If user is already in the highest tier we don't have to move their assets and issue new shares
        uint tierId = membership.tier;
        if(tierId != PLATINUM_LONG){

            // Calculate fromShares, assets and toShares for moveBetweenTiers
            uint256 fromShares = vault.balanceOf(memberAddress, tierId);
            uint256 assets = vaultHelpers.previewRedeem(tierId, fromShares);
            uint256 toShares = vaultHelpers.previewDeposit(PLATINUM_LONG, assets);

            // Move user assets from one tier to another and issue new shares
            vault.moveBetweenTiers(memberAddress, tierId, PLATINUM_LONG, assets, fromShares, toShares);

            // Update OG Member Tier and Balance
            _updateMembershipTierAndBalance(memberAddress, PLATINUM_LONG, assets);
        }

        vault.addOneFreeSpace(vaultHelpers.getCommonIdentifier(membership.tier));

        // Modify OG flag and lockup times and mint 
        _makeMembershipOG(memberAddress);

        emit MakeOG(msg.sender, memberAddress);
    }

    function removeExpiredMemberships(address[] calldata members) public onlyRole(YEET_ROLE) {
        uint256 membersLength = members.length;

        for (uint i = 0; i < membersLength; i++) {
            address member = members[i];
            Membership memory membership = _memberships[member];
            // do and extra check here to be sure we only pass valid addresses
            if(membership.expirationBufferEndTs < block.timestamp && membership.balance > 0){
                uint256 tierId = membership.tier;
                uint256 userBalance = vaultHelpers.previewRedeem(tierId, vault.balanceOf(member, tierId));
                _setExpiredMembershipBalance(member, userBalance);
                vault.addOneFreeSpace(vaultHelpers.getCommonIdentifier(membership.tier));
            }
        }
    }

}

