pragma solidity ^0.8.18;

import "lib/openzeppelin-contracts/contracts/token/ERC20/ERC20.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC20/utils/SafeERC20.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC20/IERC20.sol";
import "lib/openzeppelin-contracts/contracts/security/ReentrancyGuard.sol";
import "lib/openzeppelin-contracts/contracts/access/Ownable.sol";
import "lib/openzeppelin-contracts/contracts/utils/math/Math.sol";

import "../TilttVault.sol";

contract RewardVesting is Ownable, ReentrancyGuard {
    using SafeERC20 for ERC20;
    using Math for uint256;

    uint256 amount;
    uint256 duration;
    uint256 released;

    uint256 contractBalance;

    uint256 private start;

    IERC20 private immutable _token;

    TilttVault private immutable _vault;

    bool private initFlag;

    constructor(address token, address vault) {
        require(token != address(0), "ZERO_TOKEN_ADDRESS");
        require(vault != address(0), "ZERO_VAULT_ADDRESS");
        _token = ERC20(token);
        _vault = TilttVault(vault);
    }

    // ACCESS CONTROL

    modifier onlyVault() {
        _checkVault();
        _;
    }

    function _checkVault() internal view {
        require(msg.sender == address(_vault), "caller is not the Vault");
    }

    // GETTERS
    function getToken() external view returns (address) {
        return address(_token);
    }   

    function getInitFlag() external view returns (bool) {
        return initFlag;
    }

    function getTimeSinceStart() external view returns (uint256) {
        return block.timestamp - start + 1;
    }

    function getDuration() external view returns (uint256) {
        return duration;
    }

    function getStart() external view returns (uint256) {
        return start;
    }

    function createVestingSchedule(uint256 _amount, uint256 _duration) external onlyOwner {
        require(initFlag == false, "VESTING_SCHEDULE_HAS_BEEN_CREATED_BEFORE");
        require(_amount > 0, "AMOUNT_HAS_TO_BE_GREATER_THAN_ZERO");
        require(_duration > 0, "DURATION_HAS_TO_BE_GREATER_THAN_ZERO");

        // Transfer tokens that will be vested
        SafeERC20.safeTransferFrom(_token, msg.sender, address(this), _amount);

        // Set amount, duration and start for the duration
        amount = _amount;
        contractBalance = amount;
        duration = _duration;
        start = block.timestamp;

        initFlag = true;
    }

    function release() external nonReentrant onlyVault returns (uint256){
        if(!initFlag || contractBalance == 0) return 0;
        uint256 releasableAmount = computeReleasableAmount();

        contractBalance -= releasableAmount;

        released += releasableAmount;
        // Transfer releasable tokens to vault
        SafeERC20.safeTransfer(_token, address(_vault), releasableAmount);

        return releasableAmount;
    }

    function computeReleasableAmount() public view returns (uint256) {
        if(!initFlag || contractBalance == 0) return 0;
        uint256 timeSinceVestingStarted = block.timestamp - start + 1;
        // During the linear vesting period
        if(timeSinceVestingStarted <= duration){
            return amount * timeSinceVestingStarted / duration - released;
        //After the vesting period
        } else {
            return amount - released;
        }
    }
}

