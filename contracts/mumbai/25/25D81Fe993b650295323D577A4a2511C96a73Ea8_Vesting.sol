pragma solidity ^0.8.18;

import "lib/openzeppelin-contracts-upgradeable/contracts/token/ERC1155/ERC1155Upgradeable.sol";
import "lib/openzeppelin-contracts-upgradeable/contracts/access/AccessControlUpgradeable.sol";
import "lib/openzeppelin-contracts-upgradeable/contracts/proxy/utils/UUPSUpgradeable.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC20/ERC20.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC20/utils/SafeERC20.sol";
import "lib/openzeppelin-contracts/contracts/utils/math/Math.sol";
import "lib/openzeppelin-contracts/contracts/utils/cryptography/ECDSA.sol";
import "./interfaces/ITLTPriceOracle.sol";
import "./MembershipHandler.sol";
import "./tokens/TilttNFT.sol";
import "./vesting/RewardVesting.sol";
import "./VaultHelpers.sol";

// Authors:
// Gabor Kovacs | github: gabkov     |
// Marton Moro  | github: martonmoro |

uint256 constant PLATINUM_SHORT = 0;
uint256 constant PLATINUM_LONG = 1;
uint256 constant GOLD_SHORT = 2;
uint256 constant GOLD_LONG = 3;
uint256 constant SILVER_SHORT = 4;
uint256 constant SILVER_LONG = 5;
uint256 constant BRONZE = 6;

enum Tiers {
    Platinum,
    Gold,
    Silver,
    Bronze
}

struct Tier {
    uint256 totalSupply;      // shares
    uint256 assetBalance;     // TLT
    uint256 tierCost;
    uint256 rewardMultiplier; // multiplier used for reward distribution
    TilttNFT collection;
    bool isShortLockup;
}

// Highly modified ERC-4626 to be a MultiToken Vault with the help of ERC1155 (in this case multi tier) 
// based on the openzeppelin's ERC-4626 implementation particularry this version: 
// https://github.com/OpenZeppelin/openzeppelin-contracts/blob/6b17b334300f8268acdfc88b77b87395f6aa5174/contracts/token/ERC20/extensions/ERC4626.sol
// since inflation attack is not possible in our usecase, we went with 
// the slightly easier accounting logic, thats why we chose the earlier 
// version which don't have the inflation attack mitigation
contract TilttVault is Initializable, ERC1155Upgradeable, AccessControlUpgradeable, UUPSUpgradeable {
    using SafeERC20 for ERC20;
    using Math for uint256;
    using ECDSA for bytes32;

    event Deposit(
        address indexed caller,
        uint256 assets,
        uint256 shares,
        uint256 tierId
    );

    event Withdraw(
        address indexed caller,
        uint256 assets,
        uint256 shares,
        uint256 tierId
    );

    event Move(
        address indexed caller,
        uint256 fromTierId,
        uint256 toTierId,
        uint256 assets,
        uint256 fromShares,
        uint256 toShares
    );

    event DistributeReward(
        address indexed caller,
        uint256 amount
    );

    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");

    // average 1 month duration
    uint256 private constant ONE_MONTH = 2629744;

    bool fromRebalance;

    uint32 public oracleTwapDuration;

    uint256 public shortLockupDuration;
    uint256 public longLockupDuration;

    // USDC has 6 decimal places
    uint256 public platinumCost; 
    uint256 public goldCost;
    uint256 public silverCost;

    ERC20 private _asset;
    ITLTPriceOracle public tltOracle;
    MembershipHandler public membershipHandler;
    RewardVesting public rewardVesting;
    VaultHelpers public vaultHelpers;

    address private vestingContract;
    
    address private _whitelistSignerAddress;

    mapping(Tiers => uint256) public freeSpaces;

    mapping(uint256 => Tier) tiers;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();   
    }

    function _authorizeUpgrade(address newImplementation) internal onlyRole(UPGRADER_ROLE) override{}

    function initialize(address tilttAddress, 
                address platinumsNFTAddress, 
                address goldNFTAddress, 
                address silverNFTAddress,
                address bronzeNFTAddress) initializer public {
        require(tilttAddress != address(0), "NULL_ADDRESS");
        require(platinumsNFTAddress != address(0), "NULL_ADDRESS");
        require(goldNFTAddress != address(0), "NULL_ADDRESS");
        require(silverNFTAddress != address(0), "NULL_ADDRESS");
        require(bronzeNFTAddress != address(0), "NULL_ADDRESS");
        __ERC1155_init("");
        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);

        _asset = ERC20(tilttAddress);

        // init Platinum tiers
        tiers[PLATINUM_SHORT].collection = TilttNFT(platinumsNFTAddress);
        tiers[PLATINUM_SHORT].isShortLockup = true;
        
        tiers[PLATINUM_LONG].collection = TilttNFT(platinumsNFTAddress);

        setPlatinumCost(25000 * 10**6); // 5000 TLT 

        // init Gold tiers
        tiers[GOLD_SHORT].collection = TilttNFT(goldNFTAddress);
        tiers[GOLD_SHORT].isShortLockup = true;
        
        tiers[GOLD_LONG].collection = TilttNFT(goldNFTAddress);
        
        setGoldCost(5000 * 10**6); // 1000 TLT

        // init Silver tiers
        tiers[SILVER_SHORT].collection = TilttNFT(silverNFTAddress);
        tiers[SILVER_SHORT].isShortLockup = true;
        
        tiers[SILVER_LONG].collection = TilttNFT(silverNFTAddress);

        setSilverCost(1000 * 10**6); // 200  TLT

        tiers[BRONZE].collection = TilttNFT(bronzeNFTAddress);

        freeSpaces[Tiers.Platinum] = 100;
        freeSpaces[Tiers.Gold] = 300;
        freeSpaces[Tiers.Silver] = 600;

        oracleTwapDuration = 1800; // 30 sec
        shortLockupDuration = ONE_MONTH * 6;
        longLockupDuration = ONE_MONTH * 12;

        // Set all the reward multipliers to 1
        for (uint i = 0; i < 7; i++) {
            setTierRewardMultiplier(i, 1);            
        }

    }

    // MODIFIERS

    // Modifier to check if user has been whitelisted
    modifier onlyWhitelisted(bytes calldata signature, address user){
        vaultHelpers.checkWhitelist(signature, user);
        _;
    }

    modifier onlyIfFreeSpaces(uint256 tierId){
        vaultHelpers.checkFreeSpaces(tierId);
        _;
    }

    modifier onlyVesting(){
        _checkVesting();
        _;
    }

    modifier onlyMembershipHandler(){
        _checkMembershipHandler();
        _;
    }

    function _checkVesting() internal view{
        require(msg.sender == vestingContract);
    }

    function _checkMembershipHandler() internal view{
        require(msg.sender == address(membershipHandler));
    }

    // GETTERS

    function asset() public view virtual returns (address) {
        return address(_asset);
    }

    function getTier(uint256 tierId) public view returns (Tier memory){
        return tiers[tierId];
    }

    // SETTERS
    function setVestingContract(address vestingContractAddress) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(vestingContractAddress != address(0), "ZERO_VESTING_ADDRESS");
        vestingContract = vestingContractAddress;
    }

    function setMembershipHandler(address membershipHandlerAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(membershipHandlerAddress != address(0), "ZERO_MEMBERSHIP_HANDLER_ADDRESS");
        membershipHandler = MembershipHandler(membershipHandlerAddress);
    }

    function removeTierFreeSpaces(uint256 platinumNewSpaces, uint256 goldNewSpaces, uint256 silverNewSpaces) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(freeSpaces[Tiers.Platinum] >= platinumNewSpaces, "Too many platinum space remove");
        require(freeSpaces[Tiers.Gold] >= goldNewSpaces, "Too many gold space remove");
        require(freeSpaces[Tiers.Silver] >= silverNewSpaces, "Too many silver space remove");
        freeSpaces[Tiers.Platinum] -= platinumNewSpaces;
        freeSpaces[Tiers.Gold] -= goldNewSpaces;
        freeSpaces[Tiers.Silver] -= silverNewSpaces;
    }

    function addTierFreeSpaces(uint256 platinumNewSpaces, uint256 goldNewSpaces, uint256 silverNewSpaces) public onlyRole(DEFAULT_ADMIN_ROLE) {
        freeSpaces[Tiers.Platinum] += platinumNewSpaces;
        freeSpaces[Tiers.Gold] += goldNewSpaces;
        freeSpaces[Tiers.Silver] += silverNewSpaces;
    }

    function addOneFreeSpace(Tiers tier) public onlyMembershipHandler {
        freeSpaces[tier] += 1;
    }

    function removeOneFreeSpace(Tiers tier) public onlyMembershipHandler {
        freeSpaces[tier] -= 1;
    }

    function setTltOracle(address oracleAddress) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(oracleAddress != address(0), "ZERO_TLT_ORACLE_ADDRESS");
        tltOracle = ITLTPriceOracle(oracleAddress);
    }

    function setTltOracleTwapDuration(uint32 newDurationInSeconds) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(newDurationInSeconds >= 1800, "New duration must be at least 30 mins");
        oracleTwapDuration = newDurationInSeconds;
    }

    function setShortLockupDuration(uint256 months) public onlyRole(DEFAULT_ADMIN_ROLE) {
        shortLockupDuration = ONE_MONTH * months;
    }

    function setLongLockupDuration(uint256 months) public onlyRole(DEFAULT_ADMIN_ROLE) {
        longLockupDuration = ONE_MONTH * months;
    }

    // we scale up the number in here
    function setPlatinumCost(uint256 newCost) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(newCost > goldCost, "New Platinum cost has to be greater than current Gold cost");
        platinumCost = newCost;
        tiers[PLATINUM_SHORT].tierCost = platinumCost;
        tiers[PLATINUM_LONG].tierCost = platinumCost;
    }

    function setGoldCost(uint256 newCost) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(newCost > silverCost, "New Gold cost has to be greater than current Silver cost");
        require(newCost < platinumCost, "New Gold cost has to be smaller than current Platinum cost");
        goldCost = newCost;
        tiers[GOLD_SHORT].tierCost = goldCost;
        tiers[GOLD_LONG].tierCost = goldCost;
    }

    function setSilverCost(uint256 newCost) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(newCost < goldCost, "New Dreaner cost has to be smaller than current Gold Cost");
        silverCost = newCost;
        tiers[SILVER_SHORT].tierCost = silverCost;
        tiers[SILVER_LONG].tierCost = silverCost;
    }

    function setTierRewardMultiplier(uint256 tierId, uint8 newMultiplier) public onlyRole(DEFAULT_ADMIN_ROLE) {
        tiers[tierId].rewardMultiplier = newMultiplier;
    }

    function setRewardVesting(address _rewardVestingAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_rewardVestingAddress != address(0), "ZERO_REWARD_VESTING_ADDRESS");
        rewardVesting = RewardVesting(_rewardVestingAddress);
    }

    function setVaultHelpers(address _vaultHelpers) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_vaultHelpers != address(0), "ZERO_VAULT_HELPERS_ADDRESS");
        vaultHelpers = VaultHelpers(_vaultHelpers);
    }
    

    // USER REWARD HANDLING LOGIC
    function distributeRewards(uint256 reward) public {
        require(address(rewardVesting) != address(0), "ZERO_REWARD_VESTING_ADDRESS");
        uint256 released = rewardVesting.release();
        uint256 weightedSumOfAssets;
        if( !fromRebalance ) {
            SafeERC20.safeTransferFrom(_asset, msg.sender, address(this), reward);
        }
        uint256 numberOfTiers = 7;
        uint256[] memory product = new uint256[](numberOfTiers);
        for (uint256 i = 0; i < numberOfTiers; i++) {
            product[i] = vaultHelpers.totalAssets(i) * tiers[i].rewardMultiplier;
            weightedSumOfAssets += product[i];
        }
        for (uint256 i = 0; i < numberOfTiers; i++) {
            uint256 rewardForTier;
            rewardForTier = (reward + released) * product[i] / weightedSumOfAssets;
            tiers[i].assetBalance += rewardForTier;
        }
        emit DistributeReward(msg.sender, reward + released);
    }

    // HARVEST REWARDS
    function harvestYield(uint256 tierId) public returns (uint256) {
        uint256 harvestableYield = vaultHelpers.previewYield(msg.sender);
        require(harvestableYield > 0, "NO_YIELD");
        uint256 shares = vaultHelpers.previewWithdraw(tierId, harvestableYield);

        _withdraw(msg.sender, tierId, harvestableYield, shares);

        return harvestableYield;
    }

    // DEPOSIT/WITHDRAWAL LOGIC

    function deposit(uint256 tierId, uint256 assets, bytes calldata signature) public onlyWhitelisted(signature, msg.sender) returns (uint256) {
        
        uint256 shares = vaultHelpers.previewDeposit(tierId, assets);
        require(shares != 0, "ZERO_SHARES");

        // if user already has membership
        if (membershipHandler.hasMembership(msg.sender)){
            require(membershipHandler.getMembershipTier(msg.sender) == tierId,  "Tier mismatch");
            _deposit(msg.sender, tierId, assets, shares);
            membershipHandler.addAssetsToMembershipBalance(msg.sender, assets);
        } else {
            Tiers tierGroup = vaultHelpers.getCommonIdentifier(tierId);
            require(freeSpaces[tierGroup] > 0 || tierGroup == Tiers.Bronze, "No free spaces for the selected tier");
            // check deposited TLT value and determine Tier
            bool isShortLockup = tiers[tierId].isShortLockup;
            uint256 tier = vaultHelpers.estimateTier(uint128(assets), isShortLockup);
            require(tier != type(uint256).max, "Not enough funds and bronze tier is disabled");
            require(tier == tierId, "Tier mismatch");

            _deposit(msg.sender, tierId, assets, shares);
            uint256 duration = isShortLockup ? shortLockupDuration : longLockupDuration;
            if(tierId == BRONZE){
                duration = 0;
            }

            membershipHandler.createMembership(msg.sender, tier, assets, block.timestamp + duration, isShortLockup, 0);

            if(tierId != BRONZE){ // bronze tier doesn't get an NFT and doesn't have free spots
                freeSpaces[tierGroup] -= 1;
            }
            
            // mint the membership NFT for the new user
            tiers[tierId].collection.safeMint(msg.sender);            
        }

        return shares;
    }

    function vestingDeposit(address user, uint256 tierId, uint256 assets, uint256 _duration) external onlyVesting onlyIfFreeSpaces(tierId){
        uint256 shares = vaultHelpers.previewDeposit(tierId, assets);
        require(shares != 0, "ZERO_SHARES");

        // check deposited TLT value and determine Tier
        bool isShortLockup = tiers[tierId].isShortLockup;
        uint256 tier = vaultHelpers.estimateTier(uint128(assets), isShortLockup);
        require(tier != type(uint256).max, "Not enough funds and bronze tier is disabled");
        require(tier == tierId, "Tier mismatch");
        _vestingDeposit(user, tierId, assets, shares);

        uint256 duration = _duration + (isShortLockup ? shortLockupDuration : longLockupDuration);
        membershipHandler.createMembership(user, tier, assets, block.timestamp + duration, isShortLockup, block.timestamp + _duration);
        freeSpaces[vaultHelpers.getCommonIdentifier(tierId)] -= 1;
        
        // mint the membership NFT for the new user
        tiers[tierId].collection.safeMint(user); 
    }

    function redeem(uint256 tierId, uint256 shares) public returns (uint256) {
        // load it to memory to save gas on the checks
        Membership memory membership = membershipHandler.getMembership(msg.sender);
        require(membership.balance > 0, "Not member");
        require(membership.tier == tierId,  "Tier mismatch");
        require(membership.lockupEndTs <= block.timestamp || membership.isOG, "Tokens locked"); // bronze tier has no lockup so it will always pass this

        uint256 assets = vaultHelpers.previewRedeem(tierId, shares);
        require(assets != 0, "ZERO_ASSETS");

        // check if shares value still covers the tier fee uppon withdrawal - only for not OG members
        if(!membership.isOG){
            uint256 userTotalAssets = vaultHelpers.convertToAssets(tierId, balanceOf(msg.sender, tierId));
            uint256 balanceAfterWithdraw = userTotalAssets - assets;
            // if tier comes back as lower tierId (so higher tier), thats still valid
            require(vaultHelpers.estimateTier(uint128(balanceAfterWithdraw), membership.isShortLockup) <= membership.tier,
                "Membership withdrawal limit reached, please withdraw less");
        }

        _withdraw(msg.sender, tierId, assets, shares);

        // set remaining assets as the new membership balance
        uint256 remainingAssets = vaultHelpers.convertToAssets(tierId, balanceOf(msg.sender, tierId));
        membershipHandler.setNewMembershipBalance(msg.sender, remainingAssets);
        
        return assets;
    }

    function mintNFT(address user, uint256 tierId) external onlyMembershipHandler {
        tiers[tierId].collection.safeMint(user);
    }

    function withdraw(address user, uint256 tierId, uint256 assets, uint256 shares) external onlyMembershipHandler {
        _withdraw(user, tierId, assets, shares);
    }

    function deposit(address user, uint256 tierId, uint256 assets, uint256 shares) external onlyMembershipHandler {
        _deposit(user, tierId, assets, shares);
    }

    function moveBetweenTiers(address user, uint256 fromTierId, uint256 toTierId, uint256 assets, uint256 fromShares, uint256 toShares) external onlyMembershipHandler {
        _moveBetweenTiers(user, fromTierId, toTierId, assets, fromShares, toShares);
    }


    // INTERNAL FUNCTIONS

    function _deposit(address user, uint256 tierId, uint256 assets, uint256 shares) internal virtual {
        Tier storage tier = tiers[tierId];

        SafeERC20.safeTransferFrom(_asset, user, address(this), assets);
        tier.assetBalance += assets;

        _mint(user, tierId, shares, "");
        tier.totalSupply += shares;

        emit Deposit(user, assets, shares, tierId);
    }

    function _vestingDeposit(address user, uint256 tierId, uint256 assets, uint256 shares) internal virtual{
        Tier storage tier = tiers[tierId];

        SafeERC20.safeTransferFrom(_asset, msg.sender, address(this), assets);
        tier.assetBalance += assets;

        _mint(user, tierId, shares, "");
        tier.totalSupply += shares;

        emit Deposit(user, assets, shares, tierId);
    }

    function _withdraw(address user, uint256 tierId, uint256 assets, uint256 shares) internal virtual {
        Tier storage tier = tiers[tierId];

        _burn(user, tierId, shares);
        tier.totalSupply -= shares;

        emit Withdraw(user, assets, shares, tierId);

        tier.assetBalance -= assets;
        SafeERC20.safeTransfer(_asset, user, assets);
    }

    function _moveBetweenTiers(address user, uint256 fromTierId, uint256 toTierId, uint256 assets, uint256 fromShares, uint256 toShares) internal virtual {
        // Withdraw
        Tier storage fromTier = tiers[fromTierId];

        _burn(user, fromTierId, fromShares);
        fromTier.totalSupply -= fromShares;
        fromTier.assetBalance -= assets;

        // Deposit
        Tier storage toTier = tiers[toTierId];

        toTier.assetBalance += assets;
        _mint(user, toTierId, toShares, "");
        toTier.totalSupply += toShares;

        emit Move(user, fromTierId, toTierId, assets, fromShares, toShares);
    }

    // HELPERS

    // If someone sends money to the contract it should be distributed among the vaults
    function rebalance() public {

        uint256 vaultsAssetBalance;
        // 7 is the number of tiers
        for (uint i = 0; i < 7; i++) {
            vaultsAssetBalance += tiers[i].assetBalance;
        }

        uint256 vaultBalanceOf = _asset.balanceOf(address(this));
        require(vaultBalanceOf > vaultsAssetBalance, "There are no free funds in the contract awaiting distribution");

        uint256 rebalanceAmount = vaultBalanceOf - vaultsAssetBalance;
        fromRebalance = true;
        distributeRewards( rebalanceAmount );
        fromRebalance = false;
    }
    // this is required because we inherit both from ERC1155 and AccessControl
    // https://forum.openzeppelin.com/t/derived-contract-must-override-function-supportsinterface/6315
    function supportsInterface(bytes4 interfaceId) public view override(ERC1155Upgradeable, AccessControlUpgradeable) returns (bool){
        return super.supportsInterface(interfaceId);
    }

    function uri(uint256 id) public view virtual override returns (string memory) {}
}

