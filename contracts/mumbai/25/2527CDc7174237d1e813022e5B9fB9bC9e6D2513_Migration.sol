pragma solidity 0.8.17;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "hardhat-deploy/solc_0.8/proxy/Proxied.sol";
import "./Governed.sol";
import "./interfaces/IForceNFT.sol";
import "./interfaces/IControl.sol";

contract Migration is Initializable, Proxied, Governed {
    IControl public control;

    mapping(uint256 => address) public oldNFTs;
    mapping(uint256 => address) public newNFTs;

    event Migrate(address user, uint256 lvl, uint256 tokenId);

    function setNewNFTs(address[] memory nfts) external onlyGovernor {
        require(nfts.length == 8, "NFTs count should be 8");

        for (uint256 i = 0; i < 8; ) {
            newNFTs[i] = nfts[i];

            unchecked {
                i++;
            }
        }
    }

    function setOldNFTs(address[] memory nfts) external onlyGovernor {
        require(nfts.length == 8, "NFTs count should be 8");

        for (uint256 i = 0; i < 8; ) {
            oldNFTs[i] = nfts[i];

            unchecked {
                i++;
            }
        }
    }

    function migrate(uint256 level, uint256 tokenId) external {
        _migrateInternal(level, IForceNFT(oldNFTs[level]), msg.sender, tokenId);
    }

    function migrateAll() external {
        address user = msg.sender;

        uint256 nftAmount;
        for (uint256 lvl = 0; lvl < 8; ) {
            IForceNFT oldNFT = IForceNFT(oldNFTs[lvl]);
            nftAmount = oldNFT.balanceOf(user);

            if (nftAmount != 0) {
                for (uint256 i = nftAmount; i > 0; ) {
                    unchecked {
                        i--;
                    }

                    uint256 tokenId = oldNFT.tokenOfOwnerByIndex(user, i);
                    _migrateInternal(lvl, oldNFT, user, tokenId);
                }
            }

            unchecked {
                lvl++;
            }
        }
    }

    function initialize(IControl _control) public proxied initializer {
        setGovernor(msg.sender);
        control = _control;
    }

    function _migrateInternal(
        uint256 level,
        IForceNFT oldNFT,
        address user,
        uint256 tokenId
    ) internal {
        // slither-disable-next-line incorrect-equality
        require(oldNFT.ownerOf(tokenId) == user, "User is not the owner of the NFT");

        emit Migrate(user, level, tokenId);
        control.migrateNFT(user, level, tokenId, oldNFT.energy(tokenId));
        oldNFT.burn(tokenId);
    }
}

