// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";

contract Lucky is Context, Ownable {
    struct LuckyConf {
        bool completed;
        address owner;
        uint256 random;
        uint16 size;
        string[] ticketList;
        uint32[] luckyIndexList;
    }
    // config map
    mapping(string => LuckyConf) private luckyConfMap;
    // push once
    mapping(string => mapping(string => bool)) public luckySet;
    // op white list
    mapping(address => bool) public whitelist;

    /**
    * @dev Choose Lucky
     */
    event ChooseLucky(string indexed id, string ticket);
    event AddTicket(string indexed id, string ticket);

    constructor()
    Ownable()
    {
    }

    function setWhitelist(address op, bool val)
    external
    onlyOwner
    {
        whitelist[op] = val;
    }

    modifier onlyWhitelist() {
        require(whitelist[_msgSender()], "Lucky: caller is not in the whitelist");
        _;
    }

    function _getRandom(string calldata id, uint256 nonce)
    private
    view
    returns (uint256)
    {
        return uint256(
            keccak256(abi.encodePacked(blockhash(block.number - 1), id, nonce))
        );
    }

    /**
     * @dev create lucky and set config
     */
    function createLucky(string calldata id, uint16 size)
    external
    onlyWhitelist
    {
        require(size <= 1000, 'createLucky:Max size 1000');
        require(luckyConfMap[id].owner == address(0), 'createLucky: Exists');
        LuckyConf memory lConf;
        lConf.completed = false;
        lConf.owner = _msgSender();
        lConf.size = size;
        luckyConfMap[id] = lConf;
    }

    /**
     * @dev it's batch function
     */
    function addLuckyTicketBatch(string calldata id, string[] calldata ticketList)
    external
    onlyWhitelist
    {
        require(luckyConfMap[id].owner == _msgSender(), 'addLuckyTicketBatch: Not owner');
        require(luckyConfMap[id].completed == false, 'addLuckyTicketBatch: completed');
        for (uint256 i = 0; i < ticketList.length; i++) {
            string memory ticket = ticketList[i];
            if (luckySet[id][ticket] != true) {
                luckyConfMap[id].ticketList.push(ticket);
                luckySet[id][ticket] = true;
                emit AddTicket(id, ticket);
            }
        }
    }

    /**
     * @dev choose lucky and completed
     */
    function chooseLucky(string calldata id)
    external
    onlyWhitelist
    {
        require(luckyConfMap[id].owner == _msgSender(), 'ChooseLucky: Not owner');
        require(luckyConfMap[id].completed == false, 'ChooseLucky: completed');
        luckyConfMap[id].completed = true;
        LuckyConf storage lConf = luckyConfMap[id];
        uint256 maxSize = Math.min(lConf.size, lConf.ticketList.length);
        uint256[] memory tmpList = _tmpList(lConf.ticketList.length);
        uint256 tmpIndex = block.number;
        for (uint256 i = 0; i < maxSize; i++) {
            uint256 random = _getRandom(id, tmpIndex);
            tmpIndex = random % tmpList.length;
            lConf.luckyIndexList.push(uint32(tmpList[tmpIndex]));
            tmpList = _tmpListSlice(tmpList, tmpIndex);
        }
        for (uint256 i = 0; i < maxSize; i++) {
            emit ChooseLucky(id, lConf.ticketList[lConf.luckyIndexList[i]]);
        }
    }

    function _tmpListSlice(uint256[] memory list, uint256 rmIndex)
    private
    pure
    returns (uint256[] memory)
    {
        uint256[] memory tmpList = new uint256[](list.length - 1);
        for (uint256 i = 0; i < list.length; i++) {
            if (i > rmIndex) {
                tmpList[i - 1] = list[i];
            } else if (i < rmIndex) {
                tmpList[i] = list[i];
            }
        }
        return tmpList;
    }

    function _tmpList(uint256 length)
    private
    pure
    returns (uint256[] memory)
    {
        uint256[] memory tmpList = new uint256[](length);
        for (uint256 i = 0; i < length; i++) {
            tmpList[i] = i;
        }
        return tmpList;
    }

    function getLuckyIndex(string calldata id)
    external
    view
    returns (uint32[] memory)
    {
        return luckyConfMap[id].luckyIndexList;
    }

    function getLuckyTicket(string calldata id)
    external
    view
    returns (string[] memory)
    {
        LuckyConf memory lConf = luckyConfMap[id];
        uint256 len = lConf.luckyIndexList.length;
        string[] memory ticketList = new string[](len);
        for (uint256 i = 0; i < len; i++) {
            ticketList[i] = lConf.ticketList[lConf.luckyIndexList[i]];
        }
        return ticketList;
    }
}