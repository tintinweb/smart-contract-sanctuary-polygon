 */
contract Counter is AutomationCompatibleInterface {
    /**
     * Public counter variable
     */
    uint256 public counter;
    rentLendMarketplace public lyncAddress;
    address _owner;
    /**
     * Use an interval in seconds and a timestamp to slow execution of Upkeep
     */
    uint256 public immutable interval;
    uint256 public lastTimeStamp;

    constructor(uint256 updateInterval, address payable addLync) {
        interval = updateInterval;
        lastTimeStamp = block.timestamp;
        _owner = msg.sender;
        lyncAddress = rentLendMarketplace(addLync);
    }

    function checkUpkeep(
        bytes calldata /* checkData */
    )
        external
        view
        override
        returns (
            bool upkeepNeeded,
            bytes memory /*performData*/
        )
    {
        (, uint256 _length) = lyncAddress.checkTokenDurationExp();

        upkeepNeeded = _length > 0;

        // We don't use the checkData in this example. The checkData is defined when the Upkeep was registered.
    }

    function performUpkeep(
        bytes calldata /*performData*/
    ) external override {
        lyncAddress.checkAndReturnRentedNfts();
        // We don't use the performData in this example. The performData is generated by the Automation Node's call to your checkUpkeep function
    }

    function setLyncAddress(address payable _lyncAddress) external {
        require(msg.sender == _owner, "Only owner can call tis function");
        lyncAddress = rentLendMarketplace(_lyncAddress);
    }
}

