pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./IFudCoinERC20.sol";

/**
 * @dev This contract is for managing FudCoin claims. It extends AccessControlEnumerable and ReentrancyGuard to have access control functionality and to prevent re-entrancy attacks. 
 */
contract FudCoinClaim is AccessControlEnumerable, ReentrancyGuard, Ownable {
    
    // Enables the use of ECDSA-related functions for bytes32.
    using ECDSA for bytes32;

    // Declares a constant that holds the keccak256 hash of "MANAGER_ROLE".
    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");
    
    // Instance of IFudCoinERC20 which represents the FudCoin token.
    IFudCoinERC20 FudCoin;

    // The address that will sign the claims.
    address private _signer;

    // Boolean value to check whether the claim is enabled or not.
    bool private _enabledClaim;
    
    // Mapping of address to array of tickets which represents the successful claims.
    mapping(address => Ticket[]) private claimsSuccess;

    mapping(uint256 => bool) private zft2ClaimsSuccess;

    mapping(address => bool) private vipClaimsSuccess;

    // Struct to represent a ticket.
    struct Ticket {
        uint256 tid;
        uint256 amount;
    }

    event ClaimSuccess(address indexed account, uint256 amount, uint256 tid);

    /**
     * @dev Constructs the contract and initializes the signer, FudCoin contract address, roles, and claim status.
     *
     * @param signer The address that will be used for signing claims.
     * @param initFudCoinAddress The initial address of the FudCoin ERC20 token contract.
     */
    constructor (address signer, address initFudCoinAddress) {
        FudCoin = IFudCoinERC20(initFudCoinAddress);
        _signer = signer;    
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender); 
        _setupRole(MANAGER_ROLE, msg.sender); 
        _enabledClaim = true;
    }

    function setFudCoinAddress(address newFudCoinAddress) external onlyRole(DEFAULT_ADMIN_ROLE)  {
        FudCoin = IFudCoinERC20(newFudCoinAddress);
    }

    /**
     * @dev Allows the admin role to enable or disable the claim functionality.
     *
     * @param enable The status to set for the claim functionality.
     */
    function setEnableClaim(bool enable) external onlyRole(DEFAULT_ADMIN_ROLE)  {
        _enabledClaim = enable;
    }

    /**
     * @dev Allows the admin role to change the signer address.
     *
     * @param newSigner The new signer address.
     */
    function setSigner(address newSigner) external onlyRole(DEFAULT_ADMIN_ROLE)  {
        _signer = newSigner;
    }

    /**
     * @dev Allows the admin role to set a new manager.
     *
     * @param manager The address of the new manager.
     */
    function setManager(address manager) external onlyRole(DEFAULT_ADMIN_ROLE) {
        grantRole(MANAGER_ROLE, manager);
    }

    /**
     * @dev Allows the admin role to remove an existing manager.
     *
     * @param manager The address of the manager to remove.
     */
    function removeManager(address manager) external onlyRole(DEFAULT_ADMIN_ROLE) {
        revokeRole(MANAGER_ROLE, manager);
    }

    /**
     * @dev Checks if the address provided is a manager.
     *
     * @param manager The address to check.
     * @return Returns true if the address is a manager, false otherwise.
     */
    function isManager(address manager) public view returns (bool) {
        return hasRole(MANAGER_ROLE, manager);
    }

    /**
     * @dev Modifier to ensure that the caller is not another contract.
     */
    modifier callerIsUser() {
        require(tx.origin == msg.sender, "The caller is another contract");
        _;
    }

     /**
     * @dev Claim FudCoins by providing a signature. This function is protected from re-entrancy attacks and can only be called by an EOA (Externally Owned Account).
     *
     * @param amount The amount to claim.
     * @param timestamp The timestamp of the claim.
     * @param tid The ticket id of the claim.
     * @param signature The signature for the claim.
     */
    function claimBySignature(uint256 amount, uint64 timestamp, uint256 tid, bytes memory signature) external callerIsUser nonReentrant {
        require(_enabledClaim, "currently unabled");

        require(amount > 0, "claim value is empty");

        require(timestamp > 0, "timestamp is empty");

        require(tid > 0, "tid is empty");

        require(!checkAlreadyClaim(msg.sender, tid), "this tid already claimed");

        require(isAuthorized(msg.sender, amount, timestamp, tid, signature), "auth failed");

        FudCoin.mint(msg.sender, amount);
        addSuccessClaim(msg.sender,tid, amount);
        emit ClaimSuccess(msg.sender, amount, tid);
    }

    /**
     * @dev Returns the successful claims made by the sender.
     *
     * @param sender The address of the claimer.
     * @return An array of successful claim Tickets.
     */
    function getAlreadyClaimsSuccess(address sender) public view returns (Ticket[] memory) {
        return claimsSuccess[sender];
    }

    /**
     * @dev Records a successful claim made by the sender.
     *
     * @param sender The address of the claimer.
     * @param tid The ticket id of the claim.
     * @param amount The amount of the claim.
     */
    function addSuccessClaim(address sender, uint256 tid, uint256 amount) internal {
        Ticket memory ticket;
        ticket.tid = tid;
        ticket.amount = amount;
        claimsSuccess[sender].push(ticket);
    }
    
    /**
     * @dev Checks if a claim has already been made by the sender using the provided ticket id.
     *
     * @param sender The address of the claimer.
     * @param tid The ticket id of the claim.
     * @return A boolean indicating whether the claim has already been made.
     */
    function checkAlreadyClaim(address sender, uint256 tid) internal view returns (bool) {
        for (uint i = 0; i < claimsSuccess[sender].length; i++) {
            if (claimsSuccess[sender][i].tid == tid) {
                return true;
            }
        }

        return false;
    }
    
    /**
     * @dev Checks if the sender is authorized to make the claim.
     *
     * @param sender The address of the claimer.
     * @param amount The amount of the claim.
     * @param timestamp The timestamp of the claim.
     * @param tid The ticket id of the claim.
     * @param signature The signature for the claim.
     * @return A boolean indicating whether the sender is authorized.
     */
    function isAuthorized(address sender, uint256 amount, uint64 timestamp, uint256 tid, bytes memory signature) internal view returns (bool) {
        bytes32 hashMessage = getMessageHash(sender, amount, timestamp, tid);
        bytes32 ethHashMessage = getEthSignedMessageHash(hashMessage);
        address recoverSignerAddr = recoverSigner(ethHashMessage, signature);
        return recoverSignerAddr == _signer;
    }

    /**
     * @dev Returns all the addresses that have the manager role.
     *
     * @return An array of manager addresses.
     */
    function getManagers() external view returns (address[] memory) {
        uint256 managerCount = getRoleMemberCount(MANAGER_ROLE);
        address[] memory managers = new address[](managerCount);

        for (uint256 i = 0; i < managerCount; i++) {
            managers[i] = getRoleMember(MANAGER_ROLE, i);
        }

        return managers;
    }

    /**
     * @dev Generates a hash for the claim.
     *
     * @param claimUser The address of the claimer.
     * @param amount The amount of the claim.
     * @param timestamp The timestamp of the claim.
     * @param tid The ticket id of the claim.
     * @return The hash of the claim.
     */
    function getMessageHash(address claimUser, uint256 amount, uint256 timestamp, uint256 tid) internal pure returns (bytes32) {
        return keccak256(abi.encodePacked(claimUser, amount, timestamp, tid));
    }
    
    /**
     * @dev Generates an Ethereum signed message hash.
     *
     * @param messageHash The hash to sign.
     * @return The Ethereum signed message hash.
     */
    function getEthSignedMessageHash(bytes32 messageHash) internal pure returns (bytes32) {
        return keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", messageHash));
    }

    /**
     * @dev Recovers the signer from the Ethereum signed message hash and the signature.
     *
     * @param ethSignedMessageHash The Ethereum signed message hash.
     * @param signature The signature.
     * @return The address of the signer.
     */
    function recoverSigner(bytes32 ethSignedMessageHash, bytes memory signature) internal pure returns (address) {
        require(signature.length == 65, "Invalid signature length");
        
        bytes32 r;
        bytes32 s;
        uint8 v;
        
        assembly {
            r := mload(add(signature, 32))
            s := mload(add(signature, 64))
            v := byte(0, mload(add(signature, 96)))
        }
        
        if (v < 27) {
            v += 27;
        }
        require(v == 27 || v == 28, "Invalid signature recovery");
        
        return ecrecover(ethSignedMessageHash, v, r, s);
    }

    /**
     * @dev Allows the admin role to withdraw a specified amount of ERC20 tokens from the contract.
     *
     * @param token The ERC20 token to withdraw.
     * @param amount The amount of the token to withdraw.
     */
    function withdrawERC20(IERC20 token, uint256 amount) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(address(token) != address(0), "Invalid token address");
        uint256 tokenBalance = token.balanceOf(address(this));
        require(tokenBalance >= amount, "Insufficient token balance");
        token.transfer(msg.sender, amount);
    }
}

