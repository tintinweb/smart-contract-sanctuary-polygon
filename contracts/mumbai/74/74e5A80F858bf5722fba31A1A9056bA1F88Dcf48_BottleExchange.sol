import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "hardhat/console.sol";

///@notice This Smart Contract is the base of the bottle collections that CoCo is going to have,
///@notice this collections are made to be exchanged when a person collects all the bottle.
///@notice This exchange is going to be made by other smart contract that is the one that is received by the constructor "_exchange"

contract BottleCollection is
  ERC721,
  ERC721URIStorage,
  Pausable,
  AccessControl,
  Ownable
{
  using Strings for uint256;
  using Counters for Counters.Counter;

  bytes32 public constant PRICE_MODIFICATOR_ROLE =
    keccak256("PRICE_MODIFICATOR_ROLE");
  bytes32 public constant PAUSE_MINT_MODIFICATOR_ROLE =
    keccak256("PAUSE_MINT_MODIFICATOR_ROLE");
  bytes32 public constant WITHDRAW_MODIFICATOR_ROLE =
    keccak256("WITHDRAW_MODIFICATOR_ROLE");
  bytes32 public constant TOKEN_AND_ADDRESSES_MODIFICATOR_ROLE =
    keccak256("TOKEN_AND_ADDRESSES_MODIFICATOR_ROLE");
  ///@dev this role is the only that can modify the supplies and limits of minting and tokens
  bytes32 public constant LIMIT_MODIFICATOR_ROLE =
    keccak256("LIMIT_MODIFICATOR_ROLE");
  bytes32 public constant URI_MODIFICATOR_ROLE =
    keccak256("URI_MODIFICATOR_ROLE");

  Counters.Counter private _tokenIdCounter;

  struct Offer {
    address bidder;
    uint256 expirationDate;
    uint256 offerAmountLeft;
    uint256 refundAmountPerToken;
    Seller[] owners;
    bool active;
  }

  struct Seller {
    address seller;
    uint256[] tokensInBid;
  }

  ///@dev the chainlink priceFeed
  AggregatorV3Interface internal priceFeed;

  ///@dev array of tokens reserved for one person
  uint256[] private arrayTokensReserved;
  uint256 public maxSupply;
  uint256 public maxTokensPerAddress;
  uint256 public supply;

  ///@dev the quantity of USD passes by the constructor as a 7th param has 6 decimals in order to can have prices less than 1 dollar; (e.i 1000000 == 1USD)
  uint256 public decimalsUSD = 6;

  ///@dev this mapping saves the tokens owned by an address
  mapping(address => uint256[]) public ownerTokens;

  ///@dev last offer made
  Offer public lastOffer;

  /// @dev mapping for the quantity of tokens minted by all of the addresses
  mapping(address => uint256) public quantityTokensMinted;

  ///@dev Array of tokens allowed in the marketplace
  address[] public tokensAllowed;

  ///@dev Price of each token allowed in the marketplace
  mapping(address => uint256) public priceTokens;
  /// @dev mapping for the quantity of tokens minted by all of the addresses
  mapping(address => uint256) public balanceTokens;

  /// @dev mapping for the quantity of tokens reserved for an address because of an Offer
  mapping(address => uint256) public balanceUserOfferTokens;

  ///@dev Quantity of non-answered offers of each address
  mapping(address => uint256) public passiveNoAnswer;

  ///@dev address that is going to be compared in the mint to determine which payment is the one used.
  ///@notice If you pass the same value of this variable, you'll have to pay in the main currency of the blockchain
  address public mainCurrency;
  address public usdc;

  ///@dev address of the exchange smart contract
  address public exchange;

  uint256 public usdPrice;

  string public baseURI;
  string public baseExtension = ".json";

  constructor(
    string memory _name,
    string memory _symbol,
    string memory _initBaseURI,
    address _priceFeed, ///@dev address of the priceFeed on the chain you are deploying
    address _usdc, ///@dev address of the usdc smart contract on the chain you are deploying
    address _mainCurrency, ///@dev address of the wrapped currency smart contract on the chain you are deploying
    uint256 _usdPrice, ///@dev Price in USD of each NFT (1000000 == 1USD)
    uint256 _usdcPrice, ///@dev Price in USDC of each NFT (1000000 == 1USDC)
    address _exchange
  ) ERC721(_name, _symbol) {
    _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
    priceFeed = AggregatorV3Interface(_priceFeed);
    mainCurrency = _mainCurrency;
    tokensAllowed.push(_usdc);
    priceTokens[_usdc] = _usdcPrice;
    usdPrice = _usdPrice;
    usdc = _usdc;
    _tokenIdCounter.increment();
    baseURI = _initBaseURI;
    maxSupply = 78;
    maxTokensPerAddress = 78;
    supply = 0;
    exchange = _exchange;
  }

  modifier onlyQuantityAllowed(address account, uint256 quantity) {
    require(
      quantityTokensMinted[account] + quantity <= maxTokensPerAddress,
      "You are trying to mint more than the quantity allowed per address"
    );
    _;
  }

  function getPrice(uint256 quantity) public view returns (uint256) {
    // (, int256 price, , , ) = priceFeed.latestRoundData();
    int256 price = 90000000;
    // uint256 decimals  = priceFeed.decimals();
    uint256 decimals = 8;
    return
      (quantity * (usdPrice) * 10**(18 + decimals - decimalsUSD)) /
      uint256(price);
  }

  function isTokenAllowed(address _token) public view returns (bool) {
    for (uint256 i = 0; i < tokensAllowed.length; i++) {
      if (_token == tokensAllowed[i] || _token == mainCurrency) {
        return true;
      }
    }
    return false;
  }

  ///@dev returns the token index of a token allowed to pay, returns 100000000000000000 in case of not founded
  function tokenIndex(address _token) public view returns (uint256) {
    for (uint256 i = 0; i < tokensAllowed.length; i++) {
      if (_token == tokensAllowed[i]) {
        return i;
      }
    }
    return 100000000000000000;
  }

  function pause() public onlyRole(PAUSE_MINT_MODIFICATOR_ROLE) {
    _pause();
  }

  function unpause() public onlyRole(PAUSE_MINT_MODIFICATOR_ROLE) {
    _unpause();
  }

  ///@dev internal funcions to manage the minting payment. Wih an ERC20 and with the main currency
  function buyWithToken(address _token, uint256 price) internal {
    require(isTokenAllowed(_token), "Token to pay is not allowed");
    require(
      ERC20(_token).balanceOf(_msgSender()) >= price,
      "Insufficient funds to buy"
    );
    require(
      ERC20(_token).allowance(_msgSender(), address(this)) >= price,
      "Insufficient allowance to buy"
    );
    ERC20(_token).transferFrom(_msgSender(), address(this), price);
    balanceTokens[_token] += price;
  }

  function buyWithCurrency(uint256 price) internal {
    require(msg.value >= price, "Insufficient funds to buy");
  }

  ///@dev functions to add, remove a token, add function also works to edit
  function addTokenToBuy(address _token, uint256 _priceToken)
    external
    onlyRole(TOKEN_AND_ADDRESSES_MODIFICATOR_ROLE)
  {
    if (!isTokenAllowed(_token)) {
      tokensAllowed.push(_token);
    }
    priceTokens[_token] = _priceToken;
  }

  function removeTokenToBuy(address _token)
    external
    onlyRole(TOKEN_AND_ADDRESSES_MODIFICATOR_ROLE)
  {
    uint256 index = tokenIndex(_token);
    tokensAllowed[index] = tokensAllowed[tokensAllowed.length - 1];
    tokensAllowed.pop();
    delete priceTokens[_token];
  }

  /// @dev minting of the tokens where the user need to pass the quantity argument
  function safeMint(uint256 quantity, address _token)
    public
    payable
    whenNotPaused
    onlyQuantityAllowed(_msgSender(), quantity)
  {
    require(quantity > 0, "You need to mint at least 1 NFT");
    require(
      quantity + supply <= maxSupply,
      "The quantity of tokens is higher than maxSupply or are already reserved"
    );
    uint256 price = getPrice(quantity);
    _setApprovalForAll(_msgSender(), address(this), true);
    if (_token == mainCurrency) {
      buyWithCurrency(price);
    } else {
      buyWithToken(_token, priceTokens[_token] * quantity);
    }
    for (uint256 i = 0; i < quantity; i++) {
      uint256 tokenId = _tokenIdCounter.current();
      _safeMint(msg.sender, tokenId);
      ownerTokens[msg.sender].push(tokenId);
      _tokenIdCounter.increment();
      supply += 1;
    }
  }

  /// @dev function created to be used only inside this contract
  function transferOwnerTokens(
    address from,
    address to,
    uint256 tokenId
  ) internal {
    for (uint256 i = 0; i < ownerTokens[from].length; i++) {
      if (ownerTokens[from][i] == tokenId) {
        ownerTokens[to].push(tokenId);
        for (uint256 j = i; j < ownerTokens[from].length - 1; j++) {
          ownerTokens[from][j] = ownerTokens[from][j + 1];
        }
        ownerTokens[from].pop();
        break;
      }
    }
  }

