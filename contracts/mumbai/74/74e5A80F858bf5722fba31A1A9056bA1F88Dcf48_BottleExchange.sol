    require(_msgSender() == exchange, "Only the exchange can burn tokens");
    _burn(tokenId);
  }

  function checkAddress(
    Seller[] storage checkedAddresses,
    address addressToCheck
  ) internal view returns (bool) {
    for (uint256 i = 0; i < checkedAddresses.length; i++) {
      if (checkedAddresses[i].seller == addressToCheck) {
        return false;
      }
    }
    return true;
  }

  /// @notice This function was built in order to have a case where a person almost collected the whole
  /// @notice collection and wants to collect and exchange all the collection, then we would contact the other persons who owns NFTs
  /// @notice and pay them and reserve to the collector the tokens left
  /// @notice

  function makeAnOfferForAll(uint256 refundAmountPerToken) external payable {
    uint256 tokensInBid = (maxSupply - balanceOf(_msgSender()));
    require(
      _exists(maxSupply),
      "You can't offer because the collection is not totally minted"
    );
    require(
      ((balanceOf(_msgSender()) * 100) / maxSupply) >= 90,
      "You have to have more than the 90 percent of the collection in order to make an offer for the rest of the tokens"
    );
    require(
      (refundAmountPerToken * tokensInBid) >=
        (tokensInBid * priceTokens[usdc]) +
          (((tokensInBid * priceTokens[usdc]) * 50) / 100),
      "You have to pay the price for all the NFTs"
    );
    require(
      ERC20(usdc).balanceOf(_msgSender()) >=
        (refundAmountPerToken * tokensInBid),
      "Your balance has to be equal or greater than the payment amount"
    );
    require(
      ERC20(usdc).allowance(_msgSender(), address(this)) >=
        (refundAmountPerToken * tokensInBid),
      "Your allowance has to be equal or greater than the payment amount"
    );
    require(
      lastOffer.active == false || lastOffer.expirationDate <= block.timestamp,
      "There is a bid active, you can't make a new one"
    );
    delete lastOffer.owners;
    ERC20(usdc).transferFrom(
      _msgSender(),
      address(this),
      refundAmountPerToken * tokensInBid
    );
    balanceUserOfferTokens[_msgSender()] += refundAmountPerToken * tokensInBid;
    lastOffer.offerAmountLeft = refundAmountPerToken * tokensInBid;
    lastOffer.refundAmountPerToken = refundAmountPerToken;
    for (uint256 i = 1; i <= maxSupply; i++) {
      address payable tokenOwner = payable(ownerOf(i));
      if (tokenOwner != _msgSender()) {
        if (checkAddress(lastOffer.owners, tokenOwner)) {
          passiveNoAnswer[tokenOwner] += 1;
          Seller memory seller = Seller(tokenOwner, ownerTokens[tokenOwner]);
          lastOffer.owners.push(seller);
        }
        if (passiveNoAnswer[tokenOwner] >= 6) {
          console.log("transfer payment");
          uint256 amountToTransfer = refundAmountPerToken *
            ownerTokens[tokenOwner].length;
          ERC20(usdc).transfer(tokenOwner, amountToTransfer);
          balanceUserOfferTokens[_msgSender()] -= amountToTransfer;
          for (uint256 j = 0; j < ownerTokens[tokenOwner].length; j++) {
            _transfer(tokenOwner, _msgSender(), ownerTokens[tokenOwner][j]);
          }
          lastOffer.offerAmountLeft -= amountToTransfer;
          passiveNoAnswer[tokenOwner] = 0;
        }
      }
    }
    lastOffer.active = true;
    lastOffer.bidder = _msgSender();
    lastOffer.expirationDate = block.timestamp + (30 * 24 * 60 * 60); //days, hours, minutes, seconds = 1 month
  }

  function cancelLastOffer() public {
    require(
      lastOffer.bidder == _msgSender(),
      "You have to be the last bidder in order to cancel the last bid"
    );
    lastOffer.active = false;
    for (uint256 i = 0; i < lastOffer.owners.length; i++) {
      if (passiveNoAnswer[lastOffer.owners[i].seller] > 0) {
        passiveNoAnswer[lastOffer.owners[i].seller] -= 1;
      }
    }
    ERC20(usdc).transfer(_msgSender(), lastOffer.offerAmountLeft);
    balanceUserOfferTokens[_msgSender()] -= lastOffer.offerAmountLeft;
  }

  function getOfferAmountBack() public {
    require(
      lastOffer.active == false ||
        lastOffer.expirationDate <= block.timestamp ||
        lastOffer.bidder != _msgSender(),
      "There can't be an offer active to get the amount back or you can't be the last bidder"
    );
    _transferUSDC(balanceUserOfferTokens[_msgSender()]);
  }

  function _transferUSDC(uint256 amountToTransfer) internal {
    ERC20(usdc).transfer(_msgSender(), amountToTransfer);
    balanceUserOfferTokens[_msgSender()] -= amountToTransfer;
  }

  function respondAnOffer(bool response) public {
    require(
      lastOffer.active == true && lastOffer.expirationDate >= block.timestamp,
      "There is not a bid active, you can't answer"
    );
    passiveNoAnswer[_msgSender()] = 0;
    if (response == true) {
      for (uint256 i = 0; i < ownerTokens[_msgSender()].length; i++) {
        _transfer(_msgSender(), lastOffer.bidder, ownerTokens[_msgSender()][i]);
      }
      uint256 amountToTransfer = lastOffer.refundAmountPerToken *
        ownerTokens[_msgSender()].length;
      ERC20(usdc).transfer(_msgSender(), amountToTransfer);
      balanceUserOfferTokens[lastOffer.bidder] -= amountToTransfer;
    }
  }

  /// @dev return the URI associated to a tokenId
  function tokenURI(uint256 tokenId)
    public
    view
    override(ERC721, ERC721URIStorage)
    returns (string memory)
  {
    return
      string(
        abi.encodePacked(
          baseURI,
          balanceOf(ownerOf(tokenId)).toString(),
          baseExtension
        )
      );
  }

  function totalSupply() external view returns (uint256) {
    return supply;
  }

  /// @dev setters of the variables
  function setBaseURI(string memory _newBaseURI)
    public
    onlyRole(URI_MODIFICATOR_ROLE)
  {
    baseURI = _newBaseURI;
  }

  ///@dev newPrice value has to have 6 decimals
  function setUsdPrice(uint256 newPrice)
    external
    onlyRole(PRICE_MODIFICATOR_ROLE)
  {
    usdPrice = newPrice;
  }

  function setExchange(address _exchange)
    external
    onlyRole(TOKEN_AND_ADDRESSES_MODIFICATOR_ROLE)
  {
    exchange = _exchange;
  }

  function setPriceFeed(address _priceFeed)
    external
    onlyRole(TOKEN_AND_ADDRESSES_MODIFICATOR_ROLE)
  {
    priceFeed = AggregatorV3Interface(_priceFeed);
  }

  /// @dev function to set the max quantity of tokens allowed per address
  function setMaxTokensPerAddress(uint256 newMax)
    external
    onlyRole(LIMIT_MODIFICATOR_ROLE)
  {
    maxTokensPerAddress = newMax;
  }

  /// @dev function to set the max token supply
  function setMaxTokenSupply(uint256 newMax)
    external
    onlyRole(LIMIT_MODIFICATOR_ROLE)
  {
    maxSupply = newMax;
  }

  /**
   * Withdraws money to the addres specified in the first parameter.
   * @param to Receiver address.
   */
  function withdrawMoneyTo(address to, uint256 _amount)
    public
    onlyRole(WITHDRAW_MODIFICATOR_ROLE)
  {
    if (_amount <= address(this).balance) {
      address payable payableTo = payable(to);
      payableTo.transfer(_amount);
    } else {
      address payable payableTo = payable(to);
      payableTo.transfer(address(this).balance);
    }
  }

  function withdrawTokensTo(
    address to,
    uint256 _amount,
    address _token
  ) public onlyRole(WITHDRAW_MODIFICATOR_ROLE) {
    if (_amount <= balanceTokens[_token]) {
      ERC20(_token).increaseAllowance(to, _amount);
      ERC20(_token).transfer(to, _amount);
      balanceTokens[_token] -= _amount;
    } else {
      ERC20(_token).increaseAllowance(to, balanceTokens[_token]);
      ERC20(_token).transfer(to, balanceTokens[_token]);
      balanceTokens[_token] = 0;
    }
  }

  // The following functions are overrides required by Solidity.
  function supportsInterface(bytes4 interfaceId)
    public
    view
    override(ERC721, AccessControl)
    returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }
