pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "./BottlesSmartContracts/CoCoBottleCollectionV1.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

contract BottleExchange is ERC721, AccessControl, Ownable {
  using Strings for uint256;
  using Counters for Counters.Counter;
  struct Collections {
    BottleCollection instance;
    uint256 collectionId;
    string POAPURI;
    bool exchanged;
    bool exist;
  }

  /// @dev role created for the exchange that is going to burn the tokens
  bytes32 public constant COLLECTIONS_MODIFICATOR_ROLE =
    keccak256("COLLECTIONS_MODIFICATOR_ROLE");
  bytes32 public constant MANUALLY_EXCHANGE_ROLE =
    keccak256("MANUALLY_EXCHANGE_ROLE");
  bytes32 public constant URI_MODIFICATOR_ROLE =
    keccak256("URI_MODIFICATOR_ROLE");

  /// @dev declaration of variables and structures
  Counters.Counter private _tokenIdCounter;
  mapping(address => Collections) public collections;
  mapping(uint256 => Collections) public collectionsById;
  uint256 lastCollection;
  mapping(address => uint256[]) ownerTokens;

  constructor(string memory _name, string memory _symbol)
    ERC721(_name, _symbol)
  {
    _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
    _tokenIdCounter.increment();
    lastCollection = 0;
  }

  function exchangeCollection(address _collection) public {
    require(
      collections[_collection].exist == true,
      "This collection doesn't exist yet"
    );
    require(
      _exists(collections[_collection].collectionId) == false,
      "This collection was already exchanged"
    );
    require(
      collections[_collection].instance.balanceOf(_msgSender()) ==
        collections[_collection].instance.maxSupply(),
      "You have to have all the collection in order to exchange it"
    );
    for (
      uint256 i = 1;
      i <= collections[_collection].instance.maxSupply();
      i++
    ) {
      collections[_collection].instance.burn(i);
    }
    collections[_collection].exchanged = true;
    _mint(_msgSender(), collections[_collection].collectionId);
    ownerTokens[_msgSender()].push(collections[_collection].collectionId);
    _tokenIdCounter.increment();
  }

  ///@dev this exchange function is going to be only for collections that won't be burned out
  function exchangeCollectionManually(address _collection, address receiver)
    public
    onlyRole(MANUALLY_EXCHANGE_ROLE)
  {
    require(
      collections[_collection].exist == true,
      "This collection doesn't exist yet"
    );
    require(
      _exists(collections[_collection].collectionId) == false,
      "This collection was already exchanged"
    );
    collections[_collection].exchanged = true;
    _mint(receiver, collections[_collection].collectionId);
    ownerTokens[receiver].push(collections[_collection].collectionId);
    _tokenIdCounter.increment();
  }

  function addCollection(address _collection, string memory _uri)
    public
    onlyRole(COLLECTIONS_MODIFICATOR_ROLE)
  {
    require(
      collections[_collection].exist != true,
      "This collection is already in the list"
    );
    collections[_collection].instance = BottleCollection(_collection);
    collections[_collection].POAPURI = _uri;
    collections[_collection].exchanged = false;
    collections[_collection].exist = true;
    collections[_collection].collectionId = lastCollection + 1;
    collectionsById[lastCollection + 1] = collections[_collection];
    lastCollection += 1;
  }

  function editCollection(address _collection, string memory _uri)
    public
    onlyRole(COLLECTIONS_MODIFICATOR_ROLE)
  {
    require(
      collections[_collection].exist == true,
      "This collection is not in the list"
    );
    collections[_collection].instance = BottleCollection(_collection);
    collections[_collection].POAPURI = _uri;
    collectionsById[collections[_collection].collectionId] = collections[
      _collection
    ];
  }

  /// @dev return the URI associated to a tokenId
  function tokenURI(uint256 tokenId)
    public
    view
    override(ERC721)
    returns (string memory)
  {
    return collectionsById[tokenId].POAPURI;
  }

  function totalSupply() external view returns (uint256) {
    return _tokenIdCounter.current();
  }

  /// @dev transfer functions
  function safeTransferFrom(
    address from,
    address to,
    uint256 tokenId,
    bytes memory _data
  ) public override {
    require(false, "POAPs are not transferable");
    _safeTransfer(from, to, tokenId, _data);
  }

  function transferFrom(
    address from,
    address to,
    uint256 tokenId
  ) public virtual override {
    require(false, "POAPs are not transferable");
    _transfer(from, to, tokenId);
  }

  // The following functions are overrides required by Solidity.
  function supportsInterface(bytes4 interfaceId)
    public
    view
    override(ERC721, AccessControl)
    returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }
}

