pragma solidity ^0.8.0;

import "./IPriceOracle.sol";
import "./BaseRegistrarImplementation.sol";
import "../registry/Homo.sol";



import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";

import {Resolver} from "../resolvers/Resolver.sol";

import {IReverseRegistrar} from "../reverseRegistrar/IReverseRegistrar.sol";

/**
 * @dev A registrar controller for registering and renewing names at fixed cost.
 */
contract PolygonRegistrarController is Ownable {

    uint constant public MIN_REGISTRATION_DURATION = 365 days;

    bytes4 constant private INTERFACE_META_ID = bytes4(keccak256("supportsInterface(bytes4)"));
    bytes4 constant private COMMITMENT_CONTROLLER_ID = bytes4(
        keccak256("rentAllPrice(string,uint256,bool)") ^
        keccak256("available(string)") ^
        keccak256("makeCommitment(string,address,bytes32)") ^
        keccak256("commit(bytes32)") ^
        keccak256("register(string,address,uint256,bytes32)") ^
        keccak256("renew(string,uint256)")
    );

    bytes4 constant private COMMITMENT_WITH_CONFIG_CONTROLLER_ID = bytes4(
        keccak256("registerWithConfig(string,address,uint256,bytes32,address,address)") ^
        keccak256("makeCommitmentWithConfig(string,address,bytes32,address,address)")
    );

    BaseRegistrarImplementation base;

    IPriceOracle prices;

    Homo  homo;
   
    mapping(address=>bool) allRoles;

    event NameRegistered(string name, bytes32 indexed label, address indexed owner, uint cost, uint expires);
    event NameRenewed(string name, bytes32 indexed label, uint cost, uint expires);
    event NewPriceOracle(address indexed oracle);
    event AddRoles(address indexed user);
    event RemoveRoles(address indexed user); 

    constructor(BaseRegistrarImplementation _base, Homo _homo, IPriceOracle _prices){
        base = _base;
        homo=_homo;
        prices = _prices;  
    }

     function rentPrice(
        string memory name,
        uint256 duration
    ) public view  returns (IPriceOracle.Price memory price) {
        bytes32 label = keccak256(bytes(name));
        price = prices.price(name, base.nameExpires(uint256(label)), duration);
    }

    function addRoles(address koler)public onlyOwner{
        allRoles[koler]=true;
        emit AddRoles(koler);
    }
    function removeRoles(address koler)public onlyOwner{
        allRoles[koler]=false;
        emit RemoveRoles(koler);
    }

    function rentAllPrice(
        string memory name,
        uint256 duration,
        bool discount
    ) public view  returns (IPriceOracle.Price memory price) {
         price = prices.priceAll(name,duration,discount);
    }


     function valid(string memory name) public pure returns (bool) {
        bytes memory nameBytes = bytes(name);
        
        if(nameBytes.length < 3 || nameBytes.length>=64) {
            return false;
        }
        
        for(uint i=0; i<nameBytes.length; i++) {
            uint8  char = uint8(nameBytes[i]);
            if(!((char >= 48 && char <= 57) || (char >= 65 && char <= 90) ||(char >= 97 && char <= 122) ||(char == 95))){
                return false;
            } 
        }
        return true;
    }

    function available(string memory name) public view returns(bool) {
        bytes32 label = keccak256(bytes(name));
        return valid(name) && base.available(uint256(label));
    }


    function registerHomo(
        string memory name, 
        address owner, 
        uint duration, 
        address roles,
        uint256 percent
    )public payable{
        registerAllWithConfig(name, owner, duration, roles, percent,  address(0), address(0));
    }

    function registerFirstHomo(
        string memory name, 
        address owner, 
        uint duration, 
        address roles,
        uint256 percent,
        address resolver, 
        address addr,
        address resverse
        )public payable{
            registerAllWithConfig(name, owner, duration,roles, percent,  resolver, addr);
            if (resverse != address(0)) {
                IReverseRegistrar(resverse).setName(owner,name);
            }
        }

     function registerAllWithConfig(
        string memory name, 
        address owner, 
        uint duration,
        address roles,
        uint256 percent,
        address resolver,
        address addr) public payable {
            bool discount=false;
            require(percent<=30);

            if (percent>10){
                require(allRoles[roles]);
            }

            if (percent>0){
                discount=true;
            }
           
            uint256 cost = _consumeAllCommitment(name, duration,discount);
            // uint256 cost = 100000000000000000;

            _registerWithConfig(name, owner,  duration, cost, resolver, addr);

            if(!isContract(roles)) {
                 uint256 rolesValue=cost*percent/100;
                 if (rolesValue>0){
                    payable(roles).transfer(rolesValue);
                 }
                payable(msg.sender).transfer(msg.value - (cost-rolesValue));
            }

        }

    function  _registerWithConfig(
        string memory name, 
        address owner,
        uint duration,
        uint256 cost,
        address resolver,
        address addr
    )internal{
            uint expires;
            bytes32 label = keccak256(bytes(name));
            uint256 tokenId = uint256(label);
            if(resolver != address(0)) {
                // Set this contract as the (temporary) owner, giving it
                // permission to set up the resolver.
                expires = base.register(tokenId, address(this), duration);
                // The nodehash of this label
                bytes32 nodehash = keccak256(abi.encodePacked(base.baseNode(), label));

                //Set the resolver
               homo.setResolver(nodehash, resolver);

                // Configure the resolver
                if (addr != address(0)) {
                    Resolver(resolver).setAddr(nodehash, addr);
                }

                // Now transfer full ownership to the expeceted owner
                base.reclaim(tokenId, owner);
                base.transferFrom(address(this), owner, tokenId);
            } else {
                require(addr == address(0));
                expires = base.register(tokenId, owner, duration);
            }
            emit NameRegistered(name, label, owner, cost, expires);
    }


    function renew(string memory name, uint duration) external payable {
         IPriceOracle.Price memory price = rentPrice(name, duration);

        require(msg.value >= price.base);

        bytes32 label = keccak256(bytes(name));
        uint expires = base.renew(uint256(label), duration);
        
        if(msg.value >  price.base) {
           payable(msg.sender).transfer(msg.value -  price.base);
        }

        emit NameRenewed(name, label,  price.base, expires);
    }

     function transferNode(address to,string calldata name)external{
        bytes32 label = keccak256(bytes(name));
        base.transferFrom(msg.sender, to, uint256(label));
     }

     function setUp(address to,string calldata name)external {
         bytes32 label = keccak256(bytes(name));
         base.transferOwer(msg.sender, to, uint256(label));
     }

    // function setAddress(string calldata name, address resolveaddress)external {
    //     // 判断是否为controller
    //      bytes32 label = keccak256(bytes(name));
    //      base.transferOwer(msg.sender, to, uint256(label));
    //  }

    function setPriceOracle(IPriceOracle _prices) public onlyOwner {
        prices = _prices;
        emit NewPriceOracle(address(prices));
    }

    function withdraw() public onlyOwner {
       payable(msg.sender).transfer(address(this).balance);
    }

    function supportsInterface(bytes4 interfaceID) external pure returns (bool) {
        return interfaceID == INTERFACE_META_ID ||
               interfaceID == COMMITMENT_CONTROLLER_ID ||
               interfaceID == COMMITMENT_WITH_CONFIG_CONTROLLER_ID;
    }
    // 计算金额
    function _consumeAllCommitment(
        string memory name, 
        uint duration, 
        bool discount
    ) internal returns (uint256) {
        require(available(name));

        IPriceOracle.Price memory price = rentAllPrice(name, duration,discount);

        require(duration >= MIN_REGISTRATION_DURATION && duration % MIN_REGISTRATION_DURATION==0);
        require(msg.value >= price.base);
        return price.base;
    }
    function isContract(address addr) internal view returns (bool) {
        uint256 size;
        assembly { size := extcodesize(addr) }
        return size > 0;
   }

  
}

