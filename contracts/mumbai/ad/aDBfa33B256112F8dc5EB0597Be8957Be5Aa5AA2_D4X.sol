import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@uniswap/lib/contracts/libraries/TransferHelper.sol";
import "@uniswap/v3-periphery/contracts/interfaces/IQuoterV2.sol";

import "./interfaces/IFactory.sol";
import "./interfaces/exchange/IExchange.sol";
import "./interfaces/ID4X.sol";
import "./interfaces/IAlp.sol";
import "./interfaces/IInsuranceFund.sol";
import "./interfaces/IExtraProtocol.sol";

import "./MixinResolver.sol";

import "hardhat/console.sol";

/**
 * @title D4X
 *
 * This is the contract with which the trader interacts. He can open and close a position.
 * Also positions can be liquidated by the liquidator.
 *
 */
contract D4X is ID4X, Ownable, MixinResolver {
    uint256 internal constant PERCENT_DECIMALS = 1e10;
    uint256 public constant YEAR_SECONDS = 31536000;
    uint256 public constant FEE_LEVERAGE_PERCENT = 1e9; // 10%

    /* ========== ADDRESS RESOLVER CONFIGURATION ========== */

    bytes32 private constant FACTORY_CONTRACT = "Factory";
    bytes32 private constant EXTRA_PROTOCOL_CONTRACT = "ExtraProtocol";
    bytes32 private constant INSURANCE_FUND_CONTRACT = "InsuranceFund";

    IFactory public _factory;
    IInsuranceFund public _iFund;
    IExtraProtocol public _extraProtocol;

    uint256 public _positionId;
    uint256 public _liquidationDeltaPercent = 1e9; // 10%
    uint256 public _liquidatorFeePercent = 5e6; // 0.05%
    uint256 public _alpFeePercent = 1e7; // 0.10%
    uint256 public _ifPercent = 1e9; // 10%
    uint256 public _liquidationCount = 10; // 10%

    /// @dev Holds a mapping of position ids to positions.
    mapping(uint256 => PositionParams) public _positions;

    /// @dev Holds a mapping of position ids to indexes in positionsArr.
    mapping(uint256 => uint256) public _positionIds;

    /// @dev Array of all positions
    uint256[] positionsArr;

    constructor(address resolver) MixinResolver(resolver) {}

    /**
     * @notice to set the addresses required for the contract
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function resolverAddressesRequired()
        public
        pure
        override
        returns (bytes32[] memory addresses)
    {
        addresses = new bytes32[](3);
        addresses[0] = FACTORY_CONTRACT;
        addresses[1] = EXTRA_PROTOCOL_CONTRACT;
        addresses[2] = INSURANCE_FUND_CONTRACT;
    }

    /**
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function _resolveAddressesFinish() internal override {
        _extraProtocol = IExtraProtocol(
            requireAndGetAddress(EXTRA_PROTOCOL_CONTRACT)
        );
        _factory = IFactory(requireAndGetAddress(FACTORY_CONTRACT));
        _iFund = IInsuranceFund(requireAndGetAddress(INSURANCE_FUND_CONTRACT));
    }

    /**
     * @notice Set new liquidation delta
     * @param val New value
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setLiquidationDelta(uint256 val) external onlyOwner {
        _liquidationDeltaPercent = val;
    }

    /**
     * @notice Set new liquidator fee percent
     * @param val New value
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setLiquidatorFeePercent(uint256 val) external onlyOwner {
        _liquidatorFeePercent = val;
    }

    /**
     * @notice Set new alp fee percent
     * @param val New value
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setAlpFeePercent(uint256 val) external onlyOwner {
        _alpFeePercent = val;
    }

    function setLiquidationCount(uint256 val) external onlyOwner {
        _liquidationCount = val;
    }

    /**
     * @notice Creating position
     *
     * @param tokenSell Token address to be sent
     * @param tokenBuy Token address to be swaped
     * @param amount Amount of `tokenSell`
     * @param leverage Leverage amount
     * @param path Best route for exchange
     *
     * Emits a `PositionCreated` event
     *
     * Requirements:
     *
     * - `amount` cannot be less then msg.sender balance.
     * -  alp for `tokenSell` must exist.
     * - `tokenBuy` must be available.
     * - `exchange` cannot be zero address.
     * - the caller must approve `token` for at least `amount`.
     *
     */
    function createPosition(
        address tokenSell,
        address tokenBuy,
        uint256 amount,
        uint256 leverage,
        bytes calldata path,
        IExchange.DEX dexType
    ) external {
        TransferHelper.safeTransferFrom(
            tokenSell,
            msg.sender,
            address(this),
            amount
        );

        address alpAddr = _factory.getAlp(tokenSell);
        require(alpAddr != address(0), "D4X: alp address is ZERO_ADDRESS");

        IAlp alp = IAlp(alpAddr);
        require(alp.isTokenAvailable(tokenBuy), "D4X: token is not available");

        (uint256 totalAmount, ) = alp.requestReserve(leverage, amount);

        IExchange exchange = _factory.getExchange(dexType);

        require(
            address(exchange) != address(0),
            "D4X: exchange address is ZERO_ADDRESS"
        );

        totalAmount += amount;

        TransferHelper.safeApprove(tokenSell, address(exchange), totalAmount);

        (, uint256 amountOut) = exchange.swap(
            IExchange.SwapParams({
                amountIn: totalAmount,
                amountOut: 0,
                tokenIn: tokenSell,
                tokenOut: tokenBuy,
                timestamp: block.timestamp,
                path: path,
                dexType: dexType,
                recipient: address(this)
            })
        );

        require(amountOut > 0, "D4X: amountOut 0");

        _positions[++_positionId] = PositionParams({
            amount: amount, // amount without leverage
            leverage: leverage,
            amountOut: amountOut, // amount tokens after swap
            timestamp: block.timestamp,
            tokenSell: tokenSell,
            tokenBuy: tokenBuy,
            trader: msg.sender,
            status: PositionStatus.OPEN,
            dexType: dexType
        });

        TransferHelper.safeApprove(
            tokenBuy,
            address(_extraProtocol),
            amountOut
        );
        _extraProtocol.deposit(amountOut, _positionId, tokenBuy);

        positionsArr.push(_positionId);
        _positionIds[_positionId] = positionsArr.length - 1;

        emit PositionCreated(_positionId);
    }

    /**
     * @notice Closing position
     *
     * @param id Position ID
     * @param path Best route for exchange
     *
     * @return position Current position by `id`
     * @return amountOut Amount after swap
     * @return extraProfit Profit from the extra protocols
     * @return alp of the position
     */
    function _closePosition(
        uint256 id,
        bytes calldata path
    )
        internal
        returns (
            PositionParams storage position,
            uint256 amountOut,
            uint256 extraProfit,
            IAlp alp
        )
    {
        position = _positions[id];

        address alpAddr = _factory.getAlp(position.tokenSell);
        require(alpAddr != address(0), "D4X: alp address is ZERO_ADDRESS");

        alp = IAlp(alpAddr);

        require(
            position.status == PositionStatus.OPEN,
            "D4X: Position is not open"
        );

        uint256 balance = _extraProtocol.balanceOf(id);
        _extraProtocol.withdraw(id);

        IExchange exchange = _factory.getExchange(position.dexType);
        TransferHelper.safeApprove(
            position.tokenBuy,
            address(exchange),
            balance
        );

        (, amountOut) = exchange.swap(
            IExchange.SwapParams({
                amountIn: balance,
                amountOut: 0,
                tokenIn: position.tokenBuy,
                tokenOut: position.tokenSell,
                timestamp: block.timestamp,
                path: path,
                dexType: position.dexType,
                recipient: address(this)
            })
        );

        // uint256 rate = amountOut / balance;
        extraProfit = ((balance - position.amountOut) * amountOut) / balance;

        uint256 lastId = positionsArr[positionsArr.length - 1];
        uint256 index = _positionIds[id];
        positionsArr[index] = lastId;
        _positionIds[lastId] = index;
        positionsArr.pop();
    }

    /**
     * @notice Distribution active between alp, trader and IF
     *
     * @param position Current position by `id`
     * @param amountOut Amount after swap
     * @param extraProfit Profit from the extra protocols
     * @param alp of the position
     *
     * @return alpAmount Amount transfered to alp
     * @return traderAmount Amount transfered to trader
     */
    function _distributionActive(
        PositionParams storage position,
        uint256 amountOut,
        uint256 extraProfit,
        IAlp alp
    )
        internal
        returns (uint256 alpAmount, uint256 traderAmount, uint256 feeToIf)
    {
        uint256 feeForLeverage = (amountOut *
            _calcFeeLeverage(position.timestamp)) / PERCENT_DECIMALS;

        feeToIf =
            (feeForLeverage * _ifPercent) /
            PERCENT_DECIMALS +
            (extraProfit * _ifPercent) /
            PERCENT_DECIMALS;

        uint256 leverageAmountOut = position.amount * (position.leverage - 1);

        alpAmount = leverageAmountOut + feeForLeverage + extraProfit - feeToIf;
        // amountOut += extraProfit;

        if (alpAmount + feeToIf > amountOut) {
            uint256 delta = alpAmount + feeToIf - amountOut;

            // If there are not enough tokens for upcoming transfers, the protocol requests
            // additional funds in IF.

            if (feeToIf < delta) {
                amountOut +=
                    _iFund.coverLoss(position.tokenSell, delta - feeToIf) +
                    feeToIf;
                feeToIf = 0;
            } else {
                feeToIf -= delta;
            }
        }

        // At the time of distribution actives between ALP, IF, liquidator and trader, the protocol
        // transfers maximum possible number for each side. The priority is ALP, IF and trader

        // 100 80 10

        if (amountOut > alpAmount) {
            traderAmount = amountOut - alpAmount - feeToIf;

            // 100 - 80 - 10 = 10 | 90

            TransferHelper.safeTransfer(
                position.tokenSell,
                position.trader,
                traderAmount
            );
        } else if (amountOut < alpAmount) {
            alpAmount = amountOut;
        }

        TransferHelper.safeApprove(position.tokenSell, address(alp), alpAmount);
        alp.returnReserve(leverageAmountOut, alpAmount);

        if (feeToIf > 0) {
            TransferHelper.safeTransfer(
                position.tokenSell,
                address(_iFund),
                feeToIf
            );
        }
    }

    /**
     * @notice Distribution active between alp, trader, IF and liquidator
     *
     * @param position Current position by `id`
     * @param amountOut Amount after swap
     * @param extraProfit Profit from the extra protocols
     * @param alp of the position
     *
     * @return alpAmount Amount transfered to alp
     * @return traderAmount Amount transfered to trader
     * @return liquidatorAmount Amount transfered to liquidator
     */
    function _distributionActiveLiquidation(
        PositionParams storage position,
        uint256 amountOut,
        uint256 extraProfit,
        IAlp alp
    )
        internal
        returns (
            uint256 alpAmount,
            uint256 traderAmount,
            uint256 liquidatorAmount,
            uint256 feeToIf
        )
    {
        uint256 feeForLeverage = (amountOut *
            _calcFeeLeverage(position.timestamp)) / PERCENT_DECIMALS;
        feeToIf =
            (feeForLeverage * _ifPercent) /
            PERCENT_DECIMALS +
            (extraProfit * _ifPercent) /
            PERCENT_DECIMALS;
        uint256 leverageAmountOut = position.amount * (position.leverage - 1);

        alpAmount = leverageAmountOut + feeForLeverage + extraProfit - feeToIf;
        liquidatorAmount =
            (amountOut * _liquidatorFeePercent) /
            PERCENT_DECIMALS;
        // amountOut += extraProfit;

        if (alpAmount + liquidatorAmount + feeToIf > amountOut) {
            uint256 delta = alpAmount + liquidatorAmount + feeToIf - amountOut;

            // If there are not enough tokens for upcoming transfers, the protocol requests
            // additional funds in IF.

            if (feeToIf < delta) {
                amountOut +=
                    _iFund.coverLoss(position.tokenSell, delta - feeToIf) +
                    feeToIf;
                feeToIf = 0;
            } else {
                feeToIf -= delta;
            }
        }

        // At the time of distribution actives between ALP, IF, liquidator and trader, the protocol
        // transfers maximum possible number for each side. The priority is ALP, IF, liquidator
        // and trader

        if (alpAmount < amountOut) {
            if (alpAmount + liquidatorAmount > amountOut) {
                liquidatorAmount = amountOut - alpAmount - feeToIf;
            } else if (alpAmount + liquidatorAmount < amountOut) {
                traderAmount =
                    amountOut -
                    alpAmount -
                    liquidatorAmount -
                    feeToIf;
                TransferHelper.safeTransfer(
                    position.tokenSell,
                    position.trader,
                    traderAmount
                );
            }
            TransferHelper.safeTransfer(
                position.tokenSell,
                msg.sender,
                liquidatorAmount
            );
        } else {
            alpAmount = amountOut;
        }

        TransferHelper.safeApprove(position.tokenSell, address(alp), alpAmount);
        alp.returnReserve(leverageAmountOut, alpAmount);

        if (feeToIf > 0) {
            TransferHelper.safeTransfer(
                position.tokenSell,
                address(_iFund),
                feeToIf
            );
        }
    }

    /**
     * @notice Closing position
     *
     * @param id Position ID
     * @param path Best route for exchange
     *
     * Emits a `PositionClose` event
     *
     * Requirements:
     *
     * -  position `id` must exist.
     * -  position must be open
     *
     */
    function closePosition(uint256 id, bytes calldata path) external {
        require(
            _positions[id].trader == msg.sender,
            "D4X: Only trader can close position"
        );

        (
            PositionParams storage position,
            uint256 amountOutFact,
            uint256 extraProfit,
            IAlp alp
        ) = _closePosition(id, path);

        (
            uint256 amountToAlp,
            uint256 amountToTrader,
            uint256 feeToIf
        ) = _distributionActive(position, amountOutFact, extraProfit, alp);

        position.status = PositionStatus.CLOSE;

        emit PositionClose(
            id,
            amountOutFact,
            amountToAlp,
            amountToTrader,
            feeToIf
        );
    }

    /**
     * @notice Fee leverage calculation
     * @param dateStart Date when position started
     * @return fee leverage
     */
    function _calcFeeLeverage(
        uint256 dateStart
    ) internal view returns (uint256) {
        return
            // 1e3 * 3600 / 31536000
            (FEE_LEVERAGE_PERCENT * (block.timestamp - dateStart)) /
            YEAR_SECONDS;
    }

    /**
     * @notice Checking if the position need to be liquidated
     *
     * @param position Current position
     * @param amountOut Amount after swap
     *
     * @return boolean to indicate if position need to be liquidated
     */
    function _liquidationCheck(
        PositionParams memory position,
        uint256 amountOut
    ) internal view returns (bool) {
        uint256 feeForLeveragePercent = _calcFeeLeverage(position.timestamp);

        uint256 marketRate = (position.amount * position.leverage * 1e18) /
            position.amountOut;
        uint256 marginCallRate = (marketRate *
            (position.leverage - 1) +
            (marketRate * feeForLeveragePercent) /
            PERCENT_DECIMALS +
            (marketRate * _liquidationDeltaPercent) /
            PERCENT_DECIMALS) / position.leverage;

        uint256 rate = (1e18 * amountOut) / position.amountOut;

        return rate <= marginCallRate;
    }

    /**
     * @notice Liquidation position
     *
     * @param id Position ID
     * @param path Best route for exchange
     *
     * Emits a `PositionLiquidation` event
     *
     * Requirements:
     *
     * -  position `id` must exist.
     * -  position must be open
     *
     */
    function liquidation(uint256 id, bytes calldata path) external {
        (
            PositionParams storage position,
            uint256 amountOut,
            uint256 extraProfit,
            IAlp alp
        ) = _closePosition(id, path);

        require(
            _liquidationCheck(position, amountOut),
            "D4X: Liquidation condition not met"
        );

        (
            uint256 alpAmount,
            uint256 traderAmount,
            uint256 liquidatorAmount,
            uint256 feeToIf
        ) = _distributionActiveLiquidation(
                position,
                amountOut,
                extraProfit,
                alp
            );

        position.status = PositionStatus.LIQUIDATION;

        emit PositionLiquidation(
            id,
            amountOut,
            alpAmount,
            traderAmount,
            liquidatorAmount,
            feeToIf,
            msg.sender
        );
    }

    /**
     * @notice Liquidation positions
     *
     * @param ids Position IDs
     * @param path Best routes for exchange
     *
     * Emits a `PositionLiquidation` event
     *
     * Requirements:
     *
     * -  position `ids` must exist.
     * -  positions must be open
     *
     */
    function liquidation(uint256[] memory ids, bytes[] calldata path) external {
        for (uint256 i = 0; i < ids.length; i++) {
            this.liquidation(ids[i], path[i]);
        }
    }

    /**
     * @return Array of all position ids that need to be liquidated
     */
    function getLiquidationPositions()
        external
        view
        returns (uint256[] memory)
    {
        uint256[] memory arr = new uint256[](positionsArr.length);
        uint256 count;
        PositionParams memory position;

        for (uint256 i = 0; i < positionsArr.length; i++) {
            position = _positions[positionsArr[i]];

            IExchange exchange = _factory.getExchange(position.dexType);

            require(
                address(exchange) != address(0),
                "D4X: exchange address is ZERO_ADDRESS"
            );

            uint256 amountOut = exchange.getAmountOut(
                position.tokenBuy,
                position.tokenSell,
                position.amountOut
            );

            if (amountOut == 0) continue;

            if (_liquidationCheck(position, amountOut)) {
                arr[count++] = positionsArr[i];

                if (count == _liquidationCount) break;
            }
        }

        uint256[] memory result = new uint256[](count);
        for (uint256 i = 0; i < count; i++) {
            result[i] = arr[i];
        }

        return result;
    }
}

