/**
 * @title DropuERC721AUpgradeable
 * @notice This contract contains shared logic to be inherited by all implementation contracts
 */
contract DropuERC721AUpgradeable is
ERC4907AUpgradeable,
ERC721AQueryableUpgradeable,
OwnableUpgradeable,
PausableUpgradeable,
ReentrancyGuardUpgradeable,
RevokableOperatorFiltererUpgradeable
{
    using DropuStorage for DropuStorage.State;

    uint256 public constant defaultDropuFeePerToken = 0.0007 ether;
    address public constant dropuPayoutAddress =
    0xea9d52ad8Ba7859491FcE4Ae43FafFB8884A6469;

    // ============ BASE FUNCTIONALITY ============

    /**
     * @dev Overrides the default ERC721A _startTokenId() so tokens begin at 1 instead of 0
     */
