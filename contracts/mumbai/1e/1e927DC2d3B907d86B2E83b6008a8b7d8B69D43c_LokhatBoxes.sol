import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./utils/GenesisRemover.sol";
contract Earnings{
    using GenesisRemover for GenesisRemover.FIFO;
    GenesisRemover.FIFO private fifo;
    using SafeMath for uint256;
    address private owner;
    uint256 public totalGenesisValue = 0;
    struct earning {
        uint256 value;
        uint256 earned;
    }
    mapping(address => earning) internal EarnedTokens;
    address[] private holders;
    uint256 _totalHolders = 0;
    uint256 public owner_percent = 80;
    constructor() {
        owner = msg.sender;
    }
    // ========== Errors ==========
    error No_earnings();
    function deleteExpiredGenesis() public {
        address _address = fifo.removeExpired();
        if(_address == address(0)) return;
        for (uint256 i = 0; i < holders.length; i++) { //TODO: optimiser
            if (holders[i] == _address) {
                holders[i] = holders[holders.length - 1];
                delete holders[holders.length - 1];
                holders.pop();
                return;
            }
        }
    }
    function removeGenesis(address _address,uint256 value) internal {//TODO:internal
        uint256 res_value=EarnedTokens[_address].value.sub(value,"You have not enought genesis");
        EarnedTokens[_address].value = res_value;
        if(res_value == 0){
            // delete this genesis owner and let him 1week to get is money
            fifo.addDeletion(_address);
        }
    }
    function transferGenesis(address _from,address _to,uint256 value) internal {//TODO:internal
        uint256 res_value=EarnedTokens[_from].value.sub(value,"You have not enought genesis");
        EarnedTokens[_to].value = EarnedTokens[_to].value.add(value);
        EarnedTokens[_from].value = res_value;
        if(res_value == 0){
            // delete this genesis owner and let him 1week to get is money

        }
    }
    function addGenesis(address _address,uint256 value) internal {//TODO:internal
        if(EarnedTokens[_address].value == 0){
            holders.push(_address);
        }
        EarnedTokens[_address].value = EarnedTokens[_address].value.add(value);
        totalGenesisValue = totalGenesisValue.add(value);
    }
    function Redistribute(uint256 value) internal {//TODO:internal
        uint256 owner_value = value.mul(owner_percent).div(100);
        EarnedTokens[owner].earned = EarnedTokens[owner].earned.add(owner_value);
        uint256 user_staying_value = value - owner_value;
        for (uint256 i = 0; i < holders.length; i++) {
            uint256 user_current_value = user_staying_value.mul(EarnedTokens[holders[i]].value).mul(100).div(totalGenesisValue);
            EarnedTokens[msg.sender].earned = EarnedTokens[msg.sender].earned.add(user_current_value);
            user_staying_value = user_staying_value.sub(user_current_value);
        }
        EarnedTokens[owner].earned = EarnedTokens[owner].earned.add(user_staying_value);
    }

    function getEarned() external view returns (uint256) {
        return EarnedTokens[msg.sender].earned;
    }
    function withdraw() external {
        if(EarnedTokens[msg.sender].earned == 0) revert No_earnings();
        uint256 value = EarnedTokens[msg.sender].earned;
        EarnedTokens[msg.sender].earned = 0;
        payable(msg.sender).transfer(value);
    }
}

