pragma solidity ^0.8.0;
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155URIStorage.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
// import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import './Earnings.sol';

contract Cards is Earnings,VRFConsumerBaseV2 {
    using SafeMath for uint256;

    // LinkTokenInterface LINKTOKEN;

    // Chainlink VRF Variables
    VRFCoordinatorV2Interface private immutable i_vrfCoordinator;
    uint64 private immutable i_subscriptionId;
    bytes32 private immutable i_gasLane;
    uint32 private immutable i_callbackGasLimit;
    uint16 private constant REQUEST_CONFIRMATIONS = 3;
    uint32 private constant NUM_WORDS = 1;
    mapping(uint256 => address) requestToSender;

    // cards limits
    uint16 public constant MAXIMUM_CASE = 1_000;
    uint256 public MintCardId = MAXIMUM_CASE+1;
    uint256 public immutable TIME_END_GENESIS;

    address internal owner;
    enum CardType {
        COMMON,
        RARE,
        EPIC,
        LEGENDARY,
        UNOBTAINABLE
    }
    struct MintableCard {
        string name;
        string uri;
        CardType cardType;
    }
    struct Card {
        string name;
        string uri;
        CardType cardType;
        bool genesis;
        uint256 date;
        uint256 id;
    }
    mapping(CardType => MintableCard[]) internal MintableCards;
    mapping(address => Card[]) internal MintedCards;
    // ============ Events ============
    event CardGenerated(string name, string url, CardType cardType);
    event BoxOpeningStarted(uint256 indexed requestId,uint256 indexed boxid,address indexed opener);
    // ============ Errors ============
    error Unauthorized();
    error Card_not_found();
    error Value_empty();
    error Maximum_reached();
    error Add_card_first();
    constructor(
        address vrfCoordinatorV2,
        uint64 subscriptionId,
        bytes32 gasLane, // keyHash
        uint32 callbackGasLimit
    ) VRFConsumerBaseV2(vrfCoordinatorV2) {
        i_vrfCoordinator = VRFCoordinatorV2Interface(vrfCoordinatorV2);
        i_gasLane = gasLane;
        i_subscriptionId = subscriptionId;
        i_callbackGasLimit = callbackGasLimit;
        TIME_END_GENESIS=block.timestamp + 3 weeks;
    }

    function mint(address, Card memory) internal virtual{
        revert("Not implemented");
    }

    function getMintableCard(CardType cardType,uint256 id) // if over enum limit it auto revert
        public
        view
        returns (MintableCard memory)
    {
        if(id >= MintableCards[cardType].length) revert Card_not_found();
        return MintableCards[cardType][id];
    }
    // function getMintables(CardType _cardType,uint256 id) // if over enum limit it auto revert
    //     public
    //     view
    //     OnlyOwner
    //     returns (MintableCard memory card)
    // {
    //     return getMintableCard(_cardType,id);
    // }

    function addCards(
        CardType[] memory cardType,
        string[] memory name,
        string[] memory url
    ) external OnlyOwner {
        for(uint256 i=0;i<cardType.length;i++){
            if(bytes(name[i]).length == 0 || bytes(url[i]).length == 0) revert Value_empty();
            if(MintableCards[cardType[i]].length >= MAXIMUM_CASE) revert Maximum_reached();
            MintableCards[cardType[i]].push(MintableCard(name[i], url[i],cardType[i]));
        }
    }
    function updateCard(
        CardType cardType,
        uint256 id,
        string memory _name,
        string memory _uri
    ) external OnlyOwner {
        if(id >= MintableCards[cardType].length) revert Card_not_found();
        if(bytes(_name).length == 0 || bytes(_uri).length == 0) revert Value_empty();
        MintableCard memory card = getMintableCard(cardType,id);
        card.name = _name;
        card.uri = _uri;
        card.cardType = cardType;
        MintableCards[cardType][id] = card;
    }
    function changeRarityCard(
        CardType cardType,
        uint256 id,
        CardType newCardType
    ) public OnlyOwner {//0.589kib
        require(cardType != newCardType);
        MintableCard memory card = getMintableCard(cardType,id);
        card.cardType = newCardType;
        MintableCards[newCardType].push(card);
        delete MintableCards[cardType][id];
    }

    function generateRandomCard(uint256 boxid) internal { //TODO: put the random case proba
        for(uint8 i=0;i<(uint8(CardType.LEGENDARY)+1);i++){
            if(MintableCards[CardType(i)].length ==0) revert Add_card_first();
        }
        uint256 requestId =i_vrfCoordinator.requestRandomWords(
            i_gasLane,
            i_subscriptionId,
            REQUEST_CONFIRMATIONS,
            i_callbackGasLimit,
            NUM_WORDS
        );
        // uint256 requestId = VRF_COORDINATOR.requestRandomWords(keyHash,VRF_subscriptionId,requestConfirmations,callbackGasLimit,numWords);
        requestToSender[requestId] = msg.sender;
        emit BoxOpeningStarted(requestId, boxid, msg.sender);
    }
    function extractBits(uint256 value,uint8 split) internal pure returns (uint256, uint256) {
        uint256 firstBits = value >> (256-split); // Shift the value 256-x bits to the right
        uint256 remainingBits = value & ((1 << (256-split)) - 1); // Mask the first x bits
        
        return (firstBits, remainingBits);
    }
    function pow(uint256 base, uint256 exponent) internal pure returns (uint256) {
        if (exponent == 0) {
            return 1;
        }else if (exponent == 1) {
            return base;
        }else if (base == 0 && exponent != 0) {
            return 0;
        }else {
            uint256 z = base;
            for (uint256 i = 1; i < exponent; i++){
                z = z.mul(base);
            }
            return z;
        }
    }
    function getRarity (uint256 random) internal pure  returns (CardType) {
        uint256 rarity = random % 100;
        if (rarity < 50) {
            return CardType.COMMON;
        }else if (rarity < 80) {
            return CardType.RARE;
        }else if (rarity < 95) {
            return CardType.EPIC;
        }else {
            return CardType.LEGENDARY;
        }
    }
    function fulfillRandomWords(
        uint256 requestId,
        uint256[] memory randomWords
    ) internal override{
        address sender = requestToSender[requestId];

        (uint256 cardTypeIndex,uint256 cardIndex) = extractBits(randomWords[0],8);
        CardType cardType = getRarity(cardTypeIndex);

        uint256 cards_of_type_length=MintableCards[cardType].length;
        // require(cards_of_type_length>0,"Contact admin, not all rarity have cards");
        if(cards_of_type_length==0) revert Card_not_found();
        cardIndex %= cards_of_type_length;

        MintableCard memory selectedCard = getMintableCard(cardType,cardIndex);
        
        bool is_genesis = block.timestamp < TIME_END_GENESIS;
        Card memory card_to_send = Card(
            selectedCard.name, 
            selectedCard.uri, 
            cardType, 
            is_genesis,
            block.timestamp,
            MintCardId++
        );

        if(is_genesis){
            addGenesis(sender,pow(uint256(cardType)+1,2));
        }
        MintedCards[sender].push(card_to_send); // Ajoute côté contrat l'adresse en owner de la carte
        mint(sender,card_to_send);
    }

    function cancelSubscription() external OnlyOwner {
        i_vrfCoordinator.cancelSubscription(i_subscriptionId, msg.sender);
    } 
    // function add_link_fund(uint96 amount) public {
    //     LINKTOKEN.transferAndCall(
    //         address(i_vrfCoordinator),
    //         amount,
    //         abi.encode(i_subscriptionId)
    //     );
    // }

    modifier OnlyOwner() {
        if(msg.sender != owner) revert Unauthorized();
        _;
    }
}

