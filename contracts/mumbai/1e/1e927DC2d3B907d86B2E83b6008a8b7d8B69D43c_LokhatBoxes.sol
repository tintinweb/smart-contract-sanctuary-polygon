contract LokhatBoxes is
    ERC1155,
    // AccessControl,
    ERC1155Burnable,
    ERC1155Supply,
    ERC1155URIStorage,
    // ContextMixin,
    // NativeMetaTransaction,
    Cards
{
    using SafeMath for uint256;
    uint256 public MintedBoxes = 0;
    string public constant name = "LokhateBoxes";
    enum CaseLevel {
        //TODO: plus un enum pour les types de caisses
        BASIC,
        BASICPLUS
    }

    struct Box {
        uint256 id;
        CaseLevel level;
        string name;
        uint256 mintedSupply; //added this to allow for tracking from 0
        uint256 supply;
        uint256 price;
    }

    Box[] public boxes;
    string public contractURI;
    // ========== Errors ==========
    error Not_enough_boxes();
    error Not_enough_money();
    error Max_boxes_reached();
    error Select_one();
    error Invalid_Id();
    error Sold_out();


    function uri(uint256 tokenId) public view override(ERC1155URIStorage, ERC1155) returns (string memory) {
        return super.uri(tokenId);
    }

    constructor(
        address vrfCoordinatorV2,
        uint64 subscriptionId,
        bytes32 gasLane, // keyHash
        uint32 callbackGasLimit,
        string memory _contractURI
    ) Cards(vrfCoordinatorV2, subscriptionId, gasLane, callbackGasLimit) ERC1155(name) {
        // _initializeEIP712(name);
        owner = msg.sender;
        contractURI=_contractURI;
    }

    function getBoxes() external view returns (Box[] memory) {
        return boxes;
    }

    function getBox(uint256 id) external view returns (Box memory) {
        return boxes[id];
    }

    function addBoxes(
        string[] memory _name,
        CaseLevel[] memory _caselevel,
        uint256[] memory _supply,
        uint256[] memory _price
    ) public onlyOwner {
        uint256 _id;
        for (uint256 i = 0; i < _name.length; i++) {
            _id = boxes.length;
            if(_id >= MAXIMUM_CASE) revert Max_boxes_reached();
            boxes.push(Box(_id, _caselevel[i], _name[i], 0, _supply[i], _price[i]));
        }
    }

    function updateBox(
        uint256 _id,
        CaseLevel _caselevel,
        string memory _name,
        uint256 _supply,
        uint256 _price
    ) external onlyOwner {
        if(_id >= boxes.length) revert Invalid_Id();
        Box memory box = boxes[_id];
        box.level = _caselevel;
        box.name = _name;
        box.supply = _supply;
        box.price = _price;
        boxes[_id] = box;
    }

    function buyBoxes(uint256[] memory ids) external payable  { //whenNotPaused
        if(ids.length == 0) revert Select_one();
        if(msg.value == 0) revert Not_enough_money();

        // Calculate the total cost
        uint256 mintCostTotal = 0;

        uint256[] memory amounts = new uint256[](boxes.length);

        for (uint256 i = 0; i < ids.length; i++) {
            if(ids[i] >= boxes.length) revert Invalid_Id();
            Box memory box = boxes[ids[i]];
            mintCostTotal = mintCostTotal.add(box.price);
            amounts[ids[i]]++;
            if(box.mintedSupply + amounts[i] > box.supply) revert Sold_out();
        }

        if(msg.value < mintCostTotal) revert Not_enough_money();
        uint256[] memory boxes_ids = new uint256[](boxes.length);
        for (uint256 i = 0; i < boxes.length; i++) {
            boxes[i].mintedSupply += amounts[i]; // Increment the minted supply for the box
            MintedBoxes = MintedBoxes.add(amounts[i]); // Increment the minted supply
            boxes_ids[i] = i;
        }

        _mintBatch(msg.sender, boxes_ids, amounts, "");
        Redistribute(mintCostTotal);
        // Refund any excess payment
        if (msg.value > mintCostTotal) {
            uint256 refundAmount = msg.value - mintCostTotal;
            payable(msg.sender).transfer(refundAmount);
        }
    }

    function openBox(uint256 boxid) external { //whenNotPaused
        if(balanceOf(msg.sender, boxid) == 0) revert Not_enough_boxes();
        _burn(msg.sender, boxid, 1);
        generateRandomCard(boxid);
    }

    function mint(address wallet, Card memory cardToMint) internal override(Cards) {
        _mint(wallet, cardToMint.id, 1, "");
        _setURI(cardToMint.id, cardToMint.uri);
        emit CardGenerated(cardToMint.name, cardToMint.uri, cardToMint.cardType);
    }

    function getBoxesByAddress(address wallet) internal view returns (uint256[] memory) {
        uint256[] memory amounts = new uint256[](boxes.length);

        for (uint256 i = 0; i < boxes.length; i++) {
            Box memory boxInLoop = boxes[i];
            amounts[i] = balanceOf(wallet, boxInLoop.id);
        }

        return amounts;
    }

    function getBoxesBought(address wallet) external view returns (uint256[] memory) {
        return getBoxesByAddress(wallet);
    }

    function getBoxesBought() external view returns (uint256[] memory) {
        return getBoxesByAddress(msg.sender);
    }

    modifier onlyOwner() {
        if(msg.sender != owner) revert Unauthorized();
        _;
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155, ERC1155Supply)  { //whenNotPaused
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }
    // function supportsInterface(
    //     bytes4 interfaceId
    // ) public view override(ERC1155, AccessControl) returns (bool) {
    //     return super.supportsInterface(interfaceId);
    // }

    // function pause() public onlyOwner {
    //     _pause();
    // }

    // function unpause() public onlyOwner {
    //     _unpause();
    // }

    //==================================================================================================================================================================

    function totalSupply(uint256 id) public view override returns (uint256) {
        if(id >= boxes.length) revert Invalid_Id();
        return boxes[id].supply;
    }
    // useless
    function totalSupply() external view returns (uint256) {//0.112kib
        uint256 total = 0;
        for (uint i = 0; i < boxes.length; i++) {
            total += boxes[i].supply;
        }
        return total;
    }


    // function mintedSupply() external view returns (uint256) {
    //     uint256 totalMinted = 0;
    //     for (uint i = 0; i < boxes.length; i++) {
    //         totalMinted += boxes[i].mintedSupply;
    //     }
    //     return totalMinted;
    // }

    // function mintedSupply(uint256 id) external view returns (uint256) {
    //     if(id >= boxes.length) revert Invalid_Id();
    //     return boxes[id].mintedSupply;
    // }


    // //
    // function _msgSender()
    //     internal
    //     override
    //     view
    //     returns (address sender)
    // {
    //     return ContextMixin.msgSender();
    // }

    // address[] internal proxys;//+0.208 kib
    // //0x2545943c4d9f6f4a617ceaba91bd13ed37def1ad
    // //0x53d791f18155c211ff8b58671d0f7e9b50e596ad
    // //0x207fa8df3a17d96ca7ea4f2893fcdcb78a304101
