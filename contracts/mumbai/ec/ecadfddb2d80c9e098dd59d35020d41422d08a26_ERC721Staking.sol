pragma solidity ^0.8.18;

contract ERC721Staking_4StakeLock is ERC721Staking_3StakeUnstake {
    using EnumerableSet for EnumerableSet.UintSet;

    address public sellCoinAddr;
    uint public nftLockSeconds;
    uint public nftSellPrice;

    function zUpdateSellDetails(
        address _sellCoinAddr,
        uint _nftLockSeconds,
        uint _nftSellPrice
    ) external onlyOwner {
        sellCoinAddr = _sellCoinAddr;
        nftLockSeconds = _nftLockSeconds;
        nftSellPrice = _nftSellPrice;
    }

    function unstakeNFTs(uint256[] calldata _tokenIds) external pure override {
        require(false, "unstakeNFTs is disabled");
        _tokenIds;
    }

    function sellNFTs(uint[] calldata _tokenIds) external whenNotPaused {
        // transfer remaining rewards of user generated by nft
        claimReward(_tokenIds);

        // transfer user NFTs to his adress
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            uint256 _currId = _tokenIds[i];
            // token ownership is already checked in claimReward()
            require(
                TokenInfoTable[_currId].stakedAt + nftLockSeconds <
                    block.timestamp,
                "NFT lock period is not over"
            );
            _clearStakeInfo(_currId);

            // transfer nft to staking-contract-owner
            IERC721(nftAddr).safeTransferFrom(address(this), owner(), _currId);
        }
        // nft-owner will get the sell-price of nft
        uint totalSellPrice = nftSellPrice * _tokenIds.length;
        if (sellCoinAddr == address(0)) {
            payable(msg.sender).transfer(totalSellPrice);
        } else {
            IERC20(sellCoinAddr).transfer(msg.sender, totalSellPrice);
        }
    }
}

