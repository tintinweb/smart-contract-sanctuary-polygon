contract ERC721Staking_2Admin is
    ERC721Staking_1View,
    PausableUpgradeable,
    OwnableUpgradeable
{
    using EnumerableSet for EnumerableSet.UintSet;

    function zUpdateRarity(
        uint256[] calldata _tokenIds,
        uint256[] calldata _rarities
    ) external onlyOwner {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            TokenInfoTable[_tokenIds[i]].rarityType = _rarities[i];
        }
    }

    function zUpdateRewardInfo(
        address _rwdAddr,
        uint256[5] memory _nftRwdPerSec,
        uint256 _stakingEndTime
    ) external onlyOwner {
        rwdAddr = _rwdAddr;
        nftRwdPerSec = _nftRwdPerSec;
        stakingEndTime = _stakingEndTime;
    }

    function zWithdrawRewards(
        address _tokenAddr,
        uint256 _amount
    ) external onlyOwner {
        if (_tokenAddr == address(0)) payable(owner()).transfer(_amount);
        else IERC20(_tokenAddr).transfer(owner(), _amount);
    }

    function zEmergencyNFTWithdraw(
        uint256[] calldata _tokenIds,
        address[] calldata _recipients
    ) external onlyOwner {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            IERC721(nftAddr).safeTransferFrom(
                address(this),
                _recipients[i],
                _tokenIds[i]
            );
        }
    }

    function zEmergenyDataModifications(
        uint256[] calldata _type,
        bool[] calldata _isAdd,
        uint256[] calldata _tokenId,
        address[] calldata _user,
        uint256[] calldata _time
    ) external onlyOwner {
        for (uint256 i = 0; i < _type.length; i++) {
            if (_isAdd[i] == false) {
                if (_type[i] == 1) {
                    TokenInfoTable[_tokenId[i]].lastCalcTime = 0;
                    TokenInfoTable[_tokenId[i]].tokenOwner = address(0);
                } else if (_type[i] == 2) {
                    UserInfoTable[_user[i]].stakedNftsSet.remove(_tokenId[i]);
                } else if (_type[i] == 3) {
                    allStakedTokenIds.remove(_tokenId[i]);
                }
            } else {
                if (_type[i] == 1) {
                    TokenInfoTable[_tokenId[i]].lastCalcTime = _time[i];
                    TokenInfoTable[_tokenId[i]].tokenOwner = _user[i];
                } else if (_type[i] == 2) {
                    UserInfoTable[_user[i]].stakedNftsSet.add(_tokenId[i]);
                } else if (_type[i] == 3) {
                    allStakedTokenIds.add(_tokenId[i]);
                }
            }
        }
    }

    function zPause() public onlyOwner {
        _pause();
    }

    function zUnpause() public onlyOwner {
        _unpause();
    }
}

// File contracts/ERC721Staking/ERC721Staking_3StakeUnstake.sol

pragma solidity ^0.8.18;

contract ERC721Staking_3StakeUnstake is ERC721Staking_2Admin {
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.UintSet;

    function stakeNFTs(uint256[] memory _tokenIds) external whenNotPaused {
        require(block.timestamp < stakingEndTime, "Staking period is ended");

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            uint256 _currId = _tokenIds[i];

            // update TokenInfo, UserInfo and allStakedTokendIds
            TokenInfoTable[_currId].tokenOwner = msg.sender;
            TokenInfoTable[_currId].stakedAt = block.timestamp;
            TokenInfoTable[_currId].lastCalcTime = block.timestamp;
            UserInfoTable[msg.sender].stakedNftsSet.add(_currId);
            allStakedTokenIds.add(_currId);

            // transfer nft from user-wallet to nftStake-contract
            IERC721(nftAddr).safeTransferFrom(
                msg.sender,
                address(this),
                _currId
            );
        }
    }

    event RewardsClaimed(address rewardRecipient, uint256 _rwdAmtClaimed);

    function claimReward(uint256[] calldata _tokenIds) public whenNotPaused {
        uint256 _totalRwd;
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            uint256 _currId = _tokenIds[i];
            require(
                TokenInfoTable[_currId].tokenOwner == msg.sender,
                "You are not owner of the NFT"
            );

            uint256 _currRwd = currentRewardOf(_currId);
            _totalRwd = _totalRwd.add(_currRwd);
            TokenInfoTable[_currId].rwdsClaimed = TokenInfoTable[_currId]
                .rwdsClaimed
                .add(_currRwd);
            TokenInfoTable[_currId].lastCalcTime = block.timestamp;
        }
        IERC20(rwdAddr).transfer(msg.sender, _totalRwd);
        emit RewardsClaimed(msg.sender, _totalRwd);
    }

    function _clearStakeInfo(uint256 _currId) internal {
        // make future rewards zero, remove token-ownership
        TokenInfoTable[_currId].lastCalcTime = 0; // 0 for nfts which are not staked
        TokenInfoTable[_currId].tokenOwner = address(0); // ZERO_ADDR for nfts which are not staked

        // remove nft from UserToken-list and allStakedNFTs-list
        UserInfoTable[msg.sender].stakedNftsSet.remove(_currId);
        allStakedTokenIds.remove(_currId);
    }

    function unstakeNFTs(
        uint256[] calldata _tokenIds
    ) external virtual whenNotPaused {
        // transfer remaining rewards of user generated by nft
        claimReward(_tokenIds);

        // transfer user NFTs to his adress
        for (uint256 i = 0; i < _tokenIds.length; i++) {
