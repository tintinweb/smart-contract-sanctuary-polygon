    string public constant VERSION = "1.3.0";

    //-----------------------------------------Method 1---------------------------------------------//
    function _authorizeUpgrade(address newImplementation) internal view override {
        (newImplementation);
        require(msg.sender == address(this), "BloctoAccount: only self");
    }

