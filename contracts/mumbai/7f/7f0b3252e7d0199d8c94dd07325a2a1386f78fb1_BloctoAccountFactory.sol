        bytes32 r,
        bytes32 s,
        uint256 nonce,
        address authorizedAddress,
        address newImplementation
    ) external onlyInvokeCosigner(v, r, s, nonce, authorizedAddress, abi.encodePacked(newImplementation)) {
        _upgradeTo(newImplementation);
    }

