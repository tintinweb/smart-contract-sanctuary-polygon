pragma solidity =0.8.9;

import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/utils/cryptography/SignatureChecker.sol";

import "./TokenHolder.sol";
import "../SmartWalletInitializer.sol";
import "../utils/SingletonArray.sol";
import "../interfaces/WyvernProxyRegistry.sol";

struct SenderData {
    bytes32 whitelist;
}

struct PartialLeaf {
    uint256 transferType;
    uint256 gasPrice;
    uint256 gas;
    uint256 walletOption; // 0 = no wallet, 1 = create smart wallet, 3 = create smart wallet + wyvern proxy
    address transportKey;
    bool confirm;
}

struct ReceiverData {
    address payable dest;
    uint256 walletOption;
}

struct ConfirmData {
    address payable from;
    uint256 walletOption;
    address payable dest;
    uint256 destWalletOption;
    bytes32 amountsHash;
    bytes data;
}

struct Token {
    uint256 ercId;
    address token;
    uint256 tokenId;
    uint256 amount;
}

abstract contract SignedTransferable is SingletonArray, EIP712, TokenHolder { // abstract so we don't have to define the contructor
    using SafeERC20 for IERC20;
    using Address for address;

    event Confirm(address indexed from, address indexed toOwner, bytes32 whitelistLeafHash, uint256[] ercIds, address[] tokens, uint256[] tokenIds, uint256[] amounts);

    address payable public immutable smartWalletInitializer;
    address private immutable wyvernRegistryProxy;
    OwnableDelegateProxy public userWyvernProxy;

    mapping(bytes32 => ConfirmData) internal confirms;
    mapping(address => bool) private voidTransportKeys; // prevents double spend

    constructor(address payable smartWalletInitializer_, address wyvernRegistryProxy_) EIP712("SmartWallet", "1.0.0") {
        require(smartWalletInitializer_ != address(0), "Zero Address");
        smartWalletInitializer = smartWalletInitializer_;
        wyvernRegistryProxy = wyvernRegistryProxy_; // can be address(0) on chains which don't support opensea
    }

    function registerWyvrenProxy() public virtual returns (address) { // can be called by anybody, allows someone else to pay for wyvren initialization
        require(address(userWyvernProxy) == address(0), "Already Initialized");
        WyvernProxyRegistry wyvrenRegistry = WyvernProxyRegistry(SignedTransferable(impl).getWyvernRegistryProxy());
        userWyvernProxy = wyvrenRegistry.proxies(address(this));
        if (address(userWyvernProxy) == address(0)) {
            userWyvernProxy = wyvrenRegistry.registerProxy();    
        }
        return address(userWyvernProxy);
    }

    function getUserWyvernProxy() public view returns (address) {
        return address(userWyvernProxy);
    }

    function getWyvernRegistryProxy() public view returns (address) {
        return wyvernRegistryProxy;
    }

    function getSmartWalletInitializer() public view returns (address payable smartWalletInitializer_) {
        return smartWalletInitializer;
    }

    function voidTransportKey(address transportKey) external onlyOwner {
        voidTransportKeys[transportKey] = true;
    }

    function isVoidTransportKey(address transportKey) external view returns(bool) {
        return voidTransportKeys[transportKey];
    }

    function _authSenderDataSignature(address signer, bytes32 whitelistRoot, bytes memory senderSignedRoot) private view returns (bool){
        bytes32 digest = _hashTypedDataV4(keccak256(abi.encode(
            keccak256("SenderData(bytes32 whitelist)"),
            whitelistRoot
        )));
        return recoverSigner(signer, digest, senderSignedRoot);
    }

    function _authReceiverDataSignature(address signer, ReceiverData memory receiverData, bytes memory data, bytes memory receiverSignedData) private view returns (bool) {
        bytes32 digest = _hashTypedDataV4(keccak256(abi.encode(
            keccak256("ReceiverData(address dest,uint256 walletOption,bytes data)"),
            receiverData.dest,
            receiverData.walletOption,
            keccak256(data)
        )));
        return recoverSigner(signer, digest, receiverSignedData);
    }

    function recoverSigner(address signer, bytes32 digest, bytes memory signature) private view returns (bool) {
        return SignatureChecker.isValidSignatureNow(signer, digest, signature); // supports smart wallets
    }

    function isInWhitelist(bytes32 whitelist, bytes memory leafData, bytes32[] memory whitelistProof) public pure returns (bool) {
        return MerkleProof.verify(whitelistProof, whitelist, keccak256(leafData));
    }

    function _isAuthorizedBySender(
        PartialLeaf memory pLeaf,
        bytes32 whitelistRoot,
        bytes32[] memory whitelistProof,
        bytes calldata whitelistLeaf,
        bytes calldata senderSignedRoot
    ) private view returns (bool) {
        return _authSenderDataSignature(address(this), whitelistRoot, senderSignedRoot) &&
            isInWhitelist(whitelistRoot, whitelistLeaf, whitelistProof) &&
            !voidTransportKeys[pLeaf.transportKey];
    }

    function signedTransfer(
        bytes calldata whitelistLeaf,
        bytes32 whitelistRoot,
        bytes32[] memory whitelistProof,
        bytes calldata senderSignedRoot,
        bytes calldata receiverData,
        bytes calldata receiverSignedData
    ) external nonReentrant {
        PartialLeaf memory pLeaf = abi.decode(whitelistLeaf, (PartialLeaf));
        require(pLeaf.transferType == 1, "Invalid Transfer Type");
        require(_isAuthorizedBySender(pLeaf, whitelistRoot, whitelistProof, whitelistLeaf, senderSignedRoot), "Sender Unauthorized");
        voidTransportKeys[pLeaf.transportKey] = true; // void the sender transfer key to ensure it's only used once

        ReceiverData memory _receiverData;
        bytes memory data;
        (_receiverData, data) = abi.decode(receiverData, (ReceiverData, bytes));
        require(_authReceiverDataSignature(pLeaf.transportKey, _receiverData, data, receiverSignedData), "Receiver Unauthorized");

        _handleTransfer(_receiverData, whitelistLeaf, data);
    }

    function signedTransferBatch(
        bytes calldata whitelistLeaf,
        bytes32 whitelistRoot,
        bytes32[] memory whitelistProof,
        bytes calldata senderSignedRoot,
        bytes calldata receiverData,
        bytes calldata receiverSignedData
    ) external nonReentrant {
        PartialLeaf memory pLeaf = abi.decode(whitelistLeaf, (PartialLeaf));
        require(pLeaf.transferType == 2, "Invalid Transfer Type");
        require(_isAuthorizedBySender(pLeaf, whitelistRoot, whitelistProof, whitelistLeaf, senderSignedRoot), "Sender Unauthorized");
        voidTransportKeys[pLeaf.transportKey] = true; // void the sender transfer key to ensure it's only used once

        ReceiverData memory _receiverData;
        bytes memory data;
        (_receiverData, data) = abi.decode(receiverData, (ReceiverData, bytes));
        require(_authReceiverDataSignature(pLeaf.transportKey, _receiverData, data, receiverSignedData), "Receiver Unauthorized");

        _handleBatchTransfer(_receiverData, whitelistLeaf, data);
    }

    function confirm(
        bytes32 whitelistLeafHash,
        uint256[] memory ercIds,
        address[] memory tokens,
        uint256[] memory tokenIds,
        uint256[] memory amounts
    ) external onlyOwner {
        ConfirmData memory _confirmData = confirms[whitelistLeafHash];
        require(keccak256(abi.encodePacked(ercIds, tokens, tokenIds, amounts)) == _confirmData.amountsHash, "Invalid Amounts");

        ReceiverData memory receiverData = ReceiverData(_confirmData.dest, _confirmData.destWalletOption);
        _confirmData.dest = _createReceiverWallet(receiverData, _confirmData.walletOption);

        _transferBatch(_confirmData.dest, ercIds, tokens, tokenIds, amounts, _confirmData.data);
        delete confirms[whitelistLeafHash];
    }

    function _handleTransfer(
        ReceiverData memory _receiverData,
        bytes calldata whitelistLeaf,
        bytes memory data
    ) private {
        PartialLeaf memory pLeaf;
        Token memory tokenObj;
        (pLeaf, tokenObj) = abi.decode(whitelistLeaf, (PartialLeaf, Token));
        if (pLeaf.confirm) {
            bytes32 amountsHash = keccak256(abi.encodePacked(
                _asSingletonArray(tokenObj.ercId),
                _asSingletonArray(tokenObj.token),
                _asSingletonArray(tokenObj.tokenId),
                _asSingletonArray(tokenObj.amount)
            ));
            bytes32 whitelistLeafHash = keccak256(whitelistLeaf);
            _saveConfirm(
                _receiverData, whitelistLeafHash, amountsHash, pLeaf.walletOption,
                _asSingletonArray(tokenObj.ercId),
                _asSingletonArray(tokenObj.token),
                _asSingletonArray(tokenObj.tokenId),
                _asSingletonArray(tokenObj.amount),
                data
            );
            return;
        }
        _receiverData.dest = _createReceiverWallet(_receiverData, pLeaf.walletOption);
        _transfer(_receiverData.dest, tokenObj.ercId, tokenObj.token, tokenObj.tokenId, tokenObj.amount, data);
