pragma solidity =0.8.9;

import "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import "@openzeppelin/contracts/utils/introspection/IERC165.sol";

import "../extensions/GasPayerWallet.sol";
import "../extensions/WETHHolder.sol";
import "../extensions/ERC721Holder.sol";
import "../extensions/ERC1155Holder.sol";

struct TransferBatch {
    uint256[] ids;
    uint256[] amounts;
}

struct TransferToken {
    uint256 ercId;
    address token;
    uint256 id;
    uint256 amount;
}

abstract contract TokenHolder is GasPayerWallet, WETHHolder, ERC721Holder, ERC1155Holder {
    function transfer(
        address payable dest,
        uint256 ercId,
        address token,
        uint256 tokenId,
        uint256 amount,
        bytes memory data
    ) external onlyOwner nonReentrant {
        _transfer(dest, ercId, token, tokenId, amount, data);
    }

    function _transfer(
        address payable dest,
        uint256 ercId,
        address token,
        uint256 tokenId,
        uint256 amount,
        bytes memory data
    ) internal {
        if (ercId == 1155) {
            _safeTransferFromERC1155(token, address(this), dest, tokenId, amount, data);
        } else if (ercId == 721 && amount == 1) {
            _safeTransferFromERC721(token, address(this), dest, tokenId, data);
        } else if (ercId == 20 && tokenId == 20) {
            _safeTransferERC20(token, dest, amount);
        } else if (ercId == 1 && token == address(0) && tokenId == 1) {
            _transferETH(amount, dest);
        } else {
            revert("Unsupported ercId");
        }
    }

    function transferBatch(
        address payable dest,
        uint256[] memory ercIds,
        address[] memory tokens,
        uint256[] memory tokenIds,
        uint256[] memory amounts,
        bytes memory data
    ) external onlyOwner nonReentrant {
        _transferBatch(dest, ercIds, tokens, tokenIds, amounts, data);
    }

    function _transferBatch(
        address payable dest,
        uint256[] memory ercIds,
        address[] memory tokens,
        uint256[] memory tokenIds,
        uint256[] memory amounts,
        bytes memory data
    ) internal {
        require(
                tokenIds.length == amounts.length &&
                ercIds.length == tokens.length &&
                tokens.length == amounts.length &&
                amounts.length > 0,
        "Invalid Lengths");

        TransferBatch memory batch = TransferBatch(
            new uint256[](tokens.length),
            new uint256[](tokens.length)
        );
        uint256 batchI;

        for (uint i = 0; i < tokenIds.length; i++) {
            TransferToken memory tokenObj = TransferToken(ercIds[i], tokens[i], tokenIds[i], amounts[i]);
            if (tokenObj.ercId == 1155) {
                batch.ids[batchI] = tokenObj.id;
                batch.amounts[batchI] = tokenObj.amount;
                batchI++;
                if (i + 1 == tokenIds.length) { // last loop
                    _safeBatchTransferFromERC1155(tokenObj.token, address(this), dest, batch.ids, batch.amounts, data);
                    break;
                }
            } else if (tokenObj.ercId == 721  && tokenObj.amount == 1) {
                _safeTransferFromERC721(tokenObj.token, address(this), dest, tokenObj.id, data);
            } else if (tokenObj.ercId == 20 && tokenObj.id == 20) { // token ercId == 20
                _safeTransferERC20(tokenObj.token, dest, tokenObj.amount);
            } else if (tokenObj.ercId == 1 && tokenObj.token == address(0) && tokenObj.id == 1) {
                _transferETH(tokenObj.amount, dest);
            } else {
                revert('ercId not supported');
            }

            if ( // Use ERC1155 batch send to cut down on gas costs, erc 1155s that can be batched must be in sequence or batch won't happen
                tokenObj.ercId == 1155 &&
                tokens[i + 1] != tokenObj.token
            ) {
                _safeBatchTransferFromERC1155(tokenObj.token, address(this), dest, batch.ids, batch.amounts, data);
                batch.ids = new uint256[](tokens.length - i);
                batch.amounts = new uint256[](tokens.length - i);
                batchI = 0;
            }
        }
    }

    function supportsInterface(bytes4 interfaceId) public pure override(ERC1155Holder, ERC721Holder) returns (bool) {
        return
            interfaceId == type(IERC721Receiver).interfaceId || // 0x150b7a02
            interfaceId == type(IERC1155Receiver).interfaceId || // 0x4e2312e0
            super.supportsInterface(interfaceId);
    }
}

