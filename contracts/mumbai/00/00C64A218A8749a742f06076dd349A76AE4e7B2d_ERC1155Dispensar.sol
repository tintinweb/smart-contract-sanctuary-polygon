        ReceiverData memory _receiverData,
        bytes32 whitelistLeafHash,
        bytes32 amountsHash,
        uint256 walletOption,
        uint256[] memory ercIds,
        address[] memory tokens,
        uint256[] memory tokenIds,
        uint256[] memory amounts,
        bytes memory data
    ) private {
        confirms[whitelistLeafHash] = ConfirmData(
            owner,
            walletOption,
            _receiverData.dest,    // validate this with receiver
            _receiverData.walletOption,
            amountsHash, // saves space
            data
        );
        emit Confirm(owner, _receiverData.dest, whitelistLeafHash, ercIds, tokens, tokenIds, amounts);
    }

    function _createReceiverWallet(ReceiverData memory _receiverData, uint256 walletOption) private returns (address payable) {
        require(_receiverData.walletOption <= walletOption, "Unapproved Wallet Creation");
        if (_receiverData.walletOption == 0) return _receiverData.dest; // don't make receiver a smart wallet, just direct deposit
        SmartWalletInitializer smartWalletReg = SmartWalletInitializer(SignedTransferable(impl).getSmartWalletInitializer());
        address payable wallet = smartWalletReg.initWallet(_receiverData.dest);
        if (_receiverData.walletOption >= 2) {
            SignedTransferable(wallet).registerWyvrenProxy();
        }
        return wallet;
    }
}

