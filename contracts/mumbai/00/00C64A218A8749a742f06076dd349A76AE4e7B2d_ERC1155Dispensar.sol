pragma solidity =0.8.9;

import "@openzeppelin/contracts/metatx/ERC2771Context.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/presets/ERC1155PresetMinterPauser.sol";

import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "@openzeppelin/contracts/utils/cryptography/SignatureChecker.sol";
import "@openzeppelin/contracts/metatx/MinimalForwarder.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";

import "../SmartWalletInitializer.sol";
import "../integrations/SignedTransferable.sol";

contract ERC1155Dispensar is ERC2771Context, Ownable, EIP712, ERC1155PresetMinterPauser {
    uint256 public constant totalPercent = 1000000;

    address public dispenseKeyPublic;
    uint256 public numRolls;
    uint256 public maxId;

    address public trustedForwarder;
    address payable public smartWalletInitializer;

    mapping(bytes32 => bool) voidList;

    struct InitialMint {
        uint256[] ids;
        uint256[] amounts;
    }

    struct MinterData {
        address minter;
        bytes32 receiverDataHash;
        uint256 entropy; // allows for a minter to give out multple uses to the same user
    }

    struct DispenserData {
        address payable dest;
        uint256 walletOption;
        bytes32 dataHash;
        bytes data;
        bytes signedReceiverData;
    }

    struct DispencePairing {
        uint256 rarity;
        uint256 amount;
        uint256 procStart;
        uint256 procEnd;
    }
    mapping(uint256 => DispencePairing) dispencePairings;

    struct InitDispensar {
        uint256[] initAmounts;
        uint256[] rarities;
        uint256[] despenseAmounts;
        uint256[] procStarts;
        uint256[] procEnds;
        uint256 numRolls;
    }

    constructor(
        string memory uri,
        address _trustedForwarder,
        address _dispenseKeyPublic,
        address payable _smartWalletInitializer,
        InitDispensar memory initConfig
    ) ERC2771Context(_trustedForwarder) ERC1155PresetMinterPauser(uri) EIP712("Dispensery", "1.0.0") {
        uint256[] memory initAmounts = initConfig.initAmounts;
        maxId = initAmounts.length - 1;
        // uint maxRaity = 0;
        for (uint id = 0; id <= maxId; id++) {
            require(initConfig.procStarts[id] >= 0 && initConfig.procStarts[id] <= maxId, "Invalid ProcStart");
            require(initConfig.procEnds[id] >= 0 && initConfig.procEnds[id] <= maxId, "Invalid ProcEnd");
            require(initConfig.despenseAmounts[id] > 0 && initConfig.despenseAmounts[id] <= initAmounts[id], "Invalid Disp Amt");
            require(initConfig.rarities[id] > 0, "Invalid Rarity");

            // require(maxRaity <= initConfig.rarities[id], "Invalid Rarity Order");
            // maxRaity = initConfig.rarities[id];

            _mint(address(this), id, initAmounts[id], "");
            dispencePairings[id] = DispencePairing(
                initConfig.rarities[id],
                initConfig.despenseAmounts[id],
                initConfig.procStarts[id],
                initConfig.procEnds[id]
            );
        }
        trustedForwarder = _trustedForwarder;
        dispenseKeyPublic = _dispenseKeyPublic;
        numRolls = initConfig.numRolls;
        smartWalletInitializer = _smartWalletInitializer;
    }

    function dispence(
        bytes calldata minterData,
        bytes calldata signedMinterData,
        DispenserData calldata receiverData
    ) public virtual returns(address) {
        MinterData memory _minterData = abi.decode(minterData, (MinterData));
        require(_authMinterDataSignature(_minterData, signedMinterData), "Minter Unauthorized");
        require(voidList[bytes32(signedMinterData)] == false, "Double Spend");
        voidList[bytes32(signedMinterData)] = true;

        require(
            receiverData.dataHash == keccak256(receiverData.data) &&
            keccak256(abi.encode(receiverData.dest, receiverData.walletOption, receiverData.dataHash)) == _minterData.receiverDataHash,
            "Invalid Data Hash"
        );
        require(authReceiverDataSignature(dispenseKeyPublic, receiverData), "Receiver Unauthorized");

        uint256[] memory ids;
        uint256[] memory amounts;
        (ids, amounts) = roll(signedMinterData);

        address payable dest = _createReceiverWallet(receiverData, 2);
        _safeBatchTransferFrom(address(this), dest, ids, amounts, receiverData.data);
        return dest;
    }

    function _createReceiverWallet(DispenserData memory _receiverData, uint256 walletOption) private returns (address payable) {
        require(_receiverData.walletOption <= walletOption, "Unapproved Wallet Creation");
        if (_receiverData.walletOption == 0) return _receiverData.dest; // don't make receiver a smart wallet, just direct deposit
        SmartWalletInitializer smartWalletReg = SmartWalletInitializer(smartWalletInitializer);
        address payable wallet = smartWalletReg.initWallet(_receiverData.dest);
        if (_receiverData.walletOption >= 2) {
            SignedTransferable(wallet).registerWyvrenProxy();
        }
        return wallet;
    }

    function roll(bytes calldata signedMinterData) internal returns(uint256[] memory, uint256[] memory) {
        uint256[] memory ids = new uint256[](numRolls);
        uint256[] memory amounts = new uint256[](numRolls);
        uint256[] memory accum = new uint256[](maxId + 1);

        uint startI = 0;
        uint endI = maxId;
        bool didDispense = false;
        for (uint j = 0; j < numRolls; j++) {
            uint256 chance = (uint(keccak256(abi.encodePacked(signedMinterData, j))) % totalPercent) + 1; // 0 => num NOT including number

            uint256 tokenPos = 0; // 1 indexed
            for (uint id = startI; id <= endI; id++) {
                uint256 rarity = dispencePairings[id].rarity;
                if (rarity == 0) continue; // no tokens left
                tokenPos = id + 1;
                if (chance <= rarity) break;
            }
            if (tokenPos == 0) break; // No more tokens to take
            uint256 tokenId = tokenPos - 1;
            ids[j] = tokenId;

            uint256 curBal = balanceOf(address(this), tokenId);
            if (accum[tokenId] + dispencePairings[tokenId].amount > curBal) {
                amounts[j] = curBal - accum[tokenId];
                didDispense = true;
                startI = dispencePairings[tokenId].procStart;
                endI = dispencePairings[tokenId].procEnd;
                delete dispencePairings[tokenId];
                continue;
            }
            amounts[j] = dispencePairings[tokenId].amount;

            startI = dispencePairings[tokenId].procStart;
            endI = dispencePairings[tokenId].procEnd;

            accum[tokenId] += dispencePairings[tokenId].amount;
            if (accum[tokenId] == curBal) {
                delete dispencePairings[tokenId];
            }
            didDispense = true;
        }
        require(didDispense, "Tokens Empty");
        return (ids, amounts);
    }

    function authReceiverDataSignature(address signer, DispenserData memory receiverData) public view returns (bool) {
        bytes32 digest = _hashTypedDataV4(keccak256(abi.encode(
            keccak256("ReceiverData(address dest,uint256 walletOption,bytes32 dataHash)"),
            receiverData.dest,
            receiverData.walletOption,
            receiverData.dataHash
        )));
        return recoverSigner(signer, digest, receiverData.signedReceiverData);
    }

    function setDispenseKey(address _dispenseKeyPublic) external onlyOwner {
        dispenseKeyPublic = _dispenseKeyPublic;
    }

    function _authMinterDataSignature(MinterData memory minterData, bytes memory signedMinterData) private view returns (bool) {
        bytes32 digest = _hashTypedDataV4(keccak256(abi.encode(
            keccak256("MinterData(address minter,bytes32 receiverDataHash,uint256 entropy)"),
            minterData.minter,
            minterData.receiverDataHash,
            minterData.entropy
        )));
        return hasRole(MINTER_ROLE, minterData.minter) && recoverSigner(minterData.minter, digest, signedMinterData);
    }

    function recoverSigner(address minter, bytes32 digest, bytes memory signature) private view returns (bool) {
        return SignatureChecker.isValidSignatureNow(minter, digest, signature);
    }

    function getMaxId() external view returns (uint256) {
        return maxId;
    }

    function _msgSender() internal view virtual override(ERC2771Context, Context) returns (address sender) {
        return ERC2771Context._msgSender();
    }

    function _msgData() internal view virtual override(ERC2771Context, Context) returns (bytes calldata) {
        return ERC2771Context._msgData();
    }
}

