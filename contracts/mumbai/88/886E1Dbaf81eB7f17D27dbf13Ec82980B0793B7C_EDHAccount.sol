pragma solidity ^0.8.9;

import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

// A value that, when set, must provide the tag matched at the time.
library TypedMaps {
    using EnumerableSet for EnumerableSet.Bytes32Set;

    struct TypedMap {
        EnumerableSet.Bytes32Set keys;
        mapping(bytes32=>TypedValue) values;
    }

    uint8 constant public VALUE_TYPE_ADDRESS = 0x10;
    uint8 constant public VALUE_TYPE_BYTES = 0x20;
    uint8 constant public VALUE_TYPE_BYTES32 = 0x21;
    uint8 constant public VALUE_TYPE_STRING = 0x2E;
    uint8 constant public VALUE_TYPE_UINT = 0x30;

    struct TypedValue {
        uint8   valueType;
        address addressValue;
        bytes   bytesValue;
        bytes32 bytes32Value;
        uint    uintValue;
    }

    // return previous tag
    function set(TypedMap storage self, bytes32 key, address value) internal {
        self.keys.add(key);
        self.values[key] = TypedValue({
            valueType: VALUE_TYPE_ADDRESS,
            addressValue: value,
            bytesValue: "",
            bytes32Value: bytes32(0),
            uintValue: 0
        });
    }

    function set(TypedMap storage self, bytes32 key, bytes memory value) internal {
        self.keys.add(key);
        self.values[key] = TypedValue({
            valueType: VALUE_TYPE_ADDRESS,
            addressValue: address(0),
            bytesValue: value,
            bytes32Value: bytes32(0),
            uintValue: 0
        });
    }

    function set(TypedMap storage self, bytes32 key, string memory value) internal {
        self.keys.add(key);
        self.values[key] = TypedValue({
            valueType: VALUE_TYPE_ADDRESS,
            addressValue: address(0),
            bytesValue: bytes(value),
            bytes32Value: bytes32(0),
            uintValue: 0
        });
    }

    function set(TypedMap storage self, bytes32 key, bytes32 value) internal {
        self.keys.add(key);
        self.values[key] = TypedValue({
            valueType: VALUE_TYPE_ADDRESS,
            addressValue: address(0),
            bytesValue: "",
            bytes32Value: value,
            uintValue: 0
        });
    }

    function set(TypedMap storage self, bytes32 key, uint value) internal {
        self.keys.add(key);
        self.values[key] = TypedValue({
            valueType: VALUE_TYPE_ADDRESS,
            addressValue: address(0),
            bytesValue: "",
            bytes32Value: bytes32(0),
            uintValue: value
        });
    }

    function getValue(TypedMap storage self, bytes32 key) internal view returns (TypedValue storage) {
        return self.values[key];
    }

    function getType(TypedMap storage self, bytes32 key) internal view returns (uint8) {
        return getValue(self, key).valueType;
    }

    function getAddress(TypedMap storage self, bytes32 key) internal view returns (address) {
        return getValue(self, key).addressValue;
    }

    function getBytes(TypedMap storage self, bytes32 key) internal view returns (bytes memory) {
        return getValue(self, key).bytesValue;
    }

    function getString(TypedMap storage self, bytes32 key) internal view returns (string memory) {
        return string(getValue(self, key).bytesValue);
    }

    function getBytes32(TypedMap storage self, bytes32 key) internal view returns (bytes32) {
        return getValue(self, key).bytes32Value;
    }

    function getUint(TypedMap storage self, bytes32 key) internal view returns (uint) {
        return getValue(self, key).uintValue;
    }

    function getKeys(TypedMap storage self) internal view returns (bytes32[] memory) {
        return self.keys.values();
    }
}

