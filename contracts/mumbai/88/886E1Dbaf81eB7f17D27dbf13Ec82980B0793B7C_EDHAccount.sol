abstract contract DeviceOwnerUpgradeable is ContextUpgradeable {
    using EnumerableSet for EnumerableSet.AddressSet;

    event EDHDeviceRegistered(address indexed device, bytes32 devicePublicKey, address approverDevice);
    event EDHDeviceRevoked(address indexed device, bytes32 devicePublicKey, address approverDevice);

    struct DeviceItem {
        // shared encryption public key at the time of registration
        bytes32 accountEncryptionPublicKey;
        bytes32 deviceEncryptionPublicKey;
        // nonce|nacl.box(device_name, DvEck, AcEcK)
        bytes deviceName;
    }

    struct DeviceItemView {
        address deviceAddress;
        // shared encryption public key at the time of registration
        bytes32 accountEncryptionPublicKey;
        bytes32 deviceEncryptionPublicKey;
        // nonce|nacl.box(device_name, DvEck, AcEcK)
        bytes deviceName;
    }

    EnumerableSet.AddressSet private activeAddrs;
    EnumerableSet.AddressSet private revokedAddrs;
    mapping(address=>DeviceItem) private items;

    function __DeviceOwner_init() internal onlyInitializing {
        __DeviceOwner_init_unchained();
    }

    function __DeviceOwner_init_unchained() internal onlyInitializing {
    }

    modifier onlyActiveDevice() {
        _checkActiveDevice();
        _;
    }

    /**
     * @dev Throws if the sender is not the owner device.
     */
    function _checkActiveDevice() internal view virtual {
        require(isActiveDevice(_msgSender()), "Caller is not active device");
    }

    function _addDevice(bytes32 acPubKey, address device, bytes32 dvPubKey, bytes memory dvName, address approver) internal {
        if (activeAddrs.add(device)) {
            items[device] = DeviceItem({
                accountEncryptionPublicKey: acPubKey,
                deviceEncryptionPublicKey: dvPubKey,
                deviceName: dvName
            });

            emit EDHDeviceRegistered(device, dvPubKey, approver);
        }
    }

    function _revokeDevice(address device, address approver) internal {
        if (activeAddrs.remove(device)) {
            revokedAddrs.add(device);
            bytes32 dvPubKey = items[device].deviceEncryptionPublicKey;

            emit EDHDeviceRevoked(device, dvPubKey, approver);
        }
    }

    function activeDeviceCount() internal view returns (uint) {
        return activeAddrs.length();
    }

    // return list of active device addresses.
    function activeDevices() public view returns (address[] memory) {
        return activeAddrs.values();
    }

    // return list of active device addresses.
    function revokedDevices() public view returns (address[] memory) {
        return revokedAddrs.values();
    }

    function isActiveDevice(address _address) public view returns (bool) {
        if (_address == address(0)) { return false; }
        return activeAddrs.contains(_address);
    }

    function isRevokedDevice(address _address) public view returns (bool) {
        if (_address == address(0)) { return false; }
        return revokedAddrs.contains(_address);
    }

    uint constant public DEVICE_STATUS_UNLINKED = 0;
    uint constant public DEVICE_STATUS_ACTIVE = 1;
    uint constant public DEVICE_STATUS_REVOKED = 2;

    function getDeviceStatus(address _address) public view returns (uint) {
        if (_address == address(0)) { return DEVICE_STATUS_UNLINKED; }
        if (activeAddrs.contains(_address)) { return DEVICE_STATUS_ACTIVE; }
        if (revokedAddrs.contains(_address)) { return DEVICE_STATUS_REVOKED; }
        return DEVICE_STATUS_UNLINKED;
    }

    function getActiveDevice(address _address) public view returns (DeviceItemView memory device) {
        if (activeAddrs.contains(_address)) {
            device.deviceAddress = _address;
            device.accountEncryptionPublicKey = items[_address].accountEncryptionPublicKey;
            device.deviceEncryptionPublicKey = items[_address].deviceEncryptionPublicKey;
            device.deviceName = items[_address].deviceName;
        }
    }

    function getActiveDevices() public view returns (DeviceItemView[] memory devices) {
        uint dvlen = activeAddrs.length();
        if (dvlen > 0) {
            devices = new DeviceItemView[](dvlen);
            for (uint i = 0; i < dvlen; i++) {
                DeviceItem storage item = items[activeAddrs.at(i)];
                devices[i].deviceAddress = activeAddrs.at(i);
                devices[i].accountEncryptionPublicKey = item.accountEncryptionPublicKey;
                devices[i].deviceEncryptionPublicKey = item.deviceEncryptionPublicKey;
                devices[i].deviceName = item.deviceName;
            }
        }
    }

    function getRevokedDevice(address _address) public view returns (DeviceItemView memory device) {
        if (revokedAddrs.contains(_address)) {
            device.deviceAddress = _address;
            device.accountEncryptionPublicKey = items[_address].accountEncryptionPublicKey;
            device.deviceEncryptionPublicKey = items[_address].deviceEncryptionPublicKey;
            device.deviceName = items[_address].deviceName;
        }
    }

