pragma solidity ^0.8.0;

import "hardhat/console.sol";

import "@openzeppelin/contracts/interfaces/IERC1271.sol";

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/introspection/ERC165Upgradeable.sol";
import "./DeviceOwnerUpgradeable.sol";
import "./ERC2771ContextUpgradeable.sol";
import "./TypedMaps.sol";

contract EDHAccount is OwnableUpgradeable, EIP712Upgradeable, DeviceOwnerUpgradeable, ERC2771ContextUpgradeable, ERC165Upgradeable {
    using ECDSA for bytes32;
    using TypedMaps for TypedMaps.TypedMap;

    event EDHAccountCreated(bytes32 accountPublicKey);
    event EDHAccountKeyUpdated(address indexed device, bytes32 newPublicKey, bytes32 prevPublicKey);

    // A device can sign a ticket for another device to self register.
    bytes32 private constant _REGISTER_DEVICE_TYPE_HASH =
        keccak256("RegisterDeviceTicket(address account,address device,bytes32 accountEncryptionPubKey,bytes32 deviceEncryptionPubKey,bytes deviceName)");

    address private registry;

    // States
    bytes32 [] encPublicKeys;
    TypedMaps.TypedMap kvMap;
    // mapping(bytes32=>KeyVersionedValue) kvValues;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {
        __Ownable_init();
        __EIP712_init("EDHAccount", "1");
        // We do not need trusted forwarder on master copy.
    }

    // Initialize
    function initialize(
        address _registry,
        address forwarder,
        bytes32 accountEncryptionPubKey,
        address deviceAddress,
        bytes32 deviceEncryptionPubKey,
        bytes memory deviceName
    ) public initializer {
        // Initializes the domain separator and parameter caches
        __Ownable_init();
        __EIP712_init("EDHAccount", "1");
        __ERC2771Context_init(forwarder);

        registry = _registry;
        encPublicKeys.push(accountEncryptionPubKey);

        _addDevice(accountEncryptionPubKey, deviceAddress, deviceEncryptionPubKey, deviceName, deviceAddress);

        emit EDHAccountCreated(accountEncryptionPubKey);
    }

    // ERC2771 - Meta Transactions
    function _msgSender() internal view virtual override(ContextUpgradeable, ERC2771ContextUpgradeable) returns (address sender) {
        return ERC2771ContextUpgradeable._msgSender();
    }

    function _msgData() internal view virtual override(ContextUpgradeable, ERC2771ContextUpgradeable) returns (bytes calldata) {
        return ERC2771ContextUpgradeable._msgData();
    }

    // EIP-1271: Standard Signature Validation Method for Contracts
    bytes4 constant internal EIP1271_MAGICVALUE = 0x1626ba7e;

    // EIP-1271: Standard Signature Validation Method for Contracts
    // Returns true is signed by active devices.
    function isValidSignature(
        bytes32 _hash, bytes memory _signature
    ) public view returns (bytes4 magicValue) {
        address signer = ECDSA.recover(_hash, _signature);
        if (isActiveDevice(signer)) {
                return EIP1271_MAGICVALUE;
        }
        return 0xffffffff;
    }

    // returns signature that used to be valid but is now revoked. (signed by now-revoked devices).
    function isRevokedSignature(
        bytes32 _hash, bytes memory _signature
    ) public view returns (bool) {
        address signer = ECDSA.recover(_hash, _signature);
        return isRevokedDevice(signer);
    }

    //ERC-165
    function supportsInterface(
        bytes4 interfaceId
    ) public view virtual override returns (bool) {
        return interfaceId == type(IERC1271).interfaceId
            || super.supportsInterface(interfaceId);
    }

    function getRegistry() public view returns (address) {
        return registry;
    }

    function encryptionPublicKey() public view returns (bytes32 key) {
        return encPublicKeys[encPublicKeys.length - 1];
    }

    function encryptionPublicKeys() public view returns (bytes32[] memory keys) {
        return encPublicKeys;
    }

    function updateEncryptionPublicKey(bytes32 key) public onlyActiveDevice {
        require(key != bytes32(0), "Key cannot be empty");

        bytes32 prvKey = encryptionPublicKey();
        encPublicKeys.push(key);

        emit EDHAccountKeyUpdated(_msgSender(), key, prvKey);
    }

    function registerDevice(
        bytes32 accountEncryptionPubKey,
        address deviceAddress,
        bytes32 deviceEncryptionPubKey,
        bytes memory deviceName
    ) public onlyActiveDevice {
        require(accountEncryptionPubKey == encryptionPublicKey(), "Outdate account public key");
        require(!(isActiveDevice(deviceAddress) || isRevokedDevice(deviceAddress)), "Duplicated device");

        _addDevice(accountEncryptionPubKey, deviceAddress, deviceEncryptionPubKey, deviceName, _msgSender());
    }

    function registerDeviceByTicket(
        bytes32 accountEncryptionPubKey,
        bytes32 deviceEncryptionPubKey,
        bytes memory deviceName,
        bytes memory signature
    ) public {
        address issuer = _hashTypedDataV4(
            keccak256(abi.encode(
                _REGISTER_DEVICE_TYPE_HASH,
                address(this), _msgSender(),
                accountEncryptionPubKey, deviceEncryptionPubKey,
                keccak256(deviceName)
            ))
        ).recover(signature);
        require(isActiveDevice(issuer), "Issuer is not active device");

        _addDevice(accountEncryptionPubKey, _msgSender(), deviceEncryptionPubKey, deviceName, issuer);
    }

    function revokeDevice(address deviceAddress) public onlyActiveDevice {
        require(isActiveDevice(deviceAddress), "Not active device");
        _revokeDevice(deviceAddress, _msgSender());
    }

    bytes32 constant public KEY_ACCOUNT_STORE_URI = keccak256("io.evernetwork.edh.account.uri");
    bytes32 constant public KEY_ACCOUNT_STORE_ROOT_KEY = keccak256("io.evernetwork.edh.account.key");
    bytes32 constant public KEY_TAGGED_PUBLIC_KEY = keccak256("io.evernetwork.edh.tagged-public-key");

    // Generic get/set BUT with current encryptionKey as a check.
    function setDataAddress(
        bytes32 key, bytes32 publicKey, address value
    ) public onlyActiveDevice {
        require(publicKey == encryptionPublicKey(), "Not active public key");

        bytes32 tagKey = keccak256(abi.encode(key, KEY_TAGGED_PUBLIC_KEY));
        kvMap.set(key, value);
        kvMap.set(tagKey, publicKey);
    }

    function setDataBytes(
        bytes32 key, bytes32 publicKey, bytes memory value
    ) public onlyActiveDevice {
        require(publicKey == encryptionPublicKey(), "Not active public key");
        bytes32 tagKey = keccak256(abi.encode(key, KEY_TAGGED_PUBLIC_KEY));
        kvMap.set(key, value);
        kvMap.set(tagKey, publicKey);
    }

    function setDataString(
        bytes32 key, bytes32 publicKey, string memory value
    ) public onlyActiveDevice {
        setDataBytes(key, publicKey, bytes(value));
    }

    function setDataBytes32(
        bytes32 key, bytes32 publicKey, bytes32 value
    ) public onlyActiveDevice {
        require(publicKey == encryptionPublicKey(), "Not active public key");
        bytes32 tagKey = keccak256(abi.encode(key, KEY_TAGGED_PUBLIC_KEY));
        kvMap.set(key, value);
        kvMap.set(tagKey, publicKey);
    }

    function setDataUint(
        bytes32 key, bytes32 publicKey, uint value
    ) public onlyActiveDevice {
        require(publicKey == encryptionPublicKey(), "Not active public key");
        bytes32 tagKey = keccak256(abi.encode(key, KEY_TAGGED_PUBLIC_KEY));
        kvMap.set(key, value);
        kvMap.set(tagKey, publicKey);
    }

    function getDataAddress(bytes32 key) public view returns (bytes32 publicKey, address addressValue) {
        bytes32 tagKey = keccak256(abi.encode(key, KEY_TAGGED_PUBLIC_KEY));
        publicKey = kvMap.getBytes32(tagKey);
        addressValue = kvMap.getAddress(key);
    }

    function getDataBytes(bytes32 key) public view returns (bytes32 publicKey, bytes memory bytesValue) {
        bytes32 tagKey = keccak256(abi.encode(key, KEY_TAGGED_PUBLIC_KEY));
        publicKey = kvMap.getBytes32(tagKey);
        bytesValue = kvMap.getBytes(key);
    }

    function getDataString(bytes32 key) public view returns (bytes32 publicKey, string memory stringValue) {
        bytes32 tagKey = keccak256(abi.encode(key, KEY_TAGGED_PUBLIC_KEY));
        publicKey = kvMap.getBytes32(tagKey);
        stringValue = kvMap.getString(key);
    }

    function getDataBytes32(bytes32 key) public view returns (bytes32 publicKey, bytes32 bytes32Value) {
        bytes32 tagKey = keccak256(abi.encode(key, KEY_TAGGED_PUBLIC_KEY));
        publicKey = kvMap.getBytes32(tagKey);
        bytes32Value = kvMap.getBytes32(key);
    }

    function getDataUint(bytes32 key) public view returns (bytes32 publicKey, uint uintValue) {
        bytes32 tagKey = keccak256(abi.encode(key, KEY_TAGGED_PUBLIC_KEY));
        publicKey = kvMap.getBytes32(tagKey);
        uintValue = kvMap.getUint(key);
    }

    function getAddress(bytes32 key) public view returns (address) {
        return kvMap.getAddress(key);
    }

    function getBytes(bytes32 key) public view returns (bytes memory) {
        return kvMap.getBytes(key);
    }

    function getString(bytes32 key) public view returns (string memory stringValue) {
        stringValue = kvMap.getString(key);
    }

    function getBytes32(bytes32 key) public view returns (bytes32 bytes32Value) {
        bytes32Value = kvMap.getBytes32(key);
    }

    function getUint(bytes32 key) public view returns (uint uintValue) {
        uintValue = kvMap.getUint(key);
    }

    function getEncryptionKey(bytes32 key) public view returns (bytes32 keyValue) {
        bytes32 tagKey = keccak256(abi.encode(key, KEY_TAGGED_PUBLIC_KEY));
        keyValue = kvMap.getBytes32(tagKey);
    }

    function setEncryptionKey(
        bytes32 key, bytes32 value
    ) public onlyActiveDevice {
        bytes32 tagKey = keccak256(abi.encode(key, KEY_TAGGED_PUBLIC_KEY));
        kvMap.set(tagKey, value);
    }

    function setAddress(
        bytes32 key, address value
    ) public onlyActiveDevice {
        kvMap.set(key, value);
    }

    function setBytes(
        bytes32 key, bytes memory value
    ) public onlyActiveDevice {
        kvMap.set(key, value);
    }

    function setString(
        bytes32 key, string memory value
    ) public onlyActiveDevice {
        setBytes(key, bytes(value));
    }

    function setBytes32(
        bytes32 key, bytes32 value
    ) public onlyActiveDevice {
        kvMap.set(key, value);
    }

    function setUint(
        bytes32 key, uint value
    ) public onlyActiveDevice {
        kvMap.set(key, value);
    }

    // for call execute function.
    struct Call {
        address target;
        bytes callData;
    }

    // Owner device can make account execute any call to other target.
    // Like MultiSigWallet.
    function execute(address target, bytes memory callData) public onlyActiveDevice returns (bytes memory) {
        return AddressUpgradeable.functionCall(target, callData, "execute call not succeeded");
    }

    // handle multicall on self instance.
    function multicall(bytes[] memory data) public returns (bytes[] memory results) {
        results = new bytes[](data.length);
        for (uint256 i = 0; i < data.length; i++) {
            results[i] = AddressUpgradeable.functionCall(address(this), data[i], "multicall: delegate call failed");
        }
        return results;
    }

}

