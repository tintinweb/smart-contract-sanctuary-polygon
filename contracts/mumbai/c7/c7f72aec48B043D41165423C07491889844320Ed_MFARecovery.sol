}

interface BalancePortfolioABT{

    function InclockedAmt(address, int256) external;
    function DeclockedAmt(address, int256) external;
    function IncunlockedAmt(address, int256) external; 
    function DecunlockedAmt(address, int256) external;
    function getLockedBalance(address) external view returns(int256);
    function getUnlockedBalance(address) external view returns(int256);
    function IncliquidityBalance(address _user, int256 amt) external;
    function DecliquidityBalance(address _user, int256 amt) external;
    function getliquidityBalance(address _user) external view returns(int256);
    function setOwner(address) external; 
    function getOwner() external view returns(address); 
}
interface SACStorage{
    function isEOARegistered(address EOA) external view returns(bool);
    function isSmartAccount(address SA) external view returns(bool);
    function getSA(address EOA) external view returns(address);
}

interface MFA{
    function changeMFAtoIntialState(address) external returns(bool);
    function checkApprovalStatus(address) external returns(bool);
}


contract vaultErc20 is Initializable{
    address public tokenAddress;
    string public tokenName;
    
    function initialize(address _tokenAddress, string memory _tokenName) external initializer {
        tokenAddress = _tokenAddress;
        tokenName = _tokenName;
    }

    event Deposit(address indexed depositor, address indexed to, uint256 amount);
    event Withdrawal(address indexed withdrawer,address indexed from, uint256 amount);
    function deposit(uint256 amount, address to, address erc20, address ABTerc20Addrs, address SACStorageAddrs) external {
        require(amount > 0, "Must deposit a positive amount");
        SACStorage sacStorage = SACStorage(SACStorageAddrs);
        bool isValid = sacStorage.isSmartAccount(msg.sender);
        require(isValid,"not a valid caller");
        ERC20Interface token = ERC20Interface(erc20);
        token.transferFrom(to, address(this), amount);
        BalancePortfolioABT balancePortfolioABT = BalancePortfolioABT(ABTerc20Addrs);
        balancePortfolioABT.IncunlockedAmt(msg.sender,int256(amount));
        emit Deposit(msg.sender,address(this),amount);
    }

    function withdraw(address user,uint256 amount,address reciever, address erc20, address ABTerc20Addrs, address SACStorageAddrs,address _MFAaddress) external {
        require(amount > 0, "Must withdraw a positive amount");
        MFA mfa = MFA(_MFAaddress);
        require(mfa.checkApprovalStatus(user),"Please approve to withdraw First");
        mfa.changeMFAtoIntialState(user);
        SACStorage sacStorage = SACStorage(SACStorageAddrs);
        bool isValid = sacStorage.isSmartAccount(msg.sender);
        require(isValid,"not a valid caller");
        BalancePortfolioABT balancePortfolioABT = BalancePortfolioABT(ABTerc20Addrs);
        int256 bal = balancePortfolioABT.getUnlockedBalance(msg.sender);
        require(bal>=int256(amount),"not sufficient");
        balancePortfolioABT.DecunlockedAmt(msg.sender,int256(amount));
        bool isSA = sacStorage.isSmartAccount(reciever);
        if(isSA ==true) {
            balancePortfolioABT.IncunlockedAmt(reciever,int256(amount));
        }
        else{
            ERC20Interface token = ERC20Interface(erc20);
            token.transfer(reciever, amount);
        }
        
        emit Withdrawal(msg.sender,address(this), amount);
    }
    function addLiquidity(int256 amount, address balancePortfolioABTAddres, address SACStorageAddrs) external{
        require(amount > 0, "Must add a positive amount");
        SACStorage sacStorage = SACStorage(SACStorageAddrs);
        bool isValid = sacStorage.isSmartAccount(msg.sender);
        require(isValid,"not a valid caller");

        BalancePortfolioABT balancePortfolioABT = BalancePortfolioABT(balancePortfolioABTAddres);
        int256 unlockBal = balancePortfolioABT.getUnlockedBalance(msg.sender);
        require(unlockBal >= amount, "not sufficient balance");
        balancePortfolioABT.DecunlockedAmt(msg.sender,amount);
        // LPBalanceABT lPBalanceABT = LPBalanceABT(LPABTaddres);
        // lPBalanceABT.IncLPBalanceAmt(address(this),amount);
        balancePortfolioABT.IncliquidityBalance(msg.sender,amount);
    }
    function removeLiquidity(int256 amount,address balancePortfolioABTAddres, address SACStorageAddrs) external {
        require(amount > 0, "Must remove a positive amount");
        SACStorage sacStorage = SACStorage(SACStorageAddrs);
        bool isValid = sacStorage.isSmartAccount(msg.sender);
        require(isValid,"not a valid caller");
        BalancePortfolioABT balancePortfolioABT = BalancePortfolioABT(balancePortfolioABTAddres);
        int256 liquidityBal = balancePortfolioABT.getliquidityBalance(msg.sender);
        require(liquidityBal >= amount, "not sufficient balance");
        // LPBalanceABT lPBalanceABT = LPBalanceABT(LPABTaddres);
        // lPBalanceABT.DecLPBalanceAmt(address(this),amount);
        balancePortfolioABT.IncunlockedAmt(msg.sender,amount);
        balancePortfolioABT.DecliquidityBalance(msg.sender,amount);
    }
    function LPmatch(address traderSA, address lpSA, address dextrSA, address buyToken, address sellToken, int256 orderAmt, int256 matchedAmt, int256 fees,address SACStorageAddrs) external {
        SACStorage sacStorage = SACStorage(SACStorageAddrs);
        bool isValid = sacStorage.isSmartAccount(msg.sender);
        require(isValid,"not a valid caller");
        int256 traderBal = BalancePortfolioABT(sellToken).getUnlockedBalance(traderSA);
        require(traderBal >= orderAmt, "not enough balance");
        int256 lpReward = (fees*2)/3;
        fees = fees - lpReward;
        int256 lpBal = BalancePortfolioABT(buyToken).getliquidityBalance(lpSA);
        require(lpBal >= matchedAmt, "lp has not enough balance");
        
        // trader settle
        BalancePortfolioABT(sellToken).DecunlockedAmt(traderSA,orderAmt+fees+lpReward);  
        BalancePortfolioABT(buyToken).IncunlockedAmt(traderSA,matchedAmt);

        // LP settle 
        BalancePortfolioABT(buyToken).DecliquidityBalance(lpSA,matchedAmt);
        BalancePortfolioABT(sellToken).IncliquidityBalance(lpSA,orderAmt+lpReward);

        // dextr fees send
        BalancePortfolioABT(sellToken).IncunlockedAmt(dextrSA,fees);
    }

   
}

