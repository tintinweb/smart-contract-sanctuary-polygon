pragma solidity ^0.8.0;
contract MFARecovery {
    address immutable public MFAGrpSbt;
    address immutable public MFAMembrSbt;
    address immutable public FactoryVaultAddrs;
    struct recovery {
       address anotherAddressForWithdrawl;
    }
    uint256 public MFA_Id = 1;
    mapping (address => bool) public isCreated;
    mapping (uint256 => bool) internal Idexist;
    mapping (uint256 => recovery) public MFAIdInfo;
    mapping (address => uint256) public userMFAId;
    constructor (address _MFAGrpSbt, address _MFAMembrSbt, address _FactoryVaultAddrs){
        MFAGrpSbt = _MFAGrpSbt;
        MFAMembrSbt = _MFAMembrSbt;
        FactoryVaultAddrs = _FactoryVaultAddrs;
    }
    event Recover(address Recover, uint256 RecoveryId);

    function CreateMFA(recovery memory _recovery, MFAGroupSbt.sbt memory _sbtGrp)
        external 
    {
        require(isCreated[msg.sender] == false, "MFA Group is already created");
        _sbtGrp.owner = msg.sender;
        userMFAId[msg.sender] = MFA_Id;
        MFAGroupSbt(MFAGrpSbt).mint(msg.sender,_sbtGrp,MFA_Id);
        MFAMemberSbt(MFAMembrSbt).mint(_recovery.anotherAddressForWithdrawl,MFA_Id);
        MFAIdInfo[MFA_Id] = _recovery;
        emit Recover(_recovery.anotherAddressForWithdrawl,MFA_Id);
        isCreated[msg.sender] = true;
        Idexist[MFA_Id] = true;
        MFA_Id++;
    }
    function approve(uint256 id) external {
        require(Idexist[id]==true, "Id does not exist");
        (address owner,bool isApproved) = MFAMemberSbt(MFAMembrSbt).getOwnerAndStatus(msg.sender,id);
        require(!isApproved,"alreay approved");
        require(msg.sender == owner, "not a correct MFAMembrSbt");
        MFAMemberSbt(MFAMembrSbt).changeApprovedStatus(msg.sender,id);
        MFAGroupSbt(MFAGrpSbt).updateCount(id);
        (int8 cnt, int8 req) = MFAGroupSbt(MFAGrpSbt).reqAndcnt(id); 
        if(cnt >= req){
            MFAGroupSbt(MFAGrpSbt).changeApprovalStatus(id);
        }
    }
    function checkApprovalStatus(address _user) external returns(bool){
        bool isValut = FactoryVault(FactoryVaultAddrs).getisVal(msg.sender);
        require(isValut,"not a valid caller");
        uint256 MFA_Id = userMFAId[_user];
        (int8 cnt, bool isTrue) = MFAGroupSbt(MFAGrpSbt).checkGroupApproveStatus(MFA_Id);
        return isTrue;
    }
    function changeMFAtoIntialState(address _user) external returns(bool){
        bool isValut = FactoryVault(FactoryVaultAddrs).getisVal(msg.sender);
        require(isValut,"not a valid caller");
        uint256 MFA_Id = userMFAId[_user];
        (int8 cnt, bool isTrue) = MFAGroupSbt(MFAGrpSbt).checkGroupApproveStatus(MFA_Id);
        require(isTrue, "Please approve first");
        // MFAMemberSbt(MFAMembrSbt).changeToIntialState(msg.sender,MFA_Id);
        MFAMemberSbt(MFAMembrSbt).changeToIntialState(MFAIdInfo[MFA_Id].anotherAddressForWithdrawl,MFA_Id);
        MFAGroupSbt(MFAGrpSbt).changeToIntialState(MFA_Id);
        return true;
    }
}