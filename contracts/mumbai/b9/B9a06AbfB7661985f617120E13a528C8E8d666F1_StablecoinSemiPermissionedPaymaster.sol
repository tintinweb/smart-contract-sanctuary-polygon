    * @dev Should only be used in execution.
    */
    function _collectStablecoin(PostOpMode mode, address sender, ChargeMode chargeMode, uint256 fee, uint256 maxFeePerGas, uint256 maxPriorityFeePerGas, uint256 actualGasCost) internal {
        uint256 gasPrice = getGasPrice(maxFeePerGas, maxPriorityFeePerGas);
        uint256 actualTokenCost = PaymasterUtils.calculateTotalTokenCost(chargeMode, fxRate, actualGasCost + COST_OF_POST * gasPrice, fee, tokenDecimal);
        if (actualTokenCost > 0) {
            IERC20 token = IERC20(supportedStablecoin);
            if (mode != PostOpMode.postOpReverted) {
                // do not attempt collecting upon 2nd revert so the paymaster is not getting throttled;
                // there could be potential sybil attacks which could be mitigated by reputation system
                token.safeTransferFrom(sender, address(this), actualTokenCost);
