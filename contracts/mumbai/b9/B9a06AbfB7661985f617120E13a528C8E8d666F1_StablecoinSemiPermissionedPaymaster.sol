/**
* For semi-permissioned mode, the fxRate will be maintained by Circle, no signature is required from offchain service.
*/
contract StablecoinSemiPermissionedPaymaster is BaseStablecoinPaymaster {

    using UserOperationLib for UserOperation;
    using PaymasterUtils for UserOperation;
    using PaymasterUtils for PaymasterMetaData;
    using SafeERC20 for IERC20;

    // constants still work for upgradable contracts because the compiler does not reserve storage slot
    // and every occurance is replaced by the respective constant expression
    uint256 private constant TIMESTAMP_START = 20;
    uint256 private constant METADATA_START = 84;

    function initialize(IEntryPoint _entryPoint, address _supportedStablecoin, uint8 _tokenDecimal) public initializer {
        entryPoint = _entryPoint;
        supportedStablecoin = _supportedStablecoin;
        tokenDecimal = _tokenDecimal;
        __UUPSUpgradeable_init();
        __Ownable_init();
        __Pausable_init();
        // TODO: setExchangeRate
    }

    /**
     * We can't really charge, increaseAllowance or approve here because they use modifier that accesses token storage.
     * paymasterAndData[:20] : address(this)
     * paymasterAndData[20:84] : abi.encode(validUntil, validAfter)
     */
    function _validatePaymasterUserOp(UserOperation calldata userOp, bytes32 userOpHash, uint256 maxCost)
    internal override returns (bytes memory context, uint256 validationData) {
        // unused
        (userOpHash, maxCost);

        // TODO: check the allowance in simulation then replenish OR we can do a multicall in postOp

        (uint48 validUntil, uint48 validAfter) = parsePaymasterAndData(userOp.paymasterAndData);

        senderNonce[userOp.getSender()]++;

        // testing purpose only
//        _collectStablecoin(PostOpMode.opSucceeded, userOp.sender, ChargeMode.GAS_ONLY, 0, userOp.maxFeePerGas, userOp.maxPriorityFeePerGas, maxCost);

        // we only allow gas_only for permissionless requests
        return (abi.encode(userOp.getSender(), userOp.maxFeePerGas, userOp.maxPriorityFeePerGas), _packValidationData(false, validUntil, validAfter));
    }

    /**
     * Perform the post-operation to refund the sender for the gas in most of cases.
     * In normal mode, use transferFrom to withdraw enough tokens from the sender's balance.
     * In case the transferFrom fails, the _postOp reverts and the entryPoint will call it again,
     * this time in *postOpReverted* mode.
     */
    function _postOp(PostOpMode mode, bytes calldata context, uint256 actualGasCost) internal override {
        (address sender, uint256 maxFeePerGas, uint256 maxPriorityFeePerGas) = abi.decode(context, (address, uint256, uint256));
        _collectStablecoin(mode, sender, ChargeMode.GAS_ONLY, 0, maxFeePerGas, maxPriorityFeePerGas, actualGasCost);
    }

    /**
     * paymasterAndData[:20] : address(this)
     * paymasterAndData[20:84] : abi.encode(validUntil, validAfter)
     */
    function parsePaymasterAndData(bytes calldata paymasterAndData)
    public pure returns (uint48 validUntil, uint48 validAfter) {
        (validUntil, validAfter) = abi.decode(paymasterAndData[TIMESTAMP_START : METADATA_START], (uint48, uint48));
    }
}

