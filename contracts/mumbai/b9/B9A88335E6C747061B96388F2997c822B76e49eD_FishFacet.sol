pragma solidity ^0.8.18;

import '@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol';
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";

import { LibDiamond } from "../libraries/LibDiamond.sol";

struct DumpedNFT {
  bytes32 uid;
  uint256 dbId;
  address from;
  address collection;
  uint256 tokenId;
  string transferTx;
  uint256 index;
  bool claimed;
  bool exists;
}

struct FishRequest {
  bool fulfilled;
  bool exists;
  bool hasClaim;
  address player;
  bytes32[] wonTokens;
}

struct AppStorage {
  mapping(bytes32 => DumpedNFT) dumpedNFTs;
  bytes32[] availableNFTs;

  address botAddress;
  address nftStorageAddress;

  mapping(uint256 => uint256) prices;
  mapping(uint256 => FishRequest) fishRequests;

  // Chainlink VRF2
  uint64 vrfSubscriptionId;
  uint32 vrfCallbackGasLimit;
  uint16 vrfRequestConfirmations;
  bytes32 vrfKeyHash;
  VRFCoordinatorV2Interface vrfCoordinator;
}

library LibAppStorage {
  function appStorage() internal pure returns (AppStorage storage s) {
    assembly {
      s.slot := 0
    }
  }
}

contract Modifiers {
  AppStorage internal s;

  modifier onlyBotOrOwner() {
    require(s.botAddress == msg.sender || LibDiamond.contractOwner() == msg.sender, "Not allowed");
    _;
  }
}

