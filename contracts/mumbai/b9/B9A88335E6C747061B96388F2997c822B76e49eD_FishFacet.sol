pragma solidity ^0.8.18;

import '@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol';
import '@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol';

import { Modifiers, FishRequest } from "../libraries/LibAppStorage.sol";

contract FishFacet is Modifiers {
  event NewFishingAttempt(
    uint256 indexed requestId,
    address indexed player,
    uint256 quantity
  );
  event RequestFulfilled(
    uint256 indexed requestId,
    uint256[] randomWords,
    bytes32[] wonTokens
  );

  error OnlyCoordinatorCanFulfill(address have, address want);

  function getPrice(uint256 _quantity) external view returns (uint256 value) {
    return s.prices[_quantity];
  }

  function getCoordinator() external view returns (address coordinator) {
    return address(s.vrfCoordinator);
  }

  function getRequest(uint256 _reqId) external view returns (FishRequest memory request) {
    return s.fishRequests[_reqId];
  }

  function fish(uint256 _quantity) external payable {
    require(s.prices[_quantity] != 0, "Invalid quantity");
    require(msg.value == s.prices[_quantity], "Invalid price");
    require(_quantity <= s.availableNFTs.length, "Not enough NFTs available");

    uint256 requestId = s.vrfCoordinator.requestRandomWords(
      s.vrfKeyHash,
      s.vrfSubscriptionId,
      s.vrfRequestConfirmations,
      s.vrfCallbackGasLimit,
      uint32(_quantity)
    );

    s.fishRequests[requestId] = FishRequest({
      fulfilled: false,
      exists: true,
      hasClaim: false,
      player: msg.sender,
      wonTokens: new bytes32[](0)
    });

    emit NewFishingAttempt(requestId, msg.sender, _quantity);
  }

  function fulfillRandomWords(uint256 _requestId, uint256[] memory _randomWords)
    internal
  {
    s.fishRequests[_requestId].fulfilled = true;

    for (uint256 i; i < _randomWords.length; i++) {
      
      uint256 rand;
      do {
        rand = uint256(_randomWords[i]) % s.availableNFTs.length;
        _randomWords[i]++;
      } while (inArray(s.fishRequests[_requestId].wonTokens, s.availableNFTs[rand]));
      
      s.fishRequests[_requestId].wonTokens.push(s.availableNFTs[rand]);
    }

    emit RequestFulfilled(_requestId, _randomWords, s.fishRequests[_requestId].wonTokens);
  }

  function rawFulfillRandomWords(
    uint256 _requestId,
    uint256[] memory _randomWords
  ) external {
    if(
      msg.sender != address(s.vrfCoordinator)
    ) {
      revert OnlyCoordinatorCanFulfill({
        have: msg.sender,
        want: address(s.vrfCoordinator)
      });
    }

    fulfillRandomWords(_requestId, _randomWords);
  }

  function inArray(bytes32[] memory arr, bytes32 value) internal pure returns (bool) {
    for (uint i = 0; i < arr.length; i++) {
      if (arr[i] == value) {
        return true;
      }
    }

    return false;
  }
}

