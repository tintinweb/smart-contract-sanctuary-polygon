pragma solidity ^0.8.0;

import {Factory} from "channels/v1/utils/Factory.sol";
import {EDITIONS_DIRECT_DROP, CHANNEL_TYPE} from "channels/v1/utils/Types.sol";
import {EditionsDirectDrop, Edition, EditionListing} from "channels/v1/drops/EditionsDirectDrop.sol";
import {Clones} from "@openzeppelin/contracts/proxy/Clones.sol";

contract EditionsDirectDropFactory is Factory {
    event EditionsDirectDropDeployed(
        address indexed drop,
        address indexed channel
    );

    error NotChannel();

    address public dropImpl;
    address public usdc;

    uint256 private constant ROYALTY_BPS = 6_000;

    constructor(
        address registry,
        address dropImpl_,
        address usdc_
    ) Factory(1, registry, EDITIONS_DIRECT_DROP) {
        dropImpl = dropImpl_;
        usdc = usdc_;
    }

    modifier onlyChannel() {
        if (!registry.isDeployment(CHANNEL_TYPE, msg.sender))
            revert NotChannel();
        _;
    }

    function createDrop(
        uint256 dropStartDate_,
        uint256 dropEndDate_,
        Edition[] memory editions_,
        EditionListing[] memory listings_,
        string memory name_,
        string memory symbol_,
        string memory series_,
        uint256 volume_,
        bytes32 salt
    ) public onlyChannel returns (address drop) {
        drop = predictDropAddress(salt);

        emit EditionsDirectDropDeployed(drop, msg.sender);

        Clones.cloneDeterministic(dropImpl, salt);

        EditionsDirectDrop(drop).initialize(
            msg.sender,
            usdc,
            dropStartDate_,
            dropEndDate_,
            editions_,
            listings_,
            name_,
            symbol_,
            series_,
            volume_,
            ROYALTY_BPS
        );

        _addDeployment(drop);
    }

    function predictDropAddress(
        bytes32 salt
    ) public view returns (address drop) {
        return Clones.predictDeterministicAddress(dropImpl, salt);
    }
}

