pragma solidity ^0.8.0;

import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {EditionsDropBase, Edition} from "./EditionsDropBase.sol";
import {IEditionsDirectDrop, EditionListing} from "./EditionsDirectDrop.i.sol";
import {EditionsDirectDropStorage} from "./EditionsDirectDropStorage.sol";
import {EditionsDropBaseStorage} from "./EditionsDropBaseStorage.sol";

import {NativeMetaTransaction} from "channels/v1/utils/NativeMetaTransaction.sol";
import {EDITIONS_DIRECT_DROP} from "channels/v1/utils/Types.sol";

contract EditionsDirectDrop is
    EditionsDropBase,
    IEditionsDirectDrop,
    NativeMetaTransaction
{
    using EditionsDirectDropStorage for EditionsDirectDropStorage.Layout;
    using EditionsDropBaseStorage for EditionsDropBaseStorage.Layout;

    // =============================================================
    //                          CONSTRUCTOR
    // =============================================================

    // Maybe move to initializer and use name_?
    constructor() NativeMetaTransaction(EDITIONS_DIRECT_DROP) {}

    function initialize(
        address channel_,
        address usdc_,
        uint256 dropStartDate_,
        uint256 dropEndDate_,
        Edition[] memory editions_,
        EditionListing[] memory listings_,
        string memory name_,
        string memory symbol_,
        string memory series_,
        uint256 volume_,
        uint256 royaltyBps_
    ) external initializerERC721A {
        __EditionsDirectDrop_init_unchained(
            channel_,
            usdc_,
            dropStartDate_,
            dropEndDate_,
            editions_,
            listings_,
            name_,
            symbol_,
            series_,
            volume_,
            royaltyBps_
        );
    }

    function __EditionsDirectDrop_init_unchained(
        address channel_,
        address usdc_,
        uint256 dropStartDate_,
        uint256 dropEndDate_,
        Edition[] memory editions_,
        EditionListing[] memory listings_,
        string memory name_,
        string memory symbol_,
        string memory series_,
        uint256 volume_,
        uint256 royaltyBps_
    ) internal onlyInitializingERC721A {
        if (dropStartDate_ >= dropEndDate_) revert InvalidDropWindow();

        __EditionsDropBase_init(
            editions_,
            name_,
            symbol_,
            series_,
            volume_,
            royaltyBps_
        );
        EditionsDirectDropStorage.layout()._channel = channel_;
        EditionsDirectDropStorage.layout()._usdc = IERC20(usdc_);
        EditionsDirectDropStorage.layout()._dropStartDate = dropStartDate_;
        EditionsDirectDropStorage.layout()._dropEndDate = dropEndDate_;

        emit DropSaleWindowSet(dropStartDate_, dropEndDate_);

        EditionListing[] storage _listings = EditionsDirectDropStorage
            .layout()
            ._listings;

        for (uint24 i; i < listings_.length; i++) {
            EditionListing memory _listing = listings_[i];
            _listings.push(_listing);
            emit EditionListingSet(i, _listing.quantity, _listing.price);
        }
    }

    modifier onlyChannel() {
        if (msg.sender != EditionsDirectDropStorage.layout()._channel)
            revert Unauthorized();
        _;
    }

    // =============================================================
    //                          GETTERS
    // =============================================================

    function listings(
        uint24 editionNumber_
    ) public view returns (EditionListing memory listing) {
        EditionListing[] storage _listings = EditionsDirectDropStorage
            .layout()
            ._listings;
        if (editionNumber_ > _listings.length) revert InvalidEdition();
        return _listings[editionNumber_];
    }

    function dropStartDate() public view returns (uint256) {
        return EditionsDirectDropStorage.layout()._dropStartDate;
    }

    function dropEndDate() public view returns (uint256) {
        return EditionsDirectDropStorage.layout()._dropEndDate;
    }

    function mint(
        uint256 quantity_,
        uint24 editionNumber_,
        address recipient_
    ) public onlyChannel {
        _validateDropWindow();

        EditionListing storage listing = _getListing(editionNumber_);

        if (quantity_ > listing.quantity) revert SoldOut();

        _mint(quantity_, editionNumber_, recipient_);

        listing.quantity -= quantity_;
    }

    /**
     * @dev assumes this contract is pre-approved for USDC transfer of at least quantity * price
     */
    function purchaseEditionFor(
        uint256 quantity_,
        uint24 editionNumber_,
        address recipient_
    ) public {
        _validateDropWindow();

        EditionListing storage listing = _getListing(editionNumber_);

        if (quantity_ > listing.quantity) revert SoldOut();

        address paymentTo = EditionsDropBaseStorage
            .layout()
            ._editions[editionNumber_]
            .recipient;

        uint256 totalPrice = listing.price * quantity_;

        EditionsDirectDropStorage.layout()._usdc.transferFrom(
            _msgSender(),
            paymentTo,
            totalPrice
        );

        // Mint double checks if valid edition
        _mint(quantity_, editionNumber_, recipient_);

        listing.quantity -= quantity_;

        emit PrimarySale(
            _msgSender(),
            recipient_,
            editionNumber_,
            quantity_,
            totalPrice
        );
    }

    function purchaseEdition(uint256 quantity_, uint24 editionNumber_) public {
        purchaseEditionFor(quantity_, editionNumber_, _msgSender());
    }

    /**
     *   @dev Set drop window start and end dates. Can't adjust after drop has concluded.
     */
    function updateDropWindow(
        uint256 dropStartDate_,
        uint256 dropEndDate_
    ) external onlyChannel {
        if (block.timestamp > EditionsDirectDropStorage.layout()._dropEndDate)
            revert DropConcluded();

        if (dropStartDate_ >= dropEndDate_) revert InvalidDropWindow();

        EditionsDirectDropStorage.layout()._dropStartDate = dropStartDate_;
        EditionsDirectDropStorage.layout()._dropEndDate = dropEndDate_;

        emit DropSaleWindowSet(dropStartDate_, dropEndDate_);
    }

    /**
     * @dev Set listing price and quantity. Can't adjust after drop has concluded.
     */
    function updateListing(
        uint24 editionNumber_,
        EditionListing memory listing_
    ) external onlyChannel {
        if (block.timestamp > EditionsDirectDropStorage.layout()._dropEndDate)
            revert DropConcluded();

        EditionListing[] storage _listings = EditionsDirectDropStorage
            .layout()
            ._listings;

        if (editionNumber_ > _listings.length) revert InvalidEdition();
        _listings[editionNumber_] = listing_;

        emit EditionListingSet(
            editionNumber_,
            listing_.quantity,
            listing_.price
        );
    }

    /**
     * @dev Update the metadata or recipient of an edition in case of emergency.
     */
    function updateEdition(
        uint24 editionNumber_,
        Edition memory edition_
    ) external onlyChannel {
        _validateMetadataUnfrozen();

        _updateEdition(editionNumber_, edition_);
    }

    /**
     * @dev Freezes the drop metadata. Is locked permanently after.
     */
    function freezeMetadata() external onlyChannel {
        _freezeMetadata();
    }

    function _msgSenderERC721A() internal view override returns (address) {
        return _msgSender();
    }

    /**
     * @dev Validates timestamp against the drop window
     */
    function _validateDropWindow() internal view {
        if (block.timestamp < EditionsDirectDropStorage.layout()._dropStartDate)
            revert DropNotStarted();

        if (block.timestamp > EditionsDirectDropStorage.layout()._dropEndDate)
            revert DropConcluded();
    }

    /**
     * @dev Get the listing based on editionNumber_. Checks if editionNumber_ is valid
     */
    function _getListing(
        uint256 editionNumber_
    ) internal view returns (EditionListing storage listing) {
        EditionListing[] storage _listings = EditionsDirectDropStorage
            .layout()
            ._listings;

        if (editionNumber_ > _listings.length) revert InvalidEdition();

        listing = _listings[editionNumber_];
    }
}

