    //                          CONSTRUCTOR
    // =============================================================

    function __EditionsDropBase_init(
        Edition[] memory editions_,
        string memory name_,
        string memory symbol_,
        string memory series_,
        uint256 volume_,
        uint256 royaltyBps_
    ) internal onlyInitializingERC721A {
        __EditionsDropBase_init_unchained(
            editions_,
            name_,
            symbol_,
            series_,
            volume_,
            royaltyBps_
        );
    }

    function __EditionsDropBase_init_unchained(
        Edition[] memory editions_,
        string memory name_,
        string memory symbol_,
        string memory series_,
        uint256 volume_,
        uint256 royaltyBps_
    ) internal onlyInitializingERC721A {
        __ERC721A_init(name_, symbol_);
        EditionsDropBaseStorage.layout()._series = series_;
        EditionsDropBaseStorage.layout()._volume = volume_;
        EditionsDropBaseStorage.layout()._royaltyBps = royaltyBps_;

        Edition[] storage editions = EditionsDropBaseStorage.layout()._editions;

        for (uint24 i; i < editions_.length; i++) {
            Edition memory _edition = editions_[i];
            editions.push(_edition);
            emit EditionSet(i, _edition.arweaveCID, _edition.recipient);
        }

        emit DropCreated(msg.sender, name_, symbol_, series_, volume_);
    }

    // =============================================================
    //                          GETTERS
    // =============================================================

    function series() public view returns (string memory) {
        return EditionsDropBaseStorage.layout()._series;
    }

    function volume() public view returns (uint256) {
        return EditionsDropBaseStorage.layout()._volume;
    }

    function getEdition(
        uint24 editionNumber_
    ) public view returns (Edition memory edition) {
        Edition[] storage editions = EditionsDropBaseStorage.layout()._editions;
        if (editionNumber_ > editions.length) revert InvalidEdition();
        return editions[editionNumber_];
    }

