    //                          INTERNAL
    // =============================================================

    function _mint(
        uint256 quantity,
        uint24 editionNumber,
        address to
    ) internal virtual {
        if (editionNumber >= EditionsDropBaseStorage.layout()._editions.length)
            revert InvalidEdition();

        uint256 fromTokenId = _nextTokenId();
        EditionsDropBaseStorage.layout()._editionNumber = editionNumber;

        _mint(to, quantity);

        EditionsDropBaseStorage.layout()._editionNumber = 0;

        emit TokenEditionSet(
            fromTokenId,
            fromTokenId + quantity,
            editionNumber
