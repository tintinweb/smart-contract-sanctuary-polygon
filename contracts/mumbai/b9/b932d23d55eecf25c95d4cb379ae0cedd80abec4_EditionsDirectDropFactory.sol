     * @dev Returns the drop royalty information
     */
    function royaltyInfo(
        uint256 _tokenId,
        uint256 _salePrice
    ) external view override returns (address receiver, uint256 royaltyAmount) {
        receiver = EditionsDropBaseStorage
            .layout()
            ._editions[getEditionNumber(_tokenId)]
            .recipient;

        royaltyAmount =
            (_salePrice * EditionsDropBaseStorage.layout()._royaltyBps) /
            1_000_000;
