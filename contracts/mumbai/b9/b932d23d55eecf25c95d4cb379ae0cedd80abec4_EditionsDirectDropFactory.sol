        // if not a mint, return previously set edition number for batch
        return previousExtraData;
    }

    function _setEdition(
        uint256 tokenId,
        uint24 editionNumber
    ) internal virtual {
        // Initialize ownership struct at tokenId incase it is in the middle of a batch
        _initializeOwnershipAt(tokenId);
        // Also initialize the next tokenId too so setting extra data
        // doesn't affect tokens ahead of us in same batch
        _initializeOwnershipAt(tokenId + 1);
        _setExtraDataAt(tokenId, editionNumber);

        emit TokenEditionSet(tokenId, tokenId + 1, editionNumber);
    }

    function _updateEdition(
        uint24 editionNumber_,
        Edition memory edition_
    ) internal virtual {
        Edition[] storage editions = EditionsDropBaseStorage.layout()._editions;
        if (editionNumber_ > editions.length) revert InvalidEdition();
        editions[editionNumber_] = edition_;

        emit EditionSet(
            editionNumber_,
            edition_.arweaveCID,
            edition_.recipient
        );
    }

    function _freezeMetadata() internal virtual {
        EditionsDropBaseStorage.layout()._metadataFrozen = true;
    }

    function _validateMetadataUnfrozen() internal view {
        if (EditionsDropBaseStorage.layout()._metadataFrozen)
            revert EditionMetadataFrozen();
    }
}

