     * @dev Returns the Uniform Resource Identifier (URI) for `tokenId` token.
     */
    function tokenURI(
        uint256 tokenId
    ) public view virtual override returns (string memory) {
        if (!_exists(tokenId)) revert URIQueryForNonexistentToken();

        string memory baseURI = _baseURI();
        bytes32 cid = EditionsDropBaseStorage
            .layout()
            ._editions[getEditionNumber(tokenId)]
            .arweaveCID;
        // Inspired by: https://github.com/soundxyz/sound-protocol/blob/master/contracts/core/utils/ArweaveURILib.sol
        return
            string.concat(
                baseURI,
                Base64.encode(abi.encodePacked(cid), true, true)
