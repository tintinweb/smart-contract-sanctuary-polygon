pragma solidity ^0.8.18;

import {ApplicationFollowModule} from '../../core/modules/follow/ApplicationFollowModule.sol';
import {TransparentUpgradeableProxy} from '../../upgradeability/TransparentUpgradeableProxy.sol';
import {IBondHub} from '../../interfaces/IBondHub.sol';

contract TokenDeployer {
    event AppModuleDeployed(
        string id,
        address ownerAddress,
        address proxy,
        address appFollowModuleImpl,
        uint256 timestamp
    );

    function lzDeployApplicationFollowModule(
        address hub,
        address ownerAddress,
        string memory id
    ) external returns (address,address) {
        ApplicationFollowModule token = new ApplicationFollowModule(hub,ownerAddress,id);
        // IBondHub(hub).whitelistFollowModule(address(token), true);
        // return ( address(token), address(0));
        bytes memory functionData = abi.encodeWithSelector(
            ApplicationFollowModule.initialize.selector,
            ownerAddress,
            id
        );

        TransparentUpgradeableProxy proxy = new TransparentUpgradeableProxy(address(token),
        ownerAddress,
        functionData);

        emit AppModuleDeployed(id, ownerAddress, address(proxy), address(token), block.timestamp);

        return ( address(proxy), address(token));
    }
}

