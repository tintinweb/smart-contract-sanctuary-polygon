     */
    enum ProtocolState {
        Unpaused,
        PublishingPaused,
        Paused
    }

    /**
     * @notice is an enumeration that contains the relationship types between profiles.
     *
     * @param Follow Follow someone.
     * @param Referral Referrer relationship.
