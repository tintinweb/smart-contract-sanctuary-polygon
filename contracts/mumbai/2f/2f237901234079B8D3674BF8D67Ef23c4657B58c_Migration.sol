pragma solidity 0.8.17;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/cryptography/SignatureChecker.sol";
import "hardhat-deploy/solc_0.8/proxy/Proxied.sol";
import "./Governed.sol";
import "./interfaces/IMetaCore.sol";
import "./interfaces/IForceNFT.sol";
import "./interfaces/IControl.sol";
import "./interfaces/IControlOld.sol";
import "./interfaces/IRegistry.sol";
import "./interfaces/IEnergyCoin.sol";
import "./interfaces/IMetaPayment.sol";

contract Migration is Initializable, Proxied, Governed {
    using SignatureChecker for address;

    enum Version {
        V1,
        V2
    }

    IControl public control;
    IControlOld public oldControlV1;
    IControlOld public oldControlV2;
    IMetaCore public mCore;

    address public signerAddress;
    mapping(address => uint256) public nonces;

    mapping(uint256 => address) public oldNFTsV1;
    mapping(uint256 => address) public oldNFTsV2;
    mapping(uint256 => address) public newNFTs;
    IRegistry public registry;

    event Migrate(Version v, address user, uint256 lvl, uint256 tokenId);
    event BurnNFTAndGetEnergy(
        address indexed user,
        Version indexed v,
        uint256 nftLevel,
        uint256 nftID,
        uint256 energyAmount
    );

    function burnNFTAndGetEnergy(
        Version v,
        uint256 nftLevel,
        uint256 nftID,
        uint256 energyAmount,
        uint256 nonce,
        uint256 blockLimit,
        bytes memory signature
    ) external {
        require(nonces[msg.sender] == nonce, "Nonce already used");
        require(blockLimit >= block.number, "Block limit passed");
        IForceNFT nft;
        if (v == Version.V1) {
            nft = IForceNFT(oldNFTsV1[nftLevel]);
        } else {
            nft = IForceNFT(oldNFTsV2[nftLevel]);
        }
        require(nft.ownerOf(nftID) == msg.sender, "Not NFT owner");
        nonces[msg.sender]++;

        bytes32 message = keccak256(abi.encodePacked(msg.sender, v, nftLevel, nftID, energyAmount, nonce, blockLimit));
        message = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", message));
        require(signerAddress.isValidSignatureNow(message, signature), "Invalid sig");

        _giveEnergy(msg.sender, energyAmount);
        nft.burn(nftID);
        control.giveEmptyJustNFT(msg.sender);

        emit BurnNFTAndGetEnergy(msg.sender, v, nftLevel, nftID, energyAmount);
    }

    function setRegistry(address _registry) external onlyGovernor {
        registry = IRegistry(_registry);
    }

    function setSignerAddress(address signer) external onlyGovernor {
        signerAddress = signer;
    }

    function setNewNFTs(address[] memory nfts) external onlyGovernor {
        require(nfts.length == 8, "NFTs count should be 8");

        for (uint256 i = 0; i < 8; ) {
            newNFTs[i] = nfts[i];

            unchecked {
                i++;
            }
        }
    }

    function setOldNFTs(address[] memory nftsV1, address[] memory nftsV2) external onlyGovernor {
        require(nftsV1.length == nftsV2.length && nftsV1.length == 8, "NFTs count should be 8");

        for (uint256 i = 0; i < 8; ) {
            oldNFTsV1[i] = nftsV1[i];
            oldNFTsV2[i] = nftsV2[i];

            unchecked {
                i++;
            }
        }
    }

    function migrate(
        Version v,
        uint256 level,
        uint256 tokenId,
        uint256 nonce,
        uint256 blockLimit,
        bytes memory signature
    ) external {
        require(nonces[msg.sender] == nonce, "Nonce already used");
        require(blockLimit >= block.number, "Block limit passed");

        nonces[msg.sender]++;

        bytes32 message = keccak256(abi.encodePacked(msg.sender, v, level, tokenId, nonce, blockLimit));
        message = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", message));
        require(signerAddress.isValidSignatureNow(message, signature), "Invalid sig");

        if (v == Version.V1) {
            _migrateInternal(Version.V1, level, IForceNFT(oldNFTsV1[level]), msg.sender, tokenId);
        } else {
            _migrateInternal(Version.V2, level, IForceNFT(oldNFTsV2[level]), msg.sender, tokenId);
        }
    }

    function migrateAll() external {
        revert("Paused");
        address user = msg.sender;

        uint256 nftAmountV1;
        uint256 nftAmountV2;
        for (uint256 lvl = 0; lvl < 8; ) {
            IForceNFT oldNFTv1 = IForceNFT(oldNFTsV1[lvl]);
            IForceNFT oldNFTv2 = IForceNFT(oldNFTsV2[lvl]);

            nftAmountV1 = oldNFTv1.balanceOf(user);
            nftAmountV2 = oldNFTv2.balanceOf(user);

            if (nftAmountV1 != 0) {
                for (uint256 i = nftAmountV1; i > 0; ) {
                    unchecked {
                        i--;
                    }

                    uint256 tokenId = oldNFTv1.tokenOfOwnerByIndex(user, i);
                    _migrateInternal(Version.V1, lvl, oldNFTv1, user, tokenId);
                }
            }
            if (nftAmountV2 != 0) {
                for (uint256 i = nftAmountV2; i > 0; ) {
                    unchecked {
                        i--;
                    }

                    uint256 tokenId = oldNFTv2.tokenOfOwnerByIndex(user, i);
                    _migrateInternal(Version.V2, lvl, oldNFTv2, user, tokenId);
                }
            }

            unchecked {
                lvl++;
            }
        }
    }

    function initialize(
        IControl _control,
        IControlOld _oldControlV1,
        IControlOld _oldControlV2,
        IMetaCore _mCore
    ) public proxied initializer {
        setGovernor(msg.sender);
        control = _control;
        oldControlV1 = _oldControlV1;
        oldControlV2 = _oldControlV2;
        mCore = _mCore;
    }

    function _giveEnergy(address to, uint256 amount) internal {
        IMetaPayment metaPayment = IMetaPayment(mCore.getPaymentChannelAddress());
        IEnergyCoin energy = IEnergyCoin(registry.getInternalEnergyCoin());
        energy.mint(address(metaPayment), amount);
        metaPayment.increaseBalance(address(energy), mCore.checkRegistration(to), amount);
    }

    function _migrateInternal(Version v, uint256 level, IForceNFT oldNFT, address user, uint256 tokenId) internal {
        // slither-disable-next-line incorrect-equality
        require(oldNFT.ownerOf(tokenId) == user, "User is not the owner of the NFT");

        bool setAlreadyReceivedEmptyNFT = false;
        if (level == 0) {
            if (v == Version.V1) {
                setAlreadyReceivedEmptyNFT = oldControlV1.alreadyReceivedEmpty(user);
            } else {
                setAlreadyReceivedEmptyNFT = oldControlV2.alreadyReceivedEmptyNFT(mCore.checkRegistration(user));
            }
        }

        emit Migrate(v, user, level, tokenId);
        control.migrateNFT(user, level, tokenId, oldNFT.energy(tokenId), setAlreadyReceivedEmptyNFT);
        oldNFT.burn(tokenId);
    }
}

