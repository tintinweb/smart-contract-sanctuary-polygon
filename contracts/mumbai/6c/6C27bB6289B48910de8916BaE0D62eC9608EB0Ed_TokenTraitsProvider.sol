pragma solidity 0.8.17;

import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";

import "./interfaces/ITokenTraitsProvider.sol";
import "../../../../libraries/MiscUtils.sol";

import "hardhat/console.sol"; // TODO remove

contract TokenTraitsProvider is
	Initializable,
	UUPSUpgradeable,
	AccessControlEnumerableUpgradeable,
	ITokenTraitsProvider
{
	using Strings for uint256;

	// ====================================================
	// ROLES
	// ====================================================
	bytes32 constant TOKEN_TRAITS_WRITER_ROLE = keccak256("TOKEN_TRAITS_WRITER_ROLE");

	// ====================================================
	// EVENTS
	// ====================================================
	event TraitMetadataConfigured(address tokenAddress, uint256 traitId);
	event TraitChanged(address tokenAddress, uint256 tokenId, uint256 traitId);

	// ====================================================
	// ERRORS
	// ====================================================
	error MetadataAlreadyInitialized(address tokenAddress, uint256 traitId);
	error TraitMetadataUninitialized(address tokenAddress, uint256 traitId);
	error InvalidTraitMetadata(string reason);
	error InvalidDataType(address tokenAddress, uint256 traitId, DataType expected, DataType received);
	error TraitAlreadyInitialized(address tokenAddress, uint256 traitId);
	error InvalidTraitIncrement(string reason);
	error InvalidTraitDecrement(string reason);
	error TraitDoesNotExist(address tokenAddress, uint256 tokenId, uint256 traitId);
	// ====================================================
	// STATE
	// ====================================================
	/// @notice mapping of address/traitId/TraitMetadata
	mapping(address => mapping(uint256 => TraitMetadata)) private collectionTraitMetadata;

	/// @notice mapping of address/tokenId/traitId
	mapping(address => mapping(uint256 => uint256[])) private tokenTraitIds;

	/// @notice mapping of address/traitId
	mapping(address => uint256[]) private collectionTraitIds;

	/// @notice mapping of address/tokenId/traitId/TokenTrait
	mapping(address => mapping(uint256 => mapping(uint256 => TokenTrait))) private tokenTraits;

	/// @notice mapping of address/tokenId/traitId/TokenTrait
	mapping(address => mapping(uint256 => TokenTrait)) private collectionTraits;

	// ====================================================
	// CONSTRUCTOR & INIT
	// ====================================================
	function initialize() public initializer {
		__AccessControl_init();
		__UUPSUpgradeable_init();

		_setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
		_setRoleAdmin(TOKEN_TRAITS_WRITER_ROLE, DEFAULT_ADMIN_ROLE);
	}

	function _authorizeUpgrade(address newImplementation) internal override onlyRole(DEFAULT_ADMIN_ROLE) {}

	// ====================================================
	// INTERNAL
	// ====================================================
	function _setRawTrait(
		address tokenAddress,
		uint256 tokenId,
		uint256 traitId,
		bytes memory value,
		DataType dataType
	) internal {
		TraitMetadata memory traitMetadata = _requireTraitMetadata(tokenAddress, traitId);

		require(IERC721(tokenAddress).ownerOf(tokenId) != address(0), "TokenTraitsProvider: Token does not exist");
		if (dataType != DataType.NULL && traitMetadata.dataType != dataType) {
			revert InvalidDataType(tokenAddress, traitId, traitMetadata.dataType, dataType);
		}

		bool isCommon = traitMetadata.isCommon;
		DataType storedType = isCommon
			? collectionTraits[tokenAddress][traitId].dataType
			: tokenTraits[tokenAddress][tokenId][traitId].dataType;

		if (storedType != DataType.NULL && traitMetadata.behavior != Behavior.UNRESTRICTED) {
			revert TraitAlreadyInitialized(tokenAddress, traitId);
		}

		if (storedType == DataType.NULL) {
			if (isCommon) {
				collectionTraits[tokenAddress][traitId].value = value;
				collectionTraits[tokenAddress][traitId].dataType = traitMetadata.dataType;
				collectionTraitIds[tokenAddress].push(traitId);
			} else {
				tokenTraits[tokenAddress][tokenId][traitId].dataType = traitMetadata.dataType;
				tokenTraits[tokenAddress][tokenId][traitId].value = value;
				tokenTraitIds[tokenAddress][tokenId].push(traitId);
			}
		}

		emit TraitChanged(tokenAddress, tokenId, traitId);
	}

	function _requireTraitMetadata(address tokenAddress, uint256 traitId) internal view returns (TraitMetadata memory) {
		TraitMetadata memory traitMetadata = collectionTraitMetadata[tokenAddress][traitId];
		if (traitMetadata.behavior == Behavior.NOT_SET) {
			revert TraitMetadataUninitialized(tokenAddress, traitId);
		}
		return traitMetadata;
	}

	// ====================================================
	// UTILS
	// ====================================================
	function _traitValueToString(TokenURITrait memory trait) internal pure returns (string memory) {
		DataType dataType = trait.dataType;

		if (dataType == DataType.STRING) {
			string memory value = abi.decode(trait.value, (string));
			return string(abi.encodePacked('"', value, '"'));
		} else if (dataType == DataType.BOOL) {
			bool value = abi.decode(trait.value, (bool));
			return value ? "true" : "false";
		} else if (dataType == DataType.UINT) {
			uint256 value = abi.decode(trait.value, (uint256));
			return value.toString();
		} else if (dataType == DataType.INT) {
			int256 value = abi.decode(trait.value, (int256));
			return MiscUtils.toString(value);
		}

		revert InvalidDataType(address(0), 999999999999999, trait.dataType, trait.dataType);
	}

	function _attributeJSON(TokenURITrait memory trait) internal pure returns (string memory) {
		string memory value = _traitValueToString(trait);
		return string(abi.encodePacked('{"trait_type":"', trait.name, '","value":', value, "}"));
	}

	function _generateAttributesJSON(TokenURITrait[] memory allTraits) internal pure returns (string memory) {
		string memory finalString = "";

		bool isFirstElement = true;
		for (uint256 idx; idx < allTraits.length; ++idx) {
			TokenURITrait memory trait = allTraits[idx];

			// Skip if its not an attribute type
			if (trait.isTopLevelProperty == true) {
				continue;
			}

			// Skip including attribute if the string value is empty
			if (trait.dataType == DataType.STRING && bytes(abi.decode(trait.value, (string))).length == 0) {
				continue;
			}

			string memory json = _attributeJSON(trait);
			string memory comma = isFirstElement ? "" : ",";
			finalString = string(abi.encodePacked(finalString, comma, json));
			isFirstElement = false;
		}

		return finalString;
	}

	function _generatePropertiesJSON(TokenURITrait[] memory allTraits) internal pure returns (string memory) {
		string memory propertiesJSON = "";
		bool isFirstElement = true;
		for (uint256 idx; idx < allTraits.length; ++idx) {
			TokenURITrait memory trait = allTraits[idx];
			if (trait.isTopLevelProperty == false) {
				continue;
			}

			string memory value = _traitValueToString(trait);
			string memory comma = isFirstElement ? "" : ",";

			propertiesJSON = string(abi.encodePacked(propertiesJSON, comma, '"', trait.name, '":', value));
			isFirstElement = false;
		}

		return propertiesJSON;
	}

	// ====================================================
	// ROLE GATED
	// ====================================================
	function configureCollectionTraitMetadata(
		address tokenAddress,
		uint256 traitId,
		string memory name,
		uint256 behavior,
		uint256 dataType,
		bool topLevel,
		bool isCommon
	) public onlyRole(TOKEN_TRAITS_WRITER_ROLE) {
		if (collectionTraitMetadata[tokenAddress][traitId].behavior != Behavior.NOT_SET) {
			revert MetadataAlreadyInitialized(tokenAddress, traitId);
		}

		if (Behavior(behavior) == Behavior.NOT_SET) {
			revert InvalidTraitMetadata("Behavior");
		}

		if (DataType(dataType) == DataType.NULL) {
			revert InvalidTraitMetadata("DataType");
		}

		if (bytes(name).length == 0) {
			revert InvalidTraitMetadata("Name");
		}

		collectionTraitMetadata[tokenAddress][traitId] = TraitMetadata({
			name: name,
			behavior: Behavior(behavior),
			dataType: DataType(dataType),
			topLevel: topLevel,
			isCommon: isCommon
		});

		emit TraitMetadataConfigured(tokenAddress, traitId);
	}

	function setStringTrait(
		address tokenAddress,
		uint256 tokenId,
		uint256 traitId,
		string memory value
	) public onlyRole(TOKEN_TRAITS_WRITER_ROLE) {
		_setRawTrait(tokenAddress, tokenId, traitId, abi.encode(value), DataType.STRING);
	}

	function setStringTraitBatch(
		address tokenAddress,
		uint256[] calldata tokenIds,
		uint256[] calldata traitIds,
		string[] calldata values
	) public onlyRole(TOKEN_TRAITS_WRITER_ROLE) {
		for (uint256 i = 0; i < traitIds.length; i++) {
			setStringTrait(tokenAddress, tokenIds[i], traitIds[i], values[i]);
		}
	}

	function setUintTrait(
		address tokenAddress,
		uint256 tokenId,
		uint256 traitId,
		uint256 value
	) public onlyRole(TOKEN_TRAITS_WRITER_ROLE) {
		_setRawTrait(tokenAddress, tokenId, traitId, abi.encode(value), DataType.UINT);
	}

	function setUintTraitBatch(
		address tokenAddress,
		uint256[] calldata tokenIds,
		uint256[] calldata traitIds,
		uint256[] calldata values
	) public onlyRole(TOKEN_TRAITS_WRITER_ROLE) {
		for (uint256 i = 0; i < traitIds.length; i++) {
			setUintTrait(tokenAddress, tokenIds[i], traitIds[i], values[i]);
		}
	}

	function setIntTrait(
		address tokenAddress,
		uint256 tokenId,
		uint256 traitId,
		int256 value
	) public onlyRole(TOKEN_TRAITS_WRITER_ROLE) {
		_setRawTrait(tokenAddress, tokenId, traitId, abi.encode(value), DataType.INT);
	}

	function setIntTraitBatch(
		address tokenAddress,
		uint256[] calldata tokenIds,
		uint256[] calldata traitIds,
		int256[] calldata values
	) public onlyRole(TOKEN_TRAITS_WRITER_ROLE) {
		for (uint256 i = 0; i < traitIds.length; i++) {
			setIntTrait(tokenAddress, tokenIds[i], traitIds[i], values[i]);
		}
	}

	function setBoolTrait(
		address tokenAddress,
		uint256 tokenId,
		uint256 traitId,
		bool value
	) public onlyRole(TOKEN_TRAITS_WRITER_ROLE) {
		_setRawTrait(tokenAddress, tokenId, traitId, abi.encode(value), DataType.BOOL);
	}

	function setBoolTraitBatch(
		address tokenAddress,
		uint256[] calldata tokenIds,
		uint256[] calldata traitIds,
		bool[] calldata values
	) public onlyRole(TOKEN_TRAITS_WRITER_ROLE) {
		for (uint256 i = 0; i < traitIds.length; i++) {
			setBoolTrait(tokenAddress, tokenIds[i], traitIds[i], values[i]);
		}
	}

	function incrementTrait(
		address tokenAddress,
		uint256 tokenId,
		uint256 traitId,
		uint256 amount
	) external override onlyRole(TOKEN_TRAITS_WRITER_ROLE) {
		if (amount == 0) {
			revert InvalidTraitIncrement("Invalid Amount");
		}

		TraitMetadata memory traitMetadata = _requireTraitMetadata(tokenAddress, traitId);

		if (traitMetadata.behavior != Behavior.INCREMENT && traitMetadata.behavior != Behavior.UNRESTRICTED) {
			revert InvalidTraitIncrement("Only Incrementable");
		}
		bool isCommon = traitMetadata.isCommon;
		// Make sure that the trait wasn't previously initialized to another data type
		DataType dataType = isCommon
			? collectionTraits[tokenAddress][traitId].dataType
			: tokenTraits[tokenAddress][tokenId][traitId].dataType;
		if (dataType != traitMetadata.dataType) {
            revert InvalidDataType(tokenAddress, traitId, traitMetadata.dataType, dataType);
        }

		mapping(uint256 => TokenTrait) storage allTraits = isCommon
			? collectionTraits[tokenAddress]
			: tokenTraits[tokenAddress][tokenId];

		if (dataType == DataType.UINT) {
			uint256 newValue = abi.decode(allTraits[traitId].value, (uint256)) + uint256(amount);
			allTraits[traitId].value = abi.encode(newValue);
		} else if (dataType == DataType.INT) {
			int256 newValue = abi.decode(allTraits[traitId].value, (int256)) + int256(amount);
			allTraits[traitId].value = abi.encode(newValue);
		} else {
			revert InvalidTraitIncrement("Not Incrementable");
		}

		emit TraitChanged(tokenAddress, tokenId, traitId);
	}

	function decrementTrait(
		address tokenAddress,
		uint256 tokenId,
		uint256 traitId,
		uint256 amount
	) external onlyRole(TOKEN_TRAITS_WRITER_ROLE) {
		if (amount == 0) {
			revert InvalidTraitDecrement("Invalid Amount");
		}

		TraitMetadata memory traitMetadata = _requireTraitMetadata(tokenAddress, traitId);

		if (traitMetadata.behavior != Behavior.DECREMENT && traitMetadata.behavior != Behavior.UNRESTRICTED) {
			revert InvalidTraitDecrement("Only Decrementable");
		}
		bool isCommon = traitMetadata.isCommon;
		
		DataType dataType = isCommon
			? collectionTraits[tokenAddress][traitId].dataType
			: tokenTraits[tokenAddress][tokenId][traitId].dataType;

		if (dataType != traitMetadata.dataType) {
			revert InvalidDataType(tokenAddress, traitId, traitMetadata.dataType, dataType);
		}

		mapping(uint256 => TokenTrait) storage allTraits = isCommon
			? collectionTraits[tokenAddress]
			: tokenTraits[tokenAddress][tokenId];

		if (dataType == DataType.UINT) {
			uint256 oldValue = abi.decode(allTraits[traitId].value, (uint256));
			if (amount > oldValue) {
				revert InvalidTraitDecrement("Below zero");
			}

			uint256 newValue = oldValue - amount;
			allTraits[traitId].value = abi.encode(newValue);
		} else if (dataType == DataType.INT) {
			int256 newValue = abi.decode(allTraits[traitId].value, (int256)) - int256(amount);
			allTraits[traitId].value = abi.encode(newValue);
		} else {
			revert InvalidTraitDecrement("Not Decrementable");
		}

		emit TraitChanged(tokenAddress, tokenId, traitId);
	}

	// ====================================================
	// PUBLIC
	// ====================================================
	function getTraitMetadata(address tokenAddress, uint256 traitId) public view returns (TraitMetadata memory) {
		return collectionTraitMetadata[tokenAddress][traitId];
	}

	function getTokenTraitIds(address tokenAddress, uint256 tokenId) external view returns (uint256[] memory) {
		return tokenTraitIds[tokenAddress][tokenId];
	}

	function getCollectionTraitIds(address tokenAddress) external view returns (uint256[] memory) {
		return collectionTraitIds[tokenAddress];
	}

	function getTraitBytesValue(
		address tokenAddress,
		uint256 tokenId,
		uint256 traitId
	) external view returns (bytes memory) {
		bool isCommon = _requireTraitMetadata(tokenAddress, traitId).isCommon;
		DataType dataType = isCommon
			? collectionTraits[tokenAddress][traitId].dataType
			: tokenTraits[tokenAddress][tokenId][traitId].dataType;

		if (dataType == DataType.NULL) {
			revert TraitDoesNotExist(tokenAddress, tokenId, traitId);
		}
		bytes memory value = isCommon
			? collectionTraits[tokenAddress][traitId].value
			: tokenTraits[tokenAddress][tokenId][traitId].value;
		return value;
	}

	function getTraitStringValue(
		address tokenContract,
		uint256 tokenId,
		uint256 traitId
	) external view returns (string memory) {
		bool isCommon = _requireTraitMetadata(tokenContract, traitId).isCommon;
		DataType dataType = isCommon
			? collectionTraits[tokenContract][traitId].dataType
			: tokenTraits[tokenContract][tokenId][traitId].dataType;
		if (dataType != DataType.STRING) {
			revert InvalidDataType(tokenContract, traitId, DataType.STRING, dataType);
		}

		bytes memory value = isCommon
			? collectionTraits[tokenContract][traitId].value
			: tokenTraits[tokenContract][tokenId][traitId].value;
		return abi.decode(value, (string));
	}

	function getTraitIntValue(address tokenContract, uint256 tokenId, uint256 traitId) external view returns (int256) {
		bool isCommon = _requireTraitMetadata(tokenContract, traitId).isCommon;
		DataType dataType = isCommon
			? collectionTraits[tokenContract][traitId].dataType
			: tokenTraits[tokenContract][tokenId][traitId].dataType;

		if (dataType != DataType.INT) {
			revert InvalidDataType(tokenContract, traitId, DataType.INT, dataType);
		}
		bytes memory value = isCommon
			? collectionTraits[tokenContract][traitId].value
			: tokenTraits[tokenContract][tokenId][traitId].value;
		return abi.decode(value, (int256));
	}

	function getTraitUintValue(
		address tokenContract,
		uint256 tokenId,
		uint256 traitId
	) external view returns (uint256) {
		bool isCommon = _requireTraitMetadata(tokenContract, traitId).isCommon;
		DataType dataType = isCommon
			? collectionTraits[tokenContract][traitId].dataType
			: tokenTraits[tokenContract][tokenId][traitId].dataType;

		if (dataType != DataType.UINT) {
			revert InvalidDataType(tokenContract, traitId, DataType.UINT, dataType);
		}

		bytes memory value = isCommon
			? collectionTraits[tokenContract][traitId].value
			: tokenTraits[tokenContract][tokenId][traitId].value;
		return abi.decode(value, (uint256));
	}

	function getTraitBoolValue(address tokenContract, uint256 tokenId, uint256 traitId) external view returns (bool) {
		bool isCommon = _requireTraitMetadata(tokenContract, traitId).isCommon;
		DataType dataType = isCommon
			? collectionTraits[tokenContract][traitId].dataType
			: tokenTraits[tokenContract][tokenId][traitId].dataType;

		if (dataType != DataType.BOOL) {
			revert InvalidDataType(tokenContract, traitId, DataType.BOOL, dataType);
		}
		bytes memory value = isCommon
			? collectionTraits[tokenContract][traitId].value
			: tokenTraits[tokenContract][tokenId][traitId].value;
		return (abi.decode(value, (int256)) == 1);
	}

	function hasTrait(address tokenContract, uint256 tokenId, uint256 traitId) external view override returns (bool) {
		bool isCommon = _requireTraitMetadata(tokenContract, traitId).isCommon;
		return
			isCommon
				? collectionTraits[tokenContract][traitId].dataType != DataType.NULL
				: tokenTraits[tokenContract][tokenId][traitId].dataType != DataType.NULL;
	}

	function generateTokenURI(
		address tokenContract,
		uint256 tokenId,
		TokenURITrait[] memory extraTraits
	) external view returns (string memory) {
		// Gather all dynamic trait ids
		uint256[] memory _tokenTraitIds = this.getTokenTraitIds(tokenContract, tokenId);
		uint256[] memory _collectionTraitIds = this.getCollectionTraitIds(tokenContract);

		// Fetch and process dynamic traits for this token
		TokenURITrait[] memory allTraits = new TokenURITrait[](
			_collectionTraitIds.length + _tokenTraitIds.length + extraTraits.length
		);

		for (uint256 idx; idx < _collectionTraitIds.length; ++idx) {
			uint256 traitId = _collectionTraitIds[idx];
			TraitMetadata memory traitMetadata = this.getTraitMetadata(tokenContract, traitId);

			allTraits[idx].name = traitMetadata.name;
			allTraits[idx].dataType = traitMetadata.dataType;
			allTraits[idx].isTopLevelProperty = traitMetadata.topLevel;
			allTraits[idx].value = this.getTraitBytesValue(tokenContract, tokenId, traitId);
		}

		for (uint256 idx; idx < _tokenTraitIds.length; ++idx) {
			uint256 traitId = _tokenTraitIds[idx];
			TraitMetadata memory traitMetadata = this.getTraitMetadata(tokenContract, traitId);

			allTraits[idx].name = traitMetadata.name;
			allTraits[idx].dataType = traitMetadata.dataType;
			allTraits[idx].isTopLevelProperty = traitMetadata.topLevel;
			allTraits[idx].value = this.getTraitBytesValue(tokenContract, tokenId, traitId);
		}

		// Append the extra traits onto the allTraits array
		for (uint256 idx; idx < extraTraits.length; ++idx) {
			allTraits[_tokenTraitIds.length + idx] = extraTraits[idx];
		}

		// Generate JSON strings
		string memory propertiesJSON = _generatePropertiesJSON(allTraits);
		string memory attributesJSON = _generateAttributesJSON(allTraits);
		string memory comma = bytes(propertiesJSON).length > 0 ? "," : "";

		string memory metadata = string(
			abi.encodePacked("{", propertiesJSON, comma, '"attributes":[', attributesJSON, "]}")
		);

		return string(abi.encodePacked("data:application/json;base64,", Base64.encode(bytes(metadata))));
	}
}