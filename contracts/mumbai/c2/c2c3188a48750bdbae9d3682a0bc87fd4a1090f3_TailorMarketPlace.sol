contract TailorMarketPlace is
    UUPSUpgradeable,
    ReentrancyGuardUpgradeable,
    TailorControlAccess
{
    // @custom:oz-upgrades-unsafe-allow delegatecall
    uint256[500] private __placeholder;
    using EnumerableSet for EnumerableSet.AddressSet;
    EnumerableSet.AddressSet private acceptedCurrencies;
    using OrderTypes for OrderTypes.NFTListing;
    using OrderTypes for OrderTypes.TakerOrder;

    uint96 internal _secondaryMarketFee;

    address payable internal _feesAddress;

    address internal _cloneFactory;
    uint256 constant _BASIS_POINTS = 10000;

    mapping(uint256 => OrderTypes.NFTListing) private marketNFTData;
    mapping(address => address payable) private userEscrow;

    event NFTListed(
        address collection, // address of the NFT contract
        address signer, // sender of the sell TakerOrder
        address feesAddress, // sender of the sell TakerOrder
        address strategy,
        uint256 tokenId, // token Id of the item
        OrderTypes.NFTState status,
        bytes32 listingId
    );

    event NFTSold(
        address collection, // address of the NFT contract
        address signer, // sender of the sell TakerOrder
        address feesAddress, // sender of the sell TakerOrder
        uint256 feesAmount,
        address royaltyAddress,
        uint256 royaltyfeesAmount,
        address prevOwner, // sender of the sell TakerOrder
        uint256 tokenId, // token Id of the item
        uint256 price,
        bytes32 listingId
    );
    event NewBidNFT(
        address collection, // address of the NFT contract
        address signer, // sender of the sell TakerOrder
        address highestbidder, // sender of the sell TakerOrder
        address feesAddress, // sender of the sell TakerOrder
        uint256 tokenId, // token Id of the item
        uint256 price,
        uint256 prevBid,
        bytes32 listingId
    );
    event ModifiedListing(bytes32 listingId);
    event CancelListing(bytes32 listingId);

    event EscrowFundsWithdrawn(address to, uint256 amount);
    event AuctionFundsWithdrawn(
        address collection,
        uint256 tokenId,
        address strategy,
        uint256 amount
    );

    modifier onlySellableCollection(
        bool _openForSaleBeforeEvent,
        bool _openForSaleAfterEvent
    ) {
        require(
            _openForSaleBeforeEvent || _openForSaleAfterEvent,
            "Event sales are not open"
        );
        _;
    }

    modifier onlyNFTOwner(
        address collection,
        address signer,
        uint256 tokenId
    ) {
        console.log(collection, signer, tokenId);
        address owner = ITailorNFTCollectible(collection).ownerOf(tokenId);
        require(owner == signer, "Not NFT Owner");
        _;
    }

    modifier notBiddedOn(uint256 tokenId) {
        require(
            marketNFTData[tokenId].highestbidder == address(0),
            "Already a current bid"
        );
        _;
    }

    modifier notListed(uint256 tokenId) {
        require(!_listed(tokenId), "Item already listed");
        _;
    }
    modifier acceptedCurrency(address currency) {
        require(isCurrencyAccepted(currency), "currency not accepted");
        _;
    }
    modifier isListed(uint256 tokenId) {
        require(_listed(tokenId), "Item not listed");
        _;
    }

    function _listed(uint256 tokenId) private view returns (bool) {
        return marketNFTData[tokenId].owner != address(0);
    }

    modifier mpAllowed(address collection, uint256 tokenId) {
        require(
            IERC721(collection).isApprovedForAll(
                IERC721(collection).ownerOf(tokenId),
                address(this)
            )
        );
        _;
    }

    function initialize(
        address feesAddress,
        uint96 secondaryMarketFee,
        address _WETH
    ) external initializer {
        __ReentrancyGuard_init();
        __setFee(feesAddress, secondaryMarketFee);
        __TailorControlAccess_init();
        _addCurrency(address(0)); //payments in ether
        _addCurrency(_WETH);
    }

    function __addCloneFactory(address _address) external onlyAdmin {
        require(_address != address(0), "cloneFactory is addressZero");
        _cloneFactory = _address;
        // _whitelistedStrategies.add(_address);
        console.log(_cloneFactory, " added ");
    }

    function __setFee(address feesAddress, uint96 secondaryMarketFee) private {
        _feesAddress = payable(feesAddress);
        _secondaryMarketFee = secondaryMarketFee;
    }

    function _addCurrency(address currency) private {
        // require(IERC165(currency).supportsInterface(type(IERC20).interfaceId),"unsupported interface");
        acceptedCurrencies.add(currency);
    }

    function addCurrency(IERC20 currency) external onlyAdmin {
        _addCurrency(address(currency));
    }

    function isCurrencyAccepted(address currency) public view returns (bool) {
        return acceptedCurrencies.contains(currency);
    }

    function _createListing(OrderTypes.NFTListing memory NFTOrder) private {
        _fillRoyaltyAndFees(NFTOrder);

        NFTOrder.status == OrderTypes.NFTState.LISTED
            ? _buyListing(NFTOrder)
            : _auctionListing(NFTOrder);

        emit NFTListed(
            NFTOrder.collection,
            NFTOrder.owner,
            _feesAddress,
            NFTOrder.strategy,
            NFTOrder.tokenId,
            NFTOrder.status,
            NFTOrder.constantHash()
        );

        marketNFTData[NFTOrder.tokenId] = NFTOrder;
    }

    /**  @dev create an escrow if the user doens't have one  **/

    function _buyListing(OrderTypes.NFTListing memory NFTOrder) private {
        if (userEscrow[NFTOrder.owner] == address(0)) {
            userEscrow[NFTOrder.owner] = ICloneFactory(_cloneFactory)
                .createEscrowClone(NFTOrder.owner, address(this));
        }
        NFTOrder.strategy = userEscrow[NFTOrder.owner];
    }

    /**
     *@notice create a strategy
     * @dev call the clone factory to create an auction clone
     **/
    function _auctionListing(OrderTypes.NFTListing memory NFTOrder) private {
        NFTOrder.strategy = payable(
            ICloneFactory(_cloneFactory).createAuctionClone(
                NFTOrder.owner,
                NFTOrder.currencyToken,
                address(this)
            )
        );
    }

    /**
     * @dev Allows ticket to be LISTED on secondary Market
     * Check if tokenID owner is the owner that calls the function
     */
    function secondaryMarketListing(
        OrderTypes.NFTListing calldata NFTOrder,
        bool openForSaleBeforeEvent,
        bool openForSaleAfterEvent
    )
        external
        onlySellableCollection(openForSaleBeforeEvent, openForSaleAfterEvent)
        onlyNFTOwner(
            NFTOrder.collection,
            NFTOrder.owner,
            NFTOrder.tokenId
        )
        notListed(NFTOrder.tokenId)
        onlyAdmin
        acceptedCurrency(NFTOrder.currencyToken)
    {
        _validateListingOrderParams(NFTOrder);
        _createListing(NFTOrder);
    }

    function getEscrow(address user) external view returns (address) {
        return userEscrow[user];
    }

    function _validateListingOrderParams(
        OrderTypes.NFTListing calldata ticketOrder
    ) internal pure {
        require(
            ticketOrder.owner != address(0),
            "Seller must be not address(0)"
        );

        require(
            ticketOrder.acceptedPrice > ticketOrder.reservePrice,
            "accepted price needs to be bigger than the reserve price"
        );
    }

    function _validateOrderParams(
        OrderTypes.NFTListing memory NFTListing,
        OrderTypes.TakerOrder calldata takerOrder
    ) internal view {
        require(
            NFTListing.owner != takerOrder.taker,
            "NFTOwner can't bid on his own NFT"
        );
        require(
            NFTListing.sellstatus == OrderTypes.PurchaseState.SELLABLE,
            "NFT is not SELLABLE"
        );
        require(
            NFTListing.currencyToken == takerOrder.currencyToken,
            "tokens don't match"
        );
        require(
            !(NFTListing.currencyToken == address(0) &&
                takerOrder.taker.balance < takerOrder.price),
            "User has not enough matic"
        );
    }

    function _validateBiddingOrderParams(
        OrderTypes.NFTListing memory NFTListing,
        OrderTypes.TakerOrder calldata takerOrder
    ) internal view {
        _validateOrderParams(NFTListing, takerOrder);
        require(
            NFTListing.status == OrderTypes.NFTState.AUCTIONABLE,
            "NFT does not have the status AUCTIONABLE"
        );
        require(
            block.timestamp < NFTListing.endTime,
            "Auction must not be closed"
        );
        require(
            takerOrder.price >= ((NFTListing.minBid) * 12) / 10,
            "Price < minimum bid price + condition"
        );
        require(
            !(NFTListing.currencyToken != address(0) &&
                IERC20(NFTListing.currencyToken).allowance(
                    takerOrder.taker,
                    address(this)
                ) <
                takerOrder.price),
            "User has not enough tokens"
        );
    }

    function _validateBuyingOrderParams(
        OrderTypes.NFTListing memory NFTListing,
        OrderTypes.TakerOrder calldata takerOrder
    ) internal view {
        _validateOrderParams(NFTListing, takerOrder);
        require(
            NFTListing.status == OrderTypes.NFTState.LISTED,
            "NFT is not LISTED"
        );
        require(
            userEscrow[NFTListing.owner] != address(0),
            "No escrow contract"
        );
        require(
            NFTListing.acceptedPrice == takerOrder.price,
            "Prices do not match"
        );
        require(
            !(NFTListing.currencyToken != address(0) &&
                IERC20(NFTListing.currencyToken).allowance(
                    takerOrder.taker,
                    userEscrow[NFTListing.owner]
                ) <
                takerOrder.price),
            "User has not enough tokens"
        );
    }

    /**
     * @notice if the require fields are respected, the buyer send the amount to the escrow
     *create a lighter struct witch indicates all transfers to be done.
     * if the transfers are executed, an event is emited, if not it stays in custody
     * @dev if the payment is in ether, the user sends the amount to the escrox by calling the function with a msg.vaue.
     *then the amount is splitted beetween the seller and the royalty and fees receivers
     */
    function secondaryMarketBuying(
        OrderTypes.TakerOrder calldata takerOrder,
        bool openForSaleBeforeEvent,
        bool openForSaleAfterEvent
    )
        external
        payable
        onlyAdmin
        onlySellableCollection(openForSaleBeforeEvent, openForSaleAfterEvent)
    {
        // 0. Fetch Listing data from storage
        OrderTypes.NFTListing memory ticketNFT = marketNFTData[
            takerOrder.tokenId
        ];

        _validateBuyingOrderParams(ticketNFT, takerOrder);

        OrderTypes.PaymentParams memory params = ticketNFT.getPaymentParams(
            takerOrder.taker
        );

        address originalOwner = ticketNFT.owner;

        // 1. Transfer Funds
        bool withdrawn = IEscrowUser(userEscrow[ticketNFT.owner])
            .transferFeesAndFunds{value: msg.value}(params);
        if (withdrawn) {
            emit EscrowFundsWithdrawn(
                ticketNFT.owner,
                userEscrow[ticketNFT.owner].balance
            );
        }
        // 2. Transfer NFT
        _transferNFT(
            ticketNFT.collection,
            ticketNFT.owner,
            takerOrder.taker,
            ticketNFT.tokenId
        );

        emit NFTSold(
            ticketNFT.collection,
            takerOrder.taker,
            _feesAddress,
            params.paymentInfo[1].amount,
            params.paymentInfo[0].to,
            params.paymentInfo[0].amount,
            originalOwner,
            ticketNFT.tokenId,
            ticketNFT.acceptedPrice,
            ticketNFT.constantHash()
        );
        // 3. Delete marketplace listing
        delete marketNFTData[ticketNFT.tokenId];
    }

    /**
     * @dev Allows to bid on AUCTIONNABLE tickets
     * Verify that CALL contains msg.value >= minBid
     * Verify that tickets is AUCTIONNABLE and SELLABLE
     */
    function secondaryMarketBidding(
        OrderTypes.TakerOrder calldata takerOrder,
        bool openForSaleBeforeEvent,
        bool openForSaleAfterEvent
    )
        external
        payable
        onlySellableCollection(openForSaleBeforeEvent, openForSaleAfterEvent)
        onlyAdmin
    {
        OrderTypes.NFTListing storage ticketNFT = marketNFTData[
            takerOrder.tokenId
        ];

        _validateBiddingOrderParams(ticketNFT, takerOrder);

        // 1. Reimburse Last Bidder
        IAuctionStrategy(ticketNFT.strategy).reimburseLastBidder(
            ticketNFT.highestbidder
        );

        // 2. If accepted price is reached
        if (_hasReachedPrice(ticketNFT.acceptedPrice, takerOrder.price)) {
            _executeBidPrice(takerOrder.taker, takerOrder.tokenId);
        } else {
            // 3. Place normal bid
            _transferBidPrice(ticketNFT, takerOrder);
        }
    }

    /**
     * @notice if the accepted price is reached, the auction is realisated
     */
    function _executeBidPrice(address to, uint256 tokenId) private {
        require(to != address(0), "Highestbidder is address(0)");

        OrderTypes.NFTListing memory ticketNFT = marketNFTData[tokenId];

        // 1. Set Auction to CLOSE
        IAuctionStrategy(ticketNFT.strategy).changeAuctionStatus(1);

        ticketNFT.highestbidder = to;
        // 2. Transfer Funds
        IERC20(ticketNFT.currencyToken).transferFrom(
            to,
            ticketNFT.strategy,
            ticketNFT.acceptedPrice
        );

        // 3. Transfer NFT
        _executeTransfer(ticketNFT);
    }

    /**
     *@dev withdraw the last bid, transfer the token then delete the listing
     */
    function _executeTransfer(OrderTypes.NFTListing memory ticketNFT) private {
        address originalOwner = ticketNFT.owner;
        // 1. Withdraw Last Bid
        _withdrawlastBid(ticketNFT);

        // 2. Transfer NFT
        _transferNFT(
            ticketNFT.collection,
            ticketNFT.owner,
            ticketNFT.highestbidder,
            ticketNFT.tokenId
        );

        emit NFTSold(
            ticketNFT.collection,
            ticketNFT.owner,
            _feesAddress,
            (ticketNFT.acceptedPrice * ticketNFT.secondaryMarketFee) /
                _BASIS_POINTS,
            ticketNFT.royaltyFeeRecipient,
            (ticketNFT.acceptedPrice * ticketNFT.royaltyFeeAmount) /
                _BASIS_POINTS,
            originalOwner,
            ticketNFT.tokenId,
            ticketNFT.acceptedPrice,
            ticketNFT.constantHash()
        );

        delete marketNFTData[ticketNFT.tokenId];
    }

    function _transferNFT(
        address collection,
        address from,
        address to,
        uint256 tokenId
    ) internal {
        ITailorNFTCollectible(collection).transferFrom(from, to, tokenId);
    }

    /**
     *@dev withdraw funds from the strategy then delete strategy contract
     */
    function _withdrawlastBid(OrderTypes.NFTListing memory listedOrder)
        private
    {
        uint256 balance = IERC20(listedOrder.currencyToken).balanceOf(
            listedOrder.strategy
        );

        OrderTypes.PaymentInfo[3] memory auctionParams = listedOrder
            .getAuctionVariables(balance);

        IAuctionStrategy(listedOrder.strategy).withdrawBalance(auctionParams);
        IAuctionStrategy(listedOrder.strategy).destruct();

        emit AuctionFundsWithdrawn(
            listedOrder.collection,
            listedOrder.tokenId,
            listedOrder.strategy,
            balance
        );
    }

    /**
     * @notice Transfer fees and funds to royalty recipient, protocol, and seller
     */
    function _transferBidPrice(
        OrderTypes.NFTListing storage listedOrder,
        OrderTypes.TakerOrder calldata takerOrder
    ) private nonReentrant {
        IERC20(listedOrder.currencyToken).transferFrom(
            takerOrder.taker,
            listedOrder.strategy,
            takerOrder.price
        );
        uint256 prevBid = listedOrder.acceptedPrice;
        listedOrder.highestbidder = takerOrder.taker;
        listedOrder.minBid = takerOrder.price;

        emit NewBidNFT(
            listedOrder.collection,
            listedOrder.owner,
            listedOrder.highestbidder,
            listedOrder.feesAddress,
            takerOrder.tokenId,
            takerOrder.price,
            prevBid,
            listedOrder.constantHash()
        );
    }

    /**
     *@notice call the nft contract to get the royalties data and get the fees data from this contract
     */

    function _fillRoyaltyAndFees(
        OrderTypes.NFTListing memory order //faire Ã§a offchain
    ) private view {
        // 1. Protocol fees
        order.feesAddress = _feesAddress;
        order.secondaryMarketFee = _secondaryMarketFee;

        // 2. Royalty fees
        (
            order.royaltyFeeRecipient,
            order.royaltyFeeAmount
        ) = _calculateRoyaltyFee(order.collection, order.tokenId);
    }

    function _calculateRoyaltyFee(address collection, uint256 tokenId)
        internal
        view
        returns (address payable, uint96)
    {
        // 1. Check if there is royalty info
        (uint128 collectionId, , ) = ITailorNFTCollectible(collection)
            .getNFTinfo(tokenId);

        (address royaltyReceiver, uint96 royaltyAmount) = ITailorNFTCollectible(
            collection
        ).getCollectionRoyalty(collectionId);
        return (payable(royaltyReceiver), royaltyAmount);
    }

    /**
     *@notice end auction manually */
    function endAuction(uint256 tokenId) external onlyAdmin {
        OrderTypes.NFTListing storage ticketNFT = marketNFTData[tokenId];
        require(
            ticketNFT.status == OrderTypes.NFTState.AUCTIONABLE,
            "NFT is not AUCTIONABLE"
        );
        // require(block.timestamp >= ticketNFT.endTime, "Bidding ongoing");
        IAuctionStrategy(ticketNFT.strategy).changeAuctionStatus(1);
        _executeTransfer(ticketNFT);
    }

    /**
     * @notice called if an auction is ended
     */
    // function _executeAuction(uint256 tokenID) private {
    //     address originalOwner = ticketNFT.owner;
    //     // 1. Transfer NFT
    //     (ticketNFT.reservePrice <= ticketNFT.minBid) && //reserve price reached
    //         (ticketNFT.owner ==
    //             IERC721(ticketNFT.collection).ownerOf(ticketNFT.tokenId)) && //the seller still have the token
    //         (ticketNFT.highestbidder != address(0)) //there is at least one bidder
    //         ? _executeTransfer(ticketNFT, originalOwner)
    //         : _cancelAuction(tokenID);
    // }

    /**
     * @notice function called if the reserve price is not reached at the end of auction
     * @dev reimburse last bidder by calling the escrow
     */
    function cancelAuction(uint256 tokenId)
        external
        isListed(tokenId)
        onlyNFTOwner(
            marketNFTData[tokenId].collection,
            marketNFTData[tokenId].owner,
            marketNFTData[tokenId].tokenId
        )
        notBiddedOn(tokenId)
    {
        OrderTypes.NFTListing storage NFTOrder = marketNFTData[tokenId];
        require(NFTOrder.reservePrice <= NFTOrder.minBid, "ReservePrice met");
        //the new listing struct must have the sames tokenid, collection, owner and start time
        require(
            NFTOrder.constantHash() ==
                marketNFTData[NFTOrder.tokenId].constantHash(),
            "changes of collection, startTime or owner forbidden"
        );
        emit CancelListing(NFTOrder.constantHash());
        delete marketNFTData[tokenId];
    }

    function _hasReachedDeadline(uint256 deadline) private view returns (bool) {
        return block.timestamp >= deadline;
    }

    function _hasReachedPrice(uint256 askPrice, uint256 price)
        private
        pure
        returns (bool)
    {
        return askPrice <= price;
    }

    function modifyListing(OrderTypes.NFTListing memory NFTOrder)
        external
        onlyNFTOwner(
            NFTOrder.collection,
            NFTOrder.owner,
            NFTOrder.tokenId
        )
        isListed(NFTOrder.tokenId)
        notBiddedOn(NFTOrder.tokenId)
    {
        //the new listing struct must have the sames tokenid, collection, owner and start time
        require(
            NFTOrder.constantHash() ==
                marketNFTData[NFTOrder.tokenId].constantHash(),
            "changes of collection, startTime or owner forbidden"
        );
        marketNFTData[NFTOrder.tokenId] = NFTOrder;
        emit ModifiedListing(NFTOrder.constantHash());
    }

    function getMarketListing(uint256 tokenID)
        public
        view
        returns (OrderTypes.NFTListing memory ticketNFT)
    {
        return marketNFTData[tokenID];
    }

    function viewSecondaryMarketFee() internal view virtual returns (uint96) {
        return _secondaryMarketFee;
    }

    /**
     * @dev UUPSUpgradeableContract Methods .
     * WARNING: KEEP THIS FUNCTION INTO INHERITTED CONTRACTS TO AVOID LOSE OF UPGRADEABILILITY
     */
    function _authorizeUpgrade(address implementation)
        internal
        override
        // onlyTailor
        onlyRole(UPGRADER_ROLE)
    {}
}

