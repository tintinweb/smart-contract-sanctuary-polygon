/**
 * Should look the stacks of the auction in this contract
 */
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "../libraries/OrderTypesV2.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

contract AuctionStrategy is Initializable, Context, ReentrancyGuardUpgradeable {
    using SafeERC20 for IERC20;

    address private initialOwner;
    address private lastbidder;
    address private WETH;
    address private marketplace;

    enum AuctionStatus {
        OPEN, //0
        CLOSE //1
    }
    AuctionStatus public status;
    address currenthighestbidder;
    using OrderTypes for OrderTypes.PaymentInfo;

	modifier onlyMarketPlace(){
		require(msg.sender == marketplace, "Not MarketPlace");
		_;
	}

	modifier onlySeller() {
        require(msg.sender == initialOwner, "Not initialOwner");
        _;
    }

    modifier isEnded() {
        require(status == AuctionStatus.CLOSE, "Auction not Closed");
		_;
    }

    function initialize(
        address _signer,
        address _WETH,
        address _marketplace
    ) external initializer {
        initialOwner = _signer;
        WETH = _WETH;
        marketplace = _marketplace;
    }

    modifier isFinalOwner() {
        require(msg.sender != address(0), "not final owner");
        _;
    }

    function withdrawBalance(
        OrderTypes.PaymentInfo[3] memory recipients
    ) external onlyMarketPlace isEnded {
		require(IERC20(WETH).balanceOf(address(this)) > 0, "nothing to withdraw");
        for (uint8 u = 0; u < 3; u++) {
            executeTransfer(recipients[u]);
        }
    }

	function executeTransfer(OrderTypes.PaymentInfo memory recipients)
        private
		nonReentrant
    {
        if (recipients.to != address(0) && recipients.amount != 0) {
			console.log("withdraw ", recipients.amount, "to ", recipients.to);
            IERC20(WETH).safeTransfer(recipients.to, recipients.amount);   
        }
    }

    function destruct() external onlyMarketPlace {
        require(IERC20(WETH).balanceOf(address(this)) == 0, "the balance is not empty");
        selfdestruct(payable(initialOwner));
    }

    function getBalance() public view returns (uint256) {
        return IERC20(WETH).balanceOf(address(this));
    }

    function reimburseLastBidder(address highestbidder) external onlyMarketPlace nonReentrant {
        if (highestbidder == address(0)) {
            return;
        }

        IERC20(WETH).safeTransfer(
            highestbidder,
            IERC20(WETH).balanceOf(address(this))
        );
    }

    function changeAuctionStatus(AuctionStatus state) external onlyMarketPlace{
        status = state;
    }

    receive() external payable {}

    fallback() external {}
}

