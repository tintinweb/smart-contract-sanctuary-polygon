// SPDX-License-Identifier: MIT
pragma solidity ^0.8.17;

import {ERC165, IERC1155} from "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import {ERC1155Holder} from "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import {EnumerableSet} from "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import {Paginator} from "@dlsl/dev-modules/libs/arrays/Paginator.sol";
import {TypeCaster} from "@dlsl/dev-modules/libs/utils/TypeCaster.sol";
import {IAccessControl} from "@openzeppelin/contracts/access/IAccessControl.sol";

import {AccessControlExtension} from "./extensions/AccessControlExtension.sol";
import {ICLDTNFT} from "./interfaces/ICLDTNFT.sol";
import {IBridge} from "./interfaces/IBridge.sol";

contract Bridge is AccessControlExtension, ERC1155Holder, IBridge {
    using Paginator for *;
    using TypeCaster for uint256;
    using EnumerableSet for EnumerableSet.UintSet;

    bytes32 public constant BRIDGE_ADMIN = keccak256("BRIDGE_ADMIN");

    address public source;
    address public destination;

    EnumerableSet.UintSet private _availableTokens;

    constructor(address source_, address destination_, address accessControl_) {
        _setSource(source_);
        _setDestination(destination_);
        _setAccessControl(accessControl_);
    }

    function setAvailableTokens(
        uint256[] calldata tokenIds_,
        bool status_
    ) external hasRole(BRIDGE_ADMIN) {
        if (status_) {
            for (uint256 i = 0; i < tokenIds_.length; i++) {
                _availableTokens.add(tokenIds_[i]);
            }
        } else {
            for (uint256 i = 0; i < tokenIds_.length; i++) {
                _availableTokens.remove(tokenIds_[i]);
            }
        }
    }

    function setSource(address source_) external hasRole(BRIDGE_ADMIN) {
        _setSource(source_);
    }

    function setDestination(address destination_) external hasRole(BRIDGE_ADMIN) {
        _setDestination(destination_);
    }

    function onERC1155Received(
        address operator_,
        address from_,
        uint256 tokenId_,
        uint256 amount_,
        bytes memory data_
    ) public override returns (bytes4) {
        _move(from_, tokenId_.asSingletonArray(), amount_.asSingletonArray());

        return super.onERC1155Received(operator_, from_, tokenId_, amount_, data_);
    }

    function onERC1155BatchReceived(
        address operator_,
        address from_,
        uint256[] memory tokenIds_,
        uint256[] memory amounts_,
        bytes memory data_
    ) public override returns (bytes4) {
        _move(from_, tokenIds_, amounts_);

        return super.onERC1155BatchReceived(operator_, from_, tokenIds_, amounts_, data_);
    }

    function getAvailableTokens(
        uint256 offset_,
        uint256 limit_
    ) external view returns (uint256[] memory) {
        return _availableTokens.part(offset_, limit_);
    }

    function _setSource(address source_) private {
        require(
            ERC165(source_).supportsInterface(type(IERC1155).interfaceId),
            "Bridge: invalid source"
        );

        source = source_;
    }

    function _setDestination(address destination_) private {
        require(
            ERC165(destination_).supportsInterface(type(ICLDTNFT).interfaceId),
            "Bridge: invalid destination"
        );

        destination = destination_;
    }

    function _move(address from_, uint256[] memory tokenIds_, uint256[] memory amounts_) private {
        require(msg.sender == source, "Bridge: invalid source caller");
        require(from_ != address(0), "Bridge: mint is disallowed");
        require(tokenIds_.length > 0, "Bridge: empty tokens array");

        string[] memory uris_ = new string[](tokenIds_.length);
        for (uint256 i = 0; i < tokenIds_.length; i++) {
            require(
                _availableTokens.contains(tokenIds_[i]),
                "Bridge: token isn't in '_availableTokens' list"
            );
            require(amounts_[i] == 1, "Bridge: invalid amount");

            _availableTokens.remove(tokenIds_[i]);
            uris_[i] = "";
        }

        ICLDTNFT(destination).safeMintBatch(from_, tokenIds_, uris_);
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.17;

import {IAccessControl} from "@openzeppelin/contracts/access/IAccessControl.sol";
import {IERC165} from "@openzeppelin/contracts/utils/introspection/IERC165.sol";
import {Context} from "@openzeppelin/contracts/utils/Context.sol";

abstract contract AccessControlExtension is Context {
    address public accessControl;

    modifier hasRole(bytes32 role_) {
        require(IAccessControl(accessControl).hasRole(role_, _msgSender()), "ACE: forbidden");
        _;
    }

    function _setAccessControl(address accessControl_) internal {
        require(
            IERC165(accessControl_).supportsInterface(type(IAccessControl).interfaceId),
            "ACE: invalid access control"
        );

        accessControl = accessControl_;
    }
}

