pragma solidity ^0.8.17;

interface ICLDTNFT {
    /**
     * @notice Mint many NFTs
     * @param to_ Receiver address
     * @param tokenIds_ NFTs to mint
     * @param tokenURI_ NFTs URI
     */
    function safeMintBatch(
        address to_,
        uint256[] calldata tokenIds_,
        string[] calldata tokenURI_
    ) external;

    /**
     * @notice Set URI for many NFTs
     * @param tokenIds_ NFTs ID
     * @param tokenURI_ NFTs URI
     */
    function setTokenURIBatch(uint256[] calldata tokenIds_, string[] calldata tokenURI_) external;

    /**
     * @notice Return token URI
     * @param tokenId_ NFT Id
     */
    function tokenURI(uint256 tokenId_) external view returns (string calldata);
}