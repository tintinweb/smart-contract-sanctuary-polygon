pragma solidity ^0.8.17;

interface IBridge {
    /**
     * @notice Setup tokens available for move from ERC1155 to ERC721
     * @param tokenIds_ NFTs
     * @param status_ True - add to transfer list, false - remove
     */
    function setAvailableTokens(uint256[] calldata tokenIds_, bool status_) external;

    /**
     * @notice Setup source contract, ERC1155
     * @param source_ ERC1155 address
     */
    function setSource(address source_) external;

    /**
     * @notice Setup destination contract, ERC721
     * @param destination_ ERC721 address
     */
    function setDestination(address destination_) external;
}

