contract LiquidLabs is Ownable, ReentrancyGuard {


    // Interfaces for ERC20 and ERC721
      // Constructor function
    //constructor(IERC721 _nftCollection) {
        //nftCollection = _nftCollection;
    //}
    


    IERC721 public immutable nftCollection = IERC721(0x2289a081c2785196bAEd59F557B0Ef9dEf949776);
    using SafeMath for uint;
    address payable private manager; // Contract owner address
    constructor() {manager = payable(msg.sender);}
    uint256 interestrate = 100; 
    uint256 paybackduration = 5 minutes; //days
    uint256 protocolfee = 20;



    // Staker info
    struct LendNFt {
        // Lend offer amount
        uint256 offeramount;
        // Lend time
        uint256 timeOfLastUpdate;
        // Calculated, but unclaimed rewards for the User. The rewards are
        bool claimStatus;
        // User can cancel their progress and can claim back their liquids
        bool isuserCanceled;
        //Borrowrs address
        address borrower;
        //Borrowrs address
        address lender;
        //which nft ID is borrowed
        uint256 borrewdnftid;
    }

    mapping(address => LendNFt[]) public lender;
    mapping(uint256 => address) public lenderAddress;
    address[] public lendersArray;
     
  
    function lend() external payable {
        require(msg.value > 0);
        lender[msg.sender].push(LendNFt(msg.value, block.timestamp, false, false, manager, msg.sender, 0));
        lendersArray.push(msg.sender);
    }

    function checktolends(address _addr, uint256 _index) view public returns(uint256, uint256, bool, bool, address, uint256) {

                uint256 _offeramount = lender[_addr][_index].offeramount;
                uint256 _timeOfLastUpdate = lender[_addr][_index].timeOfLastUpdate;
                bool _claimStatus =  lender[_addr][_index].claimStatus;
                bool _isuserCanceled =  lender[_addr][_index].isuserCanceled;
                address _borrower = lender[_addr][_index].borrower;
              
                uint256 _borrewdnftid = lender[_addr][_index].borrewdnftid;

                if(_timeOfLastUpdate == 0){
                        return (0, 0 , false, false, msg.sender, 0);
                }else {
                        return (_offeramount, _timeOfLastUpdate, _claimStatus, _isuserCanceled, _borrower, _borrewdnftid);
                }
   }

   function getallmylends(address _addr) view public returns(uint256) {
       return  lender[_addr].length;
   }    
   
   function getallalllends() view public returns(uint256) {
       return  lendersArray.length;
   }

   



       // Borrower info
    struct BorrowNFT {
        // Lend offer amount
        uint256 nftid;
        // Lend time
        uint256 borrowtime;
        // Calculated, but unclaimed rewards for the User. The rewards are
        bool paidStatus;
        //we need store lender address here
        address lender;
        //also we need lendid too.
        uint256 lendid;
    }

    mapping(address => BorrowNFT[]) public borrower;
    mapping(uint256 => address) public borrowerAddress;
  



    //this function is related by the borrow.
    function borrow(uint256 _tokenId, uint256 _index, address _addr) external nonReentrant {
        require(nftCollection.ownerOf(_tokenId) == msg.sender, "Can't stake tokens you don't own!");
        require(lender[_addr][_index].borrower == manager , "This offer already used by another person.");
        require(lender[_addr][_index].claimStatus == false, "This offerhas been end");
        require(lender[_addr][_index].isuserCanceled == false, "This offer has been closed by the lender");

        nftCollection.transferFrom(msg.sender, address(this), _tokenId);
        borrowerAddress[_tokenId] = msg.sender;
        borrower[msg.sender].push(BorrowNFT(_tokenId, block.timestamp, false, _addr, _index));
 
        lender[_addr][_index].borrower = msg.sender;
        lender[_addr][_index].borrewdnftid = _tokenId;
        payable(msg.sender).transfer(lender[_addr][_index].offeramount);


    }
    
    function getallmyborrows(address _addr) view public returns(uint256) {
       return  borrower[_addr].length;
    }


    function checktoborrows(address _addr, uint256 _index) view public returns(uint256, uint256, bool,address, uint256, uint256) {
                uint256 _nftid = borrower[_addr][_index].nftid;
                uint256 _borrowtime = borrower[_addr][_index].borrowtime;
                bool _paidStatus =  borrower[_addr][_index].paidStatus;
                address _lender  =  borrower[_addr][_index].lender;
                uint256 _lendid = borrower[_addr][_index].lendid;
                uint256 calcint = lender[_lender][_lendid].offeramount.div(100).mul(interestrate);
                uint256 finalcal = lender[_lender][_lendid].offeramount + calcint;

                if(_borrowtime == 0){
                        return (0, 0 , false, msg.sender, 0, 0);
                }else {
                        return (_nftid, _borrowtime, _paidStatus, _lender, _lendid, finalcal);
                }
   }

    function repay(uint256 _tokenId, uint256 _index, address _addr) external payable nonReentrant {
        require(borrowerAddress[_tokenId] == msg.sender);
        borrower[msg.sender][_index].borrowtime = block.timestamp;
        uint256 calcint = lender[_addr][_index].offeramount.div(100).mul(interestrate);
        uint256 calcpfree = calcint.div(100).mul(protocolfee);
        uint256 finalamount = lender[_addr][_index].offeramount.add(calcint);
        require(msg.value >= finalamount);

        
        payable(manager).transfer(calcpfree);
        payable(borrower[msg.sender][_index].lender).transfer(finalamount.sub(calcpfree));

        borrowerAddress[_tokenId] = address(0);
        nftCollection.transferFrom(address(this), msg.sender, _tokenId);
    }

    function canceloffer(uint256 _index) external payable {
        require(lender[msg.sender][_index].borrower == manager , "This offer already used by another person.");
        require(lender[msg.sender][_index].isuserCanceled == false, "This offer has been closed by the lender");
        lender[msg.sender][_index].isuserCanceled = true;
        payable(msg.sender).transfer(lender[msg.sender][_index].offeramount);
    }

    function claimunpaidNFT(uint256 _index) external {

        address _getborrower = lender[msg.sender][_index].borrower;
        require(lender[msg.sender][_index].borrower != manager , "Dept is not active");
        require(lender[msg.sender][_index].isuserCanceled == false, "This offer has been closed by the lender");
        require(lender[msg.sender][_index].borrower != manager , "Dept is not active");
        uint256 _borrowtime = borrower[_getborrower][_index].borrowtime;
        require(_borrowtime < _borrowtime + 5 minutes, "You just need to wait at least 5 minutes");
        uint256 _nftid = borrower[_getborrower][_index].nftid;
        borrowerAddress[_nftid] = address(0);
        nftCollection.transferFrom(address(this), msg.sender, _nftid);


    }





    function setInterest(uint256 _interestRate) public onlyOwner() {
        interestrate = _interestRate;
    }

    function getInterest() public view returns(uint256) {
        return interestrate;
    }


    function setDuration(uint256 _paybackduration) public onlyOwner() {
        paybackduration = _paybackduration;
    }

    function getDuration() public view returns(uint256) {
        return paybackduration;
    }

    function emergencywithdrawal(uint256 _eth) public onlyOwner() {
        payable(manager).transfer(_eth);
    }


    function changemanager(address _newmanager) public onlyOwner() {
        manager = payable(_newmanager);
    }

    function changeprotocolfee(uint256 _pfee) public onlyOwner() {
        protocolfee = _pfee;
    }
    
    


