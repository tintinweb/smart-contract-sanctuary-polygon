    function improve(
        uint256 _tokenId
    ) external onlyValidFutbala(_tokenId) onlySkillLearningNFT returns (bool) {
        Futbaler storage futbalar = futbalers[_tokenId];
        futbalar._skill.skillLevel = futbalar._skill.skillLevel + 1;
        futbalar._skill.lastSkillImprovementSession = block.timestamp;
        return true;
    }

    /**
     * @dev Function to set skill
     * @param _tokenId {uint32} futbalar token id
     * @param _skill {uint32} skill to set.
     * @return {bool} true if training is successful.
     */

    function setSkill(uint256 _tokenId, uint32 _skill) external returns (bool) {
        Futbaler storage futbalar = futbalers[_tokenId];
        futbalar._skill.skill = _skill;
        futbalar._skill.skillLevel = 1;
        futbalar._skill.lastSkillLearningSession = block.timestamp;
        return true;
    }

    /**
     * @dev Function to get total futbalar stats
     * @param _tokenId {uint32} futbalar token id
     * @return _totalStats {uint32} sum of stats for player.
     */
    function getTotalFutbalarStats(
        uint256 _tokenId
    ) internal view onlyValidFutbala(_tokenId) returns (uint32 _totalStats) {
        Futbaler storage futbalar = futbalers[_tokenId];
        for (uint32 index = 0; index < TOTAL_STATS; index++) {
            _totalStats += futbalar._stats[index];
        }
    }

    /**
     * @dev Function to get scpre fee for a tier
     * @param _tier {uint32} tier index in which one wants to scout player.
     * @return {uint256} scouting fee
     */
    function getTierFee(uint32 _tier) public view returns (uint256) {
        return scoutingTierFees[_tier];
    }

    /**
     * @dev Callback function of chainlink VRF
     * @param _requestId {bytes32} request id of random number request
     * @param _randomness {uint256} Random number generated by chainlink VRF
     */
    function fulfillRandomness(
        bytes32 _requestId,
        uint256 _randomness
    ) internal override {
        if (requestTypes[_requestId] == "scout") {
            uint32 _numberOfPlayerstoMint = numberOfMint[_requestId];

            uint256[] memory randomNumbers = new uint256[](
                _numberOfPlayerstoMint
            );

            randomNumbers = expandValues(_randomness, _numberOfPlayerstoMint);

            for (uint256 i = 0; i < _numberOfPlayerstoMint; i++) {
                createFutbaler(_requestId, randomNumbers[i]);
            }
        }
    }

    /**
     * @dev Function to create Futbala token for person
     * @param _requestId {bytes32} request id of random number request
     * @param _randomNumber {uint256} Random number generated by chainlink VRF
     */
    function createFutbaler(
        bytes32 _requestId,
        uint256 _randomNumber
    ) internal {
        // Get tier of mint request from request id
        uint32 tier = requestTiers[_requestId];
        // get address of person in which account player to be minted
        address to = playerOwners[_requestId];
        // get player metadata based on mint request
        PlayerMeta memory playerMeta = generatePlayerMeta(_requestId);
        // Get random stats of futbaler from random number
        uint32[6] memory stats = generateRandomStats(_randomNumber);
        // initial skills to zero
        Skill memory skill;

        // create futbaler
        Futbaler memory player = Futbaler(
            getRandomRarity(tier, _randomNumber),
            getRandomRole(_randomNumber),
            EnergyStatus.NORMAL,
            playerMeta,
            skill,
            stats,
            1
        );
        _totalSupply++;
        // mint futbaler
        _safeMint(to, _totalSupply);
        // Assgin futbaler to tokenId
        futbalers[_totalSupply] = player;

        emit Minted(to, _totalSupply);
    }

    /**
     * @dev generate futbalaer original metadata
     * @param requestId {bytes32} requestId of random number request
     * @return _playerMeta {PlayerMeta} PlayerMeta struct
     */
    function generatePlayerMeta(
        bytes32 requestId
    ) internal view returns (PlayerMeta memory _playerMeta) {
        _playerMeta = PlayerMeta(
            playerOwners[requestId], // Owner of the player
            block.timestamp,
            originalMinters[requestId] // Person who has paid for minting
        );
    }

    function shuffle(
        uint8[6] memory statsOrder
    ) internal view returns (uint8[6] memory) {
        for (uint8 i = 0; i < statsOrder.length; i++) {
            uint256 n = i +
                (uint256(keccak256(abi.encodePacked(block.timestamp))) %
                    (statsOrder.length - i));
            uint8 temp = statsOrder[n];
            statsOrder[n] = statsOrder[i];
            statsOrder[i] = temp;
        }

        return statsOrder;
    }

    function expandValues(
        uint256 randomNumber,
        uint256 count
    ) internal pure returns (uint256[] memory) {
        uint256[] memory expandedValues = new uint256[](count);

        for (uint256 i = 0; i < count; i++) {
            expandedValues[i] = uint256(keccak256(abi.encode(randomNumber, i)));
        }

        return expandedValues;
    }

    /**
     * @dev generate futbalaer random stats (This function needs improvement)
     * @param _randomness {uint256} Random number generated by chainlink VRF
     * @return _stats {uint32[]} array of random stats
     */
    function generateRandomStats(
        uint256 _randomness
    ) internal view returns (uint32[6] memory) {
        //randomize the stats order
        uint8[6] memory statsOrder = [0, 1, 2, 3, 4, 5];
        shuffle(statsOrder);

        uint32 statsLeft = 120;

        uint256[] memory expandedValues;

        expandedValues = expandValues(_randomness, TOTAL_STATS);
        uint32[6] memory _stats;

        //Loop each stat only once
        for (uint32 i = 0; i < TOTAL_STATS; i++) {
            // Generate random stat number between 0 and 40 using an array of 6 random numbers
            uint32 statToBeGiven = uint32(expandedValues[i] % 40);

            // Add base points (100) and bonus points [0-40] to each one respecting the 120 max
            if (statsLeft >= statToBeGiven) {
                _stats[statsOrder[i]] = 100 + statToBeGiven;
                statsLeft = statsLeft - statToBeGiven;
            } else {
                _stats[statsOrder[i]] = 100 + statsLeft;
                statsLeft = 0;
            }
        }

        return _stats;
    }

    /**
     * @dev Generate random rarity based on tier and random number
     * @param _tier {uint32} tier index in which one wants to scout player.
     * @param _randomNumber {uint256} Random number generated by chainlink VRF
     * @return _rarity {Rarity} random rarity
     */
    function getRandomRarity(
        uint32 _tier,
        uint256 _randomNumber
    ) internal view returns (Rarity _rarity) {
        // Generate rarity based on tier selected
        uint256 randomNumberInRange = (_randomNumber % 100) + 1;

        for (uint32 i = 1; i <= TOTAL_RARITY; i++) {
            if (
                randomNumberInRange >=
                ((scoutingTierProbability[_tier][Rarity(i - 1)]) + 1) &&
                randomNumberInRange <= scoutingTierProbability[_tier][Rarity(i)]
            ) {
                _rarity = Rarity(i);
                break;
            }
        }
    }

    /**
     * @dev Generate random role based on random number
     * @param _randomNumber {uint256} Random number generated by chainlink VRF
     * @return _role {Role} random role
     */
    function getRandomRole(
        uint256 _randomNumber
    ) internal view returns (Role _role) {
        uint256 randomNumberInRange = (_randomNumber % TOTAL_ROLES) + 1;
        _role = Role(randomNumberInRange);
    }

    /**
     * @dev Function to get futbaler based on tokenId
     * @param _tokenId {uint256} tokenId of futbaler
     * @return _futbaler {Futbaler} Futbaler struct
     */
    function getFutbaler(
        uint256 _tokenId
    )
        public
        view
        onlyValidFutbala(_tokenId)
        returns (Futbaler memory _futbaler)
    {
        _futbaler = futbalers[_tokenId];
    }

    /**
     * @dev Function to get futbaler based on tokenId
     * @param _tokenId {uint256} tokenId of futbaler
     * @return _futbaler {Futbaler} Futbaler struct
     */
    function getFutbalerSkillLevel(
        uint256 _tokenId
    ) public view onlyValidFutbala(_tokenId) returns (uint32) {
        Futbaler memory player = futbalers[_tokenId];
        return player._skill.skillLevel;
    }

    /**
     * @dev Function to get futbaler based on tokenId
     * @param _tokenId {uint256} tokenId of futbaler
     * @return _futbaler {Futbaler} Futbaler struct
     */
    function getFutbalerSkill(
        uint256 _tokenId
    ) public view onlyValidFutbala(_tokenId) returns (uint32) {
        Futbaler memory player = futbalers[_tokenId];
        return player._skill.skill;
    }

    /**
     * @dev Function to get futbaler based on tokenId
     * @param _tokenId {uint256} tokenId of futbaler
     * @return _futbaler {Futbaler} Futbaler struct
     */
    function getFutbalerExperienceLevel(
        uint256 _tokenId
    ) public view onlyValidFutbala(_tokenId) returns (uint32) {
        Futbaler memory player = futbalers[_tokenId];
        return player._experienceLevel;
    }

    // Admin functions

    /**
     * @dev Function to withdtaw tokens from contract
     * @param _to {address} address where withdrawal needs to be sent
     * @param _amount {uint256} amount to be withdrawn
     */
    function withdrawFunds(address _to, uint256 _amount) public onlyOwner {
        require(_amount > 0, "Amount must be greater than 0");
        require(
            acceptedCurrency.balanceOf(address(this)) >= _amount,
            "Amount must be less than or equal to balance"
        );
        require(
            acceptedCurrency.transfer(_to, _amount),
            "Futbala : Transfer failed"
        );
    }

    /**
     * @dev sets accepted currency to mint player
     * @param _acceptedCurrency {address} Any ERC20 token
     */
    function setCurrency(address _acceptedCurrency) external onlyOwner {
        require(
            _acceptedCurrency != address(0),
            "FutbalaAdmin : Invalid currency"
        );
        acceptedCurrency = IERC20(_acceptedCurrency);
    }

    function setTrainingNFT(address _trainingNFT) external onlyOwner {
        require(
            _trainingNFT != address(0),
            "FutbalaAdmin : Invalid training NFT contract address"
        );
        trainingNFT = _trainingNFT;
    }

    function setSkillImprovementNFT(
        address _skillImprovementNFT
    ) external onlyOwner {
        require(
            _skillImprovementNFT != address(0),
            "FutbalaAdmin : Invalid training NFT contract address"
        );
        skillImprovementNFT = _skillImprovementNFT;
    }

    function setSkillLearningNFT(address _skillLearningNFT) external onlyOwner {
        require(
            _skillLearningNFT != address(0),
            "FutbalaAdmin : Invalid skill learning NFT contract address"
        );
        skillLearningNFT = _skillLearningNFT;
    }

    /**
     * @dev sets tier wise fees
     * @param _scoutingTierFees {uint256[]} Array of tier wise fees in wei
     */
    function setScoutingTierFees(
        uint256[] calldata _scoutingTierFees
    ) external onlyOwner {
        for (uint256 i = 0; i < _scoutingTierFees.length; i++) {
            require(
                _scoutingTierFees[i] > 0,
                "FutbalaAdmin : Invalid scouting tier fees"
            );
            scoutingTierFees.push(_scoutingTierFees[i]);
        }
    }

    /**
     * @dev get tier wise fees
     * @param _index {uint256} index of tier
     */
    function getScoutingTierFees(
        uint256 _index
    ) external view returns (uint256) {
        return scoutingTierFees[_index];
    }

    /**
     * @dev Set capping for stats of rarity
     * @param _rarity {uint32} Rarity
     * @param _capping {uint32} Capping of stats for player
     */
    function setRarityCapping(
        uint32 _rarity,
        uint32 _capping
    ) external onlyValidRarity(_rarity) onlyOwner {
        Rarity rarity = Rarity(_rarity);
        rarityCapping[rarity] = _capping;
    }

    /**
     * @dev Set capping for stats of rarity
     * @param _level {uint32} Experience Level
     * @param _statsSum {uint32} sum of stats for player
     * @param _rarity {uint32} Rarity
     */
    function setExperienceList(
        uint32 _level,
        uint32 _statsSum,
        uint32 _rarity
    ) external onlyValidRarity(_rarity) onlyValidExperience(_level) onlyOwner {
        statsToLevel[_level] = _statsSum;
        rarityToLevel[_level] = _rarity;
    }

    /**
     * @dev Set probability list for tier wise, rarity wise.
     * @param _tierId {uint256} Tier for which probability is to be set
     * @param _rarity {uint32} Rarity for which probability is to be set
     * @param _probability {uint256} Probability (between 0 and 100)
     */
    function setRarityList(
        uint256 _tierId,
        uint32 _rarity,
        uint256 _probability
    )
        external
        onlyValidTier(_tierId)
        onlyValidRarity(_rarity)
        onlyValidProbability(_probability)
        onlyOwner
    {
        Rarity rarity = Rarity(_rarity);
        scoutingTierProbability[_tierId][rarity] = _probability;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal override(ERC721Upgradeable, ERC721EnumerableUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

