pragma solidity ^0.8.9;

import {IERC20} from "@openzeppelin/contracts/interfaces/IERC20.sol";

contract FutbalaStorage {
    string internal baseURI;

    uint32 TOTAL_RARITY;
    uint32 TOTAL_ROLES;
    uint32 TOTAL_STATS;
    uint32 MAX_MINT_PER_TRANSACTION;
    uint32 RANDOM_STATS_TO_BE_GIVEN;
    uint32 MAX_RANDOM_STATS_TO_BE_GIVEN;
    uint32 TOTAL_LEVELS;
    uint32 public minCapValue; // Minimum cap value for a stat

    /*** Mutable Variables ****/
    IERC20 public acceptedCurrency; // Any ERC20 token
    address public trainingNFT; // Training NFT contract address
    address public skillLearningNFT; // Skill Learning NFT contract address
    address public skillImprovementNFT; // Skill Learning NFT contract address
    bytes32 internal s_keyHash; // Hash of the key used to sign the VRF proof
    uint256 internal s_fee; //   Fee to call chainlink VRF
    uint256 public _totalSupply; // Total supply of the Futbala token (Number of players)

    /**
     *@dev Role of player which can be assigned at a time of scouting
     */
    enum Role {
        DEFAULT,
        GK,
        RB,
        LB,
        CB,
        LM,
        RM,
        CM,
        CF,
        RF,
        LF
    }

    enum Rarity {
        DEFAULT,
        COMMON,
        UNCOMMON,
        RARE,
        EPIC,
        LEGENDARY
    }

    /**
     *@dev Player's Current status based previous match
     */
    enum EnergyStatus {
        NORMAL,
        RESTED,
        EXAUSTED,
        INJURED
    }

    /**
     *@dev Stats of player
     */
    enum Stats {
        SHT,
        PAS,
        DEF,
        CTH,
        FCS,
        NRG
    }

    /**
     *@dev {tokenId} -> {Futbaler} mapping
     */
    mapping(uint256 => Futbaler) public futbalers;

    /**
     *@dev  Scouting Fee Array
     */
    uint256[] public scoutingTierFees;

    /**
     *@dev {tierId} => {Rarity} => {Percentage} Mapping to store Tier/Rarity/Probability Mapping
     */
    mapping(uint256 => mapping(Rarity => uint256))
        public scoutingTierProbability;

    /**
     *@dev {Rarity} => {Capping} Maximum number of stats can be achieved by rarity
     */
    mapping(Rarity => uint32) public rarityCapping;

    /**
     *@dev {Level} => {statsSum} level to Stats sum mapping
     */
    mapping(uint32 => uint32) public statsToLevel;

    /**
     *@dev {Level} =< {rarity} Level to Rarity mapping
     */
    mapping(uint32 => uint32) public rarityToLevel;

    /**
     * @dev Mappings to associate randomNumbers with various requests.
     */
    mapping(bytes32 => bytes32) internal requestTypes;
    mapping(bytes32 => uint32) internal numberOfMint;
    mapping(bytes32 => uint32) internal requestTiers;
    mapping(bytes32 => address) internal playerOwners;
    mapping(bytes32 => address) internal originalMinters;

    /**
     *@dev Attributes of Player
     */
    struct PlayerMeta {
        address originalMinter;
        uint256 birthAt;
        address referral;
    }

    /**
     *@dev Player's Last sessions
     */
    struct Skill {
        uint32 skill;
        uint32 skillLevel;
        uint256 lastTrainingSession;
        uint256 lastSkillLearningSession;
        uint256 lastSkillImprovementSession;
    }

    struct Futbaler {
        Rarity _rarity;
        Role _role;
        EnergyStatus _energyStatus;
        PlayerMeta _meta;
        Skill _skill;
        uint32[6] _stats;
        uint32 _experienceLevel;
    }
}

