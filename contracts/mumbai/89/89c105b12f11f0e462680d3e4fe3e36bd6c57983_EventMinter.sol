pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "../interface/IEvent.sol";
import "hardhat/console.sol";

contract EventMinter is Ownable {
    using ECDSA for bytes32;

    IEvent public immutable evt;
    address private _signer;

    event SignerChanged(address signer);
    event Claimed(uint tokenId, address user);
    event Airdroped(uint256 indexed tokenId, address indexed to);

    modifier creatorOnly(uint256 tokenId) {
        require(evt.creators(tokenId) == _msgSender(), "ONLY_CREATOR_ALLOWED");
        _;
    }

    constructor(address evt_, address signer) {
        evt = IEvent(evt_);
        _signer = signer;
    }

    function setSigner(address signer) external onlyOwner {
        _signer = signer;
        emit SignerChanged(signer);
    }

    function getSigner() external view returns (address) {
        return _signer;
    }

    function create(IEvent.Activity memory act) external {
        address creator = msg.sender;
        act.creator = creator;

        require(_checkkAct(act), "INVALID_ACTIVITY");

        evt.create(act, "0x");
    }

    function claim(uint256 tokenId, bytes calldata signature) external {
        bytes32 hash = keccak256(
            abi.encodePacked(
                "claim",
                tokenId,
                address(evt),
                address(msg.sender)
            )
        );

        require(_verify(hash, signature), "Invalid signer");

        require(evt.balanceOf(msg.sender, tokenId) == 0, "Already Minted");

        evt.mint(msg.sender, tokenId, 1, "0x");

        emit Claimed(tokenId, msg.sender);

        _checkMintable(tokenId);
    }

    function airdrop(
        uint256 tokenId,
        address[] calldata receivers
    ) external creatorOnly(tokenId) {
        for (uint i = 0; i < receivers.length; i++) {
            evt.mint(receivers[i], tokenId, 1, "0x");
            emit Airdroped(tokenId, receivers[i]);
        }

        _checkMintable(tokenId);
    }

    function updateActivity(
        uint256 tokenId,
        IEvent.Activity calldata act
    ) external creatorOnly(tokenId) {
        require(_checkkAct(act), "INVALID_ACTIVITY");

        evt.setActivity(tokenId, act);
    }

    function setCustomURI(uint256 tokenId, string memory uri) external {
        IEvent.Activity memory act = evt.getActivity(tokenId);

        act.uri = uri;
        evt.setActivity(tokenId, act);

        evt.setCustomURI(tokenId, uri);
    }

    function _checkMintable(uint256 tokenId) internal view {
        IEvent.Activity memory act = evt.getActivity(tokenId);

        if (act.supply > 0) {
            require(
                evt.totalSupply(tokenId) <= uint256(act.supply),
                "Over limit"
            );
        }

        require(block.timestamp >= act.startTs, "Not in time");
    }

    function _checkkAct(
        IEvent.Activity memory act
    ) internal pure returns (bool) {
        if (act.endTs <= act.startTs) return false;

        return true;
    }

    function _verify(
        bytes32 hash,
        bytes calldata signature
    ) internal view returns (bool) {
        return (_recover(hash, signature) == _signer);
    }

    function _recover(
        bytes32 msgHash,
        bytes calldata signature
    ) internal pure returns (address) {
        return msgHash.toEthSignedMessageHash().recover(signature);
    }
}

