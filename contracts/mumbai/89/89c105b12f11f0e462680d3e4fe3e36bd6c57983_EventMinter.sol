pragma solidity ^0.8.0;

interface IEvent {
    struct Activity {
        uint32 startTs;
        uint32 endTs;
        uint192 supply;
        string uri;
        address creator;
    }

    function exists(uint256 _id) external view returns (bool);

    function creators(uint256 tokenId) external returns (address);

    function create(
        Activity memory act,
        bytes memory data
    ) external returns (uint256);

    function mint(
        address _to,
        uint256 _id,
        uint256 _quantity,
        bytes memory _data
    ) external;

    function acts(uint256) external view returns (Activity memory act);

    function getActivity(
        uint256 tokenId
    ) external view returns (Activity memory act);

    function setActivity(uint256 tokenId, Activity memory act) external;

    function setCustomURI(uint256 tokenId, string memory uri) external;

    function totalSupply(uint256 tokenId) external view returns (uint256);

    function balanceOf(
        address account,
        uint256 tokenId
    ) external view returns (uint256);
}

