pragma solidity ^0.8.16;

import "openzeppelin-contracts/contracts/utils/Create2.sol";
import "openzeppelin-contracts/contracts/proxy/ERC1967/ERC1967Proxy.sol";
import "openzeppelin-contracts/contracts/access/Ownable.sol";
import { BeaconProxy } from "openzeppelin-contracts/contracts/proxy/beacon/BeaconProxy.sol";
import "./Account.sol";
import "../lib/Types.sol";
import "../../../proxy/UpgradeableBeacon.sol";
import "../../../deployer/Create2Deployer.sol";

/**
 * A sample factory contract for SimpleAccount
 * A UserOperations "initCode" holds the address of the factory, and a method call (to createAccount, in this sample factory).
 * The factory's createAccount returns the target account address even if it is already installed.
 * This way, the entryPoint.getSenderAddress() can be called either before or after the account is created.
 */
contract AccountFactory is Ownable {
    uint256 public constant version = 3;
    MAccount public immutable accountImplementation;
    address public immutable accountBeacon;
    event AccountCreated(address indexed newAccAddress, string extId);

    constructor(IEntryPoint _entryPoint){
        accountImplementation = new MAccount(_entryPoint);
        accountBeacon =  address(new UpgradeableBeacon{ salt: Types.SALT }(address(accountImplementation), address(this)));
    }

    function createAccount(address owner, string memory externalId) external returns (MAccount ret) {
        address addr = getAddress(owner, externalId, Types.SALT);
        uint codeSize = addr.code.length;
        if (codeSize > 0) {
            return MAccount(payable(addr));
        }
        bytes memory data = abi.encodeWithSelector(
            MAccount.initialize.selector,
            owner,
            externalId
        );
        address proxyAddress = deploy(
            abi.encodePacked(
                type(BeaconProxy).creationCode,
                abi.encode(accountBeacon, data)
            ),
            Types.SALT
        );
        ret = MAccount(payable(proxyAddress));
        emit AccountCreated(proxyAddress, externalId);
    }

    function deploy(bytes memory code, bytes32 salt) public returns (address) {
        address addr;
        require(code.length != 0, "Create2: bytecode length is zero");
        assembly {
            addr := create2(0, add(code, 0x20), mload(code), salt)
            if iszero(extcodesize(addr)) {
                revert(0, 0)
            }
        }
        return addr;
    }

    /**
     * calculate the counterfactual address of this account as it would be returned by createAccount()
     */
    function getAddress(address owner, string memory externalId, bytes32 salt) public view returns (address) {
        return Create2.computeAddress(salt, keccak256(abi.encodePacked(
                type(BeaconProxy).creationCode,
                abi.encode(
                    accountBeacon,
                    abi.encodeCall(MAccount.initialize, (owner, externalId))
                )
            )));
    }

    function upgradeAccountBeacon(address newImpl) external onlyOwner {
        UpgradeableBeacon(accountBeacon).upgradeTo(newImpl);
    }
}

