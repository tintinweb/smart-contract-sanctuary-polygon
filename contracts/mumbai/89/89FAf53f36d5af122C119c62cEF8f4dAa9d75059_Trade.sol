import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
// import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/IERC20MetadataUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "./BasicMetaTransaction.sol";
import "./interfaces/IPaza.sol";
import "hardhat/console.sol";

contract Trade is
    Initializable,
    BasicMetaTransaction,
    AccessControlUpgradeable
{
    bytes32 public constant FACTORY_ROLE = keccak256("FACTORY_ROLE");

    uint256 public constant PCT_BASE = 1000;
    using SafeERC20Upgradeable for IERC20MetadataUpgradeable;
    using SafeERC20Upgradeable for IPaza;
    IERC20MetadataUpgradeable public usdc;

    IPaza public paza;

    uint public price;

    event Pazabuy(address user, uint256 _amount);
    event Pazasell(address user, uint256 _amount);

    constructor() {
        _disableInitializers();
    }

    function initialize(
        address _usdc,
        address _paza,
        address _admin,
        address _factory,
        uint _pazaPrice
    ) public initializer {
        usdc = IERC20MetadataUpgradeable(_usdc);
        paza = IPaza(_paza);
        price = _pazaPrice;
        _grantRole(DEFAULT_ADMIN_ROLE, _admin);
        _grantRole(FACTORY_ROLE, _factory);
        __AccessControl_init();
        __Context_init();
        __ERC165_init();
    }

    modifier validation(address _address) {
        require(_address != address(0), "zero address");
        _;
    }

    /**
     * @notice used to give the Factory Role
     * @param _factory address of factory contract
     **/

    function setFactoryRole(
        address _factory
    ) external onlyRole(DEFAULT_ADMIN_ROLE) validation(_factory) {
        _grantRole(FACTORY_ROLE, _factory);
    }

    /**
     * @notice used to set the usdc contract
     * @param _usdc address of USDC contract
     **/

    function setUsdcContract(
        address _usdc
    ) external onlyRole(DEFAULT_ADMIN_ROLE) validation(_usdc) {
        usdc = IERC20MetadataUpgradeable(_usdc);
    }

    function getTokenPrice() public view returns (uint256) {
        return price;
    }

     function setPazaContract(
        address _paza
    ) external onlyRole(DEFAULT_ADMIN_ROLE) validation(_paza) {
        paza = IPaza(_paza);
    }

    /**
     * @notice used to set the Paza Price in 
     * @param _price set Paza Price(amount of Paza with decimals for 1$)
     **/

    function setPazaPrice(uint _price) external onlyRole(DEFAULT_ADMIN_ROLE) {
        price = _price;
    }

    /**
     * @notice used to buy the paza token by depositing the usdc
     * @param _amount amount of paza token to buy with PCT_BASE
     **/

    function buy(uint256 _amount) public {
        uint usdcAmount = ((_amount * 10 ** usdc.decimals()) * 10 ** paza.decimals()) /
            (price * PCT_BASE );
        require(usdcAmount >= 1000, "Invalid Amount");
        console.log("usdc amount-- ", (_amount * 10 ** paza.decimals()) / PCT_BASE);
        require(
            usdc.balanceOf(_msgSender()) >= usdcAmount,
            "Insufficient balance"
        );
        usdc.safeTransferFrom(_msgSender(), address(this), usdcAmount);
        paza.safeTransfer(
            _msgSender(),
            (_amount * 10 ** paza.decimals()) / PCT_BASE
        );
        emit Pazabuy(
            _msgSender(),
            (_amount * 10 ** paza.decimals()) / PCT_BASE
        );
    }

    /**
     * @notice used to sell the paza token to get back the usdc
     * @param _amount of paza token to sell with PCT_BASE
     **/

    function sell(uint256 _amount) public {
        require(
            paza.balanceOf(_msgSender()) >=
                (_amount * 10 ** paza.decimals()) / PCT_BASE,
            "Insufficient balance"
        );
        paza.safeTransferFrom(
            _msgSender(),
            address(this),
            (_amount * 10 ** paza.decimals()) / PCT_BASE
        );
        uint usdcAmount = (_amount * 10 ** usdc.decimals() * 10 ** paza.decimals()) /
            (price * PCT_BASE);
        require(usdcAmount >= 1000, "Invalid Amount");
        usdc.safeTransfer(_msgSender(), usdcAmount);
        emit Pazasell(
            _msgSender(),
            (_amount * 10 ** paza.decimals()) / PCT_BASE
        );
    }

    /**
     * @notice used to buy the paza token with approval
     * @param _payee address from which the  usdc will be transferred to _beneficiary
     * @param _beneficiary address that will receive the paza tokenzzzzl
     * @param _amount of paza token with decimals without PCT BASE
     **/

    function buyWithApproval(
        address _payee,
        address _beneficiary,
        uint256 _amount
    ) external onlyRole(FACTORY_ROLE) {
        uint usdcAmount = (_amount * 10 ** usdc.decimals() ) /
            (price);
        require(usdcAmount >= 1000, "Invalid Amount");
        usdc.safeTransferFrom(_payee, address(this), usdcAmount);
        paza.safeTransfer(_beneficiary, _amount);
    }

    function _msgSender()
        internal
        view
        override(ContextUpgradeable, BasicMetaTransaction)
        returns (address sender)
    {
        if (msg.sender == address(this)) {
            bytes memory array = msg.data;
            uint256 index = msg.data.length;
            assembly {
                // Load the 32 bytes word from memory with the address on the lower 20 bytes, and mask those.
                sender := and(
                    mload(add(array, index)),
                    0xffffffffffffffffffffffffffffffffffffffff
                )
            }
        } else {
            return msg.sender;
        }
    }
}

