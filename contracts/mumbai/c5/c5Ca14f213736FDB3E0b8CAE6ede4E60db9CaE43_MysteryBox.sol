// SPDX-License-Identifier: MIT
// - text code for discount X%
// - text code for discount Y%
// - text code for discount Z%
// - fixed amount of CRH token
// - mint another NFT X for free
// - mint another NFT Y for free
// - fixed amount of USD token
pragma solidity 0.8.17;

import "erc721a/contracts/extensions/ERC721AQueryable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

contract MysteryBox is ERC721AQueryable, Ownable, ReentrancyGuard {
    using SafeERC20 for IERC20;

    // base token uri for NFT
    string internal _baseTokenURI;

    // mapping for allowed addresses
    mapping(address => bool) public isAllowed;

    string public baseUri =
        "https://storage.googleapis.com/crypto-hunters-test/";
    event randomIndex(uint256 index);

    struct PrizePoolInfo {
        uint256 prizeType; // 1 = text code ,  2 = free nft transfer, 3 = free Erc20 Token (USDT or CRH)
        uint256 totalPrizes;
        uint256 rewarded;
    }
    struct MysterBoxInfo {
        uint256 prizeType;
        bool claimed;
        address user;
        uint256 tokenId;
        uint256 poolId;
    }
    struct NFTsTokensInfo {
        address NFTAddress;
        address NFTOwner;
    }
    struct TokensInfo {
        address TokenAddress;
        uint256 decimals;
        uint256 NumOfTokensReward;
    }
    PrizePoolInfo[] public prizePools;
    MysterBoxInfo[] public nftBoxes;
    mapping(uint256 => MysterBoxInfo) public tokenIdToBoxInfo;
    mapping(uint256 => NFTsTokensInfo) public poolIdtoNftsTokenInfo;
    mapping(address => uint256[]) internal nftAddressToTokenIds;
    mapping(uint256 => TokensInfo) public poolIdtoTokensInfo;

    /**
     * @dev modifier to detect if address is allowed for buying coins
     */
    modifier whenAllowed() {
        require(
            isAllowed[msg.sender],
            "MysteryBox: address is not allowed to call this function"
        );
        _;
    }

    /// @dev Sets main dependencies and constants
    /// @param name_ 721A nft name
    /// @param symbol_ 721A nft symbol

    constructor(
        string memory name_,
        string memory symbol_
    ) ERC721A(name_, symbol_) {}

    /// @dev add nft inside pool
    /// @param _NftAddress nft address of token as rewarded
    /// @param _NFTOwner nft owner who ownes tokens for reward
    /// @param _ids total ids for prizes

    function addNftpool(
        address _NftAddress,
        address _NFTOwner,
        uint256[] memory _ids
    ) external onlyOwner {
        require(
            _ids.length > 0,
            "MysteryBox: number of prizes should be greater then zero"
        );
        require(
            IERC721(_NftAddress).isApprovedForAll(_NFTOwner, address(this)),
            "no permission to transfer NFT"
        );
        PrizePoolInfo memory pool = PrizePoolInfo({
            prizeType: 2,
            totalPrizes: _ids.length,
            rewarded: 0
        });
        NFTsTokensInfo memory NftInfo = NFTsTokensInfo({
            NFTAddress: _NftAddress,
            NFTOwner: _NFTOwner
        });
        for (uint256 i = 0; i < _ids.length; i++) {
            nftAddressToTokenIds[_NftAddress].push(_ids[i]);
        }

        poolIdtoNftsTokenInfo[prizePools.length] = NftInfo;
        prizePools.push(pool);
    }

    /// @dev  more ids to  nft  pool
    /// @param _NftAddress nft address of token as rewarded
    /// @param _NFTOwner nft owner who ownes tokens for reward
    /// @param _ids total ids for prizes

    function addIdsToNftPool(
        address _NftAddress,
        address _NFTOwner,
        uint256[] memory _ids
    ) external onlyOwner {
        require(
            _ids.length > 0,
            "MysteryBox: number of prizes should be greater then zero"
        );
        require(
            IERC721(_NftAddress).isApprovedForAll(_NFTOwner, address(this)),
            "no permission to transfer NFT"
        );
        for (uint256 i = 0; i < _ids.length; i++) {
            nftAddressToTokenIds[_NftAddress].push(_ids[i]);
        }
    }

    /// @dev add token inside prize pool
    /// @param _totalPrizes total number of prizes
    /// @param _tokenAddress token address of rewarded
    /// @param _decimals number of decimals for token
    /// @param _NumOfTokensReward Num of tokens for reward

    function addTokenPool(
        uint256 _totalPrizes,
        address _tokenAddress,
        uint256 _decimals,
        uint256 _NumOfTokensReward
    ) external onlyOwner {
        require(
            _totalPrizes > 0,
            "MysteryBox: number of total prizes should be greater then zero"
        );
        require(
            _NumOfTokensReward > 0,
            "MysteryBox: number of tokens reward should be greater then zero"
        );
        IERC20(_tokenAddress).safeTransferFrom(
            msg.sender,
            address(this),
            _totalPrizes * _NumOfTokensReward * (10 ** _decimals)
        );
        PrizePoolInfo memory pool = PrizePoolInfo({
            prizeType: 3,
            totalPrizes: _totalPrizes,
            rewarded: 0
        });

        TokensInfo memory TokenInfo = TokensInfo({
            TokenAddress: _tokenAddress,
            decimals: _decimals,
            NumOfTokensReward: _NumOfTokensReward
        });

        poolIdtoTokensInfo[prizePools.length] = TokenInfo;

        prizePools.push(pool);
    }

    /// @dev add text code prize inside pool
    /// @param _totalPrizes total number of prizes for reward

    function addTextCode(uint256 _totalPrizes) external onlyOwner {
        require(
            _totalPrizes > 0,
            "MysteryBox: Number total prizes should be greater then zero"
        );
        PrizePoolInfo memory pool = PrizePoolInfo({
            prizeType: 1,
            totalPrizes: _totalPrizes,
            rewarded: 0
        });

        prizePools.push(pool);
    }


    /// @dev Set the base URI
    /// @param baseURI_ Base path to metadata

    function setBaseURI(string memory baseURI_) public onlyOwner {
        _baseTokenURI = baseURI_;
    }

    /// @dev Get current base uri

    function _baseURI() internal view override returns (string memory) {
        return _baseTokenURI;
    }

    /**
     * @dev Returns the starting token ID. override of ERC721A function
     * To change the starting token ID, please override this function.
     */
    function _startTokenId() internal view virtual override returns (uint256) {
        return 1;
    }

    function totalPrizes() external view returns (uint256) {
        uint256 length = prizePools.length;
        uint256 total = 0;
        for (uint256 pid = 0; pid < length; ++pid) {
            PrizePoolInfo memory pool = prizePools[pid];
            total = total + pool.totalPrizes;
        }
        return total;
    }

    function claimRewards() external nonReentrant {
        uint256[] memory tokenIds = this.tokensOfOwner(msg.sender);
        for (uint256 i = 0; i < tokenIds.length; i++) {
            MysterBoxInfo storage box = tokenIdToBoxInfo[tokenIds[i]];
            if (box.user == msg.sender && !box.claimed) {
                if (box.prizeType == 3) {
                    TokensInfo memory token = poolIdtoTokensInfo[box.poolId];
                    uint256 amount = token.NumOfTokensReward *
                        10 ** token.decimals;
                    IERC20(token.TokenAddress).safeTransfer(msg.sender, amount);
                }
                if (box.prizeType == 2) {
                    NFTsTokensInfo memory nft = poolIdtoNftsTokenInfo[
                        box.poolId
                    ];
                    IERC721(nft.NFTAddress).safeTransferFrom(
                        nft.NFTOwner,
                        msg.sender,
                        nftAddressToTokenIds[nft.NFTAddress][
                            nftAddressToTokenIds[nft.NFTAddress].length - 1
                        ]
                    );
                    nftAddressToTokenIds[nft.NFTAddress].pop();
                }
                box.claimed = true;
            }
        }
    }

    function mintBox() external nonReentrant whenAllowed {
        require(totalSupply() <= this.totalPrizes(), "Max limit exceeds");
        while (1 == 1) {
            uint256 tries = 0;
            uint256 index = random(prizePools.length, tries);
            emit randomIndex(index);
            PrizePoolInfo storage pool = prizePools[index];
            if ((pool.totalPrizes - pool.rewarded) > 0) {
                pool.rewarded = pool.rewarded + 1;
                MysterBoxInfo memory boxInfo = MysterBoxInfo({
                    prizeType: pool.prizeType,
                    claimed: false,
                    user: msg.sender,
                    tokenId: _nextTokenId(),
                    poolId: index
                });
                nftBoxes.push(boxInfo);
                tokenIdToBoxInfo[_nextTokenId()] = boxInfo;
                _mint(msg.sender, 1);
                break;
            }
            if (tries > 50) {
                break;
            }
            tries++;
        }
    }

    /// @dev random number generator
    /// @param number max number of length
    /// @param counter a number as seed

    function random(
        uint256 number,
        uint256 counter
    ) public view returns (uint256) {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        block.timestamp,
                        block.difficulty,
                        msg.sender,
                        counter
                    )
                )
            ) % number;
    }

    /// @dev Withdraw all tokens in emergency
    /// @param _token token address to withdraw

    function withdrawAllRewardTokens(address _token) external onlyOwner {
        uint256 amount = IERC20(_token).balanceOf(address(this));
        IERC20(_token).safeTransfer(msg.sender, amount);
    }

    ///@dev set text code as claimed only by owner
    /// @param _tokenId tokens id to set
    function setTextCodeStatus(uint256[] calldata _tokenId) external onlyOwner {
        for (uint256 i = 0; i < _tokenId.length; ++i) {
            MysterBoxInfo storage box = tokenIdToBoxInfo[_tokenId[i]];
            if (box.prizeType == 1) {
                box.claimed = true;
            }
        }
    }

    /// @dev change prize pool settings
    /// @param _poolid id of pool to set
    /// @param _totalPrizes total number of prizes
    /// @param _rewarded total number of rewarded

    function setPool(
        uint256 _poolid,
        uint256 _totalPrizes,
        uint256 _rewarded
    ) external onlyOwner {
        PrizePoolInfo storage pool = prizePools[_poolid];
        pool.rewarded = _rewarded;
        pool.totalPrizes = _totalPrizes;
    }

    function prizePoolsLength() external view returns (uint256) {
        return prizePools.length;
    }

    /// @dev all token ids of nft contract inside prize pool
    /// @param _nftAddress token address to withdraw
    function tokenidsOfNft(
        address _nftAddress
    ) public view returns (uint256[] memory) {
        return nftAddressToTokenIds[_nftAddress];
    }

    /**
     * @dev setting allowed addresses
     * @param allowed boolean True if enables, False otherwise
     */

    function setAllowed(
        address[] calldata addresses,
        bool allowed
    ) external onlyOwner {
        for (uint256 i = 0; i < addresses.length; ++i) {
            isAllowed[addresses[i]] = allowed;
        }
    }
}

