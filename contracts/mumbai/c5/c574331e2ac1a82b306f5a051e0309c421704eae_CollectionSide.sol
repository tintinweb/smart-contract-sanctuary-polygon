pragma solidity ^0.8.19;






contract CollectionSide is ERC721, Ownable, ReentrancyGuard {

    struct Result {
        string FusedType;
        uint FusedId;
    }

    uint public MAX_LEGENDARY_COMPANIONS = 5;
    uint public MAX_ELEMENTS = 1440;
    uint public mintPricePack1 = 0.001 ether;
    uint public mintPricePack2 = 0.002 ether;
    uint public mintPricePack3 = 0.003 ether;
    uint public mintPricePack4 = 0.004 ether;

    address payable public withdrawalAddress;
    string private  baseTokenUri;

    // Compteurs pour chaque type et sous-type de token
    mapping(uint => uint) public counters;
    mapping(uint => uint) public itemCounters;
    mapping(uint => uint) public companionCounters;
    
    // Les règles de fusion
    mapping(bytes32 => Result) public elementFusionRules;
    mapping(bytes32 => Result) public itemFusionRules;
    mapping(bytes32 => Result) public companionFusionRules;

    constructor(address payable _withdrawalAddress) ERC721("MyNFT", "MNFT") Ownable(msg.sender) {
    withdrawalAddress = _withdrawalAddress;
    initializeCounters();
    initializeElementFusionRules();
}


    modifier callerIsUser() {
        require(tx.origin == msg.sender, "CryptoProject :: Cannot be called by a contract");
        _;
    }

function mintElements(uint packNumber) public payable {
    uint numberOfTokens;
    if (packNumber == 1) {
        require(msg.value >= mintPricePack1, "Not enough Ether to mint!");
        numberOfTokens = 10;
    } else if (packNumber == 2) {
        require(msg.value >= mintPricePack2, "Not enough Ether to mint!");
        numberOfTokens = 22;
    } else if (packNumber == 3) {
        require(msg.value >= mintPricePack3, "Not enough Ether to mint!");
        numberOfTokens = 36;
    } else if (packNumber == 4) {
        require(msg.value >= mintPricePack4, "Not enough Ether to mint!");
        numberOfTokens = 50;
    } else {
        revert("Invalid packNumber");
    }

    for (uint i = 0; i < numberOfTokens; i++) {
        uint tokenId = counters[0];
        _mint(msg.sender, tokenId);
        counters[0]++;
    }
}



// Minting a new NFT
function mintItem(string memory fusedType, uint id) public onlyOwner {
    uint tokenId;
     if(keccak256(abi.encodePacked((fusedType))) == keccak256(abi.encodePacked(("item")))) {
        tokenId = itemCounters[id];
        itemCounters[id]++;
    } else if(keccak256(abi.encodePacked((fusedType))) == keccak256(abi.encodePacked(("companion")))) {
        tokenId = companionCounters[id];
        companionCounters[id]++;
    } else {
        revert("Invalid type");
    }
    _mint(msg.sender, tokenId);
}




function airdrop(address to, uint numberOfTokens) public onlyOwner {
    for (uint i = 0; i < numberOfTokens; i++) {
        uint tokenId = counters[0];
        _mint(to, tokenId);
        counters[0]++;
    }
}

    // Fusion de deux NFTs
    function fuse(uint tokenId1, uint identifiant1, string memory tokenType1, uint tokenId2, uint identifiant2, string memory tokenType2) public onlyOwner {
        require(ownerOf(tokenId1) == msg.sender && ownerOf(tokenId2) == msg.sender, "Not token owner");

        bytes32 id;
        if (keccak256(abi.encodePacked(identifiant1)) < keccak256(abi.encodePacked(identifiant2))) {
            id = keccak256(abi.encodePacked(tokenType1, uint(identifiant1), tokenType2, uint(identifiant2)));
        } else {
            id = keccak256(abi.encodePacked(tokenType2, uint(identifiant2), tokenType1, uint(identifiant1)));
        }

        if (keccak256(abi.encodePacked((tokenType1))) == keccak256(abi.encodePacked(("element"))) 
        && keccak256(abi.encodePacked((tokenType2))) == keccak256(abi.encodePacked(("element")))) {
            Result memory result = elementFusionRules[id];
            require(bytes(result.FusedType).length > 0, "Invalid fusion pair (fusion rule issue with elements");
            mintItem(result.FusedType, result.FusedId);
        } else if ((keccak256(abi.encodePacked((tokenType1))) == keccak256(abi.encodePacked(("item"))) 
        && keccak256(abi.encodePacked((tokenType2))) == keccak256(abi.encodePacked(("element")))) 
        || (keccak256(abi.encodePacked((tokenType1))) == keccak256(abi.encodePacked(("element"))) 
        && keccak256(abi.encodePacked((tokenType2))) == keccak256(abi.encodePacked(("item"))))) {
        Result memory result1 = itemFusionRules[id];
        Result memory result2 = companionFusionRules[id];
        if(bytes(result1.FusedType).length > 0) {
          mintItem(result1.FusedType, result1.FusedId);
            if (counters[result1.FusedId] != 0) {}
        }
        else if (bytes(result2.FusedType).length > 0) {
           mintItem(result2.FusedType, result2.FusedId);
        }
        else {
            revert("Invalid fusion pair (fusion rule issue with items/companions)");
        }
    }
    else {
        revert("Invalid fusion pair (Type issue)");
    }

        // Destruction des tokens originaux
        _burn(tokenId1);
        _burn(tokenId2);
    }


    function rand()
    public
    view
    returns(uint256)
{
    uint256 seed = uint256(keccak256(abi.encodePacked(
        block.timestamp + block.prevrandao +
        ((uint256(keccak256(abi.encodePacked(block.coinbase)))) / (block.timestamp)) +
        block.gaslimit + 
        ((uint256(keccak256(abi.encodePacked(msg.sender)))) / (block.timestamp)) +
        block.number
    )));

    return ((seed - ((seed / 18) * 18))+1);
}

function withdraw() external {
    uint256 balance = address(this).balance;
    require(balance > 0, "No funds to withdraw");
    address payable to = payable(withdrawalAddress);
    to.transfer(balance);
    }

    function setTokenUri(string memory _baseTokenUri) external onlyOwner{
        baseTokenUri = _baseTokenUri;
    }

function debug( uint identifiant1, string memory tokenType1, uint identifiant2, string memory tokenType2) view  external returns (Result memory){
            bytes32 id;
            id = keccak256(abi.encodePacked(tokenType1, uint(identifiant1), tokenType2, uint(identifiant2)));
             Result memory result = elementFusionRules[id];
           return result;

}

function debug2( uint identifiant1, string memory tokenType1, uint identifiant2, string memory tokenType2) view  external returns (uint){
            bytes32 id;
            id = keccak256(abi.encodePacked(tokenType1, uint(identifiant1), tokenType2, uint(identifiant2)));
             Result memory result = elementFusionRules[id];
           uint length = bytes(result.FusedType).length;
    return length;

}


   
function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
    require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

    return bytes(baseTokenUri).length > 0 
        ? string(abi.encodePacked(baseTokenUri, Strings.toString(tokenId), ".json")) 
        : "";
}



