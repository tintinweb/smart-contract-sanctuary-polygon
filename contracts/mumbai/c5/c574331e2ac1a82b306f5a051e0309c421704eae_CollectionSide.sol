    }

 // Ajout d'une nouvelle règle de fusion
    function addFusionRule(string memory type1, uint id1, string memory type2, uint id2, string memory resultingType, uint resultingId) public onlyOwner {
        bytes32 id;
        if (keccak256(abi.encodePacked(type1, id1)) < keccak256(abi.encodePacked(type2, id2))) {
            id = keccak256(abi.encodePacked(type1, id1, type2, id2));
        } else {
            id = keccak256(abi.encodePacked(type2, id2, type1, id1));
        }

        Result memory result = Result(resultingType, resultingId);
        if (keccak256(abi.encodePacked(resultingType)) == keccak256(abi.encodePacked("item"))) {
            itemFusionRules[id] = result;
        } else if (keccak256(abi.encodePacked(resultingType)) == keccak256(abi.encodePacked("companion"))) {
            companionFusionRules[id] = result;
        } else {
            revert("Invalid type");
        }
    }


