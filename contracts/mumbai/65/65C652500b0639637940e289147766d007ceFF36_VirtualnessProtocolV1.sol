        require(sell.order.side == Side.SELL, "Sell side order is not available");
        require(buy.order.side == Side.BUY, "Buy side order is not available");
        require(sell.order.trader != address(0), "Sell order trader is not tx sender");
        require(buy.order.trader != address(0), "Buy order trader is not tx sender");

        bytes32 sellHash = _hashOrder(sell.order, nonces[sell.order.trader]);

        //Check start and expiry time of sell order 
        validateOrderTime(sell.order);

        //Validates the order signature and important attributes
        _validateOrder(sell, sellHash, uint96(buy.order.collection.amount));
        
        fulfilled = matchAndTransfer(sell.order, buy.order);

        orderFilledStatus[sellHash] = orderFilledStatus[sellHash] + uint96(buy.order.collection.amount);
        if(orderFilledStatus[sellHash] == sell.order.collection.amount) {
            cancelledOrFilled[sellHash] = true;
        }

        emit OrdersMatched(
            sell.order.trader,
            buy.order.trader,
            sell.order,
            buy.order
        );
        
        return fulfilled;
    }


    /**
     * @dev fulfill the lazy minting order
     *
     * Requirements:
     *
     * @param sell should be passed - Sell order created by NFT creator
     * @param buy should be passed - Buy order created by buyer
     */
    function fulfillLazyMintOrder(Order calldata sell, Order calldata buy, MintData memory mintData) 
        external 
        payable
        returns (bool fulfilled)
    {
        require(sell.order.side == Side.SELL, "Sell side order is not available");
        require(buy.order.side == Side.BUY, "Buy side order is not available");
        require(sell.order.trader != address(0), "Sell order trader is not tx sender");
        require(buy.order.trader != address(0), "Buy order trader is not tx sender");

        bytes32 sellHash = _hashOrder(sell.order, nonces[sell.order.trader]);

        //Check start and expiry time of sell order 
        validateOrderTime(sell.order);

        //Validates the order signature and important attributes
        _validateOrder(sell, sellHash, uint96(buy.order.collection.amount));
        
        fulfilled = matchAndLazyMint(sell.order, buy.order, mintData);

        orderFilledStatus[sellHash] = orderFilledStatus[sellHash] + uint96(buy.order.collection.amount);
        if(orderFilledStatus[sellHash] == sell.order.collection.amount) {
            cancelledOrFilled[sellHash] = true;
        }

        emit OrdersMatched(
            sell.order.trader,
            buy.order.trader,
            sell.order,
            buy.order
        );
        
        return fulfilled;
    }


    /**
     * @dev fulfill the airdrop
     *
     * Requirements:
     *
     * @param sell should be passed - Sell order created by buyer
     * @param buy should be passed - Buy order created by NFT creator
     */
    function fulfillDrop(Order calldata sell, Order calldata buy) 
        external
        returns (bool fulfilled)
    {

        require(sell.order.side == Side.SELL, "Sell side order is not available");
        require(buy.order.side == Side.BUY, "Buy side order is not available");
        require(sell.order.trader != address(0), "Sell order trader is not tx sender");
        require(buy.order.trader != address(0), "Buy order trader is not tx sender");

        bytes32 sellHash = _hashOrder(sell.order, nonces[sell.order.trader]);

        //Check start and expiry time of sell order 
        validateOrderTime(sell.order);

        //Validates the order signature and important attributes
        _validateOrder(sell, sellHash, uint96(buy.order.collection.amount));
        
        require ( (sell.order.side != buy.order.side) &&
            (sell.order.collection.collection == buy.order.collection.collection) &&
            (sell.order.collection.tokenId == buy.order.collection.tokenId) &&
            (sell.order.collection.assetType == buy.order.collection.assetType), "Order parameters cannot be matched");

        
        tokenTransfer(sell.order.collection.collection,sell.order.trader,buy.order.recipient,sell.order.collection.tokenId,buy.order.collection.amount,
            sell.order.collection.assetType);
        
        orderFilledStatus[sellHash] = orderFilledStatus[sellHash] + uint96(buy.order.collection.amount);
        if(orderFilledStatus[sellHash] == sell.order.collection.amount) {
            cancelledOrFilled[sellHash] = true;
        }

        emit OrdersMatched(
            sell.order.trader,
            buy.order.trader,
            sell.order,
            buy.order
        );
        
        return fulfilled;
    }

    
    /**
     * @dev Validate the order
     *
     * Requirements:
     *
     * @param sell should be passed - Sell order created by buyer
     * 
     * 
     * Validation:
     * 
     *  - Validates the order start and expiry time 
     *  - Validates buy and sell order parameters
     *  - Validates buy and sell order signature
     *  - Validates buy order hash status using cancelledOrFilled flag
     */
    function _validateOrder(Order calldata sell, bytes32 sellHash, uint96 buyOrderAmount) 
        internal 
        view 
    {
    
        require(cancelledOrFilled[sellHash] == false, "Sell order has been cancelled/Fulfilled");

        require((orderFilledStatus[sellHash] + uint96(buyOrderAmount)) <= sell.order.collection.amount, "Sell order limit crossed");

        require(sell.order.fees.length != 0, "Platform fee is not mentioned");

        require(sell.order.collection.assetType == AssetType.ERC721 || sell.order.collection.assetType == AssetType.ERC1155, "AssetType is invalid");

        require(validateSignature(sell, sellHash) == sell.order.trader, "Sell order signature is invalid");
    }


    /**
     * @dev Match the order and perform ERC20 and ERC721/ERC1155 transfer
     *
     * Requirements:
     *
     * @param sell should be passed - Sell order created by buyer
     * @param buy should be passed - Buy order created by NFT creator
     * 
     */
    function matchAndTransfer(OrderParameters calldata sell, OrderParameters calldata buy)  
        internal 
        returns(bool) 
    {
        bool canMatch; 
        uint256 price; 
        uint256 tokenId; 
        uint256 amount;
        uint256 totalPrice;

        if (sell.collection.assetType == AssetType.ERC721) {
            (canMatch, price, tokenId, amount) = matchOrderERC721(sell, buy);
        } else if (sell.collection.assetType == AssetType.ERC1155){
            (canMatch, price, tokenId, amount) = matchOrderERC1155(sell, buy);
        } 
        
        require(canMatch, "Order parameters cannot be matched");

        unchecked {
            totalPrice = price * amount;
        }

        transferFunds(sell.trader, msg.sender, sell.collection.paymentToken, sell.fees, totalPrice, sell.collection.collection, tokenId);

        tokenTransfer(sell.collection.collection, sell.trader, buy.recipient, tokenId, amount, sell.collection.assetType);

        return true;
    }


    /**
     * @dev Match the order and perform ERC20 and ERC721/ERC1155 transfer
     *
     * Requirements:
     *
     * @param sell should be passed - Sell order created by buyer
     * @param buy should be passed - Buy order created by NFT creator
     * 
     */
    function matchAndLazyMint(OrderParameters calldata sell, OrderParameters calldata buy, MintData memory mintData)  
        internal 
        returns(bool) 
    {
        bool canMatch; 
        uint256 price; 
        uint256 tokenId; 
        uint256 amount;
        uint256 totalPrice;

        if (sell.collection.assetType == AssetType.ERC721) {
            (canMatch, price, tokenId, amount) = matchOrderERC721(sell, buy);
        } else if (sell.collection.assetType == AssetType.ERC1155){
            (canMatch, price, tokenId, amount) = matchOrderERC1155(sell, buy);
        } 
        
        require(canMatch, "Order parameters cannot be matched");

        unchecked {
            totalPrice = price * amount;
        }

        transferFunds(sell.trader, msg.sender, sell.collection.paymentToken, sell.fees, totalPrice, sell.collection.collection, tokenId);

        tokenLazyMint(sell.collection.collection, buy.recipient, amount, sell.collection.assetType, mintData);

        return true;
    }

    /**
     * @dev Cancel an order, preventing it from being matched. Must be called by the trader of the order
     * @param order Order to cancel
     */
    function cancelOrder(OrderParameters calldata order) public {
        /* Assert sender is authorized to cancel order. */
        require(msg.sender == order.trader);

        bytes32 hash = _hashOrder(order, nonces[order.trader]);

        if (!cancelledOrFilled[hash]) {
            /* Mark order as cancelled, preventing it from being matched. */
            cancelledOrFilled[hash] = true;
            emit OrderCancelled(hash);
        }
    }

    /**
     * @dev Cancel multiple orders
     * @param orders Orders to cancel
     */
    function cancelOrders(OrderParameters[] calldata orders) external {
        for (uint8 i = 0; i < orders.length; i++) {
            cancelOrder(orders[i]);
        }
    }

    /**
     * @dev Cancel all current orders for a user, preventing them from being matched. 
     * Must be called by the trader of the order
     */
    function incrementCounter() external {
        nonces[msg.sender] += 1;
        emit CounterIncremented(msg.sender, nonces[msg.sender]);
    }

    uint256[50] private __gap;
}

