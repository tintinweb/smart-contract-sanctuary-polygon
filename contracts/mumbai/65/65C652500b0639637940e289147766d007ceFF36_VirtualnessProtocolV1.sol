pragma solidity ^0.8.17;
pragma abicoder v2;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC2981Upgradeable.sol";
import "./lib/OrderStructs.sol";
import "./lib/ExecutionDelegate.sol";


contract TransferExecutor is Initializable, OwnableUpgradeable, ExecutionDelegate {

    event ApprovePaymentToken(address indexed user);
    event RemovePaymentToken(address indexed user);

    uint256 public constant BASIS_POINT = 10000;
    mapping(address => bool) public paymentToken_;
    //mapping(address => bool) public platformTreasury;

    /**
     * @dev Approve/Whitelist payment token for making nft transfers
     * @param tokenAddress address of token to be approved as paymentToken
     */
    function approvePaymentToken(address tokenAddress) external onlyOwner() {
        paymentToken_[tokenAddress] = true;
        emit ApprovePaymentToken(tokenAddress);
    }

    /**
     * @dev Remove approval of payment token to execute NFT transfer
     * @param tokenAddress address of token to be removed from paymentToken list
     */
    function removePaymentToken(address tokenAddress) external onlyOwner() {
        paymentToken_[tokenAddress] = false;
        emit RemovePaymentToken(tokenAddress);
    }

    /**
     * @dev Execute all ERC20 token / ETH transfers associated with an order match (fees and buyer => seller transfer)
     * @param seller seller
     * @param buyer buyer
     * @param paymentToken payment token
     * @param fees fees
     * @param price price
     */
    function transferFunds( 
        address seller,
        address buyer,
        address paymentToken,
        Fee[] calldata fees,
        uint256 price,
        address collection,
        uint256 tokenId
    ) 
        internal 
    {
        if (paymentToken == address(0)) {
            require(msg.value == price, "Order price value is less");
        }

        /* Take fee. */
        uint256 receiveAmount = _transferFees(fees, paymentToken, buyer, price);

        /* Take Royalty*/

        (address receiver, uint256 royaltyAmount) = IERC2981Upgradeable(collection).royaltyInfo(tokenId, price);
           
        if(receiver != address(0)) {
            _transferTo(paymentToken, buyer, receiver, royaltyAmount);
           receiveAmount = receiveAmount - royaltyAmount;
        }

        /* Transfer remainder to seller. */
        _transferTo(paymentToken, buyer, seller, receiveAmount);
    }

    /**
     * @dev Charge a fee in ETH or WETH
     * @param fees fees to distribute
     * @param paymentToken address of token to pay in
     * @param from address to charge fees
     * @param price price of token
     */
    function _transferFees(
        Fee[] calldata fees,
        address paymentToken,
        address from,
        uint256 price
    ) internal returns (uint256) {

        uint256 totalFee = 0;
        for (uint8 i = 0; i < fees.length; i++) {
            if(fees[i].feeType != FeeType.ROYALTY) {
                uint256 fee = (price * fees[i].rate) / BASIS_POINT;
                _transferTo(paymentToken, from, fees[i].recipient, fee);
                totalFee += fee;
            }
        }

        require(totalFee <= price, "Total amount of fees are more than the price");
         /* Amount that will be received by seller. */
        uint256 receiveAmount = price - totalFee;

        return (receiveAmount);
    }

    /**
     * @dev Transfer amount in ETH or WETH
     * @param paymentToken address of token to pay in
     * @param from token sender
     * @param to token recipient
     * @param amount amount to transfer
     */
    function _transferTo(
        address paymentToken,
        address from,
        address to,
        uint256 amount
    ) internal {
        if (amount == 0) {
            return;
        }

        if (paymentToken == address(0)) {
            /* Transfer funds in ETH. */
            payable(to).transfer(amount);
        } else if (paymentToken_[paymentToken]) {
            /* Transfer funds. */
            transferERC20(paymentToken, from, to, amount);
        } else {
            revert("Invalid payment token");
        }
    }

    /**
     * @dev Execute call through delegate proxy
     * @param collection collection contract address
     * @param from seller address
     * @param to buyer address
     * @param tokenId tokenId
     * @param assetType asset type of the token
     */
    function tokenTransfer(
        address collection,
        address from,
        address to,
        uint256 tokenId,
        uint256 amount,
        AssetType assetType
    ) internal {
        /* Assert collection exists. */
        require(_exists(collection), "Collection does not exist");

        /* Call execution delegate. */
        if (assetType == AssetType.ERC721) {
            transferERC721(collection, from, to, tokenId);
        } else if (assetType == AssetType.ERC1155) {
            transferERC1155(collection, from, to, tokenId, amount);
        } else {
             revert("Unsupported asset type");
        }

    }

    /**
     * @dev Execute call through delegate proxy
     * @param collection collection contract address
     * @param to buyer address
     * @param amount total no of token
     * @param assetType asset type of the token
     * @param mintData Lazy Mint to be executed at collection contract
     */
    function tokenLazyMint(
        address collection,
        address to,
        uint256 amount,
        AssetType assetType,
        MintData memory mintData
    ) internal {
        /* Assert collection exists. */
        require(_exists(collection), "Collection does not exist");

        /* Call execution delegate. */
        if (assetType == AssetType.ERC721) {
            LibERC721LazyMint.Mint721Data memory data = LibERC721LazyMint.Mint721Data({
               tokenId: mintData.tokenId,
               tokenURI: mintData.tokenURI,
               creators: mintData.creators,
               royalties: mintData.royalties,
               signatures: mintData.signatures
            });
            mintERC721(collection, to, data);
        } else if (assetType == AssetType.ERC1155) {
            LibERC1155LazyMint.Mint1155Data memory data = LibERC1155LazyMint.Mint1155Data({
               tokenId: mintData.tokenId,
               tokenURI: mintData.tokenURI,
               supply: mintData.supply,
               creators: mintData.creators,
               royalties: mintData.royalties,
               signatures: mintData.signatures
            });
            mintERC1155(collection, to, data, amount);
        } else {
            revert("Unsupported asset type");
        }

    }

    /**
     * @dev Determine if the given address exists
     * @param what address to check
     */
    function _exists(address what)
        internal
        view
        returns (bool)
    {
        uint size;
        assembly {
            size := extcodesize(what)
        }
        return size > 0;
    }

    uint256[50] private __gap;
}

