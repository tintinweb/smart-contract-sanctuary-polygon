    function createAmp(
    string memory _name,
    uint256 _price,
    uint256 _amount,
    string memory _graphUri
) public payable virtual  returns (uint256) {

    _setDefaultRoyalty(owner, 250); 
    _tokenIds.increment();
    //id increment 
    require(totalAmp <= 999, "Over 1000 AMP minted");
    require (msg.value == _amount * lillupPrice, "Please Enter Correct Price");
    uint256 newTokenId = _tokenIds.current();
    
    idToAmp[newTokenId].id = _tokenIds.current();
    idToAmp[newTokenId].creator = payable(msg.sender);
    idToAmp[newTokenId].name = _name;
    idToAmp[newTokenId].price = _price;
    idToAmp[newTokenId].amount = _amount;
    idToAmp[newTokenId].graphUri = _graphUri;
    idToAmp[newTokenId].valid = true;
    
    _mint(msg.sender, newTokenId, _amount, "");


    totalAmp += _amount;

    return newTokenId;       
}
    
    function sendAmp(
        address _to,
        uint256 _id,
        uint256 _amount
    ) public onlyCreators nonReentrant {

        idToPay[_id] = Payment(
            idToPay[_id].id = nextPaymentId,
            _to,
            idToPay[_id]._state = PaymentStatus.PENDINGPAYMENT
            
        );
        nextPaymentId++;


        
        //require(msg.value == listingPrice, "Price must be equal to listing price");
        idToAmp[_id] = Amp(
