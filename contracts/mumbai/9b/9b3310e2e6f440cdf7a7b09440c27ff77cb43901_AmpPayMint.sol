            idToAmp[_id].price,
            _amount,
            payable(msg.sender),
            idToAmp[_id].valid,
            idToAmp[_id].name,
            idToAmp[_id].graphUri

        );
        
        


        //set enum status to pending payment
        
        _safeTransferFrom(_msgSender(), _to, _id, _amount, "");

    }

