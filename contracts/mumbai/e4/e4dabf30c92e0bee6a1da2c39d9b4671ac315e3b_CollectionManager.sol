pragma solidity 0.8.13;

import "@openzeppelin/contracts/access/Ownable.sol";
import "ERC721A/contracts/IERC721A.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "./MemeAsset.sol";
import "./IValidator.sol";

/**
 *   @param salt a random number used to ensure ticket uniqueness
 *   @param name the token name
 *   @param symbol the token symbol
 *   @param executor the address who will be the owner of newly deployed collection
 */
struct DeployOrder {
    uint256 salt;
    string name;
    string symbol;
    address executor;
}

/**
 *   @param salt a random number used to ensure ticket uniqueness
 *   @param collectionId ID of the collection within which the tokens will be minted
 *   @param quantity the amount of tokens to mint. Should be the same as the `quantity` variable passed to this method.
 *   @param executor the address who will be the owner of newly minted tokens
 */
struct MintOrder {
    uint256 salt;
    uint256 collectionId;
    uint256 quantity;
    address executor;
}

/**
 * @param salt a random number used to ensure ticket uniqueness
 * @param collectionIdFrom ID of a collection that the token should be transferred from
 * @param collectionIdTo ID of a collection that the token should be transferred to
 * @param assetId ID of a token within collectionIdFrom that will be transferred
 */
struct TransferOrder {
    uint256 salt;
    uint256 collectionIdFrom;
    uint256 collectionIdTo;
    uint256 assetId;
}

/**
 * @title MemeAsset
 *
 * @dev
 *  A contract for managing multiple instances of MemeAsset contract. Enables deployment of new collections, minting tokens in existing collections and transferring tokens between existing collections.
 *  Contains a list of all deployed collections.
 *  Deploy, mint and transfer functions may be called only providing a cryptographic ticket signed by the managing authority.
 *
 */
contract CollectionManager is Ownable {
    using Strings for address;

    event CollectionDeployed(address indexed creator, address indexed collectionAddress, uint256 indexed collectionId, bytes32 orderHash);
    event AssetMinted(
        uint256 indexed collectionId,
        address indexed collectionAddress,
        address indexed minter,
        uint256 tokenIdRangeStart,
        uint256 quantity,
        bytes32 orderHash 
    );
    event AssetTransferred(
        uint256 indexed assetIdBefore,
        uint256 assetIdAfter,
        address indexed assetAddressFrom,
        address indexed assetAddressTo,
        bytes32 orderHash
    );

    address public collectionImplementation;
    IValidator public validator;

    /**
     *   @notice main mapping used for interacting with deployed collections
     */
    mapping(uint256 => address) public collections;

    mapping(bytes32 => bool) public ticketFulfilled;
    uint256 public collectionCounter;
    string public baseURI;

    /**
     * @param implementation a deployed instance of MemeAsset contract, functions of which will be used as proxy targets.
     */
    constructor(address implementation) {
        collectionImplementation = implementation;
    }

    /**
     * @notice
     *   A function used to deploy new MemeAsset collections, as described by DeployOrder
     *   @param order data describing the deployed collection and action executor
     *   @param signature represents a ticket and should contain an ECDSA signature. The signed message should be a keccak hash of serialized `{salt, msg.sender (address), name, symbol}`, as above.
     *   @return id ID of deployed collection, by which it can be uniquely identified later
     */
    function deploy(DeployOrder calldata order, bytes calldata signature) external returns (uint256) {
        _validateDeployment(order, signature);
        address newCollection = Clones.clone(collectionImplementation);
        MemeAsset(newCollection).initialize(
            order.executor, order.name, order.symbol, string.concat(baseURI, newCollection.toHexString(), "/")
        );
        collections[collectionCounter] = newCollection;
        emit CollectionDeployed(order.executor, collections[collectionCounter], collectionCounter, _deployOrderToHash(order));
        return collectionCounter++;
    }

    /**
     * @notice
     *   A function used to mint tokens within the existing collection
     *    @param order structure describing the minted tokens and action executor
     *    @param signature represents a ticket and should contain an ECDSA signature. The signed message should be a hash of serialized `{salt, msg.sender (address), collectionId, quantity}`, as above.
     */
    function mint(MintOrder calldata order, bytes calldata signature) external {
        _validateMint(order, signature);
        uint256 rangeStart = MemeAsset(collections[order.collectionId]).totalMinted();
        MemeAsset(collections[order.collectionId]).mint(order.executor, order.quantity);
        emit AssetMinted(
            order.collectionId, collections[order.collectionId], order.executor, rangeStart, order.quantity, _mintOrderToHash(order)
            );
    }

    /**
     * @notice
     * A function used to transfer a token between existing collections. Effectively burns a token from one collection and mints it in the other one
     * In case this method is called by platform owner (owner()), no signature is needed (it can be empty bytes). In any other case, it is mandatory.
     * NOTE: asset ID won't stay the same after transfer
     *   @param order data describing the transferred token
     *   @param signature represents a ticket and should contain an ECDSA signature. The signed message should be a hash of serialized `{salt, msg.sender (address), collectionIdFrom, collectionIdTo, assetId}`, as above.
     */
    function transferBetweenCollections(TransferOrder calldata order, bytes calldata signature) external {
        _validateTransfer(order, signature);
        address assetOwner = MemeAsset(collections[order.collectionIdFrom]).ownerOf(order.assetId);
        MemeAsset(collections[order.collectionIdFrom]).burn(order.assetId);
        MemeAsset(collections[order.collectionIdTo]).mint(assetOwner, 1);
        uint256 assetIdAfter = MemeAsset(collections[order.collectionIdTo]).totalMinted() - 1;
        emit AssetTransferred(
            order.assetId, assetIdAfter, collections[order.collectionIdFrom], collections[order.collectionIdTo], _transferOrderToHash(order)
            );
    }

    /**
     * @dev
     * Validates a mint ticket from `mint` function.
     */
    function _validateMint(MintOrder calldata order, bytes calldata signature) internal {
        require(order.collectionId <= collectionCounter, "CollectionManager: wrong collection ID");
        require(collections[order.collectionId] != address(0), "CollectionManager: no collection uses this ID");
        bytes32 hashed = _mintOrderToHash(order);
        require(_verifyUniqueness(hashed), "CollectionManager: ticket not unique");
        require(_verifySignature(hashed, signature), "CollectionManager: invalid signature");
        _markTicketAsUsed(hashed);
    }

    function _mintOrderToHash(MintOrder calldata order) internal pure returns (bytes32) {
        bytes memory message = abi.encode(order);
        return keccak256(message);
    }

    /**
     * @dev
     * Validates a deployment ticket from `deploy` function.
     */
    function _validateDeployment(DeployOrder calldata order, bytes calldata signature) internal {
        bytes32 hashed = _deployOrderToHash(order);
        require(_verifyUniqueness(hashed), "CollectionManager: ticket not unique");
        require(_verifySignature(hashed, signature), "CollectionManager: invalid signature");
        _markTicketAsUsed(hashed);
    }

    function _deployOrderToHash(DeployOrder calldata order) internal pure returns (bytes32) {
        bytes memory message = abi.encode(order);
        return keccak256(message);
    }

    /**
     * @dev
     * Validates a transfer ticket from `transferBetweenCollections` function. If `msg.sender` is the contract owner, any `signature` will pass.
     */
    function _validateTransfer(TransferOrder calldata order, bytes calldata signature) internal {
        require(
            collections[order.collectionIdFrom] != address(0), "CollectionManager: collectionIdFrom does not exist."
        );
        require(collections[order.collectionIdTo] != address(0), "CollectionManager: collectionIdTo does not exist.");
        if (validator.isValidationNeededForOrigin(msg.sender)) {
            bytes32 hashed = _transferOrderToHash(order);
            require(_verifyUniqueness(hashed), "CollectionManager: Ticket not unique");
            require(_verifySignature(hashed, signature), "CollectionManager: Invalid signature");
            _markTicketAsUsed(hashed);
        }
    }

    function _transferOrderToHash(TransferOrder calldata order) internal pure returns (bytes32) {
        bytes memory message = abi.encode(order);
        return keccak256(message);
    }

    /**
     * @dev
     * Delegates signature validation to external contract.
     */
    function _verifySignature(bytes32 hashed, bytes memory signature) internal returns (bool) {
        return validator.validate(hashed, signature);
    }

    /**
     * @dev
     * Keep track of used tickets to prevent replay attacks.
     */
    function _markTicketAsUsed(bytes32 hashed) internal {
        ticketFulfilled[hashed] = true;
    }

    function _verifyUniqueness(bytes32 hashed) internal view returns (bool) {
        return !ticketFulfilled[hashed];
    }

    /**
     * @notice
     * Set base URI that will be used in newly deployed contracts in the format of `{baseURI}/{collection address}/`.
     */
    function setBaseURI(string memory baseUri) public onlyOwner {
        baseURI = baseUri;
    }

    /**
     * @dev
     * Set external contract for ticket validation.
     */
    function setValidator(address validator_) external onlyOwner {
        validator = IValidator(validator_);
    }
}

pragma solidity 0.8.13;

interface IValidator {
    function validate(bytes32 hashed, bytes memory signature) external returns (bool);
    function isValidationNeededForOrigin(address origin) external returns (bool);
}

pragma solidity 0.8.13;

import "@openzeppelin/contracts/access/Ownable.sol";
import "ERC721A/contracts/ERC721A.sol";
import "@openzeppelin/contracts/proxy/utils/Initializable.sol";

/**
 * @title MemeAsset
 *
 * @dev A contract representing a thematically connected collection of meme assets. Implementation of ERC721A.
 * Optimized for lower gas during deploys and batch mints.
 *
 * Assumptions:
 *
 * - Minter of token ID 0 is interpreted as a collection creator. Current owner of token ID 0 is a collection owner.
 */

contract MemeAsset is Ownable, Initializable, ERC721A {
    mapping(uint256 => address) private _originalMintersPacked;
    string private _baseTokenURI = "";
    // Name and symbol moved to child class to allow initialization
    string private _name;
    string private _symbol;

    constructor() ERC721A("", "") {}

    function initialize(address creator_, string memory name_, string memory symbol_, string memory baseURI_)
        external
        initializer
    {
        _name = name_;
        _symbol = symbol_;
        _baseTokenURI = baseURI_;
        _transferOwnership(msg.sender);
        _setOriginalMinterForCurrentBatch(creator_);
        _mint(creator_, 1);
    }

    function mint(address minter, uint256 quantity) external onlyOwner {
        _setOriginalMinterForCurrentBatch(minter);
        _mint(minter, quantity);
    }

    /**
     * @notice
     *  Burns a token, callable only by owner.
     *  NOTE: Since the owner is a contract (CollectionManager), the only scenario
     *          under which a token may be burned is when someone calls a
     *          `transferBetweenCollections` method in the owner contract.
     */

    function burn(uint256 tokenId) external onlyOwner {
        _burn(tokenId, false);
    }

    /**
     * @return originalMinter (address): address which originally deployed the collection, i.e. the caller of `deploy` function in CollectionManager.
     */
    function getOriginalMinter(uint256 tokenId) public view returns (address) {
        address originalMinter = _originalMintersPacked[tokenId];
        if (originalMinter == address(0)) {
            while (tokenId > 0) {
                originalMinter = _originalMintersPacked[--tokenId];
                if (originalMinter == address(0)) continue;
                return originalMinter;
            }
        }
        return originalMinter;
    }

    function _setOriginalMinterForCurrentBatch(address minter) internal {
        _originalMintersPacked[_nextTokenId()] = minter;
    }

    function getCollectionOwner() external view returns (address) {
        return ownerOf(0);
    }

    function getCollectionCreator() external view returns (address) {
        return getOriginalMinter(0);
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    function setBaseURI(string memory baseURI) public onlyOwner {
        _baseTokenURI = baseURI;
    }

    /**
     * @dev Returns the token collection name.
     */
    function name() public view override returns (string memory) {
        return _name;
    }

    /**
     * @dev Returns the token collection symbol.
     */
    function symbol() public view override returns (string memory) {
        return _symbol;
    }

    function totalMinted() external view returns (uint256) {
        return _totalMinted();
    }
}