import "@openzeppelin-contracts/token/ERC721/ERC721.sol";
import "@openzeppelin-contracts/token/ERC20/IERC20.sol";
import "@openzeppelin-contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin-contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin-contracts/access/Ownable.sol";

import "./EmojiUtils/EmojiUtils.sol";
import "./UnicodeLib.sol";
import "./TraitCheckers/ITraitChecker.sol";

import "forge-std/console.sol";

import {IUsernameArtProxy} from "./IUsernameArtProxy.sol";

interface ITokenInfo {
    function decimals() external view returns(uint);
}


contract UsernameNFT is ERC721Enumerable, Ownable {
    using SafeERC20 for IERC20;
    
    uint256 private _tokenIdCounter;                
    uint256 constant COSTS_LENGTH = 8;

    string[] internal _forbiddenChars;

    address public artProxy;                                // proxy contract for upgr. metadata
    address public emojiUtils;                                // proxy contract for upgr. metadata

    mapping(address => bool) public allowedTokens;          // token used for payment
    mapping(address => uint[]) internal _costPerToken;      // the cost of a username given a payment token  
    mapping(address => bool) public admins;                 // NFT admins
    
    mapping(string => bool) private _usernameExists;        // username register flags
    mapping(string => bool) private _isForbidden;           // check if a char is forbidden (eg. string can't contain spaces " ")

    mapping(uint => string) public usernames;               // tokenId to username
    mapping(uint => uint) internal _date;                   // tokenId to creation date
    mapping(uint => uint) internal _length;                 // tokenId to length

    mapping(string => address) public traitToTraitChecker;
    mapping(uint => string[]) public traits;                // tokenId to traits  
    mapping(uint => string[]) public traitsCategories;                // tokenId to traits  

    bool public MAX_LENGTH_FLAG;
    uint public MAX_LENGTH;


    event TraitAdded(address indexed traitCheckerContract, string indexed traitName);
    event TraitEdited(string indexed traitName, address indexed oldTraitCheckerContract, address indexed newTraitCheckerContract);
    event TraitRemoved(string indexed raitName, address indexed oldTraitCheckerContract);

    event ArtProxyUpdated(address indexed oldArtProxy, address indexed newArtProxy);
    event EmojiUtilsUpdated(address indexed emojIUtils, address indexed newEmojiUtils);

    event ForbiddenCharAdded(string indexed s);
    event ForbiddenCharRemoved(string indexed s);

    event PaymentTokenAdded(address indexed token);
    event PaymentTokenRemoved(address indexed token);

    event TokensWithdrawn(address indexed token, address indexed recipient, uint256 indexed amount);
    event CostsSet(address indexed token, uint[] indexed costs);

    event MaxLengthSet(uint indexed newMaxLength);

    event MetadataUpdate(uint256 indexed tokenId);
    event UsernameMinted(uint256 indexed tokenId, string indexed username, address indexed minter);


    modifier onlyAdminOrOwner {
        require(msg.sender == owner() || admins[msg.sender]);
        _;
    }

    constructor(address _artProxy, address _emojiUtils) ERC721("UsernameNFT", "USERNAME") {
        if(_artProxy != address(0)) artProxy = _artProxy; 
        if(_emojiUtils != address(0)) emojiUtils = _emojiUtils;

        address _usdt = 0x55d398326f99059fF775485246999027B3197955;

        allowedTokens[_usdt] = true; //usdt
        uint[] memory _costs = new uint[](COSTS_LENGTH);
        _costs[0] = 10000;
        _costs[1] = 4000;
        _costs[2] = 1000;
        _costs[3] = 300;
        _costs[4] = 50;
        _costs[5] = 20;
        _costs[6] = 7;
        _costs[7] = 5;

        _setCosts(_usdt, _costs);

        string memory _forbid = " ";
        _isForbidden[_forbid] = true;
        _forbiddenChars.push(_forbid);

        MAX_LENGTH_FLAG = true;
        MAX_LENGTH = 32;
    }

   
    /*------------------*/
    /*      user        */
    /*------------------*/

    function mintUsername(string memory _username, address _paymentToken, string[] calldata _rawTraits, bytes32[][] calldata _proofs) public  {
        require(allowedTokens[_paymentToken], "UsernameNFT: Token not in allowlist");
        require(_validateUsername(_username), "UsernameNFT: username not valid!");
        uint l = _getLength(_username);
        
        require(!MAX_LENGTH_FLAG || l <= MAX_LENGTH, "UsernameNFT: username too long");
        uint256 cost = _getCost(l, _paymentToken);
        
        _usernameExists[_toLower(_username)] = true;
        usernames[_tokenIdCounter] = _toLower(_username);
        _date[_tokenIdCounter] = block.timestamp;
        _length[_tokenIdCounter] = l;

        _updateTraits(_username, _tokenIdCounter, _rawTraits, _proofs);

        _tokenIdCounter += 1;
        IERC20(_paymentToken).safeTransferFrom(msg.sender, address(this), cost);
        safeMint(_msgSender(), _tokenIdCounter - 1);
        emit UsernameMinted(_tokenIdCounter - 1, _username, msg.sender);
    }

    function _updateTraits(string memory _username, uint _tokenId, string[] memory _rawTraits, bytes32[][] calldata _proofs) internal {
        require(_rawTraits.length == _proofs.length, "UsernameNFT: rawTraits and proofs length mismatch");
        traits[_tokenIdCounter] = new string[](_rawTraits.length);
        traitsCategories[_tokenIdCounter] = new string[](_rawTraits.length);

        for(uint i = 0; i < _rawTraits.length; i++) {
            (string memory cat, string memory t) = _checkTrait(_username, _rawTraits[i], _proofs[i]);
            traits[_tokenIdCounter][i] = t;
            traitsCategories[_tokenIdCounter][i] = cat;
        }
    }

    function _checkTrait(
        string memory _username,
        string memory _rawTrait,
        bytes32[] memory _proof
    ) internal view returns (string memory, string memory) {
        require(traitToTraitChecker[_rawTrait] != address(0), "UsernameNFT: invalid raw trait");
        ITraitChecker checker = ITraitChecker(traitToTraitChecker[_rawTrait]);
        string memory traitCategory;
        string memory trait;
        if (_proof.length > 0) {
            (traitCategory, trait) = checker.getTraitWithProof(_username, _proof);
        } else {
            (traitCategory, trait) = checker.getTrait(_username);
        }
        require(bytes(trait).length > 0, "UsernameNFT: trait check failed");
        return (traitCategory, trait);
    }


    function updateTraits(uint tokenId, string[] calldata _rawTraits, bytes32[][] calldata _proofs) external {
        _updateTraits(usernames[tokenId], tokenId, _rawTraits, _proofs);
        emit MetadataUpdate(tokenId);
    }

    /*------------------*/
    /*      internal    */
    /*------------------*/
    function _validateUsername(string memory username) internal view returns(bool) {
        string memory _lower = _toLower(username);
        if(_usernameExists[_lower]) return false;
        if(_checkForbiddenChar(username)) return false;
        return true;
    }

    
    function _checkForbiddenChar(string memory str) internal view returns (bool) {
        bytes memory inputBytes = bytes(str);
        uint len = _forbiddenChars.length;
        for (uint i = 0; i < inputBytes.length; i++) {
            for (uint j = 0; j < len; j++) {
                bytes memory _check = bytes(_forbiddenChars[j]);
                // we force [0] since there's only 1 byte in _forbiddenChars
                if (bytes1(inputBytes[i]) == _check[0]) {
                    return true;
                }
            }
        }
        return false;
    }

    function _toLower(string memory _str) internal pure returns (string memory) {
        bytes memory strBytes = bytes(_str);
        uint len = strBytes.length;
        bytes memory lowerBytes = new bytes(len);
        for (uint i = 0; i < len; i++) {
            uint8 size = UnicodeLib.unicodeSize(strBytes, i);
            if(size != 1) { //We have a unicode char, skip 
                for (uint j = 0; j < size; j++) {
                    lowerBytes[i + j] = strBytes[i + j];
                }
                i += size - 1;
            }
            // Convert uppercase character to lowercase
            if ( (strBytes[i] >= bytes1(uint8(65))) && (strBytes[i] <= bytes1(uint8(90))) ) {
                lowerBytes[i] = bytes1(uint8(strBytes[i]) + 32);
            } else {
                lowerBytes[i] = strBytes[i];
            }
        }
        return string(lowerBytes);
    }
    
    function safeMint(address to, uint256 _tokenId) internal {
        _safeMint(to, _tokenId);
    }

    /*------------------*/
    /*      VIEW        */
    /*------------------*/

    function validateUsername(string memory username) external view returns(bool){
        string memory _lower = _toLower(username);
        if(_usernameExists[_lower]) return false;
        if(_checkForbiddenChar(username)) return false;
        return true;
    }

    function forbiddenChar() external view returns(string[] memory){
        return _forbiddenChars;
    }

    function toLower(string memory _str) external pure returns(string memory loweredString) {
        loweredString = _toLower(_str); 
    }

    function _getCost(uint length, address _paymentToken) private view returns (uint256) {
        uint _decimals = ITokenInfo(_paymentToken).decimals();
        uint _mult = 10**_decimals;
        if(length > COSTS_LENGTH) length = COSTS_LENGTH;

        uint _cost = _costPerToken[_paymentToken][length - 1];
        return _cost * _mult;
    }

    function _getLength(string memory _username) internal view returns (uint) {
        bytes memory b = bytes(_username);
        uint byteLength = b.length;
        uint charLength = 0;
        for (uint i = 0; i < byteLength; ) {
            charLength++;
            i += EmojiUtils(emojiUtils).getSizeOfChar(b, i);
        }
        return charLength;
    }

    function getLength(string memory _username) external view returns (uint) {
        return _getLength(_username);
    }

    function isUsernameAvailable(string memory username) public view returns (bool) {
        return _usernameExists[username];
    }

    function costPerToken(address _token) external view returns(uint[] memory){
        return _costPerToken[_token];
    }

    function tokenURI(uint256 _tokenId) public view override returns (string memory) {
        // if(!_exists(_tokenId)) return "";
        // require(_usernameExists[usernames[_tokenId]]);
        return IUsernameArtProxy(artProxy)._tokenURI(usernames[_tokenId], _date[_tokenId], _length[_tokenId], traitsCategories[_tokenId], traits[_tokenId]);
    }


    /*------------------*/
    /*      owner       */
    /*------------------*/

    function setAdmins(address _admin, bool _what) external onlyOwner {
        admins[_admin] = _what;
    }

    function _setCosts(address _token, uint[] memory costs) internal {
        require(costs.length == COSTS_LENGTH);
        _costPerToken[_token] = costs;
    }

    function setCosts(address _token, uint[] memory costs) external onlyAdminOrOwner {
        _setCosts(_token, costs);
        emit CostsSet(_token, costs);
    }

    function setMaxLength(uint _newMaxLength) external onlyAdminOrOwner {
        MAX_LENGTH = _newMaxLength;
        emit MaxLengthSet(_newMaxLength);
    }

    function addPaymentToken(address token) external onlyAdminOrOwner {
        require(!allowedTokens[token]);
        allowedTokens[token] = true;
        emit PaymentTokenAdded(token);
    }

    function removePaymentToken(address token) external onlyAdminOrOwner {
        require(allowedTokens[token]);
        allowedTokens[token] = false;
        emit PaymentTokenRemoved(token);
    }

    function withdrawTokens(address token, uint256 amount) external onlyAdminOrOwner {
        IERC20 erc20Token = IERC20(token);
        uint256 contractBalance = erc20Token.balanceOf(address(this));
        require(contractBalance >= amount, "UsernameNFT: Insufficient token balance");
        erc20Token.safeTransfer(msg.sender, amount);
        emit TokensWithdrawn(token, msg.sender, amount);
    }

    function addTraits(address[] memory _traitCheckerContracts, string[] memory _traitNames) external onlyAdminOrOwner {
        require(_traitCheckerContracts.length == _traitNames.length, "UsernameNFT: addTraits different parameters length");
        for (uint i = 0; i < _traitCheckerContracts.length; i++) {
            require(_traitCheckerContracts[i] != address(0), "UsernameNFT: Trait checker is address 0");
            require(traitToTraitChecker[_traitNames[i]] == address(0), "UsernameNFT: trait already added");
            traitToTraitChecker[_traitNames[i]] = _traitCheckerContracts[i];
            emit TraitAdded(_traitCheckerContracts[i], _traitNames[i]);
        }
    }

    function addTrait(address _traitCheckerContract, string memory _traitName) external onlyAdminOrOwner {
        require(_traitCheckerContract != address(0), "UsernameNFT: Trait checker is address 0");
        require(traitToTraitChecker[_traitName] == address(0), "UsernameNFT: trait already added");
        traitToTraitChecker[_traitName] = _traitCheckerContract;
        emit TraitAdded(_traitCheckerContract, _traitName);
    }

    function editTrait(string memory _traitName, address newTraitCheckerContract) external onlyAdminOrOwner {
        require(newTraitCheckerContract != address(0), "UsernameNFT: Trait checker does not exist");
        require(traitToTraitChecker[_traitName] != address(0), "UsernameNFT: Trait has not been added");
        address oldTraitCheckerContract = traitToTraitChecker[_traitName];
        traitToTraitChecker[_traitName] = newTraitCheckerContract;
        emit TraitEdited(_traitName, oldTraitCheckerContract, newTraitCheckerContract);
    }

    function removeTrait(string memory _traitName) external onlyAdminOrOwner {
        require(address(traitToTraitChecker[_traitName]) != address(0), "UsernameNFT: Trait has not been added");
        address removedTraitCheckerContract = traitToTraitChecker[_traitName];
        delete traitToTraitChecker[_traitName];
        emit TraitRemoved(_traitName, removedTraitCheckerContract);
    }


    function updateArtProxy(address _newArtProxy) external onlyOwner {
        require(_newArtProxy != address(0));
        address oldArtProxy = artProxy;
        artProxy = _newArtProxy;
        emit ArtProxyUpdated(oldArtProxy, _newArtProxy);
    }

    function updateEmojiUtils(address _newEmojiUtils) external onlyOwner {
        require(_newEmojiUtils != address(0));
        address oldEmojiUtils = emojiUtils;
        emojiUtils = _newEmojiUtils;
        emit EmojiUtilsUpdated(oldEmojiUtils, _newEmojiUtils);
    }

    function pushForbiddenChar(string memory _str) external onlyAdminOrOwner {
        require(!_isForbidden[_str]);
        require(bytes(_str).length == 1);
        _forbiddenChars.push(_str);
        _isForbidden[_str] = true;
        emit ForbiddenCharAdded(_str);
    }

    function removeForbiddenChar(string memory _str) external onlyAdminOrOwner {
        require(_isForbidden[_str]);
        uint i = 0;
        uint len = _forbiddenChars.length;
        for(i; i < len; i++){
            bytes memory _check = bytes(_forbiddenChars[i]);

            if(_check[0] == bytes1( bytes(_str))) {

                _forbiddenChars[i] = _forbiddenChars[len-1];
                _forbiddenChars.pop();
                _isForbidden[_str] = false;
                break;
            }
        }
        emit ForbiddenCharRemoved(_str);
    }

}

