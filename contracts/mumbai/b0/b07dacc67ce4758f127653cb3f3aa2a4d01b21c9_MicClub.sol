pragma solidity ^0.8.13;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "operator-filter-registry/src/DefaultOperatorFilterer.sol";
import "./Base.sol";

contract MicClub is Base {
    uint256[] public phaseCount;
    uint256 public mintPrice;
    uint128 public currentPhase = 1;
    uint128 public status;

    mapping(address => uint256) public recipientsPhase;

    address payable private WALLET =
        payable(0x281f3A9C678766EeCb082aDc0883d574ccDfE927);

    event SetPhaseMaxCount(address sender, uint256 phase, uint256 count);
    event UpdateMintPrice(uint256 newPrice);
    event WhiteList(address user, uint256 phase, bool isWhiteList);

    modifier isPre() {
        require(status == 0, "not pre");
        _;
    }

    modifier isPublic() {
        require(status == 1, "not public");
        _;
    }

    constructor(
        string memory _name,
        string memory _symbol,
        string memory _baseUri,
        address _receiver,
        uint96 _fee,
        uint256 _mintPrice,
        address _admin1,
        address _admin2
    ) Base(_name, _symbol, _baseUri, _receiver, _fee) {
        phaseCount.push(type(uint256).max);
        mintPrice = _mintPrice;
        _grantRole(DEFAULT_ADMIN_ROLE, _admin1);
        _grantRole(DEFAULT_ADMIN_ROLE, _admin2);
    }

    function setReceivedWallet(address _wallet) external isAdmin {
        WALLET = payable(_wallet);
    }

    function setStatus(uint128 status_) external isAdmin {
        status = status_;
    }

    function setMintPrice(uint256 price_) external isAdmin {
        mintPrice = price_;
        emit UpdateMintPrice(price_);
    }

    function startNextPhase() external isAdmin {
        if (phaseCount[currentPhase - 1] == type(uint256).max)
            revert Errors.NOT_REACH_PHASE_MINT_COUNT();
        uint256 sum;
        for (uint i; i < phaseCount.length; i++) {
            sum += phaseCount[i];
        }
        if (sum != totalSupply()) revert Errors.NOT_REACH_MAX_PHASE_COUNT();
        unchecked {
            ++currentPhase;
        }
        if (currentPhase > type(uint8).max) revert Errors.INVALID_PHASE();
        phaseCount.push(type(uint256).max);
    }

    function setPhaseMaxCount(uint256 count_) external isAdmin {
        uint256 sum = count_ + getPrePhaseTotalCount();
        if (sum < totalSupply()) revert Errors.INVALID_MAX_COUNT();
        phaseCount[currentPhase - 1] = count_;
        emit SetPhaseMaxCount(msg.sender, currentPhase, count_);
    }

    function addRecipientsPhase(address[] memory recipients_) external isAdmin {
        if (currentPhase > type(uint8).max) revert Errors.INVALID_PHASE();
        for (uint i; i < recipients_.length; i++) {
            recipientsPhase[recipients_[i]] |= (1 << (currentPhase - 1));
            emit WhiteList(recipients_[i], currentPhase, true);
        }
    }

    function removeRecipientsPhase(
        address[] memory recipients_
    ) external isAdmin {
        if (currentPhase > type(uint8).max) revert Errors.INVALID_PHASE();
        for (uint i; i < recipients_.length; i++) {
            recipientsPhase[recipients_[i]] &= ~(1 << (currentPhase - 1));
            emit WhiteList(recipients_[i], currentPhase, false);
        }
    }

    function isPhaseWhiteAddress(address user) public view returns (bool) {
        return (recipientsPhase[user] & (1 << (currentPhase - 1))) != 0;
    }

    function getPrePhaseTotalCount() internal view returns (uint256 preTotal) {
        for (uint i; i < phaseCount.length - 1; i++) {
            preTotal += phaseCount[i];
        }
    }

    function beforeVerified(uint256 price_) internal {
        if (Address.isContract(msg.sender)) revert Errors.ONLY_ALLOW_EOA();
        if (phaseCount[currentPhase - 1] != type(uint256).max) {
            if (
                totalSupply() >=
                getPrePhaseTotalCount() + phaseCount[currentPhase - 1]
            ) revert Errors.REACH_PHASE_MINT_COUNT();
        }

        if (msg.value < price_) revert Errors.INVALID_PRICE();

        if (msg.value > price_) {
            Address.sendValue(payable(msg.sender), msg.value - price_);
        }
    }

    function preMint() external payable whenNotPaused nonReentrant isPre {
        if (!isPhaseWhiteAddress(msg.sender)) revert Errors.NOT_IN_WHITELIST();
        beforeVerified(mintPrice);
        _safeMint(msg.sender, totalSupply() + 1);
        recipientsPhase[msg.sender] &= ~(1 << (currentPhase - 1));
    }

    function publicMint() external payable whenNotPaused nonReentrant isPublic {
        beforeVerified(mintPrice);
        _safeMint(msg.sender, totalSupply() + 1);
    }

    function genesisMintTo(address _to, uint256 num_) external payable whenNotPaused nonReentrant isAdmin {
        for (uint i; i != num_;) {
            if (phaseCount[currentPhase - 1] != type(uint256).max) {
                if (
                    totalSupply() >=
                    getPrePhaseTotalCount() + phaseCount[currentPhase - 1]
                ) revert Errors.REACH_PHASE_MINT_COUNT();
            }

            _safeMint(_to, totalSupply() + 1);
            unchecked {
                ++i;
            }
        }
    }

    function getTokenDetail(
        uint256 tokenId
    ) external view returns (address, uint256) {
        uint256 sum = 1;
        address owner = ownerOf(tokenId);
        for (uint i; i < phaseCount.length - 1; i++) {
            sum += phaseCount[i];
            if (tokenId < sum) {
                return (owner, i + 1);
            }
        }
        return (owner, phaseCount.length);
    }

    function retrieveETH() public isAdmin {
        Address.sendValue(WALLET, address(this).balance);
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.13;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "operator-filter-registry/src/DefaultOperatorFilterer.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./Errors.sol";

contract Base is
    ERC2981,
    ERC721Enumerable,
    DefaultOperatorFilterer,
    ReentrancyGuard,
    Pausable,
    AccessControl
{
    string public baseUri;
    mapping(address => bool) public blacklists;

    event EthReceived(address sender, uint value);
    event BlackList(address user, bool isBlack);

    modifier notInBlacklist(address user) {
        require(!blacklists[user], "black list user");
        _;
    }

    modifier isAdmin() {
        _checkRole(DEFAULT_ADMIN_ROLE);
        _;
    }

    constructor(
        string memory _name,
        string memory _symbol,
        string memory _baseUri,
        address _receiver,
        uint96 _fee
    ) ERC721(_name, _symbol) {
        if (_receiver == address(0)) revert Errors.INVALID_RECEIVER_ADDRESS();
        baseUri = _baseUri;
        _setDefaultRoyalty(_receiver, _fee);
        _pause();
    }

    function setBaseURI(string calldata _baseUri) external isAdmin {
        baseUri = _baseUri;
    }

    function _baseURI() internal view override returns (string memory) {
        return baseUri;
    }

    function addBlacklist(address[] calldata users_) external isAdmin {
        for (uint i; i < users_.length; i++) {
            blacklists[users_[i]] = true;
            emit BlackList(users_[i], true);
        }
    }

    function removeBlacklist(address[] calldata users_) external isAdmin {
        for (uint i; i < users_.length; i++) {
            delete blacklists[users_[i]];
            emit BlackList(users_[i], false);
        }
    }

    function setDefaultRoyalty(
        address receiver,
        uint96 feeNumerator
    ) external isAdmin {
        _setDefaultRoyalty(receiver, feeNumerator);
    }

    function pause() public isAdmin {
        _pause();
    }

    function unpause() public isAdmin {
        _unpause();
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal override notInBlacklist(from) notInBlacklist(to) {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        override(ERC2981, ERC721Enumerable, AccessControl)
        returns (bool)
    {
        return
            ERC2981.supportsInterface(interfaceId) ||
            ERC721Enumerable.supportsInterface(interfaceId) ||
            AccessControl.supportsInterface(interfaceId);
    }

    receive() external payable {
        emit EthReceived(msg.sender, msg.value);
    }

    // =============================================================
    // to obey the   opensea  Operator Filter Registry standard
    // override the ERC721 transfer and approval methods
    // relate with "operator-filter-registry/src/DefaultOperatorFilterer.sol";
    // =============================================================

    function setApprovalForAll(
        address operator,
        bool approved
    ) public override(ERC721, IERC721) onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    function approve(
        address operator,
        uint256 tokenId
    ) public override(ERC721, IERC721) onlyAllowedOperatorApproval(operator) {
        super.approve(operator, tokenId);
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override(ERC721, IERC721) onlyAllowedOperator(from) {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override(ERC721, IERC721) onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    ) public override(ERC721, IERC721) onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId, data);
    }
}

