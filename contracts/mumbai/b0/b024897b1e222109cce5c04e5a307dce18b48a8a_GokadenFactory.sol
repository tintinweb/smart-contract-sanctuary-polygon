// SPDX-License-Identifier: UNLICENSED
pragma solidity 0.8.17;


import "openzeppelin-contracts/proxy/Clones.sol";
import "openzeppelin-contracts/access/Ownable2Step.sol";
import "openzeppelin-contracts/security/ReentrancyGuard.sol";
import "openzeppelin-contracts/utils/Strings.sol";
import "./Gokaden.sol";

contract GokadenFactory is Ownable2Step, ReentrancyGuard {
    using Clones for address;
    using Strings for uint256;

    address public master;
    address[] public allClones;
    uint256 public cloneCount;
    address private _gokadenAdmin;
    mapping(address=>address) private _authorToGokaden;

    bool private _singleGokadenPerAuthor;

    event NewGokadenCreated(address);

    constructor(address _master, address gokadenAdmin_) {
        master = _master;
        _gokadenAdmin = gokadenAdmin_;
        _singleGokadenPerAuthor = true;
    }

    function setMaster (address _newMaster) external onlyOwner{
        master = _newMaster;
    }

    function createGokaden(address author) external nonReentrant onlyOwner {
        if(_singleGokadenPerAuthor == true) {
            require(_authorToGokaden[author] == address(0), "GokadenFactory: Only one Gokaden per author allowed");
        }
       string memory contractName = string.concat("Gokaden", " ", cloneCount.toString() );
       string memory symbol = string.concat("G",cloneCount.toString() );
       address clone = master.clone();
       _authorToGokaden[author] = clone;
       allClones.push(clone);
       Gokaden(payable(clone)).initialize(contractName, symbol, _gokadenAdmin, author);
       cloneCount += 1;
       
       emit NewGokadenCreated(clone);
    }

    function toggleOneGokadenPerAuthor() external onlyOwner {
        _singleGokadenPerAuthor = !_singleGokadenPerAuthor;
    }

    function getCloneCount() external view returns(uint256){
        return cloneCount;
    }

    function getGokadenByAuthor(address author) external view returns(address){
        return _authorToGokaden[author];
    }
}

