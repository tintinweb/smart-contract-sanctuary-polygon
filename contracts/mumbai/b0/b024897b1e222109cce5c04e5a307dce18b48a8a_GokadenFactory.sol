pragma solidity 0.8.17;

import "./interfaces/IGokaden.sol";
import "openzeppelin-contracts-upgradeable/proxy/utils/Initializable.sol";
import "openzeppelin-contracts-upgradeable/access/Ownable2StepUpgradeable.sol";
import "openzeppelin-contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "openzeppelin-contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "openzeppelin-contracts-upgradeable/token/common/ERC2981Upgradeable.sol";
import "./GokadenFactory.sol";

contract Gokaden is IGokaden, Initializable, ERC721URIStorageUpgradeable, ERC2981Upgradeable, 
                AccessControlUpgradeable, Ownable2StepUpgradeable {
    
    uint256 private _index = 1;
    mapping(uint256 => string) private _tokenURIs;
    mapping(uint256=>string) private _tokenIdToID;

    function initialize(string memory name, 
    string memory symbol, 
    address gokadenAdmin, 
    address contractOwner) external initializer {
        __Ownable2Step_init();
        __AccessControl_init();
        __ERC2981_init_unchained();
        __ERC721_init(name, symbol);
        __ERC721URIStorage_init();
        _setDefaultRoyalty(contractOwner, 500);
        _setupRole(DEFAULT_ADMIN_ROLE, contractOwner);
        _setupRole(DEFAULT_ADMIN_ROLE, gokadenAdmin);
        _setDefaultRoyalty(contractOwner, 500);
    }

    constructor () initializer {
    }

    /**
    * @notice Mints 1 NFT to @param recipient
    * @param tokenURI TokenURI of newly minted token
    * emits an {NFTMinted} event
    */
    function mintNFTs(string memory tokenURI, string memory ID) external onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 tokenId = _index;
        if (bytes(tokenURI).length == 0) revert InvalidUri();
        
        unchecked {
            _index += 1;
        }

        _tokenIdToID[tokenId] = ID;
        _mint(msg.sender, tokenId);
        _setTokenURI(tokenId, tokenURI);
        
        emit NFTMinted(msg.sender, tokenId);
    }

    function setRoyalties(address recipient, uint96 royaltiesPoints) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(recipient != address(0), "Error: Address Zero");
        _setDefaultRoyalty(recipient, royaltiesPoints);

        emit RoyaltiesChanged(recipient, royaltiesPoints);
    }

    function setTokenUri(string memory newTokenUri, uint256 tokenId) external onlyRole(DEFAULT_ADMIN_ROLE){
        _setTokenURI(tokenId, newTokenUri);

        emit NewTokenURISet(newTokenUri);
    }

    /**
     * @notice It allows the owner of the contract to burn a specific token Id
     * @param tokenId the token to be burned
     * @dev it reduces total Supply by 1
     */
    function burn(uint256 tokenId) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _burn(tokenId);
        emit NFTBurned(tokenId);
    }

    /**
     * @notice it gets the ID of the next NFT that will be minted
     */

    function getCurrentId() external view returns (uint256) {
        return _index;
    }

    /**
     * @notice returns the current total supply
     * @dev Etherscan uses it to inform of total token supply
     */
    function totalSupply() external view returns(uint256){
        return _index - 1;
    }

    function getArtworkId(uint256 tokenId) external view returns(string memory){
        return _tokenIdToID[tokenId];
    }

    function supportsInterface(bytes4 interfaceId) public view  override(ERC721Upgradeable, ERC2981Upgradeable, AccessControlUpgradeable) returns (bool){
    bytes4 INTERFACE_ID_ERC721 = 0x80ac58cd;
    bytes4 INTERFACE_ID_ERC2981 = 0x2a55205a;
    bytes4 INTERFACE_ID_ERC165 = 0x01ffc9a7;

    return 
            interfaceId == INTERFACE_ID_ERC721 ||
            interfaceId == INTERFACE_ID_ERC2981 ||
            interfaceId == INTERFACE_ID_ERC165 || 
            super.supportsInterface(interfaceId);
        
    }
    
}

