    function reveal(string memory subcollectionName) external onlyRole(TRUSTED_SIGNER_ROLE) {
        uint256 subcollectionIdx = getSubcollectionIdxByName(subcollectionName);
        require(!subcollections[subcollectionIdx].isRevealed, "Already revealed");
