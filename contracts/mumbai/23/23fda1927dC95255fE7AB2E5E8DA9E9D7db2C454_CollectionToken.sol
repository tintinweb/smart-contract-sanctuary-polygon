     */ 
    function mintGoldenToken(
        string memory subcollectionName,
        uint256 goldenTicketNumber,
        address to
    ) external onlyRole(LOTTERY_ROLE) {
        uint256 subcollectionIdx = getSubcollectionIdxByName(subcollectionName);
        require(isLotteryOver(subcollectionName), "Lottery is not over");
        require(
            goldenTicketNumber <= subcollections[subcollectionIdx].params.goldSupply &&
                goldenTicketNumber > 0,
