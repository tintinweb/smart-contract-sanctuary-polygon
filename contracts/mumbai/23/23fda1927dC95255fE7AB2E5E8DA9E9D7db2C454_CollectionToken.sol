        if (!hasRole(CLAIMER_ROLE, _msgSender())) {
            require(msg.value == subcollection.params.mintPrice, "Invalid msg value");
            require(
                _mintedBy[_msgSender()][subcollectionIdx] <=
                    subcollection.params.maxMintPerAddress,
                "Max supply per address"
            );
            if (subcollection.whitelistMintEnabled) {
                require(
                    _verifyProof(
                        subcollectionName,
                        keccak256(
                            bytes.concat(keccak256(abi.encode(_msgSender())))
                        ),
                        proof
                    ),
                    "Not on whitelist"
                );
            }
        }
        require(
            subcollection.totalMinted < subcollection.params.mainSupply,
            "Subcollection is sold"
        );
        uint256 vrfRequestId = COORDINATOR.requestRandomWords(
            _keyHash,
            _subscriptionId,
            REQUEST_CONFIRMATIONS,
            CALLBACK_GAS_LIMIT,
            NUM_WORDS
        );
        _vrfRequests[vrfRequestId] = MintRequest({
            subcollectionName: subcollectionName,
            mintTo: to
        });

        emit MintRequested(subcollectionName, to, vrfRequestId);
    }

