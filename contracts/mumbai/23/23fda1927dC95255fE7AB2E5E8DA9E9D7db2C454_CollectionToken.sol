     */
    function upgradeToken(
        string memory subcollectionName,
        uint256 tokenId,
        bytes memory signature
    ) external {
        uint256 subcollectionIdx = getSubcollectionIdxByName(subcollectionName);
        require(
            subcollectionIdx == getSubcollectionIdxByTokenId(tokenId),
            "Invalid subcollection"
        );
        string memory upgradedTokenURI = subcollections[subcollectionIdx].params.upgradedTokenURI;
        require(isSubcollectionRevealed(subcollectionName), "Subcollection must be revealed");
        require(bytes(_tokenURIs[tokenId]).length == 0, "Token is already upraded");
        _requireMinted(tokenId);

        bytes32 messageHash = keccak256(
            abi.encodePacked(
                address(this),
                _msgSender(),
                subcollectionName,
                tokenId
            )
        );
        bytes32 hash = ECDSA.toEthSignedMessageHash(messageHash);
        address signer = ECDSA.recover(hash, signature);
        require(hasRole(TRUSTED_SIGNER_ROLE, signer), "Invalid signature");
        _setTokenURI(tokenId, string(abi.encodePacked(upgradedTokenURI, tokenId.toString(), ".json")));
        emit MetadataUpdate(tokenId);
