    function _mint(
        string memory subcollectionName,
        address to,
        uint256 trueRandom
    ) private {
        uint256 subcollectionIdx = getSubcollectionIdxByName(subcollectionName);
        Subcollection storage subcollection = subcollections[subcollectionIdx];
        require(!subcollection.isRevealed, "Already revealed");
        require(
            subcollection.totalMinted < subcollection.params.mainSupply,
            "Subcollection is sold"
        );
        require(
            _mintedBy[_msgSender()][subcollectionIdx] <=
                subcollection.params.maxMintPerAddress,
            "Max supply per address"
        );

        uint256 tokenId = _nextRandomId(
            subcollectionIdx,
            subcollection.params.mainSupply,
            subcollection.totalMinted,
            subcollection.startIndex,
            trueRandom
        );
        // console.log(tokenId);
        _safeMint(to, tokenId);
        subcollection.totalMinted += 1;
        _subIdByTokenId[tokenId] = subcollectionIdx;
        if (subcollection.totalMinted == subcollection.params.mainSupply) {
            emit SubcollectionSold(subcollectionName);
