        uint public maxdutchAuction;
    uint public  auctionCeilPrice;
    uint public auctionFloorPrice;
    uint public DURATION;
    uint public startAt;
    uint public discountRate ; // per secod the price will be decreased by 10

  
 function getPrice() public view returns (uint) {
       
        uint timeElapsed = block.timestamp - startAt;
        
        uint discount = discountRate * timeElapsed;
        if(discount>auctionFloorPrice) {
            return auctionFloorPrice;
        }
        else {
        return auctionCeilPrice - discount;
        }
    }

    function configureDutchAuction (
        //  uint _dutchAuctionTokenID, // this is the tokenID of which type of NFT will the dutch mint will be for 
         uint _maxdutchAuction, // max total supply of dutch nft // this is the tokenID of which type of NFT will the bonded mint will be for 
        uint _auctionFloorPrice,  
        uint _auctionCeilPrice, 
        uint _auctionDuration, 
        uint _auctionStartTime, 
        uint _auctionDiscountRate // per second discount rate in wei
    ) public {
        require(hasRole(GOVERNANCE_ROLE, msg.sender), "need governance role");
        // require(_dutchAuctionTokenID != fixedMintTokenID, "token id already given to fixedMint");
        // require(_dutchAuctionTokenID != bondedCurveTokenID, "token id already given to fixedMint");

         dutchAuctionTokenID = 3;
         maxdutchAuction = _maxdutchAuction;
         auctionCeilPrice = _auctionCeilPrice;
         auctionFloorPrice = _auctionFloorPrice;
         DURATION = _auctionDuration;
         startAt = _auctionStartTime;
         discountRate = _auctionDiscountRate; 
    

    }
  


    function dutchAuction(address to, uint256 amount)
        public
        whenNotPaused()
        payable

    {     require(startAt < block.timestamp);
          require(msg.value >=  getPrice()* amount, "Insufficient funds!");
          require(totalSupply(dutchAuctionTokenID) + amount <= maxdutchAuction, "can't mint more than supply");
          super._mint(to, dutchAuctionTokenID, amount, "");
          if(msg.value > getPrice()) {
              payable(msg.sender).transfer(msg.value-getPrice());
          }
      
    }


    function giveGovernanceRole(address _to) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "need governance role");
            _grantRole(GOVERNANCE_ROLE, msg.sender);
    } 
    function revokeGovernanceRole(address _to) external {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "need governance role");
        require(hasRole(GOVERNANCE_ROLE, _to), "need governance role");
            _revokeRole(GOVERNANCE_ROLE, msg.sender);
    } 
  



}

