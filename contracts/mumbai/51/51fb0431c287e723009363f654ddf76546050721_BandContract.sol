import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
// rarible royalty contracts
import "contracts/@rarible/royalties/contracts/LibRoyaltiesV2.sol";
// import "contracts/@rarible/royalties/contracts/LibPart.sol";
// import "contracts/@rarible/royalties/contracts/impl/RoyaltiesV2Impl.sol";
// /// @custom:security-contact [emailÂ protected]
contract AliveERC1155 is  
                        Initializable, UUPSUpgradeable ,AccessControlUpgradeable,
                        ERC2981Upgradeable,
                        ERC1155PausableUpgradeable, 
                        // ERC1155BurnableUpgradeable, 
                        ERC1155SupplyUpgradeable {
    bytes32  constant GOVERNANCE_ROLE = keccak256("GOVERNANCE_ROLE");
    bytes32  constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
     uint public songId;

    
    address payable aliveHouse;
    address aliveManager;


    uint256 public constant EXPERIENCE = 1;


    address mintRoyaltyReceiver;


    uint public fixedMintTokenID;
    

        event fixedMintEvent(address minter, uint amount, uint tokenID);


    function initialize( address _royaltyReceiverSecondary,  // royalityReceiver
        uint96 _royaltyFeesInBips, // this royality is for secondary sale and mint, which will be 0xsplit address of  band members + alivehouse
        // uint _fixedMintTokenID,  // this is the tokenID of which type of NFT will the fixed mint will be for 
        uint _maxFixedMint,  // max total supply of fixedmint nft
        uint _fixedMintPrice,  // price of the fixedMintToken
        address royaltyReceiverSplit // split wallet addres of the band
        ,string memory _uri, string memory _contractURI , uint _songID
        ) public initializer { 

        __ERC1155_init(_uri);
        // require(_fixedMintTokenID <= 3);
         _pause();
         songId = _songID;

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(GOVERNANCE_ROLE,  msg.sender);
        _grantRole(PAUSER_ROLE,  msg.sender);

        mintRoyaltyReceiver = royaltyReceiverSplit;
        
        aliveHouse = payable(0x0dd68c06Af920CA069CDc27d05AA9EB65F85990A);
        contracturi=_contractURI;

        _setDefaultRoyalty(_royaltyReceiverSecondary, _royaltyFeesInBips);
// fixed mint details
         fixedMintTokenID = 1;
         maxFixedMint = _maxFixedMint;
         fixedMintPrice = _fixedMintPrice;

    }

    function _authorizeUpgrade(address newImplementation) internal override  {
      require(msg.sender == aliveManager, "only alive");
    }




    function setURI(string memory newuri) public onlyRole(GOVERNANCE_ROLE) {
        _setURI(newuri);
    }

    
    function uri(uint256 id) override public view returns (string memory) {
        return string(
            abi.encodePacked(super.uri(id), StringsUpgradeable.toString(id), ".json")
        );
        // return(_uris[id]);
    }

function setRoyalties(uint _tokenId, address payable _royaltiesReceipientAddress, uint96 _percentageBasisPoints) public  onlyRole(GOVERNANCE_ROLE) {
       
        _setTokenRoyalty(_tokenId,_royaltiesReceipientAddress,_percentageBasisPoints );


    }
   

   string contracturi;

    function batchAirDrop(address[] memory _arrayOfReceivers, uint _amountForEach) public  onlyRole(GOVERNANCE_ROLE) {
               
       require(totalSupply(fixedMintTokenID) + _arrayOfReceivers.length <= maxFixedMint, "can't mint more than supply");
        for (uint256 index = 0; index < _arrayOfReceivers.length;) {

             super._mint(_arrayOfReceivers[index], fixedMintTokenID, _amountForEach, "");

                unchecked {
                     index++;
                 }
                }

                

    }
  function contractURI() public view returns (string memory) {
     
        return contracturi;
    }



    
    function pause() public virtual {
        require(hasRole(PAUSER_ROLE, msg.sender), "ERC1155PresetMinterPauserAutoId: must have pauser role to pause");
        _pause();
    }

    function unpause() public virtual {
        require(hasRole(PAUSER_ROLE, msg.sender), "ERC1155PresetMinterPauserAutoId: must have pauser role to unpause");
        _unpause();
    }

    uint public maxFixedMint;
    uint public fixedMintPrice;



    /// @param to the recipient of the token
    /// @param amount amount to mint 
    
    function fixedMint(address to, uint256 amount)
        public
        whenNotPaused()
        payable
        
    {   
        require(msg.value >=  fixedMintPrice* amount, "Insufficient funds!");
        require(totalSupply(fixedMintTokenID) + amount <= maxFixedMint, "can't mint more than supply");
        super._mint(to, fixedMintTokenID, amount, "");
        uint fee = msg.value/10; // 10% fee to alive
        payable(aliveHouse).transfer(fee);
        payable(mintRoyaltyReceiver).transfer(msg.value - fee);
        // _totalSupply[fixedMintTokenID] = _totalSupply[fixedMintTokenID] + amount;
        emit fixedMintEvent(to, amount, fixedMintTokenID);

    }

  
 
  

    function _beforeTokenTransfer(address operator, 
                    address from, 
                    address to, 
                    uint256[] memory ids, 
                    uint256[] memory amounts, 
                    bytes memory data)
        internal 
        // whenNotPaused
        override( ERC1155SupplyUpgradeable, ERC1155PausableUpgradeable)
    {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    // The following functions are overrides required by Solidity.

    function supportsInterface(bytes4 interfaceId)
        public
        view 
        override(ERC1155Upgradeable, ERC2981Upgradeable, AccessControlUpgradeable)
        returns (bool)
    {
         if(interfaceId == LibRoyaltiesV2._INTERFACE_ID_ROYALTIES) {
            return true;
        }
        return super.supportsInterface(interfaceId);
    }


    // function burn(
    //     address account,
    //     uint256 id,
    //     uint256 value)
    //         public  
    //         override(ERC1155BurnableUpgradeable) { 
    //     super.burn(account, id, value);
      

    // }

  

  

}

