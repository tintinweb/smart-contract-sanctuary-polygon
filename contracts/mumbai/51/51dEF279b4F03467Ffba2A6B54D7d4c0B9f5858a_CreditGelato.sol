        address lastCoin = _coins[_coins.length - 1];

        _coinsIndexes[lastCoin] = coinIndex + 1;
        _coinsIndexes[coin] = 0;

        _coins[coinIndex] = lastCoin;

        _coins.pop();
    }

    /**
     * @notice to set the addresses required for the contract
     *
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function resolverAddressesRequired()
        public
        pure
        override
        returns (bytes32[] memory addresses)
    {
        addresses = new bytes32[](2);

        addresses[0] = FACTORY_CONTRACT;
        addresses[1] = INSURANCE_FUND_CONTRACT;
    }

    /**
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function _resolveAddressesFinish() internal override {
        _factory = IFactory(requireAndGetAddress(FACTORY_CONTRACT));
        _creditSystem = ICreditSystem(
            requireAndGetAddress(INSURANCE_FUND_CONTRACT)
        );

        // TODO: Bad solution, exchange number can be diferent

        _exchange = _factory.getExchange(_indexExchange);
    }

    /**
     * @notice This function retrieves all coins that need to be
     * covered, executed by the keeper
     *
     * @return upkeepNeeded boolean to indicate whether the keeper should call
     * performUpkeep or not.
     * @return performData Array of coins that need to be refunded and array of amounts
     * in bytes that the keeper should call performUpkeep with
     *
     * @dev This function checks all `_coins' for a condition: if used amount of coin
     * is greater then _coinsMinAmountsIn[coin] this coin must be refunded.
     * This function contains the logic that will be executed
     * off-chain by Chainlink Upkeep to see if performUpkeep should be executed.
     */
    function check() public view returns (address[] memory, uint256[] memory) {
        uint size = 0;

        // If used amount of coin is greater then _coinsMinAmountsIn[coin]
        // this coin must be refunded.
        for (uint256 i = 0; i < _coins.length; i++) {
            uint256 usedAmount = _creditSystem.getUsedCreditBalance(_coins[i]);

            if (usedAmount > _coinsMinAmountsIn[_coins[i]]) size++;
        }

        address[] memory coins = new address[](size);
        uint[] memory amounts = new uint[](size);
        uint j = 0;

        // Filling coins array and amounts array with values
        for (uint256 i = 0; i < _coins.length; i++) {
            uint256 usedAmount = _creditSystem.getUsedCreditBalance(_coins[i]);

            if (usedAmount > _coinsMinAmountsIn[_coins[i]]) {
                coins[j] = _coins[i];
                amounts[j] = usedAmount;
                j++;
            }
        }

        return (coins, amounts);
    }

    /**
     * @notice Execution covering
     *
     * @dev This function calculates balances for each coin and then tries to cover.
     * This function will be executed by Chainlink Upkeep
     * when checkUpkeep function retunrs true.
     */
    function perform(address[] memory coins, uint256[] memory amounts) public {
        for (uint256 k = 0; k < coins.length; k++) {
            // Get all available tokens by which the coin[k] could be covered
            address[] memory tokens = _factory.getTokens();

            // Calculate balances for all tokens
            uint256[] memory balances = _calculateBalances(
                coins[k],
                tokens,
                amounts[k]
            );

            uint256 sum = 0;
            uint i = 0;

            console.log("BEFORE WHILE", amounts[k], tokens[i]);

            // Covering coin[k] using tokens
            while (sum < amounts[k] && i < balances.length && balances[i] > 0) {
                _creditSystem.transferFreeCoins(tokens[i], balances[i]);

                if (tokens[i] == coins[k]) {
                    sum += balances[i];
                } else {
                    TransferHelper.safeApprove(
                        tokens[i],
                        address(_exchange),
                        balances[i]
                    );

                    console.log("swap");

                    (, uint256 amountOut) = _exchange.swap(
                        IExchange.SwapParams({
                            amountIn: balances[i],
                            amountOut: 0,
                            tokenIn: tokens[i],
                            tokenOut: coins[k],
                            timestamp: block.timestamp,
                            path: "0x",
                            dexType: _indexExchange,
                            recipient: address(_creditSystem)
                        })
                    );

                    sum += amountOut;
                }

                i++;
            }

            console.log(sum);
        }
    }

    /**
     * @notice Calculating `tokens` balances. Receives a structured sorted
     * list of coins and balances which could be used for covering
     *
     * @param coin Token address
     * @param tokens Tokens with which the `coin` can be refunded
     * @param amount Amount of `coin` to refund
     * @return array `tokens` balances
     *
     * @dev This function calculates balances `token`.
     */
    function _calculateBalances(
        address coin,
        address[] memory tokens,
        uint256 amount
    ) internal view returns (uint256[] memory) {
        uint256[] memory rates = new uint256[](tokens.length);
        uint256[] memory balances = new uint256[](tokens.length);

        uint256 sum = 0;

        uint decimals = ERC20(coin).decimals();

        for (uint256 i = 0; i < tokens.length; i++) {
            if (_excludeTokens[tokens[i]]) continue;

            // Get credit balance of token
            balances[i] = _creditSystem.getFreeBalance(tokens[i]);

            console.log("balance: ", balances[i]);

            uint decimalsIn = ERC20(tokens[i]).decimals();

            // Get amountIn of token in coin equivalent with amountOut = amount
            //rates[i] = _exchange.getAmountIn(tokens[i], coin, amount);
            rates[i] = coin == tokens[i]
                ? 10 ** decimals
                : _exchange.getAmountOut(tokens[i], coin, 10 ** decimalsIn);

            console.log("rate", rates[i]);

            // If _coinsMinAmountsOut[coin] is greater then coin balance * rate
            // refund is not possible
            if (_coinsMinAmountsOut[coin] > balances[i] * rates[i])
                balances[i] = 0;

            sum += balances[i] * rates[i];
        }

        require(sum > 0, "Sum cannot be 0");

        uint256[] memory valuations = new uint256[](rates.length);

        // Calculate valuation for each token
        for (uint256 i = 0; i < rates.length; i++) {
            valuations[i] = (balances[i] * rates[i] * decimals) / sum;
        }

        // Sort by valuations
        valuations.quickSort(tokens, balances);

        return balances;
    }
}

