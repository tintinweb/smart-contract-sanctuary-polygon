pragma solidity =0.8.17;

import "../libraries/AppStorage.sol";
import "../libraries/StringUtils.sol";
import "./PoseState.sol";

import { IPoseRegistry } from "../interface/IPoseRegistry.sol";
import { IPoseStaking } from "../interface/IPoseStaking.sol";
import { IPoseToken } from "../interface/IPoseToken.sol";
import { PoseHelpers } from "./PoseHelpers.sol";

import { EnumerableSet } from "openzeppelin-contracts/utils/structs/EnumerableSet.sol";
import { ERC165 } from "openzeppelin-contracts/utils/introspection/ERC165.sol";
import { ECDSA } from "openzeppelin-contracts/utils/cryptography/ECDSA.sol";

contract PoseRegistry is IPoseRegistry, PoseHelpers, ERC165 {
    using EnumerableSet for EnumerableSet.AddressSet;
    using StringUtils for string;

    IPoseToken private poseToken;
    IPoseStaking public poseStaking;

    uint256 poseVersion;

    function initialize() external initializer {
        s.domainHash = getEIP712DomainHash("PoseRegistry", "1", address(this));
        __Pausable_init();
        __Ownable_init();
    }

    /**
     * @inheritdoc ERC165
     */
    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC165) returns (bool) {
        return interfaceId == type(IPoseRegistry).interfaceId || super.supportsInterface(interfaceId);
    }

    /* -------------------------------------------------------------------------- */
    /*                                Vault Functions                             */
    /* -------------------------------------------------------------------------- */
    /**
     * @notice These functions handle vault creation and management
     *
     */

    function registerVault() external override {
        s.isVault[msg.sender] = true;
        // Link vault to itself as linked wallet.
        s.linkedVault[msg.sender] = msg.sender;
        emit VaultRegistered(msg.sender, block.timestamp);
        emit WalletRegistered(msg.sender, msg.sender, block.timestamp);
    }

    // Give your vault a name for some POSE
    function nameVault(string calldata _name) external override {
        if (_name.strlen() > 24) revert NameToLong();
        if (!isVault(msg.sender)) revert NotVault();
        if (s.vaultReverseLookup[_name] != address(0) || bytes(s.vaultNames[msg.sender]).length != 0) {
            revert AlreadyRegistered();
        }
        // Burn tokens from sender
        poseToken.burnFrom(msg.sender, 100 * 1e18);
        // Set name
        s.vaultNames[msg.sender] = _name;
        s.vaultReverseLookup[_name] = msg.sender;
    }

    function transferName(address _vault) external override {
        if (!isVault(_vault)) revert NotVault();
        string memory _name = s.vaultNames[msg.sender];

        delete s.vaultNames[msg.sender];
        s.vaultNames[_vault] = _name;
        s.vaultReverseLookup[_name] = _vault;
    }

    function removeName() external override {
        delete s.vaultReverseLookup[s.vaultNames[msg.sender]];
        delete s.vaultNames[msg.sender];
    }

    // Gets name of a vault from address
    function getVaultName(address _vault) external view override returns (string memory) {
        return s.vaultNames[_vault];
    }

    // Get address of vault from name
    function getVaultReverseLookup(string calldata _name) external view override returns (address) {
        return s.vaultReverseLookup[_name];
    }

    // Check if registered vault
    function isVault(address _vaultAddress) public view override returns (bool) {
        return s.isVault[_vaultAddress];
    }

    /* -------------------------------------------------------------------------- */
    /*                           Collection Functions                             */
    /* -------------------------------------------------------------------------- */
    /**
     * @notice These functions handle nft collection management
     *
     */

    function registerCollection(address _collectionAddress, uint32 _chainId) external override onlyReflectionManager {
        if (s.collections[_chainId][_collectionAddress]) revert AlreadyRegistered();
        // Setup collection
        s.collections[_chainId][_collectionAddress] = true;

        emit CollectionRegistered(_collectionAddress, _chainId, block.timestamp);
    }

    function purgeCollection(address _collectionAddress, uint32 _chainId) external override onlyReflectionManager {
        if (!s.collections[_chainId][_collectionAddress]) revert NotRegisterd();
        // Setup collection
        s.collections[_chainId][_collectionAddress] = false;

        emit CollectionPurged(_collectionAddress, _chainId, block.timestamp);
    }

    function isCollectionRegistered(address _collectionAddress, uint32 _chainId) external view override returns (bool) {
        return s.collections[_chainId][_collectionAddress];
    }

    /* -------------------------------------------------------------------------- */
    /*                               Wallet Functions                             */
    /* -------------------------------------------------------------------------- */
    /**
     * @notice These functions handle linking and verifying user wallets to
     *         users Pose vaults using EIP712 signatures.
     */

    /**
     * @dev Associate user wallet with Pose vault. Requires signature from wallet
     *      being registered. A vault can be associated with multiple vaults.
     */
    function registerWallet(Wallet memory wallet, bytes memory signature) external override {
        // Verfiy signer
        address recoveredSignatureSigner = verifyWalletSignature(wallet, signature);
        if (recoveredSignatureSigner != wallet.linkWallet) revert BadSignature();
        if (!isVault(msg.sender)) revert NotVault();

        // Ensure wallet has not be previously registered to a vault.
        if (getWalletOwner(wallet.linkWallet) != address(0)) revert AlreadyLinked();

        // Add to vault set
        EnumerableSet.AddressSet storage vaultWalletSet = s.linkedWallets[wallet.poseWallet];
        vaultWalletSet.add(wallet.linkWallet);
        // Add to wallet => vault mapping
        s.linkedVault[wallet.linkWallet] = wallet.poseWallet;

        emit WalletRegistered(wallet.linkWallet, msg.sender, block.timestamp);
    }

    function getWalletInSet(address _vault, address _wallet) external view override returns (bool) {
        return s.linkedWallets[_vault].contains(_wallet);
    }

    /**
     * @dev Allows vault to unregister linked wallet.
     */
    function unregisterWallet(address _wallet) external override {
        // Ensure wallet linked to vault
        if (getWalletOwner(_wallet) != msg.sender) revert NotLinked();

        // Lookup linked vault and remove linkwallet from its set
        EnumerableSet.AddressSet storage vaultWalletSet = s.linkedWallets[s.linkedVault[_wallet]];
        vaultWalletSet.remove(_wallet);

        // Set wallet => vault to address(0)
        s.linkedVault[_wallet] = address(0);

        emit WalletUnregistered(_wallet, msg.sender, block.timestamp);
    }

    /**
     * @dev Return which vault has linked to a wallet.
     */
    function getWalletOwner(address _wallet) public view override returns (address) {
        return s.linkedVault[_wallet];
    }

    /* -------------------------------------------------------------------------- */
    /*                                  NFT Functions                             */
    /* -------------------------------------------------------------------------- */
    /**
     * @notice These functions handle reflecting NFT onwership from users linked
     *         wallets to Pose vaults.
     */

    // Called by relayer to reflect all NFTs in a linked wallet
    function reflectNFTs(ReflectNftParams[] calldata _reflectNftParams) external override onlyReflectionManager {
        if (_reflectNftParams.length == 0) revert NoParams();

        for (uint256 i = 0; i < _reflectNftParams.length; i++) {
            _reflectNFT(
                _reflectNftParams[i].collectionAddress,
                _reflectNftParams[i].chainId,
                _reflectNftParams[i].tokenId,
                _reflectNftParams[i].linkWallet
            );
        }
    }

    function _reflectNFT(
        address _collectionAddress,
        uint32 _chainId,
        uint32 _tokenId,
        address _linkWallet
    ) internal onlyRegisteredCollection(_collectionAddress, _chainId) {
        // Load Token
        Token memory token = s.tokens[_chainId][_collectionAddress][_tokenId];
        // If this is intial reflect set timestamp to block timestamp and add to total count.
        if (token.reflectedTime == 0) {
            token.reflectedTime = block.timestamp;
            s.totalTokenCount += 1;
        }

        // Update rewards for the previous vault and new vault before changing token count
        poseStaking.updateVaultReward(getWalletOwner(token.owningWallet));
        poseStaking.updateVaultReward(getWalletOwner(_linkWallet));

        // Decrement token count for previous vault if exists
        _decrementTokenCount(getWalletOwner(token.owningWallet));
        // Increment token count for new vault
        _incrementTokenCount(getWalletOwner(_linkWallet));

        // Set owning wallet
        token.owningWallet = _linkWallet;
        // Add Token struct to tokens mapping.
        s.tokens[_chainId][_collectionAddress][_tokenId] = token;
        emit NFTReflected(_linkWallet, _collectionAddress, _chainId, _tokenId, block.timestamp);
    }

    // Function to remove NFTs if needed
    function purgeNFTs(ReflectNftParams[] calldata _reflectNftParams) external override onlyReflectionManager {
        if (_reflectNftParams.length == 0) revert NoParams();

        for (uint256 i = 0; i < _reflectNftParams.length; i++) {
            _purgeNFT(
                _reflectNftParams[i].collectionAddress,
                _reflectNftParams[i].chainId,
                _reflectNftParams[i].tokenId,
                _reflectNftParams[i].linkWallet
            );
        }
    }

    function _purgeNFT(address _collectionAddress, uint32 _chainId, uint32 _tokenId, address _linkWallet) internal {
        if (!isNftReflected(_collectionAddress, _chainId, _tokenId)) revert NotReflected();
        Token memory token = s.tokens[_chainId][_collectionAddress][_tokenId];
        // Decrement token count for previous owning vault, update vault balance
        if (getWalletOwner(token.owningWallet) != address(0)) {
            poseStaking.updateVaultReward(getWalletOwner(token.owningWallet));
            _decrementTokenCount(getWalletOwner(token.owningWallet));
        }

        delete s.tokens[_chainId][_collectionAddress][_tokenId];
        // Decrease total token count
        s.totalTokenCount -= 1;
        emit NFTPurged(_linkWallet, _collectionAddress, _chainId, _tokenId, block.timestamp);
    }

    // Check for linked wallet address in "vault.wallets.index" set and return bool.
    function isNftReflected(address _collectionAddress, uint32 _chainId, uint32 _tokenId) public view override returns (bool exists) {
        if (s.tokens[_chainId][_collectionAddress][_tokenId].reflectedTime > 0) {
            return true;
        }
    }

    function getNftOwner(address _collectionAddress, uint32 _chainId, uint32 _tokenId) external view override returns (address) {
        return s.tokens[_chainId][_collectionAddress][_tokenId].owningWallet;
    }

    function getNftInfo(address _collectionAddress, uint32 _chainId, uint32 _tokenId) external view override returns (Token memory token) {
        return s.tokens[_chainId][_collectionAddress][_tokenId];
    }

    function getVaultTokenCount(address _vault) external view override returns (uint32) {
        return s.vaultTokenCounts[_vault];
    }

    function getTotalTokenCount() external view override returns (uint256) {
        return s.totalTokenCount;
    }

    function setPoseVersion(uint256 version) external onlyOwner {
        poseVersion = version;
    }

    /* -------------------------------------------------------------------------- */
    /*                                Modifiers                                   */
    /* -------------------------------------------------------------------------- */

    // Throws if called by any sender that isn't a registered vault
    modifier onlyRegisteredCollection(address _collectionAddress, uint32 _chainId) {
        if (!s.collections[_chainId][_collectionAddress]) revert NotRegisterd();
        _;
    }

    modifier onlyReflectionManager() {
        if (!isReflectionManger(msg.sender)) revert NotManager();
        _;
    }

    /* -------------------------------------------------------------------------- */
    /*                           Manager Functions                                */
    /* -------------------------------------------------------------------------- */

    function addReflectionManagers(address[] calldata _reflectionManagers) external override onlyOwner {
        for (uint256 index = 0; index < _reflectionManagers.length; index++) {
            s.reflectionManagers[_reflectionManagers[index]] = true;
            emit ReflectionManagerAdded(_reflectionManagers[index]);
        }
    }

    function removeReflectionManagers(address[] calldata _reflectionManagers) external override onlyOwner {
        for (uint256 index = 0; index < _reflectionManagers.length; index++) {
            s.reflectionManagers[_reflectionManagers[index]] = false;
            emit ReflectionManagerRemoved(_reflectionManagers[index]);
        }
    }

    function setTokenContract(address tokenAddress) external onlyOwner {
        poseToken = IPoseToken(tokenAddress);
    }

    function setStakingContract(address stakingAddress) external onlyOwner {
        poseStaking = IPoseStaking(stakingAddress);
    }

    function isReflectionManger(address account) public view override returns (bool) {
        return s.reflectionManagers[account];
    }

    function _incrementTokenCount(address _vault) internal {
        if (_vault != address(0)) {
            s.vaultTokenCounts[_vault] = s.vaultTokenCounts[_vault] + 1;
            emit VaultCountUpdated(_vault, s.vaultTokenCounts[_vault]);
        }
    }

    function _decrementTokenCount(address _vault) internal {
        if (s.vaultTokenCounts[_vault] > 0) {
            s.vaultTokenCounts[_vault] = s.vaultTokenCounts[_vault] - 1;
            emit VaultCountUpdated(_vault, s.vaultTokenCounts[_vault]);
        }
    }
}

