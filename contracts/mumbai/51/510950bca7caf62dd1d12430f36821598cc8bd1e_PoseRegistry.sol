/**
 * @title Staking contract that allow Pose vaults to earn POSE tokens for reflected NFTs they hold.
 */

interface IPoseStaking {
    /// @notice Emitted when a user claims POSE
    event PoseClaimed(address valut, uint256 amount);

    /// @notice Emitted when `calculateEpochRewards()` is called and rewards are calculated.
    event EpochClosed(uint256 time, uint256 totalTokenCount, uint256 rewardPerToken);

    /// @notice Emitted when 'setStakingEmissions()` is called.
    event StakingEmissionsUpdated(uint256 epochRewards);

    /**
     * -----------  WRITE -----------
     */

    /**
     * @notice Initializes the staking rewards and begins the first epoch.
     * @dev Can only be initialized once
     */
    function initializeStaking() external;

    /**
     * @notice Sets the per-epoch global POSE rewards.
     * @param _rewardsPerEpoch Amount of POSE distributed per epoch.
     */
    function setStakingEmissions(uint256 _rewardsPerEpoch) external;

    /**
     * @notice Updates the POSE token contract address.
     * @dev Should be set on deployment. Can't be in constructor due to staking contract dependency
     * in token contract.
     * @param tokenAddress Address of POSE token contract
     */
    function setTokenContract(address tokenAddress) external;

    /**
     * @notice Used to update the Pose registry address if needed.
     * @param registryAddress Address of Pose Registry contract.
     */
    function setRegistryContract(address registryAddress) external;

    /**
     * @notice Used to update the Pose Delegate contract address if needed.
     * @param delegationAddress Address of Pose Delegate contract
     */
    function setDelegationContract(address delegationAddress) external;

    /**
     * @notice Calculates the global epoch rewards per-token: `(rewardsPerEpoch * 1e18) / totalTokenCount`
     * @dev Function can be called after each epoch by anyone.
     */
    function calculateEpochRewards() external;

    /**
     * @notice Updates a vaults rewards for the NEXT epoch based on the total number of NFTs linked to it.
     * @dev This function is called by Pose Registry whenever a NFT is reflected or purged and should not need to be
     * called directly but it is public for anyone to do so if necessary.
     * @param vault Vault address
     */
    function updateVaultReward(address vault) external;

    /**
     * @notice Claims staking rewards and mints ERC20 POSE tokens to caller.
     * @dev Can only be called by the specified vault or an address it has delegated to.
     * @param vault Vault address
     */
    function claimPose(address vault) external;

    /**
     * -----------  READ -----------
     */

    /**
     * @notice Gets the current epoch rewards for a vault.
     * @param vault Vault address
     * @return Earned POSE rewards for vault.
     */
    function getVaultReward(address vault) external view returns (uint256);

    /**
     * @notice Helper function to see how per-token rewards update during an epoch as NFTs are reflected.
     * @dev The vaules returned are not final until the epoch ends but could be helpful for updating projected earnings.
     */
    function getUpcomingRewardPerToken() external view returns (uint256);
}

