    error NameToLong();
    error NotVault();
    error NotRegisterd();
    error AlreadyRegistered();
    error BadSignature();
    error AlreadyLinked();
    error NotLinked();
    error NoParams();
    error NotReflected();
    error NotManager();

    // Wallet Events
    event WalletRegistered(address wallet, address poseVault, uint256 time);
    event WalletUnregistered(address wallet, address poseVault, uint256 time);
    event VaultRegistered(address poseVault, uint256 time);
    event VaultCountUpdated(address vault, uint32 count);
    // Collection Events
    event CollectionRegistered(address collectionAddress, uint32 chainId, uint256 time);
    event CollectionPurged(address collectionAddress, uint32 chainId, uint256 time);
    // NFT Events
    event NFTReflected(address wallet, address collection, uint32 chainId, uint32 tokenId, uint256 time);
    event NFTPurged(address wallet, address collection, uint32 chainId, uint32 tokenId, uint256 time);
    // Admin Events
    event ReflectionManagerAdded(address indexed reflectionManager);
    event ReflectionManagerRemoved(address indexed reflectionManager);
}

