pragma solidity ^0.8.13;

import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "forge-std/console.sol";

contract TokenLocker is Ownable {
    uint256 private immutable CHAIN_ID;

    address public emitter;
    uint256 public lockIndex;
    struct Lock {
        address owner;
        address recipient;
        address token;
        uint256 amount;
        bool executed;
    }
    mapping(uint256 => Lock) public locks;

    event NewLock(
        address owner,
        uint256 lockId,
        uint256 toChain,
        address payToken,
        uint256 payTokenAmount,
        address buyToken,
        uint256 buyTokenAmount
    );
    event SetRecipient(
        uint256 lockId,
        address recipient,
        uint256 recipientLockId
    );
    event Executed(uint256 lockId);
    event RequestCancel(uint256 lockId);
    event Canceled(uint256 lockId);

    constructor() {
        CHAIN_ID = block.chainid;
    }

    function setEmitter(address _emitter) external onlyOwner {
        emitter = _emitter;
    }

    function hash(
        uint256 lockId,
        string memory action
    ) public view returns (bytes32) {
        // 해시할 때 체인 ID를 포함해서 다른 체인에서 사용된 서명값을 재사용하는 행위 차단
        return keccak256(abi.encodePacked(CHAIN_ID, lockId, action));
    }

    function getEthSignedHash(
        bytes32 messageHash
    ) public pure returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    "\x19Ethereum Signed Message:\n32",
                    messageHash
                )
            );
    }

    /// @param action CLAIM 또는 CANCEL
    function checkSign(
        string memory action,
        uint256 lockId,
        bytes32 digest,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public view {
        require(digest == hash(lockId, action), "invalid digest");

        console.logBytes32(getEthSignedHash(digest));

        address signer = ecrecover(getEthSignedHash(digest), v, r, s);
        require(signer == emitter, "invalid signer");
    }

    function create(
        uint256 toChain,
        address payToken,
        uint256 payTokenAmount,
        address buyToken,
        uint256 buyTokenAmount
    ) external payable {
        uint256 lockId = lockIndex++;
        // unchecked {
        //     lockId = lockIndex++;
        // }

        locks[lockId] = Lock({
            owner: _msgSender(),
            recipient: address(0),
            token: payToken,
            amount: payTokenAmount,
            executed: false
        });

        if (payToken == address(0)) {
            require(
                msg.value == payTokenAmount,
                "AtomicSwap: not enought token"
            );
        } else {
            IERC20(payToken).transfer(address(this), payTokenAmount);
        }

        emit NewLock(
            _msgSender(),
            lockId,
            toChain,
            payToken,
            payTokenAmount,
            buyToken,
            buyTokenAmount
        );
    }

    function setRecipient(
        uint256 lockId,
        address recipient,
        uint256 recipientLockId
    ) external {
        require(locks[lockId].owner == _msgSender(), "only owner");
        require(locks[lockId].recipient == address(0), "already set");

        locks[lockId].recipient = recipient;

        emit SetRecipient(lockId, recipient, recipientLockId);
    }

    /// @notice 주문 실행. 주문의 실행하기 위해서는 실행에 필요한 sign 필요.
    function execute(
        uint256 lockId,
        bytes32 digest,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external {
        require(lockId < lockIndex, "invalid Lock ID");
        require(locks[lockId].executed == false, "already executed");

        checkSign("EXECUTE", lockId, digest, v, r, s);

        address token = locks[lockId].token;
        if (token == address(0)) {
            payable(locks[lockId].recipient).transfer(locks[lockId].amount);
        } else {
            IERC20(token).transfer(
                locks[lockId].recipient,
                locks[lockId].amount
            );
        }

        locks[lockId].executed = true;

        emit Executed(lockId);
    }

    function requesetCancel(uint256 lockId) external {
        emit RequestCancel(lockId);
    }

    /// @notice 주문 취소. 주문의 취소하기 위해서는 취소에 필요한 sign 필요.
    function cancel(
        uint256 lockId,
        bytes32 digest,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external {
        require(lockId < lockIndex, "invalid Lock ID");
        require(locks[lockId].executed == false, "already executed");

        checkSign("CANCEL", lockId, digest, v, r, s);

        address token = locks[lockId].token;
        if (token == address(0)) {
            payable(locks[lockId].owner).transfer(locks[lockId].amount);
        } else {
            IERC20(token).transfer(locks[lockId].owner, locks[lockId].amount);
        }

        locks[lockId].executed = true;

        emit Canceled(lockId);
    }
}

