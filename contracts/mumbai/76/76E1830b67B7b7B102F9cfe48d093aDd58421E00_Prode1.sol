pragma solidity >=0.8.0;

import {LibProde} from "../libraries/LibProde.sol";
import {LibDiamond} from "../libraries/LibDiamond.sol";
import {LibReentrancy} from "../libraries/LibReentrancy.sol";
import {ERC20} from "../openzeppelin/token/ERC20/ERC20.sol";
import "hardhat/console.sol";

contract Prode1 {
    uint256 public constant DAYS_TO_CLAIM_PRIZE = 30;
    //30d*24h*60m*60s
    uint256 public constant SECONDS_TO_CLAIM_PRIZE =
        DAYS_TO_CLAIM_PRIZE * 24 * 60 * 60;

    modifier onlyOwner() {
        LibDiamond.enforceIsContractOwner();
        _;
    }

    modifier nonReentrancy() {
        LibReentrancy.nonReentrantBefore();
        _;
        LibReentrancy.nonReentrantAfter();
    }

    function min(uint256 a, uint256 b) internal pure returns (uint256) {
        return a < b ? a : b;
    }

    function getAllTournaments()
        external
        view
        returns (LibProde.TournamentDTO[] memory result)
    {
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        mapping(uint64 => LibProde.Tournament) storage currentTournaments = bs
            .tournaments;
        result = new LibProde.TournamentDTO[](bs.numTournaments);
        for (uint64 i = 1; i <= bs.numTournaments; i++) {
            result[i - 1] = LibProde.getTournamentDTO(
                currentTournaments[i],
                true
            );
        }
    }

    function getTournament(
        uint64 tournamentId
    ) external view returns (LibProde.TournamentDTO memory) {
        return
            LibProde.getTournamentDTO(
                LibProde._getTournament(tournamentId),
                false
            );
    }

    function getWinnerBets(
        uint64 tournamentId
    ) external view returns (LibProde.WinnerBetDTO[] memory winnerBets) {
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        require(
            tournamentId > 0 && tournamentId <= bs.numTournaments,
            "Tournament doesn't exist"
        );
        LibProde.WinnerBet[] storage _winnerBets = bs
            .tournaments[tournamentId]
            .winnerBets;
        if (_winnerBets.length > 0) {
            winnerBets = new LibProde.WinnerBetDTO[](_winnerBets.length);
            for (uint256 i = 0; i < _winnerBets.length; i++) {
                LibProde.Bet storage bet = bs.tournamentBets[tournamentId][
                    _winnerBets[i].id
                ];
                LibProde.Score storage betScore = bs.tournamentBetScores[
                    tournamentId
                ][_winnerBets[i].id];
                winnerBets[i].id = bet.id;
                winnerBets[i].owner = bet.owner;
                winnerBets[i].userId = bet.userId;
                winnerBets[i].timestamp = bet.timestamp;
                // Score
                winnerBets[i].resultPoints = betScore.resultPoints;
                winnerBets[i].goalsPoints = betScore.goalsPoints;
                winnerBets[i].bonusPoints = betScore.bonusPoints;
                winnerBets[i].score = betScore.score;
                // WinnerBet
                winnerBets[i].prize = _winnerBets[i].prize;
                winnerBets[i].claimed = _winnerBets[i].claimed;
            }
        }
        return winnerBets;
    }

    function getUserBets(
        uint64 tournamentId,
        address userAddress,
        uint16 userId
    ) external view returns (LibProde.Bet[] memory) {
        LibProde.ProdeStorage storage ps = LibProde.prodeStorage();
        require(
            tournamentId > 0 && tournamentId <= ps.numTournaments,
            "Tournament doesn't exist"
        );

        // If there's a userId, use that
        mapping(uint64 => uint16[]) storage tournamentBetIds = userId > 0
            ? ps.betsByUserId[userId]
            : ps.bets[userAddress];

        uint16[] storage betsIds = tournamentBetIds[tournamentId];

        LibProde.Bet[] memory bets = new LibProde.Bet[](betsIds.length);

        for (uint256 i = 0; i < betsIds.length; i++) {
            bets[i] = ps.tournamentBets[tournamentId][betsIds[i] - 1];
        }

        return bets;
    }

    // When claiming fiat prizes only the contract owner can claim it and then send the fiat to the winner
    function claimPrizeFiat(
        uint64 tournamentId,
        uint16 userId,
        address erc20address
    ) external payable onlyOwner nonReentrancy {
        claimPrize(tournamentId, userId, erc20address);
    }

    function claimPrizeCrypto(
        uint64 tournamentId,
        address erc20address
    ) external payable nonReentrancy {
        claimPrize(tournamentId, 0, erc20address);
    }

    function claimPrize(
        uint64 tournamentId,
        uint16 userId,
        address erc20address
    ) internal {
        erc20address = LibProde.getStableAddress(erc20address);
        LibProde.ProdeStorage storage ps = LibProde.prodeStorage();
        require(
            tournamentId > 0 && tournamentId <= ps.numTournaments,
            "Tournament doesn't exist"
        );
        LibProde.Tournament storage tournament = ps.tournaments[tournamentId];
        LibProde.WinnerBet[] storage winnerBets = tournament.winnerBets;
        ERC20 ierc20 = ERC20(erc20address);
        uint256 balance = ierc20.balanceOf(address(this));
        uint256 amountToTransfer = 0;
        for (uint256 i = 0; i < winnerBets.length; i++) {
            LibProde.Bet memory bet = ps.tournamentBets[tournamentId][
                winnerBets[i].id - 1
            ];
            if (bet.owner == msg.sender && bet.userId == userId) {
                require(
                    winnerBets[i].claimed == false,
                    "You already claimed your prize, piyuelo ;)"
                );
                require(
                    !isPrizeExpired(tournament.dateFinished),
                    "You didn't claim your prize within the days you had to do it and now it's expired, sorry :("
                );
                // make the transfer
                uint256 prize = winnerBets[i].prize;
                amountToTransfer += prize;
                console.log(
                    "Tournament pot: ",
                    tournament.pot,
                    "Winner prize: ",
                    prize
                );
                winnerBets[i].claimed = true;
            }
        }
        require(
            amountToTransfer > 0,
            "You have nothing to claim, keep participating :)"
        );
        require(
            balance >= amountToTransfer,
            "Ups, contract has insufficient balance to pay you. Please contact [emailÂ protected]"
        );
        console.log("Sending ", amountToTransfer, ierc20.symbol(), msg.sender);
        ierc20.transfer(msg.sender, amountToTransfer);
    }

    function getUnclaimedFees() external view onlyOwner returns (uint256) {
        LibProde.ProdeStorage storage ps = LibProde.prodeStorage();
        return ps.unclaimedFees;
    }

    function claimTournamentFees(
        address erc20address
    ) external payable onlyOwner nonReentrancy {
        erc20address = LibProde.getStableAddress(erc20address);
        LibProde.ProdeStorage storage ps = LibProde.prodeStorage();
        require(ps.unclaimedFees > 0, "There's nothing to claim");

        ERC20 ierc20 = ERC20(erc20address);
        uint256 balance = ierc20.balanceOf(address(this));
        console.log("Contract ERC20 balance:", balance);
        require(
            balance >= ps.unclaimedFees,
            "Ups, contract has insufficient balance to pay you :)"
        );
        console.log("Sending ", ps.unclaimedFees, ierc20.symbol(), msg.sender);
        ierc20.transfer(msg.sender, ps.unclaimedFees);
        ps.unclaimedFees = 0;
    }

    function getUnclaimedPrizes() external view onlyOwner returns (uint256) {
        LibProde.ProdeStorage storage ps = LibProde.prodeStorage();
        return ps.unclaimedPrizes;
    }

    function isPrizeExpired(uint256 dateFinished) internal view returns (bool) {
        console.log(
            "Prize dateFinished ==>",
            (dateFinished),
            " |  + SECONDS_TO_CLAIM_PRIZE==>",
            SECONDS_TO_CLAIM_PRIZE
        );

        console.log(
            "Prize expiration date ==>",
            (dateFinished + SECONDS_TO_CLAIM_PRIZE),
            " | Current date==>",
            block.timestamp
        );
        return (dateFinished + SECONDS_TO_CLAIM_PRIZE) < block.timestamp;
    }

    // This function asumes tournament[i+1].dateFinished > tournament[i].dateFinished
    function calculateUnclaimedPrizes() external onlyOwner {
        LibProde.ProdeStorage storage ps = LibProde.prodeStorage();
        require(
            ps.numTournaments > ps.lastTournamendIdWithPrizesExpired,
            "Nothing to calculate"
        );
        uint256 totalUnclaimedPrizes = 0;
        uint64 lastTournamendIdWithPrizesExpired = ps
            .lastTournamendIdWithPrizesExpired;
        for (
            uint64 i = lastTournamendIdWithPrizesExpired;
            i < ps.numTournaments;
            i++
        ) {
            LibProde.Tournament storage tournament = ps.tournaments[i];
            if (
                tournament.status == LibProde.TournamentStatus.FINISHED &&
                isPrizeExpired(tournament.dateFinished)
            ) {
                lastTournamendIdWithPrizesExpired = i + 1;
                for (uint j = 0; j < tournament.winnerBets.length; j++) {
                    if (!tournament.winnerBets[j].claimed) {
                        totalUnclaimedPrizes += tournament.winnerBets[j].prize;
                    }
                }
            } else {
                // Just check finished & claim expired tournaments
                break;
            }
        }
        if (totalUnclaimedPrizes > 0)
            ps.unclaimedPrizes += totalUnclaimedPrizes;
        if (
            lastTournamendIdWithPrizesExpired >
            ps.lastTournamendIdWithPrizesExpired
        )
            ps
                .lastTournamendIdWithPrizesExpired = lastTournamendIdWithPrizesExpired;
    }

    function assignUnclaimedPrizesToTournament(
        uint256 amountToAssign,
        uint64 tournamentId
    ) external onlyOwner nonReentrancy {
        LibProde.ProdeStorage storage ps = LibProde.prodeStorage();
        require(
            tournamentId > 0 && tournamentId <= ps.numTournaments,
            "Tournament doesn't exist"
        );
        require(
            amountToAssign > 0 && amountToAssign <= ps.unclaimedPrizes,
            "We don't have that amount to assign :)"
        );
        LibProde.Tournament storage tournament = ps.tournaments[tournamentId];
        require(
            tournament.status == LibProde.TournamentStatus.PENDING,
            "Tournament should be in PENDING state"
        );
        tournament.pot += amountToAssign;
        ps.unclaimedPrizes -= amountToAssign;
    }
}

