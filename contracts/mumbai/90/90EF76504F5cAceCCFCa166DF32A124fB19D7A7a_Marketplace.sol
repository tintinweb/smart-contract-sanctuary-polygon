pragma solidity ^0.8.4;

// Str1 + Str2
function strcat(
    string memory str1,
    string memory str2
) pure returns (string memory) {
    return string(abi.encodePacked(str1, str2));
}

// Retrun small value
function min(uint256 a, uint256 b) pure returns (uint256) {
    if (a <= b) {
        return a;
    } else {
        return b;
    }
}

function contains(address[] memory array, address value) pure returns (bool) {
    for (uint256 i = 0; i < array.length; i++) {
        if (array[i] == value) {
            return true;
        }
    }
    return false;
}

function getIndex(uint[] memory arr, uint target) pure returns (int) {
    for (uint i = 0; i < arr.length; i++) {
        if (arr[i] == target) {
            return int(i); // cast to int since -1 is used as a sentinel value
        }
    }
    return -1;
}

