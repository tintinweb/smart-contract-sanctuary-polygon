pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";

contract EnumerableNFT is
    ERC721Upgradeable,
    ERC721EnumerableUpgradeable,
    ERC721URIStorageUpgradeable
{
    // STATE VARS
    mapping(uint256 => uint256) internal _tokenOnSaleIdToPrice;

    // Overrides
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal override(ERC721Upgradeable, ERC721EnumerableUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function _burn(
        uint256 tokenId
    ) internal override(ERC721Upgradeable, ERC721URIStorageUpgradeable) {
        super._burn(tokenId);
    }

    function tokenURI(
        uint256 tokenId
    )
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    // Return all NFTs
    function getAllNfts()
        public
        view
        returns (
            uint256[] memory ids,
            string[] memory uris,
            address[] memory owners,
            uint256[] memory prices
        )
    {
        uint256 nftsNum = totalSupply();

        uint256[] memory tokenIds = new uint256[](nftsNum);
        string[] memory tokenUris = new string[](nftsNum);
        address[] memory tokenOwners = new address[](nftsNum);
        uint256[] memory tokenPrices = new uint256[](nftsNum);

        for (uint256 i = 0; i < nftsNum; i++) {
            uint256 id = tokenByIndex(i);
            tokenIds[i] = id;
            tokenUris[i] = tokenURI(id);
            tokenOwners[i] = ownerOf(id);
            tokenPrices[i] = _tokenOnSaleIdToPrice[id];
        }

        return (tokenIds, tokenUris, tokenOwners, tokenPrices);
    }

    // Return NFTs owned by signer
    function getNftsOwned()
        public
        view
        returns (
            uint256[] memory ids,
            string[] memory uris,
            address[] memory owners,
            uint256[] memory prices
        )
    {
        uint256 nftsOwnedNum = balanceOf(tx.origin);

        uint256[] memory tokenIds = new uint256[](nftsOwnedNum);
        string[] memory tokenUris = new string[](nftsOwnedNum);
        address[] memory tokenOwners = new address[](nftsOwnedNum);
        uint256[] memory tokenPrices = new uint256[](nftsOwnedNum);

        for (uint256 i = 0; i < nftsOwnedNum; i++) {
            uint256 id = tokenOfOwnerByIndex(tx.origin, i);
            tokenIds[i] = id;
            tokenUris[i] = tokenURI(id);
            tokenOwners[i] = tx.origin;
            tokenPrices[i] = _tokenOnSaleIdToPrice[id];
        }

        return (tokenIds, tokenUris, tokenOwners, tokenPrices);
    }

    // Returns number of tokens on sale
    function getNftsOnSaleNum() public view returns (uint256) {
        uint256 nftsOnSaleNum = 0;
        uint256 totalTokens = totalSupply();
        for (uint256 i = 0; i < totalTokens; i++) {
            if (_tokenOnSaleIdToPrice[tokenByIndex(i)] != 0) {
                nftsOnSaleNum += 1;
            }
        }
        return nftsOnSaleNum;
    }

    // Return NFTs on sale
    function getNftsOnSale()
        public
        view
        returns (
            uint256[] memory ids,
            string[] memory uris,
            address[] memory owners,
            uint256[] memory prices
        )
    {
        uint256 nftsOnSaleNum = getNftsOnSaleNum();
        uint256[] memory tokenIds = new uint256[](nftsOnSaleNum);
        string[] memory tokenUris = new string[](nftsOnSaleNum);
        address[] memory tokenOwners = new address[](nftsOnSaleNum);
        uint256[] memory tokenPrices = new uint256[](nftsOnSaleNum);

        uint256 index = 0;
        for (uint256 i = 0; i < totalSupply(); i++) {
            if (_tokenOnSaleIdToPrice[tokenByIndex(i)] != 0) {
                uint256 id = tokenByIndex(i);
                tokenIds[index] = id;
                tokenUris[index] = tokenURI(id);
                tokenOwners[index] = ownerOf(id);
                tokenPrices[index] = _tokenOnSaleIdToPrice[id];
                index += 1;
            }
        }

        return (tokenIds, tokenUris, tokenOwners, tokenPrices);
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/IERC721Metadata.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "hardhat/console.sol";

import "./NFT.sol";
import "./Utils.sol";

contract Marketplace is ReentrancyGuard, Ownable {
    // LIBRARY AUGS
    using Strings for uint;
    using Counters for Counters.Counter;
    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.UintSet;

    // STRUCTS
    struct Collection {
        address payable author;
        address contractAddress;
        string title;
        string symbol;
        string description;
        uint totalItems;
    }
    struct CollectionOutput {
        address payable author;
        address contractAddress;
        string title;
        string symbol;
        string description;
        uint totalItems;
    }
    struct Sale {
        address payable author;
        address contractAddress;
        uint256 tokenId;
        uint256 price;
        uint256 offerCount;
    }

    struct SaleOutput {
        uint saleId;
        address author;
        address contractAddress;
        uint tokenId;
        uint price;
        string tokenUri;
    }

    struct Offer {
        address payable offerAddress;
        uint price;
    }

    struct OfferOutput {
        uint offerId;
        address author;
        address contractAddress;
        address offerAddress;
        uint tokenId;
        uint price;
        string tokenUri;
    }
    struct Auction {
        address payable author;
        address contractAddress;
        uint tokenId;
        address highestBidder;
        uint highestBid;
        uint endTime;
        bool isActive;
    }
    struct AuctionOutput {
        uint auctionId;
        address author;
        address contractAddress;
        uint tokenId;
        string tokenUri;
        address highestBidder;
        uint highestBid;
        uint endTime;
        bool isActive;
    }

    // EVENTS
    event CollectionCreated(
        address indexed author,
        address indexed contractAddress
    );
    event SaleCreated(
        address indexed author,
        address indexed contractAddress,
        uint indexed tokenId,
        uint price
    );
    event SaleCanceled(
        address indexed author,
        address indexed contractAddress,
        uint indexed tokenId
    );
    event SaleSuccessed(
        address indexed newOwner,
        address indexed contractAddress,
        uint indexed tokenId
    );
    event OfferCreated(
        address indexed author,
        address indexed contractAddress,
        address indexed newOwner,
        uint tokenId,
        uint price
    );
    event OfferCanceled(
        address indexed author,
        address indexed contractAddress,
        uint indexed tokenId
    );
    event OfferAccepted(
        address indexed newOwner,
        address indexed contractAddress,
        uint indexed tokenId
    );
    event AuctionCreated(
        address indexed author,
        address indexed contractAddress,
        uint indexed tokenId,
        uint auctionId
    );
    event AuctionNewBid(
        address indexed contractAddress,
        address indexed bidder,
        uint amount
    );
    event AuctionResolved(
        address indexed contractAddress,
        uint indexed tokenId,
        address indexed winner
    );
    event CommissionReceivedByMarketplace(uint commission);

    // Mappings
    mapping(address => Collection) private collections;
    mapping(uint => Collection) private launchpadCollections;
    mapping(uint => Sale) private sales;
    mapping(uint => mapping(uint => Offer)) private offersById; // offer Array per saleId
    mapping(address => mapping(uint => uint256)) private offersByUser; // Each offer price for per saleId per user
    mapping(address => mapping(uint => uint256)) private offersByUserIndex; // Each offer price for per saleId per user
    // mapping(uint => Offer) private offers;
    mapping(uint => Auction) private auctions;

    // STATE VARS
    address public collectionImplementation; // Address of Collection implementation contract

    uint public collectionCount; // Current Collection number
    uint public saleCount; // Current Sale number
    uint public offerCount;
    uint public auctionCount; // Current Auction number

    // MODIFIERS

    // Constructor
    constructor(address _collectionImplementation) payable {
        collectionImplementation = _collectionImplementation;
    }

    // Function to create an NFT minimal proxy contract
    function createCollection(
        string calldata _title,
        string calldata _symbol,
        string calldata _description,
        string calldata _uri,
        uint256 _price,
        uint256 _supply,
        bool _isLaunchpad
    ) external nonReentrant {
        address clonedContractAddress = Clones.clone(collectionImplementation);
        NFT(clonedContractAddress).initialize(
            _title,
            _symbol,
            _description,
            _uri,
            _price,
            _supply,
            payable(address(this))
        );
        if (_isLaunchpad) {
            collectionCount = collectionCount + 1;
            launchpadCollections[collectionCount] = Collection(
                payable(msg.sender),
                clonedContractAddress,
                _title,
                _symbol,
                _description,
                _supply
            );
        } else {
            require(collections[msg.sender].contractAddress == address(0), "You cannot have more than 2 collections!");
            collections[msg.sender] = Collection(
                payable(msg.sender),
                clonedContractAddress,
                _title,
                _symbol,
                _description,
                _supply
            );
        }
        emit CollectionCreated(msg.sender, clonedContractAddress);
    }
    
    // Function to get collection by user address
    function getCollectionByAddress( address _userAddress) public view returns(Collection memory){
        require(_userAddress != address(0), "Collection:Invalid user address!");
        return collections[_userAddress];
    }


    // Function to get launchpad collections by id
    function getCollectionById(
        uint _collectionId
    ) public view returns (Collection memory) {
        require(
            _collectionId <= collectionCount,
            "Collection: Invaild collection ID!"
        );
        return launchpadCollections[_collectionId];
    }

    // get all collections
    function getCollections() public view returns (Collection[] memory) {
        Collection[] memory _collections = new Collection[](collectionCount);
        for (
            uint collectionId = 1;
            collectionId <= collectionCount;
            collectionId++
        ) {
            Collection memory collection = getCollectionById(collectionId);
            _collections[collectionId - 1] = collection;
        }
        return _collections;
    }

    // Create Sale
    function createSale(
        address _contractAddress,
        uint _tokenId,
        uint _price
    ) external nonReentrant {
        IERC721 nftContract = IERC721(_contractAddress);
        require(
            nftContract.ownerOf(_tokenId) == msg.sender ||
                nftContract.isApprovedForAll(
                    nftContract.ownerOf(_tokenId),
                    msg.sender
                ),
            "Sale: Not authorized to transfer NFT!"
        );
        require(_price > 0, "INVALID PRICE: must bigger than zero!");
        nftContract.transferFrom(msg.sender, address(this), _tokenId);
        saleCount = saleCount + 1;
        sales[saleCount] = Sale(
            payable(msg.sender),
            _contractAddress,
            _tokenId,
            _price,
            0
        );
        emit SaleCreated(msg.sender, _contractAddress, _tokenId, _price);
    }

    function cancelSale(uint _saleId) external nonReentrant {
        Sale memory sale = getSaleById(_saleId);
        require(sale.author == msg.sender, "Sale: Invalid sale author");
        IERC721 nftContract = IERC721(sale.contractAddress);
        nftContract.transferFrom(address(this), msg.sender, sale.tokenId);
        if (_saleId != saleCount) sales[_saleId] = getSaleById(saleCount);
        delete sales[saleCount];
        saleCount = saleCount - 1;
        emit SaleCanceled(msg.sender, sale.contractAddress, sale.tokenId);
    }

    function buySale(uint _saleId) external payable nonReentrant {
        Sale memory sale = getSaleById(_saleId);
        require(msg.value >= sale.price, "Sale: Insufficient deposit amount");
        IERC721 nftContract = IERC721(sale.contractAddress);
        nftContract.transferFrom(address(this), msg.sender, sale.tokenId);
        bool success;
        (success, ) = payable(sale.author).call{value: msg.value}("");
        require(success, "Sale: Cound not send to money.");
        if (_saleId != saleCount) sales[_saleId] = getSaleById(saleCount);
        delete sales[saleCount];
        saleCount = saleCount - 1;
        emit SaleSuccessed(msg.sender, sale.contractAddress, sale.tokenId);
    }

    // Create Offer
    function createOffer(
        uint _saleId,
        uint _price
    ) external nonReentrant {
        require(_price > 0, "INVALID PRICE: must bigger than zero!");
        uint256 count = sales[_saleId].offerCount;
        if(offersByUser[_msgSender()][_saleId] == 0){          
            sales[_saleId].offerCount = count + 1;
            offersByUserIndex[_msgSender()][_saleId] = count + 1;
        }
        uint256 index = offersByUserIndex[_msgSender()][_saleId];
        offersById[_saleId][index] = Offer(payable(_msgSender()), _price);
        offersByUser[_msgSender()][_saleId] = _price;
        emit OfferCreated(
            sales[_saleId].author,
            msg.sender,
            sales[_saleId].contractAddress,
            sales[_saleId].tokenId,
            _price
        );
    }

    function cancelOffer(uint _offerId) external nonReentrant {
        require(offersByUser[_msgSender()][_offerId] > 0, "offer: Invalid offer author");
        // refund to the offer maker
        uint256 index = offersByUserIndex[_msgSender()][_offerId];
        uint256 count = sales[_offerId].offerCount;
        if (count != index) {
            offersById[_offerId][index] = offersById[_offerId][count];
            offersByUser[_msgSender()][_offerId] = 0;
            offersByUserIndex[_msgSender()][_offerId] = 0;
        }
        delete offersById[_offerId][count];
        sales[_offerId].offerCount = count - 1;
        emit OfferCanceled(msg.sender, sales[_offerId].contractAddress, sales[_offerId].tokenId);
    }

    function acceptOffer(uint _offerId) external payable nonReentrant {
        Sale memory sale = getSaleById(_offerId);
        require(msg.value >= sale.price, "Sale: Insufficient deposit amount");
        IERC721 nftContract = IERC721(sale.contractAddress);
        nftContract.transferFrom(address(this), msg.sender, sale.tokenId);
        bool success;
        (success, ) = payable(sale.author).call{value: msg.value}("");
        require(success, "Sale: Cound not send to money.");
        if (_offerId != saleCount) sales[_offerId] = getSaleById(saleCount);
        delete sales[saleCount];
        saleCount = saleCount - 1;
        emit OfferAccepted(msg.sender, sale.contractAddress, sale.tokenId);
    }

    // Function to get sale by id
    function getSaleById(uint _saleId) public view returns (Sale memory) {
        require(_saleId <= saleCount, "Sale: Invaild sale ID!");
        return sales[_saleId];
    }

    // Function to get all sales
    function getSales() public view returns (SaleOutput[] memory) {
        SaleOutput[] memory _sales = new SaleOutput[](saleCount);
        for (uint i = 1; i <= saleCount; i++) {
            Sale memory sale = getSaleById(i);
            IERC721Metadata nftContract = IERC721Metadata(sale.contractAddress);
            string memory tokenUri = nftContract.tokenURI(sale.tokenId);
            _sales[i - 1] = SaleOutput(
                i,
                sale.author,
                sale.contractAddress,
                sale.tokenId,
                sale.price,
                tokenUri
            );
        }
        return _sales;
    }

    // Function to get offer by id
    function getOfferById(uint _offerId) public view returns (Offer[] memory) {
        require(_offerId <= saleCount, "Offer: Invaild offer ID!");
        uint256 offerLength = sales[_offerId].offerCount;
        Offer[] memory offerById = new Offer[](offerLength);
        for(uint i = 1; i <= offerLength; i ++){
            offerById[i - 1] = offersById[_offerId][i];
        }
        return offerById;
    }

    // create Auction
    function createAuction(
        address _contractAddress,
        uint _tokenId,
        uint _startingBid,
        uint _endTime
    ) external nonReentrant {
        IERC721 nftContract = IERC721(_contractAddress);
        nftContract.transferFrom(msg.sender, address(this), _tokenId);

        auctionCount = auctionCount + 1;

        auctions[auctionCount] = Auction(
            payable(msg.sender),
            _contractAddress,
            _tokenId,
            address(0),
            _startingBid,
            _endTime,
            true
        );

        emit AuctionCreated(
            msg.sender,
            _contractAddress,
            _tokenId,
            auctionCount
        );
    }

    function auctionBid(uint _auctionId) external payable {
        Auction memory auction = getAuctionById(_auctionId);
        require(auction.isActive, "Auction: Auction not is Activated");
        require(auction.endTime >= block.timestamp, "Auction: Auction ended");
        require(
            msg.value > auction.highestBid,
            strcat("Auction: Must be at least ", auction.highestBid.toString())
        );
        if (auction.highestBidder != address(0)) {
            payable(auction.highestBidder).transfer(auction.highestBid);
        }
        auction.highestBidder = msg.sender;
        auction.highestBid = msg.value;

        auctions[_auctionId] = auction;

        emit AuctionNewBid(
            auction.contractAddress,
            auction.highestBidder,
            auction.highestBid
        );
    }

    function auctionResolve(uint _auctionId) external {
        Auction memory auction = getAuctionById(_auctionId);
        require(auction.isActive, "Auction: Auction is not Activated");
        require(auction.endTime >= block.timestamp, "Auction: Auction ended");
        IERC721 nftContract = IERC721(auction.contractAddress);
        if (auction.highestBidder != address(0)) {
            nftContract.transferFrom(
                address(this),
                auction.highestBidder,
                auction.tokenId
            );

            emit AuctionResolved(
                auction.contractAddress,
                auction.tokenId,
                auction.highestBidder
            );
        } else {
            nftContract.transferFrom(
                address(this),
                auction.author,
                auction.tokenId
            );
            emit AuctionResolved(
                auction.contractAddress,
                auction.tokenId,
                auction.author
            );
        }
        auction.isActive = false;
        if (_auctionId != auctionCount)
            auctions[_auctionId] = getAuctionById(auctionCount);
        delete auctions[auctionCount];
        auctionCount = auctionCount - 1;
    }

    // Function to get auction by id
    function getAuctionById(
        uint _auctionId
    ) public view returns (Auction memory) {
        require(_auctionId <= auctionCount, "Auction: Invaild auction ID!");
        return auctions[_auctionId];
    }

    function getAuctions() public view returns (AuctionOutput[] memory) {
        AuctionOutput[] memory _auctions = new AuctionOutput[](auctionCount);
        for (uint auctionId = 1; auctionId <= auctionCount; auctionId++) {
            Auction memory auction = getAuctionById(auctionId);
            IERC721Metadata nftContract = IERC721Metadata(
                auction.contractAddress
            );
            string memory tokenUri = nftContract.tokenURI(auction.tokenId);
            _auctions[auctionId - 1] = AuctionOutput(
                auctionId,
                auction.author,
                auction.contractAddress,
                auction.tokenId,
                tokenUri,
                auction.highestBidder,
                auction.highestBid,
                auction.endTime,
                auction.isActive
            );
        }
        return _auctions;
    }

    receive() external payable {
        emit CommissionReceivedByMarketplace(msg.value);
    }
}

