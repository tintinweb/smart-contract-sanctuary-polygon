import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";

contract IncomeStream is ERC721Holder {
    address erc721;
    uint256 nftid;

    event Forwarded(address indexed from, address indexed to, uint amount);
    uint256 public constant version = 1;
    constructor(address _erc721, uint256 _nftid) {
        require(_erc721 != address(0), "NOT_A_ZERO_ADDRESS!");
        erc721 = _erc721;
        nftid = _nftid;
    }

    /// @dev Function receive native token when someone send it to this contract and forward it to NFT holder
    receive() external payable {
        IERC721 currentErc721 = IERC721(erc721);
        emit Forwarded(msg.sender, currentErc721.ownerOf(nftid), msg.value);
        (bool os, ) = payable(currentErc721.ownerOf(nftid)).call{
            value: msg.value
        }("");
        require(os, "FAILED");
    }

    /// @dev Function withdraw erc20
    /// @param _erc20 _erc20 contract address.
    /// @param _amount Amount erc20 want to withdraw.
    function withdrawErc20(address _erc20, uint256 _amount) public {
        require(
            _amount > 0,
            "INVALID_AMOUNT"
        );
        require(
            IERC20(_erc20).balanceOf(address(this)) >= _amount,
            "CONTRACT_INSUFFICIENT_FUND"
        );
        IERC721 currentErc721 = IERC721(erc721);
        require(
            currentErc721.ownerOf(nftid) == msg.sender,
            "INVALID_OWNER"
        );
        bool success = IERC20(_erc20).transfer(currentErc721.ownerOf(nftid), _amount);
        require(success, "WITHDRAW_ERC20_SUCCESS");
    }

    /// @dev Function withdraw erc721
    /// @param _erc721 erc721 contract address.
    /// @param _nftid NFT ID
    function withdrawErc721(address _erc721, uint256 _nftid) public {
        require(
            IERC721(_erc721).ownerOf(_nftid) == address(this),
            "CONTRACT_IS_NOT_NFT_OWNER"
        );
        IERC721 currentErc721 = IERC721(erc721);
        require(
            currentErc721.ownerOf(nftid) == msg.sender,
            "INVALID_OWNER"
        );
        IERC721(_erc721).transferFrom(address(this), currentErc721.ownerOf(nftid), _nftid);
    }
}

