// SPDX-License-Identifier: MIT
pragma solidity ^0.8.17;

import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/ConfirmedOwner.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "../../extensions/v1/ERC721/GMG_ERC721_BaseInterface.sol";
import "../../extensions/v1/ERC1155/GMG_ERC1155_BaseInterface.sol";

/// @title MintManager
/// @notice Picks a Nissan Car variant based on Chainlink's VRF given an user has made a minting request and paid its cost.
/// @dev Requires creating a subscription for Chainlink's VRF service prior to the deployment.
contract MintManager is
    VRFConsumerBaseV2,
    ConfirmedOwner,
    ReentrancyGuard
{

    using SafeERC20 for IERC20;
    using Strings for uint256;

    /// @notice Details of a request
    struct Request {
        address requestOwner;
        address[] variants;
        uint256 tokenStandard;
        uint256 amount;
        address paymentToken;
    }

    /// @dev Chainlink coordinator address wrapped in 'VRFCoordinatorV2Interface'.
    VRFCoordinatorV2Interface COORDINATOR;

    /// @dev Chainlink VRF subscription_Id pointing to the subscription responsible with LINK funds consumed by this contract.
    uint64 public s_subscriptionId;

    /// @notice A mapping from request_id to its request details.
    mapping(uint256 => Request) public s_requestIdToRequest;

    /// @dev The gas lane to use, which specifies the maximum gas price to bump to.
    /// @dev For a list of available gas lanes on each network, see https://docs.chain.link/docs/vrf/v2/subscription/supported-networks/#configurations
    bytes32 public keyHash;

    /// @dev How many blocks the VRF service waits before writing a fulfillment to the chain
    /// @dev The default is 3, it can be set higher.
    uint16 public requestConfirmations = 3;

    /// @dev The limit for how much gas to use for the callback request to the contract’s fulfillRandomWords() function.
    uint32 public callbackGasLimit = 2500000;

    /// @notice Emits an event when the Chainlink coordinator successfully fulfills a minting request.
    event NFTMintedAll(
        uint256 requestId,
        address indexed owner,
        uint256[] randomWords,
        uint256 amount,
        address[] indexed variants,
        uint256[] indexed tokenIds
    );

    /// @notice Emits an event when a user successfully creates a minting request.
    event NFTRequest(
        uint256 requestId,
        address indexed requester,
        address[] indexed variants,
        uint256 amount
    );

    /// @notice Initializes the neccesary Chainlink VRF, tokenURI and checkoutWallet fields.
    constructor(
        uint64 _subscriptionId,
        address _vrfCoordinator,
        bytes32 _keyHash
    )
        VRFConsumerBaseV2(_vrfCoordinator)
        ConfirmedOwner(msg.sender)
    {
        COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
        s_subscriptionId = _subscriptionId;
        keyHash = _keyHash;
    }

    /// @notice Creates a minting request of the given variant and amount that will later be fulfilled by Chainlink's VRF service.
    /// @dev Reverts if the available variant minting supply is zero.
    /// @dev Reverts if the requested minting amount is zero.
    /// @dev Reverts if the requested minting amount is greater than the available variant supply.
    /// @dev Reverts if the provided payment token is not set for the given variant.
    /// @dev Reverts if the Chainlink VRF subscription is not set and funded.
    /// @param _amount The amount of NFTs to requested to be minted.
    /// @param _paymentToken The payment token used to pay for the request.
    function requestRandomNFTs(
        address[] memory _variants,
        uint256 _tokenStandard,
        uint32 _amount,
        address _paymentToken
    ) external {

        require(_amount > 0 && _amount <= 10, "Amount must be greater than zero and less or equal to 10");
        require(_tokenStandard == 1 || _tokenStandard == 2, "Invalid type: ERC721 = 1 | ERC1155 = 2");
        require(getVariantsTotalRemainingSupply(_variants) >= _amount, "Not enough NFTs left to fulfill request");

        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            uint32(1)
        );

        s_requestIdToRequest[requestId] = Request({
            requestOwner: msg.sender,
            variants: _variants,
            tokenStandard: _tokenStandard,
            amount: _amount,
            paymentToken: _paymentToken
        });

        emit NFTRequest(requestId, msg.sender, _variants, _amount);

    }

    /// @notice Mints the corresponding amount of NFTs of a certain variant specified in the details of the request.
    function requestNFTs(
        address _variant,
        uint256 _tokenStandard,
        uint256 _tokenType,
        uint256 _amount,
        address _paymentToken
    ) external {

        require(_tokenStandard == 1 || _tokenStandard == 2, "Invalid type: ERC721 = 1 | ERC1155 = 2");

        uint256[] memory noRandomWords = new uint256[](0);
        uint256[] memory allTokenIds = new uint256[](_amount);
        address[] memory allVariants = new address[](_amount);

        for (uint256 index = 0; index < _amount; index++) {
            allVariants[index] = address(_variant);
            GMG_ERC721_BaseInterface variant = GMG_ERC721_BaseInterface(_variant);
            require(variant.getRandom() == false, "Variant only available as random mint");
            require(getVariantRemainingSupply(address(variant)) >= 1);
            uint256 tokenCost = variant.getTokenCost(_paymentToken);
            require(tokenCost > 0, "Token cost not set");
            address checkoutWallet = variant.getCheckoutWallet();
            require(checkoutWallet != address(0), "Checkout wallet not set");
            IERC20 token = IERC20(_paymentToken);
            SafeERC20.safeTransferFrom(
                token,
                msg.sender,
                checkoutWallet,
                tokenCost
            );
            uint256 mintedId = variant.mintType(msg.sender, _tokenType);
            allTokenIds[index] = mintedId;
        }

        emit NFTMintedAll(0, msg.sender, noRandomWords, _amount, allVariants, allTokenIds);

    }

    /// @notice Mints the corresponding amount of NFTs of a certain variant specified in the details of the request tracked by the provided id.
    /// @notice The color codes having different rarities are assigned to the minted NFTs based on Chainlink's provably-fair and verifiable randomness.
    /// @dev Reverts if the callbackGasLimit set at the time of the minting request is smaller than the actual gas exectuion cost.
    /// @param _requestId Id of the request that will get fulfilled.
    /// @param _randomWords Array of random numbers of length equal to the minting amount of the request.
    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory _randomWords
    ) internal override {

        Request memory request = s_requestIdToRequest[_requestId];
        address requestOwner = request.requestOwner;
        address[] memory variants = request.variants;
        uint256[] memory allTokenIds = new uint256[](uint256(request.amount));
        address[] memory allVariants = new address[](uint256(request.amount));
        uint256 tokenCost;
        address checkoutWallet;
        address randomVariant;

        // ALL VARIANTS ARE ERC721
        if(request.tokenStandard == 1) {
            if(variants.length == 1) {
                // Use randomWords for color only
                for (uint256 i = 0; i < uint256(request.amount); i++) {
                    allVariants[i] = variants[0];
                    GMG_ERC721_BaseInterface variant = GMG_ERC721_BaseInterface(variants[0]);
                    require(getVariantRemainingSupply(variants[0]) >= 1);
                    tokenCost = variant.getTokenCost(request.paymentToken);
                    require(tokenCost > 0, "Token cost not set");
                    checkoutWallet = variant.getCheckoutWallet();
                    require(checkoutWallet != address(0), "Checkout wallet not set");
                    IERC20 token = IERC20(request.paymentToken);
                    SafeERC20.safeTransferFrom(
                        token,
                        requestOwner,
                        checkoutWallet,
                        tokenCost
                    );
                    uint256 randomKeccak = uint256(keccak256(abi.encodePacked(_randomWords[0], block.timestamp, block.number, i)));
                    uint256 mintedId = variant.mintRandomType(requestOwner, randomKeccak);
                    allTokenIds[i] = mintedId;
                }
            } else {
                // Use randomWords for both variant and color
                for (uint256 i = 0; i < uint256(request.amount); i++) {
                    uint256 randomKeccak = uint256(keccak256(abi.encodePacked(_randomWords[0], block.timestamp, block.number, i)));
                    randomVariant = pickRandomVariant(variants, randomKeccak);
                    require(randomVariant != address(0), "Not enough NFTs left to fulfill request");
                    allVariants[i] = randomVariant;
                    GMG_ERC721_BaseInterface variant = GMG_ERC721_BaseInterface(randomVariant);
                    require(getVariantRemainingSupply(randomVariant) >= 1);
                    tokenCost = variant.getTokenCost(request.paymentToken);
                    require(tokenCost > 0, "Token cost not set");
                    checkoutWallet = variant.getCheckoutWallet();
                    require(checkoutWallet != address(0), "Checkout wallet not set");
                    IERC20 token = IERC20(request.paymentToken);
                    SafeERC20.safeTransferFrom(
                        token,
                        requestOwner,
                        checkoutWallet,
                        tokenCost
                    );
                    uint256 mintedId = variant.mintRandomType(requestOwner, randomKeccak);
                    allTokenIds[i] = mintedId;
                }
            }
        }

        emit NFTMintedAll(_requestId, requestOwner, _randomWords, uint256(request.amount), allVariants, allTokenIds);

    }

    /// @notice Based on a list of NFTs and an _amount of them to mint, gets the max allowance needed in the _paymentToken
    function getMaxAllowance(address[] memory _variants, uint256 _amount, address _paymentToken) public view returns (uint256) {
        uint256 maxVariantPrice = 0;
        for (uint256 index = 0; index < _variants.length; index++) {
            address _variant = _variants[index];
            GMG_ERC721_BaseInterface variant = GMG_ERC721_BaseInterface(_variant);
            uint256 variantPrice = variant.getTokenCost(_paymentToken);
            require(variantPrice > 0, "Price not set for this payment token");
            if(variantPrice > maxVariantPrice) maxVariantPrice = variantPrice;
        }
        uint256 maxAllowance = maxVariantPrice * _amount;
        return maxAllowance;
    }

    /// @notice Gets the supply that's left for an available variant.
    function getVariantRemainingSupply(address _variant) public view returns (uint256) {
        GMG_ERC721_BaseInterface variant = GMG_ERC721_BaseInterface(_variant);
        return variant.maxSupply() - variant.totalSupply();
    }

    /// @notice Gets the total remaining supply for a list of variants.
    function getVariantsTotalRemainingSupply(address[] memory _variants) public view returns (uint256) {
        uint256 currentSum = 0;
        for(uint256 i = 0; i < _variants.length; i++) {
            currentSum += getVariantRemainingSupply(_variants[i]);
        }
        return currentSum;
    }

    /// @notice Calculates the variant of the next minted NFT based on the given random number.
    /// @param _randomNumber Source of randomness in the calculation of the variant.
    function pickRandomVariant(address[] memory _variants, uint256 _randomNumber) public view returns (address) {

        uint256 randomIndex = _randomNumber % _variants.length;
        if(getVariantRemainingSupply(_variants[randomIndex]) > 0) {
            return _variants[randomIndex];
        } else {
            randomIndex = randomIndex + 1;
            for (uint256 index = 1; index < _variants.length - 1; index++) {
                if(randomIndex > _variants.length - 1) randomIndex = 0;
                if(getVariantRemainingSupply(_variants[randomIndex]) > 0)
                    return _variants[randomIndex];
                else
                    randomIndex = randomIndex + index;
            }
        }

        // If no variants left, returns ZERO_ADDRESS
        return address(0);

    }

    /// @notice Sets a new value for the callback gas limit.
    /// @param _callbackGasLimit the new value of the callback gas limit.
    function setCallbackGasLimit(uint32 _callbackGasLimit) public onlyOwner nonReentrant {
        callbackGasLimit = _callbackGasLimit;
    }

    /// @notice Sets a new value for the subscription_Id.
    /// @param _subscriptionId the new value of the subscription_Id.
    function setSubscriptionId(uint64 _subscriptionId) public onlyOwner nonReentrant {
        s_subscriptionId = _subscriptionId;
    }

    /// @notice Sets a new value for the key hash.
    /// @param _keyHash the new value of key hash.
    function setKeyHash(bytes32 _keyHash) public onlyOwner nonReentrant {
        keyHash = _keyHash;
    }

    /// @notice Sets a new value for the request confirmations.
    /// @param _requestConfirmations the new value of the request confirmations.
    function setRequestConfirmations(uint16 _requestConfirmations) public onlyOwner nonReentrant {
        requestConfirmations = _requestConfirmations;
    }

}