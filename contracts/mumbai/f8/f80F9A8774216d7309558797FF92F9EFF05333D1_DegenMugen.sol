import "hardhat/console.sol";

//public - all can access
//external - Cannot be accessed internally, only externally
//internal - only this contract and contracts deriving from it can access
//private - can be accessed only from this contract

abstract contract NFTInterface {
    function ownerOf(uint256 id) external view virtual returns (address);
}

contract DegenMugen {
    NFTInterface NFTContract;

    address public owner;
    mapping(address => bool) public isAllowed;
    address public constant testAddress =
        0x2011b35B461760B3FE3ade6550d2e9aB0188597a;
    address NFTContractAddress;
    uint256 public playerCount; // Define a new state variable for tracking the player count
    string public name;

    // Open 0 , Closed 1 , Resolved 2 , Cancelled 3
    uint256 state;

    uint256 public totalBetOne;
    uint256 public totalBetTwo;
    uint256 public minimumBet;

    address payable[] public players;

    struct Player {
        uint256 amountBet;
        uint256 teamSelected;
    }

    struct Game {
        uint256 p1;
        uint256 p2;
        uint256 stage;
    }
    struct Percent {
        uint256 percentTotalFees;
        uint256 percentContractFees;
        uint256 percentP1Fees;
        uint256 percentP2Fees;
        uint256 percentStageFees;
        uint256 divideNumber;
        uint256 ninetyPercent;
    }

    struct GameAddress {
        address payable p1Address;
        address payable p2Address;
        address payable stageAddress;
    }

    // Address of the player and => the user info
    mapping(address => Player) public playerInfo;
    mapping(address => bool) public isPlayer;

    Game game;
    Percent percent;
    GameAddress gameAddress;

    event changeState(address indexed _from, uint256 _value);
    event betMade(address indexed _from, uint256 _value, uint256 _player);
    event fight(uint256 p1, uint256 p2, uint256 stage);
    event fightWinner(uint256 winner);
    event trPaid(address _to, uint256 amount);

    event debugEventStr(string _value);
    event debugEventUint(
        address indexed _from,
        string _message,
        uint256 _value
    );
    event myDebug(string value);

    constructor(address _address) {
        name = "DegenMugen v0.1.4";
        owner = msg.sender;
        state = 2;
        minimumBet = 1_000_000_000;
        NFTContractAddress = _address;
        NFTContract = NFTInterface(_address);
        //asupprimer
        addAllowedAddress(testAddress);
        initContract();
    }

    modifier onlyOwner() {
        require(
            msg.sender == owner || isAllowed[msg.sender],
            "Only owner or allowed addresses can call this function"
        );
        _;
    }

    function addAllowedAddress(address _address) public onlyOwner {
        isAllowed[_address] = true;
    }

    // Open 0 , Closed 1 , Resolved 2 , Cancelled 3

    modifier bettingOpen() {
        require(state == 0, "Betting is not Open.");
        _;
    }

    modifier bettingResolvedOrCancelled() {
        require(
            state == 2 || state == 3,
            "Betting is not Resolved or Cancelled."
        );
        _;
    }

    receive() external payable {
        console.log("APPEL RECEIVE DegenMugen \n");
    }

    fallback() external payable {
        console.log("APPEL FALLBACK DegenMugen\n");
    }

    function initContract() public {
        percent.percentTotalFees = 1000;
        percent.percentContractFees = 4000;
        percent.percentP1Fees = 3000;
        percent.percentP2Fees = 2000;
        percent.percentStageFees = 1000;
        percent.divideNumber = 10000;
        percent.ninetyPercent = 9000;
        delete players; // Delete all the players array
        totalBetOne = 0;
        totalBetTwo = 0;
    }

    function setMinimumBet(uint256 _minimumBet) external onlyOwner {
        require(_minimumBet > 0, "Minimum bet must be greater than 0");
        minimumBet = _minimumBet;
    }

    function setNFTContractAddress(address _address) external onlyOwner {
        NFTContract = NFTInterface(_address);
    }

    function getNFTOwner(uint256 id) public view returns (address) {
        return NFTContract.ownerOf(id);
    }

    function checkPlayerExists(address player) public view returns (bool) {
        return isPlayer[player];
    }

    function setState(uint256 _state) public {
        state = _state;
        emit changeState(msg.sender, _state);
    }

    function getState() public view returns (uint256) {
        return state;
    }

    function bet(uint256 amount, uint256 player) public payable bettingOpen {
        require(!checkPlayerExists(msg.sender), "Player already exists");
        require(playerCount < 128, "Maximum number of players reached"); // Check player count
        require(
            msg.value >= minimumBet,
            string(
                abi.encodePacked(
                    "Minimum bet amount not met. Minimum: ",
                    uint256ToString(minimumBet),
                    ". Sent: ",
                    uint256ToString(msg.value)
                )
            )
        );
        isPlayer[msg.sender] = true; // set the player as active
        playerInfo[msg.sender].amountBet = msg.value;
        playerInfo[msg.sender].teamSelected = player;

        players.push(payable(msg.sender));
        playerCount++; // Increment player count

        if (player == 1) {
            totalBetOne += msg.value;
        } else {
            totalBetTwo += msg.value;
        }
        emit betMade(msg.sender, amount, player);
    }

    function payOwner(
        uint256 p1Fees,
        uint256 p2Fees,
        uint256 stageFees,
        uint256 contractFees
    ) internal {
        console.log("before  NFT pay 1\n");
        emit debugEventStr("before  NFT pay 1\n");
        console.log("valeur de contractFees: %s", contractFees);
        console.log("valeur de p1Fees: %s \n", p1Fees);
        console.log("valeur de p2Fees: %s \n", p2Fees);
        console.log("valeur de stageFees: %s \n", stageFees);
        console.log("valeur de stageFees: %s \n", gameAddress.p1Address);

        sendToOwner(gameAddress.p1Address, p1Fees);
        sendToOwner(gameAddress.p2Address, p2Fees);
        sendToOwner(gameAddress.stageAddress, stageFees);
        address payable contractAddress = payable(address(this));
        sendToOwner(contractAddress, contractFees);

        console.log("THE END OF FUNCTION\n");
        emit debugEventStr("THE END OF FUNCTION\n");
    }

    function sendToOwner(address payable recipient, uint256 amount) internal {
        if (amount > 0) {
            (bool sent, ) = recipient.call{value: amount}("");
            if (sent) {
                emit trPaid(recipient, amount);
            } else {
                console.log("Failed to send Ether to recipient (sendToOwner)");
                emit debugEventStr(
                    "Failed to send Ether to recipient (sendToOwner)"
                );
            }
        }
    }

    function distributePrizes(uint256 teamWinner) public {
        address[] memory winners = getWinners(teamWinner);
        (uint256 WinnerBet, uint256 LoserBet) = getWinnerAndLoserBet(
            teamWinner
        );
        uint256 totalFees = getTotalFees(WinnerBet, LoserBet);
        emit debugEventStr("dans distributePrizes");
        distributeToWinner(winners, WinnerBet, LoserBet);
        emit debugEventStr("APRES distributeToWinner");
        calculateFeesAndDistribute(totalFees, teamWinner);
        emit debugEventStr("APRES calculateFeesAndDistribute");
    }

    function getWinners(
        uint256 teamWinner
    ) internal view returns (address[] memory) {
        address[] memory result = new address[](players.length);
        uint256 count = 0;
        for (uint256 i = 0; i < players.length; i++) {
            if (playerInfo[players[i]].teamSelected == teamWinner) {
                result[count] = players[i];
                count++;
            }
        }
        return result;
    }

    function getWinnerAndLoserBet(
        uint256 teamWinner
    ) internal view returns (uint256, uint256) {
        uint256 loserBet;
        uint256 winnerBet;

        if (teamWinner == 1) {
            loserBet = totalBetTwo;
            winnerBet = totalBetOne;
        } else {
            loserBet = totalBetOne;
            winnerBet = totalBetTwo;
        }

        loserBet = (loserBet * percent.ninetyPercent) / percent.divideNumber;
        winnerBet = (winnerBet * percent.ninetyPercent) / percent.divideNumber;

        return (winnerBet, loserBet);
    }

    function getTotalFees(
        uint256 WinnerBet,
        uint256 LoserBet
    ) internal view returns (uint256) {
        return
            ((WinnerBet + LoserBet) * percent.percentTotalFees) /
            percent.divideNumber;
    }

    function distributeToWinner(
        address[] memory winners,
        uint256 WinnerBet,
        uint256 LoserBet
    ) internal {
        for (uint256 i = 0; i < winners.length; i++) {
            if (winners[i] != address(0)) {
                uint256 amountBet = (playerInfo[winners[i]].amountBet *
                    percent.ninetyPercent) / percent.divideNumber;
                uint256 tmp = amountBet + (amountBet * LoserBet) / WinnerBet;
                (bool sent, ) = winners[i].call{value: tmp}("");
                require(sent, "Failed to send Ether to winner");
                emit trPaid(winners[i], tmp);
            }
        }
    }

    function calculateFeesAndDistribute(
        uint256 totalFees,
        uint256 teamWinner
    ) internal {
        uint256 contractFees = (totalFees * percent.percentContractFees) /
            percent.divideNumber;
        uint256 p1Fees = (totalFees * percent.percentP1Fees) /
            percent.divideNumber;
        uint256 p2Fees = (totalFees * percent.percentP2Fees) /
            percent.divideNumber;
        uint256 stageFees = (totalFees * percent.percentStageFees) /
            percent.divideNumber;
        gameAddress.p1Address = payable(getNFTOwner(game.p1));
        gameAddress.p2Address = payable(getNFTOwner(game.p2));
        gameAddress.stageAddress = payable(getNFTOwner(game.stage));
        if (game.p2 == teamWinner) {
            (p1Fees, p2Fees) = (p2Fees, p1Fees);
        }
        payOwner(p1Fees, p2Fees, stageFees, contractFees);
    }

    function AmountOne() public view returns (uint256) {
        return totalBetOne;
    }

    function AmountTwo() public view returns (uint256) {
        return totalBetTwo;
    }

    function resetAmount() public {
        totalBetOne = 0;
        totalBetTwo = 0;
    }

    function changeName(string memory newName) public {
        name = newName;
    }

    function startFight(
        uint256 p1,
        uint256 p2,
        uint256 stage
    ) public bettingResolvedOrCancelled {
        require(
            NFTContract.ownerOf(p1) != address(0),
            "Invalid NFT for player 1"
        );
        require(
            NFTContract.ownerOf(p2) != address(0),
            "Invalid NFT for player 2"
        );
        require(
            NFTContract.ownerOf(stage) != address(0),
            "Invalid NFT for stage"
        );
        game.p1 = p1;
        game.p2 = p2;
        game.stage = stage;
        emit fight(p1, p2, stage);
        setState(0);
    }

    function resetVar() internal {
        delete players; // Delete all the players array
        totalBetOne = 0;
        totalBetTwo = 0;
        playerCount = 0; // Reset player count
    }

    function endFight(uint256 winner) public onlyOwner {
        emit fightWinner(winner);
        console.log("Dans endFight\n");
        emit debugEventStr("Dans endFight\n");
        distributePrizes(winner);
        setState(2);
        resetVar();
        console.log("FIN endFight\n");
        emit debugEventStr("FIN endFight\n");
    }

    function balanceOfSC() public view returns (uint256) {
        return address(this).balance;
    }

    function uint256ToString(
        uint256 value
    ) internal pure returns (string memory) {
        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }
}

