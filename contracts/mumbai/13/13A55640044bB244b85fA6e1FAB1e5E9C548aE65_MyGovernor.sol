contract MyGovernor is Governor, GovernorCompatibilityBravo, GovernorVotes, GovernorTimelockControl, AccessControl {

    bytes32 public constant ADMIN_ROLE = keccak256('ADMIN_ROLE');
    bytes32 public constant PROPOSER_ROLE = keccak256('PROPOSER_ROLE');
    //bytes32 public constant EXECUTOR_ROLE = keccak256('EXECUTOR_ROLE');
    //bytes32 public constant CANCELLER_ROLE = keccak256('CANCELLER_ROLE');
    //IFeeDistributor private _feeDistributor;
    IVotingEscrow private _votingEscrow;
    uint private counter = 0;
    constructor(
        IVotingEscrow votingEscrow,
        //IFeeDistributor feeDistributor,
        IVotes _token,
        string memory name,
        TimelockController _timelock
    ) GovernorVotes(_token) Governor(name, votingEscrow) GovernorTimelockControl(_timelock) {
        address admin = address(this);
        _votingEscrow = votingEscrow;


        _setRoleAdmin(ADMIN_ROLE, ADMIN_ROLE);
        _setRoleAdmin(PROPOSER_ROLE, ADMIN_ROLE);
        //_setRoleAdmin(EXECUTOR_ROLE, TIMELOCK_ADMIN_ROLE);
        //_setRoleAdmin(CANCELLER_ROLE, TIMELOCK_ADMIN_ROLE);

        // self administration
        _setupRole(ADMIN_ROLE, admin);
        _setupRole(ADMIN_ROLE, msg.sender);
        // optional admin
        if (admin != address(0)) {
            _setupRole(ADMIN_ROLE, admin);
        }
        _setupRole(PROPOSER_ROLE, admin);
        _setupRole(PROPOSER_ROLE, msg.sender);
        // register proposers and cancellers
        /*for (uint256 i = 0; i < proposers.length; ++i) {
            _setupRole(PROPOSER_ROLE, proposers[i]);
            _setupRole(CANCELLER_ROLE, proposers[i]);
        }

        // register executors
        for (uint256 i = 0; i < executors.length; ++i) {
            _setupRole(EXECUTOR_ROLE, executors[i]);
        }*/
    }

    // The functions below are overrides required by Solidity.
    /**
     * @dev See {IGovernor-state}.
     */
    function state(uint256 proposalId) public view override(Governor, IGovernor, GovernorTimelockControl) returns (ProposalState) {
        return super.state(proposalId);
    }

    /**
     * @dev See {IGovernorCompatibilityBravo-propose}.
     */
     function propose(
        address[] memory targets,
        uint256[] memory values,
        bytes[] memory calldatas,
        string memory description,
        address token
      
    ) public onlyRole(PROPOSER_ROLE) returns (uint256) {
      
        return propose(targets, values, calldatas, description, token, 0, 0,0);
    }

    function propose(
        address[] memory targets,
        uint256[] memory values,
        bytes[] memory calldatas,
        string memory description,
        address token,
        uint256 ,
        uint256 ,
        uint256
    ) public override(Governor, IGovernor, GovernorCompatibilityBravo) onlyRole(PROPOSER_ROLE) returns (uint256) {
        address[] memory new_targets = new address[](targets.length + 1);
        uint256[] memory new_values = new uint256[](values.length + 1);
        bytes[] memory new_calldatas = new bytes[](calldatas.length + 1);
        

        for (uint256 i = 0; i < targets.length ; ++i) {
            new_targets[i] = targets[i ];
            new_values[i] = values[i ];
            new_calldatas[i] = calldatas[i ];
        }

        //setwinner
        new_targets[new_targets.length - 1] = address(this);
        new_values[new_values.length - 1] = 0;
        new_calldatas[new_calldatas.length - 1] = abi.encodeWithSignature('set_Winner()');

       
        return super.propose(new_targets, new_values, new_calldatas, description, token, 0, 0, counter++);
    }

    /**
     * @dev See {IGovernorCompatibilityBravo-execute}.
     */

    function execute(uint256 proposalId) public payable virtual override(GovernorCompatibilityBravo) onlyRole(ADMIN_ROLE) {
        super.execute(proposalId);
    }

    /**
     * @dev See {IGovernorCompatibilityBravo-cancel}.
     */

    function cancel(uint256 proposalId) public virtual override onlyRole(ADMIN_ROLE) {
        

        _cancel(proposalId);
    }

    function _cancel(
      
        uint256 proposalId
    ) internal override(Governor, GovernorTimelockControl) returns (uint256) {
        return super._cancel(proposalId);
    }

    /**
     * @dev See {IGovernorCompatibilityBravo-_executor}.
     */

    function _executor() internal view override(Governor, GovernorTimelockControl) returns (address) {
        return super._executor();
    }

    /**
     * @dev See {IGovernor-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId) public view override(Governor, IERC165, GovernorTimelockControl,AccessControl) onlyRole(ADMIN_ROLE) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    /**
     * @dev See {IGovernor-queue}.
     */

    function queue(
        address[] memory targets,
        uint256[] memory values,
        bytes[] memory calldatas,
        bytes32 descriptionHash,
        uint256 proposalId
    ) public override(IGovernorTimelock, GovernorTimelockControl) onlyRole(ADMIN_ROLE) returns (uint256) {
        return super.queue(targets, values, calldatas, descriptionHash, proposalId);
    }

  
}

