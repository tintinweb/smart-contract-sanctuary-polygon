/// @title Handles ERC20 deposits and deposit executions.
/// @author Router Protocol.
/// @notice This contract is intended to be used with the Bridge contract.
contract VoyagerExecuteHandler is
    Initializable,
    UUPSUpgradeable,
    ContextUpgradeable,
    AccessControlUpgradeable,
    ReentrancyGuardUpgradeable,
    PausableUpgradeable
{
    using SafeMathUpgradeable for uint256;
    using SafeERC20Upgradeable for IERC20Upgradeable;

    bytes32 public constant RESOURCE_SETTER = keccak256("RESOURCE_SETTER");
    bytes32 public constant PAUSER = keccak256("PAUSER");

    address public _oneSplitAddress;
    IHandlerReserve public _reserve;
    IGateway public gatewayContract;
    IUsdcMinter public usdcMinter;
    address public _WETH;
    string routerBridge;

    // keccak256(abi.encode(sourceChainId)) + nonce => blockNumber
    mapping(bytes32 => mapping(uint64 => uint256)) public executeRecord;

    // token contract address => is reserve
    mapping(address => bool) private _contractWhitelist;

    // token contract address => is burnable
    mapping(address => bool) private _burnList;

    /**--------------------EVENTS------------------- */

    // executeType =
    // 0 for only reserve token transfers
    // 1 for reserve token transfers and execute
    // 2 for only non-reserve token transfers
    // 3 for non-reserve token transfers and execute
    // 4 for only LP token transfers
    // 5 for LP token transfers and execute
    event Execute(
        uint8 executeType,
        bytes32 indexed sourceChainIdBytes,
        uint64 indexed depositNonce,
        address settlementToken,
        uint256 settlementAmount,
        address recipient
    );

    modifier isGateway() {
        _isGateway();
        _;
    }

    modifier isNotAlreadyExecuted(bytes32 srcChainIdBytes, uint64 nonce) {
        require(executeRecord[srcChainIdBytes][nonce] == 0, "already executed");
        _;
    }

    function _isGateway() private view {
        require(msg.sender == address(gatewayContract), "Only gateway");
    }

    function initialize(address WETH) external initializer {
        __Context_init_unchained();
        __AccessControl_init_unchained();
        __ReentrancyGuard_init_unchained();

        _WETH = WETH;
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(RESOURCE_SETTER, msg.sender);
        _setupRole(PAUSER, msg.sender);
    }

    function _authorizeUpgrade(address newImplementation) internal override onlyRole(DEFAULT_ADMIN_ROLE) {}

    /// @notice Pauses execution on the handler.
    /// @notice Only callable by an address that currently has the PAUSER role.
    function pause() public onlyRole(PAUSER) whenNotPaused {
        _pause();
    }

    /// @notice Unpauses execution on the handler.
    /// @notice Only callable by an address that currently has the PAUSER role.
    function unpause() public onlyRole(PAUSER) whenPaused {
        _unpause();
    }

    /// @notice Function to set execute record
    /// @param  sourceChainIdBytes chainIdBytes for source chain
    /// @param  nonce nonce for the transaction
    function setExecuteRecord(bytes32 sourceChainIdBytes, uint64 nonce) internal {
        executeRecord[sourceChainIdBytes][nonce] = block.number;
    }

    /// @notice Function to set router bridge address
    /// @notice Only RESOURCE_SETTER can call this function
    /// @param  bridge address of bridge on router chain
    function setRouterBridge(string memory bridge) external onlyRole(RESOURCE_SETTER) {
        routerBridge = bridge;
    }

    /// @notice Function to set gateway contract address
    /// @notice Only RESOURCE_SETTER can call this function
    /// @param  gateway address of gateway contract
    function setGatewayContract(address gateway) external onlyRole(RESOURCE_SETTER) {
        gatewayContract = IGateway(gateway);
    }

    /// @notice Function used to set reserve handler
    /// @notice Only RESOURCE_SETTER can call this function
    /// @param reserve address of the reserve handler
    function setReserve(address reserve) external onlyRole(RESOURCE_SETTER) {
        _reserve = IHandlerReserve(reserve);
    }

    /// @notice Function used to set USDC Minter handler
    /// @notice Only RESOURCE_SETTER can call this function
    /// @param _usdcMinter address of the USDC Minter Handler contract
    function setUsdcMinter(address _usdcMinter) external onlyRole(RESOURCE_SETTER) {
        usdcMinter = IUsdcMinter(_usdcMinter);
    }

    /// @notice Sets onesplit address.
    /// @param contractAddress Address of OneSplit contract
    function setOneSplitAddress(address contractAddress) external onlyRole(RESOURCE_SETTER) {
        require(contractAddress != address(0), "address != 0");
        _oneSplitAddress = address(contractAddress);
    }

    /// @notice Function to fetch if a token is whitelisted for swaps
    /// @param tokenAddress Address of token contract.
    /// @return true if whitelisted
    function isContractWhitelisted(address tokenAddress) public view returns (bool) {
        return _contractWhitelist[tokenAddress];
    }

    /// @notice Function to fetch if a token is burnable
    /// @param tokenAddress Address of token contract.
    /// @return true if burnable
    function isBurnable(address tokenAddress) public view returns (bool) {
        return _burnList[tokenAddress];
    }

    /// @notice Function to whitelist a token for swaps
    /// @dev Can only be called by the RESOURCE_SETTER
    /// @param contractAddress Address of contract to be called when a deposit is made and a deposited is executed.
    function setResource(address contractAddress, bool isResource) public onlyRole(RESOURCE_SETTER) {
        _setResource(contractAddress, isResource);
    }

    /// @notice First verifies {contractAddress} is whitelisted, then sets {_burnList}[{contractAddress}]
    /// to true.
    /// @dev Can only be called by the bridge
    /// @param contractAddress Address of contract to be used when making or executing deposits.
    /// @param status Boolean flag to change burnable status.
    function setBurnable(address contractAddress, bool status) public onlyRole(RESOURCE_SETTER) {
        _setBurnable(contractAddress, status);
    }

    function setLiquidityPool(address contractAddress, address lpAddress) public onlyRole(RESOURCE_SETTER) {
        _reserve._setLiquidityPool(contractAddress, lpAddress);
        _contractWhitelist[lpAddress] = true;
        _burnList[lpAddress] = true;
    }

    /// @notice Sets liquidity pool owner for an existing LP.
    /// @dev Can only be set by the bridge
    /// @param oldOwner Address of the old owner of LP
    /// @param newOwner Address of the new owner for LP
    /// @param tokenAddress Address of ERC20 token
    /// @param lpAddress Address of LP.
    function setLiquidityPoolOwner(
        address oldOwner,
        address newOwner,
        address tokenAddress,
        address lpAddress
    ) public onlyRole(RESOURCE_SETTER) {
        _reserve._setLiquidityPoolOwner(oldOwner, newOwner, tokenAddress, lpAddress);
    }

    /// @notice Sets a resource.
    /// @param contractAddress Address of ERC20 token
    function _setResource(address contractAddress, bool isResource) internal {
        require(contractAddress != address(0), "contract address can't be zero");
        _contractWhitelist[contractAddress] = isResource;
    }

    /// @notice Sets a resource burnable.
    /// @param contractAddress Address of ERC20 token
    /// @param status true for burnable, false for not burnable
    function _setBurnable(address contractAddress, bool status) internal {
        require(_contractWhitelist[contractAddress], "provided contract is not whitelisted");
        _burnList[contractAddress] = status;
    }

    /// @notice Function to get chain ID bytes
    /// @param  _chainId chain Id of the chain
    function getChainIdBytes(string memory _chainId) public pure returns (bytes32) {
        return keccak256(abi.encode(_chainId));
    }

    /// @notice Function to get Router Brigde address
    function getRouterBridge() external view returns (string memory) {
        return routerBridge;
    }

    /// @notice Function to handle the request for execution received from Router Chain
    /// @param requestSender Address of the sender of the transaction on the source chain.
    /// @param packet Packet coming from the router chain.
    function iReceive(
        string memory requestSender,
        bytes memory packet,
        string memory // srcChainId
    ) external isGateway nonReentrant whenNotPaused {
        require(keccak256(bytes(requestSender)) == keccak256(bytes(routerBridge)), "only Voyager middleware");

        uint8 txType = abi.decode(packet, (uint8));
        bytes32 srcChainIdBytes;
        uint64 depositNonce;
        address settlementToken;
        uint256 settlementAmount;
        IExecute.UsdcStruct memory usdcStruct;

        if (txType == 0) {
            IExecute.ReserveExecuteSwapInfo memory executeDetails;
            (, srcChainIdBytes, executeDetails, usdcStruct) = abi.decode(
                packet,
                (uint8, bytes32, IExecute.ReserveExecuteSwapInfo, IExecute.UsdcStruct)
            );

            depositNonce = executeDetails.depositNonce;
            _handleUsdcMint(usdcStruct);
            (settlementToken, settlementAmount) = executeProposalForReserveToken(srcChainIdBytes, executeDetails);

            emit Execute(0, srcChainIdBytes, depositNonce, settlementToken, settlementAmount, executeDetails.recipient);
        }

        if (txType == 1) {
            IExecute.ReserveExecuteSwapInfo memory executeDetails;
            IExecute.ArbitraryInstruction memory arbitraryInstruction;
            (, srcChainIdBytes, executeDetails, arbitraryInstruction, usdcStruct) = abi.decode(
                packet,
                (uint8, bytes32, IExecute.ReserveExecuteSwapInfo, IExecute.ArbitraryInstruction, IExecute.UsdcStruct)
            );
            depositNonce = executeDetails.depositNonce;

            _handleUsdcMint(usdcStruct);
            (settlementToken, settlementAmount) = executeProposalForReserveToken(srcChainIdBytes, executeDetails);
            _executeArbitraryInstruction(arbitraryInstruction, srcChainIdBytes, settlementToken, settlementAmount);

            emit Execute(1, srcChainIdBytes, depositNonce, settlementToken, settlementAmount, executeDetails.recipient);
        }

        if (txType == 2) {
            IExecute.NonReserveExecuteSwapInfo memory executeDetails;
            (, srcChainIdBytes, executeDetails, usdcStruct) = abi.decode(
                packet,
                (uint8, bytes32, IExecute.NonReserveExecuteSwapInfo, IExecute.UsdcStruct)
            );

            depositNonce = executeDetails.depositNonce;
            _handleUsdcMint(usdcStruct);
            (settlementToken, settlementAmount) = executeProposalForNonReserveToken(srcChainIdBytes, executeDetails);

            emit Execute(2, srcChainIdBytes, depositNonce, settlementToken, settlementAmount, executeDetails.recipient);
        }

        if (txType == 3) {
            IExecute.NonReserveExecuteSwapInfo memory executeDetails;
            IExecute.ArbitraryInstruction memory arbitraryInstruction;
            (, srcChainIdBytes, executeDetails, arbitraryInstruction, usdcStruct) = abi.decode(
                packet,
                (uint8, bytes32, IExecute.NonReserveExecuteSwapInfo, IExecute.ArbitraryInstruction, IExecute.UsdcStruct)
            );

            depositNonce = executeDetails.depositNonce;

            _handleUsdcMint(usdcStruct);
            (settlementToken, settlementAmount) = executeProposalForNonReserveToken(srcChainIdBytes, executeDetails);
            _executeArbitraryInstruction(arbitraryInstruction, srcChainIdBytes, settlementToken, settlementAmount);

            emit Execute(3, srcChainIdBytes, depositNonce, settlementToken, settlementAmount, executeDetails.recipient);
        }

        if (txType == 4) {
            IExecute.LPExecuteSwapInfo memory executeDetails;
            (, srcChainIdBytes, executeDetails) = abi.decode(packet, (uint8, bytes32, IExecute.LPExecuteSwapInfo));
            depositNonce = executeDetails.depositNonce;

            (settlementToken, settlementAmount) = executeProposalForLPToken(srcChainIdBytes, executeDetails);

            emit Execute(4, srcChainIdBytes, depositNonce, settlementToken, settlementAmount, executeDetails.recipient);
        }

        if (txType == 5) {
            IExecute.LPExecuteSwapInfo memory executeDetails;
            IExecute.ArbitraryInstruction memory arbitraryInstruction;
            (, srcChainIdBytes, executeDetails, arbitraryInstruction) = abi.decode(
                packet,
                (uint8, bytes32, IExecute.LPExecuteSwapInfo, IExecute.ArbitraryInstruction)
            );

            depositNonce = executeDetails.depositNonce;

            (settlementToken, settlementAmount) = executeProposalForLPToken(srcChainIdBytes, executeDetails);
            _executeArbitraryInstruction(arbitraryInstruction, srcChainIdBytes, settlementToken, settlementAmount);

            emit Execute(5, srcChainIdBytes, depositNonce, settlementToken, settlementAmount, executeDetails.recipient);
        }

        setExecuteRecord(srcChainIdBytes, depositNonce);
    }

    function _handleUsdcMint(IExecute.UsdcStruct memory usdcStruct) internal {
        if (usdcStruct.isUsdcDeposit) {
            usdcMinter.mintUsdcToReserveHandler(usdcStruct.message, usdcStruct.attestation);
        }
    }

    function _executeArbitraryInstruction(
        IExecute.ArbitraryInstruction memory arbitraryInstruction,
        bytes32 srcChainIdBytes,
        address settlementToken,
        uint256 settlementAmount
    ) internal {
        (bool success, ) = arbitraryInstruction.destContractAddress.call(
            abi.encodeWithSelector(
                0x141c9069, // voyagerReceive(bytes memory sourceSenderAddress, bytes32 srcChainIdBytes, bytes memory data, address settlementToken, uint256 settlementAmount)
                arbitraryInstruction.sourceSenderAddress,
                srcChainIdBytes,
                arbitraryInstruction.data,
                settlementToken,
                settlementAmount
            )
        );
        require(success, "unsuccessful arbitrary call");
    }

    function executeProposalForReserveToken(
        bytes32 srcChainIdBytes,
        IExecute.ReserveExecuteSwapInfo memory swapDetails
    )
        internal
        isNotAlreadyExecuted(srcChainIdBytes, swapDetails.depositNonce)
        returns (address settlementToken, uint256 settlementAmount)
    {
        require(_contractWhitelist[swapDetails.destTokenAddress], "token not whitelisted");
        (settlementToken, settlementAmount) = handleExecuteForReserveToken(swapDetails);
    }

    /// @notice Handles execution of transfer for reserve tokens
    /// @param swapDetails swapInfo struct for the swap details
    /// @return destinationTokenAddress and destinationTokenAmount
    function handleExecuteForReserveToken(
        IExecute.ReserveExecuteSwapInfo memory swapDetails
    ) internal returns (address, uint256) {
        if (_burnList[swapDetails.destTokenAddress]) {
            _reserve.mintERC20(swapDetails.destTokenAddress, swapDetails.recipient, swapDetails.destTokenAmount);
        } else {
            uint256 reserveBalance = IERC20Upgradeable(swapDetails.destTokenAddress).balanceOf(address(_reserve));
            if (reserveBalance < swapDetails.destTokenAmount) {
                _reserve.mintWrappedERC20(
                    swapDetails.destTokenAddress,
                    swapDetails.recipient,
                    swapDetails.destTokenAmount
                );
                return (_reserve._contractToLP(swapDetails.destTokenAddress), swapDetails.destTokenAmount);
            } else {
                if (swapDetails.destTokenAddress == _WETH && swapDetails.isDestNative) {
                    _reserve.withdrawWETH(_WETH, swapDetails.destTokenAmount);
                    _reserve.safeTransferETH(swapDetails.recipient, swapDetails.destTokenAmount);
                } else {
                    _reserve.releaseERC20(
                        swapDetails.destTokenAddress,
                        swapDetails.recipient,
                        swapDetails.destTokenAmount
                    );
                }
            }
        }

        return (swapDetails.destTokenAddress, swapDetails.destTokenAmount);
    }

    function executeProposalForLPToken(
        bytes32 srcChainIdBytes,
        IExecute.LPExecuteSwapInfo memory swapDetails
    )
        internal
        isNotAlreadyExecuted(srcChainIdBytes, swapDetails.depositNonce)
        returns (address settlementToken, uint256 settlementAmount)
    {
        require(_contractWhitelist[swapDetails.destStableTokenAddress], "token not whitelisted");

        require(
            _reserve._contractToLP(swapDetails.destStableTokenAddress) == swapDetails.destTokenAddress,
            "invalid data for LP execute"
        );

        handleExecuteForLPToken(swapDetails);
        settlementToken = swapDetails.destTokenAddress;
        settlementAmount = swapDetails.destTokenAmount;
    }

    /// @notice Handles execution of transfer for LP tokens
    /// @param swapDetails swapInfo struct for the swap details
    function handleExecuteForLPToken(IExecute.LPExecuteSwapInfo memory swapDetails) internal {
        _reserve.mintWrappedERC20(
            swapDetails.destStableTokenAddress,
            swapDetails.recipient,
            swapDetails.destTokenAmount
        );
    }

    function executeProposalForNonReserveToken(
        bytes32 srcChainIdBytes,
        IExecute.NonReserveExecuteSwapInfo memory swapDetails
    )
        internal
        isNotAlreadyExecuted(srcChainIdBytes, swapDetails.depositNonce)
        returns (address settlementToken, uint256 settlementAmount)
    {
        require(_contractWhitelist[swapDetails.destStableTokenAddress], "token not whitelisted");
        require(
            !(_reserve._contractToLP(swapDetails.destStableTokenAddress) == address(swapDetails.destTokenAddress)) &&
                !(swapDetails.destTokenAddress == swapDetails.destStableTokenAddress),
            "invalid data for non-reserve execute"
        );

        (settlementToken, settlementAmount) = handleExecuteForNonReserveToken(swapDetails);
    }

    /// @notice Handles execution of transfer for non-reserve tokens
    /// @param swapDetails swapInfo struct for the swap details
    /// @return destinationStableTokenAddress and destinationStableTokenAmount
    function handleExecuteForNonReserveToken(
        IExecute.NonReserveExecuteSwapInfo memory swapDetails
    ) internal returns (address, uint256) {
        uint256 returnAmount;
        if (_burnList[swapDetails.destStableTokenAddress]) {
            if (
                (swapDetails.path.length > 2) &&
                (swapDetails.path[swapDetails.path.length - 1] != swapDetails.destTokenAddress)
            ) {
                _reserve.mintERC20(
                    swapDetails.destStableTokenAddress,
                    swapDetails.recipient,
                    swapDetails.destStableTokenAmount
                );
                return (swapDetails.destStableTokenAddress, swapDetails.destStableTokenAmount);
            }
            _reserve.mintERC20(swapDetails.destStableTokenAddress, _oneSplitAddress, swapDetails.destStableTokenAmount);
        } else {
            uint256 reserveBalance = IERC20Upgradeable(swapDetails.destStableTokenAddress).balanceOf(address(_reserve));
            if (reserveBalance < swapDetails.destStableTokenAmount) {
                _reserve.mintWrappedERC20(
                    swapDetails.destStableTokenAddress,
                    swapDetails.recipient,
                    swapDetails.destStableTokenAmount
                );
                return (_reserve._contractToLP(swapDetails.destStableTokenAddress), swapDetails.destStableTokenAmount);
            } else {
                if (
                    (swapDetails.path.length > 2) &&
                    (swapDetails.path[swapDetails.path.length - 1] != swapDetails.destTokenAddress)
                ) {
                    _reserve.releaseERC20(
                        swapDetails.destStableTokenAddress,
                        swapDetails.recipient,
                        swapDetails.destStableTokenAmount
                    );
                    return (swapDetails.destStableTokenAddress, swapDetails.destStableTokenAmount);
                }
                _reserve.releaseERC20(
                    swapDetails.destStableTokenAddress,
                    _oneSplitAddress,
                    swapDetails.destStableTokenAmount
                );
            }
        }
        if (swapDetails.path.length > 2) {
            //solhint-disable avoid-low-level-calls
            (bool success, bytes memory returnData) = address(_reserve).call(
                abi.encodeWithSelector(
                    0x2214e13b, // swapMulti(address,address[],uint256,uint256,uint256[],bytes[])
                    _oneSplitAddress,
                    swapDetails.path,
                    swapDetails.destStableTokenAmount,
                    swapDetails.destTokenAmount,
                    swapDetails.flags,
                    swapDetails.dataTx
                )
            );
            if (success) {
                returnAmount = abi.decode(returnData, (uint256));
            } else {
                require(
                    IOneSplitWrap(_oneSplitAddress).withdraw(
                        swapDetails.destStableTokenAddress,
                        swapDetails.recipient,
                        swapDetails.destStableTokenAmount
                    )
                );
                return (swapDetails.destStableTokenAddress, swapDetails.destStableTokenAmount);
            }
        } else {
            (bool success, bytes memory returnData) = address(_reserve).call(
                abi.encodeWithSelector(
                    0xda041a85, //swap(address,address,address,uint256,uint256,uint256,bytes)
                    _oneSplitAddress,
                    swapDetails.destStableTokenAddress,
                    swapDetails.destTokenAddress,
                    swapDetails.destStableTokenAmount,
                    swapDetails.destTokenAmount,
                    swapDetails.flags[0],
                    swapDetails.dataTx[0]
                )
            );
            if (success) {
                returnAmount = abi.decode(returnData, (uint256));
            } else {
                require(
                    IOneSplitWrap(_oneSplitAddress).withdraw(
                        swapDetails.destStableTokenAddress,
                        swapDetails.recipient,
                        swapDetails.destStableTokenAmount
                    )
                );
                return (swapDetails.destStableTokenAddress, swapDetails.destStableTokenAmount);
            }
        }
        if (swapDetails.destTokenAddress == _WETH && swapDetails.isDestNative) {
            _reserve.withdrawWETH(_WETH, returnAmount);
            _reserve.safeTransferETH(swapDetails.recipient, returnAmount);
        } else {
            _reserve.releaseERC20(swapDetails.destTokenAddress, swapDetails.recipient, returnAmount);
        }

        return (swapDetails.destTokenAddress, returnAmount);
    }

    /// @notice Used to manually release ERC20 tokens from ERC20Safe
    /// @param tokenAddress Address of token contract to release.
    /// @param recipient Address to release tokens to.
    /// @param amount The amount of ERC20 tokens to release.
    function withdraw(address tokenAddress, address recipient, uint256 amount) external onlyRole(DEFAULT_ADMIN_ROLE) {
        IERC20Upgradeable(tokenAddress).transfer(recipient, amount);
    }
}

