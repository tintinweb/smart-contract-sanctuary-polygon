//SPDX-License-Identifier: Unlicense
pragma solidity ^0.8.0;

import "hardhat/console.sol";

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";

contract RewardPool is Ownable, ReentrancyGuard {
    
    using SafeERC20 for IERC20;
    using SafeCast for uint256;
    using SafeCast for int256;

    /* ========== STATE VARIABLES ========== */

    IERC20 CODE;
    
    // The precision factor
    uint256 public PRECISION_FACTOR = 10 ** 12;

    uint256 public PRECISION_FACTOR_FE = 10 ** 6;

    uint256 public currentTotalStaked; 
    uint256 public maxTotalStake = 100000 * (10**18); // total stake of flexible and longterm

    uint256 public minStake;
    uint256 public maxStake;

    uint128 public noWithdrawFeeTime;
    int256 public aNumber;
    int256 public bNumber;
    int256 public cNumber;
    uint128 public fixedAPR;

    address public rewardDistributionAddress;
    address public penaltyWallet;

    bool public isEmergency;

    mapping (address => UserInfo) public userInfo; // Info of each user that stakes tokens in each pool
    mapping (address => bool) public admin; // Admin of Contract

    struct UserInfo {
        uint256 amount; // How many staked tokens the user has provided
        uint128 timeStake; // Last action time of user
        uint256 totalRewardEarned; // Total reward earned
    }
    
    event Deposit(address indexed _user, uint256 amount);
    event Withdraw(address indexed _user, uint256 amount, uint256 feeAmount);
    event SetWithdrawTime(uint256 duration);
    event SetMinMax(uint256 min, uint256 max);
    event SetMaxTotalStake(uint256 _maxTotalStake);
    event Claim(address indexed _user, uint256 pending);
    event SetWithdrawFeeParam(int256 a, int256 b, int256 c);
    // event RemoveAdmin(address indexed user);
    // event AddAdmin(address indexed user);
    event UpdateAdmin(address indexed user, bool status);
    event ChangeToken(address token);
    event AdminTokenRecovery(address tokenRecovered, uint256 amount);
    event EmergencyWithdraw(uint256 amount);
    event SetPenaltyWallet(address indexed _penaltyWallet);
    event SetEmergencyWithdraw(bool _status);
    event SetRewardDistributionAddress(address indexed _rewardDistributionAddress);
   

    /* ========== CONSTRUCTOR ========== */

    constructor(address _rewardDistributionAddress, address _token, int256 _a, int256 _b, int256 _c) {
        CODE = IERC20(_token);
        rewardDistributionAddress = _rewardDistributionAddress;
        admin[msg.sender] = true;
        noWithdrawFeeTime = 30 days;
        PRECISION_FACTOR = 10 ** 12;
        fixedAPR = 1200;
        aNumber = _a;
        bNumber = _b;
        cNumber = _c;
        minStake = 1000 * (10**18);
        maxStake = 2000 * (10**18);
    }

    function removeAdmin(address _RemoveAdmin) external onlyOwner {
        admin[_RemoveAdmin] = false;

        emit UpdateAdmin(_RemoveAdmin, false);
    }

    function addAdmin(address _AddAdmin) external onlyOwner {
        admin[_AddAdmin] = true;

        emit UpdateAdmin(_AddAdmin, true);
    }

    function changeToken(address _token) external onlyAdmin {
        CODE = IERC20(_token);

        emit ChangeToken(_token);
    }

    function changeAPR(uint128 _apr) external onlyAdmin {
        fixedAPR = _apr;
    }

    /* ========== VIEWS ========== */

    function Admin(address _account) public view returns(bool) {
        return admin[_account];
    }

    function pendingReward(address _user) public view returns (uint256) {
        UserInfo storage user = userInfo[_user];

        uint256 duration = ((uint256(uint128(block.timestamp) - user.timeStake)) * PRECISION_FACTOR) / (24 * 60 * 60);

        return (((user.amount * fixedAPR) * duration) / 365) / (PRECISION_FACTOR * 10000);
    }

    /* ========== FUNCTION ========== */

    /*
     * @notice Deposit staked tokens and collect reward tokens (if any)
     * @param _amount: amount to withdraw (in rewardToken)
     */
    function deposit(uint256 _amount) external nonReentrant {
        
        UserInfo storage user = userInfo[msg.sender];

        require(_amount > 0, "Invalid Amount");

        if (user.amount < minStake) {
            require(user.amount + _amount >= minStake, "INVALID_AMOUNT");
        }

        require(user.amount + _amount <= maxStake, "INVALID AMOUNT");
        require(currentTotalStaked + _amount <= maxTotalStake, "POOL_FULL");
        
        CODE.safeTransferFrom(address(msg.sender), address(this), _amount);

        user.amount = user.amount + _amount;
        user.timeStake = uint128(block.timestamp);
        currentTotalStaked = currentTotalStaked + _amount;
        
        emit Deposit(msg.sender, _amount);
    }

    /*
     * @notice Withdraw staked tokens
     * @param _pid: id of long term pool
     */
    function withdraw(uint256 _amount) external nonReentrant {
        UserInfo storage user = userInfo[msg.sender];

        require(user.amount >= _amount, "INVALID_AMOUNT");

        if (_amount < user.amount) {
            require(user.amount - _amount >= minStake, "INVALID_AMOUNT");
        }

        require(address(penaltyWallet) != address(0), "PENALTYWALLET_IS_ZERO_ADDRESS");

        uint256 feeAmount = calculateWithdrawFee(_amount, msg.sender);

        uint256 totalWithdraw = _amount - feeAmount;

        if (feeAmount > 0) {
            CODE.safeTransfer(address(penaltyWallet), feeAmount);
        }

        CODE.safeTransfer(address(msg.sender), totalWithdraw);

        user.amount = user.amount - _amount;
        user.timeStake = uint128(block.timestamp);
        currentTotalStaked = currentTotalStaked - _amount;
        
        emit Withdraw(msg.sender, _amount, feeAmount);
    }

     /*
     * @notice Collect reward tokens
     */
    function claim() external {
        UserInfo storage user = userInfo[msg.sender];

        uint256 reward = pendingReward(msg.sender);

        CODE.safeTransferFrom(address(rewardDistributionAddress), address(msg.sender), reward);

        user.totalRewardEarned = user.totalRewardEarned + reward;
        user.timeStake = uint128(block.timestamp);

        emit Claim(msg.sender, reward);
    }


    /*
     * @notice Calculate APR withdraw fee
     * @param _user: address of user 
     */
    function calculateAPR(address _user) public view returns (uint256) {
        UserInfo storage user = userInfo[_user];

        uint256 _duration = ((uint256(uint128(block.timestamp) - user.timeStake)) * PRECISION_FACTOR) / (24 * 60 * 60);

        if (uint128(block.timestamp) < user.timeStake + noWithdrawFeeTime && _duration < ((noWithdrawFeeTime / (24 * 60 * 60)) * PRECISION_FACTOR)) {
            return ((((((PRECISION_FACTOR.toInt256()) * PRECISION_FACTOR_FE.toInt256() / aNumber) * (_duration ** 2).toInt256()) / (PRECISION_FACTOR ** 2).toInt256()) + (cNumber * PRECISION_FACTOR.toInt256() / PRECISION_FACTOR_FE.toInt256())) - (((PRECISION_FACTOR.toInt256() * PRECISION_FACTOR_FE.toInt256() / bNumber) * _duration.toInt256()) / PRECISION_FACTOR.toInt256())).toUint256();
        }

        return 0;
    }


    /**
     * @notice Calculate withdraw fee amount
     * @param _amount Withdraw Amount
     * @param _user User address
     */
    function calculateWithdrawFee(uint256 _amount, address _user) public view returns (uint256) {
        UserInfo storage user = userInfo[_user];

        uint256 _duration = ((uint256(uint128(block.timestamp) - user.timeStake)) * PRECISION_FACTOR) / (24 * 60 * 60);

        if (uint128(block.timestamp) < user.timeStake + noWithdrawFeeTime && _duration < ((noWithdrawFeeTime / (24 * 60 * 60)) * PRECISION_FACTOR)) {

            uint256 aprFee = calculateAPR(_user);
            
            return (((((_amount * aprFee) * _duration) / 365) / PRECISION_FACTOR) / (PRECISION_FACTOR * 100));
        }

        return 0;
    }


    /**
     * @notice Set min max deposit
     * @param _min min deposit amount
     * @param _max max deposit amount
     */
    function setMinMaxDeposit(uint256 _min, uint256 _max) external onlyAdmin {
        require(_min > 0 && _max > 0, "POOL::MIN_MAX_INVALID");
        require(_min < _max, "POOL::MIN_MAX_INVALID");
        
        minStake = _min;
        maxStake = _max;
        
        emit SetMinMax(_min, _max);
    }

    /**
     * @notice Set Penalty Wallet
     * @param _penaltyWallet penaltywallet
     */
    function setPenaltyWallet(address _penaltyWallet) external onlyAdmin {
        require(penaltyWallet != _penaltyWallet, "ALREADY_PENALTY_WALLET");
        penaltyWallet = _penaltyWallet;
        
        emit SetPenaltyWallet(_penaltyWallet);
    }

    /**
     * @notice set max total stake of pool
     * @param _maxTotalStake penaltywallet
     */
    function setMaxTotalStake(uint256 _maxTotalStake) external onlyAdmin {
        require(_maxTotalStake > 0, "POOL::MAX_TOTAL_STAKE_INVALID");

        maxTotalStake = _maxTotalStake;

        emit SetMaxTotalStake(_maxTotalStake);
    }

    /**
     * @notice Set Wallet Distribution
     * @param _rewardDistributionAddress penaltywallet
     */
    function setRewardDistributionAddress(address _rewardDistributionAddress) external onlyAdmin {
        require(_rewardDistributionAddress != rewardDistributionAddress, "POOL::ALREADY_DISTRIBUTION_WALLET");
        rewardDistributionAddress = _rewardDistributionAddress;

        emit SetRewardDistributionAddress(_rewardDistributionAddress);
    }

    function setWithdrawFeeParam(int256 _a, int256 _b, int256 _c) external onlyAdmin {
        aNumber = _a;
        bNumber = _b;
        cNumber = _c;

        emit SetWithdrawFeeParam(_a, _b, _c);
    }

    function setEmergency(bool _status) external onlyAdmin {
        isEmergency = _status;

        emit SetEmergencyWithdraw(_status);
    }

    /**
     * @notice withdraw all remain token when close pool
     * @dev Only callable by owner. Needs to be for emergency.
     */
    function emergencyTokenWithdraw(uint256 _amount) external onlyAdmin {
        require(isEmergency, "NOT EMERGENCY");
        CODE.safeTransfer(address(msg.sender), _amount);

        emit EmergencyWithdraw(_amount);
    }

    function emergencyWithdrawERC20() external {
        require(isEmergency, "function disabled");

        UserInfo storage user = userInfo[msg.sender];
        require(user.amount > 0, "nothing to withdraw");

        uint256 _amount = user.amount;
        user.amount = 0;

        IERC20(CODE).safeTransfer(msg.sender, _amount);

         emit EmergencyWithdraw(_amount);
    }

    function setNoWithdrawFeeTime(uint128 _duration) external onlyAdmin {
        noWithdrawFeeTime = _duration;

        emit SetWithdrawTime(_duration);
    }
    
    /**
     * @notice It allows the admin to recover wrong tokens sent to the contract
     * @param _tokenAddress: the address of the token to withdraw
     * @param _tokenAmount: the number of tokens to withdraw
     * @dev This function is only callable by admin.
     */
    function recoverWrongTokens(address _tokenAddress, uint256 _tokenAmount) external onlyAdmin {
        require(_tokenAddress != address(CODE), "Cannot be staked/reward token");

        IERC20(_tokenAddress).safeTransfer(address(msg.sender), _tokenAmount);

        emit AdminTokenRecovery(_tokenAddress, _tokenAmount);
    }

    /* ========== MODIFIERS ========== */

    modifier onlyAdmin() {
        require(admin[msg.sender] == true, "Caller is not admin");
        _;
    }

}

