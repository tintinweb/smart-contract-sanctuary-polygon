pragma solidity ^0.8.9;



/**
 * Let's get ready to Rugpull! When you mine $FONY, it goes onto the rug.
 * Anyone can pull the rug at (almost) any time, getting all the $FONY that is on it and stealing the Rug!
 * Yes, you read that correctly, any random wallet can STEAL the rug NFT from the current owner.
 * But DON'T pull the Rug too soon! Whenever the rug is pulled, mining will become a lot more difficult
 * in the future, so it'll be harder get $FONY in future rug pulls than from the current rug!
 *
 * Rugging and mining cost minePrice(), which starts at 0 but gradually increases over time.
 * Mine fee goes to current rug owner.
 * Each wallet can mine $FONY once per minute.
 * A wallet may only rug if it has mined $FONY since the previous rugging,
 * and it must wait 2 minutes since mining to pull the rug.
 * The amount of $FONY that is mined starts at 1000 and gradually decreases over time,
 * but is never less than 1.
 * This contract and its metadata are completely decentralized and have no owner,
 * but they were created by CryptoJigWorld, and you can find a web interface for mining and
 * rugging at https://jignft.com/rug
 */
contract RugPull is ERC1155 {
  using PRBMathSD59x18 for int256;

  string private constant _defuri = "https://bafybeibizmvi3lptvalxmgtcuioafea36f54nz3ecfgqskoyu2v4mib6wm.ipfs.dweb.link/metadata/{id}.json";

  int256  public constant INITIAL_MINE_YIELD   = 1000;
  uint64  public constant RUG_FONY_SUPPLY_MULT = 4;
  uint256 public constant MINE_COOLDOWN        = 1 minutes;
  uint256 public constant MINE_TO_RUG_INTERVAL = 2 minutes;
  uint256 public constant RUG_ID               = 1;
  uint256 public constant FONY_ID              = 2;

  address public currentRugHolder;
  uint256 public lastRugTS = 0;
  mapping(address => uint256) public lastMineTS;
  uint64  public totalFonyRewarded = 0;
  uint64  public fonyOnRug = 0;
  uint64  public effectiveFonySupply = 0;

  event Mine(address indexed miner, uint256 fonyMined);
  event Rug(address indexed rugged, address indexed rugger, uint256 fonyRecieved);

  constructor() ERC1155(_defuri) {
    _mint(0x000000000000000000000000000000000000dEaD, RUG_ID, 1, "");
  }

  function uri(uint256 id) public view override returns (string memory) {
    if (id == FONY_ID || fonyOnRug == 0) {
      return _defuri;
    }
    string memory numCoins;
    if (fonyOnRug > 2000) {
      numCoins = "many";
    } else if (fonyOnRug >= 1000) {
      numCoins = "6";
    } else if (fonyOnRug >= 3) {
      numCoins = "3";
    } else {
      numCoins = "1";
    }
    return string(
      abi.encodePacked(
        abi.encodePacked("https://bafybeibizmvi3lptvalxmgtcuioafea36f54nz3ecfgqskoyu2v4mib6wm.ipfs.dweb.link/metadata/rug", numCoins),
        "coins.json"
      )
    );
  }

  function totalFonyMined() public view returns (uint64) {
    return totalFonyRewarded + fonyOnRug;
  }

  function logistic(int256 x) private pure returns (int256) {
    x = (x - 46000) * 1e18;
    int256 div = 15000 * 1e18;
    int256 mul = 89 * 1e18;
    int256 nom = 100 * 100 * 1e18;
    return nom.div(mul.mul(1e18 + PRBMathSD59x18.exp2(x.div(div))));
  }

  function round(int256 x) private pure returns (int256) {
    x = x / 1e17;
    return x % 10 >= 5 ? x / 10 + 1 : x / 10;
  }

  function mineYield() public view returns (uint64) {
    int256 lg = logistic(int256(uint256(effectiveFonySupply)));
    int256 ret = round((INITIAL_MINE_YIELD * 1e18).mul(lg).div(100 * 1e18));
    return ret < 1 ? 1 : uint64(uint256(ret));
  }

  function minePrice() public view returns (uint256) {
    int256 x = int256(uint256(effectiveFonySupply)) - 12000;
    if (x < 0) {
      return 0;
    } else {
      int256 amt = (100 * 1e18 - logistic(x)).mul(PRBMathSD59x18.sqrt(x * 1e18)).div(5000 * 1e18);
      // conveniently, 1 ether = 1e18 wei
      return uint256(amt);
    }
  }

  function mine() external payable {
    require(msg.value == minePrice(), "Wrong Payment");
    require(block.timestamp >= lastMineTS[msg.sender] + MINE_COOLDOWN,
            "You must wait 1 minute between mining");
    uint64 mined = mineYield();
    fonyOnRug += mined;
    effectiveFonySupply += mined;
    lastMineTS[msg.sender] = block.timestamp;
    emit Mine(msg.sender, mined);
  }

  function rug() external payable {
    require(msg.sender != currentRugHolder, "Rug holder can't rug");
    require(msg.value == minePrice(), "Wrong Payment");
    uint256 mineTS = lastMineTS[msg.sender];
    require(mineTS > 0 && mineTS >= lastRugTS, "You must have mined $FONY since the last rug");
    require(block.timestamp >= mineTS + MINE_TO_RUG_INTERVAL,
            "You must wait 2 minutes after mining to rug");
    lastRugTS = block.timestamp;
    uint64 _fonyOnRug = fonyOnRug;
    totalFonyRewarded += _fonyOnRug;
    effectiveFonySupply += _fonyOnRug + RUG_FONY_SUPPLY_MULT * mineYield();
    fonyOnRug = 0;
    _mint(msg.sender, FONY_ID, _fonyOnRug, "");
    _safeTransferFrom(currentRugHolder, msg.sender, RUG_ID, 1, "");
    emit Rug(currentRugHolder, msg.sender, _fonyOnRug);
    currentRugHolder = msg.sender;
  }
}