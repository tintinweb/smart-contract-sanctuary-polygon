// File: contracts/Haura.sol


pragma solidity ^0.8.9;








contract Haura is ERC721A, Ownable, Royalty, ContractMetadata {
    using SafeMath for uint256;
    using Counters for Counters.Counter;

    Counters.Counter private _tokenIdTracker;

    uint256 public constant MAX_ELEMENTS = 7;
    uint256 public PRICE = 10000; // 10000
    uint256 public constant MAX_BY_MINT = 1;
    uint256 public END_SALE = 6; //Sunday, July 11, 2022, 00:00:00 UTC

    bool public isPaused = false;

    mapping(address => bool) private _whiteList;
    mapping(address => bool) private _whiteListClaimed;
    // mapping (uint256 => string) private URI;
    string public baseTokenURI;

    event CreateHauraNFT(uint256 indexed id);

    address public usdt;
    uint8 public usdtDecimals = 6;

    constructor(string memory baseURI, address _usdt) ERC721A("Haura Pre-Seed", "HPS") {
        _tokenIdTracker.increment();
        _setupOwner(msg.sender);
        setBaseURI(baseURI);
        usdt = _usdt;
        _mintAnElement(msg.sender);

    }

    function _canSetOwner() internal virtual view override returns (bool) {
        return msg.sender == owner();
    }
    modifier saleIsOpen() {   
        require(END_SALE < MAX_ELEMENTS, "Sale is not open");
        _;
    }

    function _totalSupply() internal view returns (uint256) {
        return _tokenIdTracker.current();
    }

    function mint() public payable saleIsOpen {
        uint256 total = _totalSupply().sub(1);
        uint256 _count = balanceOf(msg.sender);
        require(total <= MAX_ELEMENTS, "All Haura NFT's are sold out");
        require(_count <= MAX_BY_MINT, "Exceeds allowed NFT's per transaction");
        require(_canMint(msg.sender), "Error, Transaction failed");
        require(!_whiteListClaimed[msg.sender], "Already Minted");
        require(!isPaused, "Sale is Paused.");

        require(
            IERC20(usdt).transferFrom(
                msg.sender,
                address(this),
                PRICE.mul(10**usdtDecimals)
            ),
            "USDT transfer failed"
        );
        // require(msg.value >= salePrice(), "Value below price");

        _whiteListClaimed[msg.sender] = true;
        _mintAnElement(msg.sender);
        // _withdraw(owner(), address(this).balance);
    }


    function _mintAnElement(address _to) private {
        uint256 id = _totalSupply();
        _tokenIdTracker.increment();
        _safeMint(_to, id);
        // URI[id] = string(abi.encodePacked(_baseURI(),id,".json"));
        emit CreateHauraNFT(id);
    }

     function _canSetRoyaltyInfo()
        internal
        view
        virtual
        override
        returns (bool)
    {
        return msg.sender == owner();
    }

    function _canSetContractURI() internal view virtual override returns (bool){
        return msg.sender == owner();
    }
    
    function salePrice() public view returns (uint256) {
        return PRICE.mul(10 ** 18);
    }

    function salePriceInUSDT() public view returns (uint256) {
        return PRICE.mul(10 ** usdtDecimals);
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseTokenURI;
    }

    function setBaseURI(string memory baseURI) public onlyOwner {
        baseTokenURI = baseURI;
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(_tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        string memory uriSuffix = ".json";
        string memory currentBaseURI = _baseURI();
        uint metaId = _tokenId;

        return
            bytes(currentBaseURI).length > 0
                ? string(
                    abi.encodePacked(
                        currentBaseURI,
                        uint2str(metaId),
                        uriSuffix
                    )
                )
             : "";
    }

    function pauseMinting() external onlyOwner {
        isPaused = true;
    }

    function resumeMinting() external onlyOwner {
        isPaused = false;
    }

    function setSalePrice(uint256 _price) external onlyOwner {
        PRICE = _price;
    }

    function setUsdtDecimals(uint8 _decimals) external onlyOwner {
            usdtDecimals = _decimals;
        }

    function addToWhiteList(address[] calldata addresses) external onlyOwner {
        for (uint256 i = 0; i < addresses.length; i++) {
            require(addresses[i] != address(0), "Null address found");
            require(_whiteList[msg.sender] == false, "Error, already whitelisted");
            // require(!_whiteListClaimed[msg.sender], "Already Minted");


            _whiteList[addresses[i]] = true;
            // _whiteListClaimed[addresses[i]] = false;
        }
    }

    function _canMint(address addr) internal view returns (bool) {
        return _whiteList[addr];
    }

    function removeFromWhiteList(address[] calldata addresses)
        external
        onlyOwner
    {
        for (uint256 i = 0; i < addresses.length; i++) {
            require(addresses[i] != address(0), "Null address found");

            _whiteList[addresses[i]] = false;
        }
    }

    function withdrawAll() external onlyOwner {
        uint256 balance = IERC20(usdt).balanceOf(address(this));
        require(balance > 0);
        _withdraw(owner(), balance);
    }

    function _withdraw(address _address, uint256 _amount) private {
        require(_amount > 0);
        IERC20(usdt).transfer(_address, _amount);
    }

    function setUsdtAddress(address _usdt) external onlyOwner {
        usdt = _usdt;
    }

     function uint2str(uint256 _i)
        internal
        pure
        returns (string memory _uintAsString)
    {
        if (_i == 0) {
            return "0";
        }
        uint256 j = _i;
        uint256 len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint256 k = len;
        while (_i != 0) {
            k = k - 1;
            uint8 temp = (48 + uint8(_i - (_i / 10) * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }
}