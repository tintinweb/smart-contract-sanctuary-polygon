    if (
      secondNewestTwab.timestamp == 0 ||
      (OverflowSafeComparatorLib.checkedSub(
        _newestTwab.timestamp,
        secondNewestTwab.timestamp,
        _currentTime
      ) >= 1 days)
    ) {
