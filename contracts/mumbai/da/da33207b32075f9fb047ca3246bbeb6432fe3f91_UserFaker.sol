    ObservationLib.Observation memory secondNewestTwab = _twabs[
      RingBufferLib.prevIndex(
        RingBufferLib.newestIndex(_accountDetails.nextTwabIndex, MAX_CARDINALITY),
        MAX_CARDINALITY
