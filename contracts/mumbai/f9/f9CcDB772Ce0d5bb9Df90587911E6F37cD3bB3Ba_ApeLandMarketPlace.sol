        require(length != 0, "removeFromBlackListBatch: No address available");
        for (uint256 i; i < length; i++) {
            _removeFromBlackList(account[i]);
        }
    }
}

contract ApeLandMarketPlace is
    AccessControlEnumerable,
    Pausable,
    ERC721Holder,
    ERC1155Holder,
    BlackList
{
    using EncoderSet for EncoderSet.BytesSet;
    using ECDSA for bytes32;

    enum Asset {
        erc721,
        erc1155
    }

    struct Order {
        Asset asset;
        address token;
        uint256 tokenId;
        uint256 tokenQuantity;
        bytes data;
        uint256 salt;
    }

    struct LazyMint {
        address token;
        uint256 tokenId;
        uint256 tokenQuantity;
        bytes data;
        bytes signature;
        bool isLazyMint;
    }

    struct OrderBook {
        Order order;
        LazyMint lazyMint;
    }

    bytes32 public constant APELAND_MARKETPLACE =
        keccak256("APELAND_MARKETPLACE");
    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant BLOCKER_ROLE = keccak256("BLOCKER_ROLE");
    bytes32 public constant SIGNER_ROLE = keccak256("SIGNER_ROLE");

    mapping(address => uint256) public userNonce;

    mapping(address => EncoderSet.BytesSet) private _makerOrderHistory;
    mapping(address => EncoderSet.BytesSet) private _takerOrderHistory;

    event MakeOrder(
        address indexed account,
        address indexed token,
        uint256 indexed tokenId,
        uint256 tokenQuantity
    );
    event TakeOrder(
        address indexed account,
        address indexed token,
        uint256 indexed tokenId,
        uint256 tokenQuantity
    );
    event RevokeOrder(
        address indexed account,
        address indexed token,
        uint256 indexed tokenId,
        uint256 tokenQuantity
    );

    constructor(address account) {
        _grantRole(DEFAULT_ADMIN_ROLE, account);
        _grantRole(PAUSER_ROLE, account);
        _grantRole(BLOCKER_ROLE, account);
        _grantRole(SIGNER_ROLE, account);
        _grantRole(OPERATOR_ROLE, account);
    }

    function pause() external onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() external onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function addToBlackList(
        address[] calldata account
    ) external onlyRole(BLOCKER_ROLE) {
        _addToBlackListBatch(account);
    }

    function removeFromBlackList(
        address[] calldata account
    ) external onlyRole(BLOCKER_ROLE) {
        _removeFromBlackListBatch(account);
    }

    function makeOrder(OrderBook calldata makerOrder) external whenNotPaused {
        address msgSender = _msgSender();
        require(!isBlackListed(msgSender), "makeOrder: BlackListed user");

        Asset makerAsset = makerOrder.order.asset;

        address token;
        uint256 tokenId;
        uint256 tokenQuantity;

        if (makerOrder.lazyMint.isLazyMint) {
            token = makerOrder.lazyMint.token;
            tokenId = makerOrder.lazyMint.tokenId;
            tokenQuantity = makerOrder.lazyMint.tokenQuantity;

            ILazyMint(token).lazyMint(
                address(this),
                tokenId,
                tokenQuantity,
                makerOrder.lazyMint.data,
                makerOrder.lazyMint.signature
            );
        } else {
            token = makerOrder.order.token;
            tokenId = makerOrder.order.tokenId;
            tokenQuantity = makerOrder.order.tokenQuantity;

            if (makerAsset == Asset.erc721) {
                IERC721(token).safeTransferFrom(
                    msgSender,
                    address(this),
                    tokenId
                );
            } else {
                IERC1155(token).safeTransferFrom(
                    msgSender,
                    address(this),
                    tokenId,
                    tokenQuantity,
                    makerOrder.order.data
                );
            }
        }

        Order memory makedOrder;
        makedOrder.asset = makerAsset;
        makedOrder.token = token;
        makedOrder.tokenId = tokenId;
        makedOrder.tokenQuantity = tokenQuantity;
        makedOrder.salt = userNonce[msgSender]++;

        _makerOrderHistory[msgSender].add(_encode(makedOrder));
        emit MakeOrder(msgSender, token, tokenId, tokenQuantity);
    }

    function takeOrder(
        address account,
        uint256 index,
        address to,
        uint256 quantity,
        bytes memory signature
    ) external whenNotPaused onlyRole(OPERATOR_ROLE) {
        require(
            index < _makerOrderHistory[account].length(),
            "takeOrder: Invalid Order Index"
        );
        require(
            hasRole(
                SIGNER_ROLE,
                getSigner(account, index, to, quantity, signature)
            ),
            "takeOrder: Invalid signer Address"
        );

        bytes memory orderHash = _getMakerOrderHash(account, index);
        Order memory makerOrder = _decode(orderHash);

        address token = makerOrder.token;
        uint256 tokenId = makerOrder.tokenId;
        uint256 tokenQuantity = makerOrder.tokenQuantity;

        if (makerOrder.asset == Asset.erc721) {
            require(quantity == 0, "takeOrder: For ERC721 no quantity allowed");
            IERC721(token).safeTransferFrom(address(this), to, tokenId);
        } else {
            require(
                quantity >= 1 && quantity <= tokenQuantity,
                "Invalid token Quantity"
            );

            IERC1155(token).safeTransferFrom(
                address(this),
                to,
                tokenId,
                quantity,
                makerOrder.data
            );
        }

        _makerOrderHistory[account].remove(orderHash);

        if (tokenQuantity - quantity > 0) {
            makerOrder.tokenQuantity = tokenQuantity - quantity;
            _makerOrderHistory[account].add(_encode(makerOrder));
        }

        Order memory takedOrder;
        takedOrder.asset = makerOrder.asset;
        takedOrder.token = token;
        takedOrder.tokenId = tokenId;
        takedOrder.tokenQuantity = tokenQuantity;
        takedOrder.salt = userNonce[to]++;

        _takerOrderHistory[to].add(_encode(takedOrder));
        emit TakeOrder(account, token, tokenId, tokenQuantity);
    }

    function revokeOrder(uint256 index) external whenNotPaused {
        address msgSender = _msgSender();
        require(
            index < _makerOrderHistory[msgSender].length(),
            "revokeOrder: Invalid Order Index"
        );

        bytes memory orderHash = _getMakerOrderHash(msgSender, index);
        Order memory makerOrder = _decode(orderHash);

        address token = makerOrder.token;
        uint256 tokenId = makerOrder.tokenId;
        uint256 tokenQuantity = makerOrder.tokenQuantity;

        if (makerOrder.asset == Asset.erc721) {
            IERC721(token).safeTransferFrom(address(this), msgSender, tokenId);
        } else {
            IERC1155(token).safeTransferFrom(
                address(this),
                msgSender,
                tokenId,
                tokenQuantity,
                makerOrder.data
            );
        }

        _makerOrderHistory[msgSender].remove(orderHash);
        emit RevokeOrder(msgSender, token, tokenId, tokenQuantity);
    }

    function prepareHash(
        address account,
        uint256 index,
        address to,
        uint256 quantity
    ) public view returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    account,
                    index,
                    to,
                    quantity,
                    userNonce[account],
                    APELAND_MARKETPLACE
                )
            );
    }

    function getSigner(
        address account,
        uint256 index,
        address to,
        uint256 quantity,
        bytes memory signature
    ) private returns (address) {
        bytes32 hash = prepareHash(account, index, to, quantity)
            .toEthSignedMessageHash();
        userNonce[account]++;
        return hash.recover(signature);
    }

    function getMakerOrderDetails(
        address account,
        uint256 index
    ) external view returns (Order memory) {
        return _decode(_getMakerOrderHash(account, index));
    }

    function getTakerOrderDetails(
        address account,
        uint256 index
    ) external view returns (Order memory) {
        return _decode(_getTakerOrderHash(account, index));
    }

    function makedOrderCount(address account) external view returns (uint256) {
        return _makerOrderHistory[account].length();
    }

    function TakedOrderCount(address account) external view returns (uint256) {
        return _takerOrderHistory[account].length();
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        virtual
        override(ERC1155Receiver, ERC721Holder, AccessControlEnumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function _getMakerOrderHash(
        address account,
        uint256 index
    ) private view returns (bytes memory) {
        return _makerOrderHistory[account].at(index);
    }

    function _getTakerOrderHash(
        address account,
        uint256 index
    ) private view returns (bytes memory) {
        return _takerOrderHistory[account].at(index);
    }

    function _encode(
        Order memory orderStruct
    ) private pure returns (bytes memory) {
        return abi.encode(orderStruct);
    }

    function _decode(
        bytes memory orderHash
    ) private pure returns (Order memory) {
        return abi.decode(orderHash, (Order));
    }
}