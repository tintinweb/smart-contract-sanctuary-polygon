// SPDX-License-Identifier: MIT
pragma solidity ^0.8.14;
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./ExpressPoolDeposit.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "../investment-pool/InvestmentPoolMaster.sol";
import "../utils/ExchangeRateProxy.sol";
import "../utils/Constants.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";

contract ExpressPool is Ownable {
    
    using SafeMath for uint256;
    using ECDSA for bytes32;

    ExchangeRateProxy public exchangeProxyContract;

    InvestmentPoolMaster public investmentPoolMaster;
    address[] public depositAddresses;
    address public processorAddress;
    address public rewardsPayerAddress;
    uint256 public quoteTokenIndex;
    uint256 public minDepositAmount;
    uint256 public maxTVL;
    uint256 public currentTVL;
    uint256 public quickswapBuyRatio;
    uint256 public feeRatio;
    uint256 public poolRatio;

    struct ExchangeAddress {
        uint256 quickswapBuyRatio;
        address[] distributionAddresses;
        uint256[] distributionRatios;
    }
    mapping (address => ExchangeAddress) exchangeDepositAddresses;
    mapping (address => bool) isExchangeDeposit;

    struct Affiliate {
        address beneficiaryAddress;
        uint256 rewards;
        uint256 rewardsClaimed;
    }
    Affiliate[] public affiliates;

    enum InvestmentType {
        NORMAL,
        EXPRESS,
        EXTERNAL
    }
    struct Investment {
        address depositAddress;
        uint256 aprIndex;
        address beneficiaryAddress;
        uint256 totalValue;
        uint256 usdAmount;
        uint256 iuxAmount;
        uint256 feeAmount;
        uint256 createdTimestamp;  
        uint256 rewards;
        uint256 rewardsClaimed;     
        address affiliateAddress;
        InvestmentType investmentType;
    }
    mapping (uint256 => bool) investmentsPaid;
    Investment[] public investments;

    address public usdDepositAddress;
    address public iuxDepositAddress;
    address public iuxExchangeAddress;
    address public feeAddress;
    address[] public distributionWallets;
    uint256[] public distributionWalletRatios;


    uint256[] public rewardsVestingReleaseInstant;
    uint256[] public rewardsVestingReleaseCount;
    uint256[] public rewardsVestingReleaseInterval;

    uint256 public affiliateRewardsVestingReleaseInstant;
    uint256 public affiliateRewardsVestingReleaseCount;
    uint256 public affiliateRewardsVestingReleaseInterval;    

    uint256[] public principalLockingTime;
    uint256[] public principalVestingReleaseInstant;
    uint256[] public principalVestingReleaseCount;
    uint256[] public principalVestingReleaseInterval;

    enum ScheduledPaymentType {
        REWARD,
        AFFILIATE,
        PRINCIPAL
    }

    struct ScheduledPayment {
        ScheduledPaymentType paymentType;
        uint256 investmentIndex;
        uint256 timestamp;
        uint256 amount;
        address destination;
        bool processed;
    }

    ScheduledPayment[] public scheduledPayments;

    event AffiliateCreated(uint256 affiliateIndex);
    event InvestmentCreated(uint256 investmentIndex);
    event InvestmentMigrated(uint256 investmentIndex);    
    event RewardsClaimed(uint256 investmentIndex, uint256 amount);
    event PrincipalClaimed(uint256 investmentIndex, uint256 amount);
    event AffiliateRewardsClaimed(uint256 affiliateIndex, uint256 amount);
    event ScheduledPaymentProcessed(uint256 scheduledPaymentIndex);
    event ScheduledPaymentCreated(uint256 scheduledPaymentIndex);

    modifier onlyProcessor() {
        require(
            processorAddress == msg.sender,
            "Only the processor call this function"
        );
        _;
    }

    /*
     * You need to call this in order to be able to trade on QS
     */
    function approveQuickswapRouter(uint256 _amount, address _quickswapRouteraddress, address _tokenContractAddress) external onlyOwner {
        IERC20(_tokenContractAddress).approve(_quickswapRouteraddress, _amount);
    }

    /*
     * You need to set the minimum deposit, quickswapBuyRatio and quoteTokenIndex
     */
    function configurePool(
        uint256 _minDepositAmount,
        uint256 _feeRatio,
        uint256 _poolRatio,
        uint256 _maxTVL,
        uint256 _quickswapBuyRatio,
        uint256 _quoteTokenIndex
        ) external onlyOwner {
        minDepositAmount = _minDepositAmount;
        feeRatio = _feeRatio;
        poolRatio = _poolRatio;
        maxTVL = _maxTVL;
        quickswapBuyRatio = _quickswapBuyRatio;
        quoteTokenIndex = _quoteTokenIndex;
    }

    function configureRoles(
        address _processorAddress,
        address _rewardsPayerAddress) external onlyOwner {
        processorAddress = _processorAddress;
        rewardsPayerAddress = _rewardsPayerAddress;
    }

    function configureWallets(
        address _usdDepositAddress,
        address _iuxDepositAddress,
        address _iuxExchangeAddress,
        address _feeAddress,
        address[] memory _distributionWallets,
        uint256[] memory _distributionWalletRatios
    ) external onlyOwner {
        usdDepositAddress = _usdDepositAddress;
        iuxDepositAddress = _iuxDepositAddress;
        iuxExchangeAddress = _iuxExchangeAddress;
        feeAddress = _feeAddress;
        distributionWallets = _distributionWallets;
        distributionWalletRatios = _distributionWalletRatios;
    }


    function configurePrincipalVesting(
        uint256 _aprIndex,
        uint256 _principalLockingTime,
        uint256 _principalVestingReleaseInstant,
        uint256 _principalVestingReleaseCount,
        uint256 _principalVestingReleaseInterval
    ) external onlyOwner {
        if (_aprIndex >= principalVestingReleaseInstant.length) {
            principalLockingTime.push();
            principalVestingReleaseInstant.push();
            principalVestingReleaseCount.push();
            principalVestingReleaseInterval.push();
        }
        principalLockingTime[_aprIndex] = _principalLockingTime;
        principalVestingReleaseInstant[_aprIndex] = _principalVestingReleaseInstant;
        principalVestingReleaseCount[_aprIndex] = _principalVestingReleaseCount;
        principalVestingReleaseInterval[_aprIndex] = _principalVestingReleaseInterval;
    }    

    function configureRewardsVesting(
        uint256 _aprIndex,
        uint256 _rewardsVestingReleaseInstant,
        uint256 _rewardsVestingReleaseCount,
        uint256 _rewardsVestingReleaseInterval
    ) external onlyOwner {        
        if (_aprIndex >= rewardsVestingReleaseInstant.length) {
            rewardsVestingReleaseInstant.push();
            rewardsVestingReleaseCount.push();
            rewardsVestingReleaseInterval.push();
        }        
        rewardsVestingReleaseInstant[_aprIndex] = _rewardsVestingReleaseInstant;
        rewardsVestingReleaseCount[_aprIndex] = _rewardsVestingReleaseCount;
        rewardsVestingReleaseInterval[_aprIndex] = _rewardsVestingReleaseInterval;
    }

    function configureAffiliateRewardsVesting(
        uint256 _affiliateRewardsVestingReleaseInstant,
        uint256 _affiliateRewardsVestingReleaseCount,
        uint256 _affiliateRewardsVestingReleaseInterval
    ) external onlyOwner {
        affiliateRewardsVestingReleaseInstant = _affiliateRewardsVestingReleaseInstant;
        affiliateRewardsVestingReleaseCount = _affiliateRewardsVestingReleaseCount;
        affiliateRewardsVestingReleaseInterval = _affiliateRewardsVestingReleaseInterval;
    }    

    function configureContracts(
        address _investmentPoolMasterAddress,
        address _exchangeProxyContractAddress
        ) external onlyOwner {
        investmentPoolMaster = InvestmentPoolMaster(_investmentPoolMasterAddress);
        exchangeProxyContract = ExchangeRateProxy(_exchangeProxyContractAddress);
    }

    function _baseTokenAddress() internal view returns (address) {
        return investmentPoolMaster.baseTokenContractAddress();
    }

    function _quoteTokenAddress() internal view returns (address) {
        return investmentPoolMaster.quoteTokenContractAddressList(quoteTokenIndex);
    }    


    function deployCEXAddress(uint256 _quickswapBuyRatio, 
        address[] memory _distributionAddresses, 
        uint256[] memory _distributionRatios) external onlyOwner {
        address exchangeDepositAddress = address (new ExpressPoolDeposit(investmentPoolMaster.quoteTokenContractAddressList(quoteTokenIndex), address(investmentPoolMaster.geniusRouter())));
        ExchangeAddress memory exchangeAddress = ExchangeAddress(
            _quickswapBuyRatio,
            _distributionAddresses,
            _distributionRatios
        );
        isExchangeDeposit[exchangeDepositAddress] = true;
        exchangeDepositAddresses[exchangeDepositAddress] = exchangeAddress;
    }

    function updateCEXAddress(
        address exchangeDepositAddress,
        uint256 _quickswapBuyRatio, 
        address[] memory _distributionAddresses, 
        uint256[] memory _distributionRatios) external onlyOwner {            
        ExchangeAddress storage exchangeAddress = exchangeDepositAddresses[exchangeDepositAddress];
        exchangeAddress.quickswapBuyRatio = _quickswapBuyRatio;
        exchangeAddress.distributionAddresses = _distributionAddresses;
        exchangeAddress.distributionRatios = _distributionRatios;
    }    

    function deployDepositAddress(uint256 _count) external onlyProcessor {
        address quoteTokenAddress = investmentPoolMaster.quoteTokenContractAddressList(quoteTokenIndex);
        address routerAddress = address(investmentPoolMaster.geniusRouter());
        while(_count > 0) {
            depositAddresses.push(
                address(
                    new ExpressPoolDeposit(quoteTokenAddress, routerAddress)
                )
            );
            _count--;
        }
    }

    function createAffiliate(address beneficiaryAddress) external onlyProcessor {
        Affiliate memory affiliate = Affiliate(beneficiaryAddress, 0, 0);
        affiliates.push(affiliate);
        emit AffiliateCreated(affiliates.length - 1);
    }

    function updateAffiliateBeneficiary(
        uint256 _affiliateIndex,
        address _beneficiaryAddress
    ) external onlyProcessor {
        Affiliate storage affiliate = affiliates[_affiliateIndex];
        require(affiliate.beneficiaryAddress == address(0), "Can only set the beneficiary if it's null");
        affiliate.beneficiaryAddress = _beneficiaryAddress;
    }  

    function migrateInvestment(address _investorAddress, address _affiliateAddress, uint256 _aprIndex, uint256[] memory _amounts, uint256 timestamp, InvestmentType _investmentType) external onlyOwner {
        Investment memory investment = Investment(
            _investorAddress,
            _aprIndex,
            _investorAddress,
            _amounts[0], // total value
            _amounts[1], // usd amount
            _amounts[2], // iux amount
            _amounts[3], // fee amount
            timestamp,
            0,
            0,
            _affiliateAddress,
            _investmentType
        );
        investments.push(investment);
        currentTVL = currentTVL.add(_amounts[1]);
        emit InvestmentMigrated(investments.length - 1);
    }

    /*
     * Pretul sa se calculeze fara price impact.
     */
    function calculateNormalDeposit(uint256 _inputAmount) public view returns (uint256 _usdAmount, uint256 _iuxAmount, uint256 _feeAmount) {
        uint256 totalPoolRatio = (poolRatio.add(BASIS_POINT)).div(BASIS_POINT);
        uint256 inputUsd = _inputAmount.mul(feeRatio).div(feeRatio.add(totalPoolRatio));
        _iuxAmount = exchangeProxyContract.getAmountsOutSingleUnitPrice(inputUsd).mul(poolRatio).div(BASIS_POINT);        

        uint256 totalAmount = (inputUsd.mul(poolRatio.add(BASIS_POINT))).div(BASIS_POINT);
        _feeAmount = totalAmount.mul(feeRatio).div(BASIS_POINT);
        _usdAmount = inputUsd.sub(_feeAmount);

        return (inputUsd, _iuxAmount, _feeAmount);
    }

    /*
     * Calculate normal deposit base
     */
    function calculateNormalDepositBase(uint256 _iuxAmount) public view returns (uint256 _usdAmount, uint256 _feeAmount, uint256 _investmentValue) {
        _usdAmount = exchangeProxyContract.getAmountsInSingleUnitPrice(_iuxAmount);
        _usdAmount = _usdAmount.mul(BASIS_POINT).div(poolRatio);

        _investmentValue = _usdAmount.add(_usdAmount.mul(poolRatio).div(BASIS_POINT));
        _feeAmount = _investmentValue.mul(feeRatio).div(BASIS_POINT);  

        return (
            _usdAmount,
            _feeAmount,
            _investmentValue
        );        
    }

    function calculateDeposit(uint256 _inputAmount) public view returns (uint256 _usdAmount, uint256 _iuxDepositAmount, uint256 _usdDepositAmount, uint256 _feeAmount, uint256 _usdBuyAmount) {
        uint256 totalPoolRatio = (poolRatio.add(BASIS_POINT)).div(BASIS_POINT);
        _usdAmount = _inputAmount.mul(feeRatio).div(feeRatio.add(totalPoolRatio));
        _usdDepositAmount = _usdAmount.div(totalPoolRatio);
        _iuxDepositAmount = exchangeProxyContract.getAmountsOut(_usdDepositAmount).mul(poolRatio).div(BASIS_POINT);
        _usdBuyAmount = _usdAmount.sub(_usdDepositAmount);
        _feeAmount = _inputAmount.sub(_usdDepositAmount).sub(_usdBuyAmount);
        
        return (
            _usdAmount, // Valorea totala fara fee (1941.74)
            _iuxDepositAmount, // Valoarea de IUX rezultata (12,944)
            _usdDepositAmount, // Valoarea de USD efectiv care intra (647)
            _feeAmount, // Fee (58.25)
            _usdBuyAmount // Valoarea de USD cu care cumpar IUX (1,294)
        );
    }

    function calculateMaxDeposit() public view returns (uint256 _maxDepositAmount) {
        uint256 tvlDiff = maxTVL.sub(currentTVL);
        return tvlDiff;
        /*
        _maxDepositAmount = tvlDiff.mul(poolRatio.add(BASIS_POINT)).div(BASIS_POINT);
        return _maxDepositAmount;
        */
    }


    function processDeposit(address _depositAddress, address _beneficiaryAddress, address _affiliateAddress, uint256 _aprIndex) external onlyProcessor {
        uint256 usdBalance = IERC20(_quoteTokenAddress()).balanceOf(_depositAddress);
        require(usdBalance > minDepositAmount, "Minimum deposit is not met");

        console.log("Process deposit %s", usdBalance);

        (uint256 usdAmount, uint256 iuxDepositAmount, uint256 usdDepositAmount, uint256 feeAmount, uint256 buyAmount) = calculateDeposit(usdBalance);

        if (currentTVL.add(usdDepositAmount) > maxTVL) {
            usdBalance = calculateMaxDeposit();
            (usdAmount, iuxDepositAmount, usdDepositAmount, feeAmount, buyAmount) = calculateDeposit(usdBalance);
        }        

        investmentPoolMaster.geniusRouter().processPayment(_quoteTokenAddress(), _depositAddress, address(this), usdBalance);
        investmentPoolMaster.geniusRouter().processPayment(_baseTokenAddress(), iuxExchangeAddress, iuxDepositAddress, iuxDepositAmount);        

        Investment memory investment = Investment(
            _depositAddress,
            _aprIndex,
            _beneficiaryAddress,
            usdAmount.add(feeAmount), // total value
            usdDepositAmount, // usd amount
            iuxDepositAmount, // iux amount
            feeAmount, // fee amount
            block.timestamp,
            0,
            0,
            _affiliateAddress,
            InvestmentType.EXPRESS
        );
        investments.push(investment);
        currentTVL = currentTVL.add(usdDepositAmount);

        IERC20(_quoteTokenAddress()).transfer(usdDepositAddress,usdDepositAmount);
        IERC20(_quoteTokenAddress()).transfer(feeAddress,feeAmount);
        
        uint256 quickswapBuyAmount = buyAmount.mul(quickswapBuyRatio).div(BASIS_POINT);        
        buyQuickswap(quickswapBuyAmount, iuxDepositAddress);

        uint256 distributionAmount = buyAmount.sub(quickswapBuyAmount);
        distributeResult(_quoteTokenAddress(),distributionAmount, distributionWallets, distributionWalletRatios);
        
        emit InvestmentCreated(investments.length - 1);
    }

    function processExchangeDeposit(address _depositAddress, address _beneficiaryAddress, address _affiliateAddress, uint256 _aprIndex) external {
        require(isExchangeDeposit[_depositAddress], "This is not an exchange deposit");
        ExchangeAddress memory exchangeAddress = exchangeDepositAddresses[_depositAddress];

        uint256 usdBalance = IERC20(_quoteTokenAddress()).balanceOf(_depositAddress);
        require(usdBalance > minDepositAmount, "Minimum deposit is not met");

        (uint256 usdAmount, uint256 iuxDepositAmount, uint256 usdDepositAmount, uint256 feeAmount, uint256 buyAmount) = calculateDeposit(usdBalance);

        if (currentTVL.add(usdDepositAmount) > maxTVL) {
            usdBalance = calculateMaxDeposit();
            (usdAmount, iuxDepositAmount, usdDepositAmount, feeAmount, buyAmount) = calculateDeposit(usdBalance);
        }

        investmentPoolMaster.geniusRouter().processPayment(_quoteTokenAddress(), _depositAddress, address(this), usdBalance);
        investmentPoolMaster.geniusRouter().processPayment(_baseTokenAddress(), iuxExchangeAddress, iuxDepositAddress, iuxDepositAmount);

        Investment memory investment = Investment(
            _depositAddress,
            _aprIndex,
            _beneficiaryAddress,
            usdAmount.add(feeAmount), // total value
            usdDepositAmount, // usd amount
            iuxDepositAmount, // iux amount
            feeAmount, // fee amount
            block.timestamp,
            0,
            0,
            _affiliateAddress,
            InvestmentType.EXTERNAL
        );
        investments.push(investment);
        currentTVL = currentTVL.add(usdDepositAmount);
        require(currentTVL <= maxTVL,"Max TVL is reached");

        IERC20(_quoteTokenAddress()).transfer(usdDepositAddress,usdDepositAmount);
        IERC20(_quoteTokenAddress()).transfer(feeAddress,feeAmount);
        
        uint256 quickswapBuyAmount = buyAmount.mul(exchangeAddress.quickswapBuyRatio).div(BASIS_POINT);        
        buyQuickswap(quickswapBuyAmount, iuxDepositAddress);
        uint256 distributionAmount = buyAmount.sub(quickswapBuyAmount);
        distributeResult(_quoteTokenAddress(),distributionAmount,exchangeAddress.distributionAddresses, exchangeAddress.distributionRatios);
        
        emit InvestmentCreated(investments.length - 1);
    }

    function createNormalDeposit(uint256 _quoteAmount, address _affiliateAddress, bytes32 _affiliateHash, bytes memory _affiliateSignature, uint256 _aprIndex) external {
        require(validateSigner(_affiliateHash, processorAddress, _affiliateSignature), "Only processor can set the affiliateAddress");
        require(validateRelation(_affiliateHash, msg.sender, _affiliateAddress), "Invalid affiliateAddress");        

         (uint256 usdAmount, uint256 iuxAmount, uint256 feeAmount) = calculateNormalDeposit(_quoteAmount);
        investmentPoolMaster.geniusRouter().processPayment(_quoteTokenAddress(), msg.sender, address(this), _quoteAmount);
        investmentPoolMaster.geniusRouter().processPayment(_baseTokenAddress(), msg.sender, iuxDepositAddress, iuxAmount);

        Investment memory investment = Investment(
            msg.sender,
            _aprIndex,
            msg.sender,
            _quoteAmount, // total value
            usdAmount, // usd amount
            iuxAmount, // iux amount
            feeAmount, // fee amount
            block.timestamp,
            0,
            0,
            _affiliateAddress,
            InvestmentType.NORMAL
        );
        investments.push(investment);
        currentTVL = currentTVL.add(usdAmount);
        require(currentTVL <= maxTVL,"Max TVL is reached");

        IERC20(_quoteTokenAddress()).transfer(feeAddress,feeAmount);
        distributeResult(_quoteTokenAddress(), usdAmount, distributionWallets, distributionWalletRatios);
        
        emit InvestmentCreated(investments.length - 1);
    }

    function buyQuickswap(uint256 _quickswapBuyAmount, address _destinationAddress) internal {
        if (_quickswapBuyAmount > 0) {
            uint256 pathsLength = exchangeProxyContract.getQuickswapPathsLength();
            address[] memory paths = new address[](pathsLength);
            for (uint256 i = 0; i < pathsLength; i++) {
                paths[i] = exchangeProxyContract.quickswapPaths(i);
            }

            uint256 amountIn = exchangeProxyContract.getAmountsOut(_quickswapBuyAmount);
            QuickswapInterface(exchangeProxyContract.getQuickswapRouterAddress()).swapExactTokensForTokens(
                _quickswapBuyAmount,
                amountIn,
                paths,
                address(this),
                block.timestamp
            );
            IERC20(_baseTokenAddress()).transfer(_destinationAddress, amountIn);

        }                 
    }

    function distributeResult(address _tokenAddress, uint256 _distributionAmount, address[] memory _distributionWallets, uint256[] memory _distributionWalletRatios) internal {
        if (_distributionAmount > 0) {
            for (uint256 index = 0; index < _distributionWallets.length; index++) {
                address outputAddress = _distributionWallets[index];
                uint256 outputAmount = _distributionAmount.mul(_distributionWalletRatios[index]).div(BASIS_POINT);
                require(outputAddress != address(0), "Cannot send to null address");
                IERC20(_tokenAddress).transfer(outputAddress, outputAmount);
            }
        }
    }

    function withdrawDeposit(address _depositAddress, address _withdrawAddress, uint256 _amount) external onlyProcessor {
        address quoteTokenAddress = investmentPoolMaster.quoteTokenContractAddressList(quoteTokenIndex);

        investmentPoolMaster.geniusRouter().processPayment(quoteTokenAddress, _depositAddress, _withdrawAddress, _amount);
    }    

    function setInvestmentRewards(uint256 _investmentIndex, uint256 _rewards) external onlyProcessor {
        Investment storage investment = investments[_investmentIndex];
        investment.rewards = _rewards;
    }

    function updateInvestmentAddresses(uint256 _investmentIndex, address _beneficiaryAddress, address _affiliateAddress) external onlyProcessor {
        Investment storage investment = investments[_investmentIndex];
        investment.beneficiaryAddress = _beneficiaryAddress;
        investment.affiliateAddress = _affiliateAddress;
    }


    function claimInvestmentRewards(uint256 _investmentIndex) external {
        Investment storage investment = investments[_investmentIndex];
        
        uint256 claimableRewards = (investment.rewards).sub(investment.rewardsClaimed);
        require(claimableRewards > 0, "You don't have rewards");
        require(investment.beneficiaryAddress != address(0), "No beneficiary");
        
        _addVestingRelease(claimableRewards, rewardsVestingReleaseInstant[investment.aprIndex], rewardsVestingReleaseInterval[investment.aprIndex], rewardsVestingReleaseCount[investment.aprIndex], investment.beneficiaryAddress, ScheduledPaymentType.REWARD, _investmentIndex);
        investment.rewardsClaimed = (investment.rewardsClaimed).add(claimableRewards);
        emit RewardsClaimed(_investmentIndex, claimableRewards);
    }

    function setAffiliateRewards(uint256 _affiliateIndex, uint256 _rewards) external onlyProcessor {
        Affiliate storage affiliate = affiliates[_affiliateIndex];
        affiliate.rewards = _rewards;
    }    

    function claimAffiliateRewards(uint256 _affiliateIndex) external {
        Affiliate storage affiliate = affiliates[_affiliateIndex];
        uint256 claimableAffiliateRewards = (affiliate.rewards).sub(affiliate.rewardsClaimed);
        require(claimableAffiliateRewards > 0, "You don't have affiliate rewards");
        require(affiliate.beneficiaryAddress != address(0), "No beneficiary");
        require(affiliate.beneficiaryAddress == msg.sender, "Only the beneficiary can call this function");
        
        _addVestingRelease(claimableAffiliateRewards, affiliateRewardsVestingReleaseInstant, affiliateRewardsVestingReleaseInterval, affiliateRewardsVestingReleaseCount, affiliate.beneficiaryAddress, ScheduledPaymentType.AFFILIATE, 0);
        affiliate.rewardsClaimed = (affiliate.rewardsClaimed).add(claimableAffiliateRewards);
        emit AffiliateRewardsClaimed(_affiliateIndex, claimableAffiliateRewards);
    }    


    function claimPrincipal(uint256 _investmentIndex) external {
        Investment storage investment = investments[_investmentIndex];
        uint256 totalIUX = (investment.iuxAmount).add(exchangeProxyContract.getAmountsOut(investment.usdAmount));
        uint256 principalLockedDatetime = (investment.createdTimestamp).add(principalLockingTime[investment.aprIndex]);
        require(investment.beneficiaryAddress != address(0), "No beneficiary");
        require(investmentsPaid[_investmentIndex] == false,"Principal was already paid");
        require(principalLockedDatetime < block.timestamp, "Principal is locked");
        
        _addVestingRelease(totalIUX, principalVestingReleaseInstant[investment.aprIndex], principalVestingReleaseInterval[investment.aprIndex], principalVestingReleaseCount[investment.aprIndex], investment.beneficiaryAddress, ScheduledPaymentType.PRINCIPAL, _investmentIndex);
        investmentsPaid[_investmentIndex] = true;
        emit PrincipalClaimed(_investmentIndex, totalIUX);
    }

    function schedulePayment(ScheduledPaymentType _paymentType, uint256 _investmentIndex, uint256 _timestamp, uint256 _amount, address _beneficiary) internal {
        ScheduledPayment memory payment = ScheduledPayment(
            _paymentType,
            _investmentIndex,
            _timestamp,
            _amount,
            _beneficiary,
            false
        );
        scheduledPayments.push(payment);
        emit ScheduledPaymentCreated(scheduledPayments.length-1);
    }
    
    function _addVestingRelease(uint256 _amount, uint256 _instant, uint256 _interval, uint256 _count, address _beneficiary, ScheduledPaymentType _paymentType, uint256 _investmentIndex) internal {
        if (_instant == BASIS_POINT) {
            schedulePayment(_paymentType, _investmentIndex, block.timestamp, _amount, _beneficiary);
        } else {
            uint256 instantAmount = _amount.mul(_instant).div(BASIS_POINT);
            schedulePayment(_paymentType, _investmentIndex, block.timestamp, instantAmount, _beneficiary);
            uint256 intervalAmount = (_amount.sub(instantAmount)).div(_count);
            for (uint256 index = 1; index <= _count; index++) {
                uint256 addInterval = _interval.mul(index);
                uint256 timestamp = (block.timestamp).add(addInterval);
                schedulePayment(_paymentType, _investmentIndex, timestamp, intervalAmount, _beneficiary);
            }
        }
    }

    function processScheduledPayment(uint256 _scheduledPaymentIndex) external {
        ScheduledPayment storage scheduledPayment = scheduledPayments[_scheduledPaymentIndex];
        require(!scheduledPayment.processed,"Payment is already processed");
        require(block.timestamp > scheduledPayment.timestamp, "Time has not passed");
        investmentPoolMaster.geniusRouter().processPayment(_baseTokenAddress(), rewardsPayerAddress, scheduledPayment.destination, scheduledPayment.amount);
        scheduledPayment.processed = true;
    }

    function validateSigner(bytes32 _hash, address _address, bytes memory _signature) internal pure returns (bool) {
        bytes32 ethSignedMessageHash = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", _hash));
        return ethSignedMessageHash.recover(_signature) == _address;
    }

    function validateRelation(bytes32 _hash, address _address1, address _address2) internal pure returns (bool) {
        return _hash == keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n20", _address1, _address2));
    }
}

