        uint256 _exchangeRatio,
        uint256 _startTimestamp,
        uint256 _maxSupply,
        uint256 _minContribution,
        uint256 _contributionFee,
        uint256 _poolDuration,
        address _baseDestinationAddress,
        address _quoteDestinationAddress
    ) onlyInvestmentPoolManager external {
        isInvestmentPool[investmentPoolFactoryInterface.deployInvestmentPool(
            _exchangeRatio, 
            _startTimestamp, 
            _maxSupply, 
            _minContribution, 
            _contributionFee, 
            _poolDuration, 
            _baseDestinationAddress, 
            _quoteDestinationAddress,
