    }

    function payRewards(address _rewardsPayerAddress, address _investorAddress, uint256 _investmentIndex, uint256 _amount) public onlyInvestmentPool returns (bool) {        
        geniusRouter.emitRetrievedRewardsEvent(address(this), msg.sender, _investmentIndex, _amount);
        return geniusRouter.processPayment(baseTokenContractAddress, _rewardsPayerAddress, _investorAddress, _amount);
    }
    
    function payAffiliateRewards(address _rewardsPayerAddress, address _referralAddress, uint256 _investmentIndex, uint256 _amount) public onlyInvestmentPool returns (bool) {
        geniusRouter.emitRetrievedAffiliateRewardsEvent(address(this), msg.sender, _investmentIndex, _amount);
