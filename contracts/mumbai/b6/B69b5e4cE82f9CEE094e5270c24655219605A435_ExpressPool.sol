        );
        _;
    }

    modifier onlyInvestmentPool() {
        require(
            isInvestmentPool[msg.sender],
            "Only the pool manager can call this function"
        );
        _;
    }

    modifier onlyInvestmentPoolFactory() {
        require(
            msg.sender == address(investmentPoolFactoryInterface),
            "Only investment pool factory can call this function"
        );
        _;
    }    

    function initialize(address _investmentPoolFactoryAddress) public initializer {
        investmentPoolFactoryInterface = InvestmentPoolFactoryInterface(_investmentPoolFactoryAddress);
        __Ownable_init_unchained();
