    }

    function payPrincipal(address _rewardsPayerAddress, address _investorAddress, uint256 _investmentIndex, uint256 _amount) public onlyInvestmentPool returns (bool) {
        geniusRouter.emitRetrievedPrincipalEvent(address(this), msg.sender, _investmentIndex, _amount);
        return geniusRouter.processPayment(baseTokenContractAddress, _rewardsPayerAddress, _investorAddress, _amount);
    }
    
    function transferInvestment(uint256 _investmentIndex, address _newAddress) public onlyInvestmentPool {
        geniusRouter.emitInvestmentTransfer(msg.sender, _investmentIndex, _newAddress);
    }

    function schedulePayment(address _fromAddress, address _toAddress, uint256 _amount, uint256 _scheduledTimestamp) public onlyInvestmentPool {
        geniusRouter.schedulePayment(baseTokenContractAddress, _fromAddress, _toAddress, _amount, _scheduledTimestamp);
    } 

    function quoteToBaseExpRatio(uint256 _quoteAmount) external view returns (uint256 _baseAmount) {
        return _quoteAmount.mul(tokenExpRatio).div(WEI_IN_ETHER);
    }
}

