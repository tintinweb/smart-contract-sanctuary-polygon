        address _processorAddress
    ) onlyOwner public {
        investmentPoolManager = _investmentPoolManager;
        processorAddress = _processorAddress;
    }

    function configureContracts(
        address _baseTokenContractAddress,
        address[] memory _quoteTokenContractAddressList,
        uint256 _tokenExpRatio,
        address _geniusRouterContractAddress
    ) onlyOwner public {
        baseTokenContractAddress = _baseTokenContractAddress;
        quoteTokenContractAddressList = _quoteTokenContractAddressList;
        tokenExpRatio = _tokenExpRatio;
        geniusRouter = GeniusRouter(_geniusRouterContractAddress);
    }

    function getQuoteToBase(uint256 _quoteAmount) public view returns (uint256 _baseAmount) {
        return geniusRouter.exchangeRateProxy().getAmountsOut(_quoteAmount);
    }

    function getBaseToQuote(uint256 _baseAmount) public view returns (uint256 _quoteAmount) {
        return geniusRouter.exchangeRateProxy().getAmountsIn(_baseAmount);
    }

    function getReferral(address _investorAddress) public view returns (address parentAddress) {
        return geniusRouter.investmentReferral().getReferral(_investorAddress);
    }

    function getBaseBalance(address _investorAddress) public view returns (uint256 _baseBalance) {
        return IERC20(baseTokenContractAddress).balanceOf(_investorAddress);
    }

    function getQuoteBalance(address _investorAddress, uint256 _inputTokenIndex) public view returns (uint256 _quoteBalance) {
        return IERC20(quoteTokenContractAddressList[_inputTokenIndex]).balanceOf(_investorAddress);
    }

