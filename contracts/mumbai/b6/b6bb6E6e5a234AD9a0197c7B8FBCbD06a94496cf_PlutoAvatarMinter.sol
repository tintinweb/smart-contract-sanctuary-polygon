import {whitelistWizard} from "./utils/whitelistWizard.sol";
import {IERC20Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";

/// @title PlutoAvatarMinter.sol
/// @author Untitled Labs (https://untitledlabs.io)
contract PlutoAvatarMinter is
Initializable,
ERC721AUpgradeable,
OperatorFiltererUpgradeable,
Ownable2StepUpgradeable,
ERC2981Upgradeable,
NonblockingLzApp,
whitelistWizard
{
    // contract storage - core
    string public baseURI; // baseURI for the NFT
    bytes4 private constant _ERC721_RECEIVED = 0x150b7a02; // bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"));
    // contract storage - minting
    uint256 public maxSupply; // max supply of the NFT
    uint256 public availableMints; // available mints
    uint256 public maxMintPerAddress; // max allowed to mint per user
    address public nativeAddress ;
    // contract storage - traits
    mapping(address => bool) public registeredTraits;
    mapping(uint256 => mapping(address => uint256)) public linkedTraits;
    mapping(address => uint256) public tokensMinted;
    mapping(address => bool) public isTokenAllowed;
    mapping(address => uint256) private mintPrice;
    mapping(address => uint256) private tokensAllocated;
    
    /// @dev This empty reserved space is put in place to allow future versions to add new
    /// variables without shifting down storage in the inheritance chain.
    uint256[49] private __gap;
    
    // events
    event ChangeMintPrice(address indexed tokenAddress,uint256 price);
    event TokensWhitelisted(address tokenAddress);
    event LinkTrait(uint256 avatarId, address traitContract, uint256 traitId);
    event UnlinkTrait(uint256 avatarId, address traitContract, uint256 traitId);
    event RegisterTrait(address traitContract);
    event DeRegisterTrait(address traitContract);
    event TokenAllowed(address tokenAddress);
    // errors
    error Unauthorized(); // if the call is not from the authorised address
    error Underpriced(); // if the value attached is less than the minimum required value
    error Overpriced(); // if the value attached is more than the maximum required value
    error InvalidParams(); // if the function params are invalid
    error MaxSupplyReached(); // if the max supply is reached
    error MaxMintReached(); // if the max mint is reached
    
    // Modifiers
    modifier onlyAuthorized(
        uint256 avatarId,
        address traitContract,
        uint256 traitId
    ) {
        if (ownerOf(avatarId) != _msgSender()) {
            revert Unauthorized();
        }
        
        if (!registeredTraits[traitContract]) {
            revert InvalidParams();
        }
        
        if (
            IERC721AUpgradeable(traitContract).ownerOf(traitId) != _msgSender()
        ) {
            revert Unauthorized();
        }
        _;
    }
    
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }
    
    /// @notice Initializes the pass contract
    function initialize(
        address _subscriptionOrRegistrantToCopy,
        address _endpointAddress,
        address _designatedSigner,
        string memory _domain,
        string memory _version,
        string memory _baseURI
    ) external initializerERC721A initializer {
        __ERC721A_init("Pluto", "PL");
        __OperatorFilterer_init(_subscriptionOrRegistrantToCopy, false);
        __Ownable2Step_init();
        __nonblockingLzApp_init(_endpointAddress);
        __Wizard_init(_domain, _version, _designatedSigner);
        nativeAddress = 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;
        isTokenAllowed[nativeAddress] = true;
        maxSupply = 10000;
        maxMintPerAddress = 5;
        baseURI = _baseURI;
        mintPrice[nativeAddress] = 10 ether;
    }
    
    /// @notice This function is used to check if a address is allowed to mint free tokens
    /// @dev This function takes in a address and returns a bool
    /// @param _address Address to check
    function isFreeMintAvailable(address _address) external view returns (bool) {
        if (maxSupply == totalSupply()) {
            return false;
        }
        if (tokensMinted[_address] >= maxMintPerAddress) {
            return false;
        }
        if (tokensAllocated[_address] > 0) {
            return true;
        }
        return false;
    }
    
    // Mint functions
    
    /// @notice This function is used to mint tokens
    /// @dev This function takes in a signature from whitelist wizard and mints tokens
    /// @param signature Signature from whitelist wizard
    /// @param paymentToken Address of the token to be used for payment
    function payAndMint(whitelist calldata signature, uint256 tokenCount, address paymentToken) external payable {
        require (isTokenAllowed[paymentToken], "Token not allowed");
        validateSignature(signature, msg.sender);
        require (
            tokensMinted[msg.sender] + tokenCount <= maxMintPerAddress,
            "Max mint per address reached"
        );
        if (paymentToken == nativeAddress){
            require(msg.value == getMintPrice(nativeAddress) * tokenCount, "Invalid amount");
        } else {
            IERC20Upgradeable(paymentToken).transferFrom(msg.sender, address(this), getMintPrice(paymentToken) * tokenCount);
        }
        unchecked {
            tokensMinted[msg.sender] += tokenCount;
        }
        mintTokens(msg.sender, tokenCount);
    }
    
    // Trait Linkers
    /// Linking and Unlinking of the traits
    /// @notice Link trait with the NFT
    /// @dev Check if the msg.sender holds both trait and the Avatar
    /// @param avatarId Id of the avatar
    /// @param traitContract Address of the trait contract
    /// @param traitId Id of the trait
    function linkTrait(
        uint256 avatarId,
        address traitContract,
        uint256 traitId
    ) external onlyAuthorized(avatarId, traitContract, traitId) {
        linkedTraits[avatarId][traitContract] = traitId;
        emit LinkTrait(avatarId, traitContract, traitId);
    }
    
    /// @notice Unlink trait with the NFT
    /// @dev Check if the msg.sender holds both trait and the Avatar
    /// @param avatarId Id of the avatar
    /// @param traitContract Address of the trait contract
    /// @param traitId Id of the trait
    function unlinkTrait(
        uint256 avatarId,
        address traitContract,
        uint256 traitId
    ) external onlyAuthorized(avatarId, traitContract, traitId) {
        linkedTraits[avatarId][traitContract] = 0;
        emit UnlinkTrait(avatarId, traitContract, traitId);
    }
    
    /// @notice Get TokenURI from token ID
    /// @dev returns tokenURI if set specifically, otherwise return baseURI
    /// @param tokenId ID of the token
    function tokenURI(
        uint256 tokenId
    ) public view override returns (string memory) {
        return
            bytes(baseURI).length > 0
                ? string(abi.encodePacked(baseURI, _toString(tokenId)))
                : "";
    }
    
    // Magic Eden Requirement
    /// @notice Get the total minted tokens
    function totalMinted() public view returns (uint256) {
        return totalSupply();
    }
    
    // ERC721A functions
    function _startTokenId() internal view virtual override returns (uint256) {
        return 1;
    }
    
    // Owner-Setter
    
    /// @notice This function allocated free tokens to a address
    /// @dev This function takes in a address and a count and allocates tokens to the address
    /// @param _address Address to allocate tokens to
    /// @param count Number of tokens to allocate
    function allocateTokens(address _address, uint256 count) external onlyOwner {
        require (
            tokensAllocated[_address] + count <= maxMintPerAddress,
            "Max mint per address reached"
        );
        tokensAllocated[_address] += count;
    }
    
    /// @notice Whitelist token
    /// @param token Address of the token
    function allowToken(address token) external onlyOwner {
        isTokenAllowed[token] = true;
        emit TokenAllowed(token);
    }
    
    /// @notice Sets default royalty
    /// @param receiver Address of the receiver
    /// @param feeNumerator Fee numerator, 10000 is 100%
    function setDefaultRoyalty(
        address receiver,
        uint96 feeNumerator
    ) external onlyOwner {
        _setDefaultRoyalty(receiver, feeNumerator);
    }
    
    /// @notice Set Royalty for specific token
    /// @param tokenId Id of the token
    /// @param receiver The receiver of the Royalty
    /// @param feeNumerator The fee numerator, 10000 is 100%
    function setTokenRoyalty(
        uint256 tokenId,
        address receiver,
        uint96 feeNumerator
    ) external onlyOwner {
        _setTokenRoyalty(tokenId, receiver, feeNumerator);
    }
    
    /// @notice Deletes the default setup Royalty
    function deleteDefaultRoyalty() external onlyOwner {
        _deleteDefaultRoyalty();
    }
    
    /// @notice Resets the Royalty for specific tokenId
    /// @param tokenId The NFT tokenId
    function resetTokenRoyalty(uint256 tokenId) external onlyOwner {
        _resetTokenRoyalty(tokenId);
    }
    
    /// @notice Sets the max total supply
    /// @param _maxTotalSupply The new max total supply
    function setMaxTotalSupply(uint256 _maxTotalSupply) external onlyOwner {
        maxSupply = _maxTotalSupply;
    }
    
    /// @notice Register a trait
    /// @dev Should be used to register a trait
    /// @param traitContracts Addresses of the trait contract
    function registerTrait(address[] calldata traitContracts) external onlyOwner {
        unchecked {
            for (uint256 i = 0; i < traitContracts.length; i++) {
                registeredTraits[traitContracts[i]] = true;
                emit RegisterTrait(traitContracts[i]);
            }
        }
    }
    
    /// @notice De-register a trait
    /// @dev Should be used to de-register a trait
    /// @param traitContracts Trait contracts to be de-registered
    function deRegisterTrait(address[] calldata traitContracts) external onlyOwner {
        unchecked {
            for (uint256 i = 0; i < traitContracts.length; i++) {
                delete registeredTraits[traitContracts[i]];
                emit RegisterTrait(traitContracts[i]);
            }
        }
    }
    
    /// @notice Burn tokenId
    /// @param tokenId ID of the token to be burn
    function burn(uint256 tokenId) external onlyOwner {
        _burn(tokenId);
    }
    
    /// @notice This function is used to set the mint price for different tokens
    /// @param paymentToken Address of the token to be used for payment
    /// @param price Price of the token
    function setMintPrice(address paymentToken, uint256 price) external onlyOwner {
        mintPrice[paymentToken] = price;
        emit ChangeMintPrice(paymentToken, price);
    }
    
    /// @notice This function is used to get the mint price for different tokens
    /// @param paymentToken Address of the token to be used for payment
    function getMintPrice(address paymentToken) public view returns (uint256) {
        return mintPrice[paymentToken];
    }
    
    /// @notice Change the base URI
    /// @param newBaseURI New base uri
    function changeBaseURI(string memory newBaseURI) external onlyOwner {
        baseURI = newBaseURI;
    }
    
    /// @notice This function is used to set maximum tokens that can be minted by an user
    /// @param _maxMintPerAddress Maximum tokens that can be minted by an user
    function setMaxAllowedToMint(uint256 _maxMintPerAddress) external onlyOwner {
        maxMintPerAddress = _maxMintPerAddress;
    }
    
    // Opensea overiding functions
    function setApprovalForAll(
        address operator,
        bool approved
    ) public override onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }
    
    function approve(
        address operator,
        uint256 tokenId
    ) public payable override onlyAllowedOperatorApproval(operator) {
        super.approve(operator, tokenId);
    }
    
    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public payable override onlyAllowedOperator(from) {
        super.transferFrom(from, to, tokenId);
    }
    
    /// @inheritdoc ERC721AUpgradeable
    function supportsInterface(
        bytes4 interfaceId
    )
    public
    view
    override(ERC721AUpgradeable, ERC2981Upgradeable)
    returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
    
    // LAYERZERO FUNCTIONS
    /// @dev This function is used to mint tokens
    /// @param toSend Address to send the minted tokens
    /// @param tokensToMint Amount of tokens to mint
    function mintTokens (address toSend, uint256 tokensToMint) internal {
        if (availableMints + tokensToMint > maxSupply) {
            revert MaxMintReached();
        }
        availableMints += tokensToMint;
       _mint(toSend, tokensToMint);
    }

    /// @notice This function is used to mint tokens by receiving input from Layer 0
    function _nonblockingLzReceive(
        uint16,
        bytes memory,
        uint64,
        bytes memory _payload
    ) internal override {
        (bytes memory mintTo, uint16 tokensToMint) = abi.decode(
            _payload,
            (bytes,uint16)
        );
        address minter = bytesToAddress(mintTo);
        mintTokens(minter, uint256(tokensToMint));
    }
    
    function bytesToAddress(bytes memory data) public pure returns (address) {
        require(data.length == 20, "Invalid address length");
        
        address addr;
        assembly {
            addr := mload(add(data, 20))
        }
        return addr;
    }
}

