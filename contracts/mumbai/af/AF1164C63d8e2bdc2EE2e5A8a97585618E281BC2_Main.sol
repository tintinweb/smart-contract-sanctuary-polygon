pragma solidity ^0.8.7;

import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";

import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router01.sol";

import "@openzeppelin/contracts/access/Ownable.sol";

import {DigiToadsAccessControl} from "../access/DigiToadsAccessControl.sol";

import "../libraries/DecimalsCorrectionLibrary.sol";
import "../interfaces/IERC677.sol";

contract ChainlinkFunder is DigiToadsAccessControl {
    using DecimalCorrectionLibrary for uint256;

    uint256 public constant BPS = 100;

    IERC677 public immutable linkToken;

    address public immutable vrfCoordinator;
    address public immutable keeperRegistry;

    AggregatorV3Interface public immutable nativeToUsdPriceFeed;
    AggregatorV3Interface public immutable linkToUsdPriceFeed;

    IUniswapV2Router01 public uniV2Router;

    uint256 public nativeInUsdMinToFund = 0.5 * 10**18; // 0.5$

    uint256 public swapSlippagePercentage = 20 * BPS; // 20%;

    uint256 public vrfTopUpPercentage = 75 * BPS; // 75% of incoming fund amount;
    uint256 public keeperTopUpPercentage = 100 * BPS - vrfTopUpPercentage; // rest goes to keeper;

    uint256 public keeperId;
    uint64 public vrfSubscriptionId;

    address[] private _uniV2NativeToLinkSwapPath;

    event Fund(
        uint256 amountNative,
        uint256 keeperFundAmount,
        uint256 vrfFundAmount
    );

    constructor(
        uint256 _keeperId,
        uint64 _vrfSubscriptionId,
        address _linkToken,
        address _vrfCoordinator,
        address _keeperRegistry,
        address _cryptoToUsdPriceFeed,
        address _linkToUsdPriceFeed,
        address _uniV2Router,
        address[] memory uniV2NativeToLinkSwapPath
    ) {
        keeperId = _keeperId;
        vrfSubscriptionId = _vrfSubscriptionId;

        linkToken = IERC677(_linkToken);

        vrfCoordinator = _vrfCoordinator;
        keeperRegistry = _keeperRegistry;

        nativeToUsdPriceFeed = AggregatorV3Interface(_cryptoToUsdPriceFeed);
        linkToUsdPriceFeed = AggregatorV3Interface(_linkToUsdPriceFeed);

        uniV2Router = IUniswapV2Router01(_uniV2Router);
        _uniV2NativeToLinkSwapPath = uniV2NativeToLinkSwapPath;
    }

    function fund() external payable {
        uint256 _amountNative = msg.value;

        uint256 _amountNativeInUsd = _getNativeInUsd(_amountNative);

        require(
            _amountNativeInUsd >= nativeInUsdMinToFund,
            "Insufficient funding"
        );

        uint256 _linkReceived = _swapNativeToLink(
            _amountNative,
            _calculateAmountOutMin(_amountNativeInUsd)
        );

        uint256 _keeperFundAmount = _calcPercentageFromValue(
            _linkReceived,
            keeperTopUpPercentage
        );

        uint256 _vrfFundAmount = _linkReceived - _keeperFundAmount;

        _fundKeeper(_keeperFundAmount);
        _fundVRFSubscription(_linkReceived - _keeperFundAmount);

        emit Fund(_amountNative, _keeperFundAmount, _vrfFundAmount);
    }

    function setVrfTopUpPercentage(uint256 _newValue)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(_newValue <= 100 * BPS, "invalid new value");

        vrfTopUpPercentage = _newValue;
        keeperTopUpPercentage = 100 * BPS - _newValue;
    }

    function setMinToFundInUsd(uint256 _newVal)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        nativeInUsdMinToFund = _newVal;
    }

    function setSwapPath(address[] memory _newPath)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        delete _uniV2NativeToLinkSwapPath;
        _uniV2NativeToLinkSwapPath = _newPath;
    }

    function setSwapSlippagePercentage(uint256 _newValue)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        swapSlippagePercentage = _newValue;
    }

    function setKeeperId(uint256 _newValue)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        keeperId = _newValue;
    }

    function setRouter(IUniswapV2Router01 newRouter)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        uniV2Router = newRouter;
    }

    function setVRFSubscriptionId(uint64 _newValue)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        vrfSubscriptionId = _newValue;
    }

    function getMinRequiredNativeToFund() external view returns (uint256) {
        return (nativeInUsdMinToFund * (10**18)) / _getNativeInUsd(10**18);
    }

    function _swapNativeToLink(uint256 _amountIn, uint256 _amountOutMin)
        internal
        returns (uint256 linkReceived)
    {
        uint256[] memory _amounts = uniV2Router.swapExactETHForTokens{
            value: _amountIn
        }(
            _amountOutMin,
            _uniV2NativeToLinkSwapPath,
            address(this),
            block.timestamp
        );

        linkReceived = _amounts[_amounts.length - 1];
    }

    function _fundKeeper(uint256 _linkAmount) internal {
        if (_linkAmount == 0) return;

        linkToken.transferAndCall(
            keeperRegistry,
            _linkAmount,
            abi.encode(keeperId)
        );
    }

    function _fundVRFSubscription(uint256 _linkAmount) internal {
        if (_linkAmount == 0) return;

        linkToken.transferAndCall(
            vrfCoordinator,
            _linkAmount,
            abi.encode(vrfSubscriptionId)
        );
    }

    function _getUsdToLinkAmount(uint256 _usdAmount)
        internal
        view
        returns (uint256)
    {
        uint256 linkUsdPrice = _getLinkInUsd(10**18);
        return (_usdAmount * (10**18)) / linkUsdPrice;
    }

    function _calculateAmountOutMin(uint256 _cryptoInUsd)
        internal
        view
        returns (uint256)
    {
        uint256 linkAmount = _getUsdToLinkAmount(_cryptoInUsd);
        return
            linkAmount -
            _calcPercentageFromValue(linkAmount, swapSlippagePercentage);
    }

    function _getNativeInUsd(uint256 _amount) internal view returns (uint256) {
        return (_getBase18Answer(nativeToUsdPriceFeed) * _amount) / 10**18;
    }

    function _getLinkInUsd(uint256 _amount) internal view returns (uint256) {
        return (_getBase18Answer(linkToUsdPriceFeed) * _amount) / 10**18;
    }

    function _getBase18Answer(AggregatorV3Interface priceFeed)
        internal
        view
        returns (uint256)
    {
        uint256 _priceFeedDecimals = priceFeed.decimals();
        (, int256 _answer, , , ) = priceFeed.latestRoundData();
        return uint256(_answer).convertToBase18(_priceFeedDecimals);
    }

    function _calcPercentageFromValue(uint256 _value, uint256 _percentage)
        internal
        pure
        returns (uint256)
    {
        return (_value * _percentage) / (100 * BPS);
    }
}

