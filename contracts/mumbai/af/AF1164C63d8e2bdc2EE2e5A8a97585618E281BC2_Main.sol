pragma solidity ^0.8.0;

import {DigiToadsAccessControl} from "../access/DigiToadsAccessControl.sol";
import {Entry} from "../structs/Entry.sol";

import "hardhat/console.sol";

contract MatchMakingPool is DigiToadsAccessControl {
    /// @dev 400 - 800, 800 - 1200 ... 2k
    mapping(uint256 => Entry[]) public mmPools;
    /// @dev requestId => Entries
    mapping(uint256 => Entry[]) public mmPoolsLocked;
    /// @dev requestId => tokenId
    mapping(uint256 => uint256) public mmPoolsOverdue;

    uint256 public highestPool;
    uint256 public highestUsersCount;

    event EnteredPool(Entry indexed entry, uint256 pool);
    event EnteredOverduePool(uint256 indexed tokenId, uint256 pool);
    event EnterLockedPool(uint256 indexed requestId);
    event DeleteAllFromPool(uint256 indexed pool);
    event DeleteAllFromLockedPool(uint256 indexed requestId);
    event DeleteFromPool(uint256 indexed pool, uint256 idx);
    event DeleteFromLockedPool(uint256 indexed pool, uint256 idx);
    event DeleteFromOverduePool(uint256 indexed pool);

    function enterPool(uint256 pool, Entry memory entry)
        external
        onlyRole(MMPOOL_CONTROLLER_ROLE)
    {
        mmPools[pool].push(entry);

        if (pool > highestPool) {
            highestPool = pool;
        }

        if (mmPools[pool].length > highestUsersCount) {
            highestUsersCount = mmPools[pool].length;
        }

        console.log(highestPool, highestUsersCount);

        emit EnteredPool(entry, pool);
    }

    function enterOverduePool(uint256 requestId, uint256 tokenId)
        external
        onlyRole(MMPOOL_CONTROLLER_ROLE)
    {
        mmPoolsOverdue[requestId] = tokenId;

        emit EnteredOverduePool(tokenId, requestId);
    }

    function lockUsers(uint256 requestId, Entry[] memory tokenIds)
        external
        onlyRole(MMPOOL_CONTROLLER_ROLE)
    {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            mmPoolsLocked[requestId].push(tokenIds[i]);
        }

        emit EnterLockedPool(requestId);
    }

    function deleteAllFromPool(uint256 pool)
        external
        onlyRole(MMPOOL_CONTROLLER_ROLE)
    {
        delete mmPools[pool];

        emit DeleteAllFromPool(pool);
    }

    function deleteAllFromLockedPool(uint256 requestId)
        external
        onlyRole(MMPOOL_CONTROLLER_ROLE)
    {
        delete mmPoolsLocked[requestId];

        emit DeleteAllFromLockedPool(requestId);
    }

    function deleteFromPool(uint256 pool, uint256 idx)
        external
        onlyRole(MMPOOL_CONTROLLER_ROLE)
    {
        mmPools[pool][idx] = mmPools[pool][mmPools[pool].length - 1];
        mmPools[pool].pop();

        emit DeleteFromPool(pool, idx);
    }

    function deleteFromOverduePool(uint256 requestId)
        external
