pragma solidity ^0.8.0;

import {ERC721Characters, DigiToadsAccessControl} from "../ERC721Characters.sol";
import {Bot} from "./Bot.sol";

import {ChangeableStats} from "../structs/ChangeableStats.sol";
import {Entry} from "../structs/Entry.sol";
import {Stats} from "../structs/Stats.sol";
import {Class} from "../enums/Class.sol";

import "hardhat/console.sol";

struct BattleEntry {
    uint256 tokenId1;
    uint256 tokenId2;
    uint256 randomWord;
    address firstUser;
    address secondUser;
}

contract Battle is DigiToadsAccessControl {
    ERC721Characters public immutable charactersContract;
    Bot public immutable bot;

    /// @dev warrior -> archer, archer -> mage, mage -> warrior
    mapping(Class => Class) public antiClasses;

    event Win(uint256 indexed tokenIdWinner, uint256 indexed tokenIdLoser);

    constructor(
        Class[] memory antiClasses_,
        ERC721Characters characters_,
        Bot bot_
    ) DigiToadsAccessControl() {
        require(address(characters_) != address(0), "Zero address");

        for (uint256 i = 0; i < antiClasses_.length; i++) {
            antiClasses[Class(i)] = antiClasses_[i];
        }

        charactersContract = characters_;
        bot = bot_;
        grantRole(BATTLE_CONTROLLER_ROLE, msg.sender);
    }

    function battle(BattleEntry memory battleEntry)
        public
        onlyRole(BATTLE_CONTROLLER_ROLE)
    {
        (
            Class classFirst,
            ChangeableStats memory changeableStatsFirst,
            Stats memory statsFirst,
            ,
            ,
            uint256 energyFirst,
            ,

        ) = charactersContract.characters(battleEntry.tokenId1);

        (
            Class classSecond,
            ChangeableStats memory changeableStatsSecond,
            Stats memory statsSecond,
            ,
            ,
            uint256 energySecond,
            ,

        ) = charactersContract.characters(battleEntry.tokenId2);

        uint256 gearScoreFirst = 3 *
            (statsFirst.defensePoints + statsFirst.attackPoints) +
            statsFirst.basicStats.healthPoints +
            statsFirst.basicStats.manaPoints;

        uint256 gearScoreSecond = 3 *
            (statsSecond.defensePoints + statsSecond.attackPoints) +
            statsSecond.basicStats.healthPoints +
            statsSecond.basicStats.manaPoints;

        // stronger
        if (antiClasses[classFirst] == classSecond) {
            gearScoreFirst = (gearScoreFirst * 11) / 10;
        }
        // weaker
        else if (antiClasses[classSecond] == classFirst) {
            gearScoreSecond = (gearScoreSecond * 11) / 10;
        }

        uint256 winnerGearScore = (battleEntry.randomWord %
            (gearScoreFirst + gearScoreSecond)) + 1;

        // stack is too deep solving
        BattleEntry memory _battleEntry = battleEntry;

        if (winnerGearScore <= gearScoreFirst) {
            _updateStats(
                _battleEntry,
                changeableStatsFirst,
                changeableStatsSecond,
                _battleEntry.tokenId1,
                _battleEntry.tokenId2,
                energyFirst,
                energySecond,
                _battleEntry.firstUser,
                _battleEntry.secondUser
            );
        } else {
            _updateStats(
                _battleEntry,
                changeableStatsSecond,
                changeableStatsFirst,
                _battleEntry.tokenId2,
                _battleEntry.tokenId1,
                energySecond,
                energyFirst,
                _battleEntry.secondUser,
                _battleEntry.firstUser
            );
        }
    }

    function _updateStats(
        BattleEntry memory battleEntry,
        ChangeableStats memory winner,
        ChangeableStats memory loser,
        uint256 winnerTokenId,
        uint256 loserTokenId,
        uint256 energyWinner,
        uint256 energyLoser,
        address winnerAddress,
        address loserAddress
    ) private {
        uint256 hpAndMpModifierForWinner = (battleEntry.randomWord % 4) + 1;

        winner.healthPoints =
            (winner.healthPoints * hpAndMpModifierForWinner) /
            10;
        winner.manaPoints = (winner.manaPoints * hpAndMpModifierForWinner) / 10;

        loser.healthPoints = 0;
        loser.manaPoints = 0;

        if (winnerAddress != address(bot)) {
            charactersContract.changeCharacterStats(
                winnerTokenId,
                winner.healthPoints,
                winner.manaPoints,
                energyWinner - 1
            );
        }

        if (loserAddress != address(bot)) {
            charactersContract.changeCharacterStats(
                loserTokenId,
                loser.healthPoints,
                loser.manaPoints,
                energyLoser - 1
            );
        }

        emit Win(winnerTokenId, loserTokenId);
    }
}

