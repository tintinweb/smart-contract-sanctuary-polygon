pragma solidity ^0.8.0;

import {AutomationCompatibleInterface} from "@chainlink/contracts/src/v0.8/AutomationCompatible.sol";
import {Battle, DigiToadsAccessControl, ERC721Characters, BattleEntry} from "./Battle.sol";
import {MatchMakingPool} from "./MatchMakingPool.sol";
import {Bot} from "./Bot.sol";
import {Leaderboard} from "./Leaderboard.sol";
import {ERC20Poles} from "../ERC20Poles.sol";

import {Entry} from "../structs/Entry.sol";
import {Character} from "../structs/Character.sol";
import {ChangeableStats} from "../structs/ChangeableStats.sol";
import {Stats} from "../structs/Stats.sol";
import {VRFConsumer} from "./VRFConsumer.sol";

import {ChainlinkFunder} from "./ChainlinkFunder.sol";
import "hardhat/console.sol";

contract Main is AutomationCompatibleInterface, DigiToadsAccessControl {
    bytes32 public constant VRF_KEY_HASH =
        0x4b09e658ed251bcafeebbc69400383d49f344ace09b9576fe248bb02c003fe9f;

    uint32 public constant VRF_ENTER_POOL_CALLBACK_GAS_LIMIT = 2_000_000;
    uint32 public constant VRF_PERFORM_UPKEEP_CALLBACK_GAS_LIMIT = 2_000_000;

    ERC721Characters public immutable characters;
    VRFConsumer public immutable vrfConsumer;
    ERC20Poles public immutable poles;
    ChainlinkFunder public immutable chainLinkFunder;

    Battle private immutable _battle;
    MatchMakingPool private immutable _mmPool;
    Bot private immutable _bot;
    Leaderboard private immutable _leaderboard;

    uint256 private immutable _expToWinner;
    uint256 private immutable _expToLoser;

    uint256 private _amountToWinner;
    uint256 private _amountToLoser;

    event PoolEntered(uint256 indexed pool, uint256 indexed tokenId);
    event BattleStarted(uint256 indexed pool, uint256 indexed requestId);
    event UpkeepPerformed(uint256 indexed pool, uint256 idx, address user);
    event PreBattle(uint256 indexed tokenId1, uint256 indexed tokenId2);
    event PostBattle(
        uint256 indexed tokenId1,
        uint256 indexed tokenId2,
        address user1,
        address user2
    );
    event AddLeadershipPoints(
        address indexed winner,
        address indexed loser,
        uint256 expToWinner,
        uint256 expToLoser
    );
    event PolesAfterBattleAmountChanged(uint256 toWinner, uint256 toLoser);
    event EnterPoolFulfillCallback(
        uint256 indexed requestId,
        uint256[] randomWords
    );
    event PerformUpkeepFulfillCallback(
        address indexed user,
        uint256[] randomWords
    );

    modifier onlyConsumer() {
        require(hasRole(VRF_CONSUMER_ROLE, msg.sender), "Not a consumer");
        _;
    }

    modifier coverChainLinkExpenses() {
        chainLinkFunder.fund{value: msg.value}();
        _;
    }

    constructor(
        Battle battle,
        MatchMakingPool mmPool,
        ERC721Characters characters_,
        Bot bot,
        VRFConsumer _vrfConsumer,
        ERC20Poles _poles,
        ChainlinkFunder _chainLinkFunder,
        Leaderboard leaderboard,
        uint256 expToWinner,
        uint256 expToLoser,
        uint256 polesWinAmount,
        uint256 polesLoseAmount
    ) {
        require(address(battle) != address(0), "Zero address");
        require(address(mmPool) != address(0), "Zero address");
        require(address(characters_) != address(0), "Zero address");
        require(address(bot) != address(0), "Zero address");
        require(address(_vrfConsumer) != address(0), "Zero address");
        require(address(_poles) != address(0), "Zero address");
        require(address(_chainLinkFunder) != address(0), "Zero address");
        require(address(leaderboard) != address(0), "Zero address");
        require(expToWinner > 0, "Zero exp");
        require(expToLoser > 0, "Zero exp");

        chainLinkFunder = _chainLinkFunder;
        vrfConsumer = _vrfConsumer;
        poles = _poles;
        _battle = battle;
        _mmPool = mmPool;
        _bot = bot;
        _leaderboard = leaderboard;
        characters = characters_;
        _expToWinner = expToWinner;
        _expToLoser = expToLoser;
        _amountToWinner = polesWinAmount;
        _amountToLoser = polesLoseAmount;

        grantRole(VRF_CONSUMER_ROLE, address(this));
        grantRole(VRF_CONSUMER_ROLE, address(vrfConsumer));
        grantRole(BATTLE_CONTROLLER_ROLE, msg.sender);
    }

    function changeAmountOfPolesAfterBattle(uint256 toWinner, uint256 toLoser)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _amountToWinner = toWinner;
        _amountToLoser = toLoser;

        emit PolesAfterBattleAmountChanged(toWinner, toLoser);
    }

    function enterPool() external payable coverChainLinkExpenses {
        (Character memory character, uint256 tokenId) = characters
            .getCurrentCharacterStats(msg.sender);

        (bool locked, ) = characters.locked(tokenId);

        require(!locked, "Token is locked");
        require(character.energy > 0, "No energy");

        characters.changeLock(tokenId, true);

        uint256 gearScore = 3 *
            (character.stats.defensePoints + character.stats.attackPoints) +
            character.stats.basicStats.healthPoints +
            character.stats.basicStats.manaPoints;
        uint256 pool = (gearScore / 4000) - 1;

        if (_mmPool.getPoolLength(pool) == 3) {
            _createBattlePool(pool, tokenId);
        } else {
            _mmPool.enterPool(
                pool,
                Entry(tokenId, block.timestamp, msg.sender)
            );

            emit PoolEntered(pool, tokenId);
        }
    }

    function performUpkeep(bytes calldata performData) external {
        (uint256 pool, uint256 idx, address user) = abi.decode(
            performData,
            (uint256, uint256, address)
        );

        require(
            _mmPool.checkIfUserIsInPoolForLongTime(pool, idx),
            "Not ready for shadow fight"
        );

        require(user != address(0), "Invalid address");

        uint256 requestId = vrfConsumer.requestRandomWords(
            2,
            VRF_KEY_HASH,
            VRF_PERFORM_UPKEEP_CALLBACK_GAS_LIMIT,
            address(this),
            this.onPerformUpKeepFulfillRandomWordsCallback.selector,
            performData
        );

        _mmPool.enterOverduePool(
            requestId,
            _mmPool.getFromPool(pool, idx).characterTokenId
        );

        _mmPool.deleteFromPool(pool, idx);

        emit UpkeepPerformed(pool, idx, user);
    }

    function onEnterPoolFulfillRandomWords(
        uint256 requestId,
        uint256[] memory rndWords,
        bytes memory
    ) external onlyConsumer {
        uint256 idx1 = rndWords[0] % 3;

        // select ids of users to fight with each other
        // idx0(3) fights with idx1
        // idx2 fights with idx3
        (uint256 idx2, uint256 idx3) = idx1 == 0
            ? (1, 2)
            : (idx1 == 1 ? (0, 2) : (0, 1));

        for (
            (uint256 firstIdx, uint256 secondIdx, uint256 i) = (3, idx1, 0);
            i < 2;
            (firstIdx, secondIdx, i) = (idx2, idx3, ++i)
        ) {
            Entry memory firstUser = _mmPool.getFromLockedPool(
                requestId,
                firstIdx
            );

            Entry memory secondUser = _mmPool.getFromLockedPool(
                requestId,
                secondIdx
            );

            _doBattle(firstUser, secondUser, rndWords[i + 1]);
        }

        _mmPool.deleteAllFromLockedPool(requestId);

        emit EnterPoolFulfillCallback(requestId, rndWords);
    }

    function onPerformUpKeepFulfillRandomWordsCallback(
        uint256 requestId,
        uint256[] memory rndWords,
        bytes memory callbackData
    ) external onlyConsumer {
        (uint256 pool, , address user) = abi.decode(
            callbackData,
            (uint256, uint256, address)
        );

        uint256 firstTokenId = _mmPool.getFromOverduePool(requestId);
        uint256 secondTokenId = _bot.getFighter(pool, rndWords[0]);

        characters.changeLock(secondTokenId, true);

        _doBattle(
            Entry(firstTokenId, block.timestamp, user),
            Entry(secondTokenId, block.timestamp, address(_bot)),
            rndWords[1]
        );

        _mmPool.deleteFromOverduePool(requestId);

        emit PerformUpkeepFulfillCallback(user, rndWords);
    }

    function checkUpkeep(bytes calldata)
        external
        view
        override
        returns (bool, bytes memory)
    {
        for (uint256 poolId = 0; poolId <= _mmPool.highestPool(); poolId++) {
            for (uint256 i = 0; i < _mmPool.highestUsersCount(); i++) {
                if (_mmPool.getFromPool(poolId, i).characterTokenId == 0) {
                    continue;
                }

                if (_mmPool.checkIfUserIsInPoolForLongTime(poolId, i)) {
                    return (
                        true,
                        abi.encode(
                            poolId,
                            i,
                            _mmPool.getFromPool(poolId, i).user
                        )
                    );
                }
            }
        }

        return (false, bytes(""));
    }

    function _createBattlePool(uint256 pool, uint256 callerTokenId) internal {
        uint256 requestId = vrfConsumer.requestRandomWords(
            3,
            VRF_KEY_HASH,
            VRF_ENTER_POOL_CALLBACK_GAS_LIMIT,
            address(this),
            this.onEnterPoolFulfillRandomWords.selector,
            abi.encode(pool, callerTokenId, msg.sender)
        );
        Entry[] memory tokenIds = new Entry[](4);
        tokenIds[0] = _mmPool.getFromPool(pool, 0);
        tokenIds[1] = _mmPool.getFromPool(pool, 1);
        tokenIds[2] = _mmPool.getFromPool(pool, 2);
        tokenIds[3] = Entry(callerTokenId, block.timestamp, msg.sender);

        _mmPool.lockUsers(requestId, tokenIds);
        _mmPool.deleteAllFromPool(pool);

        emit BattleStarted(pool, requestId);
    }

    function _doBattle(
        Entry memory firstUser,
        Entry memory secondUser,
        uint256 randomWord
    ) private {
        _preBattle(secondUser.characterTokenId, firstUser.characterTokenId);
        _battle.battle(
            BattleEntry(
                secondUser.characterTokenId,
                firstUser.characterTokenId,
                randomWord,
                secondUser.user,
                firstUser.user
            )
        );
        _postBattle(
            secondUser.characterTokenId,
            firstUser.characterTokenId,
            firstUser.user,
            secondUser.user
        );
    }

    function _postBattle(
        uint256 tokenId1,
        uint256 tokenId2,
        address firstUser,
        address secondUser
    ) private {
        (
            ,
            ChangeableStats memory changeableStatsFirst,
            Stats memory statsFirst,
            ,
            ,
            ,
            ,

        ) = characters.characters(tokenId1);
        (, , Stats memory statsSecond, , , , , ) = characters.characters(
            tokenId2
        );

        uint256 winner = changeableStatsFirst.healthPoints == 0
            ? tokenId2
            : tokenId1;
        uint256 loser = winner == tokenId1 ? tokenId2 : tokenId1;

        characters.addExpToCharacter(winner, _expToWinner);
        characters.addExpToCharacter(loser, _expToLoser);

        address winnerAddress = characters.ownerOf(winner);

        uint256 gearScoreFirst = 3 *
            (statsFirst.defensePoints + statsFirst.attackPoints) +
            statsFirst.basicStats.healthPoints +
            statsFirst.basicStats.manaPoints;
        uint256 gearScoreSecond = 3 *
            (statsSecond.defensePoints + statsSecond.attackPoints) +
            statsSecond.basicStats.healthPoints +
            statsSecond.basicStats.manaPoints;

        if (_leaderboard.isSeasonStarted()) {
            _leaderboard.updateLeaderboardRating(
                winnerAddress,
                firstUser,
                secondUser,
                gearScoreFirst,
                gearScoreSecond
            );
        }

        {
            uint256 winnerGearScore = winner == tokenId1
                ? gearScoreFirst
                : gearScoreSecond;
            uint256 loserGearScore = winnerGearScore == gearScoreFirst
                ? gearScoreSecond
                : gearScoreFirst;

            if (winnerAddress != address(_bot)) {
                poles.mint(winnerAddress, winnerGearScore * _amountToWinner);
            }

            address loserAddress = characters.ownerOf(loser);

            if (loserAddress != address(_bot)) {
                poles.mint(loserAddress, loserGearScore * _amountToLoser);
            }
        }

        characters.changeLock(tokenId1, false);
        characters.changeLock(tokenId2, false);

        emit PostBattle(tokenId1, tokenId2, firstUser, secondUser);
    }

    function _preBattle(uint256 tokenId1, uint256 tokenId2) private {
        characters.recoverStats(tokenId1);
        characters.recoverStats(tokenId2);

        emit PreBattle(tokenId1, tokenId2);
    }
}

