        external
        view
        returns (Entry memory)
    {
        return mmPoolsLocked[requestId][idx];
    }

    function getFromOverduePool(uint256 requestId)
        external
        view
        returns (uint256)
    {
        return mmPoolsOverdue[requestId];
    }

