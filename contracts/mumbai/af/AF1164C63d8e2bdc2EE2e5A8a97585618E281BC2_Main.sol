pragma solidity ^0.8.0;

import {DigiToadsAccessControl} from "../access/DigiToadsAccessControl.sol";
import {ERC20Poles} from "../ERC20Poles.sol";
import {Bot} from "./Bot.sol";

import {ECDSA} from "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import {MerkleProof} from "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";

import "hardhat/console.sol";

contract Leaderboard is DigiToadsAccessControl {
    using ECDSA for bytes32;

    ERC20Poles public immutable poles;
    Bot public immutable bot;

    uint256 public immutable leaderboardInitialRating;

    uint256 public topOneWinAmount;
    uint256 public subtractionPerPlace;
    uint256 public maxLeaderboardUsers;
    bool public isSeasonStarted;
    uint256 public currentSeasonId;

    mapping(uint256 => mapping(address => bool)) public isClaimed;
    mapping(uint256 => mapping(address => uint256)) public leaderboardPoints;
    mapping(uint256 => mapping(address => bool)) public isInSeason;
    mapping(uint256 => uint256) public seasonUsers;
    mapping(uint256 => bytes32) public merkleRoot;
    mapping(uint256 => address[]) private _participants;

    event StartSeason(uint256 indexed seasonId);
    event EndSeason(uint256 indexed seasonId);
    event Claim(
        uint256 indexed seasonId,
        address indexed user,
        uint256 indexed amount
    );
    event AddLeadershipPoints(
        address indexed winner,
        address indexed loser,
        uint256 winnerPoints,
        uint256 loserPoints,
        uint256 expToWinner,
        uint256 expToLoser
    );

    constructor(
        ERC20Poles poles_,
        Bot bot_,
        uint256 topOneWinAmount_,
        uint256 subtractionPerPlace_,
        uint256 maxLeaderboardUsers_,
        uint256 leaderboardInitialPoints
    ) DigiToadsAccessControl() {
        poles = poles_;
        bot = bot_;
        topOneWinAmount = topOneWinAmount_;
        subtractionPerPlace = subtractionPerPlace_;
        maxLeaderboardUsers = maxLeaderboardUsers_;
        currentSeasonId = 0;
        isSeasonStarted = false;
        leaderboardInitialRating = leaderboardInitialPoints;
    }

    function startSeason() external onlyRole(SEASON_CONTROLLER_ROLE) {
        require(!isSeasonStarted, "Already started");

        currentSeasonId++;
        merkleRoot[currentSeasonId] = bytes32("");
        isSeasonStarted = true;

        emit StartSeason(currentSeasonId);
    }

    function endSeason(bytes32 merkleRoot_)
        external
        onlyRole(SEASON_CONTROLLER_ROLE)
    {
        require(isSeasonStarted, "Season is not started yet");

        merkleRoot[currentSeasonId] = merkleRoot_;
        isSeasonStarted = false;

        emit EndSeason(currentSeasonId);
    }

    function claim(
        uint256 amount,
        uint256 seasonId,
        bytes32[] calldata proof
    ) public {
        require(!isClaimed[currentSeasonId][msg.sender], "Already claimed");
        require(
            _validateProof(
                proof,
                seasonId,
                keccak256(abi.encodePacked(msg.sender, amount))
            ),
            "Invalid proof"
        );

        isClaimed[seasonId][msg.sender] = true;
        poles.mint(msg.sender, amount);

        emit Claim(seasonId, msg.sender, amount);
    }

    function changeTopOneWinAmount(uint256 winAmount)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        topOneWinAmount = winAmount;
    }

    function changeSubtractionPerPlace(uint256 newValue)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        subtractionPerPlace = newValue;
    }

    function changeMaxLeaderboardUsers(uint256 newValue)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        maxLeaderboardUsers = newValue;
    }

    function _validateProof(
        bytes32[] calldata proof,
        uint256 seasonId,
        bytes32 node
    ) private view returns (bool) {
        return MerkleProof.verifyCalldata(proof, merkleRoot[seasonId], node);
    }

    function getParticipants(uint256 seasonId)
        public
        view
        returns (address[] memory)
    {
        return _participants[seasonId];
    }

    function _initLeaderboardUser(address user) private {
        if (isInSeason[currentSeasonId][user]) return;

        leaderboardPoints[currentSeasonId][user] = leaderboardInitialRating;
        isInSeason[currentSeasonId][user] = true;
        _participants[currentSeasonId].push(user);
        seasonUsers[currentSeasonId]++;
    }

    function updateLeaderboardRating(
        address winner,
        address firstUser,
        address secondUser,
        uint256 gearScoreFirst,
        uint256 gearScoreSecond
    ) external onlyRole(MAIN_CONTROLLER_ROLE) {
        uint256 winnerExp = 25;
        uint256 loserExp = 10;

        uint256 gearScoreWinner;

        address loser;
        uint256 gearScoreLoser;

        _initLeaderboardUser(firstUser);
        _initLeaderboardUser(secondUser);

        if (winner == firstUser) {
            gearScoreWinner = gearScoreFirst;

            loser = secondUser;
            gearScoreLoser = gearScoreSecond;
        } else {
            gearScoreWinner = gearScoreSecond;

            loser = firstUser;
            gearScoreLoser = gearScoreFirst;
        }

        if (gearScoreWinner > gearScoreLoser + 200) {
            winnerExp = 20;
            loserExp = 5;
        } else if (gearScoreWinner < gearScoreLoser - 200) {
            winnerExp = 40;
            loserExp = 15;
        }

        leaderboardPoints[currentSeasonId][winner] += winnerExp;

        if (leaderboardPoints[currentSeasonId][loser] < loserExp) {
            leaderboardPoints[currentSeasonId][loser] = 0;
        } else {
            leaderboardPoints[currentSeasonId][loser] -= loserExp;
        }

        _deleteLeaderboardPointsIfBot(firstUser);
        _deleteLeaderboardPointsIfBot(secondUser);

        emit AddLeadershipPoints(winner, loser, leaderboardPoints[currentSeasonId][winner], leaderboardPoints[currentSeasonId][loser], winnerExp, loserExp);
    }

    function _deleteLeaderboardPointsIfBot(address user) private {
        if (user != address(bot)) return;
        delete leaderboardPoints[currentSeasonId][user];
    }
}

