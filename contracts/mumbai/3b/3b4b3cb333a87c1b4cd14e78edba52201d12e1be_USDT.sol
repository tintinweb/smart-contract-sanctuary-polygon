    constructor(address _implementation, bytes memory _data) ERC1967Proxy(_implementation, _data) {}
}

contract SparkleRewardsVaultV1 is Initializable, PausableUpgradeable, UUPSUpgradeable, EIP712Whitelisting {
    struct Campaign {
        uint256 id;
        IERC20 token;
        bool pause;
        uint256 balance;
        address operator;
        uint256 rewardsLimit;
        mapping(address => bool) claimed;
    }

    mapping(uint256 => Campaign) private campaigns;

    event AddCampaign(uint256 indexed id, address token, address operator, uint256 rewardsLimit);
    event Deposit(uint256 indexed id, uint256 amount);
    event Claim(address indexed addr, uint256 indexed id, uint256 amount);

    constructor() {
        _disableInitializers();
    }

    modifier onlyOperator(uint256 id) {
        require(msg.sender == owner() || campaigns[id].operator == msg.sender, "only operator and owner allowed");
        _;
    }

    modifier requireCampaignExists(uint256 id) {
        require(id > 0, "Invalid campaign id");
        require(campaigns[id].id != 0, "Campaign does not exist");
        require(!campaigns[id].pause, "Campaign is paused");
        _;
    }

    modifier requireCampaignNotPaused(uint256 id) {
        require(!campaigns[id].pause, "Campaign is paused");
        _;
    }

    function _authorizeUpgrade(address newImplementation)
    internal
    override
    onlyOwner
    {}

    function getImplementation() external view returns (address) {
        return _getImplementation();
    }

    function initialize() public initializer {
        __Pausable_init();
        __Ownable_init();
        __UUPSUpgradeable_init();
    }

    function addCampaign(uint256 id, IERC20 token, address operator, uint256 rewardsLimit) public onlyOwner {
        require(campaigns[id].id == 0, "Campaign already exists");
        require(id > 0, "Invalid campaign id");
        require(address(token) != address(0), "Invalid token address");
        require(operator != address(0), "Invalid operator address");
        require(rewardsLimit > 0, "Invalid rewards limit");

        Campaign storage campaign = campaigns[id];
        campaign.id = id;
        campaign.token = token;
        campaign.operator = operator;
        campaign.rewardsLimit = rewardsLimit;

        emit AddCampaign(id, address(token), operator, rewardsLimit);
    }

    // deposit tokens to a campaign
    function deposit(uint256 id, uint256 amount) external requireCampaignExists(id) onlyOperator(id) {
        require(amount > 0, "Amount must be greater than 0");

        Campaign storage campaign = campaigns[id];
        campaign.balance += amount;
        if (campaign.token.allowance(msg.sender, address(this)) < amount) {
            revert("Not enough allowance");
        }
        campaign.token.transferFrom(msg.sender, address(this), amount);

        emit Deposit(id, amount);
    }

    // claim from a campaign
    function claim(uint256 id, bytes calldata signature) external
    requireCampaignExists(id)
    requireCampaignNotPaused(id)
    requiresWhitelist(signature) {
        require(whitelistEnabled, "Whitelist is disabled");

        Campaign storage campaign = campaigns[id];
        require(campaign.balance >= campaign.rewardsLimit, "Balance is not enough");
        require(!campaign.claimed[msg.sender], "Already claimed");

        campaign.balance -= campaign.rewardsLimit;
        campaign.claimed[msg.sender] = true;

        if (campaign.token.allowance(address(this), address(this)) < campaign.rewardsLimit) {
            campaign.token.approve(address(this), campaign.rewardsLimit);
        }
        campaign.token.transferFrom(address(this), msg.sender, campaign.rewardsLimit);

        emit Claim(msg.sender, id, campaign.rewardsLimit);
    }

    // withdraw all tokens from the campaign
    function withdrawAll(uint256 id) external requireCampaignExists(id) onlyOwner {
        require(campaigns[id].balance > 0, "Balance is 0");

        Campaign storage campaign = campaigns[id];
        uint256 balance = campaign.token.balanceOf(address(this));

        campaign.balance = 0;

        if (campaign.token.allowance(address(this), address(this)) < balance) {
            campaign.token.approve(address(this), balance);
        }
        campaign.token.transferFrom(address(this), msg.sender, balance);

        emit Deposit(id, balance);
    }

    function pauseCampaign(uint256 id) external requireCampaignExists(id) onlyOperator(id) {
        require(!campaigns[id].pause, "Campaign is already paused");
        campaigns[id].pause = true;
    }

    function unpauseCampaign(uint256 id) external requireCampaignExists(id) onlyOperator(id) {
        require(campaigns[id].pause, "Campaign is not paused");
        campaigns[id].pause = false;
    }

    function getCampaign(uint256 id) external view returns (uint256, address, bool, uint256, address, uint256) {
        Campaign storage c = campaigns[id];
        return (c.id, address(c.token), c.pause, c.balance, c.operator, c.rewardsLimit);
    }
}

