    function run() public {
        address srvProxyAddress = address(bytes20(hex"a27792ade1ba0c935297d67c52427a9e78668730"));
        SparkleRewardsVaultV1 wrappedProxyV1 = SparkleRewardsVaultV1(srvProxyAddress);

        uint256 deployerPrivateKey = vm.envUint("PRIVATE_KEY");
        vm.startBroadcast(deployerPrivateKey);

        USDT usdt = new USDT(1000000000000);
        address usdtAddr = address(usdt);
        USDT(usdtAddr).approve(srvProxyAddress, 1000000000000);

        wrappedProxyV1.addCampaign(2, usdt, wrappedProxyV1.owner(), 10000000);
        wrappedProxyV1.deposit(2, 1000000000000);

        vm.stopBroadcast();
    }
}

