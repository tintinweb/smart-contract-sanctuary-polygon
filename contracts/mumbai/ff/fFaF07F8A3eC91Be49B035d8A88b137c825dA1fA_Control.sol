import "./interfaces/IMetaCore.sol";
import "./interfaces/IMetaPayment.sol";
import "./interfaces/IForceNFT.sol";
import "./interfaces/IControl.sol";
import "./interfaces/IRegistry.sol";
import "./interfaces/IMetaProject.sol";
import "./interfaces/IEnergyCoin.sol";
import "./libraries/FixedPointMath.sol";
import "./InternalEnergyCoin.sol";

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "hardhat-deploy/solc_0.8/proxy/Proxied.sol";

contract Control is Initializable, Proxied, AccessControl, IControl {
    using SafeERC20 for IERC20;
    using FixedPointMath for uint256;
    using EnumerableSet for EnumerableSet.UintSet;

    mapping(uint256 => bool) public override blockedUsers;
    mapping(address => bool) public override blockedWallets;
    mapping(uint256 => bool) public override alreadyReceivedEmptyNFT;
    mapping(uint256 => uint256) public override getFactorForEnergyTokens;
    mapping(uint256 => uint256) public requiredLevels;
    mapping(uint256 => uint256) public nftIndexes;
    mapping(uint256 => Distribution) public distributions;
    mapping(uint256 => mapping(uint256 => mapping(uint256 => bool))) public nftRewarded;
    mapping(uint256 => uint256) public override getShareForLevelFromPool;
    mapping(uint256 => mapping(uint256 => EnumerableSet.UintSet)) private nftsForIdUser;

    bytes32 public constant BLOCKER_ROLE = 0x8f2e0057cd5e35397007bcc8f5418f73dd64cc6e4073a0276563f247c3079037;
    bytes32 public constant DISTRIBUTOR_ROLE = 0xfbd454f36a7e1a388bd6fc3ab10d434aa4578f811acbbcf33afb1c697486313c;
    bytes32 public constant MIGRATOR_ROLE = 0x600e5f1c60beb469a3fa6dd3814a4ae211cc6259a6d033bae218a742f2af01d3;

    uint256 public override getAmountNFTForMerge;
    uint256 public override getPriceUpToFirstLevel;
    uint256 public override getRewardEnergyForFirstEmptyNFT;
    uint256 public override getDistributionTimePeriod;
    uint256 public indexNextDistribution;
    IRegistry public registry;

    event UserBlockStatusIsSet(uint256 indexed users, bool blocked);
    event WalletBlockStatusIsSet(address indexed wallets, bool blocked);
    event DistributionTimePeriodIsSet(uint256 distributionTimePeriod);
    event FactorForEnergyTokensIsSet(string _name, uint256 _factor);
    event PriceUpToFirstLevelIsSet(uint256 _price);
    event ShareForLevelFromPoolIsSet(uint256 indexed levelNFT, uint256 _share);
    event AmountNFTForMergeIsSet(uint256 amount);
    event ReturnedTheLeftOver(uint256 indexed _idDistribution, uint256 amount);
    event RequiredLevelsIsSet(string _name, uint256 _requeredLevel);
    event RewardIsClaimed(
        uint256 indexed _idDistribution,
        uint256 indexed _level,
        uint256 indexed idsNFTs,
        uint256 reward
    );
    event BurnToken(address indexed erc20, uint256 indexed userId, uint256 amount);
    event PoolIsDistribute(uint256 indexed indexDistribution, uint256 createdAt, uint256 amount);
    event InternalEnergyTokensGave(uint256 indexed userId, uint256 amount);

    modifier notBlockedUser(address _user) {
        uint256 senderId = getUserId(_user);
        if (blockedUsers[senderId]) {
            revert ForceNFTControlUserBlocked(senderId);
        }
        if (blockedWallets[_user]) {
            revert ForceNFTControlWalletBlocked(_user);
        }
        _;
    }

    constructor() {}

    function setNFTParameters() external override onlyRole(DEFAULT_ADMIN_ROLE) {
        IForceNFT nft = IForceNFT(registry.getForceNFT(0));
        nft.setMaxEnergy(getPriceUpToFirstLevel);

        uint256 amountNFTForMerge = getAmountNFTForMerge;
        uint256 maxLvlNFT = registry.getMaxLvlNFT();
        for (uint256 i = 1; i <= maxLvlNFT; ) {
            nft = IForceNFT(registry.getForceNFT(i));
            nft.setMaxEnergy(amountNFTForMerge);
            unchecked {
                ++i;
            }
        }
    }

    function setUsersBlockStatus(uint256[] memory users, bool blocked) external override onlyRole(BLOCKER_ROLE) {
        mapping(uint256 => bool) storage _blockedUsers = blockedUsers;
        for (uint256 i = 0; i < users.length; i++) {
            _blockedUsers[users[i]] = blocked;
            emit UserBlockStatusIsSet(users[i], blocked);
        }
    }

    function setWalletsBlockStatus(address[] memory wallets, bool blocked) external override onlyRole(BLOCKER_ROLE) {
        mapping(address => bool) storage _blockedWallets = blockedWallets;
        for (uint256 i = 0; i < wallets.length; i++) {
            _blockedWallets[wallets[i]] = blocked;
            emit WalletBlockStatusIsSet(wallets[i], blocked);
        }
    }

    function setDistributionTimePeriod(uint256 distributionTimePeriod) external override onlyRole(DISTRIBUTOR_ROLE) {
        getDistributionTimePeriod = distributionTimePeriod;
        emit DistributionTimePeriodIsSet(distributionTimePeriod);
    }

    function setFactorForEnergyTokens(string memory _name, uint256 _factor)
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        if (!registry.checkName(_name)) {
            revert ForceNFTControlThisNameIsEmpty();
        }
        getFactorForEnergyTokens[registry.metaProjectId(_name)] = _factor;
        emit FactorForEnergyTokensIsSet(_name, _factor);
    }

    function setPriceUpToFirstLevel(uint256 _price) external override onlyRole(DEFAULT_ADMIN_ROLE) {
        getPriceUpToFirstLevel = _price;
        emit PriceUpToFirstLevelIsSet(_price);
        IForceNFT nft = IForceNFT(registry.getForceNFT(0));
        nft.setMaxEnergy(_price);
    }

    // slither-disable-next-line reentrancy-events
    function setAmountNFTForMerge(uint256 amount) external override onlyRole(DEFAULT_ADMIN_ROLE) {
        getAmountNFTForMerge = amount;
        uint256 maxLvlNFT = registry.getMaxLvlNFT();
        IForceNFT nft;
        for (uint256 i = 1; i < maxLvlNFT; ) {
            nft = IForceNFT(registry.getForceNFT(i));
            nft.setMaxEnergy(amount);
            emit AmountNFTForMergeIsSet(amount);
            unchecked {
                ++i;
            }
        }
    }

    function setRequiredLevels(string memory _name, uint256 _requiredLevel)
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        if (!registry.checkName(_name)) {
            revert ForceNFTControlThisNameIsEmpty();
        }
        requiredLevels[registry.metaProjectId(_name)] = _requiredLevel;
        emit RequiredLevelsIsSet(_name, _requiredLevel);
    }

    function setShareForLevelFromPool(uint256[] calldata _shares) external onlyRole(DEFAULT_ADMIN_ROLE) {
        if (_shares.length != registry.getMaxLvlNFT() + 1) {
            revert ForceNFTControlNotEnoughValuesInArray(registry.getMaxLvlNFT() + 1);
        }
        uint256 sum;
        uint256 length = _shares.length;
        for (uint256 i = 0; i < length; ) {
            sum += _shares[i];
            getShareForLevelFromPool[i] = _shares[i];
            emit ShareForLevelFromPoolIsSet(i, _shares[i]);
            unchecked {
                ++i;
            }
        }
        if (sum != SCALE) {
            revert ForceNFTControlNotHundredPercent();
        }
    }

    function distributionPool() external override onlyRole(DISTRIBUTOR_ROLE) returns (uint256) {
        return distributionPool(IERC20(registry.getForceRewardCoin()).balanceOf(registry.getForcePool()));
    }

    // slither-disable-next-line reentrancy-no-eth
    function returnTheLeftOver(uint256 _idDistribution) external onlyRole(DISTRIBUTOR_ROLE) {
        Distribution storage _distribution = distributions[_idDistribution];
        //slither-disable-next-line timestamp
        if (distributions[_idDistribution].createdAt + getDistributionTimePeriod > block.timestamp) {
            revert ForceNFTControlDistributionIsNotOver();
        }

        emit ReturnedTheLeftOver(_idDistribution, _distribution.amount);
        // slither-disable-next-line arbitrary-send-erc20
        IERC20(registry.getForceRewardCoin()).safeTransferFrom(
            registry.getForcePool(),
            registry.getCompanyPool(),
            _distribution.amount
        );
        _distribution.amount = 0;
    }

    function migrateNFT(
        address _user,
        uint256 _level,
        uint256 _tokenID,
        uint256 _energy
    ) external notBlockedUser(_user) onlyRole(MIGRATOR_ROLE) {
        if (_level > registry.getMaxLvlNFT()) {
            revert ForceNFTControlThisLevelNFTMoreMax();
        }
        IForceNFT nft = IForceNFT(registry.getForceNFT(_level));
        IMetaCore core = IMetaCore(registry.getMetaCore());

        nft.mint(_user, _tokenID);
        nft.setEnergy(_tokenID, _energy);
        // slither-disable-next-line unused-return
        nftsForIdUser[core.getUserId(_user)][_level].add(_tokenID);
    }

    function migrateAlreadyReceivedEmptyNFT(uint256[] calldata _ids) external onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 len = _ids.length;
        for (uint256 i = 0; i < len; ) {
            alreadyReceivedEmptyNFT[_ids[i]] = true;
            unchecked {
                i++;
            }
        }
    }

    // slither-disable-next-line reentrancy-benign
    function getEmptyNFT() external override notBlockedUser(msg.sender) returns (uint256) {
        uint256 senderId = getUserId(msg.sender);
        if (alreadyReceivedEmptyNFT[senderId]) {
            revert ForceNFTControlUserAlreadyReceived();
        }
        uint256 amount = registry.getAmountMetaProject();
        for (uint256 i = 0; i < amount; ) {
            IMetaProject metaProject = IMetaProject(registry.getMetaProject(i));
            if (metaProject.getLevelForNFT(senderId) < requiredLevels[registry.getIndexProject(i)]) {
                revert ForceNFTControlTooConditionsNotMet();
            }
            unchecked {
                ++i;
            }
        }
        alreadyReceivedEmptyNFT[senderId] = true;
        return giveNFT(msg.sender, 0);
    }

    //slither-disable-next-line cyclomatic-complexity
    function mergeNFT(uint256 _level, uint256[] memory idNFT)
        external
        override
        notBlockedUser(msg.sender)
        returns (uint256[] memory)
    {
        if (_level >= registry.getMaxLvlNFT()) {
            revert ForceNFTControlWrongNFTLevel();
        }
        IForceNFT nft = IForceNFT(registry.getForceNFT(_level));
        uint256 sum;
        uint256 length = idNFT.length;
        if (length < 2) {
            revert ForceNFTControlWrongAmountNFTForMerge();
        }
        if (_level == 0) {
            for (uint256 i = 0; i < length; ) {
                if (nft.ownerOf(idNFT[i]) != msg.sender) {
                    revert ForceNFTControlSenderNotOwnerNFT(_level, idNFT[i]);
                }
                sum += nft.energy(idNFT[i]);
                if (sum > getPriceUpToFirstLevel) {
                    nft.setEnergy(idNFT[i], sum - getPriceUpToFirstLevel);
                    sum = getPriceUpToFirstLevel;
                } else if (i != 0) {
                    burnNFT(_level, idNFT[i]);
                }
                if (sum == getPriceUpToFirstLevel) {
                    break;
                }
                unchecked {
                    ++i;
                }
            }
            return updateEmptyNFT(idNFT[0], sum);
        }

        uint256[] memory newNFTs = new uint256[](3);
        for (uint256 i = 0; i < length; ) {
            if (nft.ownerOf(idNFT[i]) != msg.sender) {
                revert ForceNFTControlSenderNotOwnerNFT(_level, idNFT[i]);
            }
            sum += nft.energy(idNFT[i]);
            if (sum > getAmountNFTForMerge) {
                nft.setEnergy(idNFT[i], sum - getAmountNFTForMerge);
                sum = getAmountNFTForMerge;
            } else if (i != 0) {
                burnNFT(_level, idNFT[i]);
            }
            if (sum == getAmountNFTForMerge) {
                burnNFT(_level, idNFT[0]);
                newNFTs[0] = _level + 1;
                newNFTs[1] = giveNFT(msg.sender, _level + 1);
                return newNFTs;
            }
            unchecked {
                ++i;
            }
        }
        nft.setEnergy(idNFT[0], sum);
        newNFTs[0] = _level;
        newNFTs[1] = idNFT[0];
        newNFTs[2] = sum;
        return newNFTs;
    }

    // slither-disable-next-line reentrancy-benign,reentrancy-events
    function claimReward(
        uint256 _idDistribution,
        uint256[] memory _level,
        uint256[] memory idsNFTs
    ) external override notBlockedUser(msg.sender) {
        uint256 reward;
        Distribution storage _distribution = distributions[_idDistribution];
        if (_idDistribution >= indexNextDistribution) {
            revert ForceNFTControlThereWasNoDistribution();
        }
        if (_level.length != idsNFTs.length) {
            revert ForceNFTControlNotEqualLengthArrays();
        }
        uint256 length = idsNFTs.length;
        for (uint256 i = 0; i < length; ) {
            IForceNFT nft = IForceNFT(registry.getForceNFT(_level[i]));
            if (_level[i] > registry.getMaxLvlNFT()) {
                revert ForceNFTNoRewardsForThisLevel(_level[i]);
            }
            if (_distribution.countNFT[_level[i]] == 0) {
                revert ForceNFTNoRewardsForThisLevel(_level[i]);
            }
            if (idsNFTs[i] > _distribution.countNFT[_level[i]] - 1) {
                revert ForceNFTControlNFTCreatedAfterDistribution(_level[i], idsNFTs[i]);
            }
            if (nft.ownerOf(idsNFTs[i]) != msg.sender) {
                revert ForceNFTControlSenderNotOwnerNFT(_level[i], idsNFTs[i]);
            }
            if (nftRewarded[_idDistribution][_level[i]][idsNFTs[i]]) {
                revert ForceNFTControlNFTRewarded(_level[i], idsNFTs[i]);
            }
            nftRewarded[_idDistribution][_level[i]][idsNFTs[i]] = true;
            reward += _distribution.oneShareStableCoin[_level[i]];
            emit RewardIsClaimed(_idDistribution, _level[i], idsNFTs[i], _distribution.oneShareStableCoin[_level[i]]);
            unchecked {
                ++i;
            }
        }
        _distribution.amount -= reward;
        // slither-disable-next-line arbitrary-send-erc20
        IERC20(registry.getForceRewardCoin()).safeTransferFrom(registry.getForcePool(), msg.sender, reward);
    }

    function getDistributionOneShareStableCoin(uint256 id, uint256 level)
        external
        view
        override
        returns (uint256 oneShareStableCoin)
    {
        oneShareStableCoin = distributions[id].oneShareStableCoin[level];
    }

    function getDistributionCountNFT(uint256 id, uint256 level) external view override returns (uint256 countNFT) {
        countNFT = distributions[id].countNFT[level];
    }

    function getRequiredLevels(string memory _name) external view override returns (uint256 _requiredLevel) {
        if (!registry.checkName(_name)) {
            revert ForceNFTControlThisNameIsEmpty();
        }
        _requiredLevel = requiredLevels[registry.metaProjectId(_name)];
    }

    // slither-disable-next-line reentrancy-events
    function levelUpEmptyNFT(uint256 id, uint256[] memory amounts)
        public
        override
        notBlockedUser(msg.sender)
        returns (uint256[] memory)
    {
        IForceNFT nft = IForceNFT(registry.getForceNFT(0));
        if (nft.ownerOf(id) != msg.sender) {
            revert ForceNFTControlSenderNotOwnerNFT(0, id);
        }
        if (amounts.length != registry.getAmountMetaProject() + 1) {
            revert ForceNFTControlLengthAmountArrayIsNotCorrect();
        }
        uint256 userId = getUserId(msg.sender);
        uint256 amount;
        uint256 last = amounts.length - 1;
        uint256 nftEnergy = nft.energy(id);
        if (nftEnergy + amounts[last] < getPriceUpToFirstLevel) {
            takeAndBurnToken(registry.getInternalEnergyCoin(), userId, amounts[last]);
            nftEnergy += amounts[last];
            uint256 amountMetaProject = registry.getAmountMetaProject();
            for (uint256 i = 0; i < amountMetaProject; ) {
                if (amounts[i] == 0) {
                    i++;
                    continue;
                }
                uint256 energyConversionFactor = getFactorForEnergyTokens[registry.getIndexProject(i)];
                if (energyConversionFactor != 0) {
                    IEnergyCoin energyCoin = IEnergyCoin(registry.getEnergyToken(i));
                    amount = amounts[i].mul(energyConversionFactor);
                    if (nftEnergy + amount < getPriceUpToFirstLevel) {
                        takeAndBurnToken(address(energyCoin), userId, amounts[i]);
                        nftEnergy += amount;
                    } else {
                        uint256 burnable = (getPriceUpToFirstLevel - nftEnergy).div(energyConversionFactor);
                        takeAndBurnToken(address(energyCoin), userId, burnable);
                        nftEnergy = getPriceUpToFirstLevel;
                        break;
                    }
                }
                unchecked {
                    ++i;
                }
            }
        } else {
            takeAndBurnToken(registry.getInternalEnergyCoin(), userId, getPriceUpToFirstLevel - nftEnergy);
            nftEnergy = getPriceUpToFirstLevel;
        }
        return updateEmptyNFT(id, nftEnergy);
    }

    function distributionPool(uint256 amount) public override onlyRole(DISTRIBUTOR_ROLE) returns (uint256) {
        Distribution storage _distribution = distributions[indexNextDistribution];
        if (amount > IERC20(registry.getForceRewardCoin()).balanceOf(registry.getForcePool())) {
            revert ForceNFTDistributionAmountMoreThanBalance();
        }
        uint256 maxLvlNFT = registry.getMaxLvlNFT();
        for (uint256 i = 0; i <= maxLvlNFT; ) {
            uint256 supply = IForceNFT(registry.getForceNFT(i)).totalSupply();
            if (supply == 0) {
                unchecked {
                    ++i;
                }
                continue;
            }
            _distribution.oneShareStableCoin[i] = amount.mul(getShareForLevelFromPool[i]) / supply;
            _distribution.countNFT[i] = nftIndexes[i];
            unchecked {
                ++i;
            }
        }
        _distribution.createdAt = block.timestamp;
        _distribution.amount = amount;
        emit PoolIsDistribute(indexNextDistribution, _distribution.createdAt, amount);
        unchecked {
            return indexNextDistribution++;
        }
    }

    function initialize(address _registry, uint256[] memory _nftIndexes) public proxied initializer {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        for (uint256 i = 0; i < _nftIndexes.length; ) {
            nftIndexes[i] = _nftIndexes[i];
            unchecked {
                i++;
            }
        }
        registry = IRegistry(_registry);
        getDistributionTimePeriod = 30 days;
        getAmountNFTForMerge = 3;
        getPriceUpToFirstLevel = 3500 ether;
        getRewardEnergyForFirstEmptyNFT = 350 ether;
        getShareForLevelFromPool[1] = 0.1 ether;
        getShareForLevelFromPool[2] = 0.11 ether;
        getShareForLevelFromPool[3] = 0.12 ether;
        getShareForLevelFromPool[4] = 0.13 ether;
        getShareForLevelFromPool[5] = 0.14 ether;
        getShareForLevelFromPool[6] = 0.15 ether;
        getShareForLevelFromPool[7] = 0.25 ether;
        emit DistributionTimePeriodIsSet(30 days);
        emit AmountNFTForMergeIsSet(3);
        emit PriceUpToFirstLevelIsSet(3500 ether);
        emit ShareForLevelFromPoolIsSet(1, 0.1 ether);
        emit ShareForLevelFromPoolIsSet(2, 0.11 ether);
        emit ShareForLevelFromPoolIsSet(3, 0.12 ether);
        emit ShareForLevelFromPoolIsSet(4, 0.13 ether);
        emit ShareForLevelFromPoolIsSet(5, 0.14 ether);
        emit ShareForLevelFromPoolIsSet(6, 0.15 ether);
        emit ShareForLevelFromPoolIsSet(7, 0.25 ether);
    }

    // slither-disable-next-line reentrancy-no-eth
    function updateEmptyNFT(uint256 id, uint256 energy) internal returns (uint256[] memory) {
        IForceNFT nft = IForceNFT(registry.getForceNFT(0));
        uint256[] memory newNFTs = new uint256[](3);
        if (energy == getPriceUpToFirstLevel) {
            burnNFT(0, id);
            newNFTs[0] = 1;
            newNFTs[1] = giveNFT(msg.sender, 1);
            newNFTs[2] = giveNFT(msg.sender, 0);
        } else {
            nft.setEnergy(id, energy);
            newNFTs[0] = 0;
            newNFTs[1] = id;
            newNFTs[2] = energy;
        }
        return newNFTs;
    }

    // slither-disable-next-line reentrancy-no-eth
    function giveNFT(address _user, uint256 _level) internal returns (uint256) {
        if (_level > registry.getMaxLvlNFT()) {
            revert ForceNFTControlThisLevelNFTMoreMax();
        }
        IMetaCore core = IMetaCore(registry.getMetaCore());
        IForceNFT nft = IForceNFT(registry.getForceNFT(_level));
        nft.mint(_user, nftIndexes[_level]);
        if (_level != 0) {
            nft.setEnergy(nftIndexes[_level], 1);
        }
        // slither-disable-next-line unused-return
        nftsForIdUser[core.getUserId(_user)][_level].add(nftIndexes[_level]);
        unchecked {
            return nftIndexes[_level]++;
        }
    }

    function burnNFT(uint256 _level, uint256 id) internal {
        if (_level > registry.getMaxLvlNFT()) {
            revert ForceNFTControlThisLevelNFTMoreMax();
        }
        IMetaCore core = IMetaCore(registry.getMetaCore());
        IForceNFT nft = IForceNFT(registry.getForceNFT(_level));
        address owner = nft.ownerOf(id);
        nft.burn(id);
        // slither-disable-next-line unused-return
        nftsForIdUser[core.getUserId(owner)][_level].remove(id);
    }

    function takeAndBurnToken(
        address erc20,
        uint256 userId,
        uint256 amount
    ) internal {
        emit BurnToken(erc20, userId, amount);
        IMetaPayment metaPayment = IMetaPayment(IMetaCore(registry.getMetaCore()).getPaymentChannelAddress());
        metaPayment.decreaseBalance(erc20, userId, address(this), amount);
    }

    function getUserId(address _user) internal returns (uint256) {
        IMetaCore mCore = IMetaCore(registry.getMetaCore());
        return mCore.checkRegistration(_user);
    }
}

