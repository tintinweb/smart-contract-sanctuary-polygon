pragma solidity 0.8.17;

struct Distribution {
    uint256[8] oneShareStableCoin;
    uint256[8] countNFT;
    uint256 createdAt;
    uint256 amount;
}

interface IControl {
    function migrateNFT(
        address _user,
        uint256 _level,
        uint256 _tokenID,
        uint256 _energy
    ) external;

    function returnTheLeftOver(uint256 _idDistribution) external;

    function getEmptyNFT() external returns (uint256);

    function levelUpEmptyNFT(uint256 id, uint256[] memory amountsEnergyCoin) external returns (uint256[] memory);

    function mergeNFT(uint256 _level, uint256[] memory idNFT) external returns (uint256[] memory);

    function setFactorForEnergyTokens(string memory _name, uint256 _factor) external;

    function setPriceUpToFirstLevel(uint256 price) external;

    function setAmountNFTForMerge(uint256 amount) external;

    function setRequiredLevels(string memory _name, uint256 _requiredLevel) external;

    function setDistributionTimePeriod(uint256 distributionTimePeriod) external;

    function setNFTParameters() external;

    function setUsersBlockStatus(uint256[] memory users, bool blocked) external;

    function setWalletsBlockStatus(address[] memory wallets, bool blocked) external;

    function distributionPool() external returns (uint256);

    function distributionPool(uint256 amount) external returns (uint256);

    function claimReward(
        uint256 _idDistribution,
        uint256[] memory _level,
        uint256[] memory idsNFTs
    ) external;

    function blockedUsers(uint256 id) external view returns (bool);

    function blockedWallets(address wallet) external view returns (bool);

    function alreadyReceivedEmptyNFT(uint256 id) external view returns (bool);

    function getFactorForEnergyTokens(uint256 id) external view returns (uint256);

    function getPriceUpToFirstLevel() external view returns (uint256);

    function getAmountNFTForMerge() external view returns (uint256);

    function getShareForLevelFromPool(uint256 level) external view returns (uint256);

    function getRewardEnergyForFirstEmptyNFT() external view returns (uint256);

    function getDistributionTimePeriod() external view returns (uint256);

    function getDistributionOneShareStableCoin(uint256 id, uint256 level)
        external
        view
        returns (uint256 oneShareStableCoin);

    function getDistributionCountNFT(uint256 id, uint256 level) external view returns (uint256 countNFT);

    function getRequiredLevels(string memory _name) external view returns (uint256);
}

