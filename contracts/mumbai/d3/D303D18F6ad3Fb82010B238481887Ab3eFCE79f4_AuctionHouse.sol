// File: Marketplace/AuctionHouse.sol


pragma solidity ^0.8.0;










struct Collection {
    address collectionAddress;
    uint256 royaltyFees;
    Status status;
}

struct Listing {
    address seller;
    uint256 price;
}

interface IMarketplace {
    function getListing(address _collection, uint256 _tokenId)
        external
        view
        returns (Listing memory);

    function getCollectionData(address _collection)
        external
        view
        returns (Collection memory);

    function updateRevenue(
        uint256 marketplaceFee,
        uint256 collectionFee,
        uint256 tokenFee,
        address _collection,
        uint256 _tokenId
    ) external;

    function calculateFee(
        address _collection,
        uint256 tokenId,
        uint256 _price,
        uint256 _protocolFee
    )
        external
        view
        returns (
            uint256 amount,
            uint256 marketplaceFee,
            uint256 collectionFee,
            uint256 tokenFee
        );
}

interface IWKCS {
    function deposit() external payable;
}

enum Status {
    Unverified,
    Verified
}

contract AuctionHouse is
    Context,
    Ownable,
    Pausable,
    ReentrancyGuard,
    ERC721Holder
{
    using EnumerableSet for EnumerableSet.UintSet;
    using Address for address;
    using SafeERC20 for IERC20;

    address private immutable WKCS; // Native ERC20 token for trades
    uint16 private tradeFee; // marketplace fee 1% == 100, 1000 == 10%
    address private admin; // marketplace controller
    IMarketplace public Marketplace; // Marketplace Contract address for the IMarketplace interface

    // trade fee to be in counts of 100: 1% == 100, 10% = 1000
    constructor(
        uint16 _tradeFee,
        address _WKCS,
        address _Marketplace
    ) {
        tradeFee = _tradeFee;
        admin = _msgSender();
        WKCS = _WKCS;
        Ownable(_msgSender());
        Marketplace = IMarketplace(_Marketplace);
    }

    event AuctionStarted(
        address indexed seller,
        address indexed collection,
        uint256 tokenId,
        uint256 startingBid,
        uint256 endAuctionTime
    );
    event AuctionCancelled(
        address indexed collection,
        uint256 tokenId
    );
    event BidAccepted(
        address indexed seller,
        address indexed buyer,
        address indexed collection,
        uint256 amount,
        uint256 tokenId
    );
    event BidCreated(
        address indexed bidder,
        address indexed collection,
        uint256 tokenId,
        uint256 amount
    );
    event BidLost(
        address indexed bidder,
        address indexed collection,
        uint256 tokenId
    );
    event TradeFeeUpdated(uint256 fees);

    error CannotExecute();
    error InvalidPrice(uint price);
    error UnauthorizedAction(address caller);

    struct Auction {
        address creator;
        uint256 highestBid;
        address highestBidder;
        uint256 startingBid;
        uint256 validity;
    }
    mapping(address => mapping(uint256 => Auction)) private auction;
    mapping(address => EnumerableSet.UintSet) private collectionAuction;

    modifier isBidValid(
        uint256 _price,
        address _collection,
        uint256 _tokenId
    ) {
        Auction memory readAuction = auction[_collection][_tokenId];
        if(_price < readAuction.startingBid && _price <= readAuction.highestBid){
            revert InvalidPrice(_price);
        }
        if(readAuction.creator == _msgSender()){
            revert UnauthorizedAction(_msgSender());
        }
        require(block.timestamp < readAuction.validity, "Auction ended");
        _;
    }
    modifier canBeAuctioned(address _collection, uint256 _tokenId) {
        Collection memory collection = Marketplace.getCollectionData(_collection);
        require(
            collection.collectionAddress != address(0),
            "Collection not Listed"
        );
        Listing memory listing = Marketplace.getListing(_collection, _tokenId);
        require(
            listing.seller == address(0),
            "Token is Listed for fixed sale"
        );
        require(
            !collectionAuction[_collection].contains(_tokenId),
            "Token ID is on Auction"
        );
        _;
    }
    modifier isBidAccepted(address _collection, uint256 _tokenId) {
        Auction memory readAuction = auction[_collection][_tokenId];
        require(readAuction.creator == _msgSender(), "Not Creator");
        require(readAuction.highestBid > 0, "Cannot accept bid");
        _;
    }
    modifier auctionCanBeCancelled(address _collection, uint256 _tokenId) {
        Auction memory readAuction = auction[_collection][_tokenId];
        require(readAuction.creator == _msgSender(), "Not Creator");
        require(block.timestamp > readAuction.validity, "Auction not ended");
        _;
    }

    function getAuctionData(address _collection, uint256 _tokenId)
        public
        view
        returns (Auction memory)
    {
        return auction[_collection][_tokenId];
    }

    function getCollectionAuctions(address _collection)
        public
        view
        returns (Auction[] memory _collectionAuctions)
    {
        uint256 length = collectionAuction[_collection].length();
        _collectionAuctions = new Auction[](length);
        for (uint256 i; i < length; i++) {
            _collectionAuctions[i] = auction[_collection][
                collectionAuction[_collection].at(i)
            ];
        }
    }

    function createAuction(
        address _collection,
        uint256 _tokenId,
        uint256 _startingBid,
        uint256 _auctionExpiry
    ) public whenNotPaused nonReentrant canBeAuctioned(_collection, _tokenId) {
        IERC721(_collection).safeTransferFrom(
            _msgSender(),
            address(this),
            _tokenId
        );
        uint validity = block.timestamp + _auctionExpiry * 1 days;
        Auction storage _auction = auction[_collection][_tokenId];
        _auction.creator = _msgSender();
        _auction.startingBid = _startingBid;
        _auction.validity = validity;
        collectionAuction[_collection].add(_tokenId);
        emit AuctionStarted(
            _msgSender(),
            _collection,
            _tokenId,
            _startingBid,
            validity
        );
    }

    function createBidWithKCS(address _collection, uint256 _tokenId)
        external
        payable
    {
        IWKCS(WKCS).deposit{value: msg.value}();
        _createBid(_collection, _tokenId, msg.value);
    }

    function createBidWithWKCS(
        address _collection,
        uint256 _tokenId,
        uint256 _bidValue
    ) external {
        IERC20(WKCS).safeTransferFrom(_msgSender(), address(this), _bidValue);
        _createBid(_collection, _tokenId, _bidValue);
    }

    function _createBid(
        address _collection,
        uint256 _tokenId,
        uint256 _bidValue
    )
        private
        whenNotPaused
        nonReentrant
        isBidValid(_bidValue, _collection, _tokenId)
    {
        Auction storage writeAuction = auction[_collection][_tokenId];
        if (writeAuction.highestBidder != address(0)) {
            IERC20(WKCS).safeTransfer(
                writeAuction.highestBidder,
                writeAuction.highestBid
            );
            emit BidLost(
                writeAuction.highestBidder,
                _collection,
                _tokenId
            );
        }
        writeAuction.highestBidder = _msgSender();
        writeAuction.highestBid = _bidValue;
        emit BidCreated(_msgSender(), _collection, _tokenId, _bidValue);
    }

    function acceptBid(address _collection, uint256 _tokenId)
        public
        whenNotPaused
        nonReentrant
        isBidAccepted(_collection, _tokenId)
    {
        Auction storage writeAuction = auction[_collection][_tokenId];
        address winner = writeAuction.highestBidder;
        uint256 highestBid = writeAuction.highestBid;
        _acceptBid(_collection,_tokenId,highestBid);
        _deleteAuction(_collection, _tokenId);
        IERC721(_collection).safeTransferFrom(address(this), winner, _tokenId);
        emit BidAccepted(
            _msgSender(),
            winner,
            _collection,
            highestBid,
            _tokenId
        );
    }
    function _acceptBid(address _collection, uint256 _tokenId, uint256 highestBid) private {
        (
            uint256 amount,
            uint256 marketplaceFee,
            uint256 collectionFee,
            uint256 tokenFee
        ) = Marketplace.calculateFee(
                _collection,
                _tokenId,
                highestBid,
                tradeFee
            );
        IERC20 token = IERC20(WKCS);
        uint totalFees = marketplaceFee + collectionFee + tokenFee;
        token.safeTransfer(address(Marketplace), totalFees);
        Marketplace.updateRevenue(
            marketplaceFee,
            collectionFee,
            tokenFee,
            _collection,
            _tokenId
        );
        token.safeTransfer(_msgSender(), amount);
    }

    function _deleteAuction(address _collection, uint256 _tokenId)
        private
        returns (bool)
    {
        delete (auction[_collection][_tokenId]);
        collectionAuction[_collection].remove(_tokenId);
        return true;
    }

    function cancelAuction(address _collection, uint256 _tokenId)
        public
        nonReentrant
        auctionCanBeCancelled(_collection, _tokenId)
    {
        Auction memory readAuction = auction[_collection][_tokenId];
        address highestBidder = readAuction.highestBidder;
        uint256 highestBid = readAuction.highestBid;
        _deleteAuction(_collection, _tokenId);
        if (highestBid > 0) {
            IERC20(WKCS).safeTransfer(highestBidder, highestBid);
        }
        IERC721(_collection).safeTransferFrom(
            address(this),
            _msgSender(),
            _tokenId
        );
        emit AuctionCancelled(_collection, _tokenId);
    }

    function updateTradeFee(uint8 _newTradeFee) external whenPaused onlyOwner {
        tradeFee = _newTradeFee;
        emit TradeFeeUpdated(_newTradeFee);
    }

    function updateAdmin(address _newAdmin) external whenPaused onlyOwner {
        admin = _newAdmin;
    }

    function recoverToken(address _token)
        external
        whenPaused
        onlyOwner
    {
        require(_token != WKCS, "Cannot recover");
        if(_token == address(0)){
            uint etherBalance = address(this).balance;
            require(etherBalance > 0, "Ether balance is 0");
            (bool sent, ) = admin.call{value: etherBalance}("");
            require(sent, "Failed to send Ether");
        } else{
            IERC20 token = IERC20(_token);
            uint balanceOf = token.balanceOf(address(this));
            token.safeTransfer(_msgSender(), balanceOf);
        }
    }

    function recoverNFT(address _collection, uint256 _tokenId)
        external
        whenPaused
        onlyOwner
    {
        require(
            getAuctionData(_collection, _tokenId).creator == address(0),
            "Valid listing"
        );
        IERC721(_collection).safeTransferFrom(
            address(this),
            address(msg.sender),
            _tokenId
        );
    }

    function pauseMarketplace()
        external
        whenNotPaused
        onlyOwner
    {
        _pause();
    }

    function unpauseMarketplace()
        external
        whenPaused
        onlyOwner
    {
        _unpause();
    }

    function updateMarketplaceAddress(address _Marketplace) external whenNotPaused onlyOwner {
        Marketplace = IMarketplace(_Marketplace);
    }
}