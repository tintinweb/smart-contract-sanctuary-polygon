/**
 * CRF01: Cannot be null / equal to zero
 * CRF02: Inversion
 * CRF03: Same address
 */

contract CrowdsaleFactory {
  using ItMapSale for ItMapSale.SaleMap;

  ItMapSale.SaleMap private saleMap;

  bytes32 public constant MANAGER_ROLE =
    keccak256("MANAGER_ROLE");

  address public addressRegistry;

  address private _implementation;

  event CrowdsaleCreated(address indexed crowdsaleAddress);
  event SetNewRegistryAddress(address newImpl);
  event SetNewImplementation(address newAddr);


  modifier onlyRole(bytes32 _role) {
      address _accessControlAddress = getRoleAddr();
      require(IAccessControl(_accessControlAddress).hasRole(_role, msg.sender));
      _;
    }

  constructor(address addressRegistry_, address implementation_) {
    addressRegistry = addressRegistry_;
    _implementation = implementation_;
  }

  function createCrowdsale(
    uint256 startAt,
    uint256 endAt,
    uint256 hardCap,
    uint256 maxBuy,
    uint256 tokenPrice,
    address token,
    address stableToken,
    address priceFeed // EUR / USD
  ) public onlyRole(MANAGER_ROLE) returns (address cloneAddr) {
    require(
        startAt > 0 &&
        endAt > 0 &&
        hardCap > 0 &&
        maxBuy > 0 &&
        tokenPrice > 0 &&
        token != address(0) &&
        stableToken != address(0) &&
        priceFeed != address(0),
      "CRF01"
    );
    require(
      startAt < endAt,
      "CRF02"
    );

    cloneAddr = Clones.clone(_implementation);
    Crowdsale(cloneAddr).__Crowdsale_init(
      startAt,
      endAt,
      hardCap,
      maxBuy,
      tokenPrice,
      token,
      stableToken,
      addressRegistry,
      priceFeed

    );
    saleMap.add(cloneAddr, token);
    console.log("Address clone", cloneAddr);
    address hub = getEventAddr();
    ICrowdsaleEvent(hub).setCrowdsaleExists(cloneAddr, true);
    emit CrowdsaleCreated(cloneAddr);
    return cloneAddr;
  }

  // -----------------------------------------
  // Setters 
  // -----------------------------------------

  function setNewRegistryAddress(
    address newAddr
  ) public onlyRole(MANAGER_ROLE) {
    require(newAddr != addressRegistry, "CRF03");
    addressRegistry = newAddr;
    emit SetNewRegistryAddress(newAddr);
  }

  function setNewImplementation(
    address newAddr
  ) public onlyRole(MANAGER_ROLE) {
    require(newAddr != _implementation, "CRF03");
    _implementation = newAddr;
    emit SetNewImplementation(newAddr);
  }

  // -----------------------------------------
  // Getters addresses
  // -----------------------------------------

  function isCrowdsaleExists(address key) public view returns (bool) {
    return saleMap.saleExists(key);
  }

  function getImplementation() external view 
    onlyRole(MANAGER_ROLE) returns(address)
  {
    return _implementation;
  }

  function getRoleAddr() public view returns (address) {
    return IAddressRegistry(addressRegistry).getRoleRegAddr();
  }

  function getEventAddr() public view returns (address) {
    return IAddressRegistry(addressRegistry).getCrowdsaleEventAddr();
  }

  // -----------------------------------------
  // Util
  // -----------------------------------------

  function _convertInDecimal(
    address _token,
    uint256 _val
  ) internal view returns (uint256) {
    return _val * 10 ** IERC20Metadata(_token).decimals();
  }
}

