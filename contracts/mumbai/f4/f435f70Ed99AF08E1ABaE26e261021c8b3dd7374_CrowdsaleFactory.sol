pragma solidity ^0.8.19;

library ContribMap {
    
    struct InvestorMap {
        address[] keys;
        mapping(address => uint256) indexOf;
        mapping(address => uint256) securityTokenAmount;
        mapping(address => uint256) amountInvested;
        mapping(address => uint256) distributed;
    }

    function addInvestment(
        InvestorMap storage map,
        address key,
        uint256 amountInvest,
        uint256 securityAmount
    ) external {
        if (map.indexOf[key] == 0) {
            if (map.keys.length == 0 || map.keys[0] != key) {
                map.indexOf[key] = map.keys.length;
                map.keys.push(key);
            }
        }
        map.amountInvested[key] += amountInvest;
        map.securityTokenAmount[key] += securityAmount;
    }

    function refundAmountBeforeEnd(
        InvestorMap storage map, address key, uint256 tokenAmount, uint256 securityAmount
    ) external {
        map.securityTokenAmount[key] -= securityAmount;
        map.amountInvested[key] -= tokenAmount;
    }

    function refund(
        InvestorMap storage map, address key, uint256 tokenAmount
    ) external {
        map.amountInvested[key] -= tokenAmount;
    }

    function purchaseDistributed(InvestorMap storage map, address key, uint256 amount) external {
        map.distributed[key] += amount;
    }

    function removeInvestment(
        InvestorMap storage map, address key,
        uint256 securityAmount
    ) external {
        map.securityTokenAmount[key] -= securityAmount;
    }
    

    function reinitInvestment(InvestorMap storage map, address key) external {
        map.securityTokenAmount[key] = 0;
        map.amountInvested[key] = 0;
    }

    function getTokenAmountFor(InvestorMap storage map, address key)
        external
        view
        returns (uint256)
    {
        return map.amountInvested[key];
    }

    function getSecurityTokenAmountFor(InvestorMap storage map, address key)
        external
        view
        returns (uint256)
    {
        return map.securityTokenAmount[key];
    }

    function getKeyAtIndex(InvestorMap storage map, uint256 index)
        external
        view
        returns (address)
    {
        return map.keys[index];
    }

    function distributedAmount(InvestorMap storage map, address key)
        external
        view
        returns (uint256)
    {
        return map.distributed[key];
    }

    function investorSize(InvestorMap storage map)
        external
        view
        returns (uint256)
    {
        return map.keys.length;
    }

}

