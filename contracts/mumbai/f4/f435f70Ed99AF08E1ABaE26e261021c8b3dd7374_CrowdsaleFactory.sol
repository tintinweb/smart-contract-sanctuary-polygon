// SPDX-License-Identifier: MIT


pragma solidity ^0.8.19;

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "../registry/IAddressRegistry.sol";
import "../roles/IRole.sol";
import "@openzeppelin/contracts/access/IAccessControl.sol";
import "../hub-event/ICrowdsaleEvent.sol";

import {ContribMap} from "./utils/ContribMap.sol";

contract Crowdsale is  Initializable, Pausable {
    using SafeCast for int256;
    using SafeMath for uint256;

    using SafeERC20 for IERC20;
    using ContribMap for ContribMap.InvestorMap;

    ContribMap.InvestorMap private investorMap;

    AggregatorV3Interface internal priceFeed;

    struct AllInvestment {
        address investor;
        uint256 amountInvested;
        uint256 securityTokenAmount;
    }

    uint256 public startAt;
    uint256 public endAt;
    uint256 public hardCap;
    uint256 public maxBuy;
    uint256 public tokenPrice;

    uint256 public totalUSDTContribution;
    uint256 public totalSecuritySold;


    address internal _addressRegistry;

    bool private _isFinalized;
    bool private _isCancelled;


    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");
    bytes32 public constant WHITELISTED_ROLE = keccak256("WHITELISTED_ROLE");
    bytes32 public constant BLOCKLISTED_INVEST_ROLE = keccak256("BLOCKLISTED_INVEST_ROLE");

    event Finalize();
    event Cancel();
    event WithdrawFundsRaised(address to, uint256 amount);
    event SetDates(uint256 startTime, uint256 endTime);
    event SetGoal(uint256 hardCap);
    event SetPrice(uint256 price);
    event Invest(
        address investor,
        uint256 amountInvested,
        uint256 securityTokenValue,
        uint256 taxFee
    );
    event RemoveInvestmentAndRefundAndBlocklist(
        address account,
        uint256 tokenAmount,
        uint256 securityAmount
    );
    event RemoveInvestmentAndRefund(
        address account,
        uint256 tokenAmount,
        uint256 securityAmount
    );
    event RemoveInvestment(address investor, uint256 securityAmount);
    event RemoveAllInvestments(address investor);

    IERC20 public token;
    IERC20 public  USDT;


    modifier onlyRole(bytes32 _role) {
      address _accessControlAddress = getRoleAddr();
      require(IAccessControl(_accessControlAddress).hasRole(_role, msg.sender));
      _;
    }

    function __Crowdsale_init(
        uint256 startAt_,
        uint256 endAt_,
        uint256 hardCap_,
        uint256 maxBuy_,
        uint256 tokenPrice_,
        address token_,
        address USDT_,
        address addressRegistry_,
        address priceFeed_
    ) public initializer {
        _addressRegistry = addressRegistry_;
        require(_isFactory(), "Not factory");
        startAt = startAt_;
        endAt = endAt_;
        hardCap = hardCap_* 10 ** 18;
        maxBuy = maxBuy_ * 10 ** 18;
        tokenPrice = tokenPrice_;
        token = IERC20(token_);
        USDT = IERC20(USDT_);
        priceFeed = AggregatorV3Interface(
            priceFeed_
        );
    }

    function investManually(uint256 _securityAmount, address _account )
        external
        whenNotPaused
        onlyRole(MANAGER_ROLE)
    {   
        address _accessControlAddress = getRoleAddr();
        bool isWhitelisted = IAccessControl(_accessControlAddress).hasRole(WHITELISTED_ROLE, _account);
        bool isBlocklisted = IAccessControl(_accessControlAddress).hasRole(BLOCKLISTED_INVEST_ROLE, _account);

        require(isWhitelisted, "Beneficiary is not whitelisted");
        require(!isBlocklisted, "Beneficiary is blocklisted");

        require(!_isCancelled, "Sale is already cancelled");
        require(!_isFinalized, "Sale is already finalized");

        require(_securityAmount >= 1, "too low purchase");

        uint256 amountInDecimals = _securityAmount *
            10**IERC20Metadata(address(token)).decimals();
        uint256 rest = getRestToSell();

        require(block.timestamp >= startAt, "Sale has not yet started");
        require(block.timestamp <= endAt, "Sale has ended");

        require(amountInDecimals != 0, "Cannot buy 0 token");
        require(rest > 0, "The hardcap has been reached");
        require(amountInDecimals <= rest, "Not enough token to sell");

        uint256 usdtContribution = getRequiredAmountOfTokensFor(
            _securityAmount
        );
        investorMap.addInvestment(
            _account,
            0,
            amountInDecimals
        );
        _updateValues(amountInDecimals, usdtContribution);
        address ev = getEventAddr();
        ICrowdsaleEvent(ev).tokenPurchaseEvent(
            true,
            address(token),
            _account,
            _securityAmount,
            0
        );
    }

    function invest(uint256 _securityAmount)
        external
        whenNotPaused
    {
        require(!_isCancelled, "Sale is already cancelled");
        require(!_isFinalized, "Sale is already finalized");

        address _accessControlAddress = getRoleAddr();
        address ev = getEventAddr();

        bool isWhitelisted = IAccessControl(_accessControlAddress).hasRole(WHITELISTED_ROLE, msg.sender);
        bool isBlocklisted = IAccessControl(_accessControlAddress).hasRole(BLOCKLISTED_INVEST_ROLE, msg.sender);

        require(isWhitelisted, "Beneficiary is not whitelisted");
        require(!isBlocklisted, "Beneficiary is blocklisted");

        require(_securityAmount >= 1, "too low purchase");

        uint256 amountInDecimals = _securityAmount *
            10**IERC20Metadata(address(token)).decimals();
        uint256 rest = getRestToSell();
        require(amountInDecimals <= maxBuy, "maxBuy reached");

        require(block.timestamp >= startAt, "Sale has not yet started");
        require(block.timestamp <= endAt, "Sale has ended");

        require(amountInDecimals != 0, "Cannot buy 0 token");
        require(rest > 0, "The hardcap has been reached");
        require(amountInDecimals <= rest, "Not enough token to sell");

        uint256 usdtContribution = getRequiredAmountOfTokensFor(
            _securityAmount
        );
        _updateValues(amountInDecimals, usdtContribution);
        investorMap.addInvestment(
            msg.sender,
            usdtContribution,
            amountInDecimals
        );
        USDT.safeTransferFrom(msg.sender, address(this), usdtContribution);
        ICrowdsaleEvent(ev).tokenPurchaseEvent(
            false,
            address(token),
            msg.sender,
            _securityAmount,
            usdtContribution
        );
    }

    
    function cancel() external onlyRole(MANAGER_ROLE) {
        require(!_isCancelled, "Sale is already canceled");
        require(!_isFinalized, "Sale is already finalized");
        _isCancelled = true;
        emit Cancel();
    }

    /** Remove investment */

    function removeInvestmentAndRefundAndBlocklist(address account, uint256 tokenAmount, uint256 securityAmount)
        external onlyRole(MANAGER_ROLE)
    {
        require(account != address(0), "refundAmountFor: address can't be 0x0");
        require(tokenAmount > 0, "refundAmountFor: fiatAmount must be > 0");
        require(securityAmount > 0, "refundAmountFor: securityAmount must be > 0");

        uint256 investedAmount = getTokenAmountFor(account);
        uint256 purchasedAmount = getSecurityAmountFor(account);
        
        require(investedAmount >= tokenAmount, "refundAmountFor: amount >= investedAmount");
        require(purchasedAmount >= tokenAmount, "refundAmountFor: amount >= purchasedAmount");

        USDT.safeTransfer(account, tokenAmount);
        investorMap.refundAmountBeforeEnd(account, tokenAmount, securityAmount);
        
        address _accessControlAddress = getRoleAddr();
        IRole(_accessControlAddress).blocklistFromInvest(account);

        // Update sale state
        totalSecuritySold -= securityAmount;
        totalUSDTContribution -= tokenAmount;

        emit RemoveInvestmentAndRefundAndBlocklist(account, tokenAmount, securityAmount);

    }

    function removeInvestmentAndRefund(address account, uint256 tokenAmount, uint256 securityAmount)
        external 
        onlyRole(MANAGER_ROLE)
    {
        require(account != address(0), "refundAmountFor: address can't be 0x0");
        require(tokenAmount > 0, "refundAmountFor: amount must be > 0");

        uint256 investedAmount = getTokenAmountFor(account);
        uint256 purchasedAmount = getSecurityAmountFor(account);
        
        require(investedAmount >= tokenAmount, "refundAmountFor: amount >= investedAmount");
        require(purchasedAmount >= tokenAmount, "refundAmountFor: amount >= purchasedAmount");

        USDT.safeTransfer(account, tokenAmount);
        investorMap.refundAmountBeforeEnd(account, tokenAmount, securityAmount);

        // Update sale state
        totalSecuritySold -= securityAmount;
        totalUSDTContribution -= tokenAmount;

        emit RemoveInvestmentAndRefund(account, tokenAmount, securityAmount);
    }

    // if user paid in fiat
    function removeInvestment(
        address account,
        uint256 securityAmount
        )
        external
        onlyRole(MANAGER_ROLE)
    {
        require(!_isCancelled, "Sale is already cancelled");
        require(!_isFinalized, "Sale is already finalized");

        uint256 purchasedAmount = getSecurityAmountFor(account);

        require(purchasedAmount >= securityAmount, "SecurityAmount too big");

        investorMap.removeInvestment(account, securityAmount);
        // Update sale state
        totalSecuritySold -= securityAmount;

        emit RemoveInvestment(account, securityAmount);
    }

    function removeAllInvestments(address account)
        external
        onlyRole(MANAGER_ROLE)
    {
        require(!_isCancelled, "Sale is already cancelled");
        require(!_isFinalized, "Sale is already finalized");

        uint256 purchasedAmount = getSecurityAmountFor(account);
        uint256 investedAmount = getTokenAmountFor(account);

        require(purchasedAmount > 0, "Investor has no purchase");

        investorMap.reinitInvestment(account);
        // Update sale state
        totalUSDTContribution -= investedAmount;
        totalSecuritySold -= purchasedAmount;
        
        emit RemoveAllInvestments(account);
    }

    function refund(
        uint256 _startIndex,
        uint256 _endIndex
    ) external whenNotPaused onlyRole(MANAGER_ROLE) {
        require(!_isFinalized, "Sale is already finalized");
        require(_isCancelled, "The sale is not canceled");

        uint256 maxIndex = getTotalInvestors() - 1;

        if (_endIndex > maxIndex) _endIndex = maxIndex;

        require(_startIndex <= maxIndex, "Start index exceeds the total number of investors");
        require(_startIndex <= _endIndex, "Start index greater than end");

        address[] memory investorsAddresses = new address[](_endIndex - _startIndex + 1);
        uint256[] memory investedAmounts = new uint256[](_endIndex - _startIndex + 1);

        for (uint256 i = _startIndex; i <= _endIndex;) {

            address key = investorMap.getKeyAtIndex(i);
            uint256 investedAmount = getTokenAmountFor(key);

            USDT.safeTransfer(key, investedAmount);
            investorsAddresses[i - _startIndex] = key;
            investedAmounts[i - _startIndex] = investedAmount;
            investorMap.refund(key, investedAmount);
            unchecked { ++i; }
        }
        address eventAddr = getEventAddr();
        ICrowdsaleEvent(eventAddr).refundEvent(
            investorsAddresses,
            investedAmounts
            );
    }

    /** Success */

    function finalize() external onlyRole(MANAGER_ROLE) {
        require(!_isCancelled, "Sale is already canceled");
        require(!_isFinalized, "Sale is already finalized");
        require(getRestToSell() == 0, "The hardcap has not been reached");
        _isFinalized = true;
        emit Finalize();
    }

    function withdrawFundsRaised(address _to, uint256 _amount)
        external
        onlyRole(MANAGER_ROLE)
    {
        require(!_isCancelled, "Sale is already canceled");
        require(_isFinalized, "The sale is not yet finalized");
        require(
            getContractBalance() > 0,
            "There are no tokens in the contract."
        );
        require(_amount <= getContractBalance(), "Amount too big");
        USDT.safeTransfer(_to, _amount);
        emit WithdrawFundsRaised(_to, _amount);
    }

    function distributeTokensAfterCrowdsale(
        uint256 _startIndex,
        uint256 _endIndex
    ) external onlyRole(MANAGER_ROLE) {
        require(_isFinalized, "The sale is not finalized");
        require(
            token.balanceOf(address(this)) > 0,
            "There are no tokens in the contract"
        );
        uint256 maxIndex = getTotalInvestors() - 1;

        if (_endIndex > maxIndex) _endIndex = maxIndex;

        require(_startIndex <= maxIndex, "Start index exceeds the total number of investors");
        require(_startIndex <= _endIndex, "Start index greater than end");

        address[] memory investorsAddresses = new address[](_endIndex - _startIndex + 1);
        uint256[] memory purchasedAmounts = new uint256[](_endIndex - _startIndex + 1);
        uint256[] memory investedAmounts = new uint256[](_endIndex - _startIndex + 1);

        for (uint256 i = _startIndex; i <= _endIndex;) {

            address key = investorMap.getKeyAtIndex(i);
            uint256 purchasedAmount = getSecurityAmountFor(key);
            uint256 distributedAmount = investorMap.distributedAmount(key);

            if(purchasedAmount > distributedAmount) {
                uint256 amount = investorMap.getTokenAmountFor(key);
                token.safeTransfer(key, purchasedAmount);
                investorsAddresses[i - _startIndex] = key;
                purchasedAmounts[i - _startIndex] = purchasedAmount;
                investedAmounts[i - _startIndex] = amount;
                investorMap.purchaseDistributed(key, amount);
            }
            unchecked { ++i; }
        }
        address eventAddr = getEventAddr();
        ICrowdsaleEvent(eventAddr).distributionEvent(
            address(token), 
            investorsAddresses,
            purchasedAmounts,
            investedAmounts
            );
    }


    function emergencyWithdraw() external onlyRole(MANAGER_ROLE) {
        uint256 balance = token.balanceOf(address(this));
        token.safeTransfer(msg.sender, balance);
    }

    /** Setters */

    function setDates(uint256 _startDate, uint256 _endDate)
        external
        onlyRole(MANAGER_ROLE)
    {
        require(_startDate < _endDate, "StartDate is after EndDate.");
        startAt = _startDate;
        endAt = _endDate;
        emit SetDates(_startDate, _endDate);
    }

    function setNewTokenAddress(address _newToken)
        external
        onlyRole(MANAGER_ROLE)
    {
        require(address(token) != _newToken, "Token has already this value");
        token = IERC20(_newToken);
    }

    function setTokenPrice(uint256 _newPrice) external onlyRole(MANAGER_ROLE) {
        require(_newPrice > 0 &&_newPrice != tokenPrice, "Wrong val");
        tokenPrice = _newPrice;
        emit SetPrice(_newPrice);
    }


    function setCap(uint256 _hardCap) external onlyRole(MANAGER_ROLE) {
        require(_hardCap > 0, "Values cannot be equal to zero");
        require(_hardCap != hardCap, "Values cannot be equal to zero");
        hardCap = _hardCap;
        emit SetGoal(_hardCap);
    }

    function pause() external onlyRole(MANAGER_ROLE) {
        _pause();
    }

    function unpause() external onlyRole(MANAGER_ROLE) {
        _unpause();
    }

    /** Getters */

    function getRestToSell() public view returns (uint256) {
        return hardCap - totalSecuritySold;
    }

    function getAllInvestments(uint256 _startIndex, uint256 _endIndex)
        external
        view
        returns (AllInvestment[] memory)
    {
        uint256 maxIndex = getTotalInvestors() - 1;

        if (_endIndex > maxIndex) _endIndex = maxIndex;

        require(_startIndex <= _endIndex, "Start index greater than end");

        uint256 size = _endIndex - _startIndex + 1;

        AllInvestment[] memory investmentsStruct = new AllInvestment[](size);
        for (uint256 i = _startIndex; i <= _endIndex;) {
            address key = investorMap.getKeyAtIndex(i);
            investmentsStruct[i] = AllInvestment(
                key,
                investorMap.getTokenAmountFor(key),
                getSecurityAmountFor(key)
            );
            unchecked { ++i; }
        }
        return investmentsStruct;
    }

    function getSaleStartDate() external view returns (uint256) {
        return startAt;
    }

    function getSaleEndDate() external view returns (uint256) {
        return endAt;
    }

    function isFinalized() external view returns(bool) {
        return _isFinalized;
    }

    function isCancelled() external view returns(bool) {
        return _isCancelled;
    }

    function getHardCap() external view returns (uint256) {
        return hardCap;
    }

    function getContractBalance() public view returns (uint256) {
        return USDT.balanceOf(address(this));
    }

    function getTotalInvestors() public view returns (uint256) {
        return investorMap.investorSize();
    }

    function getLatestPrice() public view returns (int256) {
        (, int256 price, , , ) = priceFeed.latestRoundData();
        return price;
    }

    function getRequiredAmountOfTokensFor(uint256 _securityTokens)
        public
        view
        returns (uint256)
    {   
        uint256 amount = (_securityTokens * tokenPrice * uint256(getLatestPrice())) / 100;
        return amount;
    }

    /** Getters investor */

    function getTokenAmountFor(address _investorAddress)
        public
        view
        returns (uint256)
    {
        return investorMap.getTokenAmountFor(_investorAddress);
    }

    function getSecurityAmountFor(address _investorAddress)
        public
        view
        returns (uint256)
    {
        return investorMap.getSecurityTokenAmountFor(_investorAddress);
    }


    /** Getters address */

    function getRoleAddr() public view returns (address) {
        return IAddressRegistry(_addressRegistry).getRoleRegAddr();
    }

    function getCrowdsaleFactAddr() public view returns (address) {
        return IAddressRegistry(_addressRegistry).getCrowdsaleFactAddr();
    }

    function getEventAddr() public view returns (address) {
        return IAddressRegistry(_addressRegistry).getCrowdsaleEventAddr();
    }

    function _isFactory() internal view returns (bool) {
        return msg.sender == getCrowdsaleFactAddr();
    }

    /** Internal functions */

    function _updateValues(
        uint256 _securitySold,
        uint256 _usdtContribution
    ) private {
        totalSecuritySold += _securitySold;
        totalUSDTContribution += _usdtContribution;
    }

}

