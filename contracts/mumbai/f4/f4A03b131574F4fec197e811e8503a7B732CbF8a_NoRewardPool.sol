pragma solidity ^0.8.0;

import "hardhat/console.sol";

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";

contract NoRewardPool is Ownable {
    using SafeERC20 for IERC20;
    using SafeCast for uint256;
    using SafeCast for int256;

    /* =========== STATE VARIABLES ===========*/

    address public CODE;
    address public penaltyWallet;
    
    // Admin
    mapping (address => bool) public admin;
    // Info of each token can stake
    mapping(address => ExternalToken) public externalToken;
    // Info of each user that stakes tokens
    mapping(address => mapping(address => UserInfo)) public userInfo;
    
    // The external token addreses
    address[] public tokenAddresses;

    // Emergency Withdraw
    bool public canEmergencyWithdraw;

    // The precision factor
    uint256 public PRECISION_FACTOR = 10 ** 12;

    uint256 public PRECISION_FACTOR_FE = 10 ** 6;

    // Duration to withdraw with no Fee
    uint128 public noWithdrawFeeTime = 30 days;

    
    int256 public aNumber;
    int256 public bNumber;
    int256 public cNumber;
    

    struct UserInfo { 
        uint256 amount;
        uint256 stakedTime;
    }

    struct ExternalToken {
        address contractAddress;
        bool canStake;
        uint256 rate;
    }
    
    constructor(address _token, uint256 _rate, int256 _a, int256 _b, int256 _c) {
        aNumber = _a;
        bNumber = _b;
        cNumber = _c;
        CODE = _token;
        admin[msg.sender] = true;

        addExternalToken(_token, true, _rate);
    }

    event SetPenaltyWallet(address indexed _penaltyWallet);
    event SetWithdrawTime(uint256 duration);
    // event RemoveAdmin(address indexed user);
    // event AddAdmin(address indexed user);
    event UpdateAdmin(address indexed user, bool status);
    event UpdateEmergencyWithdrawStatus(bool _status);
    event SetWithdrawFeeParam(int256 a, int256 b, int256 c);
    event Deposit(address indexed user, uint256 amount, address token);
    event Withdraw(address indexed user, uint256 amount, uint256 withdrawFee, address token);
    event AddExternalToken(address token, bool canStake, uint256 rate);
    event EmergencyWithdraw(uint256 _amount);
    event ExternalTokenStatsChange(address token, bool canStake, uint256 rate);

    /* =========== FUNCTIONS ===========*/

    function deposit(address _token, uint256 _amount) external {
        UserInfo storage user = userInfo[msg.sender][_token];

        require(externalToken[_token].canStake == true, "KLAYDAO::TOKEN_NOT_ACCEPTED");
        require(_amount > 0, "KLAYDAO::Must deposit Larger than zero");
        
        IERC20(_token).transferFrom(msg.sender, address(this), _amount);

        user.amount = user.amount + _amount;
        user.stakedTime = block.timestamp;

        emit Deposit(msg.sender, _amount, _token);
    }

    function withdraw(uint256 _amount, address _token) external {

        UserInfo storage user = userInfo[msg.sender][_token];
        
        require(externalToken[_token].canStake == true, "KLAYDAO::TOKEN_NOT_ACCEPTED");
        require(user.amount >= _amount, "KLAYDAO::Not ENOUGH AMOUNT TO WITHDRAW");
        require(address(penaltyWallet) != address(0), "::PENALTYWALLET_IS_ZERO_ADDRESS");

        uint256 totalWithdraw = _amount;
        uint256 feeAmount;

        if (_token == CODE) {
            // uint256 _duration = ((uint256(uint128(block.timestamp) - user.stakedTime)) * PRECISION_FACTOR) / (24 * 60 * 60);

            // uint256 aprFee = calculateWithdrawFeeAPR(_duration, msg.sender, _token);
            // feeAmount = ((((_amount * aprFee) * _duration) / 365) / PRECISION_FACTOR) / (PRECISION_FACTOR * 100);

            feeAmount = calculateWithdrawFee(_amount, msg.sender, _token);

            // totalWithdraw = ((_amount * (PRECISION_FACTOR * 100)) - feeAmount) / (PRECISION_FACTOR * 100);
            totalWithdraw = _amount - feeAmount;
        
            if (feeAmount > 0) {
                IERC20(_token).safeTransfer(address(penaltyWallet), feeAmount);
            }
        }

        IERC20(_token).safeTransfer(address(msg.sender), totalWithdraw);

        user.amount = user.amount - _amount;

        emit Withdraw(msg.sender, _amount, feeAmount, _token);
    }

    function removeAdmin(address _RemoveAdmin) external onlyOwner {
        admin[_RemoveAdmin] = false;

        emit UpdateAdmin(_RemoveAdmin, false);
    }

    function addAdmin(address _AddAdmin) external onlyOwner {
        admin[_AddAdmin] = true;

        emit UpdateAdmin(_AddAdmin, true);
    }

    function setWithdrawFeeParam(int256 _a, int256 _b, int256 _c) external onlyAdmin {
        aNumber = _a;
        bNumber = _b;
        cNumber = _c;

        emit SetWithdrawFeeParam(_a, _b, _c);
    }

    function setNoWithdrawFeeTime(uint128 _duration) external onlyAdmin {
        noWithdrawFeeTime = _duration;

        emit SetWithdrawTime(_duration);
    }


    function setPenaltyWallet(address _penaltyWallet) external onlyAdmin {
        require(penaltyWallet != _penaltyWallet, "ALREADY_PENALTY_WALLET");
        penaltyWallet = _penaltyWallet;
        
        emit SetPenaltyWallet(_penaltyWallet);
    }

    function updateEmergencyWithdrawStatus(bool _status) external onlyAdmin {
        canEmergencyWithdraw = _status;

        emit UpdateEmergencyWithdrawStatus(_status);
    }

    function addExternalToken(
        address _token,
        bool _canStake,
        uint256 _rate
    ) public onlyAdmin {
        ExternalToken storage token = externalToken[_token];

        token.contractAddress = _token;
        token.canStake = _canStake;
        token.rate = _rate;

        tokenAddresses.push(_token);

        emit AddExternalToken(_token,  _canStake, _rate);
    }

    function setExternalToken(
        address _token,
        bool _canStake,
        uint256 _rate
    ) external onlyAdmin {
        ExternalToken storage token = externalToken[_token];

        require(token.contractAddress == _token, "KLAYDAO::TOKEN_NOT_EXISTS");

        token.canStake = _canStake;
        token.rate = _rate;

        emit ExternalTokenStatsChange(_token, _canStake, _rate);
    }

    function emergencyWithdrawERC20(address _token) external {
        require(canEmergencyWithdraw, "function disabled");
        UserInfo storage user = userInfo[msg.sender][_token];
        require(user.amount > 0, "nothing to withdraw");

        uint256 _amount = user.amount;
        user.amount = 0;

        IERC20(CODE).safeTransfer(msg.sender, _amount);

        emit EmergencyWithdraw(_amount);
    }

    /**
     * @notice withdraw all remain token when close pool
     * @dev Only callable by owner. Needs to be for emergency.
     */
    function emergencyTokenWithdraw(uint256 _amount) external onlyAdmin {
        require(canEmergencyWithdraw, "NOT EMERGENCY");
        IERC20(CODE).safeTransfer(address(msg.sender), _amount);

        emit EmergencyWithdraw(_amount);
    }

    /* =========== VIEW FUNCTION ===========*/

    /**
     * @notice Withdraw staked tokens and collect reward tokens
     * @param _token: days staked, must multiply with 100000, example: 0.5 day (time in seconds / days in seconds) ==> input = 0.5 * 100000 = 
     */
    function calculateWithdrawFeeAPR(address _user, address _token) public view returns (uint256) {
        UserInfo storage user = userInfo[_user][_token];

        require(_token == CODE, "KLAYDAO::TOKEN_NOT_ACCEPTED");

        uint256 _duration = ((uint256(uint128(block.timestamp) - user.stakedTime)) * PRECISION_FACTOR) / (24 * 60 * 60);

        if (uint128(block.timestamp) < user.stakedTime + noWithdrawFeeTime && _duration < ((noWithdrawFeeTime / (24 * 60 * 60))  * PRECISION_FACTOR)) {
            return ((((((PRECISION_FACTOR.toInt256()) * PRECISION_FACTOR_FE.toInt256() / aNumber) * (_duration ** 2).toInt256()) / (PRECISION_FACTOR ** 2).toInt256()) + (cNumber * PRECISION_FACTOR.toInt256() / PRECISION_FACTOR_FE.toInt256())) - (((PRECISION_FACTOR.toInt256() * PRECISION_FACTOR_FE.toInt256() / bNumber) * _duration.toInt256()) / PRECISION_FACTOR.toInt256())).toUint256();
        }
        
        return 0;
    }

    function calculateWithdrawFee(uint256 _amount, address _user, address _token) public view returns (uint256) {
        UserInfo storage user = userInfo[_user][_token];

        require(_token == CODE, "KLAYDAO::TOKEN_NOT_ACCEPTED");

        uint256 _duration = ((uint256(uint128(block.timestamp) - user.stakedTime)) * PRECISION_FACTOR) / (24 * 60 * 60);
 
        if (uint128(block.timestamp) < user.stakedTime + noWithdrawFeeTime && _duration < ((noWithdrawFeeTime / (24 * 60 * 60)) * PRECISION_FACTOR)) {
            
            uint256 aprFee = calculateWithdrawFeeAPR(_user, _token);
            
            return (((((_amount * aprFee) * _duration) / 365) / PRECISION_FACTOR) / (PRECISION_FACTOR * 100));
        }

        return 0;
    }

    function getListToken() public view returns(address[] memory) {
        return tokenAddresses;
    }

    function getTotalStaked(address _user, address _token)
        external
        view
        returns (uint256)
    {
        UserInfo storage user = userInfo[_user][_token];
    
        return user.amount;   
    }
    
    /* =========== MODIFIERS ===========*/

    modifier onlyAdmin() {
        require(admin[msg.sender] == true, "Caller is not admin");
        _; 
    }


    // function TEST() public view returns (uint256) {

    //     uint256 _duration = (86400 * PRECISION_FACTOR) / (24 * 60 * 60);
        
    //     return ((((((PRECISION_FACTOR.toInt256()) * PRECISION_FACTOR_FE.toInt256() / aNumber) * (_duration ** 2).toInt256()) / (PRECISION_FACTOR ** 2).toInt256()) + (cNumber * PRECISION_FACTOR.toInt256() / PRECISION_FACTOR_FE.toInt256())) - (((PRECISION_FACTOR.toInt256() * PRECISION_FACTOR_FE.toInt256() / bNumber) * _duration.toInt256()) / PRECISION_FACTOR.toInt256())).toUint256();
    // }

}


