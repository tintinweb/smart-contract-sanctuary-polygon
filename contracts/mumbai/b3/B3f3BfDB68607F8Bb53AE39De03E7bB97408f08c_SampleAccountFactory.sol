pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/Create2.sol";
import "@openzeppelin/contracts/proxy/ERC1967/ERC1967Proxy.sol";
import "hardhat/console.sol";
/**
 * A sample factory contract for SimpleAccount
 * A UserOperations "initCode" holds the address of the factory, and a method call (to createAccount, in this sample factory).
 * The factory's createAccount returns the target account address even if it is already installed.
 * This way, the entryPoint.getSenderAddress() can be called either before or after the account is created.
 */
contract SampleAccountFactory {
    SampleAccount public immutable accountImplementation;

    constructor() {
        accountImplementation = new SampleAccount(msg.sender, msg.sender);
    }

    /**
     * create an account, and return its address.
     * returns the address even if the account is already deployed.
     * Note that during UserOperation execution, this method is called only if the account is not deployed.
     * This method returns an existing account address so that entryPoint.getSenderAddress() would work even after account creation
     */
    function createAccount(address owner,uint256 salt) public returns (SampleAccount ret) {
        console.log("From contract in CreateAccount",msg.sender);
        address addr = getAddress(owner, salt);
        console.log("Got GetAddress",addr);
        uint codeSize = addr.code.length;
        if (codeSize > 0) {
            return SampleAccount(payable(addr));
        }
        ret = SampleAccount(payable(new ERC1967Proxy{salt : bytes32(salt)}(
                address(accountImplementation),
                abi.encodeCall(SampleAccount.initialize, (owner, msg.sender))
            )));
    }

    /**
     * calculate the counterfactual address of this account as it would be returned by createAccount()
     */
    function getAddress(address owner,uint256 salt) public view returns (address) {
        console.log("From contract in getAddress",msg.sender);
        return Create2.computeAddress(bytes32(salt), keccak256(abi.encodePacked(
                type(ERC1967Proxy).creationCode,
                abi.encode(
                    address(accountImplementation),
                    abi.encodeCall(SampleAccount.initialize, (owner, msg.sender))
                )
            )));
    }
}

contract SampleAccount {
    address public owner;
    address public masterOwner;
    modifier onlyMaster (){
        require(msg.sender==masterOwner, "Only masterOwner can initialize");
        _;
    }
    event SampleAccountInitialized(address indexed owner);
    constructor(address _owner, address _masterAddress) {
        owner = _owner;
        masterOwner = _masterAddress;
    }
    function initialize(address anOwner, address anMaster) public virtual {
        _initialize(anOwner, anMaster);
    }

    function recoverAccount(address anOwner) onlyMaster public virtual{
        owner = anOwner;
        
    }

    function _initialize(address anOwner, address anMaster) internal virtual {
        owner = anOwner;
        masterOwner = anMaster;
        emit SampleAccountInitialized(owner);
    }
}

