    function decimals() external view returns (uint);
}

interface IAggregator {
    function decimals() external view returns (uint);

    function latestAnswer() external view returns (uint);
}

contract BetPool is Ownable {

    enum POOl_STATUS{
        CLOSE,
        PROGRESS,
        SETTLEMENT
    }
    struct PoolPrice {
        uint aPoolValue;
        uint bPoolValue;
    }

    event PERIOD_DEFE_PRICE_LOG(
        uint period,
        uint aPoolValue,
        uint usdValueAmount
    );

     event PLACE_BET_LOG(
        address proxy,
        address token,
        uint amount
    );
    mapping(uint => mapping(address => uint)) public periodTokenAmount;

    mapping(uint => mapping(address => mapping(address => uint)))
        public periodUserTokenAmount;
  

    /** 每一期 某个token的价格 */
    mapping(uint => mapping(address => uint)) public periodTokenPrice;

    mapping(uint => uint) public periodAPoolVaule;
    /**
     * 因为B 池是多个代币的池子，所以需要到期后将多个币统一换成 U的价值
     */
    mapping(uint => uint) public periodBPoolVaule;
    mapping(uint => PoolPrice) public periodPoolPrice;


    uint public currentDrawPeriod = 0;
    uint public openPeriod = 0;
    
    mapping(address => uint) public userLastSettlePeriod;
    
    address[] public poolTokens;
    address[] public aPoolTokens;
    address[] public bPoolTokens;
    mapping(address => address) public tokenAggregators;
    address payable public vaultAdr;
    bool internal isInitialized;

    POOl_STATUS public poolStatus; // 0 关闭，1 投注期，2 结算期间

    function initialize(
        address _owner,
        address payable _vaultAdr,
        address[] memory _aPoolTokens,
        address[] memory _bPoolTokens,
        address[] memory _aPoolTokenAggregators,
        address[] memory _bPoolTokenAggregators
    ) public {
        require(!isInitialized, "already initialized");
        vaultAdr = _vaultAdr;
        aPoolTokens = _aPoolTokens;
        bPoolTokens = _bPoolTokens;

        for (uint i = 0; i < aPoolTokens.length; i++) {
            poolTokens.push(aPoolTokens[i]);
            tokenAggregators[aPoolTokens[i]] = _aPoolTokenAggregators[i];
        }

        for (uint i = 0; i < bPoolTokens.length; i++) {
            poolTokens.push(bPoolTokens[i]);
            tokenAggregators[bPoolTokens[i]] = _bPoolTokenAggregators[i];
        }
        isInitialized = true;
        transferOwnership(_owner);
    }

    function getTime() external view returns (uint) {
        return block.timestamp;
    }

    /**
     * 投注方法
     */
    function placeBet(address token, uint amount) external {
        bool result = BetVault(vaultAdr).betToken(msg.sender, token, amount);
        require(result,"bet error");
        periodTokenAmount[openPeriod][token] += amount;
        periodUserTokenAmount[openPeriod][msg.sender][token] += amount;
        uint userPeriod = userLastSettlePeriod[msg.sender];
        // 用户第一次投注
        if (userPeriod == 0) {
            userLastSettlePeriod[msg.sender] = openPeriod;
        }
        emit PLACE_BET_LOG(msg.sender,token,amount);

    }

    function setStatus(POOl_STATUS status) external onlyOwner {
       poolStatus = status;
    }


    function openOneNewPeriod(uint period,uint time) external onlyOwner {
        require(period > openPeriod, "needn't draw");
        openPeriod = period;
        poolStatus = POOl_STATUS.PROGRESS;
    }

    function drawPeriod(uint period) external onlyOwner {
        // 当前周期还在进行，不能结束
        require(period == openPeriod, "current period not end");
        require(period > currentDrawPeriod, "needn't draw");
        // 根据预言机把每个代币的价格取出来
        uint aPoolValue = getPoolValuePeriodAndSetPrice(aPoolTokens, period);

        // 以第一个token 为价格计算，
        // 举例： 如果 B pool 有ETH 和 USDC 两种代币，但是计价是以USDC 为计价，那么在传入构造参数时，就要将 USDC放在传入的第一参数中
        uint bPoolValue = getPoolValuePeriodAndSetPrice(bPoolTokens, period);

        periodAPoolVaule[openPeriod] = aPoolValue;
        periodBPoolVaule[openPeriod] = bPoolValue;

        PoolPrice memory poolPrice = PoolPrice(aPoolValue, bPoolValue);
        periodPoolPrice[openPeriod] = poolPrice;

        currentDrawPeriod = period;
        poolStatus = POOl_STATUS.SETTLEMENT;
        emit PERIOD_DEFE_PRICE_LOG(openPeriod, aPoolValue, bPoolValue);
    }

    /**
     * 结算，将用户赚钱划转到vault
     *
     */
    function settle(address user,uint userPeriod) external onlyOwner {
        // 用户必须投注过，才可能领奖
        require(userPeriod != 0, "you must pre bet");
        require(userPeriod <= currentDrawPeriod, "not period can draw");
        (uint[] memory amounts, uint endPeriod) = canGetPoolPerPeriod(
            userPeriod,
            user,
            aPoolTokens,
            bPoolTokens
        );
        userLastSettlePeriod[user] = endPeriod;
    
        BetVault(vaultAdr).settleVault(user, poolTokens, amounts);
    }

    /**
     * 计算某个周期下可以领取的a 池的token数量
     */
    function canGetPoolPerPeriod(
        uint period,
        address user,
        address[] memory aPool,
        address[] memory bPool
    ) public view returns (uint[] memory, uint) {
        // uint allBValue = periodBPoolVaule[period];
     
        uint[] memory canClaimPoolTokenAmount = new uint[](poolTokens.length);

        (uint aUserPoolValue) = getPoolValuePerPeriod(
            aPool,
            period,
            user
        );
        (uint bUserPoolValue) = getPoolValuePerPeriod(
            bPool,
            period,
            user
        );

        // 向a池投钱后，可以从b池中获得的各种代币的数量
        if (aUserPoolValue != 0 || periodPoolPrice[period].aPoolValue != 0) {
            for (uint i = 0; i < bPool.length; i++) {
                canClaimPoolTokenAmount[aPool.length + i] +=
                    (periodTokenAmount[period][bPool[i]] * aUserPoolValue) /
                    periodPoolPrice[period].aPoolValue;
            }
        }

        // 向b池投钱后，可以从a池中获得的各种代币的数量
        if (bUserPoolValue != 0 || periodPoolPrice[period].bPoolValue != 0) {
            for (uint i = 0; i < aPool.length; i++) {
                canClaimPoolTokenAmount[i] +=
                    (periodTokenAmount[period][aPool[i]] * bUserPoolValue) /
                    periodPoolPrice[period].bPoolValue;
            }
        }

        period += 1;
        return (canClaimPoolTokenAmount, period);
    }


    /*
     * 获取某个周期下的某个方向的池子里的系统总价值或用户总价值
     * 举例： 如果 B pool 有ETH 和 USDC 两种代币，但是计价是以USDC 为计价，那么在传入构造参数时，就要将 USDC放在传入的第一参数中
     */
    function getPoolValuePeriodAndSetPrice(
        address[] memory tokenPools,
        uint period
    ) public returns (uint) {
        uint poolValue = 0;
        for (uint i = 0; i < tokenPools.length; i++) {
            uint tokenPrice = IAggregator(tokenAggregators[tokenPools[i]])
                .latestAnswer();
            periodTokenPrice[openPeriod][tokenPools[i]] = tokenPrice;
            uint tokenAmount = periodTokenAmount[period][tokenPools[i]];

            if (tokenAmount == 0) {
                continue;
            }

            
            uint tokenVaule;
            if(IBaseERC20(tokenPools[i]).decimals() +
                IAggregator(tokenAggregators[tokenPools[i]]).decimals() > IBaseERC20(tokenPools[0]).decimals()){
                    uint decimals =  IBaseERC20(tokenPools[i]).decimals() +
                IAggregator(tokenAggregators[tokenPools[i]]).decimals() - IBaseERC20(tokenPools[0]).decimals();
                
                tokenVaule = (tokenAmount * tokenPrice) / (10 ** decimals);

            }else{
                uint decimals =  IBaseERC20(tokenPools[0]).decimals() - IBaseERC20(tokenPools[i]).decimals() -
                IAggregator(tokenAggregators[tokenPools[i]]).decimals();
                
                tokenVaule = (tokenAmount * tokenPrice) * (10 ** decimals);
            }

            
            poolValue += tokenVaule;
        }
        return poolValue;
    }

    function getPoolValuePerPeriod(
        address[] memory tokenPools,
        uint period,
        address user
    ) public view returns (uint) {
        uint userPoolValue = 0;
        for (uint i = 0; i < tokenPools.length; i++) {
            uint tokenPrice = periodTokenPrice[openPeriod][tokenPools[i]];

            uint userTokenAmount = 0;
            userTokenAmount = periodUserTokenAmount[period][user][
                tokenPools[i]
            ];
            

            if (userTokenAmount == 0) {
                continue;
            }

            uint userTokenVaule;
            if(IBaseERC20(tokenPools[i]).decimals() +
                IAggregator(tokenAggregators[tokenPools[i]]).decimals() > IBaseERC20(tokenPools[0]).decimals()){
                    uint decimals =  IBaseERC20(tokenPools[i]).decimals() +
                IAggregator(tokenAggregators[tokenPools[i]]).decimals() - IBaseERC20(tokenPools[0]).decimals();
                
                userTokenVaule = (userTokenAmount * tokenPrice) / (10 ** decimals);

            }else{
                uint decimals =  IBaseERC20(tokenPools[0]).decimals() - IBaseERC20(tokenPools[i]).decimals() -
                IAggregator(tokenAggregators[tokenPools[i]]).decimals();
                
                userTokenVaule = (userTokenAmount * tokenPrice) * (10 ** decimals);
            }


            userPoolValue += userTokenVaule;
        }
        return (userPoolValue);
    }
}

