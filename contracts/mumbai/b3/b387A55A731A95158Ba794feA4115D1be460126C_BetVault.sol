// interface
//contract
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {ReentrancyGuard} from "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./BetPool.sol";
import "hardhat/console.sol";

contract PoolFactory is Ownable {
    event PoolCreated(address indexed owner, address indexed pool,uint poolId);
    mapping(address => bool) public registerPools;

    uint public poolId;
    address public vaultAdr;

    function register(address _controller) public onlyOwner {
        registerPools[_controller] = true;
    }

    function create(
        address _owner,
        address payable _vaultAdr,
        address[] memory _aPoolTokens,
        address[] memory _bPoolTokens,
        address[] memory _aPoolTokenAggregators,
        address[] memory _bPoolTokenAggregators
    ) public onlyOwner returns(address pool){
        bytes memory bytecode = type(BetPool).creationCode;
        bytes32 salt = keccak256(abi.encodePacked(address(this), poolId));
        assembly {
            pool := create2(0, add(bytecode, 32), mload(bytecode), salt)
        }
        registerPools[pool] = true;

        BetPool(pool).initialize(
            _owner,
            _vaultAdr,
            _aPoolTokens,
            _bPoolTokens,
            _aPoolTokenAggregators,
            _bPoolTokenAggregators
        );

        console.log("new pool ",pool);
        emit PoolCreated(_owner,pool,poolId);
        poolId++;
        return pool;
    }
}

