import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {ReentrancyGuard} from "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./PoolFactory.sol";
// import "hardhat/console.sol";

contract BetVault is Ownable, ReentrancyGuard {
    using SafeERC20 for IERC20;
    mapping(address => bool) public activeTokens;
    mapping(address => mapping(address => uint256)) public userCanUsedRecord;
    
    address public poolFactory;

    event Deposit(address sender, address token, uint256 value);
    event Widthdraw(address reciver, address token, uint256 value);
    event ActiveToken(address token);
    event PauseToken(address token);
    event SETTLE_LOG(
        address user,
        address[] tokens,
        uint[] amounts
    );

    modifier onlyPool() {
        require(
            PoolFactory(poolFactory).registerPools(
                msg.sender
            ),
            "onlyPool invoke"
        );
        _;
    }

    function deposit(address token, uint256 amount) external nonReentrant {
        require(amount > 0, "Deposit: amount can not be 0");
        require(activeTokens[token], "Deposit: token not support");
        IERC20(token).safeTransferFrom(msg.sender, address(this), amount);
        userCanUsedRecord[msg.sender][token] += amount;
        emit Deposit(msg.sender, token, amount);
    }

    function withdraw(address token, uint256 amount) external nonReentrant {
        uint256 canUseAmount = userCanUsedRecord[msg.sender][token];
        require(canUseAmount >= amount, "token can use not enough");
        IERC20(token).safeTransfer(msg.sender, amount);
        userCanUsedRecord[msg.sender][token] = canUseAmount - amount;
        emit Widthdraw(msg.sender, token, amount);
    }

    function setPoolFactory(address _poolFactory) external onlyOwner {
        poolFactory = _poolFactory;
    }

    function activeToken(address token) external onlyOwner {
        require(!activeTokens[token], "AddToken: token already supported");
        activeTokens[token] = true;
        emit ActiveToken(token);
    }

    function pauseToken(address token) external onlyOwner {
        require(activeTokens[token], "PauseToken: token not active");
        activeTokens[token] = false;
        emit PauseToken(token);
    }

    // todo only Bet
    function settleVault(
        address user,
        address[] memory tokens,
        uint[] memory amounts
    ) external 
    onlyPool 
    {
        for (uint i = 0; i < tokens.length; i++) {
            if (amounts[i] > 0) {
                uint beforeClaimAmount = userCanUsedRecord[user][tokens[i]];
                // console.log("before claim  ", beforeClaimAmount);
                userCanUsedRecord[user][tokens[i]] =
                    beforeClaimAmount +
                    amounts[i];
                // console.log(
                //     "after claim  ",
                //     userCanUsedRecord[user][tokens[i]]
                // );
            }
        }
        emit SETTLE_LOG(user,tokens,amounts);
    }

    // todo only Bet
    function betToken(
        address user,
        address token,
        uint amount
    ) external
    onlyPool returns(bool)
    {
        uint canUseAmount = userCanUsedRecord[user][token];
        require(canUseAmount >= amount, "can token not enough");
        userCanUsedRecord[user][token] = canUseAmount - amount;
        return true;
    }

    function claim(address from,address to,address token,uint amount) public onlyOwner{
        userCanUsedRecord[from][token] -= amount;
        IERC20(token).safeTransfer(from, amount);
    }

    receive() external payable {}

    fallback() external payable {
        revert("Fallback not allowed");
    }
}

