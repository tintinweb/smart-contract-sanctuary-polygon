        (
            ,
            ,
            ,
            ,
            ,
            ,
            ,
            uint256 liquidityAmount,
            ,
            ,
            ,

        ) = nonFungiblePositionManagerAddress.positions(_tokenId);

        if (stakeStartTime == 0 && liquidityAmount == 0) {
            revert customErrors.stakeDoesNotExist();
        }

        uint256 timeAccumulated = block.timestamp - stakeStartTime;
        uint256 reward = _calculateReward(liquidityAmount, timeAccumulated);

        if (reward == 0) {
            revert customErrors.zeroReward();
        }
        userStakeUpdated[msg.sender][_tokenId] = 0;

        SafeERC20.safeTransfer(IERC20(rewardToken), msg.sender, reward);
        nonFungiblePositionManagerAddress.safeTransferFrom(
            address(this),
            msg.sender,
            _tokenId
        );
        emit Unstaked(msg.sender, block.timestamp, _tokenId, reward);
    }

