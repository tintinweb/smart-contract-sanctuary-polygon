    function _calculateReward(
        uint256 _amount,
        uint256 _time
    ) internal view returns (uint256) {
        uint256 rewardCalculated = (_amount * rewardRatePerNFT * _time) /
            (1000000 * 365 days);
        return rewardCalculated;
    }

