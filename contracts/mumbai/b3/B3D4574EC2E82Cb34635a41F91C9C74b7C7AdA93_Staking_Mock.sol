        }
        
        userStakeUpdated[msg.sender][_tokenId] = block.timestamp;
        SafeERC20.safeTransfer(IERC20(rewardToken), msg.sender, reward);
        emit Claimed(msg.sender, block.timestamp, _tokenId, reward);
    }

