        for (uint i = 0; i < _poolBeacons.length();) {
            UpgradeableBeacon beacon = UpgradeableBeacon(_poolBeacons.at(i));
            if (beacon.implementation() == poolImplementation) {
                revert PoolExists(poolImplementation);
            }
            
            unchecked { i++; }
