pragma solidity 0.8.17;

/******************************************************************************\
* @author The Consensys Team
* @title VillageDAO Expert NFT smart contract
/******************************************************************************/

import "./abstracts/Guards.sol";
import "./RewardPool.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/Base64Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";

/**
 * @dev Expert address has already this brandId associated to him
 * @param expertAddress address of the expert
 * @param brandId Id of the brand
 */
error ExpertHasAlreadyBrand(address expertAddress, uint256 brandId);

/**
 * @dev Expert address has no brandId associated to him
 * @param expertAddress address of the expert
 */
error ExpertHasNoBrand(address expertAddress);

/**
 * @dev Cannot transfer the Expert NFT
 * @param from address the transfer is originating from
 * @param to receiver address
 * @param tokenId the transferred token's ID that is being transferred
 * @param batchSize batchSize
 */
error ExpertNFTTransferForbidden(
    address from,
    address to,
    uint256 tokenId,
    uint256 batchSize
);

/**
 * @dev This expert does not have any NFT assigned to him in HardRevoke
 * @param expertAddress the address of the targeted expert
 */
error HardRevokeNoToken(address expertAddress);

/**
 * @dev Cannot approve the Expert NFT for another address
 * @param to address receiving approval
 * @param tokenId the token you would like to give delegated ownership from
 */
error NFTApproveForbidden(address to, uint256 tokenId);

/**
 * @dev This expert does not have any NFT assigned to him in SoftRevoke
 * @param expertAddress the address of the targeted expert
 */
error SoftRevokeNoToken(address expertAddress);

/**
 * @dev Only contract owner or the bespoke NFT owner can unassign
 * @param sender address of the caller
 * @param expertAddress address of the expert
 */
error UnassignForbidden(address sender, address expertAddress);

/**
 * @dev Implementation of ERC721 Non-Fungible Token Standard for VillageDAO
 * Expert.
 */
contract ExpertNFT is
    Initializable,
    ERC721Upgradeable,
    UUPSUpgradeable,
    PausableUpgradeable,
    Guards
{
    using CountersUpgradeable for CountersUpgradeable.Counter;
    /**
     * @dev struct with brandId as the Id of a brand and rank as the level of
     * the expert
     */
    struct Brand {
        uint256 brandId;
        uint256 rank;
    }

    RewardPool public rewardPool;
    /// @dev mapping of address for tokenId
    mapping(address => uint256) public tokenIds;
    /// @dev mapping of tokenId for brands
    mapping(uint256 => Brand[]) public brands;
    /// @dev array of rank brand for images
    string[] public rankImages;
    /// @dev base token URI used as a prefix by tokenURI()
    string public baseTokenURI;
    /// @dev timestamp length of the soft revoke
    uint256 public softRevocationDelay;
    /// @dev mapping of address for soft revoked timestamp
    mapping(address => uint256) public softRevokedTimestamp;
    /// @dev public base URL of VillageDAO for brands
    string public villageDAOBrandURL;
    /// @dev constant for base brand rank
    uint256 private constant BASE_BRAND_RANK = 1;
    /// @dev Counter for tokenId
    CountersUpgradeable.Counter private _tokenIdCounter;

    /**
     * @dev emits an event when a brand is assigned to expert NFT and mints
     * the NFT if it does not exists
     * @param expertAddress the address of the targeted expert
     * @param brandId the brand id
     * @param tokenId NFT expert token id
     */
    event Assign(
        address indexed expertAddress,
        uint256 indexed brandId,
        uint256 indexed tokenId
    );

    /**
     * @dev emits an event when the expert token is burnt
     * @param expertAddress the address of the targeted expert
     * @param tokenId the tokenId of the targeted NFT
     */
    event HardRevoke(address indexed expertAddress, uint256 indexed tokenId);

    /**
     * @dev emits an event when the expert is soft revoked
     * @param expertAddress the address of the targeted expert
     */
    event SoftRevoke(address indexed expertAddress);

    /**
     * @dev emits an event when a brand is removed from the expert NFT
     * @param expertAddress the address of the targeted expert
     * @param brandId the brand id
     */
    event Unassign(address indexed expertAddress, uint256 indexed brandId);

    /**
     * @dev emits an event when an update of the rank occurs
     * @param expertAddress the address of the targeted expert
     * @param brandId brand Id
     * @param rank expert rank for that brand id
     */
    event UpdateRank(
        address indexed expertAddress,
        uint256 indexed brandId,
        uint256 rank
    );

    /**
     * @dev emits an event when the villageDAO brand URL is updated
     * @param villageDAOBrandURL the villageDAO brand URL
     */
    event UpdateVillageDAOBrandURL(string villageDAOBrandURL);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    /**
     * @dev replaces the constructor method
     * @param nameExpertNFT description of the NFT
     * @param symbolExpertNFT symbol of the NFT
     * @param _baseTokenURI base URI for IPFS
     * @param imageList initial rank images list
     * @param _softRevocationDelay timestamp length of the soft revoke
     * @param _villageDAOBrandURL base URL of villageDAO to get brands
     * @param _roleManager roleManager contract address
     * @param _rewardPool rewardPool contract address
     */
    function initialize(
        string memory nameExpertNFT,
        string memory symbolExpertNFT,
        string memory _baseTokenURI,
        string[] memory imageList,
        uint256 _softRevocationDelay,
        string memory _villageDAOBrandURL,
        address _roleManager,
        address _rewardPool
    ) external initializer {
        __ERC721_init(nameExpertNFT, symbolExpertNFT);
        __UUPSUpgradeable_init();
        baseTokenURI = _baseTokenURI;
        roleManager = IRoleManager(_roleManager);
        rewardPool = RewardPool(_rewardPool);
        rankImages = imageList;
        softRevocationDelay = _softRevocationDelay;
        villageDAOBrandURL = _villageDAOBrandURL;
    }

    /**
     * @dev assign a brandId to an expert to his token. Mint the token
     * if this one does not exist.
     * @param expertAddress address of the expert
     * @param brandId Id of the brand
     */
    function assign(
        address expertAddress,
        uint256 brandId
    ) external onlyDAOFoundationOrOperator whenNotPaused {
        uint256 tokenId;

        if (balanceOf(expertAddress) > 0) {
            tokenId = tokenIds[expertAddress];
            if (getRankPerBrandPerExpert(expertAddress, brandId) != 0) {
                revert ExpertHasAlreadyBrand(expertAddress, brandId);
            }
        } else {
            tokenId = safeMint(expertAddress);
            tokenIds[expertAddress] = tokenId;
        }
        brands[tokenId].push(Brand(brandId, BASE_BRAND_RANK));
        emit Assign(expertAddress, brandId, tokenId);
    }

    /**
     * @dev burns an expert's NFT. This is done when an expert's behavior has
     * been deemed infringing VillageDAO's TOS.
     * @param expertAddress address of the naughty expert.
     */
    function hardRevoke(
        address expertAddress
    ) external onlyDAOFoundationOrOperator whenNotPaused {
        if (balanceOf(expertAddress) == 0) {
            revert HardRevokeNoToken(expertAddress);
        }
        uint256 tokenId = tokenIds[expertAddress];
        _burn(tokenId);
        delete tokenIds[expertAddress];
        delete brands[tokenId];
        emit HardRevoke(expertAddress, tokenId);
    }

    /**
     * @dev get the soft revoked state of the expert
     * @param expertAddress address of the expert
     * @return boolean of the state
     */
    function isSoftRevoked(address expertAddress) external view returns (bool) {
        if (balanceOf(expertAddress) == 0) {
            revert SoftRevokeNoToken(expertAddress);
        }
        return ((softRevokedTimestamp[expertAddress] + softRevocationDelay) >
            block.timestamp);
    }

    /**
     * @dev Triggers stopped state.
     * Requirements:
     * - the contract must not be paused.
     */
    function pause() external onlyDAOFoundationOrOperator {
        _pause();
    }

    /**
     * @dev update the base VillageDAO brand URL
     * @param _villageDAOBrandURL URL of villageDAO to get brands
     */
    function setVillageDAOBrandURL(
        string memory _villageDAOBrandURL
    ) external onlyDAOFoundationOrOperator whenNotPaused {
        villageDAOBrandURL = _villageDAOBrandURL;
        emit UpdateVillageDAOBrandURL(_villageDAOBrandURL);
    }

    /**
     * @dev put an expert's NFT in a temporary revoked state
     * @param expertAddress address of the expert
     */
    function softRevoke(
        address expertAddress
    ) external onlyDAOFoundationOrOperator whenNotPaused {
        if (balanceOf(expertAddress) == 0) {
            revert SoftRevokeNoToken(expertAddress);
        }
        softRevokedTimestamp[expertAddress] = block.timestamp;
        emit SoftRevoke(expertAddress);
    }

    /**
     * @dev remove a brandId from an expert token.
     * @param expertAddress address of the expert
     * @param brandId Id of the brand
     */
    function unassign(
        address expertAddress,
        uint256 brandId
    ) external onlyDAOFoundationOrOperatorOrOneAddress(expertAddress) {
        uint256 tokenId = tokenIds[expertAddress];
        bool isBrandIdFound = false;
        uint256 brandsLength = brands[tokenId].length;
        uint256 i;
        for (i = 0; i < brandsLength; i++) {
            if (brands[tokenId][i].brandId == brandId) {
                isBrandIdFound = true;
                break;
            }
        }
        if (isBrandIdFound) {
            brands[tokenId][i].brandId = brands[tokenId][brandsLength - 1]
                .brandId;
            brands[tokenId][i].rank = brands[tokenId][brandsLength - 1].rank;
            brands[tokenId].pop();
            emit Unassign(expertAddress, brandId);
        }
    }

    /**
     * @dev Returns to normal state.
     * Requirements:
     * - the contract must be paused.
     */
    function unpause() external onlyDAOFoundationOrOperator {
        _unpause();
    }

    /**
     * @dev update a brand rank only callable by SeasonManager contract
     * @param expertAddress address of the expert
     * @param brandId brand Id
     * @param rank expert rank for that brand id
     */
    function updateRank(
        address expertAddress,
        uint256 brandId,
        uint256 rank
    ) external onlySeasonManagerContract whenNotPaused {
        uint256 tokenId = tokenIds[expertAddress];
        for (uint256 i = 0; i < brands[tokenId].length; i++) {
            if (brands[tokenId][i].brandId == brandId) {
                brands[tokenId][i].rank = rank;
                emit UpdateRank(expertAddress, brandId, rank);
                break;
            }
        }
    }

    /**
     * @dev See {IERC721-approve}.
     * @param to address receiving approval
     * @param tokenId the token you would like to give delegated ownership from
     */
    function approve(address to, uint256 tokenId) public virtual override {
        if (to != address(0)) {
            revert NFTApproveForbidden(to, tokenId);
        }
        super.approve(to, tokenId);
    }

    /**
     * @dev return the brandIds associated to the expert
     * revert if there is no brand
     * @param expertAddress address of the expert
     * @return Array of brands for the the expert
     */
    function getBrandsPerExpert(
        address expertAddress
    ) public view returns (Brand[] memory) {
        uint256 tokenId = tokenIds[expertAddress];
        if (brands[tokenId].length == 0) {
            revert ExpertHasNoBrand(expertAddress);
        }
        return brands[tokenId];
    }

    /**
     * @dev compute the average rank of the expert
     * @param expertAddress address of the expert
     * @return the average rank rounded down
     */
    function getGlobalRankPerExpert(
        address expertAddress
    ) public view returns (uint256) {
        Brand[] memory _brands = getBrandsPerExpert(expertAddress);
        uint256 average = 0;
        for (uint256 i = 0; i < _brands.length; i++) {
            average += _brands[i].rank;
        }
        // Brands length cannot be 0 or would be reverted in getBrandsPerExpert
        // Integer division rounds DOWN to the nearest integer
        average = average / _brands.length;
        return (average);
    }

    /**
     * @dev return the rank of the queried brand associated to the expert
     * @param expertAddress address of the expert
     * @param brandId Id of the brand
     * @return the brand rank
     */
    function getRankPerBrandPerExpert(
        address expertAddress,
        uint256 brandId
    ) public view returns (uint256) {
        uint256 tokenId = tokenIds[expertAddress];
        if (tokenId == 0) {
            return 0;
        } else {
            for (uint256 i = 0; i < brands[tokenId].length; i++) {
                if (brands[tokenId][i].brandId == brandId) {
                    return (brands[tokenId][i].rank);
                }
            }
            return 0;
        }
    }

    /**
     * @dev generates on-chain the metadata
     * @param tokenId token Id of the expert
     * @return the generated metadata
     */
    function tokenURI(
        uint256 tokenId
    ) public view virtual override returns (string memory) {
        string memory imageURI = rankImages[
            getGlobalRankPerExpert(ownerOf(tokenId)) - 1
        ];
        Brand[] memory brand = getBrandsPerExpert(ownerOf(tokenId));
        uint256 brandLength = brand.length;
        string memory metadataBrands;
        string memory brandAttributes;
        for (uint256 i = 0; i < brandLength; i++) {
            metadataBrands = string.concat(
                metadataBrands,
                '{"brandId":',
                StringsUpgradeable.toString(brand[i].brandId),
                ',"brandURL":"',
                villageDAOBrandURL,
                StringsUpgradeable.toString(brand[i].brandId),
                '","rank":',
                StringsUpgradeable.toString(brand[i].rank),
                "}"
            );
            brandAttributes = string.concat(
                brandAttributes,
                '{"trait_type":"',
                villageDAOBrandURL,
                StringsUpgradeable.toString(brand[i].brandId),
                '","rank":',
                StringsUpgradeable.toString(brand[i].rank),
                "}"
            );
            if (brandLength > 1 && i < (brandLength - 1)) {
                metadataBrands = string.concat(metadataBrands, ",");
                brandAttributes = string.concat(brandAttributes, ",");
            }
        }
        metadataBrands = string.concat('"brands":[', metadataBrands, "]");
        brandAttributes = string.concat('"attributes":[', brandAttributes, "]");
        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64Upgradeable.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"',
                                symbol(),
                                '","description":"',
                                name(),
                                '","image":"',
                                _baseURI(),
                                "/",
                                imageURI,
                                '","seasonsCount":',
                                StringsUpgradeable.toString(
                                    rewardPool.expertSeasonCount(
                                        ownerOf(tokenId)
                                    )
                                ),
                                ",",
                                metadataBrands,
                                ",",
                                brandAttributes,
                                "}"
                            )
                        )
                    )
                )
            );
    }

    /**
     * @dev mint the NFT for the expert and return the tokenId
     * @param to address of the expert
     * @return tokenId of the address
     */
    function safeMint(address to) private returns (uint256) {
        _tokenIdCounter.increment();
        uint256 tokenId = _tokenIdCounter.current();
        _safeMint(to, tokenId);
        return (tokenId);
    }

    /**
     * @dev Used by UUPS Upgradability pattern to include access restriction
     * to the upgrade mechanism
     */
    function _authorizeUpgrade(address) internal view override onlyDeployer {}

    /**
     * @dev Base URI for computing {tokenURI}.
     * @return the base URI
     */
    function _baseURI() internal view virtual override returns (string memory) {
        return baseTokenURI;
    }

    /**
     * @dev Hook that is called before any token transfer. This includes minting
     * and burning (See documentation of OpenZeppelin's ERC721Upgradeable.sol).
     * @param from address the transfer is originating from
     * @param to receiver address
     * @param tokenId the transferred token's ID that is being transferred
     * @param batchSize batchSize
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal pure override {
        if (from != address(0) && to != address(0)) {
            revert ExpertNFTTransferForbidden(from, to, tokenId, batchSize);
        }
    }
}

