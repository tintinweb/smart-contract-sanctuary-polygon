import "./abstracts/Guards.sol";
import "./RewardPool.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/Base64Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";

/**
 * @dev Cannot transfer the Brand NFT
 * @param from address the transfer is originating from
 * @param to receiver address
 * @param tokenId the transferred token's ID that is being transferred
 * @param batchSize batchSize
 */
error BrandNFTTransferForbidden(
    address from,
    address to,
    uint256 tokenId,
    uint256 batchSize
);

/**
 * @dev Brand has not a tokenId
 * @param brandAddress address of the brand
 */
error BrandNotExist(address brandAddress);

/**
 * @dev Brand address has already a tokenId
 * @param brandAddress address of the brand
 */
error ExceedTokenBalance(address brandAddress);

/**
 * @dev Implementation of ERC721 Non-Fungible Token Standard for VillageDAO
 * Brand.
 */
contract BrandNFT is
    Initializable,
    ERC721Upgradeable,
    UUPSUpgradeable,
    PausableUpgradeable,
    Guards
{
    using CountersUpgradeable for CountersUpgradeable.Counter;
    /**
     * @dev struct with brandId as the Id of a brand and rank as the level of
     * the expert
     */
    struct Brand {
        uint256 brandId;
        string brandName;
        string brandURL;
    }

    RewardPool public rewardPool;
    /// @dev Counter for tokenId
    CountersUpgradeable.Counter private _tokenIdCounter;
    /// @dev mapping of address for tokenId
    mapping(address => uint256) public tokenIds;
    /// @dev mapping of tokenId for Brand
    mapping(uint256 => Brand) public brandData;

    /**
     * @dev emitted when the brand token is minted
     * @param brandAddress the address of the brand owner
     * @param brandId the brand id
     * @param tokenId the token id minted
     * @param brandName the name of the minted brand
     * @param brandUrl the URL of the minted brand
     */
    event Mint(
        address indexed brandAddress,
        uint256 indexed brandId,
        uint256 indexed tokenId,
        string brandName,
        string brandUrl
    );

    /**
     * @dev emits an event when updating the brand name
     * @param brandAddress address of the brand
     * @param brandName New brand name
     */
    event UpdateBrandName(address indexed brandAddress, string brandName);

    /**
     * @dev emits an event when updating the brand URL
     * @param brandAddress address of the brand
     * @param brandURL New brand URL
     */
    event UpdateBrandURL(address indexed brandAddress, string brandURL);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    /**
     * @dev replaces the constructor method
     * @param nameBrandNFT description of the NFT
     * @param symbolBrandNFT symbol of the NFT
     * @param _roleManager roleManager contract address
     * @param _rewardPool rewardPool contract address
     */
    function initialize(
        string memory nameBrandNFT,
        string memory symbolBrandNFT,
        address _roleManager,
        address _rewardPool
    ) external initializer {
        __ERC721_init(nameBrandNFT, symbolBrandNFT);
        __UUPSUpgradeable_init();
        roleManager = IRoleManager(_roleManager);
        rewardPool = RewardPool(_rewardPool);
    }

    /**
     * @dev retrieve brand id of a brand from tokenId
     * @param tokenId token Id of the brand
     * @return brand id
     */
    function getBrandId(uint256 tokenId) external view returns (uint256) {
        return brandData[tokenId].brandId;
    }

    /**
     * @dev get the last token ID
     * @return last token ID
     */
    function getLastTokenId() external view returns (uint256) {
        return _tokenIdCounter.current();
    }

    /**
     * @dev mint the NFT for the brand
     * @param brandAddress address of the brand owner
     * @param brandId id of the brand
     * @param brandName name of the brand
     */
    function mint(
        address brandAddress,
        uint256 brandId,
        string memory brandName,
        string memory brandURL
    ) external onlyDAOFoundationOrOperator whenNotPaused {
        if (balanceOf(brandAddress) > 0) {
            revert ExceedTokenBalance(brandAddress);
        }
        _tokenIdCounter.increment();
        uint256 tokenId = _tokenIdCounter.current();
        tokenIds[brandAddress] = tokenId;
        brandData[tokenId] = Brand(brandId, brandName, brandURL);
        _safeMint(brandAddress, tokenId);
        emit Mint(brandAddress, brandId, tokenId, brandName, brandURL);
    }

    /**
     * @dev Triggers stopped state.
     * Requirements:
     * - the contract must not be paused.
     */
    function pause() external onlyDAOFoundationOrOperator {
        _pause();
    }

    /**
     * @dev update the brand name
     * @param brandAddress address of the brand
     * @param brandName replace brand by a new name
     */
    function setBrandName(
        address brandAddress,
        string memory brandName
    ) external onlyDAOFoundationOrOperator whenNotPaused {
        uint256 tokenId = tokenIds[brandAddress];
        if (tokenId > 0) {
            brandData[tokenId].brandName = brandName;
            emit UpdateBrandName(brandAddress, brandName);
        } else {
            revert BrandNotExist(brandAddress);
        }
    }

    /**
     * @dev update the brand URL
     * @param brandAddress address of the brand
     * @param brandURL replace brand by a new URL
     */
    function setBrandURL(
        address brandAddress,
        string memory brandURL
    ) external onlyDAOFoundationOrOperator whenNotPaused {
        uint256 tokenId = tokenIds[brandAddress];
        if (tokenId > 0) {
            brandData[tokenId].brandURL = brandURL;
            emit UpdateBrandURL(brandAddress, brandURL);
        } else {
            revert BrandNotExist(brandAddress);
        }
    }

    /**
     * @dev Returns to normal state.
     * Requirements:
     * - the contract must be paused.
     */
    function unpause() external onlyDAOFoundationOrOperator {
        _unpause();
    }

    /**
     * @dev generates on-chain the metadata
     * @param tokenId token Id of the brand
     * @return the generated metadata
     */
    function tokenURI(
        uint256 tokenId
    ) public view virtual override returns (string memory) {
        _requireMinted(tokenId);
        Brand memory _brandData = brandData[tokenId];
        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64Upgradeable.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"',
                                symbol(),
                                '","description":"',
                                name(),
                                '","brandId":',
                                StringsUpgradeable.toString(_brandData.brandId),
                                ',"brandName":"',
                                _brandData.brandName,
                                '","image":"',
                                _brandData.brandURL,
                                '","seasonsCount":',
                                StringsUpgradeable.toString(
                                    rewardPool.brandSeasonCount(
                                        ownerOf(tokenId)
                                    )
                                ),
                                "}"
                            )
                        )
                    )
                )
            );
    }

    /**
     * @dev Used by UUPS Upgradability pattern to include access restriction
     * to the upgrade mechanism
     */
    function _authorizeUpgrade(address) internal view override onlyDeployer {}

    /**
     * @dev Hook that is called before any token transfer. This includes minting
     * and burning (See documentation of OpenZeppelin's ERC721Upgradeable.sol).
     * @param from address the transfer is originating from
     * @param to receiver address
     * @param tokenId the transferred token's ID that is being transferred
     * @param batchSize batchSize
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal pure override {
        if (from != address(0) && to != address(0)) {
            revert BrandNFTTransferForbidden(from, to, tokenId, batchSize);
        }
    }
}

