import "../interfaces/IRoleManager.sol";

/******************************************************************************\
* Custom Error declarations
/******************************************************************************/
error RoleNotAuthorized(bytes32[] roles, address caller);
error CallerIsNotOwner(address owner, address caller);

abstract contract Guards {
    IRoleManager public roleManager;
    bytes32 public constant DEPLOYER_ROLE = keccak256("DEPLOYER");
    bytes32 public constant DAO_FOUNDATION_ROLE = keccak256("DAO");
    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR");
    bytes32 public constant SEASON_MANAGER_CONTRACT_ROLE =
        keccak256("SEASON_MANAGER_CONTRACT");

    modifier onlyDeployer() {
        bytes32[] memory roles = new bytes32[](1);
        roles[0] = DEPLOYER_ROLE;
        checkHasRoles(roles);
        _;
    }

    modifier onlyDAOFoundation() {
        bytes32[] memory roles = new bytes32[](1);
        roles[0] = DAO_FOUNDATION_ROLE;
        checkHasRoles(roles);
        _;
    }

    modifier onlyOperator() {
        bytes32[] memory roles = new bytes32[](1);
        roles[0] = OPERATOR_ROLE;
        checkHasRoles(roles);
        _;
    }

    modifier onlyDAOFoundationOrOperator() {
        bytes32[] memory roles = new bytes32[](2);
        roles[0] = DAO_FOUNDATION_ROLE;
        roles[1] = OPERATOR_ROLE;
        checkHasRoles(roles);
        _;
    }

    modifier onlyDAOFoundationOrOperatorOrOneAddress(address owner) {
        bytes32[] memory roles = new bytes32[](2);
        roles[0] = DAO_FOUNDATION_ROLE;
        roles[1] = OPERATOR_ROLE;
        checkHasRolesOrIsOwner(roles, owner);
        _;
    }

    modifier onlySeasonManagerContract() {
        bytes32[] memory roles = new bytes32[](1);
        roles[0] = SEASON_MANAGER_CONTRACT_ROLE;
        checkHasRoles(roles);
        _;
    }

    /**
     * @dev check if at least one role has authorization
     * @param roles array of roles to check
     */
    function checkHasRoles(bytes32[] memory roles) internal view {
        bool hasAtLeastOneRole = false;
        for (uint256 i = 0; i < roles.length; i++) {
            if (roleManager.hasRole(roles[i], msg.sender)) {
                hasAtLeastOneRole = true;
            }
        }
        if (!hasAtLeastOneRole) {
            revert RoleNotAuthorized(roles, msg.sender);
        }
    }

    /**
     * @dev check if at least one role has authorization
     * @param roles array of roles to check
     * @param owner owner address
     */
    function checkHasRolesOrIsOwner(
        bytes32[] memory roles,
        address owner
    ) internal view {
        bool hasAtLeastOneRole = false;
        for (uint256 i = 0; i < roles.length; i++) {
            if (roleManager.hasRole(roles[i], msg.sender)) {
                hasAtLeastOneRole = true;
            }
        }
        if (!hasAtLeastOneRole && owner != msg.sender) {
            revert CallerIsNotOwner(owner, msg.sender);
        }
    }
}

