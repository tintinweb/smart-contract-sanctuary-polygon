pragma solidity 0.8.17;

/******************************************************************************\
* @author The Consensys Team
* @title VillageDAO Reward Pool smart contract
/******************************************************************************/

import "./abstracts/Guards.sol";
import "./interfaces/ISeasonManager.sol";
import "./BrandNFT.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableMapUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

/**
 * @dev Caller has already claimed its rewards
 * @param caller address of caller
 * @param seasonId season Id
 */
error AlreadyHasClaimedRewards(address caller, uint256 seasonId);

/**
 * @dev Caller has to have a BrandNFT
 * @param caller address of caller
 */
error CallerNotBrand(address caller);

/**
 * @dev Fail in the deposit function
 * @param reason of revert
 */
error DepositFailed(bytes reason);

/**
 * @dev ERC20 Smart Contract address already listed in the reward pool
 * @param tokenAddress address of ERC20 token Smart Contract
 */
error TokenAlreadyExist(address tokenAddress);

/**
 * @dev ERC20 not available for deposit
 * @param tokenAddress address of ERC20 token Smart Contract
 * @param amount total of tokens to deposit
 */
error TokenNotExist(address tokenAddress, uint256 amount);

/**
 * @dev Implementation of reward pool upgradable contract for VillageDAO
 */
contract RewardPool is
    Initializable,
    UUPSUpgradeable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable,
    Guards
{
    using EnumerableMapUpgradeable for EnumerableMapUpgradeable.AddressToUintMap;
    using SafeERC20Upgradeable for IERC20Upgradeable;

    /**
     * @dev struct with score and rank as the contribution of the expert
     */
    struct Rating {
        uint256 score;
        uint256 rank;
    }

    /**
     * @dev struct representing a reward with token and amount
     */
    struct Reward {
        address token;
        uint256 amount;
    }

    BrandNFT public brandNFT;
    ISeasonManager public seasonManager;
    /// @dev Bool to set the brandNFT SC address once
    bool public hasBeenSetOnceBrandNFTSC;
    /// @dev Bool to set the seasonManager SC address once
    bool public hasBeenSetOnceSeasonSC;
    /// @dev Iterable mapping of ERC20 token address for uint funding
    EnumerableMapUpgradeable.AddressToUintMap private tokens;
    /// @dev mapping of brand address for mapping of uint seasonId
    /// for mapping of ERC20 address for uint total season funding
    mapping(address => mapping(uint256 => mapping(address => uint256)))
        public brandsFunding;
    /// @dev mapping of uint seasonId for mapping of uint brandId
    /// for mapping of expert address for Rating
    mapping(uint256 => mapping(uint256 => mapping(address => Rating)))
        public seasonContributions;
    /// @dev mapping of expert address for mapping of uint seasonId for bool claimed
    mapping(address => mapping(uint256 => bool)) public claimedRewards;
    /// @dev mapping of seasonId uint for mapping of uint brandId for uint score
    mapping(uint256 => mapping(uint256 => uint256)) public brandScoreSeasons;
    /// @dev this mapping stores the experts seasons count.
    /// Each time an expert contribute to a season its count is incremented
    mapping(address => uint256) public expertSeasonCount;
    /// @dev stores if an expert has contributed to a season.
    /// stores true when expert has contributed and false when not
    mapping(address => mapping(uint256 => bool))
        private expertHasContributedToSeason;
    /// @dev this mapping stores the brand seasons count.
    /// Each time a brand contribute to a season its count is incremented
    mapping(address => uint256) public brandSeasonCount;
    /// @dev stores if a brand has contributed to a season.
    /// stores true when brand has contributed and false when not
    mapping(address => mapping(uint256 => bool))
        private brandHasContributedToSeason;
    /// @dev stores the brand ids assigned to an expert
    mapping(address => uint256[]) public expertAssignedBrands;

    /**
     * @dev emits an event when the ERC20 token is added in the list
     * @param tokenAddress address of ERC20 token Smart Contract
     */
    event AssignToken(address indexed tokenAddress);

    /**
     * @dev emits an event when ERC20 tokens are funding to the reward pool
     * @param caller address of caller
     * @param seasonId season Id
     */
    event ClaimReward(address indexed caller, uint256 indexed seasonId);

    /**
     * @dev emits an event when ERC20 tokens are funding to the reward pool
     * @param tokenAddress address of ERC20 token Smart Contract
     * @param amount total of tokens to deposit
     * @param realAmountTransfered real total of deposited tokens
     */
    event Deposit(
        address indexed tokenAddress,
        uint256 amount,
        uint256 realAmountTransfered
    );

    /**
     * @dev emits an event when setting the brandNFT Smart Contract address
     * @param brandNFTContract address of brandNFT Smart Contract
     */
    event SetBrandNFTSCAddress(address indexed brandNFTContract);

    /**
     * @dev emits an event when setting the seasonManager Smart Contract address
     * @param seasonManagerContract address of season manager Smart Contract
     */
    event SetSeasonManagerSCAddress(address indexed seasonManagerContract);

    /**
     * @dev emits an event when the DAO Foundation withdraw funds
     * @param caller address of the caller and receiver
     * @param tokenAddress address of ERC20 token Smart Contract
     * @param tokenAddress amount to withdraw
     */
    event WithdrawAmount(
        address indexed caller,
        address indexed tokenAddress,
        uint256 withdrawAmount
    );

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    /**
     * @dev replaces the constructor method
     * @param _roleManager roleManager contract address
     * @param tokensAddress list of addresses of ERC20 token Smart Contract
     */
    function initialize(
        address _roleManager,
        address[] memory tokensAddress
    ) external initializer {
        __UUPSUpgradeable_init();
        roleManager = IRoleManager(_roleManager);
        hasBeenSetOnceBrandNFTSC = false;
        hasBeenSetOnceSeasonSC = false;
        for (uint256 i = 0; i < tokensAddress.length; i++) {
            assert(EnumerableMapUpgradeable.set(tokens, tokensAddress[i], 0));
        }
    }

    /**
     * @dev add a ERC20 token to the accepted bonding list
     * @param tokenAddress address of ERC20 token Smart Contract
     */
    function assignToken(
        address tokenAddress
    ) external onlyDAOFoundationOrOperator whenNotPaused {
        if (EnumerableMapUpgradeable.contains(tokens, tokenAddress)) {
            revert TokenAlreadyExist(tokenAddress);
        }
        assert(EnumerableMapUpgradeable.set(tokens, tokenAddress, 0));
        emit AssignToken(tokenAddress);
    }

    /**
     * @dev execute withdraw of expert reward
     * @param seasonId season Id
     */
    function claimReward(uint256 seasonId) external nonReentrant whenNotPaused {
        if (claimedRewards[msg.sender][seasonId]) {
            revert AlreadyHasClaimedRewards(msg.sender, seasonId);
        }
        claimedRewards[msg.sender][seasonId] = true;
        for (uint256 i = 0; i < EnumerableMapUpgradeable.length(tokens); i++) {
            (address allowedTokenAddress, ) = EnumerableMapUpgradeable.at(
                tokens,
                i
            );
            uint256 cumulatedTokenAmount = 0;
            uint256 lastBrandTokenId = brandNFT.getLastTokenId();
            for (uint256 j = 1; j <= lastBrandTokenId; j++) {
                cumulatedTokenAmount += computeReward(
                    seasonId,
                    msg.sender,
                    allowedTokenAddress,
                    j
                );
            }
            if (cumulatedTokenAmount > 0) {
                IERC20Upgradeable(allowedTokenAddress).safeTransfer(
                    msg.sender,
                    cumulatedTokenAmount
                );
                uint256 currentBalance = EnumerableMapUpgradeable.get(
                    tokens,
                    allowedTokenAddress
                );
                assert(
                    !EnumerableMapUpgradeable.set(
                        tokens,
                        allowedTokenAddress,
                        currentBalance - cumulatedTokenAmount
                    )
                );
            }
        }
        emit ClaimReward(msg.sender, seasonId);
    }

    /**
     * @dev deposit tokens for the brand to the reward pool
     * @param tokenAddress address of ERC20 token Smart Contract
     * @param amount total of tokens to deposit
     */
    function deposit(
        address tokenAddress,
        uint256 amount
    ) external nonReentrant whenNotPaused {
        if (brandNFT.tokenIds(msg.sender) == 0) {
            revert CallerNotBrand(msg.sender);
        }
        if (!EnumerableMapUpgradeable.contains(tokens, tokenAddress)) {
            revert TokenNotExist(tokenAddress, amount);
        }
        uint256 rewardPoolTokenBalanceBefore = IERC20Upgradeable(tokenAddress)
            .balanceOf(address(this));
        IERC20Upgradeable(tokenAddress).safeTransferFrom(
            msg.sender,
            address(this),
            amount
        );
        uint256 rewardPoolTokenBalanceAfter = IERC20Upgradeable(tokenAddress)
            .balanceOf(address(this));
        uint256 realAmountTransfered = rewardPoolTokenBalanceAfter -
            rewardPoolTokenBalanceBefore;
        uint256 currentBalance = EnumerableMapUpgradeable.get(
            tokens,
            tokenAddress
        );
        assert(
            !EnumerableMapUpgradeable.set(
                tokens,
                tokenAddress,
                currentBalance + realAmountTransfered
            )
        );
        try seasonManager.getCurrentSeasonNumberId() returns (
            uint256 seasonNumberId
        ) {
            brandsFunding[msg.sender][seasonNumberId + 1][
                tokenAddress
            ] += realAmountTransfered;
            if (!brandHasContributedToSeason[msg.sender][seasonNumberId + 1]) {
                brandSeasonCount[msg.sender] += 1;
                brandHasContributedToSeason[msg.sender][
                    seasonNumberId + 1
                ] = true;
            }
        } catch (bytes memory reason) {
            if (bytes4(keccak256(bytes("NoSeasons()"))) == bytes4(reason)) {
                brandsFunding[msg.sender][0][
                    tokenAddress
                ] += realAmountTransfered;
                if (!brandHasContributedToSeason[msg.sender][0]) {
                    brandSeasonCount[msg.sender] = 1;
                    brandHasContributedToSeason[msg.sender][0] = true;
                }
            } else {
                revert DepositFailed(reason);
            }
        }
        emit Deposit(tokenAddress, amount, realAmountTransfered);
    }

    /**
     * @dev Get the amount of funding of the ERC20 token in the SC
     * @param tokenAddress address of ERC20 token
     * @return total funding of the token in the SC
     */
    function getTokenFunding(
        address tokenAddress
    ) external view returns (uint256) {
        return EnumerableMapUpgradeable.get(tokens, tokenAddress);
    }

    /**
     * @dev Get the list of the ERC20 tokens in the SC
     * @return list of the tokens in the SC
     */
    function getTokenList() external view returns (address[] memory) {
        uint256 size = EnumerableMapUpgradeable.length(tokens);
        address[] memory list = new address[](size);
        for (uint256 i = 0; i < size; i++) {
            (address key, ) = EnumerableMapUpgradeable.at(tokens, i);
            list[i] = key;
        }
        return list;
    }

    /**
     * @dev Triggers stopped state.
     * Requirements:
     * - the contract must not be paused.
     */
    function pause() external onlyDAOFoundationOrOperator {
        _pause();
    }

    /**
     * @dev set the brandNFT Smart Contract address
     * @param brandNFTContract address of brandNFT Smart Contract
     */
    function setBrandNFTAddress(
        address brandNFTContract
    ) external onlyDeployer whenNotPaused {
        if (!hasBeenSetOnceBrandNFTSC) {
            hasBeenSetOnceBrandNFTSC = true;
            brandNFT = BrandNFT(brandNFTContract);
            emit SetBrandNFTSCAddress(brandNFTContract);
        }
    }

    /**
     * @dev set the seasonManager Smart Contract address
     * @param seasonManagerContract address of season manager Smart Contract
     */
    function setSeasonManagerAddress(
        address seasonManagerContract
    ) external onlyDeployer whenNotPaused {
        if (!hasBeenSetOnceSeasonSC) {
            hasBeenSetOnceSeasonSC = true;
            seasonManager = ISeasonManager(seasonManagerContract);
            emit SetSeasonManagerSCAddress(seasonManagerContract);
        }
    }

    /**
     * @dev Store the contribution data from each season only callable by SeasonManagerContract
     * @param seasonId season id
     * @param brandId brand id
     * @param expertAddresses expert address
     * @param score expert score for that brand id
     * @param rank expert rank for that brand id
     */
    function storeSeasonContribution(
        uint256 seasonId,
        uint256 brandId,
        address expertAddresses,
        uint256 score,
        uint256 rank
    ) external onlySeasonManagerContract whenNotPaused {
        seasonContributions[seasonId][brandId][expertAddresses] = Rating(
            score,
            rank
        );
        if (score > 0) {
            brandScoreSeasons[seasonId][brandId] += score;
            if (!expertHasContributedToSeason[expertAddresses][seasonId]) {
                expertSeasonCount[expertAddresses] += 1;
                expertHasContributedToSeason[expertAddresses][seasonId] = true;
            }
        }
    }

    /**
     * @dev Returns to normal state.
     * Requirements:
     * - the contract must be paused.
     */
    function unpause() external onlyDAOFoundationOrOperator {
        _unpause();
    }

    /**
     * @dev If a brand (or anyone) has sent tokens directly to the RewardPool, the balance of the SC is messed up.
     *  The DAO Foundation should be able to withdraw this token.
     * @param tokenAddress address of ERC20 token Smart Contract
     */
    function withdrawTokens(
        address tokenAddress
    ) external nonReentrant onlyDAOFoundation {
        uint256 rewardPoolBalance = IERC20Upgradeable(tokenAddress).balanceOf(
            address(this)
        );
        uint256 tokenDepositedAmount = EnumerableMapUpgradeable.get(
            tokens,
            tokenAddress
        );
        uint256 withdrawAmount = rewardPoolBalance - tokenDepositedAmount;
        IERC20Upgradeable(tokenAddress).safeTransfer(
            msg.sender,
            withdrawAmount
        );
        emit WithdrawAmount(msg.sender, tokenAddress, withdrawAmount);
    }

    /**
     * @dev compute the reward amount available for an expert
     * @param seasonId seasonId
     * @param expertAddress address of the expert
     * @return tuple of address token and uint256 amount
     */
    function getRewardToClaim(
        uint256 seasonId,
        address expertAddress
    ) public view returns (address[] memory, uint256[] memory) {
        if (claimedRewards[expertAddress][seasonId]) {
            revert AlreadyHasClaimedRewards(expertAddress, seasonId);
        }
        address[] memory token = new address[](
            EnumerableMapUpgradeable.length(tokens)
        );
        uint256[] memory amount = new uint256[](
            EnumerableMapUpgradeable.length(tokens)
        );
        for (uint256 i = 0; i < EnumerableMapUpgradeable.length(tokens); i++) {
            (address allowedTokenAddress, ) = EnumerableMapUpgradeable.at(
                tokens,
                i
            );
            uint256 tokenAmount = 0;
            uint256 lastBrandTokenId = brandNFT.getLastTokenId();
            for (uint256 j = 1; j <= lastBrandTokenId; j++) {
                tokenAmount += computeReward(
                    seasonId,
                    expertAddress,
                    allowedTokenAddress,
                    j
                );
            }
            token[i] = allowedTokenAddress;
            amount[i] = tokenAmount;
        }
        return (token, amount);
    }

    /**
     * @dev Used by UUPS Upgradability pattern to include access restriction
     * to the upgrade mechanism
     */
    function _authorizeUpgrade(address) internal view override onlyDeployer {}

    /**
     * @dev Compute the claimable reward for an expert a season a brand and a token
     * @param seasonId The season Id
     * @param expertAddress The expert address
     * @param allowedTokenAddress The token, reward is computed
     * @param brandTokenId The brand token id
     * @return tokenAmount the amount of rewarded token for an expert
     */
    function computeReward(
        uint256 seasonId,
        address expertAddress,
        address allowedTokenAddress,
        uint256 brandTokenId
    ) internal view returns (uint256 tokenAmount) {
        uint256 decimal = 10 ** 18;
        uint256 currentExpertScore = seasonContributions[seasonId][
            brandNFT.getBrandId(brandTokenId)
        ][expertAddress].score;
        uint256 totalBrandSeasonScore = brandScoreSeasons[seasonId][
            brandNFT.getBrandId(brandTokenId)
        ];
        if (currentExpertScore > 0 && totalBrandSeasonScore > 0) {
            uint256 percentageReward = (currentExpertScore * decimal) /
                totalBrandSeasonScore;
            tokenAmount =
                (brandsFunding[brandNFT.ownerOf(brandTokenId)][seasonId][
                    allowedTokenAddress
                ] * percentageReward) /
                decimal;
        }
        return tokenAmount;
    }
}