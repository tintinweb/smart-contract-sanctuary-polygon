/// @custom:security-contact [email protected]
contract SOZOA is
    Initializable,
    ERC721Upgradeable,
    ERC721EnumerableUpgradeable,
    ERC721URIStorageUpgradeable,
    ERC2981Upgradeable,
    UUPSUpgradeable,
    OwnableUpgradeable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable,
    DefaultOperatorFiltererUpgradeable
{
    using CountersUpgradeable for CountersUpgradeable.Counter;

    /* ---------- Storage ---------- */

    CountersUpgradeable.Counter private _tokenIdCounter;

    string private __baseURI;
    string private _contractURI;

    uint256 public maxSupply;
    address public signerAddress;

    mapping(uint256 => string) public tokenDNA;
    mapping(string => bool) public mintedDNA;

    /* ---------- Events ---------- */

    event Minted(address indexed to, uint256 indexed tokenId, string dna);
    event Updated(address indexed to, uint256 indexed tokenId, string newDNA);
    event Withdrawn(address indexed to, uint256 amount);

    /* ---------- Initializer ---------- */

    /**
     * @custom:oz-upgrades-unsafe-allow constructor
     */
    constructor() {
        _disableInitializers();
    }

    function initialize(
        string memory name,
        string memory symbol,
        string memory baseURI,
        string memory __contractURI,
        uint96 feeNumerator,
        uint256 _maxSupply,
        address _signerAddress
    ) public initializer {
        __ERC721_init(name, symbol);
        __ERC721Enumerable_init();
        __ERC721URIStorage_init();
        __ERC2981_init();
        __UUPSUpgradeable_init();
        __Ownable_init();
        __Pausable_init();
        __ReentrancyGuard_init();

        __baseURI = baseURI;
        _contractURI = __contractURI;
        _setDefaultRoyalty(owner(), feeNumerator);
        maxSupply = _maxSupply;
        signerAddress = _signerAddress;
    }

    /* ---------- Metadata ---------- */

    function _baseURI() internal view override returns (string memory) {
        return __baseURI;
    }

    function contractURI() external view returns (string memory) {
        return _contractURI;
    }

    function setBaseURI(string calldata baseURI) external onlyOwner {
        __baseURI = baseURI;
        emit BatchMetadataUpdate(0, type(uint256).max);
    }

    function setContractURI(string calldata newContractURI) external onlyOwner {
        _contractURI = newContractURI;
    }

    /* ---------- Pause / Unpause ---------- */

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    /* ---------- Mint ---------- */

    function mint(
        string memory dna,
        bytes memory signature
    ) external payable whenNotPaused nonReentrant {
        // check that the max supply is not reached
        require(
            maxSupply == 0 || _tokenIdCounter.current() < maxSupply,
            'Max supply reached'
        );

        // check that the new DNA has not been minted
        require(!mintedDNA[dna], 'This DNA is already minted');

        // check that the signature is valid
        bytes32 hash = keccak256(abi.encodePacked(dna, msg.value));
        bytes32 signedHash = ECDSAUpgradeable.toEthSignedMessageHash(hash);
        address signer = ECDSAUpgradeable.recover(signedHash, signature);
        require(signer == signerAddress, 'Invalid signature');

        _tokenIdCounter.increment();
        uint256 tokenId = _tokenIdCounter.current();

        tokenDNA[tokenId] = dna;
        mintedDNA[dna] = true;

        _safeMint(msg.sender, tokenId);

        emit Minted(msg.sender, tokenId, dna);
    }

    /* ---------- Update ---------- */

    function update(
        uint256 tokenId,
        string memory newDNA,
        bytes memory signature
    ) public payable whenNotPaused nonReentrant {
        // check that the caller is the owner of the token
        require(
            _isApprovedOrOwner(msg.sender, tokenId),
            'Caller is not owner nor approved'
        );

        // check that the new DNA has not been used
        require(!mintedDNA[newDNA], 'This DNA is already used');

        // check that the signature is valid
        bytes32 hash = keccak256(abi.encodePacked(tokenId, newDNA, msg.value));
        bytes32 signedHash = ECDSAUpgradeable.toEthSignedMessageHash(hash);
        address signer = ECDSAUpgradeable.recover(signedHash, signature);
        require(signer == signerAddress, 'Invalid signature');

        // update the DNA
        string memory oldDNA = tokenDNA[tokenId];
        tokenDNA[tokenId] = newDNA;
        mintedDNA[oldDNA] = false;
        mintedDNA[newDNA] = true;

        emit Updated(msg.sender, tokenId, newDNA);
    }

    /* ---------- Withdraw ---------- */

    /**
     * @dev See https://consensys.github.io/smart-contract-best-practices/development-recommendations/general/external-calls/#dont-use-transfer-or-send
     */
    function withdraw() external onlyOwner {
        uint256 amount = address(this).balance;
        (bool success, ) = owner().call{value: amount}('');
        require(success, 'Transfer failed.');

        emit Withdrawn(owner(), amount);
    }

    /* ---------- Others ---------- */

    /**
     * @dev Returns the DNAs of the tokens from `startTokenId` to `startTokenId + maxLength - 1`.
     *
     * @param startTokenId The start token ID.
     * @param maxLength The maximum length of the returned array.
     */
    function getDNAs(
        uint256 startTokenId,
        uint256 maxLength
    ) external view returns (string[] memory) {
        require(startTokenId > 0, 'Invalid start token ID');

        if (startTokenId > totalSupply()) {
            return new string[](0);
        }

        uint256 actualLength;
        if (startTokenId + maxLength - 1 > totalSupply()) {
            actualLength = totalSupply() - startTokenId + 1;
        } else {
            actualLength = maxLength;
        }

        string[] memory tokenDNAs = new string[](actualLength);
        for (uint256 i = 0; i < actualLength; i++) {
            tokenDNAs[i] = tokenDNA[startTokenId + i];
        }

        return tokenDNAs;
    }

    /**
     * @dev Sets the royalty fee numerator for ERC2981 royalties.
     *
     * @param feeNumerator The new royalty fee numerator (0-10,000 representing 0-100%).
     */
    function setFeeNumerator(uint96 feeNumerator) external onlyOwner {
        _setDefaultRoyalty(owner(), feeNumerator);
    }

    function setMaxSupply(uint256 _maxSupply) external onlyOwner {
        maxSupply = _maxSupply;
    }

    function setSignerAddress(address _signerAddress) external onlyOwner {
        signerAddress = _signerAddress;
    }

    function _authorizeUpgrade(
        address newImplementation
    ) internal override onlyOwner {}

    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal override(ERC721Upgradeable, ERC721EnumerableUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function _burn(
        uint256 tokenId
    ) internal override(ERC721Upgradeable, ERC721URIStorageUpgradeable) {
        super._burn(tokenId);
    }

    function tokenURI(
        uint256 tokenId
    )
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        override(
            ERC721Upgradeable,
            ERC721EnumerableUpgradeable,
            ERC721URIStorageUpgradeable,
            ERC2981Upgradeable
        )
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    /* ---------- Operator Filter Registry ---------- */

    function setApprovalForAll(
        address operator,
        bool approved
    )
        public
        override(IERC721Upgradeable, ERC721Upgradeable)
        onlyAllowedOperatorApproval(operator)
    {
        super.setApprovalForAll(operator, approved);
    }

    function approve(
        address operator,
        uint256 tokenId
    )
        public
        override(IERC721Upgradeable, ERC721Upgradeable)
        onlyAllowedOperatorApproval(operator)
    {
        super.approve(operator, tokenId);
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    )
        public
        override(IERC721Upgradeable, ERC721Upgradeable)
        onlyAllowedOperator(from)
    {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    )
        public
        override(IERC721Upgradeable, ERC721Upgradeable)
        onlyAllowedOperator(from)
    {
        super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    )
        public
        override(IERC721Upgradeable, ERC721Upgradeable)
        onlyAllowedOperator(from)
    {
        super.safeTransferFrom(from, to, tokenId, data);
    }
}

