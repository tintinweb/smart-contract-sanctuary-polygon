		string memory uri,
		address _trustedForwarder,
		ERC20Permit _paymentToken,
		address admin
	) public initializer {
		__ERC721_init("BabyDiamond", "BD");
		__BaseNft_init(_trustedForwarder, _paymentToken);

		_baseUri = uri;
		_royaltyInfo = RoyaltyInfo(address(this), 0);
		maxSupply = type(uint256).max;

		_grantRole(DEFAULT_ADMIN_ROLE, admin);
	}

	// ---------- Administration ----------

	/**
	 * @notice Setting base URI to `uri`.
	 * @param uri Base URI.
	 */
	function setBaseUri(string memory uri) external onlyRole(MANAGER_ROLE) {
		_baseUri = uri;
	}

	/**
	 * @dev Overriding to allow configurable URI.
