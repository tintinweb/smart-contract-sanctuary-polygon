		uint amount,
		uint weight
	) internal returns (uint[] memory) {
		if (totalSupply() + amount > maxSupply) {
			revert MintingTooMany();
		}
		if (
			weight < STARTING_WEIGHT ||
			weight % STARTING_WEIGHT != 0 ||
			weight > MAX_WEIGHT
		) {
			revert InvalidWeight();
		}
		uint[] memory ids = new uint[](amount);
		for (uint i = 0; i < amount; i++) {
			_indexCounter.increment();
			uint256 tokenId = _indexCounter.current();
			_safeMint(to, tokenId);
