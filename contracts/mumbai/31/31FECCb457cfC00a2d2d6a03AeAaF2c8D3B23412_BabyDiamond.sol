		returns (bool)
	{
		return super.supportsInterface(interfaceId);
	}
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.19;

import '@opengsn/contracts/src/ERC2771Recipient.sol';
import '@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol';
import '@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol';
import '@openzeppelin/contracts/token/ERC20/extensions/draft-ERC20Permit.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';

/**
 * @title Base contract to provide common functionalities to Brillianz NFTs.
 * @author Dalton Tan
 * @dev Features of this contract:
 * - ERC2771 - Meta Transactions
 * - Role base access control
 * - UUPS Upgradeable
 * - Freezing tokens
 * - Withdrawing payment
 */
abstract contract BaseNft is Initializable, ERC2771Recipient, AccessControlUpgradeable, UUPSUpgradeable {
	/// @notice This call has no effect. Reverting to save gas.
	error NoChange();
	/// @notice Address is not valid.
	error AddressInvalid(address);
	/// @notice Not enough balance.
	error InsufficientBalance();
	/// @notice Token is frozen and cannot be transferred.
	error TokenFrozen(uint);

	/// @dev Emitted when freezing or unfreezing a token.
	event SetFrozenToken(uint indexed id, bool indexed isFrozen, address indexed operator);

	// User roles
	bytes32 public constant MANAGER_ROLE = keccak256('MANAGER_ROLE');
	bytes32 public constant APPROVAL_ROLE = keccak256('APPROVAL_ROLE');
	bytes32 public constant MONEY_ROLE = keccak256('MONEY_ROLE');
	bytes32 public constant UPGRADER_ROLE = keccak256('UPGRADER_ROLE');

	/// @notice This is the block number that this contract was created at.
	/// @dev This can be useful for filtering logs
	uint public blockAtCreation;
	/// @notice Payments are done with this token.
	ERC20Permit public paymentToken;
	/// @notice Mapping of whether a token is frozen. Frozen tokens can't be transferred or burned.
	mapping(uint => bool) public frozenTokens;

	function __BaseNft_init(address _trustedForwarder, ERC20Permit _paymentToken) internal onlyInitializing {
		__AccessControl_init();
		__UUPSUpgradeable_init();

		blockAtCreation = block.number;
		_setTrustedForwarder(_trustedForwarder);
		paymentToken = _paymentToken;
	}

	// ---------- Security ----------

	/**
	 * @notice Setting whether token `id` is frozen.
	 * @param id Token ID.
	 * @param isFrozen Whether the token is frozen and can't be transferred or burned.
	 */
	function setFrozenToken(uint id, bool isFrozen) external onlyRole(MANAGER_ROLE) {
		if (frozenTokens[id] == isFrozen) {
			revert NoChange();
		}
		frozenTokens[id] = isFrozen;
		emit SetFrozenToken(id, isFrozen, _msgSender());
	}

	/**
	 * @dev Blocks frozen tokens by reverting no matter the permission.
	 * @param tokenId Token ID.
	 */
	function _blockIfFrozen(uint256 tokenId) internal virtual {
		if (frozenTokens[tokenId]) {
			revert TokenFrozen(tokenId);
		}
	}

	/**
	 * @dev Returns true if we can skip the approval step. Call this in isApprovedForAll().
	 * This is used for:
	 * 1. Admins to have oversight over all tokens.
	 * 2. Trusted contracts to skip approval step.
	 */
	function _canBypassApproval(address, address) internal view returns (bool) {
		if (hasRole(APPROVAL_ROLE, _msgSender())) {
			return true;
		}
		return false;
	}

	function _authorizeUpgrade(address newImplementation) internal override onlyRole(UPGRADER_ROLE) {}

	// ---------- Withdrawing ----------

	/**
	 * @notice Withdrawing `amount` wei of payment token to `recipient`.
	 * @param recipient Address to send the token.
	 * @param amount Amount to withdraw.
	 */
	function withdrawPayment(address recipient, uint amount) external {
		withdrawToken(recipient, amount, paymentToken);
	}

	/**
	 * @notice Withdrawing `amount` wei in token `token` to `recipient`.
	 * @dev Exposing this method in case we have other tokens sent here.
	 * @param recipient Address to send the token.
	 * @param amount Amount to withdraw.
	 * @param token The token to withdraw.
	 */
	function withdrawToken(address recipient, uint amount, IERC20 token) public onlyRole(MONEY_ROLE) {
		if (recipient == address(0)) {
			revert AddressInvalid(recipient);
		}
		token.approve(address(this), amount);
		token.transferFrom(address(this), recipient, amount);
	}

	/**
	 * @notice Withdrawing `amount` wei of native token to `recipient`.
	 * @dev This is to withdraw native token e.g. ETH, MATIC
	 * @param recipient Address to send the native token.
	 * @param amount Amount to withdraw.
	 */
	function withdrawNative(address recipient, uint amount) external onlyRole(MONEY_ROLE) {
		if (recipient == address(0)) {
			revert AddressInvalid(recipient);
		}
		uint balanceBeforeTransfer = address(this).balance;
		if (amount > balanceBeforeTransfer) {
			revert InsufficientBalance();
		}
		(bool success, ) = address(recipient).call{value: amount}('');
		require(success);
		assert(address(this).balance == balanceBeforeTransfer - amount); // used for internal error checking
	}

	/**
	 * @notice Depositing native token into this contract.
	 */
	function deposit() external payable {}

	// ---------- Others ----------

	function _msgSender() internal view virtual override(ContextUpgradeable, ERC2771Recipient) returns (address) {
		return super._msgSender();
	}

	function _msgData() internal view virtual override(ContextUpgradeable, ERC2771Recipient) returns (bytes calldata) {
		return super._msgData();
	}
}