	/// @dev Emitted when combining 2 tokens.
	event ForgedTokens(
		address indexed recipient,
		uint indexed newToken,
		uint token1,
		uint token2,
		uint weight1,
		uint weight2
	);
	/// @dev Emitted burning a token for redemption.
	event RedeenToken(address indexed owner, uint indexed id, uint weight);

