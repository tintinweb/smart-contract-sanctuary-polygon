pragma solidity ^0.8.6;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@opengsn/contracts/src/ERC2771Recipient.sol";
import "hardhat/console.sol";


import "./interfaces/IWhiteList.sol";
import "./interfaces/IERC3009Partial.sol";

contract FiduciaryDuty is ERC2771Recipient, ERC20, Ownable {

    modifier onlyWhiteListed(address beneficiary){
        IWhiteList whiteList = IWhiteList(_whiteListContract);
        require(whiteList.isAddressWhiteListed(_msgSender()), "Sender of transaction should be whitelisted");
        if (beneficiary != _msgSender()) {
            require(whiteList.isAddressWhiteListed(beneficiary), "Beneficiary should be whitelisted");
        }
        _;
    }

    struct Permission {
        address from;
        address to;
        uint256 value;
        uint256 validAfter;
        uint256 validBefore;
        bytes32 nonce;
        uint8 v;
        bytes32 r;
        bytes32 s;
    }

    address private _tokenStorageAddress;
    address private _managerAddress;
    address private _whiteListContract;
    uint private _sold = 0;
    bool private _viablePoint = false;
    bool private _canceledRaise = false;
    uint private _minimal;

    event PaymentAdded(address indexed user, uint amount);
    event PaymentWithdraw(address indexed user, uint amount);


    constructor(
        address storageCrowdToken,
        string memory name,
        string memory symbol,
        uint256 totalSupply,
        address managerAddress,
        address whiteListAddress,
        address trustedForwarder,
        uint minimal
    ) ERC20(name, symbol)  {
        _tokenStorageAddress = storageCrowdToken;
        _mint(address(this), totalSupply);
        _managerAddress = managerAddress;
        _whiteListContract = whiteListAddress;
        _setTrustedForwarder(trustedForwarder);
        _minimal = minimal;
    }

    function toRaise() public view returns (uint){
        return super.totalSupply() * 10;
    }

    function raised() public view returns (uint){
        return _sold * 10;
    }

    function viablePoint() public view returns (bool){
        return _viablePoint;
    }

    function minimal() public view returns (uint){
        return _minimal;
    }


    function canceledRaise() public view returns (bool){
        return _canceledRaise;
    }

    function managerAddress() public view returns (address){
        return _managerAddress;
    }

    function forwarder() public view returns (address){
        return super.getTrustedForwarder();
    }

    function whiteListContract() public view returns (address){
        return _whiteListContract;
    }

    function sold() public view returns (uint){
        return _sold;
    }

    function tokenStorage() public view returns (address){
        return _tokenStorageAddress;
    }

    function addForPayment(
        Permission memory permission,
        address beneficiary
    ) external onlyWhiteListed(beneficiary) {
        require(_canceledRaise == false, "The raise was canceled. No more payments allowed");
        require(permission.value != 0, "Value should be greater than 0");
        require(permission.value <= ERC20(_tokenStorageAddress).balanceOf(permission.from),
            "You should have enough to give the permission");
        require(
            permission.to == address(this),
            "You should transfer to the escrow contract"
        );
        require(
            permission.value %
            (10 ** ERC20(_tokenStorageAddress).decimals()) ==
            0, "Payment should be complete units"
        );
        uint unitTransfer = permission.value / (10 ** ERC20(_tokenStorageAddress).decimals());
        require(unitTransfer % 10 == 0, "Should be divisible for 10 dollars");
        uint transferAmount = unitTransfer / 10;
        require(_sold + transferAmount <= super.totalSupply(), "You can't buy more than available");
        _sold = _sold + transferAmount;

        IERC3009Partial(_tokenStorageAddress).transferWithAuthorization(
            permission.from,
            permission.to,
            permission.value,
            permission.validAfter,
            permission.validBefore,
            permission.nonce,
            permission.v,
            permission.r,
            permission.s
        );

        ERC20(address(this)).transfer(
            beneficiary,
            transferAmount
        );

        if (_sold >= ((super.totalSupply() * _minimal) / 100)) {
            _viablePoint = true;
        }

        emit PaymentAdded(beneficiary, transferAmount);

    }

    function decimals() public view override returns (uint8) {
        return 0;
    }

    function cancelRaise() external onlyOwner {
        _canceledRaise = true;
        _viablePoint = false;
    }

    function withdrawForDuty() external onlyOwner {
        require(_canceledRaise == false, "The raise was canceled. Not allowed to get funds");
        require(_viablePoint == true, "The viable point is not reached yet");
        uint wholeAmount = ERC20(_tokenStorageAddress).balanceOf(address(this));
        ERC20(_tokenStorageAddress).transfer(_managerAddress, wholeAmount);
    }

    function withdrawPayment() external onlyWhiteListed(_msgSender()) {
        if (_canceledRaise == false) {
            require(_viablePoint == false, "The viable point was already reached");
        }
        uint refund = super.balanceOf(_msgSender());
        require(refund != 0, "Your balance should be greater than 0 for refund");
        _sold = _sold - refund;
        super._burn(_msgSender(), refund);
        super._mint(address(this), refund);
        ERC20(_tokenStorageAddress).transfer(_msgSender(), refund * 10 * 10 ** ERC20(_tokenStorageAddress).decimals());
        emit PaymentWithdraw(_msgSender(), refund);
    }

    function _msgSender() internal view override(Context, ERC2771Recipient)
    returns (address sender) {
        sender = ERC2771Recipient._msgSender();
    }

    function _msgData() internal view override(Context, ERC2771Recipient)
    returns (bytes calldata) {
        return ERC2771Recipient._msgData();
    }
}

