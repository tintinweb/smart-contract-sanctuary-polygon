// File: contracts/5_market.sol


pragma solidity >=0.4.22 <0.9.0;








contract theFluxTest_1 is IERC721Receiver, AccessControl {

    IERC20 public immutable _token;

    constructor(IERC20 token_) {
        _setupRole(ADF_ADMIN_ROLE , msg.sender);
        _token = token_;

    }

    using Counters for Counters.Counter;

    Counters.Counter private _auctionIds;
    
    mapping (uint => address) private _stableAddress; /* support other stable */

    enum auctionStatus { Non , auctioning , auctionend , auctioncancel }
    
    struct auctionListing_info {
        uint256 _info_auctionId;
        address _info_seller;
        address _info_nftAddress;
        uint256 _info_tokenid;
        uint256 _info_startPrice;
        address _info_highestbidder;
        uint256 _info_highestbid;
        uint256 _info_endTime;
        auctionStatus _info_auctionStatus;
    }

    /* auction id index */
    mapping (uint256 => uint256) internal _auctionIndex;
    /* auction info array */
    auctionListing_info[] internal _auctionidList;

    mapping(address => mapping(uint256 => uint256)) _bidsAmount;

    event auctionStartEvent(auctionListing_info);
    event auctionBidEvent (auctionListing_info);
    event auctionEndEvent (uint256 auctionId, address seller, address buyer, uint256 price);
    event auctionCancelEvent (uint256 auctionId, address seller);

    
    modifier isAuctionStart (address _nftaddress , uint256 _tokenId , uint256 _startPrice , uint _auctionPeriod ) {
        
        require (msg.sender == IERC721(_nftaddress).ownerOf(_tokenId) , "MSG.SENDER is not the nft owner");
        require (_startPrice > 0 , "startPrice must be greater than 0");
        require (_auctionPeriod == 1 || _auctionPeriod == 3 || _auctionPeriod == 7 , "auctionPeriod is not (1 day or 3 day or 7 day)");    
        _;
    }

    modifier isAuctionBid (uint256 _auctionId , uint256 _bidPrice) {

        require(_token.balanceOf(msg.sender) > _bidPrice , "msg.sender balance is less than bidPrice");
        require(_auctionidList[_auctionIndex[_auctionId]]._info_auctionStatus == auctionStatus.auctioning , "NFT not started");
        if (_auctionidList[_auctionIndex[_auctionId]]._info_highestbidder == address(0)) {
            require (_auctionidList[_auctionIndex[_auctionId]]._info_startPrice < _bidPrice , "bidPrice must be greater than startprice");
        }else {
            require (_auctionidList[_auctionIndex[_auctionId]]._info_highestbid < _bidPrice , "bidPrice must be greater than highestbid");
        }
        require(_auctionidList[_auctionIndex[_auctionId]]._info_endTime > block.timestamp , "auction ended" );
        _;
    }

    modifier isAuctionEnd(uint256 _auctionid) {
        require(hasRole(ADF_ADMIN_ROLE, msg.sender) || hasRole(AUCTIONEND_ROLE , msg.sender) , "msg.sender is missing role");
        require(_auctionidList[_auctionIndex[_auctionid]]._info_auctionStatus == auctionStatus.auctioning, "Action not started");  
        require(_auctionidList[_auctionIndex[_auctionid]]._info_endTime <= block.timestamp , "Auction is still ongoing");
        _;
    }
    modifier isAuctionCancel(uint256 _auctionid) {
        require ( _auctionidList[_auctionIndex[_auctionid]]._info_seller  == msg.sender , "MSG.SENDER is not NFT Seller.");
        require ( _auctionidList[_auctionIndex[_auctionid]]._info_highestbidder == address(0) , "Aleady biding");
        _;
    }

    mapping (uint256 => uint256) private _bidStatus;
    
    modifier nonBidReentrant (uint256 _auctionid) {

        _nonBidReentrantBefore(_auctionid);
        _;
        _nonBidReentrantAfter(_auctionid);

    }

    function _nonBidReentrantBefore (uint256 _auctionid) private {
        require (_bidStatus[_auctionid] != _ENTERED , "bidReentrancyGuard: reentrant call");

        _bidStatus[_auctionid] = _ENTERED;
    }

    function _nonBidReentrantAfter ( uint256 _auctionid ) private {
        
        _bidStatus[_auctionid] = _NOT_ENTERED;
    }

    function auctionListing(address _nftaddress , uint256 _tokenid , uint256 _startPrice , uint _auctionPeriod  ) public
        isAuctionStart(_nftaddress , _tokenid , _startPrice , _auctionPeriod )
    {
        
        auctionListing_info memory _info = auctionListing_info({
            _info_auctionId                         : _auctionIds.current() + 1,
            _info_seller                            : msg.sender,
            _info_nftAddress                        : _nftaddress,
            _info_tokenid                           : _tokenid,
            _info_startPrice                        : _startPrice,
            _info_highestbidder                     : address(0),
            _info_highestbid                        : 0, 
            _info_endTime                           : block.timestamp + 10 minutes,//block.timestamp + _auctionPeriod * 1 days,
            _info_auctionStatus                     : auctionStatus.auctioning
        });
        
        _auctionIndex[_info._info_auctionId] = _auctionidList.length;

        _auctionidList.push(_info);
        
        IERC721(_nftaddress).safeTransferFrom(msg.sender, address(this), _tokenid);

        _auctionIds.increment();

        emit auctionStartEvent ( _info );
    }

    function bid ( uint256 _auctionId , uint256 _bidPrice ) public 
        nonBidReentrant(_auctionId)
        isAuctionBid(_auctionId , _bidPrice )
    {
        auctionListing_info storage info = _auctionidList[_auctionIndex[_auctionId]];

        if (info._info_highestbidder != address(0)) {
            uint256 amount = _bidsAmount[info._info_highestbidder][info._info_auctionId];
            _bidsAmount[info._info_highestbidder][info._info_auctionId] = 0;
            _token.transfer(info._info_highestbidder, amount);
        }
                
        _token.transferFrom(msg.sender , address(this), _bidPrice);

        info._info_highestbidder = msg.sender;
        info._info_highestbid = _bidPrice;

        _bidsAmount[msg.sender][_auctionId] = _bidPrice;

        emit auctionBidEvent(info);

    }

    function end (uint256 _auctionId) public 
        isAuctionEnd(_auctionId)
    {
        
        auctionListing_info storage info = _auctionidList[_auctionIndex[_auctionId]];

        if (info._info_highestbidder == address(0)) {
            IERC721(info._info_nftAddress).safeTransferFrom(address(this), info._info_seller, info._info_tokenid);
        } else {
            
            uint256 amount = _bidsAmount[info._info_highestbidder][info._info_auctionId];
            _bidsAmount[info._info_highestbidder][info._info_auctionId] = 0;
            IERC721(info._info_nftAddress).safeTransferFrom(address(this), info._info_highestbidder, info._info_tokenid);
            
            _token.transfer(info._info_seller, amount);
        }

        info._info_auctionStatus = auctionStatus.auctionend;

        emit auctionEndEvent(_auctionId, info._info_seller, info._info_highestbidder, info._info_highestbid);
    }

    function auctionCancel( uint256 _auctionId) public 
        isAuctionCancel(_auctionId)
    {
        auctionListing_info storage info = _auctionidList[_auctionIndex[_auctionId]];

        info._info_auctionStatus = auctionStatus.auctioncancel;

        IERC721(info._info_nftAddress).safeTransferFrom(address(this) , msg.sender , info._info_tokenid);

        emit auctionCancelEvent( _auctionId , msg.sender );

    }

    function getAuctionListById( uint256 _auctionId ) public view returns (auctionListing_info memory  ) {

        if (_auctionidList.length == 0) {
            return auctionListing_info(0 ,  address(0) , address(0), 0 , 0, address(0) , 0 , 0 , auctionStatus.Non);
        }

        auctionListing_info memory info = _auctionidList[_auctionIndex[_auctionId]];

        if (info._info_auctionId != _auctionId) {

            return  auctionListing_info(0 ,  address(0) , address(0), 0 , 0, address(0) , 0 , 0 , auctionStatus.Non);
        }
        
        return info;
    } 

    function getAuctionEndTime( uint _auctionPeriod) private view returns (uint256 _auctionEndTime) {
        
        if ( _auctionPeriod == 1) {
            _auctionEndTime = block.timestamp + 30 minutes;
        } else if ( _auctionPeriod == 3) {
            _auctionEndTime = block.timestamp + 1 hours;    
        } else if ( _auctionPeriod == 7) {
            _auctionEndTime = block.timestamp + 1 days;    
        }

        return _auctionEndTime;
    }

    /* fixed sales start */

    Counters.Counter private _saleIds;

    mapping (uint256 => uint256) private _fixedPriceStatus;

    enum saleStatus { Non , saling , saled , salecancel} 

    struct marketListing_info {
        uint256 _info_saleId;
        address _info_seller;
        address _info_buyer;
        address _info_nftAddress;
        uint256 _info_tokenid;
        uint256 _info_price;
        uint256 _info_saledate;
        saleStatus _info_salestatus;
    }

    mapping (uint256 => uint256) internal _saleIndex;   
    marketListing_info[] internal _saleidList;

    event fixedsaleStartEvent(marketListing_info);
    event fixedsaleBuyEvent (uint256 saleId , address seller , address buyer , uint256 buyPrice);
    event fixedsaleCancelEvent (uint256 saleid , address seller);

    modifier isFixedSaleListing (address _nftaddress , uint256 _tokenId  , uint256 _price) {
        require (msg.sender == IERC721(_nftaddress).ownerOf(_tokenId) , "MSG.SENDER is not the nft owner");
        require (_price > 0 , "Price must be greater than 0");
        _;
    }

    modifier isFixedSaleBuy (uint256 _saleId , uint256 _price/* , uint256 _stableType*/) {
        require(_token.balanceOf(msg.sender) > _price , "msg.sender balance is less than bidPrice");
        require(_saleidList[_saleIndex[_saleId]]._info_price <= _price , "the price entered must be the same as the salling price");
        require(_saleidList[_saleIndex[_saleId]]._info_salestatus == saleStatus.saling , "saleId status is not saling");
        _;
    }

    modifier onlySeller (uint256 _saleId) {
        require ( _saleidList[_saleIndex[_saleId]]._info_seller == msg.sender  , "MSG.SENDER is not NFT Seller.");
        _;
    }

    function fixedSaleListing( address _nftaddress , uint256 _tokenId , uint256 _price ) public 
        isFixedSaleListing(_nftaddress , _tokenId , _price)
    {

        marketListing_info memory info = marketListing_info({
            _info_saleId     : _saleIds.current() + 1,
            _info_seller     : msg.sender,
            _info_buyer      : address(0),
            _info_nftAddress : _nftaddress,
            _info_tokenid    : _tokenId,
            _info_price      : _price,
            _info_saledate   : 0,
            _info_salestatus : saleStatus.saling
        });

        _saleIndex[info._info_saleId] = _saleidList.length;
        _saleidList.push(info);

        IERC721(_nftaddress).safeTransferFrom( msg.sender , address(this), _tokenId);
        
        _saleIds.increment();

        emit fixedsaleStartEvent(info);
    }

    function buyFixedSale (  uint256 _saleId , uint256 _price  ) public 
        isFixedSaleBuy(_saleId , _price )
    {
        marketListing_info storage info = _saleidList[_saleIndex[_saleId]];

        info._info_buyer = msg.sender;
        info._info_saledate = block.timestamp;
        info._info_salestatus = saleStatus.saled;

        IERC721(info._info_nftAddress).safeTransferFrom(address(this), msg.sender, info._info_tokenid);

        if ( _price > info._info_price) {
            _price = _price - info._info_price;
        }
        
        _token.transferFrom(msg.sender , info._info_seller , _price);

        emit fixedsaleBuyEvent(_saleId , info._info_seller , msg.sender , _price);

    }

    function cancelFixedSale (uint256 _saleId) public onlySeller(_saleId)
    {
        marketListing_info storage info = _saleidList[_saleIndex[_saleId]];

        IERC721(info._info_nftAddress).safeTransferFrom(address(this), msg.sender, info._info_tokenid);

        info._info_salestatus = saleStatus.salecancel;

        emit fixedsaleCancelEvent(_saleId , msg.sender);
    }


    function getMarketListByID (uint256 _saleid) public view returns(marketListing_info memory) {

        if (_saleidList.length == 0) {
            return marketListing_info(0 , address(0) , address(0) , address(0), 0 , 0 ,  0 , saleStatus.Non);
        }

        marketListing_info memory info = _saleidList[_saleIndex[_saleid]];

        if (info._info_saleId != _saleid) {
            return marketListing_info(0 , address(0) , address(0) , address(0), 0 , 0 ,  0 , saleStatus.Non);
        }

        return info;
    }

    
    /* fixed sales end */

    function setupEnder(address _ender , bool _enabled) public onlyRole(ADF_ADMIN_ROLE) {
        require(_ender != address(0) , "ender address can not be address(0)");

        if (_enabled) _setupRole(AUCTIONEND_ROLE, _ender);
        else _revokeRole(AUCTIONEND_ROLE, _ender);
    }

    function isEnder(address _ender) public view returns (bool) {
        return hasRole(AUCTIONEND_ROLE, _ender);
    }

    function onERC721Received(address , address , uint256 , bytes calldata ) public virtual override returns (bytes4) {
	
        return this.onERC721Received.selector;
    }


}