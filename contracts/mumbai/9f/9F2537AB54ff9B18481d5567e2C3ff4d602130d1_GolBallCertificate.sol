    ERC2981,
    ERC2771Context,
    ERC721Burnable
{
    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.UintSet;
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant LOGIC_ROLE = keccak256("LOGIC_ROLE");

    address private feeRecipient;
    string private generalURI;
    mapping(uint256 => string) private tokenMetadata;
    mapping(bytes32 => uint256) private golBallId;

    EnumerableSet.AddressSet private blackListedReceptorAddresses;
    EnumerableSet.AddressSet private blackListedSenderAddresses;

    // tokenId => addressTo => blacklisted
    mapping(uint256 => mapping(address => bool))
        private blackListTokenIdToReceptorAddress;
    // tokenId => addressFrom => blacklisted
    mapping(uint256 => mapping(address => bool))
        private blackListTokenIdFromSenderAddress;

    bool private paused;
    // tokenId => paused
    mapping(uint256 => bool) private pausedTokens;
    // tokenId => excluded_from_pause
    mapping(uint256 => bool) private excludedFromPause;
    EnumerableSet.UintSet private pausedTokenIds;

    // tokenId => delivered
    mapping(uint256 => bool) private tokenDelivered;

    bool private burnActivated = false;
    // tokenId => burnable
    mapping(uint256 => bool) private burnActivatedTokens;
    // tokenId => excluded from burn
    mapping(uint256 => bool) private excludedFromBurn;
    EnumerableSet.UintSet private excludedFromBurnTokenIds;

    event GolBallCertificateMinted(uint256 tokenId, address owner, string ref);

    modifier isMinted(uint256 tokenId) {
        require(_exists(tokenId), "tokenId does not exist");
        _;
    }

    modifier isNotBurnt(uint256 _tokenId) {
        if (_tokenId > 0) {
            address owner = address(0);
            try this.ownerOf(uint256(_tokenId)) returns (address _owner) {
                owner = _owner;
            } catch {
                revert("tokenId is already burnt or non existent");
            }
        }
        _;
    }

    constructor(
        MinimalForwarder _forwarder,
        string memory _name,
        string memory _symbol,
        address feeRecipient_,
        address minterAddress_,
        address adminAddress_,
        string memory generalURI_
    ) ERC721(_name, _symbol) ERC2771Context(address(_forwarder)) {
        feeRecipient = feeRecipient_;
        generalURI = generalURI_;

        // __ERC721_init(_name, _symbol);

        // __AccessControl_init();
        // __ERC721Burnable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, adminAddress_);
        _grantRole(MINTER_ROLE, minterAddress_);
        _grantRole(LOGIC_ROLE, adminAddress_);
    }

    // function initialize(
    //     string calldata _name,
    //     string calldata _symbol,
    //     address feeRecipient_,
    //     address minterAddress_,
    //     address adminAddress_,
    //     string calldata generalURI_
    // ) public virtual initializer {
    //     feeRecipient = feeRecipient_;
    //     generalURI = generalURI_;

    //     __ERC721_init(_name, _symbol);

    //     __AccessControl_init();
    //     __ERC721Burnable_init();

    //     _grantRole(DEFAULT_ADMIN_ROLE, adminAddress_);
    //     _grantRole(MINTER_ROLE, minterAddress_);
    //     _grantRole(LOGIC_ROLE, adminAddress_);
    // }

    /*
     * function to pause all transfers
     */
    function pause() public onlyRole(LOGIC_ROLE) {
        paused = true;
    }

    /*
     * function to unpause all transfers
     */
    function unpause() public onlyRole(LOGIC_ROLE) {
        paused = false;
    }

    /*
     * function to pause a specific tokenId
     */
    function pauseTokenId(
        uint256 _tokenId
    ) external isNotBurnt(_tokenId) onlyRole(LOGIC_ROLE) {
        require(!pausedTokens[_tokenId], "tokenId is already paused");
        pausedTokens[_tokenId] = true;
        excludedFromPause[_tokenId] = false;
        pausedTokenIds.add(_tokenId);
    }

    /*
     * function to unpause a specific tokenId transfer
     */
    function unpauseTokenId(
        uint256 _tokenId
    ) external isNotBurnt(_tokenId) onlyRole(LOGIC_ROLE) {
        require(pausedTokens[_tokenId], "tokenId is already unpaused");
        pausedTokens[_tokenId] = false;
        pausedTokenIds.remove(_tokenId);
    }

    /*
     * function to exclude a tokenId from general pause
     */
    function excludeFromPause(
        uint256 _tokenId
    ) external isNotBurnt(_tokenId) onlyRole(LOGIC_ROLE) {
        require(
            !excludedFromPause[_tokenId],
            "tokenId is already excluded from pause"
        );
        excludedFromPause[_tokenId] = true;
        pausedTokens[_tokenId] = false;
        pausedTokenIds.remove(_tokenId);
    }

    /*
     * function to remove the exclusion of a tokenId from general pause
     */
    function removeExclusionFromPause(
        uint256 _tokenId
    ) external isNotBurnt(_tokenId) onlyRole(LOGIC_ROLE) {
        require(
            excludedFromPause[_tokenId],
            "tokenId is not excluded from pause"
        );
        excludedFromPause[_tokenId] = false;
    }

    function isTokenIdPausedExcluded(
        uint256 _tokenId
    ) external view returns (bool) {
        return excludedFromPause[_tokenId];
    }

    function isTokenIdPaused(uint256 _tokenId) external view returns (bool) {
        return pausedTokens[_tokenId];
    }

    /*
     * function to allow every tokenId to be burnable
     */
    function activateBurn() external onlyRole(LOGIC_ROLE) {
        burnActivated = true;
    }

    /*
     * function to disallow every tokenId to be burnable
     */
    function deactivateBurn() external onlyRole(LOGIC_ROLE) {
        burnActivated = false;
    }

    /*
     * function to allow a specific tokenId to be burnt
     */
    function allowTokenIdBurnable(
        uint256 _tokenId
    ) external isNotBurnt(_tokenId) onlyRole(LOGIC_ROLE) {
        burnActivatedTokens[_tokenId] = true;
    }

    /*
     * function to disallow a specific tokenId to be burnt
     */
    function disallowTokenIdBurnable(
        uint256 _tokenId
    ) external isNotBurnt(_tokenId) onlyRole(LOGIC_ROLE) {
        require(burnActivatedTokens[_tokenId], "tokenId is not burnable");
        burnActivatedTokens[_tokenId] = false;
    }

    /*
     * function to exclude a tokenId from being burt when burn is activated for all
     */
    function excludeFromBurn(
        uint256 _tokenId
    ) external isNotBurnt(_tokenId) onlyRole(LOGIC_ROLE) {
        require(
            !excludedFromBurn[_tokenId],
            "tokenId is already excluded from burn"
        );
        excludedFromBurn[_tokenId] = true;
        excludedFromBurnTokenIds.add(_tokenId);
    }

    /*
     * function to remove the exclusion from burn for a tokenId
     */
    function removeExclusionFromBurn(
        uint256 _tokenId
    ) external isNotBurnt(_tokenId) onlyRole(LOGIC_ROLE) {
        excludedFromBurn[_tokenId] = false;
        excludedFromBurnTokenIds.remove(_tokenId);
    }

    function isTokenIdBurnable(uint256 _tokenId) external view returns (bool) {
        return burnActivatedTokens[_tokenId];
    }

    function isCollectionBurnable() external view returns (bool) {
        return burnActivated;
    }

    /*
     * function to blacklist addresses to avoid transfers
     * @param _toBlackListArray // array of addresses to blacklist
     * @param _tokenId // if it is greater than 0, it is a specific tokenId to blacklist, otherwise it is for all the collection
     * @param isReceptorAddress // true if it is a receptor address blacklist, otherwise is a sender address blacklist
     */
    function blacklistAddress(
        address[] calldata _toBlackListArray,
        uint256 _tokenId,
        bool isReceptorAddress
    ) external onlyRole(LOGIC_ROLE) isNotBurnt(_tokenId) {
        for (uint256 index = 0; index < _toBlackListArray.length; index++) {
            address toBlackList = _toBlackListArray[index];
            require(
                toBlackList != address(0),
                "blacklisting zero address is not possible"
            );

            if (_tokenId == 0) {
                if (isReceptorAddress) {
                    EnumerableSet.add(
                        blackListedReceptorAddresses,
                        toBlackList
                    );
                } else {
                    EnumerableSet.add(
                        blackListedSenderAddresses,
                        toBlackList
                    );
                }
            } else {
                if (isReceptorAddress) {
                    blackListTokenIdToReceptorAddress[_tokenId][
                        toBlackList
                    ] = true;
                } else {
                    blackListTokenIdFromSenderAddress[_tokenId][
                        toBlackList
                    ] = true;
                }
            }
        }
    }

    /*
     * function to remove addresses from blacklist to avoid transfers
     * @param _toBlackListArray // array of addresses to blacklist
     * @param _tokenId // if it is greater than 0, it is a specific tokenId to remove from blacklist, otherwise it is for all the collection
     * @param isReceptorAddress // true is a receptor address blacklist otherwise is a sender address blacklist
     */
    function removeAddressFromBlacklist(
        address[] calldata _toBlackListArray,
        uint256 _tokenId,
        bool isReceptorAddress
    ) external isNotBurnt(_tokenId) onlyRole(LOGIC_ROLE) {
        for (uint256 index = 0; index < _toBlackListArray.length; index++) {
            address toUnblackList = _toBlackListArray[index];
            require(
                toUnblackList != address(0),
                "removing from blacklist zero address is not possible"
            );

            if (_tokenId == 0) {
                if (isReceptorAddress) {
                    EnumerableSet.remove(
                        blackListedReceptorAddresses,
                        toUnblackList
                    );
                } else {
                    EnumerableSet.remove(
                        blackListedSenderAddresses,
                        toUnblackList
                    );
                }
            } else {
                if (isReceptorAddress) {
                    blackListTokenIdToReceptorAddress[_tokenId][
                        toUnblackList
                    ] = false;
                } else {
                    blackListTokenIdFromSenderAddress[_tokenId][
                        toUnblackList
                    ] = false;
                }
            }
        }
    }

    // function to check if receptor address is blacklisted for every token
    function isReceptorAddressBlacklistedForAll(
        address _address
    ) public view returns (bool) {
        return
            EnumerableSet.contains(
                blackListedReceptorAddresses,
                _address
            );
    }

    // function to check if a receptor address is blacklisted for a specific token
    function isReceptorAddressBlacklistedForAToken(
        uint256 _tokenId,
        address _addressToBlacklist
    ) external view returns (bool) {
        return blackListTokenIdToReceptorAddress[_tokenId][_addressToBlacklist];
    }

    // function to check if sender address is blacklisted for every token
    function isSenderAddressBlacklistedForAll(
        address _address
    ) public view returns (bool) {
        return
            EnumerableSet.contains(
                blackListedSenderAddresses,
                _address
            );
    }

    // function to check if a sender address is blacklisted for a specific token
    function isSenderAddressBlacklistedForAToken(
        uint256 _tokenId,
        address _addressToBlacklist
    ) external view returns (bool) {
        return blackListTokenIdFromSenderAddress[_tokenId][_addressToBlacklist];
    }

    /*
     * function to set the state of an already delivered gol-ball
     */
    function setTokenDelivered(
        uint256 _tokenId
    ) external isNotBurnt(_tokenId) onlyRole(LOGIC_ROLE) {
        tokenDelivered[_tokenId] = true;
    }

    /*
     * function to check if the state of the nft is delivered
     */
    function isTokenDelivered(uint256 _tokenId) external view returns (bool) {
        return tokenDelivered[_tokenId];
    }

    function mint(
        address to,
        string memory metadataURL,
        string memory ref,
        bool _tokenDelivered
    ) external onlyRole(MINTER_ROLE) returns (uint256) {
        require(bytes(ref).length > 0, "ref cannot be empty");
        require(
            golBallId[keccak256(bytes(ref))] == 0,
            "product ID already exists"
        );
        uint256 tokenId = totalSupply() + 1;
        _safeMint(to, tokenId);
        tokenMetadata[tokenId] = metadataURL;
        golBallId[keccak256(bytes(ref))] = tokenId;
        if (_tokenDelivered) {
            tokenDelivered[tokenId] = true;
        }
        emit GolBallCertificateMinted(tokenId, to, ref);
        return tokenId;
    }

    function tokenURI(
        uint256 tokenId
    ) public view virtual override isMinted(tokenId) returns (string memory) {
        return tokenMetadata[tokenId];
    }

    function contractURI() public view returns (string memory) {
        return generalURI;
    }

    function tokenIdByRef(string memory ref) external view returns (uint256) {
        bytes32 golBallId_ref = keccak256(bytes(ref));
        require(
            golBallId[golBallId_ref] > 0 &&
                golBallId[golBallId_ref] <= totalSupply(),
            "product ID does not exist"
        );
        return golBallId[golBallId_ref];
    }

    function setTokenMetadata(
        uint256 tokenId,
        string memory metadataURL
    ) external onlyRole(DEFAULT_ADMIN_ROLE) isMinted(tokenId) {
        tokenMetadata[tokenId] = metadataURL;
    }

    function setRoyalties(
        address receiver,
        uint96 feeNumerator
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _setDefaultRoyalty(receiver, feeNumerator);
    }

    function setGeneralURI(
        string memory generalURI_
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        generalURI = generalURI_;
    }

    function setFeeRecipient(
        address feeRecipient_
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        feeRecipient = feeRecipient_;
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        virtual
        override(
            ERC721,
            ERC2981,
            ERC721Enumerable,
            AccessControlEnumerable
        )
        returns (bool)
    {
        return
            interfaceId == type(IERC2981).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function _beforeTokenTransfer(
        address _from,
        address _to,
        uint256 _tokenId,
        uint256 _batchSize
    )
        internal
        virtual
        override(ERC721Enumerable, ERC721)
    {
        super._beforeTokenTransfer(_from, _to, _tokenId, _batchSize);

        if (_to != address(0)) {
            require(
                !EnumerableSet.contains(
                    blackListedReceptorAddresses,
                    _to
                ),
                "receptor address is blacklisted"
            );
            require(
                !blackListTokenIdToReceptorAddress[_tokenId][_to],
                "receptor address is blacklisted for this tokenId"
            );
            require(
                !EnumerableSet.contains(
                    blackListedSenderAddresses,
                    _from
                ),
                "sender address is blacklisted"
            );
            require(
                !blackListTokenIdFromSenderAddress[_tokenId][_from],
                "sender address is blacklisted for this tokenId"
            );

            if (paused) {
                if (!excludedFromPause[_tokenId]) {
                    revert("all tokens are paused");
                }
            } else {
                require(!pausedTokens[_tokenId], "tokenId paused");
            }
        }
    }

    function _msgSender()
        internal
        view
        override(Context, ERC2771Context)
        returns (address sender)
    {
        sender = ERC2771Context._msgSender();
    }

    function _msgData()
        internal
        view
        virtual
        override(Context, ERC2771Context)
        returns (bytes calldata)
    {
        return ERC2771Context._msgData();
    }

    /*
     * override original burn function to check on burnable options
     */
    function burn(uint256 _tokenId) public override {
        require(
            !excludedFromBurn[_tokenId],
            "tokenId can not be burned because it is excluded"
        );
        if (!burnActivated) {
            if (!burnActivatedTokens[_tokenId]) {
                revert("tokenId is not activated to be burned");
            }
        }
        cleanBeforeBurn(_tokenId);
        super.burn(_tokenId);
    }

    function getPausedTokens() external view returns (uint256[] memory) {
        return pausedTokenIds.values();
    }

    function getExcludedFromBurnTokenIds()
        external
        view
        returns (uint256[] memory)
    {
        return excludedFromBurnTokenIds.values();
    }

    function cleanBeforeBurn(uint256 _tokenId) internal {
        burnActivatedTokens[_tokenId] = false;
        excludedFromBurn[_tokenId] = false;
        excludedFromPause[_tokenId] = false;
        pausedTokens[_tokenId] = false;
        pausedTokenIds.remove(_tokenId);
    }

    function isNftPaused() external view returns (bool) {
        return paused;
    }
}

