pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/cryptography/EIP712.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

// TODO https://docs.opengsn.org/javascript-client/devops.html

/*
    The forwarder allows for executing transactions on behalf of another address 
    while still retaining the original sender's address as the signer of the transaction. 
    The contract is based on the Ethereum Improvement Proposal (EIP) 2771, 
    which defines a standard for gasless meta-transactions.
*/
contract MinimalForwarder is EIP712, Ownable {
    using ECDSA for bytes32;

    /*
        Contains the necessary information for executing a transaction.
    */
    struct ForwardRequest {
        address from;
        address to;
        uint256 value;
        uint256 gas;
        uint256 nonce;
        bytes data;
    }

    /*
        This hash is used to generate a typed data hash, 
        which is then signed by the original sender of the transaction
    */
    bytes32 private constant _TYPEHASH =
        keccak256(
            "ForwardRequest(address from,address to,uint256 value,uint256 gas,uint256 nonce,bytes data)"
        );

    mapping(address => uint256) private _nonces;
    mapping(address => bool) private _executors;

    modifier isExecutor(address executor) {
        require(_executors[executor], "sender must be a valid executor");
        _;
    }

    constructor() EIP712("MinimalForwarder", "0.0.1") {}

    function addExecutor(address executor) external onlyOwner {
        _executors[executor] = true;
    }

    function removeExecutor(address executor) external onlyOwner {
        _executors[executor] = false;
    }

    function checkExecutor(
        address executor
    ) external view onlyOwner returns (bool) {
        return _executors[executor];
    }

    function getNonce(address from) public view returns (uint256) {
        return _nonces[from];
    }

    function verify(
        ForwardRequest calldata req,
        bytes calldata signature
    ) public view returns (bool) {
        address signer = _hashTypedDataV4(
            keccak256(
                abi.encode(
                    _TYPEHASH,
                    req.from,
                    req.to,
                    req.value,
                    req.gas,
                    req.nonce,
                    keccak256(req.data)
                )
            )
        ).recover(signature);
        return _nonces[req.from] == req.nonce && signer == req.from;
    }

    function execute(
        ForwardRequest calldata req,
        bytes calldata signature
    ) public payable isExecutor(msg.sender) returns (bool, bytes memory) {
        require(
            verify(req, signature),
            "MinimalForwarder: signature does not match request"
        );
        _nonces[req.from] = req.nonce + 1;

        (bool success, bytes memory returndata) = req.to.call{
            gas: req.gas,
            value: req.value
        }(abi.encodePacked(req.data, req.from));

        if (gasleft() <= req.gas / 63) {
            /// @solidity memory-safe-assembly
            assembly {
                invalid()
            }
        }

        return (success, returndata);
    }
}