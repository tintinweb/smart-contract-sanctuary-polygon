pragma solidity ^0.8.0;

import {Factory} from "channels/v1/utils/Factory.sol";
import {IRegistry} from "channels/v1/Registry.i.sol";
import {AccessControl} from "@openzeppelin/contracts/access/AccessControl.sol";

contract Registry is AccessControl, IRegistry {
    mapping(bytes32 => mapping(address => bool)) public isDeployment;
    mapping(bytes32 => Factory[]) internal factories;
    mapping(bytes32 => mapping(address => bool)) public isFactory;

    constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    modifier onlyFactory(bytes32 factoryType) {
        if (!isFactory[factoryType][msg.sender]) revert NotFactory();
        _;
    }

    function addFactory(bytes32 name_, Factory factory_)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        factories[name_].push(factory_);
        uint256 version = factory_.contractVersion();
        if (factories[name_].length != version) revert WrongVersion();

        isFactory[name_][address(factory_)] = true;

        emit FactoryAdded(name_, version, address(factory_));
    }

    function getFactory(bytes32 name, uint256 version)
        public
        view
        returns (Factory factory)
    {
        Factory[] memory _factories = factories[name];
        if (_factories.length == 0) {
            return Factory(address(0));
        }
        return _factories[version - 1];
    }

    function latestFactoryVersion(bytes32 name) public view returns (uint256) {
        return factories[name].length;
    }

    /**
     * @notice Register contract deployment
     * @dev Contract type and factory identifier will be the same. i.e. keccak256(CHANNEL)
     * @param contractType  Type of deployment, e.g. keccak256(CHANNEL)
     * @param deployment    Contract address to register
     */
    function addDeployment(bytes32 contractType, address deployment)
        public
        onlyFactory(contractType)
    {
        isDeployment[contractType][deployment] = true;

        emit DeploymentAdded(contractType, deployment);
    }
}

