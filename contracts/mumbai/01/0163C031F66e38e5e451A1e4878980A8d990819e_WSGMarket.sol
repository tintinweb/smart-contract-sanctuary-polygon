pragma solidity ^0.8.4;

import "./openzeppelin/token/ERC1155/ERC1155.sol";
import "./openzeppelin/utils/Counters.sol";
import "./openzeppelin/access/Ownable.sol";
import "./openzeppelin/token/ERC1155/extensions/ERC1155Supply.sol";

contract WSGPrint is Context, ERC1155, Ownable, ERC1155Supply {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIdCounter;
    address private _marketplaceAddress;

    // Check if copy id is already created
    mapping(uint256 => mapping(uint256 => bool)) private _trackCopyNum;

    constructor(
        address marketplaceAddress_
    ) ERC1155("https://wsgallery.com/blocks/{id}.json") {
        _marketplaceAddress = marketplaceAddress_;
    }

    function setURI(string memory newuri) public onlyOwner {
        _setURI(newuri);
    }

    function getCurrentTokenId() external view returns (uint256) {
        return _tokenIdCounter.current();
    }

    function mint(address account, uint256 amount) public {
        _tokenIdCounter.increment();
        uint256 id = _tokenIdCounter.current();

        setApprovalForAll(_marketplaceAddress, true);

        // TODO: move this to maret
        // require(!_trackCopyNum[id][copyNum], "Copy already created!");
        // _trackCopyNum[id][copyNum] = true; // Track copy number
        // require(balanceOf(account, id) == 0, "Already minted!");

        _mint(account, id, amount, "");
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155, ERC1155Supply) {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }
}

