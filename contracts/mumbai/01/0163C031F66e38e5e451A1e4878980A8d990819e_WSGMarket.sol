pragma solidity ^0.8.4;

import "./openzeppelin/token/ERC721/ERC721.sol";
import "./openzeppelin/token/ERC1155/IERC1155.sol";
import "./openzeppelin/token/ERC1155/utils/ERC1155Holder.sol";
import "./openzeppelin/security/ReentrancyGuard.sol";
import "./openzeppelin/utils/Counters.sol";
import "./openzeppelin/access/Ownable.sol";
import "./token/WSGCoin.sol";
import "./WSGPrint.sol";

contract WSGMarket is ERC1155Holder, Context, ReentrancyGuard, Ownable {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    Counters.Counter private _tokensSold;

    address private _nftContractAddress;
    address private _printContractAddress;
    uint256 private _listingFee = 1 ether;
    IERC20 private _wsgc;

    enum TokenTypes {
        NFT,
        PRINT
    }

    struct MarketToken {
        uint256 itemId;
        uint256 tokenId;
        address payable seller;
        address payable owner;
        uint256 price;
        bool sold;
        TokenTypes tokenType;
        uint256 copyNum; // For nft copyNum is always 1
    }

    mapping(uint256 => MarketToken) private _idToMarketToken;

    // For print tokens - check if copy id is already created
    mapping(uint256 => mapping(uint256 => bool)) private _trackCopyNum;

    // itemId is nonce
    // tokenId is nft id
    event MarketTokenInfo(
        uint256 indexed itemId,
        uint256 indexed tokenId,
        address seller,
        address owner,
        uint256 price,
        bool sold,
        TokenTypes tokenType,
        uint256 copyNum
    );

    constructor(address wsgcAddress_) {
        _wsgc = WSGCoin(wsgcAddress_);
    }

    modifier hasNFTContractAddress() {
        require(_nftContractAddress != address(0), "NFT contract required");
        _;
    }

    modifier hasPrintContractAddress() {
        require(_printContractAddress != address(0), "Print contract required");
        _;
    }

    function setupNFTContractAddress(address nftContractAddress_) external {
        _nftContractAddress = nftContractAddress_;
    }

    function setupPrintContractAddress(address printContractAddress_) external {
        _printContractAddress = printContractAddress_;
    }

    function getListingFee() external view returns (uint256) {
        return _listingFee;
    }

    function setListingFee(uint256 listingFee) external onlyOwner {
        _listingFee = listingFee;
    }

    function getTokenIdFromItemId(
        uint256 itemId
    ) public view returns (uint256) {
        return _idToMarketToken[itemId].tokenId;
    }

    function createMarketToken(
        uint256 tokenId,
        uint256 price
    ) external payable nonReentrant hasNFTContractAddress {
        // nonReentrant is a modifier to prevent reentry attack
        require(price > 0, "Price less than one");
        require(msg.value == _listingFee, "Incorrect listing fee");

        _tokenIds.increment();
        uint256 itemId = _tokenIds.current();

        // putting it up for sale - bool - no owner
        _idToMarketToken[itemId] = MarketToken(
            itemId,
            tokenId,
            payable(_msgSender()),
            payable(address(0)),
            price,
            false,
            TokenTypes.NFT,
            1
        );

        // NFT transaction
        IERC721(_nftContractAddress).transferFrom(
            _msgSender(),
            address(this),
            tokenId
        );

        emit MarketTokenInfo(
            itemId,
            tokenId,
            _msgSender(),
            address(0),
            price,
            false,
            TokenTypes.NFT,
            1
        );
    }

    // Create token one by one with different price for each print copy
    function createPrintToken(
        uint256 id,
        uint256 copyNum,
        uint256 price
    ) external payable nonReentrant hasPrintContractAddress {
        // nonReentrant is a modifier to prevent reentry attack
        require(price > 0, "Price less than one");
        require(!_trackCopyNum[id][copyNum], "Copy already created!");

        _tokenIds.increment();
        uint256 itemId = _tokenIds.current();

        // putting it up for sale - bool - no owner
        _idToMarketToken[itemId] = MarketToken(
            itemId,
            id,
            payable(_msgSender()),
            payable(address(0)),
            price,
            false,
            TokenTypes.NFT,
            copyNum
        );

        // transaction
        IERC1155(_printContractAddress).safeTransferFrom(
            _msgSender(),
            address(this),
            id,
            1,
            ""
        );

        _trackCopyNum[id][copyNum] = true; // Track copy number

        emit MarketTokenInfo(
            itemId,
            id,
            _msgSender(),
            address(0),
            price,
            false,
            TokenTypes.PRINT,
            copyNum
        );
    }

    // Same price for all copies
    // function createBatchPrintToken(uint256 tokenId, uint256 price)
    //     external
    //     payable
    //     nonReentrant
    //     hasPrintContractAddress
    // { }

    function purchaseMarketToken(
        uint256 itemId
    ) external payable nonReentrant hasNFTContractAddress {
        uint256 price = _idToMarketToken[itemId].price;
        uint256 tokenId = _idToMarketToken[itemId].tokenId;
        require(msg.value == price, "Please submit the asking price");

        _idToMarketToken[itemId].owner = payable(_msgSender());
        _idToMarketToken[itemId].sold = true;
        _tokensSold.increment();

        // transfer the amount to the seller
        _wsgc.transferFrom(
            _msgSender(),
            _idToMarketToken[itemId].seller,
            price
        );

        // transfer the token from contract address to the buyer
        IERC721(_nftContractAddress).transferFrom(
            address(this),
            _msgSender(),
            tokenId
        );

        // buyer (owner) pays listing price to wsg market
        _wsgc.transferFrom(_msgSender(), address(this), _listingFee);

        emit MarketTokenInfo(
            itemId, // itemId
            tokenId,
            _idToMarketToken[itemId].seller, // seller
            _msgSender(), // owner
            price,
            _idToMarketToken[itemId].sold, // sold
            TokenTypes.NFT,
            1
        );
    }

    function transferItem(uint256 itemId, address buyer) public {
        uint256 tokenId = _idToMarketToken[itemId].tokenId;

        // transfer the token from contract address to the buyer
        IERC721(_nftContractAddress).transferFrom(
            address(this),
            buyer,
            tokenId
        );
    }

    // function for returning an array of created nfts for sales
    // owner == address(0)
    function fetchTokensForSales()
        external
        view
        returns (MarketToken[] memory)
    {
        uint256 itemCount = _tokenIds.current();
        uint256 unsoldItemCount = _tokenIds.current() - _tokensSold.current();
        uint256 currentIndex = 0;

        // looping over the number of items created (if number has not been sold populate the array)
        MarketToken[] memory items = new MarketToken[](unsoldItemCount);

        for (uint256 i = 1; i <= itemCount; i++) {
            if (_idToMarketToken[i].owner == address(0)) {
                MarketToken memory currentItem = _idToMarketToken[i];
                items[currentIndex] = currentItem;
                currentIndex += 1;
            }
        }

        return items;
    }

    // function for returning an array of minted nfts
    // seller == _msgSender
    function fetchTokensCreated() external view returns (MarketToken[] memory) {
        uint256 totalItemCount = _tokenIds.current();
        uint256 itemCount = 0;
        uint256 currentIndex = 0;

        // instead of .owner it will be the .seller
        for (uint256 i = 1; i <= totalItemCount; i++) {
            if (_idToMarketToken[i].seller == _msgSender()) {
                itemCount += 1;
            }
        }

        // second loop to loop through the amount you have purchased with itemcount
        // check to see if the owner address is equal to _msgSender()
        MarketToken[] memory items = new MarketToken[](itemCount);
        for (uint256 i = 1; i <= totalItemCount; i++) {
            if (_idToMarketToken[i].seller == _msgSender()) {
                uint256 currentId = _idToMarketToken[i].itemId;
                MarketToken storage currentItem = _idToMarketToken[currentId];
                items[currentIndex] = currentItem;
                currentIndex += 1;
            }
        }

        return items;
    }

    // owner == _msgSender
    function fetchMyNFTs() external view returns (MarketToken[] memory) {
        uint256 totalItemCount = _tokenIds.current();
        uint256 itemCount = 0;
        uint256 currentIndex = 0;

        for (uint256 i = 1; i <= totalItemCount; i++) {
            if (_idToMarketToken[i].owner == _msgSender()) {
                itemCount += 1;
            }
        }

        // second loop to loop through the amount you have purchased with itemcount
        // check to see if the owner address is equal to _msgSender()
        MarketToken[] memory items = new MarketToken[](itemCount);
        for (uint256 i = 1; i <= totalItemCount; i++) {
            if (_idToMarketToken[i].owner == _msgSender()) {
                uint256 currentId = _idToMarketToken[i].itemId;
                // current array
                MarketToken storage currentItem = _idToMarketToken[currentId];
                items[currentIndex] = currentItem;
                currentIndex += 1;
            }
        }
        return items;
    }
}

