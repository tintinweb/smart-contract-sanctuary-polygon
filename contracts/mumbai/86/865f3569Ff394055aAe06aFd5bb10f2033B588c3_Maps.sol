// TODO: Eval putting constants in a separate file that is imported/shared
uint constant BOARD_SIZE = 11;
uint constant VENT_CHANCE = 33;
uint constant WINDOW_CHANCE = 66;
uint constant DOOR_CHANCE = 66;
uint constant DOOR_MOD_STEP = 22;
uint8 constant MAX_DOOR_STR = 3;

uint constant START_ROOM_ID = 2; // TODO: Possibility of more than one start room?
uint8 constant UNDISCOVERED_ROOM = 1;
uint8 constant SPACE_ROOM = 0;

interface RoomTileInterface {
    function getNumRoomTiles() external view returns(uint256);
    function extGetRoomTile(uint _id) external view returns(BCTypes.RoomTile memory);
}

interface UtilsInterface {
    function smallIntBetweenVals(uint8 _min, uint8 _max) external returns (uint8);
    function getDirectionBetween(BCTypes.Position memory _firstRoom, BCTypes.Position memory _secondRoom) external pure returns(uint);
    function getNeighborRoom(uint _direction, BCTypes.Position memory _start) external pure returns (BCTypes.Position memory);
    function opposite(uint _direction) external pure returns (uint);
}

contract Maps is Ownable {

    address public game;
    UtilsInterface public utils;
    RoomTileInterface public roomTiles;

    address public actionsAddress;
    address public playersAddress;

    function setContractAddresses(
        address _gameAddress,
        address _utilsAddress,
        address _roomTilesAddress,
        address _actionsAddress,
        address _playersAddress
        ) public onlyOwner {
        game = _gameAddress;
        utils = UtilsInterface(_utilsAddress);
        roomTiles = RoomTileInterface(_roomTilesAddress);
        actionsAddress = _actionsAddress;
        playersAddress = _playersAddress;
    }

    // TODO: This array is here just to support making an empty one to put in the mapping
    BCTypes.GameTile[] private gameTiles;
    BCTypes.Map[] public maps;

    // TODO: System for options like static start and debug maps for testing
    function createMap() public onlyGame returns (uint) {
        BCTypes.Map storage newMap = maps.push();

        // Get the room list and save the number of rooms
        newMap.numRooms = uint8(_setGameRoomTiles(newMap.roomList));

        // ADD a door for walls (this will be default!)
        newMap.doors[newMap.numDoors] = BCTypes.Door(
            255,
            255, // TODO: Hack to make them invincible??
            BCTypes.DoorStatus.NO_DOOR
        );
        newMap.numDoors++;

        _staticStart(newMap);
        _buildMap(newMap);
        _addWindows(newMap);

        // Return the map id for the games contract
        return maps.length-1;
    }

    // TODO: Is there any reason NOT to just always place start bottom center???
    function _staticStart(BCTypes.Map storage _newMap) internal {
        BCTypes.GameTile storage startTile = gameTiles.push(); // TODO: Can I make an empty one and reference it this way without the array?

        uint startRow = BOARD_SIZE-1;
        uint startCol = (BOARD_SIZE/2)+1;

        _newMap.startPosition = BCTypes.Position(uint8(startRow), uint8(startCol));
        _newMap.board[startRow][startCol] = startTile;
    }

    function _addWindows(BCTypes.Map storage _newMap) internal {
        for(uint row = 0; row < BOARD_SIZE; row++){
            for (uint col = 0; col < BOARD_SIZE; col++) {
                if(_newMap.board[row][col].roomId == UNDISCOVERED_ROOM) {
                    for (uint i = 0; i < 4; i++) {
                        // Position memory neighbor = utils.getNeighborRoom(i, Position(uint8(row), uint8(col)));
                        if (_newMap.board[row][col].doors[i] == 0) {
                                BCTypes.Position memory neighborPos = utils.getNeighborRoom(i, BCTypes.Position(uint8(row), uint8(col)));
                                uint neighborId = _newMap.board[neighborPos.row][neighborPos.col].roomId;
                                if (utils.smallIntBetweenVals(0, 99) < WINDOW_CHANCE && neighborId == SPACE_ROOM) {
                                    _newMap.doors[_newMap.numDoors] = BCTypes.Door(
                                    255,
                                    255,
                                    BCTypes.DoorStatus.WINDOW);
                                    _newMap.numDoors++;
                                    _newMap.board[row][col].doors[i] = _newMap.numDoors-1; // -1 for correct index
                                    // TODO: Conform that neighbor doesn't need this door/window
                                }
                            }
                    }
                }
            }
        }
    }

    function _buildMap(BCTypes.Map storage _newMap) internal {
        uint stackCount = 0;
        uint numToPlace = _newMap.numRooms - 2; // -1 for space and -1 for empty room

        uint availableDoors = 1; // Start with 1 because tracking is done in the pop current phase

        uint doorModifier = 0;

        bool[BOARD_SIZE][BOARD_SIZE] memory found;
        found[_newMap.startPosition.row][_newMap.startPosition.col] = true;

        BCTypes.Position[30] memory stack; // TODO: Write a stack library? // TODO: This can probably be 27
        stack[stackCount] = _newMap.startPosition; // Start by enstacking the Donghaijiu location
        stackCount++;

        while (numToPlace > 0 && stackCount > 0) { // TODO: Need to track stack count to avoid crash on error if all rooms not placed
            // Pop the top item from the stack
            BCTypes.Position memory current = stack[stackCount-1];
            stackCount--;
            delete stack[stackCount]; // TODO: Confirm utility of deleting

            numToPlace--;
            availableDoors--;

            _newMap.board[current.row][current.col].roomId = UNDISCOVERED_ROOM;

            // Add vent
            if (utils.smallIntBetweenVals(0, 99) < VENT_CHANCE) {
                _newMap.board[current.row][current.col].hasVent = true;
            }

            // Also ugly but fighting stack limit
            uint[4] memory order = [uint(0), 1, 2, 3];

            // Fischer-Yates shuffle to avoid direction bias in room placement
            for (uint i = order.length-1; i > 0; i--) {
                uint8 k = utils.smallIntBetweenVals(0, uint8(i));

                (order[i], order[k]) = (order[k], order[i]);
            }

            for (uint i = 0; i < order.length; i++) {
                int newRow = int8(current.row);
                int newCol = int8(current.col);

                if(order[i] == 0) {
                    newRow--;
                } else if (order[i] == 1) {
                    newRow++;
                } else if (order[i] == 2) {
                    newCol++;
                } else if (order[i] == 3) {
                    newCol--;
                }

                // Stay in bounds, reserving outside edge for Donghaijiu only
                if (newRow < 1 ||
                    newCol < 1 ||
                    newRow > int(BOARD_SIZE-2) ||
                    newCol > int(BOARD_SIZE-2)) {
                        continue;
                }
                BCTypes.Position memory neighbor = BCTypes.Position(uint8(int8(newRow)), uint8(int8(newCol)));
                                                                    // Hideous but hashtag #stacklimit
                if (found[neighbor.row][neighbor.col] == false || _newMap.board[current.row][current.col].doors[utils.getDirectionBetween(current, neighbor)] == 0) {
                    if (availableDoors == 0 || utils.smallIntBetweenVals(0, 99) < DOOR_CHANCE - doorModifier) {
                        _newMap.doors[_newMap.numDoors] = BCTypes.Door(
                        utils.smallIntBetweenVals(0, MAX_DOOR_STR),
                        utils.smallIntBetweenVals(0, MAX_DOOR_STR),
                        BCTypes.DoorStatus.CLOSED);
                        // Pre-breach doors with strength 0
                        if (_newMap.doors[_newMap.numDoors].vsBreach == 0) {
                            _newMap.doors[_newMap.numDoors].status = BCTypes.DoorStatus.BREACHED;
                        }
                        _newMap.numDoors++;

                        doorModifier += DOOR_MOD_STEP;
                        // Prevent negative uint in DOOR_CHANCE - doorModifier
                        if (doorModifier > DOOR_CHANCE) {
                            doorModifier = DOOR_CHANCE;
                        }

                        // Only enstack if we haven't found this one yet
                        // Otherwise it will overwrite existing tiles
                        if (found[neighbor.row][neighbor.col] == false) {
                            stack[stackCount] = neighbor;
                            stackCount++;
                            availableDoors++;

                            found[neighbor.row][neighbor.col] = true;
                        }

                        // Add the door if one isn't present already
                        // TODO: Check may be unnecessary
                        if(_newMap.board[current.row][current.col].doors[order[i]] == 0) {
                            _newMap.board[current.row][current.col].doors[order[i]] = _newMap.numDoors-1;
                            _newMap.board[neighbor.row][neighbor.col].doors[utils.opposite(order[i])] = _newMap.numDoors-1;
                        }
                    } else {
                        doorModifier = 0;
                    }
                }

            }
            // Detect mapgen stuck and reset by adding a new valid room in the stack
        }
        // Cleanup Tasks

        // TODO Detect and prevent failure to deploy all tiles

        // require(numToPlace == 0, "Error: Bad mapgen.  Unplaced rooms");

        // Reset Donghaijiu ID (Was changed by map building)
        _newMap.board[_newMap.startPosition.row][_newMap.startPosition.col].roomId = uint8(START_ROOM_ID);
        _newMap.board[_newMap.startPosition.row][_newMap.startPosition.col].hasVent = false;
        // Set the first door placed to breached, this is the entrance
        _newMap.doors[1].status = BCTypes.DoorStatus.BREACHED;

        // TODO: Add signature detected
    }

    function _setGameRoomTiles(mapping (uint => BCTypes.RoomTile) storage _roomList) internal returns (uint) {
        // Doing loop because can't set nested mapping all at once
        uint number = roomTiles.getNumRoomTiles();
        for(uint i = 0; i < number; i++) {
            _roomList[i] = roomTiles.extGetRoomTile(i);
        }
        return number;
    }

    function extGetBoard(uint _mapId) public view returns (BCTypes.GameTile[][] memory) {
        BCTypes.GameTile[][] memory board = new BCTypes.GameTile[][](BOARD_SIZE);
        for (uint i = 0; i < BOARD_SIZE; i++) {
            board[i] = new BCTypes.GameTile[](BOARD_SIZE);
        }

        for (uint row = 0; row < BOARD_SIZE; row++) {
            for (uint col = 0; col < BOARD_SIZE; col++) {
                board[row][col] = maps[_mapId].board[row][col];
            }
        }

        return board;
    }

    function extGetGameTile(uint _mapId, uint _row, uint _col) public view returns (BCTypes.GameTile memory) {
        return maps[_mapId].board[_row][_col];
    }

    function extGetRoomList(uint _mapId) public view returns (BCTypes.RoomTile[] memory) {
        BCTypes.RoomTile[] memory roomList = new BCTypes.RoomTile[](maps[_mapId].numRooms);
        for (uint i = 0; i < roomList.length; i++) {
            roomList[i] = maps[_mapId].roomList[i];
        }
        return roomList;
    }

    function extGetRoom(uint _mapId, uint _row, uint _col) public view returns(BCTypes.RoomTile memory) {
        return maps[_mapId].roomList[maps[_mapId].board[_row][_col].roomId];
    }

    function extGetRoomById(uint _mapId, uint _roomId) public view returns (BCTypes.RoomTile memory) {
        return maps[_mapId].roomList[_roomId];
    }

    function extGetDoors(uint _mapId) public view returns (BCTypes.Door[] memory) {
        BCTypes.Door[] memory doors = new BCTypes.Door[](maps[_mapId].numDoors);
        for (uint i = 0; i < doors.length; i++) {
            doors[i] = maps[_mapId].doors[i];
        }

        return doors;
    }

    // TODO:  These need to validate that this is the right version of BCMaps
    function getDoor(uint _mapId, uint _row, uint _col, uint _direction) public view returns (BCTypes.Door memory){
        uint[4] memory currentRoomDoors = maps[_mapId].board[_row][_col].doors;
        return maps[_mapId].doors[currentRoomDoors[_direction]];
    }
                                                                                                                // 0 -> no change
    function setDoorFromPos(uint _mapId, uint _row, uint _col, uint _direction, BCTypes.DoorStatus _status, uint _strength) public {
        require((msg.sender == game || msg.sender == actionsAddress), "Not allowed contract");
        uint[4] memory currentRoomDoors = maps[_mapId].board[_row][_col].doors;
        BCTypes.Door storage currentDoor = maps[_mapId].doors[currentRoomDoors[_direction]];
        currentDoor.status = _status;
        if(_strength > 0) {
            currentDoor.vsHack = uint8(_strength);
            currentDoor.vsBreach = uint8(_strength);
        }
    }
    // TODO: Below is better, but having trouble with contract size
    // See ~line 265 in BCPlayers, effect for locking doors
    // function lockDownRoom(uint _mapId, uint _row, uint _col, uint _strength) public {
    //     require(msg.sender == playersAddress, "Not players contract");
    //     for(uint i = 0; i < 4; i++) {
    //         BCTypes.Door memory currentDoor = getDoor(_mapId, _row, _col, i);
    //         if (currentDoor.status == BCTypes.DoorStatus.CLOSED ||
    //             currentDoor.status == BCTypes.DoorStatus.OPEN ||
    //             currentDoor.status == BCTypes.DoorStatus.BREACHED) {
    //                 _setDoorFromPos(_mapId, _row, _col, i, BCTypes.DoorStatus.CLOSED, _strength);
    //         }
    //     }
    // }

    function setLooted(uint _mapId, uint _row, uint _col) public {
        require(msg.sender == actionsAddress, "Not actions contract");
        maps[_mapId].board[_row][_col].looted = true;
    }

    function extSetHazardStatus(uint _mapId, uint _row, uint _col, bool _hasHazard) public {
        require(msg.sender == playersAddress, "Not player contract");
        maps[_mapId].board[_row][_col].hasHazard = _hasHazard;
    }

    function extSpreadHazards(uint _mapId, uint _distance) public onlyGame {
        // TODO: CRITICAL -> this is untested
        bool[BOARD_SIZE][BOARD_SIZE] memory buffer;

        for(uint times = 0; times < _distance; times++) {
            for(uint row = 0; row < BOARD_SIZE; row++) {
                for(uint col = 0; col < BOARD_SIZE; col++) {
                    buffer[row][col] = maps[_mapId].board[row][col].hasHazard;
                }
            }

            for(uint row = 0; row < BOARD_SIZE; row++) {
                for(uint col = 0; col < BOARD_SIZE; col++) {
                    if(buffer[row][col]) {
                        for(uint dir = 0; dir < 4; dir++) {
                            uint doorId = maps[_mapId].board[row][col].doors[dir];
                            BCTypes.DoorStatus doorStatus = maps[_mapId].doors[doorId].status;
                            if(doorStatus == BCTypes.DoorStatus.BREACHED) {
                                if(row == 0 && dir == 0) {
                                    continue;
                                }
                                if (row == BOARD_SIZE-1 && dir == 1) {
                                    continue;
                                }
                                if(col == 0 && dir == 3) {
                                    continue;
                                }
                                if(col == BOARD_SIZE-1 && dir == 2) {
                                    continue;
                                }
                                BCTypes.Position memory neighborPos = utils.getNeighborRoom(dir, BCTypes.Position(uint8(row), uint8(col)));
                                BCTypes.GameTile storage neighbor = maps[_mapId].board[neighborPos.row][neighborPos.col];
                                if(neighbor.roomId != SPACE_ROOM && neighbor.roomId != START_ROOM_ID) {
                                    neighbor.hasHazard = true;
                                }
                            }
                        }
                    }
                }
            }
        }
    }


    function resetOpenDoors(uint _mapId) public onlyGame {
        for (uint i = 0; i < maps[_mapId].numDoors; i++) {
            if (maps[_mapId].doors[i].status == BCTypes.DoorStatus.OPEN) {
                maps[_mapId].doors[i].status = BCTypes.DoorStatus.CLOSED;
            }
        }
    }

    function _getUnusedRoomId(BCTypes.Map storage _map) internal returns (uint8) {
        // TODO: Explore other ways of doing use once
        uint8 roomId = utils.smallIntBetweenVals(uint8(START_ROOM_ID+1), _map.numRooms-1);

        while (_map.usedRooms[roomId] == true) {
            if (roomId == _map.numRooms-1) {
                roomId = uint8(START_ROOM_ID) + 1; // TODO: Semi-Hardcoded to account for empty and donghaijiu
            } else {
                roomId++;
            }
        }
        _map.usedRooms[roomId] = true;
        return roomId;
    }

    // Discover a room and return the uint id for that room's event
    // Return either the room's number, or 0 to indicate room already discovered
    function discoverRoom(uint _mapId, uint _row, uint _col) public onlyGame returns (uint) {
        BCTypes.GameTile storage gameTile = maps[_mapId].board[_row][_col];
        if( gameTile.roomId == UNDISCOVERED_ROOM) {
            gameTile.roomId = _getUnusedRoomId(maps[_mapId]);
            return gameTile.roomId;
        }

        return 0;
    }

    function debugForceRoomId(uint _mapId, uint _row, uint _col, uint _roomId) public onlyOwner {
        maps[_mapId].board[_row][_col].roomId = uint8(_roomId);
    }

    // Used to guarantee discovery of specific room immediately when leaving
    function debugBuildMapOneRoom(uint _roomId) public onlyGame returns(uint) {

        BCTypes.Map storage newMap = maps.push();

        newMap.numRooms = 4;
        newMap.roomList[0] = roomTiles.extGetRoomTile(0);
        newMap.roomList[1] = roomTiles.extGetRoomTile(1);
        newMap.roomList[2] = roomTiles.extGetRoomTile(2);
        newMap.roomList[3] = roomTiles.extGetRoomTile(_roomId);

        // ADD a door for walls (this will be default!)
        newMap.doors[newMap.numDoors] = BCTypes.Door(
            255,
            255, // TODO: Hack to make them invincible??
            BCTypes.DoorStatus.NO_DOOR
        );
        newMap.numDoors++;

        _staticStart(newMap);

        uint startRow = BOARD_SIZE-1;
        uint startCol = (BOARD_SIZE/2)+1;

        newMap.doors[newMap.numDoors] = BCTypes.Door(
            utils.smallIntBetweenVals(0, MAX_DOOR_STR),
            utils.smallIntBetweenVals(0, MAX_DOOR_STR),
            BCTypes.DoorStatus.BREACHED);

        newMap.numDoors++;

        newMap.board[startRow][startCol].roomId = uint8(START_ROOM_ID);
        newMap.board[startRow-1][startCol].roomId = UNDISCOVERED_ROOM;

        newMap.board[startRow][startCol].doors[0] = newMap.numDoors-1;
        newMap.board[startRow-1][startCol].doors[1] = newMap.numDoors-1;

        return maps.length-1;
    }

    modifier onlyGame {
        require(msg.sender == game);
        _;
    }
}

