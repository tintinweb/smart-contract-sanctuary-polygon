pragma solidity ^0.8.13;

import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts-upgradeable/token/common/ERC2981Upgradeable.sol";

import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155BurnableUpgradeable.sol";

import "operator-filter-registry/src/DefaultOperatorFilterer.sol";
import "./Payment.sol";
import "./RoleManagement.sol";

contract EpicSkins is
    Initializable,
    ERC1155Upgradeable,
    ERC2981Upgradeable,
    Payment,
    RoleManagement,
    ReentrancyGuardUpgradeable,
    DefaultOperatorFilterer,
    ERC1155BurnableUpgradeable
{
    using Strings for uint256;

    string public baseURI;

    // Mapping of token ID to supply
    mapping(uint256 => uint256) public tokenSupply;
    mapping(uint256 => bool) public frozenTokens;

    uint private PLAYER_MINT_FEE = 0.01 ether;

    // Mapping of token ID to URI
    mapping(uint256 => string) private _tokenURIs;

    mapping(uint256 => uint256) private _tokenPrices;

    mapping(address => string) private _playerMetadataURIs;

    mapping(address => uint256) private _mintedAmount;

    event Airdrop(
        address indexed player,
        uint256 indexed tokenId,
        uint256 indexed amount
    );

    event Award(
        address indexed player,
        uint256 indexed tokenId,
        uint256 indexed amount
    );

    event PlayerMint(
        address indexed player,
        uint256 indexed tokenId,
        uint256 indexed amount
    );

    event PermanentURI(string _value, uint256 indexed _id);

    event PlayerBatchMint(
        address indexed player,
        uint256[] indexed tokenIds,
        uint256[] indexed amounts
    );

    // Address where royalties will be transferred to
    address public royaltiesAddress;
    // Secondary market royalties in basis points (100 bps = 1%). Royalties use ERC2981 standard and support
    // OpenSea standard.
    uint256 public royaltiesBasisPoints;

    uint16 public constant ROYALTIES_BASIS = 10000;

    // shareAddressList is array of address, where you have to send all revenue split addresses like [address1, adddress2, address3]
    // shareList is an array of percents, if you have 3 owners and revenue split 20, 30, 50, you have to set [20, 30, 50]
    //_royaltyFeesInBips is royalty pernce, if you want to have roaylty like 10%, just send 1000, you can set empty from the start and set it later
    //_initBaseURI is metadata base url, you can send emty from the start, and after you use setURI method
    //_contURI is json file url, here is example: https://share.cleanshot.com/rtr3DcrS
    function initialize(
        address _owner,
        RolesAddresses[] memory _rolesAddresses,
        address[] memory shareAddressList,
        uint[] memory shareList,
        uint96 _royaltyFeesInBips,
        string memory _initBaseURI
    ) public initializer {
        __ERC1155_init("CryptoOfArms");
        __ReentrancyGuard_init();
        _initRoles(_owner, _rolesAddresses);
        __Payment_init(shareAddressList, shareList);
        _setRoyalties(_owner, _royaltyFeesInBips);

        setURI(_initBaseURI);
    }

    function updateRoles(
        RolesAddresses[] memory rolesAddresses
    ) public onlyRole(UPDATE_PERMISSONS_ROLE) {
        _updateRoles(rolesAddresses);
    }

    function revokeL3velsRoles(
        bool isRevokePermissions
    ) public onlyRole(REVOKE_L3VELS_ROLE) {
        if (isRevokePermissions) {
            revokeL3velsPermissions();
        }
    }

    function _mintToken(
        address player,
        uint256 tokenId,
        uint256 amount,
        bytes memory data
    ) private {
        require(tokenId > 0, "Token doesn't exists");

        _mint(player, tokenId, amount, data);
        _mintedAmount[player] += amount;
        tokenSupply[tokenId] += amount;
    }

    function _mintBatchToken(
        address player,
        uint256[] memory tokenIds,
        uint256[] memory amounts,
        bytes memory data
    ) private {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(tokenIds[i] > 0, "Token doesn't exists");
        }

        uint256 totalAmount = 0;
        for (uint256 i = 0; i < amounts.length; i++) {
            totalAmount += amounts[i];
        }

        _mintBatch(player, tokenIds, amounts, data);

        for (uint256 i = 0; i < tokenIds.length; i++) {
            _mintedAmount[player] += amounts[i];
            tokenSupply[tokenIds[i]] += amounts[i];
        }
    }

    function mint(
        address player,
        uint256 tokenId,
        uint256 amount,
        bytes memory data
    ) public onlyRole(MINTER_ROLE) {
        _mintToken(player, tokenId, amount, data);
    }

    function mintBatch(
        address player,
        uint256[] memory tokenIds,
        uint256[] memory amounts,
        bytes memory data
    ) public onlyRole(MINTER_ROLE) {
        _mintBatchToken(player, tokenIds, amounts, data);
    }

    function playerMint(
        uint256 tokenId,
        uint256 amount,
        bytes memory data
    ) public payable {
        uint256 tokenPrice = getAssetPrice(tokenId);
        uint256 price = tokenPrice > 0 ? tokenPrice : PLAYER_MINT_FEE;
        uint256 totalPrice = price * amount;
        require(msg.value >= totalPrice, "EpicSkins: Insufficient mint fee");

        _mintToken(msg.sender, tokenId, amount, data);

        emit PlayerMint(msg.sender, tokenId, amount);
    }

    function batchPlayerMint(
        uint256[] memory tokenIds,
        uint256[] memory amounts,
        bytes memory data
    ) public payable {
        uint256 totalAmount = 0;
        for (uint256 i = 0; i < amounts.length; i++) {
            uint256 tokenId = tokenIds[i];
            uint256 tokenPrice = getAssetPrice(tokenId);
            uint256 price = tokenPrice > 0 ? tokenPrice : PLAYER_MINT_FEE;
            totalAmount += price * amounts[i];
        }
        require(msg.value >= totalAmount, "EpicSkins: Insufficient mint fee");

        _mintBatchToken(msg.sender, tokenIds, amounts, data);

        emit PlayerBatchMint(msg.sender, tokenIds, amounts);
    }

    function award(
        address player,
        uint256 tokenId,
        uint256 amount,
        bytes memory data
    ) external onlyRole(AWARD_ROLE) {
        _mintToken(player, tokenId, amount, data);

        emit Award(player, tokenId, amount);
    }

    function airdrop(
        address player,
        uint256 tokenId,
        uint256 amount,
        bytes memory data
    ) external onlyRole(AIRDROP_ROLE) {
        _mintToken(player, tokenId, amount, data);

        emit Airdrop(player, tokenId, amount);
    }

    /**
     * @dev OpenSea contract metadata, returns a base64 encoded JSON string containing royalties basis points
     * and royalties address
     */
    function contractURI() external view returns (string memory) {
        string memory json = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        // solium-disable-next-line quotes
                        '{"name": "', // solhint-disable-line
                        "EpicSkins", // replace with your name
                        // solium-disable-next-line quotes
                        '", "description": "', // solhint-disable-line
                        "EpicSkins", // replace with your description
                        // solium-disable-next-line quotes
                        '", "image": "', // solhint-disable-line
                        "https://l3-data-dev.s3.amazonaws.com/account_ebd02d5f-1fcc-495e-92a4-84f79095307e/game_17e9d81a-3a6a-468b-bdf5-3542774d67c2/collection_ab71b1e6-45ef-4384-a048-6a1fa05b1e3b/medias/wekzsnyjshkskvfbjjbpzrvzuqjlkjhnlrxakemaiwbxkauloeicshdwvxgekxlg.jpg", // replace with your image URL
                        // solium-disable-next-line quotes
                        '", "seller_fee_basis_points": ', // solhint-disable-line
                        royaltiesBasisPoints.toString(),
                        // solium-disable-next-line quotes
                        ', "fee_recipient": "', // solhint-disable-line
                        uint256(uint160(royaltiesAddress)).toHexString(20),
                        // solium-disable-next-line quotes
                        '"}' // solhint-disable-line
                    )
                )
            )
        );

        string memory output = string(
            abi.encodePacked("data:application/json;base64,", json)
        );

        return output;
    }

    function setRoyaltyInfo(
        address _receiver,
        uint96 _royaltyFeesInBips
    ) public onlyRole(SET_ROYALTY_ROLE) {
        _setRoyalties(_receiver, _royaltyFeesInBips);
    }

    function _setRoyalties(address newAddress, uint256 newBps) internal {
        require(newBps <= ROYALTIES_BASIS, "Cannot set royalties to over 100%");

        royaltiesAddress = newAddress;
        royaltiesBasisPoints = newBps;
    }

    function royaltyInfo(
        uint256,
        uint256 salePrice
    ) public view override returns (address, uint256) {
        return (
            royaltiesAddress,
            (royaltiesBasisPoints * salePrice) / ROYALTIES_BASIS
        );
    }

    function setURI(string memory newuri) public onlyRole(URI_SETTER_ROLE) {
        _setURI(newuri);
        baseURI = newuri;
    }

    /*
     * Returns token URI for the given token ID.
     * if token URI is not empty then returns specific token URI for token id
     * If base URI is not empty then returns base URI + token id
     */
    function uri(uint256 _id) public view override returns (string memory) {
        if (bytes(_tokenURIs[_id]).length > 0) {
            return _tokenURIs[_id];
        } else if (bytes(baseURI).length > 0) {
            return string(abi.encodePacked(baseURI, _id.toString()));
        } else {
            return super.uri(_id);
        }
    }

    /*
     * Allows to update token URI for given `_tokenId` and freeze it.
     * For the transaction to succeed either `_newUri` or `_isFreezeTokenUri` as true must be specified, or both.
     * The `_newURI` cannot be the same as the current URI for the token.
     *
     * Token ID must exist, `metadataUpdatable` must be true and token URI for given token cannot be frozen.
     * Only callable by accounts with `UPDATE_TOKEN_ROLE` or `ADMIN_ROLE`.
     */
    function updateTokenUri(
        uint256 _tokenId,
        string memory _newUri,
        bool _isFreezeTokenUri
    ) public onlyRole(UPDATE_TOKEN_ROLE) {
        require(_exists(_tokenId), "Token does not exist");
        require(frozenTokens[_tokenId] != true, "Token is frozen");
        require(
            _isFreezeTokenUri || (bytes(_newUri).length != 0),
            "Either _newUri or _isFreezeTokenUri=true required"
        );

        if (bytes(_newUri).length != 0) {
            require(
                keccak256(bytes(_tokenURIs[_tokenId])) !=
                    keccak256(bytes(string(abi.encodePacked(_newUri)))),
                "New token URI is same as updated"
            );
            _tokenURIs[_tokenId] = _newUri;
            emit URI(_newUri, _tokenId);
        }
        if (_isFreezeTokenUri) {
            frozenTokens[_tokenId] = true;
            emit PermanentURI(_tokenURIs[_tokenId], _tokenId);
        }
    }

    // Used for checking if token with given ID exists
    function _exists(uint256 _tokenId) internal view virtual returns (bool) {
        return tokenSupply[_tokenId] > 0;
    }

    // Returns total supply for the given token ID
    function totalSupply(uint256 _id) public view returns (uint256) {
        return tokenSupply[_id];
    }

    function getAssetPrice(uint256 id) public view returns (uint256) {
        return _tokenPrices[id];
    }

    function setAssetPrice(
        uint256 id,
        uint256 price
    ) public onlyRole(SET_TOKEN_PRICE_ROLE) {
        _tokenPrices[id] = price;
    }

    function setAssetPrices(
        uint256[] memory ids,
        uint256[] memory prices
    ) public onlyRole(SET_TOKEN_PRICE_ROLE) {
        require(ids.length == prices.length, "ids and prices length mismatch");
        for (uint256 i = 0; i < ids.length; i++) {
            _tokenPrices[ids[i]] = prices[i];
        }
    }

    function setPlayerMetadata(string memory cid) public {
        _playerMetadataURIs[msg.sender] = cid;
    }

    function getPlayerMetadata(
        address player
    ) public view returns (string memory) {
        return _playerMetadataURIs[player];
    }

    // @notice Update public mint price
    function setPublicMintPrice(
        uint price
    ) external onlyRole(SET_PUBLIC_PRICE_ROLE) {
        PLAYER_MINT_FEE = price;
    }

    function withdraw() external {
        _withdraw(payable(msg.sender));
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        override(
            ERC1155Upgradeable,
            ERC2981Upgradeable,
            AccessControlUpgradeable
        )
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function setApprovalForAll(
        address operator,
        bool approved
    ) public override onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        uint256 amount,
        bytes memory data
    ) public override onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId, amount, data);
    }

    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public virtual override onlyAllowedOperator(from) {
        super.safeBatchTransferFrom(from, to, ids, amounts, data);
    }

    /*
     * Allows to burn given `value` amount of tokens with `id`.
     * `tokensBurnable` must be true in order for the transaction to succeed and
     * at least `value` amount of tokens must exist.
     * Only callable by accounts with `BURN_ROLE` or `ADMIN_ROLE`.
     */
    function burn(
        address player,
        uint256 id,
        uint256 value
    ) public virtual override onlyRole(BURN_ROLE) {
        super._burn(player, id, value);
        tokenSupply[id] -= value;
    }

    /*
     * Same functionality as `burn` but for a batch of tokens.
     * Input `ids` and `values` must be in direct correlation,
     * an index in both lists referring to the same token.
     */
    function burnBatch(
        address player,
        uint256[] memory ids,
        uint256[] memory values // Add "public" visibility specifier here
    ) public virtual override onlyRole(BURN_ROLE) {
        super._burnBatch(player, ids, values);
        for (uint256 i = 0; i < ids.length; i++) {
            tokenSupply[ids[i]] -= values[i];
        }
    }
}

