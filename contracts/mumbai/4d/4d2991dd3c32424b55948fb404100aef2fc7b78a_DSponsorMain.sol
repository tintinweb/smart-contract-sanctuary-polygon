pragma solidity ^0.8.9;

import './lib/ContextMixin.sol';

import '@openzeppelin/contracts/utils/Counters.sol';
import '@openzeppelin/contracts/utils/Strings.sol';

import '@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol';
import '@openzeppelin/contracts/token/ERC721/ERC721.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/utils/Base64.sol';

import '@openzeppelin/contracts/security/ReentrancyGuard.sol';

import { IDSponsorGroups, SponsorGroupSettings, MintPriceSettings } from './interfaces/IDSponsorGroups.sol';

/**
 * @title DSponsorNFT
 * @author Anthony Gourraud, nezzar.eth
 * @notice This is an NFT (ERC721) contract
 * with ERC20 pricing
 *
 * Controller can set the minting price for any currency
 * (fixed amount of wei or ERC20 tokens) and can disable a currency at any time. *
 * A treasury receives funds from mint.
 */
contract DSponsorGroups is ContextMixin, IDSponsorGroups, ReentrancyGuard, ERC721, Ownable {
  using Counters for Counters.Counter;
  using SafeERC20 for IERC20;
  using Strings for uint256;
  using Strings for uint128;

  /**
   * @dev Optional Base URI for all tokens
   */
  string public BASE_URI;

  /**
   * @dev Group ID offset multiplier to generate token ID
   */
  uint256 public constant GROUP_OFFSET_MULTIPLIER = uint256(2) ** 128;

  /**
   *  @dev Group ID counter
   */
  Counters.Counter private _groupIds;

  /**
   *  @dev Sponsoring settings
   */
  mapping(uint128 => SponsorGroupSettings) private _sponsorSettings;

  /**
   * @dev ERC20 Mint price settings
   * Note: Use address(0) for native currency
   */
  mapping(uint128 => mapping(address => MintPriceSettings)) private _mintPriceSettings;

  constructor(string memory _name, string memory _symbol, string memory _base_uri) ERC721(_name, _symbol) {
    BASE_URI = _base_uri;
  }

  ///////////////////////// MODIFIERS /////////////////////////

  modifier onlyController(uint128 groupId) {
    if (msg.sender != _sponsorSettings[groupId].controller) revert ForbiddenControllerOperation();
    _;
  }

  function openSponsorGroup(
    uint128 _maxSupply,
    address payable _treasury,
    address _controller,
    string memory _uri
  ) external returns (uint128) {
    if (_maxSupply == 0) revert MaxSupplyShouldBeGreaterThan0();
    if (_treasury == address(0)) revert CannotBeZeroAddress();
    if (_controller == address(0)) revert CannotBeZeroAddress();

    _groupIds.increment();

    uint128 groupId = uint128(_groupIds.current());

    _sponsorSettings[groupId] = SponsorGroupSettings({
      maxSupply: _maxSupply,
      currentSupply: 0,
      treasury: _treasury,
      controller: _controller,
      uri: _uri
    });

    emit OpenDSponsorGroup(groupId, _maxSupply, _treasury, _controller);

    return groupId;
  }

  /* ******************
   *  EXTERNAL FUNCTIONS
   ********************/

  function mintSponsorSpace(
    uint128 groupId,
    address currency,
    address tokenOwner,
    string calldata referralData
  ) external payable nonReentrant returns (uint256) {
    if (tokenOwner == address(0)) revert CannotBeZeroAddress();

    (bool enabled, uint256 amount) = getMintPriceForCurrency(groupId, currency);

    if (!enabled) revert ForbiddenCurrency(currency);

    if (currency == address(0) && msg.value < amount) revert AmountValueTooLow(msg.value);

    if (amount > 0) {
      if (currency == address(0)) Address.sendValue(_sponsorSettings[groupId].treasury, amount);
      else {
        /*
         * @dev Controller is responsible to set a valid contract as currency.
         * If this function does not revert, we assume it transfered tokens as expected
         */
        IERC20(currency).safeTransferFrom(_msgSender(), _sponsorSettings[groupId].treasury, amount);
      }
    }

    if (_sponsorSettings[groupId].currentSupply + 1 > _sponsorSettings[groupId].maxSupply) revert MaxSupplyExceeded();

    _sponsorSettings[groupId].currentSupply += 1;

    uint128 spaceNumber = _sponsorSettings[groupId].currentSupply;

    uint256 tokenId = _generateTokenId(groupId, spaceNumber);

    _safeMint(tokenOwner, tokenId);

    emit Mint(tokenId, groupId, spaceNumber, tokenOwner);

    return tokenId;
  }

  function setPrice(uint128 groupId, address currency, bool enabled, uint256 amount) external onlyController(groupId) {
    _mintPriceSettings[groupId][currency] = MintPriceSettings({ enabled: enabled, amount: amount });

    emit MintPriceChange(groupId, currency, enabled, amount);
  }

  function setBaseURI(string calldata _base_uri) external onlyOwner {
    BASE_URI = _base_uri;
  }

  ///////////////////////// PUBLIC GETTERS /////////////////////////

  function getMintPriceForCurrency(
    uint128 groupId,
    address currency
  ) public view returns (bool enabled, uint256 amount) {
    enabled = _mintPriceSettings[groupId][currency].enabled;
    amount = _mintPriceSettings[groupId][currency].amount;
  }

  function getController(uint128 groupId) external view returns (address) {
    return _sponsorSettings[groupId].controller;
  }

  function getMaxSupply(uint128 groupId) external view returns (uint256) {
    return _sponsorSettings[groupId].maxSupply;
  }

  function getTreasury(uint128 groupId) external view returns (address payable) {
    return _sponsorSettings[groupId].treasury;
  }

  function totalSupply() public view returns (uint256) {
    return _groupIds.current();
  }

  function totalSupplyBySponsor(uint128 groupId) public view returns (uint256) {
    return _sponsorSettings[groupId].currentSupply;
  }

  ///////////////////////// INTERNALS ///////////////////////////////

  function _generateTokenId(uint128 groupId, uint128 spaceNumber) internal pure returns (uint256) {
    return (groupId * GROUP_OFFSET_MULTIPLIER) + spaceNumber;
  }

  function _decodeGroupId(uint256 tokenId) internal pure returns (uint128) {
    return uint128(tokenId / GROUP_OFFSET_MULTIPLIER);
  }

  ///////////////////////// PUBLIC OVERRIDE GETTERS ///////////////////////////////

  function isApprovedForAll(address _owner, address _operator) public view override returns (bool isOperator) {
    /* @dev On Polygon (Main Network), if OpenSea's ERC721 Proxy Address is detected,
     * auto-return true. Otherwise, use the default ERC721.isApprovedForAll()
     * See https://docs.opensea.io/docs/polygon-basic-integration
     */
    if (block.chainid == 137 && _operator == address(0x58807baD0B376efc12F5AD86aAc70E78ed67deaE)) {
      return true;
    }
    return ERC721.isApprovedForAll(_owner, _operator);
  }

  function _baseURI() internal view virtual override returns (string memory) {
    return BASE_URI;
  }

  ///////////////////////// OVERRIDE FUNCTIONS ///////////////////////////////

  /* @dev Used instead of msg.sender as transactions won't be sent
   * by the original token owner, but by relayer.
   * See Opensea meta tx : https://docs.opensea.io/docs/polygon-basic-integration
   */
  function _msgSender() internal view override returns (address sender) {
    return ContextMixin.msgSender();
  }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import '@openzeppelin/contracts/finance/PaymentSplitter.sol';

/**
 * @title DSponsorTreasury factory
 * @author Anthony Gourraud
 * @notice Create {PaymentSplitter} contract with shares for DSponsor protocol
 */
contract DSponsorTreasuryFactory {
  /// @notice Protocol to receive 4% fees on revenues
  uint256 public constant PROTOCOL_PERCENT_FEE = 4;

  /***
   * @notice This wallet can collect protocol fees from a
   * a {PaymentSplitter} contract
   */
  address public immutable PROTOCOL_ADDRESS_FEE;

  constructor() {
    uint256 chainId = block.chainid;
    PROTOCOL_ADDRESS_FEE = chainId == 1 || // Ethereum
      chainId == 100 || // Gnosis
      chainId == 137 || // Polygon
      chainId == 42161 || // Arbitrum
      chainId == 5 // Goerli
      ? 0x5b15Cbb40Ef056F74130F0e6A1e6FD183b14Cdaf
      : 0x64E8f7C2B4fd33f5E8470F3C6Df04974F90fc2cA;
  }

  /**
   * @param beneficiary Shares funds with DSponsor protocol (96%-4%)
   *
   * @notice Returns a {PaymentSplitter} contract. *
   * A payee should invoke {PaymentSplitter-release} to withdraw.
   * See https://docs.openzeppelin.com/contracts/4.x/api/finance#PaymentSplitter
   */
  function createDSponsorTreasuryFactory(address beneficiary) external returns (address) {
    address[] memory payees = new address[](2);
    payees[0] = beneficiary;
    payees[1] = PROTOCOL_ADDRESS_FEE;
    uint256[] memory shares = new uint256[](2);
    shares[0] = 100 - PROTOCOL_PERCENT_FEE;
    shares[1] = PROTOCOL_PERCENT_FEE;

    return address(new PaymentSplitter(payees, shares));
  }
}

