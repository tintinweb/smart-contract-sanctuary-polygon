contract FundGelato is AutomateFund, GasEstimate {
    ITreasury public _treasury;

    address[] public _automates;
    uint256 public _gasPrice = 2500000050;

    constructor(address resolver) AutomateFund(resolver) {
        _automates.push(address(this));
    }

    function setTreasury(address addr) external onlyOwner {
        _treasury = ITreasury(addr);
    }

    function setGasPrice(uint val) external onlyOwner {
        _gasPrice = val;
    }

    function setAutomates(address[] calldata automates) external onlyOwner {
        _automates = automates;
    }

    function perform() public returns (uint256 sum) {
        uint256 estimateGas;

        for (uint i = 0; i < _automates.length; i++) {
            estimateGas += GasEstimate(_automates[i]).getEstimateGasPerform();
        }

        require(estimateGas > 0, "FundGelato: EstimateGas cannot be 0");

        uint gasPrice = tx.gasprice == 0 ? _gasPrice : tx.gasprice;
        uint256 feeNative = (gasPrice * estimateGas * 1.5e18) / 1e18;

        //18342
        uint256 fee = _nativeToken == _token
            ? feeNative
            : _exchange.getAmountOut(_nativeToken, _token, feeNative);

        uint256 balance = _treasury.userTokenBalance(owner(), _token);
        (bool isCheck, uint256 amount) = check(fee, balance);

        require(isCheck, "FundGelato: Fund not need");

        sum = _perform(amount);

        require(sum > 0, "FundGelato: Sum cannot be 0");

        IERC20(_token).approve(address(_treasury), sum);
        _treasury.depositFunds(owner(), _token, sum);
    }
}

