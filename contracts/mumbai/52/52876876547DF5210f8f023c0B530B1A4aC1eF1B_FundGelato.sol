     * @notice Maximum count of transactions which used in calculation of
     * maximum LINK balance have to be reached
     */
    uint256 public _maxCountTx = 20;
    address public _token;
    address public _nativeToken;

    uint8 public _tokenDecimals;

    mapping(address => bool) public _excludeToken;

    constructor(address resolver) MixinResolver(resolver) {}

    function setToken(address token) external onlyOwner {
        _token = token;
        _tokenDecimals = ERC20(_token).decimals();
    }

    function setNativeToken(address token) external onlyOwner {
        _nativeToken = token;
    }

    function excludeToken(address token) external onlyOwner {
        _excludeToken[token] = true;
    }

    function includeToken(address token) external onlyOwner {
        _excludeToken[token] = false;
    }

    /**
     * @notice to set the addresses required for the contract
     *
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function resolverAddressesRequired()
        public
        pure
        override
        returns (bytes32[] memory addresses)
    {
        addresses = new bytes32[](2);
        addresses[0] = FACTORY_CONTRACT;
        addresses[1] = INSURANCE_FUND_CONTRACT;
    }

    /**
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function _resolveAddressesFinish() internal override {
        _factory = IFactory(requireAndGetAddress(FACTORY_CONTRACT));
        _creditSystem = ICreditSystem(
            requireAndGetAddress(INSURANCE_FUND_CONTRACT)
        );
        _exchange = _factory.getExchange(IExchange.DEX.UNISWAP);
    }

    /**
     * @notice Set new minCountTx value
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setMinCountTx(uint256 minCountTx) external onlyOwner {
        require(minCountTx <= _maxCountTx, "Incorrect value");

        _minCountTx = minCountTx;
    }

