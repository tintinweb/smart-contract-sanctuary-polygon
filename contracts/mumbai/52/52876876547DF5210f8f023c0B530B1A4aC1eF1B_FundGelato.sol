     * @notice Set new maxCountTx value
     *
     * Requirements:
     *
     * - can only be called by the current owner.
     *
     */
    function setMaxCountTx(uint256 maxCountTx) public onlyOwner {
        require(maxCountTx >= _minCountTx, "Incorrect value");

        _maxCountTx = maxCountTx;
    }

    function check(
        uint fee,
        uint balance
    ) public view returns (bool isCheck, uint256 amount) {
        uint256 min = fee * _minCountTx;

        if (balance >= min) return (false, 0);

        return (true, fee * _maxCountTx - balance);
    }

