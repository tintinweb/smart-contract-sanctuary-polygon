abstract contract AutomateFund is MixinResolver, Ownable {
    using Utils for uint256[];

    bytes32 private constant FACTORY_CONTRACT = "Factory";
    bytes32 private constant INSURANCE_FUND_CONTRACT = "InsuranceFund";

    IFactory public _factory;
    ICreditSystem public _creditSystem;
    IExchange public _exchange;

    /**
     * @notice Minimum count of transactions which used in calculation of
     * minimal required LINK balance
     */
