        swordMatrix.push([0, 0, 0, 0, 1, 1, 5, 5, 6, 6, 1, 1, 0, 0, 0, 0]);
        swordMatrix.push([0, 0, 0, 0, 1, 1, 5, 5, 4, 4, 1, 1, 0, 0, 0, 0]);
