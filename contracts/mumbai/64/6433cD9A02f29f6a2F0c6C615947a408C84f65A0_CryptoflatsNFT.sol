pragma solidity ^0.8.18;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./ERC721RPass.sol";
import "../interfaces/ICryptoflatsNFT.sol";
import "./ICryptoflats.sol";

contract CryptoflatsNFT is 
    ICryptoflats,
    ERC721rPass,
    Ownable,
    ERC2981 
{
    using Strings for uint256;

    uint96 public constant DEFAULT_ROYALTY = 500; // 5%
    uint256 public constant EARLY_ACCESS_PRICE = 0.1 ether;
    uint256 public constant PUBLIC_SALE_PRICE = 0.15 ether;
    uint256 public constant MAX_SUPPLY = 1_111;
    uint256 public immutable gen;
    address payable public teamWallet;
    bytes32 public whitelistFreePurchaseRoot;
    bytes32 public whitelistEarlyAccessRoot;
    mapping(address => bool) public isWhitelistFreePurchaseUserMintedOnce;
    mapping(address => uint256) public getMintCountForEarlyAccessUser;


    constructor(
        address payable teamWallet_,
        uint256 gen_
    ) ERC721rPass("Cryptoflats-Gen0", "CNRS-0", MAX_SUPPLY) {
        gen = gen_;
        teamWallet = teamWallet_;
        _setDefaultRoyalty(msg.sender, DEFAULT_ROYALTY);
    }

    function supportsInterface(bytes4 interfaceId) 
        public 
        view 
        virtual 
        override(ERC2981, ERC721rPass) 
        returns (bool) {
        return super.supportsInterface(interfaceId);
    }


    function getNFTType(uint256 _id) external view returns (Type) {
        require(_exists(_id), "CNRS: Token doesn't exsits");
        return _idToType[_id];
    }


    function setNewTeamWallet(address payable newTeamWallet) 
        external
        onlyOwner {
        emit TeamWalletTransferred(msg.sender, teamWallet, newTeamWallet);
        teamWallet = newTeamWallet;
    }

    function setNewFreePurchaseWhitelistRoot(bytes32 newFreePurchaseWhitelistRoot) 
        external
        onlyOwner {
        emit WhitelistRootChanged(
            msg.sender,
            whitelistFreePurchaseRoot,
            newFreePurchaseWhitelistRoot,
            "Free Purchase"
        );
        whitelistFreePurchaseRoot = newFreePurchaseWhitelistRoot;
        
    }

    function setNewEarlyAccessWhitelistRoot(bytes32 newEarlyAccessWhitelistRoot) external onlyOwner {
        emit WhitelistRootChanged(
            msg.sender,
            whitelistFreePurchaseRoot,
            newEarlyAccessWhitelistRoot,
            "Early Access"
        );
        whitelistEarlyAccessRoot = newEarlyAccessWhitelistRoot;
    }

    function setSilver(uint256[] calldata _silverIds) external onlyOwner {
        for (uint256 i = 0; i < _silverIds.length; i++) {
            _idToType[_silverIds[i]] = Type.Silver;
            emit CryptoflatsNftTypeChanged(_silverIds[i], "Silver");
        }
    }
    function setGold(uint256[] calldata _goldIds) external onlyOwner {
        for (uint256 i = 0; i < _goldIds.length; i++) {
            _idToType[_goldIds[i]] = Type.Gold;
            emit CryptoflatsNftTypeChanged(_goldIds[i], "Gold");
        }
    }

    function setDiamond(uint256[] calldata _diamondIds) external onlyOwner {
        for (uint256 i = 0; i < _diamondIds.length; i++) {
            _idToType[_diamondIds[i]] = Type.Diamond;
            emit CryptoflatsNftTypeChanged(_diamondIds[i], "Diamond");
        }
    }

    function baseURI() 
        public
        pure
        returns (string memory) {
        return "ipfs://QmaswQ5i1CJUcPUY1CmJm5fhVpqPQWoGvZz7pG4WeMMUe6/";
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        override
        returns (string memory) {
        require(_exists(_tokenId), "CNRS: URI query for nonexistent token");
        string memory baseUri = baseURI();
        return bytes(baseUri).length > 0 ? string(abi.encodePacked(baseUri, _tokenId.toString(), ".json")) : "";
    }

    function mint(
        bytes32[] calldata whitelistFreePurchaseProof,
        bytes32[] calldata whitelistEarlyAccessProof
    ) external payable {
        bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
        
        if (isUserFreePurchaseWhitelist(whitelistFreePurchaseProof, leaf) == true) {
            isWhitelistFreePurchaseUserMintedOnce[msg.sender] = true;
        } else if (isUserEarlyAccessWhitelist(whitelistEarlyAccessProof, leaf) == true) {
            require(msg.value >= EARLY_ACCESS_PRICE, "CNRS: Insufficient funds");
            getMintCountForEarlyAccessUser[msg.sender]++;
        } else {
            require(msg.value >= PUBLIC_SALE_PRICE, "CNRS: Insufficient funds");
        }

        _mintRandom(msg.sender, 1);
    }


    function isUserFreePurchaseWhitelist(
        bytes32[] calldata whitelistMerkleProof,
        bytes32 leaf
    ) public view returns (bool) {
        if(isWhitelistFreePurchaseUserMintedOnce[msg.sender] == true) {
            return false;
        }

        return MerkleProof.verify(
            whitelistMerkleProof,
            whitelistFreePurchaseRoot,
            leaf
        );
    }

    function isUserEarlyAccessWhitelist(
        bytes32[] calldata whitelistMerkleProof,
        bytes32 leaf
    ) public view returns (bool) {
        if(getMintCountForEarlyAccessUser[msg.sender] >= 3) {
            return false;
        }

        return MerkleProof.verify(
            whitelistMerkleProof,
            whitelistEarlyAccessRoot,
            leaf
        );
    }

    // withdraw method
    function withdrawBalance()
        external
        onlyOwner
        returns (bool) {
        uint256 balance = address(this).balance;
        require(balance > 0, "CNRS: zero balance");
        
        (bool sent, bytes memory data) = teamWallet.call{value: balance}("");
        require(sent, "CNRS: Failed to send Ether");
        
        return sent;
    }
}

