pragma solidity >=0.8.0;

import {LibProde} from "../libraries/LibProde.sol";
import {LibDiamond} from "../libraries/LibDiamond.sol";
import {LibReentrancy} from "../libraries/LibReentrancy.sol";
import {IERC20} from "../openzeppelin/token/ERC20/IERC20.sol";
import {ERC20} from "../openzeppelin/token/ERC20/ERC20.sol";
import {IERC721} from "../openzeppelin/token/ERC721/IERC721.sol";
import {IERC1155} from "../openzeppelin/token/ERC1155/IERC1155.sol";
import "hardhat/console.sol";

contract Prode {
    uint256 public constant MATCH_RESULT_POINTS = 3;
    uint256 public constant GOALS_POINTS = 1;
    uint256 public constant BONUS_POINTS = 5;
    uint256 public constant WINNERS = 3;
    uint256 public constant POLYGON_MAINNET = 137;
    uint256 public constant POLYGON_MUMBAI = 80001;

    modifier onlyOwner() {
        LibDiamond.enforceIsContractOwner();
        _;
    }

    modifier nonReentrancy() {
        LibReentrancy.nonReentrantBefore();
        _;
        LibReentrancy.nonReentrantAfter();
    }

    event TournamentCreated(LibProde.Tournament tournament);

    function createTournament(
        string memory name,
        LibProde.Match[] memory matches,
        uint256 betPrice,
        uint256 pot,
        bool allowMultipleBets
    ) external onlyOwner returns (LibProde.Tournament memory createdTournament) {
        require(bytes(name).length > 0, "Name can't be empty");
        require(betPrice >= 0, "Bet price must be >= 0");
        //require(matches.length > 0, "The tournament must have at least one match");
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();

        // Tournament creation
        bs.numTournaments++;

        LibProde.Tournament storage newTournament = bs.tournaments[bs.numTournaments];
        newTournament.id = bs.numTournaments;
        newTournament.name = name;
        newTournament.status = LibProde.TournamentStatus.PENDING;
        for (uint256 i = 0; i < matches.length; i++) {
            newTournament.matches.push(matches[i]);
        }
        newTournament.betPrice = betPrice;
        newTournament.pot = pot;
        newTournament.allowMultipleBets = allowMultipleBets;

        createdTournament = bs.tournaments[bs.numTournaments];
        emit TournamentCreated(createdTournament);
        // End transaction
    }

    function getAllTournaments() external view returns (LibProde.Tournament[] memory result) {
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        mapping(uint256 => LibProde.Tournament) storage currentTournaments = bs.tournaments;
        result = new LibProde.Tournament[](bs.numTournaments);
        for (uint256 i = 1; i <= bs.numTournaments; i++) {
            result[i - 1] = currentTournaments[i];
        }
    }

    function getTournament(uint256 tournamentId) external view returns (LibProde.Tournament memory result) {
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        require(tournamentId > 0 && tournamentId <= bs.numTournaments, "Tournament doesn't exist");
        mapping(uint256 => LibProde.Tournament) storage currentTournaments = bs.tournaments;
        result = currentTournaments[tournamentId];
    }

    function placeBet(uint256 tournamentId, LibProde.Forecast[] memory forecasts)
        external
        payable
        nonReentrancy
        returns (LibProde.Bet memory newBet)
    {
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        require(tournamentId > 0 && tournamentId <= bs.numTournaments, "Tournament id doesn't exist");
        LibProde.Tournament storage tournament = bs.tournaments[tournamentId];
        require(tournament.status != LibProde.TournamentStatus.FINISHED, "Tournament has already finished");
        // TODO fix
        // require(tournament.matches[0].date > block.timestamp, "Tournament has already started. Wait for the next one ;)");
        // TODO además de chequear que la # de partidos sea la correcta, hay que chequear que cada partido de la apuesta matchee con su par en el torneo
        require(tournament.matches.length == forecasts.length, "Your bet doesn't have the exact quantity of matches the tournament has");
        // TODO acá hay que chequear que tenga saldo en la stablecoin que decidamos
        require(msg.sender.balance >= tournament.betPrice, "Insufficient balance to place your bet");
        require(msg.value == tournament.betPrice, "You must pay for your bet :)");
        require(tournament.allowMultipleBets || bs.bets[msg.sender][tournamentId].length == 0, "You have already made your bet in this tournament.");

        // Save new bet
        LibProde.Bet storage _bet = tournament.bets.push();
        _bet.id = tournament.bets.length;
        _bet.tournamentId = tournamentId;
        _bet.owner = msg.sender;
        _bet.amountPayed = tournament.betPrice;
        _bet.timestamp = block.timestamp;
        _bet.score = 0;
        for (uint256 i = 0; i < forecasts.length; i++) {
            _bet.forecasts.push(forecasts[i]);
        }
        // Add payed bet to tournament's pot
        tournament.pot += msg.value;
        newBet = tournament.bets[tournament.bets.length - 1];

        // Add bet to tournament's bets
        mapping(uint256 => uint256[]) storage gamblerTournamentBets = bs.bets[msg.sender];
        gamblerTournamentBets[tournamentId].push(_bet.id);
    }

    function getFinishedMatches(uint256 tournamentId) external view returns (LibProde.Match[] memory result) {
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        mapping(uint256 => LibProde.Tournament) storage currentTournaments = bs.tournaments;
        require(tournamentId > 0 && tournamentId <= bs.numTournaments, "The tournament doesn't exist");
        LibProde.Match[] storage currentMatches = currentTournaments[tournamentId].matches;
        // TODO ver si se pude crear un array dinámico o usar un mapping para no instanciar un array de más entries de las necesarias
        result = new LibProde.Match[](currentMatches.length);
        uint256 j = 0;
        for (uint256 i = 0; i < currentMatches.length; i++) {
            if (currentMatches[i].status == LibProde.MatchStatus.FINISHED) {
                result[j] = currentMatches[i];
                j++;
            }
        }
    }

    function setResults(uint256 tournamentId, LibProde.Match[] memory matchesToUpdate) external onlyOwner {
        require(matchesToUpdate.length > 0, "Provide at least one match");
        LibProde.ProdeStorage storage bs = LibProde.prodeStorage();
        mapping(uint256 => LibProde.Tournament) storage tournaments = bs.tournaments;
        require(tournamentId > 0 && tournamentId <= bs.numTournaments, "The tournament doesn't exist");
        LibProde.Tournament storage tournament = tournaments[tournamentId];
        LibProde.Match[] storage matches = tournament.matches;
        for (uint256 i = 0; i < matchesToUpdate.length; i++) {
            LibProde.Match storage matchToUpdate = matches[matchesToUpdate[i].id - 1];
            require(matchToUpdate.status != LibProde.MatchStatus.FINISHED, "You can't set the result of a finished match");
            matchToUpdate.goalsTeam1 = matchesToUpdate[i].goalsTeam1;
            matchToUpdate.goalsTeam2 = matchesToUpdate[i].goalsTeam2;
            matchToUpdate.status = LibProde.MatchStatus.FINISHED;
            tournament.finishedMatches++;
        }
        updateScore(tournament, matchesToUpdate);
        if (tournament.finishedMatches == tournament.matches.length) endTournament(tournament);
    }

    function endTournament(LibProde.Tournament storage tournament) internal onlyOwner {
        // TODO this should be dynamic
        uint8[3] memory prices = [50, 30, 20];

        // Determine the winners
        LibProde.Bet[WINNERS] memory winnerBets = getWinnerBets(tournament.bets);
        for (uint256 i = 0; i < WINNERS; i++) {
            uint256 prize = (tournament.pot * prices[i]) / 100;
            tournament.winnerBets.push(LibProde.WinnerBet({betId: winnerBets[i].id, position: i + 1, prize: prize, claimed: false}));
        }
        tournament.status = LibProde.TournamentStatus.FINISHED;
    }

    function getWinnerBets(LibProde.Bet[] memory bets) internal view onlyOwner returns (LibProde.Bet[WINNERS] memory) {
        LibProde.Bet[WINNERS] memory winnerBets;
        for (uint256 i = 0; i < bets.length; i++) {
            for (uint256 j = 0; j < winnerBets.length; j++) {
                if (bets[i].score > winnerBets[j].score || (bets[i].score == winnerBets[j].score && bets[i].timestamp < winnerBets[j].timestamp)) {
                    // current bet replaces current winner bet. Shift the array and replace
                    for (uint256 k = winnerBets.length - 1; k > j; k--) {
                        winnerBets[k] = winnerBets[k - 1];
                    }
                    winnerBets[j] = bets[i];
                    break;
                }
            }
        }
        return winnerBets;
    }

    function updateScore(LibProde.Tournament storage tournament, LibProde.Match[] memory matches) internal onlyOwner {
        LibProde.Bet[] storage bets = tournament.bets;
        for (uint256 i = 0; i < bets.length; i++) {
            for (uint256 j = 0; j < matches.length; j++) {
                LibProde.Match memory _match = matches[j];
                LibProde.Forecast storage forecast = bets[i].forecasts[_match.id - 1];
                LibProde.MatchResult matchResult = getResult(_match.goalsTeam1, _match.goalsTeam2);
                LibProde.MatchResult forecastResult = getResult(forecast.goalsTeam1, forecast.goalsTeam2);
                // L/D/V?
                if (matchResult == forecastResult) {
                    forecast.resultPoints = MATCH_RESULT_POINTS;
                }
                // Goals?
                if (_match.goalsTeam1 == forecast.goalsTeam1) forecast.goalsTeam1Points = GOALS_POINTS;
                if (_match.goalsTeam2 == forecast.goalsTeam2) forecast.goalsTeam2Points = GOALS_POINTS;
                // Perfect?
                if (forecast.resultPoints + forecast.goalsTeam1Points + forecast.goalsTeam2Points == MATCH_RESULT_POINTS + 2 * GOALS_POINTS)
                    forecast.bonusPoints = BONUS_POINTS;
                bets[i].score += (forecast.resultPoints + forecast.goalsTeam1Points + forecast.goalsTeam2Points + forecast.bonusPoints);
            }
        }
    }

    function getResult(uint256 goalsTeam1, uint256 goalsTeam2) internal view onlyOwner returns (LibProde.MatchResult) {
        if (goalsTeam1 > goalsTeam2) return LibProde.MatchResult.LOCAL;
        if (goalsTeam2 > goalsTeam1) return LibProde.MatchResult.VISITOR;
        return LibProde.MatchResult.DRAW;
    }

    function getBetsInTournamentByGambler(address gamblerAddress, uint256 tournamentId) external view returns (LibProde.Bet[] memory) {
        LibProde.ProdeStorage storage ps = LibProde.prodeStorage();
        require(tournamentId > 0 && tournamentId <= ps.numTournaments, "Tournament doesn't exist");
        mapping(uint256 => uint256[]) storage gamblerBets = ps.bets[gamblerAddress];
        uint256[] storage betsIds = gamblerBets[tournamentId];

        LibProde.Bet[] memory bets = new LibProde.Bet[](betsIds.length);

        for (uint256 i = 0; i < betsIds.length; i++) {
            bets[i] = ps.tournaments[tournamentId].bets[betsIds[i] - 1];
        }

        return bets;
    }

    function claimPrize(uint256 tournamentId, address erc20address) external payable nonReentrancy {
        if (block.chainid == POLYGON_MAINNET)
            // USDT
            erc20address = 0xc2132D05D31c914a87C6611C10748AEb04B58e8F;
        if (block.chainid == POLYGON_MUMBAI)
            //USDC
            erc20address = 0xE097d6B3100777DC31B34dC2c58fB524C2e76921;
        console.log("chainId: ", block.chainid);
        LibProde.ProdeStorage storage ps = LibProde.prodeStorage();
        require(tournamentId > 0 && tournamentId <= ps.numTournaments, "Tournament doesn't exist");
        LibProde.Tournament storage tournament = ps.tournaments[tournamentId];
        LibProde.WinnerBet[] storage winnerBets = tournament.winnerBets;
        ERC20 ierc20 = ERC20(erc20address);
        uint256 balance = ierc20.balanceOf(address(this));
        bool claimed = false;
        for (uint256 i = 0; i < winnerBets.length; i++) {
            if (tournament.bets[winnerBets[i].betId - 1].owner == msg.sender) {
                // make the transfer
                uint256 prize = winnerBets[i].prize;
                console.log("Tournament pot: ", tournament.pot, "Winner prize: ", winnerBets[i].prize);
                require(balance >= prize, "Ups, contract has insufficient balance to pay you. Please contact [email protected]");
                console.log("Sending ", prize, ierc20.symbol(), msg.sender);
                ierc20.transfer(msg.sender, prize);
                winnerBets[i].claimed = true;
                claimed = true;
                break;
            }
        }
        require(claimed == true, "You have nothing to claim, keep participating :)");
    }

    function addMatch(
        uint256 tournamentId,
        uint256 id,
        bytes32 group,
        bytes32 team1,
        bytes32 team2,
        uint256 date
    ) external onlyOwner {
        LibProde.ProdeStorage storage ps = LibProde.prodeStorage();
        require(tournamentId > 0 && tournamentId <= ps.numTournaments, "Tournament doesn't exist");
        LibProde.Tournament storage tournament = ps.tournaments[tournamentId];
        require(tournament.status == LibProde.TournamentStatus.PENDING, "Only tournament in Pending status can be modified.");

        LibProde.Match storage newMatch = tournament.matches.push();
        newMatch.id = id;
        newMatch.group = group;
        newMatch.team1 = team1;
        newMatch.team2 = team2;
        newMatch.date = date;
        newMatch.status = LibProde.MatchStatus.PENDING;
    }
}

