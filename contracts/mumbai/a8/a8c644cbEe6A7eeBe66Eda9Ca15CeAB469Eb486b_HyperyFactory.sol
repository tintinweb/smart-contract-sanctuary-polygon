pragma solidity 0.8.19;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155URIStorage.sol";
import "hardhat/console.sol";
import "./common/ERC2981PerTokenRoyalties.sol";
import "./interfaces/IHyperyAdmin.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

contract HyperyToken is
    ERC1155URIStorage,
    ERC2981PerTokenRoyalties,
    ReentrancyGuard
{
    uint256 public releaseIndex;
    address private proxy;
    address public artist;
    address payable public admin;
    string private contractUri;

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC1155, ERC2981Base)
        returns (bool)
    {
        return
            interfaceId == type(IERC2981Royalties).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function contractURI() public view returns (string memory) {
        return contractUri;
    }

    //EVENTS
    event ReleaseCreated(uint256 indexed _releaseIndex, string _metadataURI);
    event SaleToggled(uint256 indexed _releaseId, bool _saleActive);
    event ReserveAdded(uint256 indexed _releaseId);
    event PaymentSplitterSet(
        address indexed _paymentSplitter,
        uint256 indexed _releaseId
    );
    event Mint(
        address indexed _to,
        uint256 indexed _releaseId,
        uint256 amount,
        uint256 _value,
        bytes data
    );
    event MintWithToken(
        address indexed _to,
        uint256 indexed _releaseId,
        uint256 _value,
        uint256 _amount,
        bytes data
    );
    event MintReserve(
        address indexed _to,
        uint256 _value,
        uint256 indexed releaseId,
        bytes data
    );
    event Airdrop(
        address indexed _recipient,
        uint256 indexed releaseId,
        uint256 _value,
        bytes data
    );
    event BaseUriSet(string releaseURI);
    event TokenRoyaltySet(
        uint256 indexed releaseId,
        address indexed recipient,
        uint256 value
    );

    // structs
    struct Release {
        uint256 maxSupply;
        uint256 totalSupply;
        uint256 price;
        bool salesActive;
        address payable paymentSplitter;
        address token;
    }

    // mappings

    // mapping of releaseId to Release
    mapping(uint256 => Release) public releases;

    // mapping of releaseId to reserves
    mapping(uint256 => uint256) private reserves;

    constructor() ERC1155("") {
        releaseIndex = 0;
    }

    function init(
        address _proxy,
        address _artist,
        address payable _admin,
        string memory _contractUri
    ) external {
        require(proxy == address(0), "Init can only be called once");
        require(_proxy != address(0), "Must not be zero address");
        require(_artist != address(0), "Must not be zero address");
        require(_admin != address(0), "Must not be zero address");
        proxy = _proxy;
        artist = _artist;
        admin = _admin;
        contractUri = _contractUri;
    }

    modifier isProxy() {
        require(msg.sender == proxy, "msg.sender must be proxy");
        _;
    }

    function getProxy() external view returns (address) {
        return proxy;
    }

    function createRelease(
        uint256 _maxSupply,
        uint256 _price,
        address _token,
        string memory _metadataURI
    ) external isProxy {
        require(_maxSupply > 0, "Supply must be greater than 0");

        if (_token != address(0)) {
            bool isTokenSupported = IHyperyAdmin(admin).getToken(_token);

            require(isTokenSupported, "ERC20 is not supported");
        }

        Release memory release = Release(
            _maxSupply,
            0,
            _price,
            false,
            payable(address(0)),
            _token
        );
        releases[releaseIndex] = release;
        _setURI(releaseIndex, _metadataURI);
        releaseIndex++;

        emit ReleaseCreated(releaseIndex, _metadataURI);
    }

    function setPaymentSplitter(
        uint256 releaseId,
        address payable paymentSplitter
    ) external isProxy {
        Release storage release = releases[releaseId];
        release.paymentSplitter = paymentSplitter;

        emit PaymentSplitterSet(paymentSplitter, releaseId);
    }

    function toggleSale(uint256 _releaseId) external isProxy {
        require(releases[_releaseId].maxSupply > 0, "Release has no supply");

        releases[_releaseId].salesActive = !releases[_releaseId].salesActive;
        emit SaleToggled(_releaseId, releases[_releaseId].salesActive);
    }

    function addReserve(uint256 _releaseId) external isProxy {
        uint256 _reserves = reserves[_releaseId];
        Release memory _release = releases[_releaseId];
        require(
            _release.totalSupply + _reserves + 1 <= _release.maxSupply,
            "Release has no supply"
        );
        reserves[_releaseId] = _reserves + 1;
        emit ReserveAdded(_releaseId);
    }

    function getReserve(uint256 _releaseId) external view returns (uint256) {
        return reserves[_releaseId];
    }

    function mint(
        address _recipient,
        uint256 _releaseId,
        uint256 _value,
        bytes memory data
    ) external payable isProxy nonReentrant {
        require(_value > 0, "Value must be greater than 0");

        Release storage release = releases[_releaseId];
        uint256 _reserves = reserves[_releaseId];

        require(release.token == address(0), "Must mint with ERC20");
        require(release.salesActive, "Sale must be active");
        require(
            (release.totalSupply + _value) + _reserves <= release.maxSupply,
            "Cannot exceed Max Supply"
        );

        uint256 _fee = IHyperyAdmin(admin).fee();
        uint256 feeAmount = (release.price * _fee) / 10000;
        require(
            msg.value == _value * (release.price + feeAmount),
            "Incorrect Price"
        );

        uint256 payout = msg.value - feeAmount;

        _mint(artist, _releaseId, _value, data);
        _safeTransferFrom(artist, _recipient, _releaseId, _value, data);
        release.totalSupply = release.totalSupply + _value;

        (bool feeSent, ) = admin.call{value: feeAmount}("");
        require(feeSent, "Failed to send Ether to Admin");

        (bool payoutSent, ) = release.paymentSplitter.call{value: payout}("");
        require(payoutSent, "Failed to send Ether to Payment Splitter");

        emit Mint(_recipient, _releaseId, _value, msg.value, data);
    }

    function mintWithToken(
        address _recipient,
        uint256 _releaseId,
        uint256 _value,
        uint256 _amount,
        bytes memory data
    ) external payable isProxy nonReentrant {
        require(_value > 0, "Value must be greater than 0");

        Release storage release = releases[_releaseId];
        uint256 _reserves = reserves[_releaseId];

        require(release.token != address(0), "Must mint with Matic");
        require(release.salesActive, "Sale must be active");
        require(
            (release.totalSupply + _value) + _reserves <= release.maxSupply,
            "Cannot exceed Max Supply"
        );

        uint256 _fee = IHyperyAdmin(admin).fee();
        uint256 feeAmount = (release.price * _fee) / 10000;

        require(
            _amount == _value * (release.price + feeAmount),
            "Incorrect Price"
        );

        uint256 payout = _amount - feeAmount;

        _mint(artist, _releaseId, _value, data);
        _safeTransferFrom(artist, _recipient, _releaseId, _value, data);
        release.totalSupply = release.totalSupply + _value;

        SafeERC20.safeTransferFrom(
            IERC20(release.token),
            _recipient,
            admin,
            feeAmount
        );

        SafeERC20.safeTransferFrom(
            IERC20(release.token),
            _recipient,
            release.paymentSplitter,
            payout
        );

        emit MintWithToken(_recipient, _releaseId, _value, _amount, data);
    }

    function mintReserve(
        address _recipient,
        uint256 _releaseId,
        uint256 _value,
        bytes memory data
    ) external isProxy nonReentrant {
        require(_value > 0, "Value must be greater than 0");

        Release storage release = releases[_releaseId];
        uint256 _reserves = reserves[_releaseId];

        require(_reserves > 0, "No reserves available");
        require(_reserves >= _value, "Not enough reserves available");

        _mint(artist, _releaseId, _value, data);
        _safeTransferFrom(artist, _recipient, _releaseId, _value, data);
        release.totalSupply = release.totalSupply + _value;

        reserves[_releaseId] = _reserves - _value;
        emit MintReserve(_recipient, _value, _releaseId, data);
    }

    function airdrop(
        address _recipient,
        uint256 _releaseId,
        uint256 _value,
        bytes memory data
    ) external isProxy nonReentrant {
        require(_value > 0, "Value must be greater than 0");

        Release storage release = releases[_releaseId];
        uint256 _reserves = reserves[_releaseId];

        require(
            (release.totalSupply + _value) + _reserves <= release.maxSupply,
            "Cannot exceed Max Supply"
        );

        _mint(artist, _releaseId, _value, data);
        _safeTransferFrom(artist, _recipient, _releaseId, _value, data);
        release.totalSupply = release.totalSupply + _value;
        emit Airdrop(_recipient, _releaseId, _value, data);
    }

    function setURI(uint256 releaseId, string memory releaseURI)
        external
        virtual
        isProxy
    {
        _setURI(releaseId, releaseURI);
    }

    function setBaseURI(string memory baseURI) external virtual isProxy {
        _setBaseURI(baseURI);
        emit BaseUriSet(baseURI);
    }

    function setReleaseRoyalty(
        uint256 releaseId,
        address recipient,
        uint256 value
    ) external virtual isProxy {
        _setTokenRoyalty(releaseId, recipient, value);
        emit TokenRoyaltySet(releaseId, recipient, value);
    }

    function getRoyaltyInfo(uint256 releaseId, uint256 value)
        external
        view
        virtual
        returns (address royaltyAddress, uint256 royaltyValue)
    {
        (royaltyAddress, royaltyValue) = royaltyInfo(releaseId, value);
    }
}

