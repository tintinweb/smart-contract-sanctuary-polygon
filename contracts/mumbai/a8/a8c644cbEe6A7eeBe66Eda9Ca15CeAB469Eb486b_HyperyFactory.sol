pragma solidity 0.8.19;

import "@openzeppelin/contracts/access/Ownable.sol";
import "./common/CloneFactory.sol";
import "./HyperyToken.sol";
import "./common/CloneablePaymentSplitter.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./interfaces/IHyperyFactory.sol";
import "./interfaces/IHyperyAdmin.sol";

contract HyperyFactory is
    CloneFactory,
    Ownable,
    ReentrancyGuard,
    IHyperyFactory
{
    // constants

    string private constant ERR_RELEASE_NOT_VALID = "releaseId invalid";
    string private constant ERR_COLLECTION_NOT_VALID = "collectionId invalid";
    string private constant ERR_COLLECTION_DOES_NOT_EXIST =
        "collections don't exist";

    // state vars

    address public primaryHyperyToken;
    address payable public primaryPaymentSplitter;
    uint256 public numCollections;
    address payable public artist;
    address payable public admin;

    HyperyToken[] public collections;

    // mapping of collectionId -> releaseId -> paymentSplitter
    mapping(uint256 => mapping(uint256 => CloneablePaymentSplitter))
        public paymentSplitters;

    //EVENTS
    event CollectionCreated(address indexed _artist, address _collection);
    event ReleaseCreated(
        address indexed _artist,
        uint256 indexed _collectionId,
        uint256 _maxSupply,
        uint256 _price,
        address indexed _token
    );
    event PaymentSplitterInitialized(
        uint256 indexed _collectionId,
        uint256 indexed _releaseId,
        address[] _payees,
        uint256[] _shares
    );
    event PaymentSplitterUpdated(
        uint256 indexed _collectionId,
        uint256 indexed _releaseId,
        address[] _payees,
        uint256[] _shares
    );
    event SaleToggled(
        uint256 indexed _collectionId,
        uint256 indexed _releaseId
    );
    event SaleToggledAdmin(
        uint256 indexed _collectionId,
        uint256 indexed _releaseId
    );
    event Mint(
        address indexed _to,
        uint256 _value,
        uint256 _amount,
        uint256 indexed collectionId,
        uint256 indexed releaseId
    );
    event MintWithToken(
        address indexed _to,
        uint256 _value,
        uint256 indexed collectionId,
        uint256 indexed releaseId,
        uint256 _amount,
        bytes data
    );
    event MintReserve(
        address indexed _to,
        uint256 _value,
        uint256 indexed collectionId,
        uint256 indexed releaseId,
        bytes data
    );
    event ReserveAdded(uint256 indexed collectionId, uint256 indexed releaseId);
    event Airdrop(
        address indexed _recipient,
        uint256 indexed collectionId,
        uint256 indexed releaseId,
        uint256 _value,
        bytes data
    );
    event BaseUriSet(uint256 indexed collectionId, string releaseURI);
    event ReleaseUriSet(
        uint256 indexed collectionId,
        uint256 indexed releaseId,
        string releaseURI
    );

    constructor(
        address _primaryHyperyToken,
        address payable _primaryPaymentSplitter,
        address payable _admin,
        string memory contractUri
    ) {
        require(
            IHyperyAdmin(_admin).isHyperyAdmin(),
            "Invalid Hypery Admin address"
        );
        require(_primaryHyperyToken != address(0), "Must not be zero address");
        require(
            _primaryPaymentSplitter != address(0),
            "Must not be zero address"
        );

        numCollections = 0;
        primaryHyperyToken = _primaryHyperyToken;
        primaryPaymentSplitter = _primaryPaymentSplitter;
        artist = payable(msg.sender);
        admin = _admin;
        createCollection(contractUri);
    }

    modifier onlyAdmin() {
        require(payable(msg.sender) == admin, "msg.sender must be admin");
        _;
    }

    modifier onlyArtist() {
        require(payable(msg.sender) == artist, "msg.sender must be artist");
        _;
    }

    function createCollection(string memory contractUri)
        public
        onlyOwner
        nonReentrant
    {
        HyperyToken collection = HyperyToken(createClone(primaryHyperyToken));
        collection.init(address(this), artist, admin, contractUri);
        numCollections++;
        collections.push(collection);
        emit CollectionCreated(artist, address(collection));
    }

    function getCollections() external view returns (HyperyToken[] memory) {
        return collections;
    }

    function getCollection(uint256 collectionId)
        internal
        view
        returns (HyperyToken)
    {
        require(numCollections > 0, ERR_COLLECTION_DOES_NOT_EXIST);
        require(collectionId < numCollections, ERR_COLLECTION_NOT_VALID);

        HyperyToken collection = collections[collectionId];

        return collection;
    }

    function createRelease(
        uint256 collectionId,
        uint256 _maxSupply,
        uint256 _price,
        uint256 _royaltyValue,
        address _token,
        string memory _metadataURI,
        address[] calldata payees,
        uint256[] calldata shares
    ) external onlyOwner {
        require(numCollections > 0, ERR_COLLECTION_DOES_NOT_EXIST);
        require(collectionId < numCollections, ERR_COLLECTION_NOT_VALID);

        HyperyToken collection = getCollection(collectionId);

        CloneablePaymentSplitter paymentSplitter = CloneablePaymentSplitter(
            createClone(primaryPaymentSplitter)
        );
        uint256 releaseIndex = collection.releaseIndex();
        paymentSplitters[collectionId][releaseIndex] = paymentSplitter;
        collection.createRelease(_maxSupply, _price, _token, _metadataURI);

        collection.setPaymentSplitter(
            releaseIndex,
            payable(address(paymentSplitter))
        );
        collection.setReleaseRoyalty(
            releaseIndex,
            address(paymentSplitter),
            _royaltyValue
        );

        initPaymentSplitter(collectionId, releaseIndex, payees, shares);

        emit ReleaseCreated(artist, collectionId, _maxSupply, _price, _token);
    }

    function initPaymentSplitter(
        uint256 collectionId,
        uint256 releaseId,
        address[] calldata payees,
        uint256[] calldata shares
    ) internal {
        require(
            getRelease(collectionId, releaseId).maxSupply > 0,
            ERR_RELEASE_NOT_VALID
        );

        CloneablePaymentSplitter paymentSplitter = paymentSplitters[
            collectionId
        ][releaseId];
        paymentSplitter.initialize(payees, shares);

        emit PaymentSplitterInitialized(
            collectionId,
            releaseId,
            payees,
            shares
        );
    }

    function updatePaymentSplitter(
        uint256 collectionId,
        uint256 releaseId,
        address[] calldata payees,
        uint256[] calldata shares
    ) external onlyOwner {
        require(numCollections > 0, ERR_COLLECTION_DOES_NOT_EXIST);
        require(collectionId <= numCollections, ERR_COLLECTION_NOT_VALID);

        HyperyToken collection = getCollection(collectionId);

        CloneablePaymentSplitter paymentSplitter = CloneablePaymentSplitter(
            createClone(primaryPaymentSplitter)
        );
        paymentSplitters[collectionId][releaseId] = paymentSplitter;

        collection.setPaymentSplitter(
            releaseId,
            payable(address(paymentSplitter))
        );

        (, uint256 _royaltyValue) = collection._royalties(releaseId);

        collection.setReleaseRoyalty(
            releaseId,
            address(paymentSplitter),
            _royaltyValue
        );

        initPaymentSplitter(collectionId, releaseId, payees, shares);
        emit PaymentSplitterUpdated(collectionId, releaseId, payees, shares);
    }

    function getPaymentSplitter(uint256 collectionId, uint256 releaseId)
        external
        view
        returns (CloneablePaymentSplitter)
    {
        CloneablePaymentSplitter paymentSplitter = paymentSplitters[
            collectionId
        ][releaseId];
        return paymentSplitter;
    }

    function getRelease(uint256 collectionId, uint256 releaseId)
        public
        view
        returns (HyperyToken.Release memory)
    {
        HyperyToken collection = getCollection(collectionId);
        (
            uint256 maxSupply,
            uint256 totalSupply,
            uint256 price,
            bool salesActive,
            address payable paymentSplitter,
            address token
        ) = collection.releases(releaseId);

        require(releaseId < collection.releaseIndex(), ERR_RELEASE_NOT_VALID);

        HyperyToken.Release memory release = HyperyToken.Release(
            maxSupply,
            totalSupply,
            price,
            salesActive,
            paymentSplitter,
            token
        );
        return release;
    }

    function getReleaseIndex(uint256 collectionId)
        external
        view
        returns (uint256)
    {
        HyperyToken collection = getCollection(collectionId);

        return collection.releaseIndex();
    }

    function toggleSale(uint256 collectionId, uint256 releaseId)
        external
        onlyOwner
    {
        HyperyToken collection = getCollection(collectionId);

        require(releaseId < collection.releaseIndex(), ERR_RELEASE_NOT_VALID);

        collection.toggleSale(releaseId);
        emit SaleToggled(collectionId, releaseId);
    }

    function toggleSaleAdmin(uint256 collectionId, uint256 releaseId)
        external
        virtual
        override
        onlyAdmin
    {
        HyperyToken collection = getCollection(collectionId);

        require(releaseId < collection.releaseIndex(), ERR_RELEASE_NOT_VALID);

        collection.toggleSale(releaseId);
        emit SaleToggledAdmin(collectionId, releaseId);
    }

    function addReserve(uint256 collectionId, uint256 releaseId)
        external
        virtual
        override
        onlyAdmin
    {
        HyperyToken collection = getCollection(collectionId);
        collection.addReserve(releaseId);

        emit ReserveAdded(collectionId, releaseId);
    }

    function mint(
        uint256 collectionId,
        uint256 releaseId,
        uint256 _value,
        bytes memory data
    ) external payable nonReentrant {
        HyperyToken collection = getCollection(collectionId);

        require(releaseId < collection.releaseIndex(), ERR_RELEASE_NOT_VALID);

        collection.mint{value: msg.value}(msg.sender, releaseId, _value, data);
        emit Mint(msg.sender, msg.value, _value, collectionId, releaseId);
    }

    function mintNft(
        address to,
        uint256 collectionId,
        uint256 releaseId,
        uint256 _value,
        bytes memory data
    ) external payable override nonReentrant {
        HyperyToken collection = getCollection(collectionId);

        require(releaseId < collection.releaseIndex(), ERR_RELEASE_NOT_VALID);

        collection.mint{value: msg.value}(to, releaseId, _value, data);
        emit Mint(to, msg.value, _value, collectionId, releaseId);
    }

    function mintNftWithToken(
        address to,
        uint256 collectionId,
        uint256 releaseId,
        uint256 _value,
        uint256 _amount,
        bytes memory data
    ) external nonReentrant {
        HyperyToken collection = getCollection(collectionId);

        require(releaseId < collection.releaseIndex(), ERR_RELEASE_NOT_VALID);

        collection.mintWithToken(to, releaseId, _value, _amount, data);
        emit MintWithToken(
            to,
            _value,
            collectionId,
            releaseId,
            _amount,
            data
        );
    }

    function mintReserve(
        uint256 collectionId,
        uint256 releaseId,
        uint256 _value,
        bytes memory data,
        address _recipient
    ) external virtual override onlyAdmin nonReentrant {
        HyperyToken collection = getCollection(collectionId);

        require(releaseId < collection.releaseIndex(), ERR_RELEASE_NOT_VALID);

        collection.mintReserve(_recipient, releaseId, _value, data);
        emit MintReserve(_recipient, _value, collectionId, releaseId, data);
    }

    function airdropAdmin(
        uint256 collectionId,
        uint256 releaseId,
        uint256 _value,
        bytes memory data,
        address _recipient
    ) external virtual override onlyAdmin nonReentrant {
        HyperyToken collection = getCollection(collectionId);

        require(releaseId < collection.releaseIndex(), ERR_RELEASE_NOT_VALID);

        collection.airdrop(_recipient, releaseId, _value, data);
        emit Airdrop(_recipient, collectionId, releaseId, _value, data);
    }

    function airdrop(
        uint256 collectionId,
        uint256 releaseId,
        uint256 _value,
        bytes memory data,
        address _recipient
    ) external onlyArtist nonReentrant {
        HyperyToken collection = getCollection(collectionId);

        require(releaseId < collection.releaseIndex(), ERR_RELEASE_NOT_VALID);

        collection.airdrop(_recipient, releaseId, _value, data);
        emit Airdrop(_recipient, collectionId, releaseId, _value, data);
    }

    function setBaseURI(uint256 collectionId, string memory baseURI)
        external
        onlyOwner
    {
        HyperyToken collection = getCollection(collectionId);

        collection.setBaseURI(baseURI);
        emit BaseUriSet(collectionId, baseURI);
    }

    function setURI(
        uint256 collectionId,
        uint256 releaseId,
        string memory releaseURI
    ) external onlyOwner {
        HyperyToken collection = getCollection(collectionId);
        collection.setURI(releaseId, releaseURI);
        emit ReleaseUriSet(collectionId, releaseId, releaseURI);
    }

    function setURIAdmin(
        uint256 collectionId,
        uint256 releaseId,
        string memory releaseURI
    ) external virtual override onlyAdmin {
        HyperyToken collection = getCollection(collectionId);
        collection.setURI(releaseId, releaseURI);
        emit ReleaseUriSet(collectionId, releaseId, releaseURI);
    }

    function uri(uint256 collectionId, uint256 releaseId)
        external
        view
        returns (string memory)
    {
        HyperyToken collection = getCollection(collectionId);

        require(releaseId < collection.releaseIndex(), ERR_RELEASE_NOT_VALID);

        return collection.uri(releaseId);
    }

    function royaltyInfo(
        uint256 collectionId,
        uint256 releaseId,
        uint256 value
    )
        external
        view
        virtual
        returns (address royaltyAddress, uint256 royaltyValue)
    {
        HyperyToken collection = getCollection(collectionId);

        require(releaseId < collection.releaseIndex(), ERR_RELEASE_NOT_VALID);

        (royaltyAddress, royaltyValue) = collection.getRoyaltyInfo(
            releaseId,
            value
        );
    }
}

