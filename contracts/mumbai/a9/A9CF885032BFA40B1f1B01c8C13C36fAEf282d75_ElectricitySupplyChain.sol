        //     uint256 consumed = currentTime - consumer.startCycleTime;
        //     totalEnergyConsumedByConsumers += consumed;
        // }
        // return totalEnergyConsumedByConsumers;
        return distributors[_distributorIndex].consumerIds;
    }

