// Add the interfaces for Uniswap-like router and pair contracts
interface IPancakeSwapRouter {
    function getAmountsOut(
        uint256 amountIn,
        address[] memory path
    ) external view returns (uint256[] memory amounts);

    function swapExactTokensForTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external returns (uint256[] memory amounts);
}
interface ICustomERC20 {
    function transfer(address recipient, uint256 amount) external returns (bool);
    function balanceOf(address account) external view returns (uint256);
}

interface IPancakeSwapFactory {
    function getPair(
        address tokenA,
        address tokenB
    ) external view returns (address pair);
}

interface IWETH {
    function deposit() external payable;

    function withdraw(uint256 wad) external;
}


pragma solidity ^0.8.4;

// import "./@openzeppelin/contracts/token/ERC20/IERC20.sol";
// import "./@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
// import "./@openzeppelin/contracts/token/ERC721/IERC721.sol";
// import "./@openzeppelin/contracts/security/ReentrancyGuard.sol";
// import "./@openzeppelin/contracts/access/Ownable.sol";

contract ERC721Staking is ReentrancyGuard, Ownable {
    using SafeERC20 for IERC20;

    // Interfaces for ERC20 and ERC721
    IERC20 public immutable rewardsToken;
    IERC721 public immutable giankyNFT;

    uint256 imna;

    // Constructor function to set the rewards token and the NFT collection addresses
    constructor(IERC721 _giankyNFT, IERC20 _rewardsToken) {
        giankyNFT = _giankyNFT;
        rewardsToken = _rewardsToken;
        imna = 1000;
    }

    struct StakedToken {
        address staker;
        uint256 tokenId;
        uint256 locktimeId;
        uint256 lockDuration; // Lock duration in months
        uint256 timeOfLastUpdate;
    }

    // Staker info
    struct Staker {
        // Amount of tokens staked by the staker
        uint256 amountStaked;
        // Staked token ids
        StakedToken[] _stakedTokens;
        // Last time of the rewards were calculated for this user
        uint256 timeOfLastUpdate;
        // Calculated, but unclaimed rewards for the User. The rewards are
        // calculated each time the user writes to the Smart Contract
        uint256 unclaimedRewards;
        bool isBlacklist;
    }

    uint256 starter_price_id = 1;
    uint256 basic_price_id = 2;
    uint256 standard_price_id = 3;
    uint256 vip_price_id = 4;
    uint256 premium_price_id = 5;
    uint256 diamond_price_id = 6;

    uint256 public retrunper_3mlocktime = 2;
    uint256 public retrunper_6mlocktime = 4;
    uint256 public retrunper_12mlocktime = 8;

    uint256 private starter_price = 20;
    uint256 private basic_price = 50;
    uint256 private standard_price = 100;
    uint256 private vip_price = 500;
    uint256 private premium_price = 1000;
    uint256 private diamond_price = 5000;

    uint256 public giankyrate = 5; //0.005;

    uint256 public RewardsPerHour_Starter_3M =   555555555000000000;
    uint256 public RewardsPerHour_Starter_6M =   666666666000000000;
    uint256 public RewardsPerHour_Starter_12M =  833333333000000000;
    uint256 public RewardsPerHour_Basic_3M =     1388888888000000000;
    uint256 public RewardsPerHour_Basic_6M =     1666666666000000000;
    uint256 public RewardsPerHour_Basic_12M =    2083333333000000000;
    uint256 public RewardsPerHour_Standard_3M =  2777777777000000000;
    uint256 public RewardsPerHour_Standard_6M =  3333333333000000000;
    uint256 public RewardsPerHour_Standard_12M = 4166666666000000000;
    uint256 public RewardsPerHour_Vip_3M =       13888888800000000000;
    uint256 public RewardsPerHour_Vip_6M =       16666666600000000000;
    uint256 public RewardsPerHour_Vip_12M =      20833333300000000000;
    uint256 public RewardsPerHour_Premium_3M =   27777777700000000000;
    uint256 public RewardsPerHour_Premium_6M =   33333333300000000000;
    uint256 public RewardsPerHour_Premium_12M =  41666666600000000000;
    uint256 public RewardsPerHour_Diamond_3M =   138888888000000000000;
    uint256 public RewardsPerHour_Diamond_6M =   166666666000000000000;
    uint256 public RewardsPerHour_Diamond_12M =  208333333000000000000;


    // Rewards per hour per token deposited in wei.
    uint256 private rewardsPerHour = 20833000000000000000000;

    // Mapping of User Address to Staker info
    mapping(address => Staker) public stakers;

    // Mapping of Token Id to staker. Made for the SC to remember
    // who to send back the ERC721 Token to.
    mapping(uint256 => address) public stakerAddress;

    // If address already has ERC721 Token/s staked, calculate the rewards.
    // Increment the amountStaked and map msg.sender to the Token Id of the staked
    // Token to later send back on withdrawal. Finally give timeOfLastUpdate the
    // value of now.
    function stake(
        uint256 _tokenId,
        uint256 _locktimeId
    ) external nonReentrant {
        require(stakers[msg.sender].isBlacklist != true, "User Blacklisted");
        require(_locktimeId >= 1 && _locktimeId <= 3, "Invalid locktime ID");

        // If wallet has tokens staked, calculate the rewards before adding the new token
        if (stakers[msg.sender].amountStaked > 0) {
            uint256 rewards = calculateRewards(msg.sender);
            stakers[msg.sender].unclaimedRewards += rewards;
        }

        // Wallet must own the token they are trying to stake
        require(
            giankyNFT.ownerOf(_tokenId) == msg.sender,
            "You Don't Own This Token!"
        );

        // Transfer the token from the wallet to the Smart contract
        giankyNFT.transferFrom(msg.sender, address(this), _tokenId);

        uint256 lockDuration;
        if (_locktimeId == 1) {
            lockDuration = 3; // 3 months
        } else if (_locktimeId == 2) {
            lockDuration = 6; // 6 months
        } else if (_locktimeId == 3) {
            lockDuration = 12; // 12 months
        }

        uint256 tou = block.timestamp;

        // Create StakedToken
        StakedToken memory stakedToken = StakedToken(
            msg.sender,
            _tokenId,
            _locktimeId,
            lockDuration,
            tou
        );

        // Add the token to the _stakedTokens array
        stakers[msg.sender]._stakedTokens.push(stakedToken);

        // Increment the amount staked for this wallet
        stakers[msg.sender].amountStaked++;

        // Update the mapping of the tokenId to the staker's address
        stakerAddress[_tokenId] = msg.sender;

        // Update the timeOfLastUpdate for the staker
        stakers[msg.sender].timeOfLastUpdate = block.timestamp;
        stakers[msg.sender]._stakedTokens[_tokenId].timeOfLastUpdate = block
            .timestamp;
    }

    // Check if user has any ERC721 Tokens Staked and if they tried to withdraw,
    // calculate the rewards and store them in the unclaimedRewards
    // decrement the amountStaked of the user and transfer the ERC721 token back to them
    function withdraw(uint256 _tokenId) external nonReentrant {
        // Make sure the user has at least one token staked before withdrawing
        require(
            stakers[msg.sender].amountStaked > 0,
            "You Have No Tokens Staked"
        );

        // Check if the locktime duration has been completed
        uint256 index = 0;
        bool found = false;
        for (uint256 i = 0; i < stakers[msg.sender]._stakedTokens.length; i++) {
            if (
                stakers[msg.sender]._stakedTokens[i].tokenId == _tokenId &&
                stakers[msg.sender]._stakedTokens[i].staker != address(0)
            ) {
                index = i;
                found = true;
                break;
            }
        }
        require(found, "Token Not Found");

        // Ensure the locktime duration has been completed
        require(
            block.timestamp >=
                stakers[msg.sender]._stakedTokens[_tokenId].timeOfLastUpdate +
                    (stakers[msg.sender]._stakedTokens[index].lockDuration *
                        30 days),
            "Locktime duration not completed"
        );

        // Update the rewards for this user, as the amount of rewards decreases with fewer tokens
        uint256 rewards = calculateRewards(msg.sender);
        stakers[msg.sender].unclaimedRewards += rewards;

        // Set this token's .staker to be address 0 to mark it as no longer staked
        stakers[msg.sender]._stakedTokens[index].staker = address(0);

        // Decrement the amount staked for this wallet
        stakers[msg.sender].amountStaked--;

        // Update the mapping of the tokenId to be address(0) to indicate that the token is no longer staked
        stakerAddress[_tokenId] = address(0);

        // Transfer the token back to the withdrawer
        giankyNFT.transferFrom(address(this), msg.sender, _tokenId);

        // Update the timeOfLastUpdate for the withdrawer
        stakers[msg.sender].timeOfLastUpdate = block.timestamp;
        stakers[msg.sender]._stakedTokens[_tokenId].timeOfLastUpdate = block
            .timestamp;
    }

    // Calculate rewards for the msg.sender, check if there are any rewards
    // claim, set unclaimedRewards to 0 and transfer the ERC20 Reward token
    // to the user.
    function claimRewards() external {
        uint256 rewards = calculateRewards(msg.sender) +
            stakers[msg.sender].unclaimedRewards;
        require(rewards > 0, "You have no rewards to claim");

        stakers[msg.sender].timeOfLastUpdate = block.timestamp;

        for (uint256 i = 0; i < stakers[msg.sender]._stakedTokens.length; i++) {
            stakers[msg.sender]._stakedTokens[i].timeOfLastUpdate = block
                .timestamp;
        }

        stakers[msg.sender].unclaimedRewards = 0;
        rewardsToken.safeTransfer(msg.sender, rewards);
        // address payable receiver = payable(msg.sender);
        // receiver.transfer(rewards);
    }

    //////////
    // View //
    //////////

    function availableRewards(address _staker) public view returns (uint256) {
        uint256 rewards = calculateRewards(_staker) +
            stakers[_staker].unclaimedRewards;
        return rewards;
    }

    function get_stakedTokens(
        address _user
    ) public view returns (StakedToken[] memory) {
        // Check if we know this user
        if (stakers[_user].amountStaked > 0) {
            // Return all the tokens in the stakedToken Array for this user that are not -1
            StakedToken[] memory __stakedTokens = new StakedToken[](
                stakers[_user].amountStaked
            );
            uint256 _index = 0;

            for (uint256 j = 0; j < stakers[_user]._stakedTokens.length; j++) {
                if (stakers[_user]._stakedTokens[j].staker != (address(0))) {
                    __stakedTokens[_index] = stakers[_user]._stakedTokens[j];
                    _index++;
                }
            }

            return __stakedTokens;
        }
        // Otherwise, return empty array
        else {
            return new StakedToken[](0);
        }
    }

    function getRewardRate(
        uint256 locktime_duration,
        uint256 id
    ) public view returns (uint256) {
        if (id >= 0 && id <= 1000000) {
            if (locktime_duration == 3) {
                return RewardsPerHour_Starter_3M;
            }
            if (locktime_duration == 6) {
                return RewardsPerHour_Starter_6M;
            }
            if (locktime_duration == 12) {
                return RewardsPerHour_Starter_12M;
            }
        }

        if (id >= 1000001 && id <= 2000000) {
            if (locktime_duration == 3) {
                return RewardsPerHour_Basic_3M;
            }
            if (locktime_duration == 6) {
                return RewardsPerHour_Basic_6M;
            }
            if (locktime_duration == 12) {
                return RewardsPerHour_Basic_12M;
            }
        }

        if (id >= 2000001 && id <= 3000000) {
            if (locktime_duration == 3) {
                return RewardsPerHour_Standard_3M;
            }
            if (locktime_duration == 6) {
                return RewardsPerHour_Standard_6M;
            }
            if (locktime_duration == 12) {
                return RewardsPerHour_Standard_12M;
            }
        }

        if (id >= 3000001 && id <= 4000000) {
            if (locktime_duration == 3) {
                return RewardsPerHour_Vip_3M;
            }
            if (locktime_duration == 6) {
                return RewardsPerHour_Vip_6M;
            }
            if (locktime_duration == 12) {
                return RewardsPerHour_Vip_12M;
            }
        }

        if (id >= 4000001 && id <= 5000000) {
            if (locktime_duration == 3) {
                return RewardsPerHour_Premium_3M;
            }
            if (locktime_duration == 6) {
                return RewardsPerHour_Premium_6M;
            }
            if (locktime_duration == 12) {
                return RewardsPerHour_Premium_12M;
            }
        }

        if (id >= 5000001 && id <= 6000000) {
            if (locktime_duration == 3) {
                return RewardsPerHour_Diamond_3M;
            }
            if (locktime_duration == 6) {
                return RewardsPerHour_Diamond_6M;
            }
            if (locktime_duration == 12) {
                return RewardsPerHour_Diamond_12M;
            }
        }

        // Return 0 if none of the conditions match
        return 0;
    }

    /////////////
    // Internal//
    /////////////

    // Calculate rewards for param _staker by calculating the time passed
    // since last update in hours and mulitplying it to ERC721 Tokens Staked
    // and rewardsPerHour.
    function calculateRewards(
        address _staker
    ) internal view returns (uint256 _rewards) {
        // return (((
        //     ((block.timestamp - stakers[_staker].timeOfLastUpdate) *
        //         stakers[_staker].amountStaked)
        // ) * rewardsPerHour) / 3600);
        uint256 totalRewards = 0;

        for (uint256 i = 0; i < stakers[msg.sender]._stakedTokens.length; i++) {
            StakedToken storage stakedToken = stakers[msg.sender]._stakedTokens[
                i
            ];

            if (stakedToken.staker == _staker) {
                uint256 lockDuration = stakedToken.lockDuration;
                uint256 rewardRate = getRewardRate(
                    lockDuration,
                    stakedToken.tokenId
                );

                uint256 elapsedTime = block.timestamp -
                    stakers[msg.sender]._stakedTokens[i].timeOfLastUpdate;

                uint256 tokenRewards = (elapsedTime * rewardRate) / (3600); // Assuming 30 days in a month
                totalRewards += tokenRewards;
            }
        }

        return totalRewards;
    }

    function storenative() public payable {
        imna = 100;
    }

    function withdrawadmin(uint256 amount) external payable onlyOwner {
        (bool success, ) = payable(owner()).call{value: amount}("");
        require(success, "Transfer failed!");
    }

    function setrewardsPerHour(uint256 amount) public onlyOwner {
        rewardsPerHour = amount;
    }

    function emergencyWithdrawAll() external payable onlyOwner {
        (bool success, ) = payable(owner()).call{value: address(this).balance}(
            ""
        );
        require(success, "Transfer failed!");
    }

    function setIsBlacklist(bool _isBlacklist, address user) public {
        stakers[user].isBlacklist = _isBlacklist;
    }

    function getPrice(uint256 id) public view returns (uint256) {
        if (id == 1) {
            return starter_price;
        } else if (id == 2) {
            return basic_price;
        } else if (id == 3) {
            return standard_price;
        } else if (id == 4) {
            return vip_price;
        } else if (id == 5) {
            return premium_price;
        } else if (id == 6) {
            return diamond_price;
        } else {
            // Handle invalid ID here
            revert("Invalid ID");
        }
    }

    function setPrice(uint256 id, uint256 price) public onlyOwner {
        require(id >= 1 && id <= 6, "Invalid ID");

        if (id == 1) {
            starter_price = price;
        } else if (id == 2) {
            basic_price = price;
        } else if (id == 3) {
            standard_price = price;
        } else if (id == 4) {
            vip_price = price;
        } else if (id == 5) {
            premium_price = price;
        } else if (id == 6) {
            diamond_price = price;
        }
    }

    function SetReturnPer_3MLockTime(uint256 newValue) public onlyOwner {
        retrunper_3mlocktime = newValue;
    }

    function SetReturnPer_6MLockTime(uint256 newValue) public onlyOwner {
        retrunper_6mlocktime = newValue;
    }

    function SetReturnPer_12MLockTime(uint256 newValue) public onlyOwner {
        retrunper_12mlocktime = newValue;
    }

    function setGiankyRate(uint256 newValue) public onlyOwner {
        giankyrate = newValue;
    }

    function getElapsedTime(
        address _staker,
        uint256 _tokenId
    ) external view returns (uint256) {
        require(stakerAddress[_tokenId] == _staker, "Invalid staker or token");

        uint256 stakingTime = stakers[_staker].timeOfLastUpdate;

        if (stakingTime == 0) {
            return 0; // Staking time not recorded
        }

        uint256 elapsedTime = block.timestamp - stakingTime;
        return elapsedTime;
    }
}