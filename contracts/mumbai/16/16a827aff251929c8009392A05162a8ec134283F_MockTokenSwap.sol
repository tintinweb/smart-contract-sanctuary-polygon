pragma solidity 0.8.19;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "hardhat/console.sol";

contract MockTokenSwap is Ownable {
    using SafeERC20Upgradeable for IERC20Upgradeable;

    event MockTokensReceived(uint256 amount, address sender);
    event MockTokensSwapped(
        uint256 amountIn,
        uint256 amountOut,
        uint256 poolBalance,
        address sender
    );
    event BalanceWithdrawn(uint256 amount);

    struct Approx24HBalance {
        uint256 amountMockTkn;
        uint256 timestamp;
    }

    address public immutable mockTokenAddr;
    uint256 public immutable maxDailyMockTokenSwapAmount;
    uint256 public immutable d18MaticToMockConversion;
    uint256 private constant d18 = 1e18;
    mapping(address => Approx24HBalance) movingBalance;

    constructor(
        address _mockTokenAddr,
        uint256 _d18MaticToMockConversion,
        uint256 _maxDailyMockTokenSwapAmount
    ) Ownable() {
        mockTokenAddr = _mockTokenAddr;
        d18MaticToMockConversion = _d18MaticToMockConversion;
        maxDailyMockTokenSwapAmount = _maxDailyMockTokenSwapAmount;
    }

    /**
     * Swap Matic vs mocktoken at an exchange rate d18MaticToMockConversion
     */
    function swapToMockToken() external payable {
        IERC20Upgradeable mockToken = IERC20Upgradeable(mockTokenAddr);

        uint256 balance = mockToken.balanceOf(address(this));
        uint256 amountToReceive = (msg.value * d18MaticToMockConversion) / d18;
        if (amountToReceive > maxDailyMockTokenSwapAmount) {
            amountToReceive = maxDailyMockTokenSwapAmount;
        }
        Approx24HBalance storage bal = movingBalance[msg.sender];
        if (bal.timestamp > 0) {
            // calculate amount assuming expiry in 24h
            uint256 t0 = block.timestamp - 86400;
            uint256 weight = bal.timestamp < t0 ? 0 : bal.timestamp - t0;
            uint256 amountConsumed = (weight * bal.amountMockTkn) / 86400;
            uint256 maxAmount = maxDailyMockTokenSwapAmount - amountConsumed;
            amountToReceive = amountToReceive > maxAmount
                ? maxAmount
                : amountToReceive - amountConsumed;
            bal.timestamp = block.timestamp;
            bal.amountMockTkn = amountConsumed + amountToReceive;
        } else {
            bal.timestamp = block.timestamp;
            bal.amountMockTkn = amountToReceive;
        }
        require(balance >= amountToReceive, "not enough reserves");
        mockToken.transfer(msg.sender, amountToReceive);
        emit MockTokensSwapped(msg.value, amountToReceive, balance - amountToReceive, msg.sender);
    }

    /**
     * owner can withdraw matic
     */
    function withdraw() external onlyOwner {
        // get the amount of Matic stored in this contract
        uint256 amount = address(this).balance;
        (bool success, ) = this.owner().call{ value: amount }("");
        require(success, "withdraw failed");
        emit BalanceWithdrawn(amount);
    }

    /**
     * Here we deposit mock tokens that can be claimed in exchange for Matic
     * Needs approve
     * @param _amount   amount of mock token to be deposited
     */
    function depositMockToken(uint256 _amount) external {
        IERC20Upgradeable mockToken = IERC20Upgradeable(mockTokenAddr);
        mockToken.transferFrom(msg.sender, address(this), _amount);
        emit MockTokensReceived(_amount, msg.sender);
    }
}

