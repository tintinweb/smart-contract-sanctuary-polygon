        require(currentToken.tokenID > 0, "Test: invalid tokenID");
        require(currentToken.currentlyListed, "Test: invalid listing");

        uint256 totalPrice = 0;
        ShareItem memory firstShare = shares[_tokenID][1];

        address owner = firstShare.shareOwner;

        for (uint256 i = 1; i <= currentToken.totalShares; i++) {
            ShareItem memory currentShare = shares[_tokenID][i];

            // If one of the fractions is not available, transaction can't proceed

            require(currentShare.tokenID > 0, "Test: invalid tokenID");
            require(currentShare.shareID > 0, "Test: invalid shareID");

            require(currentShare.currentlyListed, "Test: invalid listing");

            require(
                currentShare.shareOwner == owner,
                "Test: all shares do not have the same owner"
            );
            totalPrice += currentShare.price;
        }

