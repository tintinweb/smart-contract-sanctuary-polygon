pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./interfaces/IAbundanceToken.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router01.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "./AbundanceMembershipManager.sol";

contract AbundanceEmpowermentBoosterManager is Ownable {
  using SafeERC20 for IERC20;
  address organizationAddress;

  IUniswapV2Router01 poolRouter;
  AbundanceMembershipManager membershipManager;

  bool private inSwapAndLiquify;

  // Abundance Sponsor Percentages
  uint256 minSponsorPercentage = 0;
  uint256 maxSponsorPercentage = 5000;
  uint256 sponsorPercentage = 3000;

  // Abundance Recruit Percentages
  uint256 minRecruitPercentage = 3000;
  uint256 maxRecruitPercentage = 7000;
  uint256 recruitPercentage = 4000;

  // Abundance State Variable Percentages
  uint256 abundanceStateVariablePercentage = 2000;
  uint256 minAbundanceStatePercentage = 1000;
  uint256 maxAbundanceStatePercentage = 8200;

  // Organization Percentages
  uint256 organizationPercentage = 1000;
  uint256 minOrganizationPercentage = 800;
  uint256 maxOrganizationPercentage = 1500;

  uint256 PERCENTAGE_DENOMINATOR = 10000;

  // admin address
  address ADMIN;

  // USDT
  IERC20 public paymentToken;

  IAbundanceToken public abundanceToken;

  // amount in USDC
  uint256 public minBoosterFee = 37e18;
  uint256 public minRecruits = 0;
  uint256 public minRecruitAmount = 1e18;

  // Zero means no limit;
  uint256 public maxRecruits = 0;

  // Emits when membership is bought or renewed
  event EmpBoosterBought(uint256 indexed membershipId, address indexed sponsor, uint256 amount);

  // Emits when bought a membership or renewed
  event RecruitBoosterShare(uint256 indexed membershipId, address indexed recruit, uint256 amount);

  constructor(
    IERC20 _paymentToken,
    address _organizationAddress,
    IAbundanceToken _abundanceToken,
    address _poolRouter,
    address _membershipManager,
    address _admin
  ) {
    ADMIN = _admin;
    paymentToken = _paymentToken;
    organizationAddress = _organizationAddress;
    abundanceToken = _abundanceToken;
    membershipManager = AbundanceMembershipManager(_membershipManager);

    poolRouter = IUniswapV2Router01(_poolRouter);
  }

  modifier onlyAdmin() {
    require(msg.sender == ADMIN, "only admin can call this method");
    _;
  }

  modifier lockTheSwap() {
    inSwapAndLiquify = true;
    _;
    inSwapAndLiquify = false;
  }

  //* internal method to check the Empowerment Booster exist
  function empBoosterExist(address sponsor) public view returns (bool) {
    // bool found = false;
    // for (uint256 i = 0; i < empBoosters.length; i++) {
    //   if (empBoosters[i].sponsor == sponsor) {
    //     found = true;
    //     break;
    //   }
    // }
    // return found;
  }

  //* Buy Empowerment Booster method
  function buyEmpBooster(
    // EmpowermentBooster struct include: sponsor's address, Empowerment Booster amount and expiry
    uint256 membershipId,
    // empowerment Booster Amount
    uint256 amount,
    // recruits list for the Empowerment Booster
    address[] memory recruitsList,
    // recruits loan amount according to the recruitsList argument
    uint256[] memory recruitsAmount,
    // recruits percentage
    uint256 recruitsAmountPercentage
  ) external onlyAdmin {
    AbundanceMembershipManager.Membership memory membership = membershipManager.getMembership(membershipId);

    // check whether the membership amount provided is enough for the Empowerment Booster
    require(amount >= minBoosterFee, "insufficient Empowerment Booster amount");

    // check whether the membership is expired or not
    require(membership.expiry > block.timestamp, "membership is expired");

    address sponsor = membershipManager.getSponsor(membershipId);
    // calling processPayment method to distribute the Empowerment Booster amount among recruits, organization and abundance state variable
    processPayment(sponsor, amount, recruitsList, recruitsAmount, recruitsAmountPercentage);

    // looping recruits to create Recruit struct and link it to Empowerment Booster id
    for (uint i = 0; i < recruitsList.length; i++) {
      // emitting Recruited event when assured the recruit is added to the Empowerment Booster
      emit RecruitBoosterShare(membershipId, recruitsList[i], recruitsAmount[i]);
    }

    // emitting EmpBoosterRenewed i.e Empowerment Booster is bought
    emit EmpBoosterBought(membershipId, sponsor, amount);
  }

  //* Internal method for payment processing used in buyEmpowermentBooster
  function processPayment(
    // membership sponsor's address
    address sponsor,
    // amount for Empowerment Booster
    uint256 amount,
    // recruits list for the Empowerment Booster
    address[] memory recruitsList,
    // recruits loan amount according to the recruitsList argument
    uint256[] memory recruitAmount,
    // recruits percentage
    uint256 recruitsAmountPercentage
  ) internal {
    require(
      recruitsAmountPercentage >= minRecruitPercentage,
      "recruits loan percentage is less than minimum recruit percentage"
    );

    require(
      recruitsAmountPercentage <= maxRecruitPercentage,
      "recruits loan percentage is greater than maximum recruit percentage"
    );

    uint256 totalPercentage = recruitsAmountPercentage + organizationPercentage + abundanceStateVariablePercentage;

    require(
      totalPercentage <= PERCENTAGE_DENOMINATOR,
      "recruits percentage is greater than maximum recruit percentage"
    );

    // calculating total recruits amount according to 30% of Empowerment Booster amount
    uint256 totalRecruitsAmount = (recruitsAmountPercentage * amount) / PERCENTAGE_DENOMINATOR;
    // calculating organization amount according to 10% of Empowerment Booster amount
    uint256 orgAmount = (organizationPercentage * amount) / PERCENTAGE_DENOMINATOR;
    // calculating abundance token variable according to 20% of Empowerment Booster amount
    uint256 abundanceAmount = (abundanceStateVariablePercentage * amount) / PERCENTAGE_DENOMINATOR;

    uint256 sumRecruitAmount = 0;

    // transfering organization amount
    paymentToken.safeTransferFrom(sponsor, organizationAddress, orgAmount);

    for (uint256 i = 0; i < recruitsList.length; i++) {
      // checking recruit's amount is equal or greater than minimum recruit's amount
      require(recruitAmount[i] >= minRecruitAmount, "recruit amount is less than minimum recruit amount");

      // transfering the amount to recruits
      paymentToken.safeTransferFrom(sponsor, recruitsList[i], recruitAmount[i]);
      sumRecruitAmount += recruitAmount[i];
    }

    // checking sum of total recruit amount is less than or equal to the 30% of the Empowerment Booster amount
    require(
      sumRecruitAmount <= totalRecruitsAmount,
      "total sum of recruit amount exceed Recruit Percentage Amount of the Empowerment Booster Amount"
    );

    // adding remaing recruits amount to liquidity pool
    if (sumRecruitAmount < totalRecruitsAmount) {
      swapAndLiquify(totalRecruitsAmount - sumRecruitAmount);
    }

    console.log(abundanceAmount);
    console.log(paymentToken.balanceOf(address(this)));

    address[] memory path = new address[](2);
    path[0] = (address(paymentToken));
    path[1] = address(abundanceToken);

    if (abundanceAmount > 0) {
      paymentToken.safeTransferFrom(sponsor, address(this), abundanceAmount);
      // paymentToken.approve(address(poolRouter), abundanceAmount);
      // poolRouter.swapExactTokensForTokens(abundanceAmount, 0, path, sponsor, block.timestamp + (30 * 60));
      swapExactTokensForTokens(abundanceAmount);
    }
  }

  //* function to modify abundanceStateVariablePercentage value
  function setStateVariableAmountPercentage(uint256 _percentage) external onlyOwner {
    require(_percentage >= 1000, "Abundance state variable percentage should be greater than and equals to 10%");
    require(_percentage <= 2000, "Abundance state variable percentage should be less than and equals to 20%");
    abundanceStateVariablePercentage = _percentage;
  }

  function getMinBoosterFee() public view returns (uint256) {
    return minBoosterFee;
  }

  function setMinBoosterFee(uint256 _minBoosterFee) external onlyOwner {
    minBoosterFee = _minBoosterFee;
  }

  function getMinMaxOrganizationPercentages() public view returns (uint256, uint256) {
    return (minOrganizationPercentage, maxOrganizationPercentage);
  }

  function setMinMaxOrganizationPercentages(uint256 _minPercentage, uint256 _maxPercentage) external onlyAdmin {
    require(_maxPercentage > _minPercentage, "Max Percentage can not be equal or less than Min Percentage");
    minOrganizationPercentage = _minPercentage;
    maxOrganizationPercentage = _maxPercentage;
  }

  function getMinMaxRecruitPercentages() public view returns (uint256, uint256) {
    return (minRecruitPercentage, maxRecruitPercentage);
  }

  function setMinMaxRecruitPercentages(uint256 _minPercentage, uint256 _maxPercentage) external onlyAdmin {
    require(_maxPercentage > _minPercentage, "Max Percentage can not be equal or less than Min Percentage");
    minRecruitPercentage = _minPercentage;
    maxRecruitPercentage = _maxPercentage;
  }

  function getMinMaxAbundanceStatePercentages() public view returns (uint256, uint256) {
    return (minAbundanceStatePercentage, maxAbundanceStatePercentage);
  }

  function setMinMaxAbundanceStatePercentages(uint256 _minPercentage, uint256 _maxPercentage) external onlyAdmin {
    require(_maxPercentage > _minPercentage, "Max Percentage can not be equal or less than Min Percentage");
    minAbundanceStatePercentage = _minPercentage;
    maxAbundanceStatePercentage = _maxPercentage;
  }

  function getMinMaxSponsorPercentages() public view returns (uint256, uint256) {
    return (minSponsorPercentage, maxSponsorPercentage);
  }

  function setMinMaxSponsorPercentages(uint256 _minPercentage, uint256 _maxPercentage) external onlyAdmin {
    require(_maxPercentage > _minPercentage, "Max Percentage can not be equal or less than Min Percentage");
    minSponsorPercentage = _minPercentage;
    maxSponsorPercentage = _maxPercentage;
  }

  function getSponsorDefaultPercentage() public view returns (uint256) {
    return sponsorPercentage;
  }

  function getRecruitDefaultPercentage() public view returns (uint256) {
    return recruitPercentage;
  }

  function getAbundanceStateDefaultPercentage() public view returns (uint256) {
    return abundanceStateVariablePercentage;
  }

  function getOrganizationDefaultPercentage() public view returns (uint256) {
    return organizationPercentage;
  }

  function setPercentages(
    uint256 _sponsorPercentage,
    uint256 _recruitPercentage,
    uint256 _abundanceStatePercentage,
    uint256 _organizationPercentage
  ) external onlyOwner {
    uint256 totalPercentages = _sponsorPercentage +
      _recruitPercentage +
      _abundanceStatePercentage +
      _organizationPercentage;

    require(totalPercentages == 10000, "sum of all given percentages must be equal to 100%");

    require(_sponsorPercentage >= minSponsorPercentage, "sponsor percentage is less than minimum sponsor percentage");
    require(
      _sponsorPercentage <= maxSponsorPercentage,
      "sponsor percentage is greater than maximum sponsor percentage"
    );

    sponsorPercentage = _sponsorPercentage;

    require(_recruitPercentage >= minRecruitPercentage, "recruit percentage is less than minimum recruit percentage");
    require(
      _recruitPercentage <= maxRecruitPercentage,
      "recruit percentage is greater than maximum recruit percentage"
    );

    recruitPercentage = _recruitPercentage;

    require(
      _organizationPercentage >= minOrganizationPercentage,
      "organization percentage is less than minimum organization percentage"
    );
    require(
      _organizationPercentage <= maxOrganizationPercentage,
      "organization percentage is greater than maximum organization percentage"
    );

    organizationPercentage = _organizationPercentage;

    require(
      _abundanceStatePercentage >= minAbundanceStatePercentage,
      "abundance state percentage is less than minimum abundance state percentage"
    );
    require(
      _abundanceStatePercentage <= maxAbundanceStatePercentage,
      "abundance state percentage is greater than maximum abundance state percentage"
    );

    abundanceStateVariablePercentage = _abundanceStatePercentage;
  }

  function swapExactTokensForTokens(uint256 tokenAmount) private returns (uint256 amountAT) {
    // generate the uniswap pair path of token -> weth
    address[] memory path = new address[](2);
    path[0] = (address(paymentToken));
    path[1] = (address(abundanceToken));

    paymentToken.approve(address(poolRouter), tokenAmount);

    // make the swap
    amountAT = poolRouter.swapExactTokensForTokens(
      tokenAmount,
      0, // accept any amount of ETH
      path,
      address(this),
      block.timestamp
    )[1];
  }

  function swapAndLiquify(uint256 balanceUSDT) private lockTheSwap {
    if (balanceUSDT > 0) {
      // split the USDT Amount into halves
      uint256 half = balanceUSDT / 2;
      uint256 otherHalf = balanceUSDT - half;

      // swap USDT tokens for AT tokens
      uint256 amountAT = swapExactTokensForTokens(half);

      // add liquidity to uniswap
      addLiquidity(otherHalf, amountAT);
    }
  }

  function addLiquidity(uint256 amountUSDT, uint256 amountAT) private {
    address tokenAT = address(abundanceToken);
    address tokenUSDT = address(paymentToken);

    // console.log(tokenAT);

    paymentToken.approve(address(poolRouter), amountUSDT);
    abundanceToken.approve(address(poolRouter), amountAT);

    // console.log("Amount USDT && AT", amountUSDT, amountAT);
    // console.log("AT balance", abdToken.balanceOf(address(this)));
    // console.log("USDT balance", usdtToken.balanceOf(address(this)));

    // add the liquidity
    poolRouter.addLiquidity(
      tokenUSDT,
      tokenAT,
      amountUSDT,
      amountAT,
      0, // slippage is unavoidable
      0, // slippage is unavoidable
      address(this),
      block.timestamp + 30 * 60
    );
  }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./interfaces/IAbundanceMembershipNFT.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router01.sol";
import "hardhat/console.sol";
import "./interfaces/IAbundanceToken.sol";

contract AbundanceMembershipManager is Ownable {
  using SafeERC20 for IERC20;

  bool private inSwapAndLiquify;

  // organization's address
  address organizationAddress;

  IUniswapV2Router01 poolRouter;

  // membership trial check
  bool trial;

  // Auto mint toggle for abundance token on renewal
  bool autoMintAbundanceToken;

  struct Recruit {
    uint256 expiry;
    uint256 amount;
  }

  struct Membership {
    uint256 tokenId;
    uint256 amount;
    uint256 expiry;
  }

  // Abundance Sponsor Percentages
  uint256 sponsorPercentage = 4000;
  uint256 minSponsorPercentage = 0;
  uint256 maxSponsorPercentage = 5000;

  uint256 minRecruitPercentage = 3000;
  uint256 maxRecruitPercentage = 7000;
  uint256 recruitPercentage = 3000;

  uint256 abundanceStateVariablePercentage = 2000;
  uint256 minAbundanceStatePercentage = 1000;
  uint256 maxAbundanceStatePercentage = 2000;

  // Organization Percentages
  uint256 organizationPercentage = 1000;
  uint256 minOrganizationPercentage = 800;
  uint256 maxOrganizationPercentage = 1500;

  uint256 PERCENTAGE_DENOMINATOR = 10000;

  uint256 trialPeriod;

  // admin address
  address ADMIN;

  // USDT
  IERC20 public paymentToken;

  //AbundanceToken
  IERC20 public abundanceToken;

  // Membership NFT
  IAbundanceMembershipNFT public membershipNFT;

  // membership id => recruit address => recruit info
  mapping(uint256 => mapping(address => Recruit)) public recruits;

  // membership id => abundance token state variable
  mapping(uint256 => uint256) public abundanceTokenStates;

  // recruit address => is member
  mapping(address => bool) public recruitedMembers;

  // recruits address => membership id
  mapping(address => uint256) public recruitsMembership;

  // amount in USDC
  uint256 public minMembershipFee = 37e18;
  uint256 public minRecruits = 0;
  uint256 public minRecruitAmount = 1e18;
  uint256 public trialMembershipFee = 1e18;

  // Zero means no limit;
  uint256 public maxRecruits = 0;

  // membershipId => bool (for auto-renewal)
  mapping(uint256 => bool) public autoMembershipRenewal;

  // membershipId => remaining recruits Amount
  mapping(uint256 => uint256) public remainingRecruitsAmount;

  // membershipId => bool (on trial)
  mapping(uint256 => bool) public trialMemberships;

  // membership array to store all members struct
  Membership[] public memberships;

  // Emits when membership is bought or renewed
  event MembershipRenewed(
    uint256 indexed membershipId,
    uint256 indexed tokenId,
    address indexed sponsor,
    uint256 amount,
    uint256 expiry
  );

  // Emits when bought a membership or renewed
  event Recruited(uint256 indexed membershipId, address indexed recruit, uint256 amount, uint256 expiry);

  event Rewarded(
    uint256 indexed membershipId,
    address indexed sponsorAddress,
    uint256 rewardAmountUSDT,
    uint256 rewardAmountAbundanceToken
  );

  modifier onlyAdmin() {
    require(msg.sender == ADMIN, "only admin can call this method");
    _;
  }

  modifier lockTheSwap() {
    inSwapAndLiquify = true;
    _;
    inSwapAndLiquify = false;
  }

  constructor(
    IAbundanceMembershipNFT _membershipNFT,
    IERC20 _paymentToken,
    address _organizationAddress,
    address _abundanceToken,
    address _poolRouter,
    address _admin
  ) {
    ADMIN = _admin;
    membershipNFT = _membershipNFT;
    paymentToken = _paymentToken;
    organizationAddress = _organizationAddress;
    trialPeriod = 7 * 24 * 60 * 60;
    abundanceToken = IERC20(_abundanceToken);
    memberships.push(Membership(0, 0, 0));
    poolRouter = IUniswapV2Router01(_poolRouter);
  }

  //* internal method to check the membership exist
  function membershipExist(address sponsor) public view returns (bool) {
    bool found = false;

    for (uint256 i = 0; i < memberships.length; i++) {
      try membershipNFT.ownerOf(memberships[i].tokenId) returns (address owner) {
        if (owner == sponsor) {
          found = true;
          break;
        }
      } catch {
        continue;
      }
    }

    return found;
  }

  //* Buy Membership method
  function buyMembership(
    // membership struct include: sponsor's address, membership amount and expiry
    Membership memory membership,
    // recruits list for the membership
    address[] memory recruitsList,
    // recruits loan amount according to the recruitsList argument
    uint256[] memory recruitsAmount,
    // recruits loan percentage
    uint256 recruitsAmountPercentage
  ) external {
    // check whether the membership amount provided is enough for the membership
    require(membership.amount >= minMembershipFee, "insufficient membership amount");

    require(recruitsList.length == recruitsAmount.length, "no of recruit list is not equal to recruits amount list");

    _buyMembership(membership, recruitsList, recruitsAmount, recruitsAmountPercentage);
  }

  //* Buy Membership method (internal)
  function _buyMembership(
    // membership struct include: sponsor's address, membership amount and expiry
    Membership memory membership,
    // recruits list for the membership
    address[] memory recruitsList,
    // recruits loan amount according to the recruitsList argument
    uint256[] memory recruitsAmount,
    // recruits loan percentage
    uint256 recruitsAmountPercentage
  ) internal {
    // checking the membership already exists
    require(!membershipExist(msg.sender), "Membership already exist");
    require(membership.expiry > block.timestamp, "membership expiry can not be less than current time");

    // Minting ERC721 membership nft for the sponsor
    uint256 tokenId = membershipNFT.mintOne(msg.sender);

    membership.tokenId = tokenId;

    console.log(membership.tokenId);

    // adding membership details to the memberships array
    memberships.push(membership);

    // membership id
    uint256 membershipId = memberships.length - 1;

    // calling processPayment method to distribute the membership amount among recruits, organization and abundance state variable
    processPayment(membershipId, msg.sender, membership.amount, recruitsList, recruitsAmount, recruitsAmountPercentage);

    // looping recruits to create Recruit struct and link it to membership id
    for (uint i = 0; i < recruitsList.length; i++) {
      if (recruitedMembers[recruitsList[i]]) {
        if (
          recruitsMembership[recruitsList[i]] != membershipId &&
          recruits[recruitsMembership[recruitsList[i]]][recruitsList[i]].expiry > block.timestamp
        ) {
          revert("recruit already exist in another membership");
        }
      }

      recruits[membershipId][recruitsList[i]] = Recruit(membership.expiry, recruitsAmount[i]);

      // marking the address is recruited
      recruitedMembers[recruitsList[i]] = true;

      recruitsMembership[recruitsList[i]] = membershipId;

      // emitting Recruited event when assured the recruit is added to the membership
      emit Recruited(membershipId, recruitsList[i], recruitsAmount[i], membership.expiry);
    }

    // emitting MembershipRenewed i.e membership is bought
    emit MembershipRenewed(membershipId, tokenId, msg.sender, membership.amount, membership.expiry);
  }

  function buyTrialMembership(
    // membership fee
    uint256 amount
  ) external {
    require(trial, "Please turn on the trial membership before running this function");
    require(amount == trialMembershipFee, "Invalid trial membership fee");

    Membership memory membership = Membership({tokenId: 0, amount: amount, expiry: block.timestamp + trialPeriod});

    address[] memory recruitsList;
    uint256[] memory recruitsAmount;

    _buyMembership(membership, recruitsList, recruitsAmount, minRecruitPercentage);

    trialMemberships[memberships.length - 1] = true;
  }

  function autoRenewMembership(
    // membership id
    uint256 membershipId,
    // amount for renewed membership
    uint256 membershipAmount,
    // new membership expiry
    uint256 expiry,
    // recruits list for the membership
    address[] memory recruitsList,
    // recruits loan amount according to the recruitsList argument
    uint256[] memory recruitAmount,
    // recruits loan percentage
    uint256 recruitsAmountPercentage
  ) external onlyOwner {
    require(autoMembershipRenewal[membershipId], "Auto renewal off");
    require(recruitsList.length == recruitAmount.length, "no of recruit list is not equal to recruits amount list");
    _renewMembership(membershipId, membershipAmount, expiry, recruitsList, recruitAmount, recruitsAmountPercentage);
  }

  function renewMembership(
    // membership id
    uint256 membershipId,
    // amount for renewed membership
    uint256 membershipAmount,
    // new membership expiry
    uint256 expiry,
    // recruits list for the membership
    address[] memory recruitsList,
    // recruits loan amount according to the recruitsList argument
    uint256[] memory recruitAmount,
    // recruits loan percentage
    uint256 recruitsAmountPercentage
  ) external {
    // getting sponsor from membership id
    address sponsor = getSponsor(membershipId);
    // checking whether the person renewing the membership is the sponsor
    require(sponsor == msg.sender, "only sponsor can renew membership");

    _renewMembership(membershipId, membershipAmount, expiry, recruitsList, recruitAmount, recruitsAmountPercentage);
  }

  //* function to renew membership
  function _renewMembership(
    // membership id
    uint256 membershipId,
    // amount for renewed membership
    uint256 membershipAmount,
    // new membership expiry
    uint256 expiry,
    // recruits list for the membership
    address[] memory recruitsList,
    // recruits loan amount according to the recruitsList argument
    uint256[] memory recruitAmount,
    // recruits loan percentage
    uint256 recruitsAmountPercentage
  ) internal {
    require(isMembershipExpired(membershipId), "membership is not expired yet");

    // getting sponsor from membership id
    address sponsor = getSponsor(membershipId);

    // check whether the membership amount provided is enough for the membership
    require(membershipAmount >= minMembershipFee, "insufficient memebership amount");

    require(expiry > block.timestamp, "membership expiry can not be less than current time");

    // adding remaining recruits Amount to liquidity pool
    swapAndLiquify(remainingRecruitsAmount[membershipId]);

    remainingRecruitsAmount[membershipId] = 0;

    address[] memory tokens = new address[](2);
    tokens[0] = address(paymentToken);
    tokens[1] = address(abundanceToken);

    uint256 abdStateVariable = abundanceTokenStates[membershipId];

    // approving abundance state variable amount to uniswap router
    paymentToken.approve(address(poolRouter), abdStateVariable);

    if (autoMintAbundanceToken) {
      poolRouter.swapExactTokensForTokens(abdStateVariable, 0, tokens, sponsor, block.timestamp + (30 * 60));
    } else {
      poolRouter.swapExactTokensForTokens(abdStateVariable, 0, tokens, address(this), block.timestamp + (30 * 60))[1];
    }

    // calling processPayment method to distribute the membership amount among recruits, organization and abundance state variable
    processPayment(membershipId, sponsor, membershipAmount, recruitsList, recruitAmount, recruitsAmountPercentage);

    // updating the membership amount
    memberships[membershipId].amount = membershipAmount;

    // updating the membership expiry
    memberships[membershipId].expiry = expiry;

    // looping recruits to create Recruit struct and link it to membership id
    for (uint i = 0; i < recruitsList.length; i++) {
      if (recruitedMembers[recruitsList[i]]) {
        if (
          recruitsMembership[recruitsList[i]] != membershipId &&
          recruits[recruitsMembership[recruitsList[i]]][recruitsList[i]].expiry > block.timestamp
        ) {
          revert("recruit already exist in another membership");
        }
      }

      recruits[membershipId][recruitsList[i]] = Recruit(expiry, recruitAmount[i]);

      // marking the address is recruited
      recruitedMembers[recruitsList[i]] = true;
      recruitsMembership[recruitsList[i]] = membershipId;
      // emitting Recruited event when assured the recruit is added to the membership
      emit Recruited(membershipId, recruitsList[i], recruitAmount[i], expiry);
    }

    // emitting MembershipRenewed i.e membership is renewed
    emit MembershipRenewed(
      membershipId,
      memberships[membershipId].tokenId,
      sponsor,
      memberships[membershipId].amount,
      memberships[membershipId].expiry
    );

    trialMemberships[membershipId] = false;
  }

  function processPaymentWithNoRecruits(
    // membership id
    uint256 membershipId,
    // membership sponsor's address
    address sponsor,
    // amount for renewed membership
    uint256 amount,
    // recruits loan percentage
    uint256 recruitsAmountPercentage
  ) internal {
    address[] memory recruitsList;
    uint256[] memory recruitsAmount;
    processPayment(membershipId, sponsor, amount, recruitsList, recruitsAmount, recruitsAmountPercentage);
  }

  //* Internal method for payment processing used in buyMembership, renewMembership
  function processPayment(
    // membership id
    uint256 membershipId,
    // membership sponsor's address
    address sponsor,
    // amount for renewed membership
    uint256 amount,
    // recruits list for the membership
    address[] memory recruitsList,
    // recruits loan amount according to the recruitsList argument
    uint256[] memory recruitAmount,
    // recruits loan percentage
    uint256 recruitsAmountPercentage
  ) internal {
    require(
      recruitsAmountPercentage >= minRecruitPercentage,
      "recruits loan percentage is less than minimum recruit percentage"
    );

    require(
      recruitsAmountPercentage <= maxRecruitPercentage,
      "recruits loan percentage is greater than maximum recruit percentage"
    );

    uint256 totalPercentage = recruitsAmountPercentage + organizationPercentage + abundanceStateVariablePercentage;

    require(
      totalPercentage <= PERCENTAGE_DENOMINATOR,
      "recruits loan percentage is greater than maximum recruit percentage"
    );
    // calculating total recruits amount according to recruitsAmountPercentage of membership
    uint256 totalRecruitsAmount = (recruitsAmountPercentage * amount) / PERCENTAGE_DENOMINATOR;
    // calculating organization amount according to 10% of membership
    uint256 orgAmount = (organizationPercentage * amount) / PERCENTAGE_DENOMINATOR;
    // calculating abundance token variable according to 20% of membership
    uint256 abundanceAmount = (abundanceStateVariablePercentage * amount) / PERCENTAGE_DENOMINATOR;

    uint256 sumRecruitAmount = 0;

    // assigning abundance token amount
    abundanceTokenStates[membershipId] = abundanceAmount;

    // transfering organization amount
    paymentToken.safeTransferFrom(sponsor, organizationAddress, orgAmount);

    for (uint256 i = 0; i < recruitsList.length; i++) {
      // checking recruit's amount is equal or greater than minimum recruit's amount
      require(recruitAmount[i] >= minRecruitAmount, "recruit amount is less than minimum recruit amount");

      // transfering the amount to recruits
      paymentToken.safeTransferFrom(sponsor, recruitsList[i], recruitAmount[i]);
      sumRecruitAmount += recruitAmount[i];
    }

    // checking sum of total recruit amount is less than or equal to the 30% of the membership amount
    require(
      sumRecruitAmount <= totalRecruitsAmount,
      "total sum of recruit amount exceed total recruit amount percentage of the membership"
    );

    // assigning the remaining recruit amount to the abundance token state variable of the sponsor
    if (sumRecruitAmount < totalRecruitsAmount) {
      remainingRecruitsAmount[membershipId] = totalRecruitsAmount - sumRecruitAmount;
      paymentToken.safeTransferFrom(sponsor, address(this), remainingRecruitsAmount[membershipId]);
    }

    // transfering abundance token state variable amount to the contract
    paymentToken.safeTransferFrom(sponsor, address(this), abundanceTokenStates[membershipId]);
  }

  function upgradeMembership(
    // membership id
    uint256 membershipId,
    // membership amount
    uint256 amount,
    // recruits loan percentage
    uint256 recruitsAmountPercentage
  ) external {
    require(!isMembershipExpired(membershipId), "Membership is expired");

    require(!trialMemberships[membershipId], "Trial membership can not upgraded");

    address sponsor = getSponsor(membershipId);

    require(sponsor == msg.sender, "Only sponsor can upgrade membership");

    uint256 prevAmount = memberships[membershipId].amount;
    memberships[membershipId].amount = amount;

    uint256 stateVariableAmount = abundanceTokenStates[membershipId];

    if (amount > prevAmount) {
      uint256 diffAmount = amount - prevAmount;

      processPaymentWithNoRecruits(membershipId, sponsor, diffAmount, recruitsAmountPercentage);

      abundanceTokenStates[membershipId] += stateVariableAmount;

      emit MembershipRenewed(
        membershipId,
        memberships[membershipId].tokenId,
        sponsor,
        amount,
        memberships[membershipId].expiry
      );
    } else {
      revert("new membership amount is equal or less than previous membership amount");
    }
  }

  function addRecruit(
    // membership id
    uint256 membershipId,
    // recruits list for the membership
    address[] memory recruitsList,
    // recruits loan amount according to the recruitsList argument
    uint256[] memory recruitsAmount
  ) external {
    require(!isMembershipExpired(membershipId), "Membership is expired");

    require(!trialMemberships[membershipId], "can not add recruits in trial membership");

    require(recruitsList.length == recruitsAmount.length, "no of recruit list is not equal to recruits amount list");

    uint256 sumRecruitAmount = 0;

    for (uint256 i = 0; i < recruitsList.length; i++) {
      if (recruitedMembers[recruitsList[i]]) {
        if (
          recruitsMembership[recruitsList[i]] != membershipId &&
          recruits[recruitsMembership[recruitsList[i]]][recruitsList[i]].expiry > block.timestamp
        ) {
          revert("recruit already exist in another membership");
        }
      }

      recruits[membershipId][recruitsList[i]] = Recruit(memberships[membershipId].expiry, recruitsAmount[i]);

      // marking the address is recruited
      recruitedMembers[recruitsList[i]] = true;

      recruitsMembership[recruitsList[i]] = membershipId;

      // emitting Recruited event when assured the recruit is added to the membership
      emit Recruited(membershipId, recruitsList[i], recruitsAmount[i], memberships[membershipId].expiry);

      // checking recruit's amount is equal or greater than minimum recruit's amount
      require(recruitsAmount[i] >= minRecruitAmount, "recruit amount is less than minimum recruit amount");

      // transfering the amount to recruits
      paymentToken.transfer(recruitsList[i], recruitsAmount[i]);
      sumRecruitAmount += recruitsAmount[i];
    }

    // checking sum of total recruit amount is less than or equal to the 30% of the membership amount
    require(
      sumRecruitAmount <= getRemainingRecruitsAmount(membershipId),
      "total sum of recruit amount exceed remaining recruits amount"
    );

    remainingRecruitsAmount[membershipId] -= sumRecruitAmount;
  }

  //* method to give user reward for some work within his state variable
  function giveReward(uint256 membershipId, uint256 rewardAmount) external onlyOwner {
    uint256 stateVariableAmount = abundanceTokenStates[membershipId];

    // check if membership is expired or not
    require(!isMembershipExpired(membershipId), "Membership is expired");
    // comparing reward with state variable amount
    require(stateVariableAmount >= rewardAmount, "reward amount is greater than state variable amount");
    // fetch sponsor address from tokenId
    address sponsor = getSponsor(membershipId);
    // approving reward amount to uniswap router
    paymentToken.approve(address(poolRouter), rewardAmount);

    address[] memory tokens = new address[](2);
    tokens[0] = address(paymentToken);
    tokens[1] = address(abundanceToken);

    uint256[] memory amounts = poolRouter.swapExactTokensForTokens(
      rewardAmount,
      0,
      tokens,
      sponsor,
      block.timestamp + (30 * 60)
    );

    abundanceTokenStates[membershipId] -= rewardAmount;

    emit Rewarded(membershipId, sponsor, rewardAmount, amounts[1]);
  }

  //* method to check whether the membership is expired or not
  function isMembershipExpired(uint256 membershipId) public view returns (bool) {
    return block.timestamp > memberships[membershipId].expiry;
  }

  //* method to fetch recruits remaining amount
  function getRemainingRecruitsAmount(uint256 membershipId) public view returns (uint256) {
    return remainingRecruitsAmount[membershipId];
  }

  //* function to fetch minimum recruit amount
  function getMinRecruitAmount() public view returns (uint256) {
    return minRecruitAmount;
  }

  //* function to modified minimum recruit amount
  function setMinRecruitAmount(uint256 _recruitAmount) external onlyOwner {
    minRecruitAmount = _recruitAmount;
  }

  //* function to get membership by sponsor address
  function getMembership(address sponsor) public view returns (Membership memory) {
    for (uint256 i = 0; i < memberships.length; i++) {
      uint256 tokenId = memberships[i].tokenId;
      if (membershipNFT.ownerOf(tokenId) == sponsor) {
        return memberships[i];
      }
    }

    revert("Membership not found");
  }

  //* function to get membership by membership id
  function getMembership(uint256 membershipId) public view returns (Membership memory) {
    if (membershipId >= memberships.length) {
      revert("Index Out of found");
    }

    return memberships[membershipId];
  }

  //* function to get abundance state variable
  function getAbundanceStateVariable(uint256 membershipId) public view returns (uint256) {
    return abundanceTokenStates[membershipId];
  }

  //* function to set whether the membership has 7 days trial or not (only admin)
  function setTrial(bool _trial) external onlyOwner {
    trial = _trial;
  }

  //* function to check whether the membership has trial or not
  function hasTrial() public view returns (bool) {
    return trial;
  }

  //* function to check whether the auto minting of abundance token is true or false
  function getAutoMintAbundanceTokenStatus() public view returns (bool) {
    return autoMintAbundanceToken;
  }

  //* function to toggle whether the minting of abundance token is automatic or not on renewal membership
  function toggleAutoAbundanceTokenMinting() external onlyOwner {
    autoMintAbundanceToken = !autoMintAbundanceToken;
  }

  //* function to toggle whether the membership is renewable or not
  function toggleAutoRenewal(uint256 membershipId) external onlyOwner {
    autoMembershipRenewal[membershipId] = !autoMembershipRenewal[membershipId];
  }

  //* function to check whether the autoRenewal for membership is set or not
  function getMembershipAutoStatus(uint256 membershipId) public view returns (bool) {
    return autoMembershipRenewal[membershipId];
  }

  //* function to set membership trial period
  function setTrialPeriod(uint256 _trialPeriod) external onlyOwner {
    trialPeriod = _trialPeriod;
  }

  //* function to get membership trial period
  function getTrialPeriod() public view returns (uint256) {
    return trialPeriod;
  }

  function setTrialMembershipFee(uint256 _trialMembershipFee) external onlyOwner {
    require(trialMembershipFee < minMembershipFee, "the entered amount is less than trial membership");
    trialMembershipFee = _trialMembershipFee;
  }

  function getTrialMembershipFee() public view returns (uint256) {
    return trialMembershipFee;
  }

  function setMinMembershipFee(uint256 _minMembershipFee) external onlyOwner {
    require(_minMembershipFee > trialMembershipFee, "the entered amount is less than trial membership");

    minMembershipFee = _minMembershipFee;
  }

  //* function to modify abundanceStateVariablePercentage value
  function setStateVariableAmountPercentage(uint256 _percentage) external onlyOwner {
    require(_percentage >= 1000, "Abundance state variable percentage should be greater than and equals to 10%");
    require(_percentage <= 2000, "Abundance state variable percentage should be less than and equals to 20%");
    abundanceStateVariablePercentage = _percentage;
  }

  function getMinMembershipFee() public view returns (uint256) {
    return minMembershipFee;
  }

  function verifyRecruits(address recruit) public view returns (uint256) {
    if (recruitsMembership[recruit] == 0) {
      revert("recruit not found");
    }
    uint256 membershipId = recruitsMembership[recruit];
    if (recruits[membershipId][recruit].expiry < block.timestamp) {
      revert("recruit not found");
    }

    return membershipId;
  }

  function calculateUSDTAmountInAT(uint256 _amount) public view returns (uint256) {
    address[] memory tokens = new address[](2);
    tokens[0] = address(paymentToken);
    tokens[1] = address(abundanceToken);

    return poolRouter.getAmountsOut(_amount, tokens)[1];
  }

  function swapExactTokensForTokens(uint256 tokenAmount) private returns (uint256 amountAT) {
    // generate the uniswap pair path of token -> weth
    address[] memory path = new address[](2);
    path[0] = (address(paymentToken));
    path[1] = (address(abundanceToken));

    paymentToken.approve(address(poolRouter), tokenAmount);

    // make the swap
    amountAT = poolRouter.swapExactTokensForTokens(
      tokenAmount,
      0, // accept any amount of ETH
      path,
      address(this),
      block.timestamp
    )[1];
  }

  function swapAndLiquify(uint256 balanceUSDT) private lockTheSwap {
    if (balanceUSDT > 0) {
      // split the USDT Amount into halves
      uint256 half = balanceUSDT / 2;
      uint256 otherHalf = balanceUSDT - half;

      // swap USDT tokens for AT tokens
      uint256 amountAT = swapExactTokensForTokens(half);

      // add liquidity to uniswap
      addLiquidity(otherHalf, amountAT);
    }
  }

  function addLiquidity(uint256 amountUSDT, uint256 amountAT) private {
    address tokenAT = address(abundanceToken);
    address tokenUSDT = address(paymentToken);

    // console.log(tokenAT);

    IERC20 abdToken = IERC20(tokenAT);
    IERC20 usdtToken = IERC20(tokenUSDT);

    IERC20(tokenUSDT).approve(address(poolRouter), amountUSDT);
    abdToken.approve(address(poolRouter), amountAT);

    console.log("Amount USDT && AT", amountUSDT, amountAT);
    console.log("AT balance", abdToken.balanceOf(address(this)));
    console.log("USDT balance", usdtToken.balanceOf(address(this)));

    // add the liquidity
    poolRouter.addLiquidity(
      tokenUSDT,
      tokenAT,
      amountUSDT,
      amountAT,
      0, // slippage is unavoidable
      0, // slippage is unavoidable
      address(this),
      block.timestamp + 30 * 60
    );
  }

  function getMinMaxOrganizationPercentages() public view returns (uint256, uint256) {
    return (minOrganizationPercentage, maxOrganizationPercentage);
  }

  function setMinMaxOrganizationPercentages(uint256 _minPercentage, uint256 _maxPercentage) external onlyAdmin {
    require(_maxPercentage > _minPercentage, "Max Percentage can not be equal or less than Min Percentage");
    minOrganizationPercentage = _minPercentage;
    maxOrganizationPercentage = _maxPercentage;
  }

  function getMinMaxAbundanceStatePercentages() public view returns (uint256, uint256) {
    return (minAbundanceStatePercentage, maxAbundanceStatePercentage);
  }

  function setMinMaxAbundanceStatePercentages(uint256 _minPercentage, uint256 _maxPercentage) external onlyAdmin {
    require(_maxPercentage > _minPercentage, "Max Percentage can not be equal or less than Min Percentage");
    minAbundanceStatePercentage = _minPercentage;
    maxAbundanceStatePercentage = _maxPercentage;
  }

  function getMinMaxSponsorPercentages() public view returns (uint256, uint256) {
    return (minSponsorPercentage, maxSponsorPercentage);
  }

  function setMinMaxSponsorPercentages(uint256 _minPercentage, uint256 _maxPercentage) external onlyAdmin {
    require(_maxPercentage > _minPercentage, "Max Percentage can not be equal or less than Min Percentage");
    minSponsorPercentage = _minPercentage;
    maxSponsorPercentage = _maxPercentage;
  }

  function getMinMaxRecruitPercentages() public view returns (uint256, uint256) {
    return (minRecruitPercentage, maxRecruitPercentage);
  }

  function setMinMaxRecruitPercentages(uint256 _minPercentage, uint256 _maxPercentage) external onlyAdmin {
    require(_maxPercentage > _minPercentage, "Max Percentage can not be equal or less than Min Percentage");
    minRecruitPercentage = _minPercentage;
    maxRecruitPercentage = _maxPercentage;
  }

  function getSponsorDefaultPercentage() public view returns (uint256) {
    return sponsorPercentage;
  }

  function getRecruitDefaultPercentage() public view returns (uint256) {
    return recruitPercentage;
  }

  function getAbundanceStateDefaultPercentage() public view returns (uint256) {
    return abundanceStateVariablePercentage;
  }

  function getOrganizationDefaultPercentage() public view returns (uint256) {
    return organizationPercentage;
  }

  function getSponsor(uint256 membershipId) public view returns (address sponsor) {
    uint256 tokenId = memberships[membershipId].tokenId;
    sponsor = membershipNFT.ownerOf(tokenId);
  }

  function setPercentages(
    uint256 _sponsorPercentage,
    uint256 _recruitPercentage,
    uint256 _abundanceStatePercentage,
    uint256 _organizationPercentage
  ) external onlyOwner {
    uint256 totalPercentages = _sponsorPercentage +
      _recruitPercentage +
      _abundanceStatePercentage +
      _organizationPercentage;

    require(totalPercentages == 10000, "sum of all given percentages must be equal to 100%");

    require(_sponsorPercentage >= minSponsorPercentage, "sponsor percentage is less than minimum sponsor percentage");
    require(
      _sponsorPercentage <= maxSponsorPercentage,
      "sponsor percentage is greater than maximum sponsor percentage"
    );
    sponsorPercentage = _sponsorPercentage;

    require(_recruitPercentage >= minRecruitPercentage, "recruit percentage is less than minimum recruit percentage");
    require(
      _recruitPercentage <= maxRecruitPercentage,
      "recruit percentage is greater than maximum recruit percentage"
    );
    recruitPercentage = _recruitPercentage;

    require(
      _organizationPercentage >= minOrganizationPercentage,
      "organization percentage is less than minimum organization percentage"
    );
    require(
      _organizationPercentage <= maxOrganizationPercentage,
      "organization percentage is greater than maximum organization percentage"
    );
    organizationPercentage = _organizationPercentage;

    require(
      _abundanceStatePercentage >= minAbundanceStatePercentage,
      "abundance state percentage is less than minimum abundance state percentage"
    );
    require(
      _abundanceStatePercentage <= maxAbundanceStatePercentage,
      "abundance state percentage is greater than maximum abundance state percentage"
    );
    abundanceStateVariablePercentage = _abundanceStatePercentage;
  }
}

