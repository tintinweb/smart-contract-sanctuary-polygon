// contracts/Academy.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControlDefaultAdminRules.sol";
import "./Academic.sol";

contract Academy is AccessControlDefaultAdminRules {
  address academicPFPaddress;
  bytes32 public constant SIGNUP_APPROVER_ROLE = keccak256("SIGNUP_APPROVER_ROLE");

  constructor() AccessControlDefaultAdminRules(
    3 days,
    msg.sender
   ) {
    Academic PFP = new Academic();
    academicPFPaddress = address(PFP);
   }

  function signup(address to) public {
    require(hasRole(SIGNUP_APPROVER_ROLE, msg.sender), "SIGNUP_APPROVER_ROLE required");
    require(Academic(academicPFPaddress).balanceOf(to) == 0, "You can only sign up a wallet once");

    Academic(academicPFPaddress).safeMint(to);
  }

  function getPFPContract() public view returns (address) {
    return academicPFPaddress;
  }
}