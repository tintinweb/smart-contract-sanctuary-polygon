pragma solidity ^0.8.17;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./interfaces/IFactoryDIFv2.sol";
import "./interfaces/ILPToken.sol";
import "./interfaces/IDIFv2.sol";
import "./interfaces/ISWAP.sol";
import "hardhat/console.sol";

contract FactoryDIFv2 is IFactoryDIFv2, AccessControl {
    using SafeERC20 for IERC20;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    uint256 public constant GRANULARITY = 1e8;

    IFactoryDIFv2.DIFPrefences private _settings;
    address private _trustedForwarder;
    address private _difBase;
    address private _lpBase;
    address private _operationWallet;
    address private _swapAddress;
    mapping(address => bool) private _allowlist;
    mapping(address => address) private dif2lp;
    address[] private difs;

    event NewDIF(string name, string symbol, address dif, address lptoken, uint256 supply);

    constructor(
        uint256 minAmount,
        address CRUSToken,
        address USDToken,
        address uniswapRouter,
        address uniswapQuoter,
        address vault,
        address difBase,
        address lpBase,
        address operationWallet,
        uint256 redeemFee,
        uint256 issueFee,
        address forwarder
    ) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(ADMIN_ROLE, msg.sender);
        _settings.minAmount = minAmount;
        _settings.CRUSToken = IERC20(CRUSToken);
        _settings.USDToken = IERC20(USDToken);
        _settings.uniswapRouter = ISwapRouter(uniswapRouter);
        _settings.uniswapQuoter = IQuoter(uniswapQuoter);
        _settings.vault = vault;
        _settings.operationWallet = operationWallet;
        _settings.redeemFee = redeemFee;
        _settings.issueFee = issueFee;
        _difBase = difBase;
        _lpBase = lpBase;
        _trustedForwarder = forwarder;
    }

    /**
     * @dev Function for creating DIF
     * @param amount CRUS token amount
     * @param maxTotalSupply Max supply of dif in USD
     * @param pathes Uniswap pathes for buying assset tokens
     * @param weights Percentages of the amount of tokens
     */
    function createDIF(
        uint256 amount,
        uint256 maxTotalSupply,
        uint256[] memory weights,
        uint256[] memory mins,
        bytes[] memory pathes,
        string memory name,
        string memory symbol
    ) external {
        require(amount >= _settings.minAmount, "Insufficient amount");
        require(
            pathes.length > 0 && pathes.length == weights.length,
            "Invalid length of arrays"
        );
        uint256 fullpercents = 0;
        for (uint256 i = 0; i < pathes.length; i++) {
            fullpercents += weights[i];
            require(
                _allowlist[toAddress(pathes[i], pathes[i].length - 20)],
                "Token is not allowed"
            );
        }
        require(fullpercents == GRANULARITY, "Invalid sum of percents");

        address lptoken = createClone(_lpBase);
        address dif = createClone(_difBase);
        ILPToken(lptoken).init(maxTotalSupply, dif, name, symbol, _trustedForwarder);
        dif2lp[dif] = lptoken;
        _settings.CRUSToken.safeTransferFrom(
            _msgSender(),
            address(this),
            amount
        );
        _swapRedeem(amount, dif);
        difs.push(dif);
        IDIFv2(dif).initialMint(
            amount,
            _msgSender(),
            lptoken,
            _settings,
            weights,
            mins,
            pathes,
            _trustedForwarder
        );
        emit NewDIF(name, symbol, dif, lptoken, maxTotalSupply);
    }

    function transferAsset(address token, address to, uint256 amount) external {
        require(_allowlist[token], "Token is not allowed");
        require(dif2lp[_msgSender()] != (address(0)), "Only DIF allowed");

        IERC20(token).safeTransferFrom(_settings.vault, to, amount);
    }

    function setPreferences(
        uint256 minAmount,
        address CRUSToken,
        address USDToken,
        address uniswapRouter,
        address uniswapQuoter,
        address vault,
        address operationWallet,
        uint256 redeemFee,
        uint256 issueFee
    ) external onlyRole(ADMIN_ROLE) {
        _settings = IFactoryDIFv2.DIFPrefences({
            minAmount: minAmount,
            redeemFee: redeemFee,
            issueFee: issueFee,
            CRUSToken: IERC20(CRUSToken),
            USDToken: IERC20(USDToken),
            uniswapRouter: ISwapRouter(uniswapRouter),
            uniswapQuoter: IQuoter(uniswapQuoter),
            vault: vault,
            operationWallet: operationWallet
        });
    }

    function setDifBase(address difBase) external onlyRole(ADMIN_ROLE) {
        _difBase = difBase;
    }

    function setLpBase(address lpBase) external onlyRole(ADMIN_ROLE) {
        _lpBase = lpBase;
    }

    function setSwapAddress(address _swap) external onlyRole(ADMIN_ROLE) {
        _swapAddress = _swap;
    }

    function setAllowedToken(
        address token,
        bool enabled
    ) external onlyRole(ADMIN_ROLE) {
        _allowlist[token] = enabled;
    }

    function toAddress(
        bytes memory _bytes,
        uint256 _start
    ) internal pure returns (address) {
        require(_start + 20 >= _start, "toAddress_overflow");
        require(_bytes.length >= _start + 20, "toAddress_outOfBounds");
        address tempAddress;
        assembly {
            tempAddress := div(
                mload(add(add(_bytes, 0x20), _start)),
                0x1000000000000000000000000
            )
        }
        return tempAddress;
    }

    function createClone(address target) internal returns (address result) {
        bytes20 targetBytes = bytes20(target);
        assembly {
            let clone := mload(0x40)
            mstore(
                clone,
                0x3d602d80600a3d3981f3363d3d373d3d3d363d73000000000000000000000000
            )
            mstore(add(clone, 0x14), targetBytes)
            mstore(
                add(clone, 0x28),
                0x5af43d82803e903d91602b57fd5bf30000000000000000000000000000000000
            )
            result := create(0, clone, 0x37)
        }
    }

    function swapRedeem(uint256 amount) public {
        require(dif2lp[_msgSender()] != (address(0)), "Only DIF allowed");
        _swapRedeem(amount, _msgSender());
    }

    function getSwapAddress() external view returns(address) {
        return _swapAddress;
    }

    function getDifAddress(uint256 index) external view returns(address) {
        return difs[index];
    }

    function getDifsCount() external view returns(uint256) {
        return difs.length;
    }

    function difExists(address dif) external view returns(bool) {
        return (dif2lp[dif] != address(0));
    }

    function getFees() external view returns(uint256 issueFee, uint256 redeemFee) {
        return (_settings.issueFee, _settings.redeemFee);
    }

    function _swapRedeem(uint256 amount, address recipient) internal {
        _settings.CRUSToken.approve(_swapAddress, amount);
        ISwap(_swapAddress).redeem(amount);
        _settings.USDToken.transfer(recipient, amount/1e12);
    }
    
    function isTrustedForwarder(address forwarder) public view virtual returns (bool) {
        return forwarder == _trustedForwarder;
    }

    function trustedForwarder() external view returns(address) {
        return _trustedForwarder;
    }

    function _msgSender() internal view virtual override returns (address sender) {
        if (isTrustedForwarder(msg.sender)) {
            // The assembly code is more direct than the Solidity version using `abi.decode`.
            /// @solidity memory-safe-assembly
            assembly {
                sender := shr(96, calldataload(sub(calldatasize(), 20)))
            }
        } else {
            return super._msgSender();
        }
    }

    function _msgData() internal view virtual override returns (bytes calldata) {
        if (isTrustedForwarder(msg.sender)) {
            return msg.data[:msg.data.length - 20];
        } else {
            return super._msgData();
        }
    }
}

