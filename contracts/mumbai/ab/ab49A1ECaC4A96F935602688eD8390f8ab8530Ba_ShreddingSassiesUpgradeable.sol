pragma solidity ^0.8.2;

import "./OperatorFilterer.sol"; // Using https://github.com/Vectorized/closedsea implementation of Operator Filterer
import "../contracts-upgradable/token/onft/ERC721/ONFT721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/common/ERC2981Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";

// LZAppUpgradeable contains ownableUpgradeable as well.
contract ShreddingSassiesUpgradeable is Initializable, ONFT721Upgradeable, ERC721EnumerableUpgradeable, OperatorFilterer, ERC2981Upgradeable {
    /// @notice Operator filter toggle switch
    bool public operatorFilteringEnabled;
    string public baseURI;

    function initialize(string memory _name, string memory _symbol, uint _minGasToTransfer, address _lzEndpoint) public initializer {
        __ONFT721Upgradeable_init(_name, _symbol, _minGasToTransfer, _lzEndpoint);
        __ERC721Enumerable_init();
        __ERC2981_init();
        _registerForOperatorFiltering();
        operatorFilteringEnabled = true;

        // Set royalty receiver to the contract creator,
        // at 7.5% (default denominator is 10000).
        _setDefaultRoyalty(msg.sender, 750);
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function airdrop(address[] calldata _tokenOwners, uint[] calldata _tokenIds) external onlyOwner {
        require(_tokenOwners.length == _tokenIds.length, "ShreddingSassies: tokenOwners and tokenIds length mismatch");
        for (uint i = 0; i < _tokenOwners.length; i++) {
            _safeMint(_tokenOwners[i], _tokenIds[i]);
        }
    }

    // overriding this function to perform a burn on the source chain instead of a transfer to the contract itself
    function _debitFrom(address _from, uint16, bytes memory, uint _tokenId) internal override {
        require(_isApprovedOrOwner(_msgSender(), _tokenId), "ONFT721: send caller is not owner nor approved");
        require(ERC721Upgradeable.ownerOf(_tokenId) == _from, "ONFT721: send from incorrect owner");
        _burn(_tokenId);
    }

    /**
     * @dev Base URI for computing {tokenURI}. If set, the resulting URI for each
     * token will be the concatenation of the `baseURI` and the `tokenId`. Empty
     * by default, can be overridden in child contracts.
     */
    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    function setBaseURI(string calldata newBaseURI) external onlyOwner {
        baseURI = newBaseURI;
    }

    // OPERATOR FILTER STUFF
    // This is an implementation based on the documentation here: https://github.com/Vectorized/closedsea
    /**
     * @dev See {IERC721-setApprovalForAll}.
     * @dev Requires override for Royalties OperatorFiltering
     */
    function setApprovalForAll(address operator, bool approved) public override(ERC721Upgradeable, IERC721Upgradeable) onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    /**
     * @dev See {IERC721-approve}.
     * @dev Requires override for Royalties OperatorFiltering
     */
    function approve(address operator, uint tokenId) public override(ERC721Upgradeable, IERC721Upgradeable) onlyAllowedOperatorApproval(operator) {
        super.approve(operator, tokenId);
    }

    function transferFrom(address from, address to, uint tokenId) public override(ERC721Upgradeable, IERC721Upgradeable) onlyAllowedOperator(from) {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(address from, address to, uint tokenId) public override(ERC721Upgradeable, IERC721Upgradeable) onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(address from, address to, uint tokenId, bytes memory data) public override(ERC721Upgradeable, IERC721Upgradeable) onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId, data);
    }

    function supportsInterface(bytes4 interfaceId) public view override(ONFT721Upgradeable, ERC721EnumerableUpgradeable, ERC2981Upgradeable) returns (bool) {
        // Supports the following `interfaceId`s:
        // - IERC165: 0x01ffc9a7
        // - IERC721: 0x80ac58cd
        // - IERC721Metadata: 0x5b5e139f
        // - IERC2981: 0x2a55205a
        return ONFT721Upgradeable.supportsInterface(interfaceId) || ERC721EnumerableUpgradeable.supportsInterface(interfaceId) || ERC2981Upgradeable.supportsInterface(interfaceId);
    }

    /**
     * @notice function to set ERC2981 royalties required by Opensea
     * @param receiver the address to receive the royalties
     * @param feeNumerator the royalty percentage in basis points of 10000 (7.5% = 750)
     */
    function setDefaultRoyalty(address receiver, uint96 feeNumerator) public onlyOwner {
        _setDefaultRoyalty(receiver, feeNumerator);
    }

    /**
     * @notice Set operator filtering on or off
     * @param value true to enable operator filtering, false to disable
     */
    function setOperatorFilteringEnabled(bool value) public onlyOwner {
        operatorFilteringEnabled = value;
    }

    function _operatorFilteringEnabled() internal view override returns (bool) {
        return operatorFilteringEnabled;
    }

    /**
     * @notice Registers (or unregisters) the current contract to a subscription contract for operator filtering
     * @param subscriptionAddress the address of the subscription contract
     * @param subscribe true to register, false to unregister
     */
    function registerForOperatorFiltering(address subscriptionAddress, bool subscribe) external onlyOwner {
        _registerForOperatorFiltering(subscriptionAddress, subscribe);
    }

    // END OPERATOR FILTER

    // The following functions are overrides required by Solidity. (Needed with addition of Enumerable Extension)
    function _beforeTokenTransfer(address from, address to, uint tokenId, uint batchSize) internal override(ERC721Upgradeable, ERC721EnumerableUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }
}

