pragma solidity ^0.8.7;

import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/KeeperCompatibleInterface.sol";
import "hardhat/console.sol";

error SimpleGame__NotOwner();
error SimpleGame__NoRandomWords();
error SimpleGame__NoGamesToPlay();
error SimpleGame__GameIsInPause();
error SimpleGame__BetAmoutNotAllowed();
error SimpleGame__RoomSizeNotAllowed();
error SimpleGame__UpkeepNotNeeded();
error SimpleGame__TransferFailed();
error SimpleGame__GameIdNotFound(uint256 gameId);
error SimpleGame__ContractAmoutIsLess(uint256 amoutAvaliable);
error SimpleGame__GameWasNotPlayed(uint256 gameId);

contract SimpleGame is VRFConsumerBaseV2, KeeperCompatibleInterface {
    struct Game {
        address[] players;
        uint256 room;
        uint256 bet;
        uint256 id;
    }
    struct Room {
        address[] players;
        uint256 id;
    }
    struct GameHistory {
        address[] players;
        uint256 betForPlayer;
        uint256 amountWin;
        address winner;
    }
    enum RequestNumberState {
        DONE,
        WAITING
    }
    struct RoomInfo {
        RoomStatus status;
        address[] players;
        uint256 roomSize;
        uint256 betForPlayer;
        uint256 amountWin;
        address winner;
    }
    enum RoomStatus {
        TO_FULLFILL,
        RUNNING,
        DONE
    }

    // Chainlink VRF Variables
    VRFCoordinatorV2Interface private immutable i_vrfCoordinator;
    uint64 private immutable i_subscriptionId;
    // see https://docs.chain.link/docs/vrf/v2/subscription/supported-networks/#configurations
    bytes32 private immutable i_keyHash;
    uint16 private constant REQUEST_CONFIRMATIONS = 3;
    uint32 private s_requestRandomWordsCallbackGasLimit;
    uint16 private s_randomWordsToRequest = 100;
    uint256 private s_lastTimeStampFromRequestRandmWords;
    uint256 private s_roomIdSequence;
    uint8 private s_maxGamesToPlay = 5;

    address private immutable i_owner;
    uint256 private immutable i_intervalFromLastRequestOfRandomWords;

    uint256[] private s_randomWords;
    uint256 private s_requestId;
    bool private s_isOpen = true;
    Game[] private s_games;
    RequestNumberState private s_requestRandomState;
    uint256 private s_withdrawable = 0;
    mapping(uint256 => GameHistory) private s_gamesPlayedHistory;

    uint256 private constant HOUSE_EDGE = 10;
    uint256[] private s_roomSizeOptions = [3, 5, 10];
    uint256[] private s_betOptions = [0.01 ether, 0.1 ether, 1 ether];

    /*
   {
        // betOption
        "0.01":{
            // roomSize
            "2":[
                {
                    players: ["addr01"],
                    id: 1
                }, // room 0
                [] // room 1
            ],
            // roomSize
            "3":[
                { 
                    players: ["addr04","addr05"],
                    id:2
                }, // room 0
                { 
                    players: ["addr04"],
                    id:4
                } // room 1
            ]
    }
    */
    mapping(uint256 => mapping(uint256 => Room[])) public s_roomsOpened;

    modifier onlyOwner() {
        if (msg.sender != i_owner) revert SimpleGame__NotOwner();
        _;
    }

    event PlayerJoined(
        uint256 indexed id,
        uint256 indexed betSize,
        uint256 indexed roomSize,
        address joinedPlayer
    );
    event GamePlayed(
        uint256 indexed id,
        uint256 indexed amoutWin,
        uint256 roomSize,
        address indexed winner,
        address[] players
    );
    event GameReady(uint256 indexed id, address[] players);

    event ReFilled(uint256 indexed games, uint256 indexed vrn);
    event Simplelog(string msg);

    event RequestRandomNumber(uint256 requestId);

    constructor(
        address vrfCoordinatorV2,
        uint64 subscriptionId,
        bytes32 keyHash,
        uint256 intervalFromLastRequestOfRandomWords,
        uint32 requestRandomWordsCallbackGasLimit
    ) VRFConsumerBaseV2(vrfCoordinatorV2) {
        i_vrfCoordinator = VRFCoordinatorV2Interface(vrfCoordinatorV2);
        i_owner = msg.sender;
        i_subscriptionId = subscriptionId;
        i_keyHash = keyHash;
        i_intervalFromLastRequestOfRandomWords = intervalFromLastRequestOfRandomWords;
        s_requestRandomState = RequestNumberState.DONE;
        s_lastTimeStampFromRequestRandmWords = block.timestamp;
        s_requestRandomWordsCallbackGasLimit = requestRandomWordsCallbackGasLimit;
        s_roomIdSequence = 0;
    }

    // ===================== //
    // Public game functions //
    // ===================== //

    function placeBet(uint256 _roomSize) public payable {
        // check
        // - if bet is in the betOptions
        // - if room size is in roomSizeOptions

        if (_isInArray(s_betOptions, msg.value) == false) {
            revert SimpleGame__BetAmoutNotAllowed();
        }
        if (_isInArray(s_roomSizeOptions, _roomSize) == false) {
            revert SimpleGame__RoomSizeNotAllowed();
        }

        //emit BetPlaced(msg.value, _roomSize, msg.sender);
        _addBet(msg.value, _roomSize, msg.sender);
    }

    function donate() public payable {}

    // ===================================== //
    // Public getter contract info functions //
    // ===================================== //

    function getVersion() public pure returns (string memory) {
        return "0.3.2";
    }

    function getBetOptions() public view returns (uint256[] memory) {
        return s_betOptions;
    }

    function getRoomSizeOptions() public view returns (uint256[] memory) {
        return s_roomSizeOptions;
    }

    function getIntervalFromLastRequestOfRandomWords()
        public
        view
        returns (uint256)
    {
        return i_intervalFromLastRequestOfRandomWords;
    }

    function getLastRoomId() public view returns (uint256) {
        return s_roomIdSequence;
    }

    function getContractOwner() public view returns (address) {
        return i_owner;
    }

    function getRandomLeft() public view returns (uint256) {
        return s_randomWords.length;
    }

    function getWithdrawable() public view returns (uint256) {
        return s_withdrawable;
    }

    function getLastTimeStampFromRequestRandmWords()
        public
        view
        returns (uint256)
    {
        return s_lastTimeStampFromRequestRandmWords;
    }

    function getContractTotalBalance() public view returns (uint) {
        return address(this).balance;
    }

    function getGamesLeft() public view returns (uint256) {
        return s_games.length;
    }

    function getRequestRandomState() public view returns (RequestNumberState) {
        return s_requestRandomState;
    }

    // ===================================== //
    // Public getter contract game functions //
    // ===================================== //

    function getIsOpen() public view returns (bool) {
        return s_isOpen;
    }

    function getRoomsLength(
        uint256 bet,
        uint256 roomSize
    ) public view returns (uint256) {
        return s_roomsOpened[bet][roomSize].length;
    }

    function getRoomInfo(
        uint256 roomId
    ) public view returns (RoomInfo memory roomInfo) {
        // search in history
        GameHistory memory gamePlayed = s_gamesPlayedHistory[roomId];
        if (gamePlayed.betForPlayer > 0) {
            return
                RoomInfo(
                    RoomStatus.DONE,
                    gamePlayed.players,
                    gamePlayed.players.length,
                    gamePlayed.betForPlayer,
                    gamePlayed.amountWin,
                    gamePlayed.winner
                );
        }

        // search in game that are wainting to be played
        for (uint i = 0; i < s_games.length; i++) {
            if (s_games[i].id == roomId) {
                Game memory game = s_games[i];
                return
                    RoomInfo(
                        RoomStatus.RUNNING,
                        game.players,
                        game.players.length,
                        game.bet,
                        0,
                        address(0)
                    );
            }
        }

        // search in room that are waiting to fullfill
        for (uint256 betIdx = 0; betIdx < s_betOptions.length; betIdx++) {
            for (
                uint256 roomSizeIdx = 0;
                roomSizeIdx < s_roomSizeOptions.length;
                roomSizeIdx++
            ) {
                Room[] memory rooms = s_roomsOpened[s_betOptions[betIdx]][
                    s_roomSizeOptions[roomSizeIdx]
                ];

                for (uint256 roomIdx = 0; roomIdx < rooms.length; roomIdx++) {
                    if (rooms[roomIdx].id == roomId) {
                        Room memory room = rooms[roomIdx];
                        return
                            RoomInfo(
                                RoomStatus.TO_FULLFILL,
                                room.players,
                                s_roomSizeOptions[roomSizeIdx],
                                s_betOptions[betIdx],
                                0,
                                address(0)
                            );
                    }
                }
            }
        }

        revert SimpleGame__GameIdNotFound(roomId);
    }

    function getGamePlayed(
        uint256 gameId
    ) public view returns (GameHistory memory gamePlayed) {
        gamePlayed = s_gamesPlayedHistory[gameId];
        if (gamePlayed.betForPlayer == 0) {
            revert SimpleGame__GameWasNotPlayed(gameId);
        }
    }

    // ================= //
    // Private functions //
    // ================= //

    // handle room for n number of players
    function _addBet(
        uint256 _betSize,
        uint256 _roomSize,
        address player
    ) private {
        // Get the numberOfPlayers with the betSize
        Room[] storage rooms = s_roomsOpened[_betSize][_roomSize];
        // Iterate over existing rooms
        bool added = false;
        for (uint256 r; r < rooms.length; r++) {
            address[] storage players = rooms[r].players;
            if (_isInAddressArray(players, player) == false) {
                uint256 roomId = rooms[r].id;
                // Add the player to the room
                players.push(player);
                added = true;
                emit PlayerJoined(roomId, _betSize, _roomSize, player);
                // If the room is full, then start the game har
                if (players.length == _roomSize) {
                    // Add to array of games to play
                    s_games.push(Game(players, _roomSize, _betSize, roomId));
                    // Clear Array
                    emit GameReady(roomId, players);
                    _popRoom(s_roomsOpened[_betSize][_roomSize], r);
                }
                break;
            }
        }
        if (added == false) {
            if (s_isOpen != true) {
                revert SimpleGame__GameIsInPause();
            }
            unchecked {
                ++s_roomIdSequence;
            }
            Room memory room = Room(new address[](1), s_roomIdSequence);
            room.players[0] = msg.sender;
            rooms.push(room);
            emit PlayerJoined(s_roomIdSequence, _betSize, _roomSize, player);
        }
    }

    function _getWinSize(
        uint256 _betSize,
        uint256 _numberOfUsers
    ) private pure returns (uint256 amoutWin, uint256 houseFee) {
        uint256 totalBet = (_betSize * _numberOfUsers);
        amoutWin = (totalBet / 100) * (100 - HOUSE_EDGE);
        houseFee = totalBet - amoutWin;
    }

    function _isInAddressArray(
        address[] memory _array,
        address _address
    ) private pure returns (bool) {
        for (uint256 i; i < _array.length; i++) {
            if (_array[i] == _address) {
                return true;
            }
        }
        return false;
    }

    function _isInArray(
        uint256[] memory _array,
        uint256 value
    ) private pure returns (bool) {
        for (uint256 i; i < _array.length; i++) {
            if (_array[i] == value) {
                return true;
            }
        }
        return false;
    }

    function _popRoom(Room[] storage _addresses, uint256 index) private {
        for (uint256 i = index; i < _addresses.length - 1; i++) {
            _addresses[i] = _addresses[i + 1];
        }
        _addresses.pop();
    }

    // =========== //
    //  OnlyOwner  //
    // =========== //

    function setCallbackGasLimit(uint32 value) public onlyOwner {
        s_requestRandomWordsCallbackGasLimit = value;
    }

    function setNumberOfRandomWordsToRequest(uint16 value) public onlyOwner {
        s_randomWordsToRequest = value;
    }

    function setIsOpen(bool isOpen) public onlyOwner {
        s_isOpen = isOpen;
    }

    function setMaxGamesToPlay(uint8 maxGamesToPlay) public onlyOwner {
        s_maxGamesToPlay = maxGamesToPlay;
    }

    function withdraw() public onlyOwner {
        uint256 toTransfert = s_withdrawable;

        (bool success, ) = payable(i_owner).call{value: toTransfert}("");
        if (!success) {
            revert SimpleGame__TransferFailed();
        }
        s_withdrawable -= toTransfert;
    }

    function hardWithdraw(uint256 amoutToWithDraw) public onlyOwner {
        uint256 contractBalance = address(this).balance;
        uint256 toTransfert;
        if (amoutToWithDraw == 0) {
            toTransfert = contractBalance;
        } else {
            if (contractBalance < amoutToWithDraw)
                revert SimpleGame__ContractAmoutIsLess(contractBalance);

            toTransfert = amoutToWithDraw;
        }

        (bool success, ) = payable(i_owner).call{value: toTransfert}("");
        if (!success) {
            revert SimpleGame__TransferFailed();
        }
        s_withdrawable -= toTransfert;
    }

    // Play all the rooms even if they are not full
    function pullThePlug() public onlyOwner {
        s_isOpen = false;
        for (uint256 b = 0; b < s_betOptions.length; b++) {
            for (uint256 r = 0; r < s_roomSizeOptions.length; r++) {
                Room[] storage rooms = s_roomsOpened[s_betOptions[b]][
                    s_roomSizeOptions[r]
                ];
                while (rooms.length > 0) {
                    uint256 i = rooms.length - 1;
                    if (rooms[i].players.length > 0) {
                        s_games.push(
                            Game(
                                rooms[i].players,
                                s_betOptions[b],
                                s_roomSizeOptions[r],
                                rooms[i].id
                            )
                        );
                        _popRoom(rooms, i);
                    }
                }
            }
        }
    }

    // =========== //
    //  Game       //
    // =========== //

    function play() public {
        uint256 gamesLength = s_games.length;
        if (gamesLength == 0) {
            revert SimpleGame__NoGamesToPlay();
        }
        uint256 randomWordsLength = s_randomWords.length;
        if (randomWordsLength == 0) {
            revert SimpleGame__NoRandomWords();
        }

        uint256 max = s_maxGamesToPlay;
        if (max > gamesLength) {
            max = gamesLength;
        }
        if (max > randomWordsLength) {
            max = randomWordsLength;
        }

        for (uint j = 1; j <= max; j++) {
            playOneGame(gamesLength - j, randomWordsLength - j);
        }
    }

    function playOneGame(uint256 gameIndex, uint256 randomIndex) private {
        Game memory game = s_games[gameIndex];
        uint256 i = s_randomWords[randomIndex] % game.room;
        address winner = game.players[i];
        (uint256 amoutWin, uint256 houseFee) = _getWinSize(game.bet, game.room);
        emit GamePlayed(game.id, amoutWin, game.room, winner, game.players);
        (bool success, ) = payable(winner).call{value: amoutWin}("");
        if (!success) {
            revert SimpleGame__TransferFailed();
        }
        s_withdrawable += houseFee;
        s_games.pop();
        s_randomWords.pop();
        s_gamesPlayedHistory[game.id] = GameHistory(
            game.players,
            game.bet,
            amoutWin,
            winner
        );
    }

    /**
     * @dev This is the function that the Chainlink Keeper nodes call
     * they look for `upkeepNeeded` to return True.
     * the following should be true for this to return true:
     */
    function checkUpkeep(
        bytes memory /* checkData */
    )
        public
        view
        override
        returns (bool upkeepNeeded, bytes memory /* performData */)
    {
        bytes memory performData = "0x0";
        if (s_randomWords.length == 0) {
            bool timePassed = ((block.timestamp -
                s_lastTimeStampFromRequestRandmWords) >
                i_intervalFromLastRequestOfRandomWords);
            // no random word
            // require only in not already requested
            return (
                timePassed || s_requestRandomState == RequestNumberState.DONE,
                performData
            );
        }

        return (s_games.length > 0, performData);
    }

    function performUpkeep(bytes calldata /* performData */) external override {
        if (s_randomWords.length == 0) {
            uint256 requestId = i_vrfCoordinator.requestRandomWords(
                i_keyHash,
                i_subscriptionId,
                REQUEST_CONFIRMATIONS,
                s_requestRandomWordsCallbackGasLimit,
                s_randomWordsToRequest
            );
            s_requestRandomState = RequestNumberState.WAITING;
            emit RequestRandomNumber(requestId);
            s_lastTimeStampFromRequestRandmWords = block.timestamp;
        } else {
            if (s_games.length > 0) {
                play();
            }
        }
    }

    function fulfillRandomWords(
        uint256 /* requestId */,
        uint256[] memory randomWords
    ) internal override {
        for (uint64 i = 0; i < randomWords.length; i++) {
            s_randomWords.push(randomWords[i]);
        }
        s_requestRandomState = RequestNumberState.DONE;
        emit ReFilled(s_games.length, s_randomWords.length);
    }
}

