pragma solidity 0.8.10;

import "lib/solmate/src/tokens/ERC721.sol";
import "openzeppelin-contracts/contracts/token/ERC721/IERC721.sol";
import "openzeppelin-contracts/contracts/utils/Strings.sol";
import "openzeppelin-contracts/contracts/utils/cryptography/ECDSA.sol";
import "openzeppelin-contracts/contracts/access/Ownable.sol";
import "openzeppelin-contracts/contracts/utils/cryptography/EIP712.sol";
import "forge-std/console.sol";

contract NFTStaking is ERC721TokenReceiver, Ownable, EIP712 {
    using ECDSA for bytes32;

    struct Stake {
        address collection;
        uint256 tokenId;
        uint256 deadline;
    }

    struct Signature {
        uint8 v;
        bytes32 r;
        bytes32 s;
    }

    event Staked(
        address indexed staker,
        address indexed collection,
        uint256 indexed tokenId
    );

    event Unstaked(
        address indexed staker,
        address indexed collection,
        uint256 indexed tokenId
    );

    mapping(address => mapping(uint256 => address)) public nftToStaker; //first address is collection address, second is tokenID, third is address of its staker
    bytes32 public constant STAKE_TYPE_HASH =
        keccak256("Stake(address collection,uint256 tokenId,uint256 deadline)");

    bytes32 public DOMAIN_SEPARATOR;

    // bytes32 public constant _TYPE_HASH =
    //     keccak256(
    //         "EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)"
    //     );

    constructor() EIP712("NftStaking", "1") {
        DOMAIN_SEPARATOR = _domainSeparatorV4();
    }

    function multiStake(
        Stake[] memory nft,
        Signature[] memory signatures
    ) public {
        require(nft.length == signatures.length, "Invalid input");
        for (uint256 i = 0; i < signatures.length; i++) {
            stake(
                nft[i].collection,
                nft[i].tokenId,
                nft[i].deadline,
                signatures[i]
            );
        }
    }

    function stake(
        address collection,
        uint256 tokenId,
        uint256 deadline,
        Signature memory signature
    ) public {
        require(deadline > block.timestamp, "Signature expired");

        address signer = getSigner(
            getStakeDigest(collection, tokenId, deadline),
            signature
        );
        require(signer != address(0) && signer == owner(), "INVALID_SIGNER");

        IERC721(collection).safeTransferFrom(
            msg.sender,
            address(this),
            tokenId
        );

        nftToStaker[collection][tokenId] = msg.sender;
        emit Staked(msg.sender, collection, tokenId);
    }

    function unstake(address collection, uint256 tokenId) public {
        require(
            nftToStaker[collection][tokenId] == msg.sender,
            "You are not the owner of this NFT"
        );

        nftToStaker[collection][tokenId] = address(0);
        IERC721(collection).safeTransferFrom(
            address(this),
            msg.sender,
            tokenId
        );
        emit Unstaked(msg.sender, collection, tokenId);
    }

    function getSigner(
        bytes32 digest,
        Signature memory signature
    ) public pure returns (address) {
        unchecked {
            return ecrecover(digest, signature.v, signature.r, signature.s);
        }
    }

    function getStakeDigest(
        address collection,
        uint256 tokenId,
        uint256 deadline
    ) public view returns (bytes32) {
        return
            keccak256(
                abi.encodePacked(
                    "\x19\x01",
                    _domainSeparatorV4(),
                    keccak256(
                        abi.encode(
                            STAKE_TYPE_HASH,
                            collection,
                            tokenId,
                            deadline
                        )
                    )
                )
            );
    }
}

