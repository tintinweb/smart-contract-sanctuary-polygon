import "witnet-solidity-bridge/contracts/UsingWitnet.sol";
import "witnet-solidity-bridge/contracts/requests/WitnetRequest.sol";

contract Retreive is UsingWitnet {
    WitnetRequestTemplate public immutable witnetRequestTemplate;
    bytes32 internal __defaultWitnetSlaHash;
    uint256 internal __myAppIds;
    mapping(uint256 => uint256) internal __witnetQueryId;
    event Requested(uint requestId);

    constructor(
        WitnetRequestBoard _witnetRequestBoard,
        WitnetRequestTemplate _witnetRequestTemplate
    ) UsingWitnet(_witnetRequestBoard) {
        require(
            _witnetRequestTemplate.registry() == _witnetRequestBoard.registry(),
            "MyContract: mistmatching registries"
        );
        require(
            _witnetRequestTemplate.class() ==
                type(WitnetRequestTemplate).interfaceId,
            "MyContract: no actual WitnetRequestTemplate"
        );
        witnetRequestTemplate = _witnetRequestTemplate;
        WitnetV2.RadonSLA memory _defaultWitnetSla = WitnetV2.RadonSLA({
            numWitnesses: 5,
            witnessCollateral: 10 ** 9,
            witnessReward: 10 ** 8,
            minerCommitRevealFee: 10 ** 7,
            minConsensusPercentage: 51
        });
        __defaultWitnetSlaHash = _witnetRequestBoard.registry().verifyRadonSLA(
            _defaultWitnetSla
        );
    }

    function triggerNewRequest(
        string[][] calldata args //[['beige-female-sole-497.mypinata.cloud', 'QmSF7rDvX1Niz5Ncpnw6NPizKohyhAZGtiDpoJvgPUg7vq', '1']]
    ) external payable returns (uint256 myAppId) {
        WitnetRequest _newRequest = WitnetRequest(
            witnetRequestTemplate.buildRequest(args)
        );
        myAppId = ++__myAppIds;
        (uint256 _queryId, uint256 _usedFunds) = _witnetPostRequest(
            _newRequest.radHash(),
            __defaultWitnetSlaHash
        );
        __witnetQueryId[myAppId] = _queryId;
        if (_usedFunds < msg.value) {
            payable(msg.sender).transfer(msg.value - _usedFunds);
        }
        emit Requested(myAppId);
    }

    function checkWitnetResultStatus(
        uint256 appId
    ) public view returns (Witnet.ResultStatus) {
        return witnet.checkResultStatus(__witnetQueryId[appId]);
    }

    function processWitnetResult(
        uint256 appId
    ) public view returns (string memory _value) {
        Witnet.ResultStatus result = checkWitnetResultStatus(appId);
        if (result == Witnet.ResultStatus.Ready) {
            Witnet.Result memory _result = witnet.readResponseResult(
                __witnetQueryId[appId]
            );
            _value = Witnet.asText(_result);
            // TODO: business logic
        } else if (result == Witnet.ResultStatus.Error)
            _value = "Error";
            // TODO: business logic
        else _value = "Pending";
    }
    /*
    function processWitnetResultError(uint256 appId) public {
        if (checkWitnetResultStatus(appId) == Witnet.ResultStatus.Error) {
            Witnet.ResultError memory _error = witnet.checkResultError(
                __witnetQueryId[appId]
            );
            // TODO: business logic
        }
    }
*/
}

