        uint256 _odds,
        string memory _gameId,
        string memory _winningTeam,
        uint16 _spread
    ) public payable initializer {
        owner = _owner;
        gasLimit = _gasLimit;
        setOracle(_oracleAddress);
        factoryAddress = _factory;
        minBetAmount = _minBetAmount;
        odds = _odds;
        gameId = _gameId;
        winningTeam = _winningTeam;
        spread = _spread;
        console.log("Bet created, winningTeam, spread", winningTeam, spread);
    }

    function getMinBetAmount() public view returns (uint256) {
        return minBetAmount;
    }

    function getOdds() public view returns (uint256) {
        return odds;
    }

    function getBetAmount() public view returns (uint256) {
        return betAmount;
    }

    function getGameId() public view returns (string memory) {
        return gameId;
    }

    function getSpread() public view returns (uint16) {
        return spread;
    }

    function getWinningTeam() public view returns (string memory) {
        return winningTeam;
    }

    function getParticipant() public view returns (address[2] memory) {
        return [address(owner), address(player)];
    }

    function enterBet() public payable {
        if (msg.value < minBetAmount) {
            revert BattleBet_NotEnoughEther();
        }

        betAmount = msg.value;
        player = payable(msg.sender);
        console.log("player has entered bet ", player);

        address[] memory authSenders = new address[](2);
        authSenders[0] = owner;
        authSenders[1] = player;

        console.log("try to addAuthorizedSenders... ");
        BattleBetFactoryInterface(factoryAddress).addAuthorizedSenders(authSenders);
        console.log("Bet Amount", betAmount);
        emit BattleBetEntered(msg.sender, betAmount);
    }

    function settleBet(string calldata source) public {
        if (msg.sender != owner && msg.sender != player) {
            revert BattleBet_NotParticipant();
        }

        uint64 subscriptionId = BattleBetFactoryInterface(factoryAddress).getSubscriptionId();
        executeRequest(source, subscriptionId);
    }

    /**
     * @notice Send a request to odds API
     * @param source JavaScript source code
     * @param subscriptionId Funtions billing subscription ID
     * @return Functions request ID
     */
    function executeRequest(
        string calldata source,
        uint64 subscriptionId
    ) public returns (bytes32) {
        Functions.Request memory req;
        req.initializeRequest(Functions.Location.Inline, Functions.CodeLanguage.JavaScript, source);

        if (secrets.length > 0) {
            req.addInlineSecrets(secrets);
        }

        string[] memory args = new string[](1);
        args[0] = gameId;
        req.addArgs(args);

        console.log("chainlink oracle address: ", getChainlinkOracleAddress());
        console.log("!!!!! GAS LIMIT: ", gasLimit);
        bytes32 assignedReqID = sendRequest(req, subscriptionId, gasLimit);
        latestRequestId = assignedReqID;
        return assignedReqID;
    }

    function stringToUint(string memory s) public pure returns (uint) {
        bytes memory b = bytes(s);
        uint result = 0;
        for (uint256 i = 0; i < b.length; i++) {
            uint256 c = uint256(uint8(b[i]));
            if (c >= 48 && c <= 57) {
                result = result * 10 + (c - 48);
            }
        }
        return result;
    }

    function compareStrings(string memory a, string memory b) internal pure returns (bool) {
        return (keccak256(abi.encodePacked((a))) == keccak256(abi.encodePacked((b))));
    }

    function closeBet(string memory result) internal {
        strings.slice memory s = result.toSlice();
        strings.slice memory delim = ":".toSlice();
        string[] memory parts = new string[](s.count(delim) + 1);
        for (uint i = 0; i < parts.length; i++) {
            parts[i] = s.split(delim).toString();
        }

        uint homeScore = stringToUint(parts[1]);
        uint awayScore = stringToUint(parts[3]);
        string memory winner = homeScore > awayScore ? parts[0] : parts[2];
        address payable winnerAccount;

        console.log("!!! winning team ", winner, winningTeam);

        if (compareStrings(winner, winningTeam)) {
            console.log("!!! bet creator has won!", owner, player);
            winnerAccount = owner;
        } else {
            console.log("!!! player has won!", player, owner);
            winnerAccount = player;
        }

        console.log("!!! try to tranfer", winnerAccount, address(this).balance);
        (bool success, ) = winnerAccount.call{value: address(this).balance}("");

        if (!success) {
            revert BattleBet_WinningTransferFailed();
        }

        console.log("!!! Bet settled!", address(this), winnerAccount);
        emit BattleBetSettled(winnerAccount);
    }

    /**
     * @notice Callback that is invoked once the DON has resolved the request or hit an error
     *
     * @param requestId The request ID, returned by sendRequest()
     * @param response Aggregated response from the user code
     * @param err Aggregated error from the user code or from the execution pipeline
     * Either response or error parameter will be set, but never both
     */
    function fulfillRequest(
        bytes32 requestId,
        bytes memory response,
        bytes memory err
    ) internal override {
        latestResponse = response;
        string memory result = string(response);
        closeBet(result);
        latestError = err;
        emit OCRResponse(requestId, response, err);
    }
}

