pragma solidity 0.8.15;

import "@solmate/auth/Auth.sol";
import {ERC721} from "@solmate/tokens/ERC721.sol";
import {LibString} from "@solmate/utils/LibString.sol";

error NonExistentTokenId();

contract ChampionNFT is ERC721, Auth {
    using LibString for uint256;

    constructor() ERC721("Champion NFT", "CHAMP") Auth(msg.sender, Authority(address(this))) {}

    function tokenURI(uint256 id) public view virtual override returns (string memory) {
        if (ownerOf(id) == address(0)) {
            revert NonExistentTokenId();
        }

        return LibString.toString(id);
    }

    function mint(address to, uint256 id) public {
        _safeMint(to, id);
    }

    function burn(uint256 id) public {
        _burn(id);
    }

    function approve(address spender, uint256 id) public override requiresAuth {
        approve(spender, id);
    }

    function transferFrom(address from, address to, uint256 id) public override requiresAuth {
        safeTransferFrom(from, to, id);
    }
}

