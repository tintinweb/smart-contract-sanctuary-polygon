pragma solidity 0.8.15;

import "@solmate/tokens/ERC20.sol";
import "@solmate/auth/Owned.sol";

import "../interfaces/IChampionNFT.sol";
import "../interfaces/IGameLogic.sol";
import "../interfaces/IPaymentToken.sol";

error AlreadyGuessed();
error InsufficientDeposit();

contract GameLogic is IGameLogic, Owned {
    event EntranceFeeReset(uint256 indexed fee);
    event GameReset();
    event Guess(address indexed player, Colour indexed colour);
    event RewardsPaid(address indexed player, uint256 indexed amount);

    enum Colour {
        Red,
        Black
    }

    Colour public colour;
    uint256 public currentTokenId;
    uint256 public entranceFee;

    IPaymentToken paymentToken;
    IChampionNFT championNFT;

    mapping(address => bool) private haveGuessed;

    constructor(address _paymentToken, address _championNFT, uint256 _entranceFee) Owned(msg.sender) {
        entranceFee = _entranceFee;
        currentTokenId = 0;

        paymentToken = IPaymentToken(_paymentToken);
        championNFT = IChampionNFT(_championNFT);
    }

    function setUpGame(Colour initialColour) external payable {
        paymentToken.deposit(address(this), entranceFee);
        _reset(initialColour);
    }

    function play(Colour guessColour, uint256 depositAmount) external {
        address player = msg.sender;

        if (haveGuessed[player]) {
            revert AlreadyGuessed();
        }

        if (depositAmount < entranceFee) {
            revert InsufficientDeposit();
        }

        haveGuessed[player] = true;

        paymentToken.deposit(address(this), depositAmount);

        emit Guess(player, guessColour);

        if (colour == guessColour) {
            payRewards(player, depositAmount);
        }

        _reset(guessColour);
    }

    function setHasGuessed(address player) public onlyOwner {
        require(haveGuessed[player] == false);
        haveGuessed[player] = true;
    }

    function setEntranceFee(uint256 _entranceFee) external onlyOwner {
        entranceFee = _entranceFee;
        emit EntranceFeeReset(entranceFee);
    }

    function withdraw(uint256 amount) external onlyOwner {
        paymentToken.transfer(msg.sender, amount);
    }

    function payRewards(address champion, uint256 depositAmount) public {
        uint256 reward = depositAmount * 2;
        paymentToken.transfer(champion, reward);
        championNFT.mint(champion, currentTokenId);

        emit RewardsPaid(champion, reward);

        currentTokenId += 1;
    }

    function _reset(Colour _colour) internal {
        colour = _colour;
        emit GameReset();
    }
}

// SPDX-License-Identifier: MIT
pragma solidity 0.8.15;

import "@solmate/tokens/ERC20.sol";
import "@solmate/auth/Owned.sol";

import "../interfaces/IPaymentToken.sol";

import {SafeTransferLib} from "@solmate/utils/SafeTransferLib.sol";

contract PaymentToken is ERC20, Owned {
    using SafeTransferLib for address;

    event Deposit(address indexed from, uint256 amount);
    event Withdrawal(address indexed to, uint256 amount);

    constructor() ERC20("Payment Token", "PTOK", 18) Owned(msg.sender) {}

    function deposit(address to, uint256 amount) public payable {
        _mint(to, amount);
        emit Deposit(to, amount);
    }

    function depositAsEOA() public payable {
        _mint(msg.sender, msg.value);
        emit Deposit(msg.sender, msg.value);
    }

    function withdraw(uint256 amount) public {
        _burn(msg.sender, amount);
        emit Withdrawal(msg.sender, amount);

        msg.sender.safeTransferETH(amount);
    }

    receive() external payable virtual {
        depositAsEOA();
    }
}

