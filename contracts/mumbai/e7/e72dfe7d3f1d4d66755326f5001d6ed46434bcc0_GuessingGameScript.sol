pragma solidity 0.8.15;

import "forge-std/Script.sol";

import "../contracts/ChampionNFT.sol";
import "../contracts/GameLogic.sol";
import "../contracts/PaymentToken.sol";

contract GuessingGameScript is Script {
    function run() external {
        GameLogic.Colour initialColour;

        ChampionNFT championNFT;
        GameLogic gameLogic;
        PaymentToken paymentToken;

        vm.startBroadcast();

        paymentToken = new PaymentToken();
        championNFT = new ChampionNFT();
        gameLogic = new GameLogic(address(paymentToken), address(championNFT), 20);

        vm.deal(address(this), 20);
        gameLogic.setUpGame{value: 20}(initialColour);

        vm.stopBroadcast();
    }
}

