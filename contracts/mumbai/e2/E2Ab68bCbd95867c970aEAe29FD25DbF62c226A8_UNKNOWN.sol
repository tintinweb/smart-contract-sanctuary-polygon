}

struct Hand {
    bool hasPotato;
    uint32 generation;
    uint8 body;
    uint8 helm;
    uint8 mainhand;
    uint8 offhand;
}

enum GameState {
    Queued,
    Minting,
    Playing,
    Paused,
    FinalRound,
    Ended
}

struct RequestStatus {
    uint256[] randomWord;
    bool fulfilled;
    bool exists;
}

