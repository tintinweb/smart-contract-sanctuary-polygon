    function hasReferrer(Tree storage self, address account) internal view returns (bool) {
        return self.linearTree[account].referrer != address(0);
    }

    /**
     * @dev Utils function for check whether an address is circular reference
     */
    function isCircularReference(Tree storage self, address referrer, address referee) internal view returns (bool) {
        address parent = referrer;

        while (parent != address(0)) {
            if (parent == referee) {
                return true;
            }
            parent = self.linearTree[parent].referrer;
        }

        return false;
    }

    /**
     * @dev A function that returns the random distribution direction.
     */
    function _randomDirection() private view returns (Direction direction) {
        uint256 random = uint256(keccak256(abi.encodePacked(block.prevrandao))) % uint256(10000);
        if (random < uint256(5000)) return Direction.RIGHT;
        else return Direction.LEFT;
    }

    function lastLeftBinaryNode(Tree storage self, address account) internal view returns (address) {
        address leftChild = account;
        while (self.binaryTree[leftChild].left != address(0)) {
            leftChild = self.binaryTree[leftChild].left;
        }
        return leftChild;
    }

    function lastRightBinaryNode(Tree storage self, address account) internal view returns (address) {
        address rightChild = account;
        while (self.binaryTree[rightChild].right != address(0)) {
            rightChild = self.binaryTree[rightChild].right;
        }
        return rightChild;
    }

    function nextBinaryNearestFreeChildren(
        Tree storage self,
        address account
    ) internal view returns (address, address) {
        address leftChild = account;
        address rightChild = account;
        while (self.binaryTree[leftChild].left != address(0) && self.binaryTree[rightChild].right != address(0)) {
            leftChild = self.binaryTree[leftChild].left;
            rightChild = self.binaryTree[rightChild].right;
        }
        return (
            self.binaryTree[leftChild].left == address(0) ? leftChild : address(0),
            self.binaryTree[rightChild].right == address(0) ? rightChild : address(0)
        );
    }

    /**
     * @dev Utils function for get next free node in binary tree
     */
    function nextFreeBinaryNode(Tree storage self, address account) internal view returns (address, Direction) {
        // first referee
        if (self.linearTree[account].referees.length == 1) {
            address parent = self.binaryTree[account].parent;
            if (self.binaryTree[parent].left == account) {
                return (lastLeftBinaryNode(self, account), Direction.LEFT);
            } else if (self.binaryTree[parent].right == account) {
                return (lastRightBinaryNode(self, account), Direction.RIGHT);
            }
        }

        (address left, address right) = nextBinaryNearestFreeChildren(self, account);
        if (right == address(0)) {
            return (left, Direction.LEFT);
        } else if (left == address(0)) {
            return (right, Direction.RIGHT);
        } else {
            Direction direction = self.binaryTree[account].direction;
            if ((direction == Direction.RANDOM && _randomDirection() == Direction.LEFT) || direction == Direction.LEFT)
                return (left, Direction.LEFT);
            return (right, Direction.RIGHT);
        }
    }

