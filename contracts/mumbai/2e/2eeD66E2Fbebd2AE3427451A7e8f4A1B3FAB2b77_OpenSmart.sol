library Timestamp {
    /** @dev The number of seconds in a day. 60*60*24=86400. */
    uint256 public constant SECONDS_PER_DAY = 24 * 60 * 60;
    /** @dev UTC+3 time zone. */
    uint256 constant UTC_OFFSET = 3 hours;

    function convertToUTC3(uint timestamp) internal pure returns (uint256) {
        return timestamp + UTC_OFFSET;
    }

    function isSameDate(uint256 timestamp1, uint256 timestamp2) internal pure returns (bool) {
        return getDay(convertToUTC3(timestamp1)) == getDay(convertToUTC3(timestamp2));
    }

    function isSameWeek(uint256 timestamp1, uint256 timestamp2) internal pure returns (bool) {
        return getWeekNumber(convertToUTC3(timestamp1)) == getWeekNumber(convertToUTC3(timestamp2));
    }

    function isSameMonth(uint256 timestamp1, uint256 timestamp2) internal pure returns (bool) {
        return getMonth(convertToUTC3(timestamp1)) == getMonth(convertToUTC3(timestamp2));
    }

    function isSameYear(uint256 timestamp1, uint256 timestamp2) internal pure returns (bool) {
        return getYear(convertToUTC3(timestamp1)) == getYear(convertToUTC3(timestamp2));
    }

    function getDay(uint256 timestamp) internal pure returns (uint256) {
        return (timestamp) / SECONDS_PER_DAY;
    }

    function getWeekNumber(uint256 timestamp) internal pure returns (uint256) {
        return ((timestamp + 1 days) / 1 weeks);
    }

    function getMonth(uint256 timestamp) internal pure returns (uint256) {
        return (uint256(((timestamp / SECONDS_PER_DAY + 4) * 1461) / 4 - 1) / 12053) + 1;
    }

    function getYear(uint timestamp) internal pure returns (uint256) {
        uint secondsPerYear = 31536000; // number of seconds in a regular year
        uint leapSeconds = 86400; // one extra second in a leap year
        uint numYearsSinceEpoch = (timestamp - 946684800 + UTC_OFFSET) / (secondsPerYear + leapSeconds);
        return (numYearsSinceEpoch + 1970);
    }

    function getCurrentTimestamp() internal view returns (uint256) {
        return convertToUTC3(block.timestamp);
    }
}

