    function register(Tree storage self, address account, address referrer, bool binaryOn) internal {
        require(referrer != address(0), "Referrer cannot be 0x0 address");
        require(!isCircularReference(self, referrer, account), "Referee cannot be one of referrer upline");
        require(!hasReferrer(self, account), "Address have been registered upline");

        self.linearTree[account].referrer = referrer;
        self.linearTree[account].registeredAt = uint48(block.timestamp);
        self.linearTree[referrer].referees.push(account);

        if (binaryOn) {
            (address parent, Direction direction) = nextFreeBinaryNode(self, referrer);
            self.binaryTree[account].parent = parent;
            if (direction == Direction.LEFT) {
                self.binaryTree[parent].left = account;
            } else {
                self.binaryTree[parent].right = account;
            }
        }

        emit Registered(account, referrer);
    }

    function claimRefReward(Tree storage self, address account, uint256 amount, uint256 timestamp) internal {
        self.linearTree[account].lastClaimedAt = uint48(timestamp);
        self.linearTree[account].claims.push(ClaimData(amount, timestamp));

        emit RefRewardClaimed(account, amount);
    }

    function claimBinaryReward(
        Tree storage self,
        address account,
        uint256 amount,
        uint256 leftTurnOver,
        uint256 rightTurnOver,
        uint256 timestamp
    ) internal {
        BinaryNode storage binaryNode = self.binaryTree[account];
        require(!Timestamp.isSameDate(binaryNode.lastClaimedAt, timestamp), "Already claimed today");

        binaryNode.leftTurnOver = uint96(leftTurnOver);
        binaryNode.rightTurnOver = uint96(rightTurnOver);
        binaryNode.lastClaimedAt = uint48(timestamp);
        binaryNode.claims.push(ClaimData(amount, timestamp));

        emit BinaryRewardClaimed(account, amount);
    }
}

