     * @param right The child node's address at right
     * @param rightTurnOver uint96
     * @param claims ClaimData[]
     */
    struct BinaryNode {
        Direction direction;
        uint48 lastClaimedAt;
        address parent;
        address left;
        uint96 leftTurnOver;
        address right;
        uint96 rightTurnOver;
        ClaimData[] claims;
    }

    /**
     * @dev Tree
     * @param linearTree - Table of nodes of the linear tree.
     * @param binaryTree - Table of nodes of the binary tree.
     */
    struct Tree {
        mapping(address => LinearNode) linearTree;
        mapping(address => BinaryNode) binaryTree;
    }

    // Events
    event Registered(address indexed account, address indexed referrer);
    event RefRewardClaimed(address indexed account, uint256 amount);
    event BinaryRewardClaimed(address indexed account, uint256 amount);

    /**
     * @dev Utils function for check whether an address has the referrer
