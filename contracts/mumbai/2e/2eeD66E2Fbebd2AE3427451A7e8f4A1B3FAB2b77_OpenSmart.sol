contract OpenSmart is
    Initializable,
    UUPSUpgradeable,
    EIP712Upgradeable,
    ReentrancyGuardUpgradeable,
    AccessControlUpgradeable,
    PausableUpgradeable,
    IERC721ReceiverUpgradeable
{
    using AddressUpgradeable for address payable;
    using ArraysUpgradeable for address[];
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;
    using Referral for Referral.Tree;
    using SafeMathUpgradeable for uint256;

    bytes32 public constant MODERATOR_ROLE = keccak256("MODERATOR_ROLE");
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");

    // solhint-disable-next-line var-name-mixedcase
    bytes32 private constant _CLAIM_REF_REWARD_TYPEHASH =
        keccak256("claimRefRewardBySignature(address sender,uint256 amount,uint256 to,uint256 nonce)");
    // solhint-disable-next-line var-name-mixedcase
    bytes32 private constant _CLAIM_BINARY_REWARD_TYPEHASH =
        keccak256(
            "claimBinaryRewardBySignature(address sender,uint256 amount,uint256 leftTurnOver,uint256 rightTurnOver,uint256 to,uint256 nonce)"
        );

    /** @dev Divisor for calculating percentages. 100/10000=0.01=1%. */
    uint256 public constant DECIMALS = 10_000;

    /** @dev The GrabCoinClub contract. */
    IGrabCoinClub public nftCollection;
    /** @dev The status of binary tree. */
    bool public isBinaryOn;
    uint256 public startedAt;

    struct PurchaseData {
        uint256 amount;
        uint256 purchasedAt;
    }

    mapping(address => PurchaseData[]) public purchases;

    struct StakeData {
        bool isStaking;
        uint16 tokenId;
        uint48 stakedAt;
        uint48 unstakedAt;
    }
    /** @dev Mapping of account address to StakerData. */
    mapping(address => StakeData) public stakers;
    mapping(uint256 => EnumerableSetUpgradeable.UintSet) private reserved; // series => token id set

    address public verifier;
    mapping(address => CountersUpgradeable.Counter) private _nonces;

    Referral.Tree private refTree;

    /** @dev Price of each series. */
    uint256[] public prices;
    /** @dev Referral rate for every level. [series][level] */
    uint16[][] public refRate;
    /** @dev Binary rate for every level. */
    uint16[] public binaryRate;

    uint256 public mintLimit;
    mapping(address => mapping(uint256 => uint256)) public mintsOf;

    event VerifierChanged(address indexed verifier, address indexed newVerifier);
    event Minted(address indexed account, uint256 indexed series, uint256 indexed quantity, uint256 price);
    event Staked(address indexed account, uint256 indexed tokenId);
    event Unstaked(address indexed account, uint256 indexed tokenId);
    event UpgradedStakedToken(address indexed account, uint256 indexed tokenId, uint256 newSeries);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(IGrabCoinClub _nftCollection, bool _isBinaryOn) public initializer {
        __EIP712_init("OpenSmart", "1");
        __ReentrancyGuard_init();
        __Pausable_init();
        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MODERATOR_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);

        nftCollection = _nftCollection;
        isBinaryOn = _isBinaryOn;
        startedAt = getCurrentTimestamp();
        verifier = _msgSender();
        mintLimit = 5;
    }

    receive() external payable {}

    function getReserved(uint256 series) public view returns (bytes32[] memory) {
        return reserved[series]._inner._values;
    }

    function _mint(uint256 series, uint256 amount) internal whenNotPaused {
        require(amount == prices[series], "Insufficient amount");
        require(mintsOf[_msgSender()][series] < mintLimit, "Mint limit");

        EnumerableSetUpgradeable.UintSet storage tokenIds = reserved[series];
        if (tokenIds.length() > 0) {
            uint256 tokenId = tokenIds.at(0);
            nftCollection.safeTransferFrom(address(this), _msgSender(), tokenIds.at(0));
            tokenIds.remove(tokenId);
        } else {
            address[] memory receivers = new address[](1);
            receivers[0] = _msgSender();
            nftCollection.mintAirdrop(series, receivers);
        }

        ++mintsOf[_msgSender()][series];

        emit Minted(_msgSender(), series, 1, amount);
    }

    function mint(uint256 series, address referrer) external payable {
        if (referrer != address(0) && !refTree.hasReferrer(_msgSender())) {
            register(referrer);
        }

        _mint(series, msg.value);

        purchases[_msgSender()].push(PurchaseData(msg.value, getCurrentTimestamp()));
    }

    function _stake(uint256 tokenId) internal whenNotPaused {
        StakeData storage stakeData = stakers[_msgSender()];
        require(!stakeData.isStaking, "Already staked");
        require(
            MathUpgradeable.max(startedAt, stakeData.unstakedAt) <= nftCollection.mintedAt(tokenId),
            "You need to mint a new one"
        );

        nftCollection.safeTransferFrom(_msgSender(), address(this), tokenId);

        stakeData.tokenId = uint16(tokenId);
        stakeData.stakedAt = uint48(getCurrentTimestamp());
        stakeData.isStaking = true;

        emit Staked(_msgSender(), tokenId);
    }

    function stake(uint256 tokenId) external {
        require(nftCollection.rawOwnerOf(tokenId) == _msgSender(), "Invalid token id");

        _stake(tokenId);
    }

    function unstake(uint256 tokenId) external whenNotPaused {
        StakeData storage stakeData = stakers[_msgSender()];
        require(stakeData.isStaking && stakeData.tokenId == tokenId, "Not staked token");

        nftCollection.safeTransferFrom(address(this), _msgSender(), tokenId);

        stakeData.unstakedAt = uint48(getCurrentTimestamp());
        stakeData.isStaking = false;

        emit Unstaked(_msgSender(), tokenId);
    }

    function upgrade(uint256 tokenId, uint256 series) external payable {
        StakeData storage stakeData = stakers[_msgSender()];
        require(stakeData.isStaking && stakeData.tokenId == tokenId, "Not staked token");
        uint256 _series = nftCollection.seriesOf(tokenId);
        require(_series < series, "The next series must be above the current series");

        stakeData.unstakedAt = uint48(getCurrentTimestamp());
        stakeData.isStaking = false;
        reserved[_series].add(tokenId);

        uint256 amount = prices[_series] + msg.value;
        _mint(series, amount);

        uint256[] memory tokenIds = nftCollection.tokensOfOwner(_msgSender());
        uint256 newTokenId = tokenIds[tokenIds.length - 1];
        _stake(newTokenId);

        purchases[_msgSender()].push(PurchaseData(msg.value, getCurrentTimestamp()));

        emit UpgradedStakedToken(_msgSender(), tokenId, series);
    }

    function calcSaleVolume(address account, uint256 from, uint256 to) public view returns (uint256 volume) {
        PurchaseData[] storage purchase = purchases[account];
        uint256 length = purchase.length;
        for (uint256 i; i < length; ) {
            uint256 purchasedAt = purchase[i].purchasedAt;
            if (from < purchasedAt && purchasedAt <= to) {
                volume = volume.add(purchase[i].amount);
            }

            unchecked {
                ++i;
            }
        }

        return volume;
    }

    function _calcLinearReward(
        address account,
        uint256 from,
        uint256 to,
        uint256 series,
        uint256 depth
    ) internal view returns (uint256 reward) {
        if (refRate[series][depth] > 0) {
            address[] storage referees = refTree.linearTree[account].referees;
            uint256 length = referees.length;
            for (uint256 i; i < length; ) {
                address child = referees.unsafeAccess(i).value;
                reward = reward.add(
                    MathUpgradeable.mulDiv(calcSaleVolume(child, from, to), refRate[series][depth], DECIMALS)
                );
                reward = reward.add(_calcLinearReward(child, from, to, series, depth + 1));

                unchecked {
                    ++i;
                }
            }
        }

        return reward;
    }

    function _calcLinearReward(address account, uint256 to) internal view returns (uint256) {
        if (stakers[account].isStaking) {
            Referral.LinearNode storage node = refTree.linearTree[account];
            uint256 from = MathUpgradeable.max(startedAt, stakers[account].stakedAt);
            from = MathUpgradeable.max(from, node.lastClaimedAt);
            if (from < to)
                return _calcLinearReward(account, from, to, nftCollection.seriesOf(stakers[account].tokenId), 0);
        }

        return 0;
    }

    function calcLinearReward(address account) public view returns (uint256) {
        return _calcLinearReward(account, getCurrentTimestamp());
    }

    function calcBinaryChildSaleVolume(address account, uint256 from, uint256 to) public view returns (uint256 volume) {
        volume = calcSaleVolume(account, from, to);

        Referral.BinaryNode storage node = refTree.binaryTree[account];
        address left = node.left;
        address right = node.right;
        if (left != address(0)) {
            volume = volume.add(calcBinaryChildSaleVolume(left, from, to));
        }
        if (right != address(0)) {
            volume = volume.add(calcBinaryChildSaleVolume(right, from, to));
        }
        return volume;
    }

    function checkActivityBinary(address account, uint256 to) public view returns (bool) {
        uint256 count;
        uint256 from = to - 30 days;
        address[] storage referees = refTree.linearTree[account].referees;
        for (uint256 i = referees.length; i > 0; ) {
            address child = referees.unsafeAccess(i - 1).value;
            uint256 registeredAt = refTree.linearTree[child].registeredAt;
            if (from < registeredAt && registeredAt <= to) {
                PurchaseData[] storage purchase = purchases[child];
                for (uint256 j = purchase.length; j > 0; ) {
                    uint256 purchasedAt = purchase[j - 1].purchasedAt;
                    if (from < purchasedAt && purchasedAt <= to) {
                        if (purchase[j - 1].amount >= prices[3]) {
                            unchecked {
                                ++count;
                            }
                            break;
                        }
                    } else break;

                    unchecked {
                        --j;
                    }
                }

                if (count >= 2) return true;
            } else break;

            unchecked {
                --i;
            }
        }
        return false;
    }

    function _calcBinaryReward(address account, uint256 to) internal view returns (uint256, uint256, uint256) {
        Referral.BinaryNode storage node = refTree.binaryTree[account];
        if (stakers[account].isStaking && checkActivityBinary(account, to)) {
            uint256 from = MathUpgradeable.max(startedAt, stakers[account].stakedAt);
            from = MathUpgradeable.max(from, node.lastClaimedAt);
            if (from < to) {
                uint256 leftVolume = node.leftTurnOver + calcBinaryChildSaleVolume(node.left, from, to);
                uint256 rightVolume = node.rightTurnOver + calcBinaryChildSaleVolume(node.right, from, to);
                uint256 minVolume = MathUpgradeable.min(leftVolume, rightVolume);
                uint256 series = nftCollection.seriesOf(stakers[account].tokenId);
                return (
                    MathUpgradeable.min(
                        MathUpgradeable.mulDiv(minVolume, binaryRate[series], DECIMALS),
                        prices[series]
                    ),
                    leftVolume - minVolume,
                    rightVolume - minVolume
                );
            }
        }

        return (0, node.leftTurnOver, node.rightTurnOver);
    }

    function calcBinaryReward(address account) public view returns (uint256, uint256, uint256) {
        return _calcBinaryReward(account, getCurrentTimestamp());
    }

    function getPurchase(address account) public view returns (PurchaseData[] memory) {
        return purchases[account];
    }

    function getLinearNode(address account) public view returns (Referral.LinearNode memory) {
        return refTree.linearTree[account];
    }

    function getBinaryNode(address account) public view returns (Referral.BinaryNode memory) {
        return refTree.binaryTree[account];
    }

    function getLastLeftBinaryNode(address account) external view returns (address) {
        return refTree.lastLeftBinaryNode(account);
    }

    function getLastRightBinaryNode(address account) external view returns (address) {
        return refTree.lastRightBinaryNode(account);
    }

    function setBinaryDirection(Referral.Direction direction) external {
        refTree.binaryTree[_msgSender()].direction = direction;
    }

    function setBinaryDirection(address account, Referral.Direction direction) external onlyRole(MODERATOR_ROLE) {
        refTree.binaryTree[account].direction = direction;
    }

    function register(address referrer) public {
        _register(_msgSender(), referrer);
    }

    function register(address account, address referrer) external onlyRole(MODERATOR_ROLE) {
        _register(account, referrer);
    }

    function _register(address account, address referrer) internal whenNotPaused nonReentrant {
        refTree.register(account, referrer, isBinaryOn);
    }

    function getCurrentTimestamp() public view returns (uint256) {
        return Timestamp.getCurrentTimestamp();
    }

    function isRegistered(address account) public view returns (bool) {
        return refTree.hasReferrer(account);
    }

    function claimRefReward() external whenNotPaused nonReentrant {
        address payable account = payable(_msgSender());
        uint256 to = getCurrentTimestamp();
        uint256 reward = _calcLinearReward(account, to);
        require(reward > 0, "No claimable reward");

        account.sendValue(reward);

        refTree.claimRefReward(account, reward, to);
    }

    function claimRefRewardBySignature(
        uint256 amount,
        uint256 to,
        bytes memory signature
    ) external whenNotPaused nonReentrant {
        address payable account = payable(_msgSender());
        bytes32 structHash = keccak256(abi.encode(_CLAIM_REF_REWARD_TYPEHASH, account, amount, to, _useNonce(account)));
        bytes32 hash = _hashTypedDataV4(structHash);
        require(ECDSAUpgradeable.recover(hash, signature) == verifier, "Invalid verifier");

        account.sendValue(amount);

        refTree.claimRefReward(account, amount, to);
    }

    function claimBinaryReward() public whenNotPaused nonReentrant {
        address payable account = payable(_msgSender());
        uint256 to = getCurrentTimestamp();
        (uint256 reward, uint256 leftTurnOver, uint256 rightTurnOver) = _calcBinaryReward(account, to);
        require(reward > 0, "No claimable reward");

        account.sendValue(reward);

        refTree.claimBinaryReward(account, reward, leftTurnOver, rightTurnOver, to);
    }

    function claimBinaryRewardBySignature(
        uint256 amount,
        uint256 leftTurnOver,
        uint256 rightTurnOver,
        uint256 to,
        bytes memory signature
    ) external whenNotPaused nonReentrant {
        address payable account = payable(_msgSender());
        bytes32 structHash = keccak256(
            abi.encode(
                _CLAIM_BINARY_REWARD_TYPEHASH,
                account,
                amount,
                leftTurnOver,
                rightTurnOver,
                to,
                _useNonce(account)
            )
        );
        bytes32 hash = _hashTypedDataV4(structHash);
        require(ECDSAUpgradeable.recover(hash, signature) == verifier, "Invalid verifier");

        account.sendValue(amount);

        refTree.claimBinaryReward(account, amount, leftTurnOver, rightTurnOver, to);
    }

    function setPrices(uint256[] calldata _prices) external onlyRole(DEFAULT_ADMIN_ROLE) {
        prices = _prices;
    }

    function setRefRate(uint16[][] memory _refLevelRate) external onlyRole(DEFAULT_ADMIN_ROLE) {
        refRate = _refLevelRate;
    }

    function setBinaryRate(uint16[] calldata _binaryRate) external onlyRole(DEFAULT_ADMIN_ROLE) {
        binaryRate = _binaryRate;
    }

    function setMintLimit(uint256 _mintLimit) public onlyRole(MODERATOR_ROLE) {
        mintLimit = _mintLimit;
    }

    function pause() public onlyRole(MODERATOR_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(MODERATOR_ROLE) {
        _unpause();
    }

    function _authorizeUpgrade(address newImplementation) internal override onlyRole(UPGRADER_ROLE) {}

    function onERC721Received(address, address, uint256, bytes memory) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }

    function withdraw(uint256 amount) external onlyRole(DEFAULT_ADMIN_ROLE) {
        payable(_msgSender()).sendValue(amount);
    }

    function transfer(uint256 tokenId, address to) external onlyRole(DEFAULT_ADMIN_ROLE) {
        EnumerableSetUpgradeable.UintSet storage tokenIds = reserved[nftCollection.seriesOf(tokenId)];
        require(tokenIds.contains(tokenId), "You can't transfer this token");

        nftCollection.safeTransferFrom(address(this), to, tokenId);
        tokenIds.remove(tokenId);
    }

    /** @dev Sets the address of the permission signer. */
    function setVerifier(address newVerifier) external onlyRole(DEFAULT_ADMIN_ROLE) {
        emit VerifierChanged(verifier, newVerifier);
        verifier = newVerifier;
    }

    /** @dev Returns the nonce for the account. */
    function nonces(address account) public view returns (uint256) {
        return _nonces[account].current();
    }

    /** @dev "Consume a nonce": return the current value and increment. */
    function _useNonce(address owner) internal virtual returns (uint256 current) {
        CountersUpgradeable.Counter storage nonce = _nonces[owner];
        current = nonce.current();
        nonce.increment();
    }
}