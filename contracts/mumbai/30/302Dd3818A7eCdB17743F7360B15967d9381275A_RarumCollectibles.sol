pragma solidity ^0.8.16;

import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155SupplyUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "./GenericTypedMessage.sol";
import "hardhat/console.sol";

contract RarumCollectibles is
    Initializable,
    ERC1155Upgradeable,
    AccessControlUpgradeable,
    PausableUpgradeable,
    ERC1155BurnableUpgradeable,
    ERC1155SupplyUpgradeable,
    ReentrancyGuardUpgradeable,
    GenericTypedMessage
{
    bytes32 public constant URI_SETTER_ROLE = keccak256("URI_SETTER_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    string public constant VERSION = "v1";

    bytes32 private constant _TRANSFER_WITH_AUTHORIZATION_TYPEHASH = keccak256("TransferAuthorization(address signer,address to,uint256[] ids,uint256[] amounts,uint256 deadline,bytes32 authorization)");
    bytes32 private constant _SET_OPERATOR_AUTHORIZATION_TYPEHASH = keccak256("OperatorAuthorization(address owner,address operator,bool approved,uint256 deadline,bytes32 authorization)");

    string public name;
    string public symbol;
    address public owner;
    mapping (uint256 => string) private _tokenURIs;
    uint256[] private registeredTokens;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    struct OperatorAuthorization {
        address owner;
        address operator;
        bool approved;
        uint32 deadline;
        bytes32 authorization;
        uint8 v;
        bytes32 r;
        bytes32 s;
    }

    struct TransferAuthorization {
        address signer;
        address to;
        uint256[] ids;
        uint256[] amounts;
        uint32 deadline;
        bytes32 authorization;
        uint8 v;
        bytes32 r;
        bytes32 s;
    }

    modifier onlyRegisteredIds(uint256[] memory ids) {
        for (uint i = 0; i < ids.length; i++) {
            require(_isRegistered(ids[i]), "RarumCollectibles: token is not registered");
        }
        _;
    }

    function initialize(
        string calldata _name,
        string calldata _symbol,
        address _operator
    )
        public
        initializer
    {
        __ERC1155_init("https://ipfs.io/ipfs/");
        __AccessControl_init();
        __Pausable_init();
        __ERC1155Burnable_init();
        __ERC1155Supply_init();
        __GenericTypedMessage_init();
        __ReentrancyGuard_init();

        _grantRole(DEFAULT_ADMIN_ROLE, _operator);

        name = _name;
        symbol = _symbol;
        owner = _operator;

        emit OwnershipTransferred(address(0), _operator);
    }

    /**************************************
     ROLE-RESTRICTED WRITE FUNCTIONS
    ***************************************/

    function setBaseURI(string memory newuri) public onlyRole(URI_SETTER_ROLE) {
        super._setURI(newuri);
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function registerId(
        uint256 id,
        string calldata cid
    )
        external
        onlyRole(MINTER_ROLE)
    {
        require(!_isRegistered(id), "RarumCollectibles: token already registered");

        registeredTokens.push(id);

        _setTokenURI(id, cid);
    }

    function registerIdBatch(
        uint256[] calldata ids,
        string[] calldata cids
    )
        external
        onlyRole(MINTER_ROLE)
    {
        require(ids.length == cids.length, "RarumCollectibles: ids and cids length mismatch");

        for (uint i = 0; i < ids.length; i++) {
            require(!_isRegistered(ids[i]), "RarumCollectibles: token already registered");

            registeredTokens.push(ids[i]);
            _setTokenURI(ids[i], cids[i]);
        }
    }

    function updateTokenURI(
        uint256 id,
        string memory cid
     )
        external
        onlyRole(ADMIN_ROLE)
    {
        require(_isRegistered(id), "RarumCollectibles: token not registered");

        _setTokenURI(id, cid);
    }

    /**
     * @dev Internal function to set the token cid for a given token.
     * @param tokenId uint256 ID of the token to set its URI
     * @param cid string ipfs cid to assign
     */
    function _setTokenURI(uint256 tokenId, string memory cid) internal {
        _tokenURIs[tokenId] = string.concat(super.uri(0), cid);

        emit URI(_tokenURIs[tokenId], tokenId);
    }

    function mint(
        address account,
        uint256 id,
        uint256 amount,
        bytes memory data
    )
        public
        onlyRole(MINTER_ROLE)
    {
        require(_isRegistered(id), "RarumCollectibles: token is not registered");

        _mint(account, id, amount, data);
    }

    function mintBatch(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    )
        public
        onlyRegisteredIds(ids)
        onlyRole(MINTER_ROLE)
    {
        _mintBatch(to, ids, amounts, data);
    }

    /**
     * @dev Updates owner
     * @param _newOwner new owner's address
     */
    function setOwner(address _newOwner)
      external
      onlyRole(ADMIN_ROLE)
    {
        require(_newOwner != address(0), "RarumCollectibles: invalid owner address");

        emit OwnershipTransferred(owner, _newOwner);

        owner = _newOwner;
    }

    /**************************************
     USER WRITE FUNCTIONS
    ***************************************/

    function authorizeOperator(
        OperatorAuthorization calldata _authorization
    )
        external
        nonReentrant
        whenNotPaused
    {
        require(_verifyOperatorSignature(_authorization), "RarumCollectibles: invalid authorization");

        _setApprovalForAll(
            _authorization.owner,
            _authorization.operator,
            _authorization.approved
        );
    }

    function _verifyOperatorSignature(
        OperatorAuthorization memory _authorization
    )
        internal
        returns (bool)
    {
        bytes32 structHash = keccak256(
            abi.encode(
                _SET_OPERATOR_AUTHORIZATION_TYPEHASH,
                _authorization.owner,
                _authorization.operator,
                _authorization.approved,
                _authorization.deadline,
                _authorization.authorization
            )
        );

        return _verifyTypedMessage(
            structHash,
            _authorization.authorization,
            _authorization.deadline,
            _authorization.owner,
            _authorization.v,
            _authorization.r,
            _authorization.s
        );
    }

    function authorizedTransfer(
        TransferAuthorization calldata _authorization
    )
        external
        nonReentrant
        whenNotPaused
        onlyRegisteredIds(_authorization.ids)
    {
        require(_verifyTransferSignature(_authorization), "RarumCollectibles: invalid authorization");

        _safeBatchTransferFrom(
            _authorization.signer,
            _authorization.to,
            _authorization.ids,
            _authorization.amounts,
            new bytes(0)
        );
    }

    function _verifyTransferSignature(
        TransferAuthorization memory _authorization
    )
        internal
        returns (bool)
    {
        bytes32 structHash = keccak256(
            abi.encode(
                _TRANSFER_WITH_AUTHORIZATION_TYPEHASH,
                _authorization.signer,
                _authorization.to,
                keccak256(abi.encodePacked(_authorization.ids)),
                keccak256(abi.encodePacked(_authorization.amounts)),
                _authorization.deadline,
                _authorization.authorization
            )
        );

        return _verifyTypedMessage(
            structHash,
            _authorization.authorization,
            _authorization.deadline,
            _authorization.signer,
            _authorization.v,
            _authorization.r,
            _authorization.s
        );
    }

    /**************************************
     READ FUNCTIONS
    ***************************************/

    function getRegisteredTokens()
        external view returns (uint256[] memory) {
        return registeredTokens;
    }

    function baseURI() public view returns (string memory) {
        return super.uri(0);
    }

    function isRegistered(
        uint256 id
    ) external view returns (bool) {
        return _isRegistered(id);
    }

    function collectionOf(
        address wallet
    ) external view returns (uint256[] memory, uint256[] memory) {
        uint256[] memory tokens = registeredTokens;
        uint256[] memory balances = new uint256[](tokens.length);

        for (uint256 i = 0; i < tokens.length; ++i) {
            balances[i] = balanceOf(wallet, tokens[i]);
        }

        return (tokens, balances);
    }

    function _isRegistered(uint256 id) internal view returns (bool) {
        return bytes(_tokenURIs[id]).length != 0;
    }

    /**
     * @dev Returns an URI for a given token ID. Returns empty string for non existent ids
     * @param tokenId uint256 ID of the token to query
     */
    function _tokenURI(uint256 tokenId) internal view returns (string memory) {
        return _tokenURIs[tokenId];
    }

    /**************************************
     OVERWRITTEN PARENT FUNCTIONS
    ***************************************/

    function uri(uint256 _id) public view override returns (string memory) {
        return _tokenURI(_id);
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    )
        internal
        override(ERC1155Upgradeable, ERC1155SupplyUpgradeable)
        whenNotPaused
    {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    // The following functions are overrides required by Solidity.

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155Upgradeable, AccessControlUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

