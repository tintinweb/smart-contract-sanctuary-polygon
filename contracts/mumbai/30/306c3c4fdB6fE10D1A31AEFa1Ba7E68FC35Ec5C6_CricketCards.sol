pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

contract CricketCards is Ownable {
    mapping(uint256 => uint256[]) public allCricketCards;
    mapping(uint256 => uint256) public cardWinsAgainstBetterPlayer;
    mapping(uint256 => uint256) public cardLossesaAgainstWorsePlayers;
    mapping(uint256 => uint256) public cardMatches;

    uint256[] preferredCards;
    uint256 minimumNoOfMatches = 100;
    uint256 winPercentage = 65;
    uint256 lossPercentage = 35;
    uint256 totalNoOfCards = 1000;
    uint256 noOfPreferredCards = 80;
    uint256 noOfStats = 9;
    uint256 lastCardId = 0;

    uint256 levelIndex = 9;

    modifier statIdExists(uint256 _statId) {
        require(_statId > 0 && _statId <= noOfStats, "invalid statId");
        _;
    }

    modifier cardIdExists(uint256 _cardId) {
        require(_cardId > 0 && _cardId <= totalNoOfCards, "invalid cardId");
        _;
    }

    function addCards(uint256[][] calldata _allStats) external onlyOwner {
        // require(_allStats.length == totalNoOfCards, "Invalid input length");
        uint256 cardId;
        uint256[] memory newCard;
        for (uint256 i = 0; i < _allStats.length; i++) {
            cardId = i + 1;
            newCard = _allStats[i];
            allCricketCards[cardId] = newCard;
        }
    }

    function addCardsInChunks(uint256[][] calldata _stats) external onlyOwner {
        uint256 cardId;
        uint256[] memory newCard;
        for (uint256 i = 0; i < _stats.length; i++) {
            cardId = lastCardId + i + 1;
            newCard = _stats[i];
            allCricketCards[cardId] = newCard;
        }
        lastCardId = cardId;
    }

    function setPreferredCards(
        uint256[] memory _prefferedCards
    ) external onlyOwner {
        preferredCards = _prefferedCards;
    }

    function getPreferredPlayer() public view returns (uint256) {
        uint256 playerIndex = ((block.timestamp) % noOfPreferredCards);
        return preferredCards[playerIndex];
    }

    function getUniquePreferredPlayer(
        uint256[] memory cards
    ) external view returns (uint256) {
        uint256 preferredCard = getPreferredPlayer();
        uint256 i = 0;
        while (i < cards.length) {
            if (preferredCard == cards[i]) {
                preferredCard = getPreferredPlayer();
                i = 0;
            } else {
                i++;
            }
        }
        return preferredCard;
    }

    function getUniquePlayer(
        uint256[] memory cards
    ) external view returns (uint256) {
        uint256 requiredCard = ((block.timestamp + block.number) %
            totalNoOfCards) + 1;
        uint256 i = 0;
        while (i < cards.length) {
            if (requiredCard == cards[i]) {
                requiredCard =
                    ((block.timestamp + block.number + i) % totalNoOfCards) +
                    1;
                i = 0; // start the loop over again
            } else {
                i++;
            }
        }
        return requiredCard;
    }

    function getCardStat(
        uint256 _cardId,
        uint256 _statId
    )
        public
        view
        statIdExists(_statId)
        cardIdExists(_cardId)
        returns (uint256)
    {
        // console.log("allCricketCards[_cardId]");
        return allCricketCards[_cardId][_statId];
    }

    function setCardMatches(
        uint256 _cardId,
        uint256 _noOfMatches
    ) external  cardIdExists(_cardId) onlyOwner {
        cardMatches[_cardId] += _noOfMatches;
    }

    function setCardWinsAgainstBetterPlayer(
        uint256 _cardId,
        uint256 _wins
    ) external  cardIdExists(_cardId) onlyOwner {
        cardWinsAgainstBetterPlayer[_cardId] += _wins;
    }

    function setCardLossesAgainstWorsePlayer(
        uint256 _cardId,
        uint256 _losses
    ) external  cardIdExists(_cardId) onlyOwner {
        cardLossesaAgainstWorsePlayers[_cardId] += _losses;
    }

    function incremetCardMatches(
        uint256 _cardId
    ) external  cardIdExists(_cardId) {
        cardMatches[_cardId] += 1;
    }

    function incrementCardWinsAgainstBetterPlayer(
        uint256 _cardId
    ) external  cardIdExists(_cardId) {
        cardWinsAgainstBetterPlayer[_cardId] += 1;
    }

    function incrementCardLossesAgainstWorsePlayer(
        uint256 _cardId
    ) external  cardIdExists(_cardId) {
        cardLossesaAgainstWorsePlayers[_cardId] += 1;
    }

    function updateCardLevel(uint256 _cardId) external cardIdExists(_cardId) {
        if (cardMatches[_cardId] >= minimumNoOfMatches) {
            if (cardWinsAgainstBetterPlayer[_cardId] >= winPercentage) {
                allCricketCards[_cardId][levelIndex]++;
                cardMatches[_cardId] = 0;
                cardWinsAgainstBetterPlayer[_cardId] = 0;
            } else if (
                cardLossesaAgainstWorsePlayers[_cardId] >= lossPercentage &&
                allCricketCards[_cardId][levelIndex] >= 1
            ) {
                allCricketCards[_cardId][levelIndex]--;
                cardMatches[_cardId] = 0;
                cardWinsAgainstBetterPlayer[_cardId] = 0;
            }
        }
    }

    function setMinimumNoOfMatches(uint256 _value) external onlyOwner {
        minimumNoOfMatches = _value;
    }

    function setWinPercentage(uint256 _value) external onlyOwner {
        winPercentage = _value;
    }

    function setLossPercentage(uint256 _value) external onlyOwner {
        lossPercentage = _value;
    }

    function setLevelIndex(uint256 _value) external onlyOwner {
        levelIndex = _value;
    }

    function setNoOfPreferredCards(uint256 _value) external onlyOwner {
        noOfPreferredCards = _value;
    }

    function setTotalNoOfCards(uint256 _value) external onlyOwner {
        totalNoOfCards = _value;
    }

    function setNoOfStats(uint256 _value) external onlyOwner {
        noOfStats = _value;
    }
}

