        otherPhasesPrice = _price;
    }

    function stopPhase1() public onlyOwner {
        isOnlyPhase1 = false;
    }

    function stopCheckingWhitelisting() public onlyOwner {
        checkWhitelisting = false;
        maxBatchSize = 4;
    }

    /**
     * @dev Public function to set NFT count per transaction limit ,only Owner
     * @param _limit uint256
     */
    function setNftcount(uint8 _limit) external onlyOwner {
        maxBatchSize = _limit;
    }

