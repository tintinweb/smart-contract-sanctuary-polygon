pragma solidity ^0.8.4;

import "@openzeppelin/contracts/governance/extensions/GovernorSettings.sol";
import "@openzeppelin/contracts/governance/extensions/GovernorCountingSimple.sol";
import "@openzeppelin/contracts/governance/extensions/GovernorVotesQuorumFraction.sol";

contract WatchSaleGovernance is GovernorCountingSimple, GovernorSettings, GovernorVotesQuorumFraction {
    address owner;
    mapping(address => uint256) public whitelist_proposer;
    address[] public whitelistedProposers;
    PriceEntry[] public priceList;

    struct WhitelistProposerEntry {
        address account;
        uint256 timestamp;
    }

    struct PriceEntry {
        uint256 timestamp;
        uint256 usdcValue;
    }

    /*
     * @notice Modifier to restrict access to only the owner
     */
    modifier onlyOwner() {
        require(msg.sender == owner, "Caller is not the owner");
        _;
    }

    /*
     * @notice Modifier to restrict access to only whitelisted accounts
     */
    modifier onlyWhitelistedProposer() {
        require(whitelist_proposer[msg.sender] >= block.timestamp, "Address Not whitelisted or Whitelist Expired");
        _;
    }

    /*
     * @notice Constructor
     */
    constructor(IVotes _token, uint256 quorumNumerator)
    Governor("WatchSaleGovernance")
    GovernorSettings(0 /* 1 block */, 50400 /* 1 week */, 0)
    GovernorVotes(_token)
    GovernorVotesQuorumFraction(quorumNumerator)
    {
        owner = msg.sender;
    }

    /**
     * @notice Adds price entries to the price list.
     * @param entries The price entries to be added.
     * @dev The `entries` array should be ordered by timestamp in ascending order.
     * @dev Only the contract owner can call this function.
     */
    function addPriceListEntries(PriceEntry[] memory entries) public onlyOwner {
        for (uint256 i = 0; i < entries.length; i++) {
            priceList.push(entries[i]);
        }
    }

    /**
     * @notice Retrieves the price list.
     * @return An array of PriceEntry representing the price list.
     * @dev The price list contains entries ordered by timestamp in ascending order.
     * @dev This function is public.
     */
    function getPriceList() public view returns (PriceEntry[] memory) {
        return priceList;
    }

    /**
     * @notice Removes all price entries from the price list.
     * @dev Only the contract owner can call this function.
     */
    function removeAllPriceListEntries() public onlyOwner {
        delete priceList;
    }

    /*
     * @notice Function to add whitelist proposer entries in batch
     * @param entries Array of WhitelistEntry structs
     */
    function addWhitelistProposerBatch(WhitelistProposerEntry[] memory entries) public onlyOwner {
        for (uint256 i = 0; i < entries.length; i++) {
            address account = entries[i].account;
            uint256 timestamp = entries[i].timestamp;
            addWhitelistProposer(account, timestamp);
        }
    }

    /*
     * @notice Function to add a single whitelist proposer entry
     * @param account The address to be added to the whitelist
     * @param timestamp The timestamp until which the address is whitelisted
     */
    function addWhitelistProposer(address account, uint256 timestamp) public onlyOwner {
        whitelist_proposer[account] = timestamp;
        whitelistedProposers.push(account);
    }

    /*
     * @notice Function to get the whitelist proposer addresses
     * @return An array of whitelisted proposer addresses
     */
    function getWhitelistedProposer() public view returns (address[] memory) {
        return whitelistedProposers;
    }

    /**
     * @notice Purges expired whitelist members by removing their entries from the whitelist.
     * @dev Only the contract owner can call this function.
     */
    function purgeExpiredWhitelistMembers() public onlyOwner {
        uint256 currentTimestamp = block.timestamp;

        // Iterate over the whitelist addresses and remove expired entries
        for (uint256 i = 0; i < whitelistedProposers.length; i++) {
            address account = whitelistedProposers[i];
            if (whitelist_proposer[account] < currentTimestamp) {
                delete whitelist_proposer[account];
                whitelistedProposers[i] = whitelistedProposers[whitelistedProposers.length - 1];
                whitelistedProposers.pop();
                i--;
            }
        }
    }

    /**
     * @notice Purges all whitelist proposer members by removing their entries from the whitelist.
     * @dev Only the contract owner can call this function.
     */
    function purgeAllWhitelistedProposers() public onlyOwner {
        for (uint256 i = 0; i < whitelistedProposers.length; i++) {
            address account = whitelistedProposers[i];
            delete whitelist_proposer[account];
        }
        delete whitelistedProposers;
    }

    /**
     * @notice Creates a proposal based on the provided parameters and the current USDC value.
     * @param offerUSDC The offering value in USDC.
     * @param targets The target addresses for the proposal.
     * @param values The values to be passed to the proposal targets.
     * @param calldatas The calldata to be passed to the proposal targets.
     * @param signatures The function signatures of the proposal targets.
     * @return The proposal ID.
     * @dev Only whitelisted addresses can call this function.
     */
    function _propose(
        uint256 offerUSDC,
        address[] memory targets,
        uint256[] memory values,
        bytes[] memory calldatas,
        string memory signatures
    ) public onlyWhitelistedProposer returns (uint256) {
        // Find the corresponding USDC value at the current timestamp
        require(priceList.length > 0, "Price List is Empty");

        uint256 usdcValueAtCurrentTime = 0;

        // Check if currentTimestamp is not lower than the first timestamp in the priceList
        require(block.timestamp >= priceList[0].timestamp, "Offer refused: currentTimeStamp lower than first timestamp in priceList");

        for (uint256 i = 0; i < priceList.length; i++) {
            if (priceList[i].timestamp >= block.timestamp) {
                usdcValueAtCurrentTime = priceList[i].usdcValue;
            } else {
                break;
            }
        }
        // Check if the offering value is not lower than the USDC value at the current time
        require(offerUSDC >= usdcValueAtCurrentTime, "Offering value is lower than current USDC value");

        return propose(targets, values, calldatas, signatures);
    }

    /*
     * @notice Internal function to check if the quorum has been reached for a proposal
     * @dev Overrides the `_quorumReached` function in the `GovernorCountingSimple` contract
     * @param proposalId The ID of the proposal
     * @return A boolean indicating whether the quorum has been reached or not
     */
    function _quorumReached(uint256 proposalId) internal view virtual override(Governor, GovernorCountingSimple) returns (bool) {
        (uint256 againstVotes, uint256 forVotes, uint256 abstainVotes) = proposalVotes(proposalId);

        return quorum(proposalSnapshot(proposalId)) <= forVotes + againstVotes + abstainVotes;
    }

    /*
     * @notice Internal function to check if the vote has succeeded for a proposal
     * @dev Overrides the `_voteSucceeded` function in the `GovernorCountingSimple` contract
     * @param proposalId The ID of the proposal
     * @return A boolean indicating whether the vote has succeeded or not
     */
    function _voteSucceeded(uint256 proposalId) internal view virtual override(Governor, GovernorCountingSimple) returns (bool) {
        (uint256 againstVotes, uint256 forVotes, uint256 abstainVotes) = proposalVotes(proposalId);

        uint256 nonVotes = token.getPastTotalSupply(proposalSnapshot(proposalId)) - againstVotes - forVotes - abstainVotes;
        return forVotes + nonVotes > againstVotes;
    }

    /**
     * @notice Method for changing the quorum numerator (N). Quorum is now reached if FOR_VOTES + AGAINST_VOTES + ABSTAIN_VOTES > N/100
     * @param newQuorumNumerator The new quorum numerator
     */
    function updateQuorumNumerator(uint256 newQuorumNumerator) public virtual override onlyOwner {
        _updateQuorumNumerator(newQuorumNumerator);
    }

    /**
     * @notice Method for cancelling a (not executed) votation.
     */
    function cancel(
        address[] memory targets,
        uint256[] memory values,
        bytes[] memory calldatas,
        bytes32 descriptionHash
    ) public virtual onlyOwner returns (uint256) {
        return super._cancel(targets, values, calldatas, descriptionHash);
    }

    // The following functions are overrides required by Solidity.

    /**
     * @notice Method for retrieving the voting delay.
     */
    function votingDelay()
    public
    view
    override(IGovernor, GovernorSettings)
    returns (uint256)
    {
        return super.votingDelay();
    }

    /**
     * @notice Method for retrieving the voting period.
     */
    function votingPeriod()
    public
    view
    override(IGovernor, GovernorSettings)
    returns (uint256)
    {
        return super.votingPeriod();
    }

    /**
     * @notice Method for retrieving the proposal threshold
     * .
     */
    function proposalThreshold()
    public
    view
    override(Governor, GovernorSettings)
    returns (uint256)
    {
        return super.proposalThreshold();
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import "hardhat/console.sol";

error PriceNotMet(address nftAddress, uint256 tokenId, uint256 price);
error ItemNotForSale(address nftAddress, uint256 tokenId);
error NotListed(address nftAddress, uint256 tokenId);
error AlreadyListed(address nftAddress, uint256 tokenId);
error NoProceeds();
error NotOwner();
error NotApprovedForMarketplace();
error PriceMustBeAboveZero();
error NotWhiteListed();

// Error thrown for isNotOwner modifier
// error IsNotOwner()

contract Marketplace is ReentrancyGuard, Ownable {
    using SafeERC20 for IERC20;

    struct Listing {
        uint256 price;
        address seller;
    }
    struct LastSale{
        uint256 price;
        uint256 date;
    }

    event ItemListed(
        address indexed seller,
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 price
    );

    event ItemCanceled(
        address indexed seller,
        address indexed nftAddress,
        uint256 indexed tokenId
    );

    event ItemBought(
        address indexed buyer,
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 price
    );

    event PaymentReleased(
        address to, 
        uint256 amount
    );

    // address of the ERC20 token that we will use for payments
    address internal paymentToken;

    uint256 private TRADING_FEE;
    uint256 private PERFORMANCE_FEE;
    uint256 private MANAGEMENT_FEE;

    mapping(address => mapping(uint256 => Listing)) private s_listings;
    mapping(address => uint256) private s_proceeds;

    // map NFT contract address -> tokenId -> lastPrice
    mapping(address => mapping(uint256 => LastSale)) private lastTokensSale;
    mapping(address => bool) whitelistedAddresses;

    constructor(address _paymentToken, uint256 tradingFee, uint256 performanceFee, uint256 managementFee) {
        paymentToken = _paymentToken;
        TRADING_FEE = tradingFee;
        PERFORMANCE_FEE = performanceFee;
        MANAGEMENT_FEE = managementFee;
    }

    // It's checking if the item is already listed.
    modifier notListed(
        address nftAddress,
        uint256 tokenId
    ) {
        Listing memory listing = s_listings[nftAddress][tokenId];
        if (listing.price > 0) {
            revert AlreadyListed(nftAddress, tokenId);
        }
        _;
    }

    modifier isWhitelisted(address _address) {
        if(!whitelistedAddresses[_address]){
            revert NotWhiteListed();
        }
        _;
    }

    modifier isListed(address nftAddress, uint256 tokenId) {
        Listing memory listing = s_listings[nftAddress][tokenId];
        if (listing.price <= 0) {
            revert NotListed(nftAddress, tokenId);
        }
        _;
    }

    modifier isOwner(
        address nftAddress,
        uint256 tokenId,
        address spender
    ) {
        IERC721 nft = IERC721(nftAddress);
        address owner = nft.ownerOf(tokenId);
        if (spender != owner) {
            revert NotOwner();
        }
        _;
    }

    // IsNotOwner Modifier - Nft Owner can't buy his/her NFT
    // Modifies buyItem function
    // Owner should only list, cancel listing or update listing
    /* modifier isNotOwner(
        address nftAddress,
        uint256 tokenId,
        address spender
    ) {
        IERC721 nft = IERC721(nftAddress);
        address owner = nft.ownerOf(tokenId);
        if (spender == owner) {
            revert IsNotOwner();
        }
        _;
    } */

    /////////////////////
    // Main Functions //
    /////////////////////
    /**
     * @notice Method for listing NFT
     * @param nftAddress Address of NFT contract
     * @param tokenId Token ID of NFT
     * @param price sale price for each item
     */
    function listItem(
        address nftAddress,
        uint256 tokenId,
        uint256 price
    )
    external
    notListed(nftAddress, tokenId)
    isOwner(nftAddress, tokenId, msg.sender)
    isWhitelisted(msg.sender)
    {
        if (price <= 0) {
            revert PriceMustBeAboveZero();
        }
        IERC721 nft = IERC721(nftAddress);
        if (nft.getApproved(tokenId) != address(this)) {
            revert NotApprovedForMarketplace();
        }
        s_listings[nftAddress][tokenId] = Listing(price, msg.sender);
        emit ItemListed(msg.sender, nftAddress, tokenId, price);
    }

    // /**
    //  * @notice Increase the amount of allowance the spender is allowed to transfer from the function caller (msg.sender) balance
    //  * @param tokenAmount Incremented amount
    //  */
    // function increasePaymentTokenAllowance(uint256 tokenAmount)
    // public
    // {
    //     IERC20(paymentToken).safeIncreaseAllowance(address(this), tokenAmount);
    // }

    /**
     * @notice Method for retrieving user's payment token allowance
     */
    function getPaymentTokenAllowance()
    public view
    returns(uint256)
    {
        return IERC20(paymentToken).allowance(msg.sender, address(this));
    }

    /**
     * @notice Method for cancelling listing
     * @param nftAddress Address of NFT contract
     * @param tokenId Token ID of NFT
     */
    function cancelListing(address nftAddress, uint256 tokenId)
    external
    isOwner(nftAddress, tokenId, msg.sender)
    isListed(nftAddress, tokenId)
    {
        delete (s_listings[nftAddress][tokenId]);
        emit ItemCanceled(msg.sender, nftAddress, tokenId);
    }

    /**
     * @notice Method for buying listing
     * @notice The owner of an NFT could unapprove the marketplace,
     * which would cause this function to fail
     * Ideally you'd also have a `createOffer` functionality.
     * @param nftAddress Address of NFT contract
     * @param tokenId Token ID of NFT
     * @param paymentTokenAmount Amount of payment token the buyer wants to pay
     */
    function buyItem(address nftAddress, uint256 tokenId, uint256 paymentTokenAmount)
    external
    // payable
    isListed(nftAddress, tokenId)
    nonReentrant
    {
        Listing memory listedItem = s_listings[nftAddress][tokenId];
        if (paymentTokenAmount < listedItem.price) {
            revert PriceNotMet(nftAddress, tokenId, listedItem.price);
        }
        require(paymentTokenAmount >= getPaymentTokenAllowance(), "Please approve tokens before transferring");

        IERC20(paymentToken).safeTransferFrom(msg.sender, address(this), paymentTokenAmount);

        (uint256 tradingFee,uint256 performanceFee,uint256 managementFee) = getTransactionFee(nftAddress, tokenId, paymentTokenAmount);
        uint256 totalFee = tradingFee + performanceFee + managementFee;
        s_proceeds[listedItem.seller] += paymentTokenAmount - totalFee;
        s_proceeds[owner()] += totalFee;
        delete (s_listings[nftAddress][tokenId]);

        IERC721(nftAddress).safeTransferFrom(listedItem.seller, msg.sender, tokenId);
        lastTokensSale[nftAddress][tokenId] = LastSale(listedItem.price,block.timestamp);
        emit ItemBought(msg.sender, nftAddress, tokenId, listedItem.price);
    }

    /**
     * @notice Method for withdrawing proceeds from sales
     */
    function withdrawProceeds() external {
        uint256 proceeds = s_proceeds[msg.sender];
        if (proceeds <= 0) {
            revert NoProceeds();
        }

        s_proceeds[msg.sender] = 0;
        IERC20(paymentToken).safeTransfer(msg.sender, proceeds);
        emit PaymentReleased(msg.sender, proceeds);
    }

    /*
     * @notice Method for updating listing
     * @param nftAddress Address of NFT contract
     * @param tokenId Token ID of NFT
     * @param newPrice Price in Wei of the item
     */
    function updateListing(
        address nftAddress,
        uint256 tokenId,
        uint256 newPrice
    )
    external
    isListed(nftAddress, tokenId)
    nonReentrant
    isOwner(nftAddress, tokenId, msg.sender)
    {
        //We should check the value of `newPrice` and revert if it's below zero (like we also check in `listItem()`)
        if (newPrice <= 0) {
            revert PriceMustBeAboveZero();
        }
        s_listings[nftAddress][tokenId].price = newPrice;
        emit ItemListed(msg.sender, nftAddress, tokenId, newPrice);
    }


    /////////////////////
    // Getter Functions //
    /////////////////////

    /**
     * @notice Method for retrieving current listing price and seller of a specific token
     * @param nftAddress Address of NFT contract
     * @param tokenId Token ID of NFT
     */
    function getListing(address nftAddress, uint256 tokenId)
    external
    view
    returns (Listing memory)
    {
        return s_listings[nftAddress][tokenId];
    }

    /**
     * @notice Method for retrieving the trading fee, the performance fee, and the management fee in this respective order. 
     * The transaction fee represents the portion that is subtracted from the sale price of the individual token and held on 
     * the contract for the contract owner.
     * @notice trading_fee = price * tradingFee / 1000;
     * @notice performance_fee = (price - tokens[tokenID].lastSalePrice )  * performanceFee / 1000;
     * @notice management_fee = num_days * avg_price * managementFee / 1000;
     * @notice where:
     * @notice num_days  = (block.timestamp -  tokens[tokenID].lastSaleDate) / 60 / 60 / 24;
     * @notice avg_price = (price + tokens[tokenID].lastSalePrice ) / 2;
     * @param nftAddress Address of NFT contract
     * @param tokenId Token ID of NFT
     * @param price Possible sale price
     */
    function getTransactionFee(address nftAddress,uint256 tokenId, uint256 price) public view 
        returns(
            uint256 tradingFee,
            uint256 performanceFee,
            uint256 managementFee) {
        uint lastSalePrice = lastTokensSale[nftAddress][tokenId].price;
        uint lastSaleDate = lastTokensSale[nftAddress][tokenId].date;

        tradingFee = price * TRADING_FEE / 1000;

        performanceFee = 0;
        if(price >= lastSalePrice){
            performanceFee = (price - lastSalePrice) * PERFORMANCE_FEE / 1000;
        }

        managementFee = 0;
        if(lastSaleDate != 0){
            uint avgPrice = (price + lastSalePrice) / 2;
            managementFee = ( block.timestamp - lastSaleDate ) * avgPrice * MANAGEMENT_FEE / 60 / 60 / 24 / 1000;
        }

        return (tradingFee, performanceFee, managementFee);
    }



    /////////////////////
    // Setter Functions //
    /////////////////////

    /**
     * @notice Method for setting performance fee parameter
     * @notice performance_fee = (price - tokens[tokenID].lastSalePrice )  * performanceFee / 1000;
     * @param newPerformanceFee the new performance fee
     */
    function setPerformanceFee(uint256 newPerformanceFee)public onlyOwner {
        PERFORMANCE_FEE = newPerformanceFee;
    }

    /**
   * @notice Method for get trading fee parameter
     */
    function getPerformanceFee()public view returns(uint256){
        return PERFORMANCE_FEE;
    }

    /**
     * @notice Method for setting trading fee parameter
     * @notice trading_fee = price * tradingFee / 1000;
     * @param newTradingFee the new trading fee
     */
    function setTradingFee(uint256 newTradingFee)public onlyOwner {
        TRADING_FEE = newTradingFee;
    }

    /**
     * @notice Method for get trading fee parameter
     */
    function getTradingFee()public view returns(uint256){
        return TRADING_FEE;
    }

    /**
     * @notice Method for setting management fee parameter
     * @notice management_fee = num_days * avg_price * managementFee / 1000;
     * @param newManagementFee the new management fee
     */
    function setManagementFee(uint256 newManagementFee)public onlyOwner{
        MANAGEMENT_FEE = newManagementFee;
    }
    /**
     * @notice Method for get management fee parameter
     */
    function getManagementFee()public view returns(uint256){
        return MANAGEMENT_FEE;
    }

    /////////////////////
    // White Listing //
    /////////////////////

    /**
     * @notice Method for adding a user to those eligible for listing items
     * @param _addressToWhitelist address of the user
     */
    function addUser(address _addressToWhitelist) public onlyOwner {
        whitelistedAddresses[_addressToWhitelist] = true;
    }

    /**
     * @notice Method for verifying if a user is eligible for listing items
     * @param _whitelistedAddress address of the user
     */
    function verifyUser(address _whitelistedAddress) public view returns(bool) {
        bool userIsWhitelisted = whitelistedAddresses[_whitelistedAddress];
        return userIsWhitelisted;
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/draft-ERC721Votes.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

contract WatchToken is ERC721, ERC721Enumerable, ERC721URIStorage, EIP712, ERC721Votes, AccessControl {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIdCounter;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    bytes32 public constant GOVERNANCE_ROLE = keccak256("GOVERNANCE_ROLE");
    uint256 public _maxSupply = 0;
    mapping(address => bool) public whitelist_owner;
    address[] public whitelistedOwners;

    /**
       @notice : This modifier check if the address that execute the transaction  is an Elephants admin or a whitelisted owner
    **/
    modifier onlyAdminOrWhitelist() {
        require(hasRole(ADMIN_ROLE, _msgSender()) || whitelist_owner[msg.sender] == true, "Caller is neither the admin nor whitelisted");
        _;
    }

    /**
       @notice : This modifier check if the address that execute the transaction  is an Elephants admin or a Smart Contract Governor
    **/
    modifier onlyAdminOrGovernance() {
        require(hasRole(ADMIN_ROLE, _msgSender()) || hasRole(GOVERNANCE_ROLE, _msgSender()), "Caller is neither the admin nor the governance");
        _;
    }

    /**
        @notice : This modifier check if the address that execute the transaction is an Elephants admin
    **/
    modifier onlyAdmin() {
        require(hasRole(ADMIN_ROLE, _msgSender()), "Caller is not the admin");
        _;
    }

    /**
     * @notice construct a new ERC721Votes smart contract
     * @param name  name of the Smart Contract
     * @param symbol  symbol of the new Smart Contract
     * @param admins  address of the Elephants admins
    **/
    constructor(string memory name, string memory symbol, address[] memory admins, uint256 maxSupply)
        ERC721(name, symbol) 
        EIP712(name, "1") {
        for (uint256 i = 0; i < admins.length; ++i) {
            _grantRole(DEFAULT_ADMIN_ROLE, admins[i]);
            _grantRole(ADMIN_ROLE, admins[i]);
        }
        _maxSupply = maxSupply;
    }

    /// @notice mint a batch of WatchToken, only Elephants admin can mint a new token
    /// @param to  First owner of this new token
    /// @param uri  URI to the token metadata
    function safeBatchMint(address to, string memory uri, uint256 amount) public onlyAdmin {
        for (uint256 i = 0; i < amount; i++) {
            safeMint(to, uri);
        }
    }


    /// @notice mint a new Token that represent a part of a watch, only Elephants admin can mint a new token
    /// @param to  First owner of this new token
    /// @param uri  URI to the token metadata
    function safeMint(address to, string memory uri) public onlyAdmin {
        require(totalSupply() < _maxSupply, "Cannot mint anymore, max supply reached");
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _safeMint(to, tokenId);
        _setTokenURI(tokenId, uri);
    }

    // The following functions are overrides required by Solidity.
    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        override(ERC721, ERC721Enumerable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _afterTokenTransfer(address from, address to, uint256 tokenId)
        internal
        override(ERC721, ERC721Votes)
    {
        super._afterTokenTransfer(from, to, tokenId);
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    /// @notice return the URI where the token metadata are stored
    /// @param tokenId  the id of the token
    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721Enumerable, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    /*
     * @notice Function to add whitelist entries in batch
     * @param entries Array of WhitelistEntry structs
     */
    function addWhitelistOwnerBatch(address[] memory addresses) public onlyAdmin {
        for (uint256 i = 0; i < addresses.length; i++) {
            address account = addresses[i];
            addWhitelistOwner(account);
        }
    }

    /*
     * @notice Function to add a single whitelist owner entry
     * @param account The address to be added to the whitelist
     */
    function addWhitelistOwner(address account) public onlyAdmin {
        whitelist_owner[account] = true;
        whitelistedOwners.push(account);
    }

    /*
     * @notice Function to remove a single whitelist owner entry
     * @param account The address to be removed from the whitelist
     */
    function removeWhitelistedOwner(address account) public onlyAdmin {
        for (uint256 i = 0; i < whitelistedOwners.length; i++) {
            if (whitelistedOwners[i] == account) {
                delete whitelistedOwners[i];
                delete whitelist_owner[account];
                break;
            }
        }
    }

    /*
     * @notice Function to get the whitelist owner addresses
     * @return An array of whitelisted addresses
     */
    function getWhitelistedOwners() public view returns (address[] memory) {
        return whitelistedOwners;
    }

    /**
     * @notice Purges all whitelist owner members by removing their entries from the whitelist.
     * @dev Only the contract owner can call this function.
     */
    function purgeAllWhitelistedOwners() public onlyAdmin {
        for (uint256 i = 0; i < whitelistedOwners.length; i++) {
            address account = whitelistedOwners[i];
            delete whitelist_owner[account];
        }
        delete whitelistedOwners;
    }

    /// @notice update the URI where the token metadata are stored, only Elephants admin can update the URI
    /// @param tokenId  the id of the token
    /// @param uri  the new URI
    function updateTokenURI(uint256 tokenId,string memory uri)public onlyAdmin{
        _setTokenURI(tokenId, uri);
    }

    /// @notice allow new Governor Smart contract to manage tokens, only Elephants admin can add a new Governor
    /// @param governor  the address of the smart contract governor
    function addGovernor(address governor) public onlyAdmin{
            _grantRole(GOVERNANCE_ROLE, governor);
    }

    /// @notice revoke permissions to to manage tokens to a Governor Smart contract , only Elephants admin can revoke permissions
    /// @param governor  the address of the smart contract governor
    function removeGovernor(address governor) public onlyAdmin{
            _revokeRole(GOVERNANCE_ROLE, governor);
    }

    /// @notice transfer a token from an address to another, only Elephants admin or whitelist member can transfer a token
    /// @param _from  the address of the token owner
    /// @param _to  the address of the new token owner
    /// @param _tokenId  the id of the token
    function transferFrom(
        address _from,
        address _to,
        uint256 _tokenId
    ) public override(ERC721, IERC721) onlyAdminOrWhitelist {
        _transfer(_from, _to, _tokenId);
    }

    /// @notice transfer a token from an address to another, only Elephants admin or whitelist member can transfer a token
    /// @param _from  the address of the token owner
    /// @param _to  the address of the new token owner
    /// @param _tokenId  the id of the token
    function safeTransferFrom(
        address _from,
        address _to,
        uint256 _tokenId
    ) public override(ERC721, IERC721) onlyAdminOrWhitelist {
        safeTransferFrom(_from, _to, _tokenId, "");
    }

    /// @notice transfer all token to a specific address, only Elephants admin or a Smart Contract Governor can transfer a token
    /// @param to  the address of the new tokens owner
    function transferAll(
        address to
    ) public virtual onlyAdminOrGovernance {
        for (uint256 i = 0; i < totalSupply(); i++) {
            uint256 tokenId = tokenByIndex(i);
            address owner = ERC721.ownerOf(tokenId);
            _transfer(owner, to, tokenId);
        }
    }
}

