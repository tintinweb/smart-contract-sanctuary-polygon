      /**
     * @dev See {IERC721-isApprovedForAll}.
     *      overide to allow fuse contract to burn NFTs on behalf of users
     *      after consent of user
     */
    function isApprovedForAll(address owner, address operator)
        public
        view
        virtual
        override(ERC721A, IERC721A)
        returns (bool)
    {
        return (operator == address(fuseInstance) || isApprovedForAll(owner , operator));
    }
    /**
     * @dev registerFusionContract funciton will register Fusion contract `_fuseInstance`
     *  to burn Redeem NFT
     *
     * Requirements:
     *
     * - The caller must be current  owner of contract or Minter.
     * - The Fusion contract must be register one time.
     * - The Address should not be EOA
     * - The Contract must implement IFuseable Interface
     *
     */

    function registerFusionContract(IFuseable fuseInstance_)
        external
        onlyOwner
        isFusion(address(fuseInstance_))
    {
        if (address(fuseInstance) != address(0x0))
            revert FusionAlreadyRegistered();
        fuseInstance = IFuseable(fuseInstance_);
    }


    /**
     * @dev getFuseInstance should Returns address of IFuseable.
     *
     */
    function getFuseInstance() external view returns (IFuseable) {
        return fuseInstance;
    }

