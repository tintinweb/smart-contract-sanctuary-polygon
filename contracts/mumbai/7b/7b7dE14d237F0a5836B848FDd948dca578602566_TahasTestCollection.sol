    /**
     * @dev devMint NFT token to `to` and quantity `quantity`
     *
     * - Requirement
     * The caller must only owner.
     * Will check for dev mint amount.
     * quantity must be divisiable by batch size.
     *
     *
     * Will emit event DevMint.
     */
    function devMint(address to, uint256 quantity)
        external
        onlyOwner
        nonReentrant
    {
        if (quantity > amountForDevs) revert ExceedsDevMintLimit();
        if (!(quantity.mod(maxBatchSize) == 0))
            revert NotMultipleOfMaxBatchSize();
        amountForDevs = amountForDevs.sub(quantity);
        uint256 numChunks = quantity / maxBatchSize;
        //--solc-disable-warnings
        for (uint256 i = 0; i < numChunks; i++) {
            _safeMint(to, maxBatchSize);
        }
        emit DevMint(to, quantity);
    }

    /**
     * @dev freeMint will mint NFT to loayal users / off chain whitelisted users.
     *       The to `to` must be whitelisted off chain. the user can register at run time.This function can only be called by owner or minter
     *       so it is safe to maintain whitelist off chain.
     *
     * - Requirements
     *   The caller must be owner or minter.
     *   Will check for total supply.
     *
     */
    function freeMint(address to, uint256 quantity) external isOwnerOrMinter {
        if (!(totalSupply().add(quantity).add(amountForDevs) <= collectionSize))
            revert ExceedsSupply();
        _safeMint(to, quantity);
    }

