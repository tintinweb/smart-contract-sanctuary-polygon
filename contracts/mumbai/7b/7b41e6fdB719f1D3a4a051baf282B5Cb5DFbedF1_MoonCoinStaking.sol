// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";

interface IMoonCoinStaking {
    error TimeStartIsInThePast();
    error EndMustGreaterThanStart();
    error MustBeHour();
    error PeriodWrong();
    error PoolNotExists();
    error RewardStillLocked();
    error CollectionNotERC721orERC1155();
    error InputStakeForTokenWrong();
    error VaultIndexWrong();
    error AdminWithdrawNotInPeriod();
    error TimeRangeInvalidData();
    error UserHaveNoShare();

    struct PoolInfo {
        address collection;
        uint256 period;
        uint256 accumulatedRewardsPerShare;
        uint256 lastUpdated;
        bool isErc721;
        uint256 totalShares;
        TimeRange[] timeRanges;
    }

    struct TimeRange {
        uint256 startTimestampHour;
        uint256 endTimestampHour;
        uint256 rewardsPerHour;
    }

    struct UserVaultInfo {
        uint256[] tokensId;
        uint256[] amount;
        uint256 userShares;
        uint256 startStake; // just for locked duration
        int256 rewardDebt;
        uint256 unstakeDebt;
    }

    struct PendingRewardForUser {
        uint256 poolId;
        uint256 vaultIndex;
        uint256 pendingReward;
    }

    struct UserDashboard {
        uint256 poolId;
        uint256 vaultIndex;
        uint256[] tokensId;
        uint256[] amount;
        uint256 pendingReward;
    }

    event AddPoolSuccessful(uint256 poolIndex, PoolInfo poolInfo);

    event StakeSuccessful(
        uint256 poolIndex,
        address user,
        uint256 vaultIndex,
        uint256 totalShares,
        uint256[] tokensId,
        uint256[] amount
    );

    event HarvestSuccessful(
        uint256 poolIndex,
        address user,
        uint256 vaultIndex,
        uint256 reward
    );

    event UnstakeSuccessful(
        uint256 poolIndex,
        address user,
        uint256 vaultIndex,
        uint256 totalShares
    );

    event UserWithDrawVaultSuccessful(
        uint256 poolIndex,
        address user,
        uint256 vaultIndex,
        uint256 amount
    );

    function addPool(PoolInfo memory payload) external;

    function stake(
        uint256 poolId,
        uint256[] memory tokensId,
        uint256[] memory amount
    ) external;

    function unstake(uint256 poolId, uint256 vaultId) external payable;

    function operatorAutoUnstake(
        uint256 poolId,
        uint256 vaultId,
        address user
    ) external;

    function setMoonCoin(address _moonCoinAddress) external;

    function setFeeReceiver(address _feeReceiver) external;

    function harvest(uint256 _poolId, uint256 _vaultId) external;

    function emergencyWithdrawMoonCoin() external;

    function pendingRewards(
        uint256 _poolId,
        address _user,
        uint256 _vaultId
    ) external view returns (uint256);

    function getPoolDetail(
        uint256 _poolId
    ) external view returns (PoolInfo memory);

    function getPoolsList() external view returns (PoolInfo[] memory);

    function totalPendingReward(address user) external view returns (uint256);

    function totalPendingRewardForCollection(
        address user,
        address collection
    ) external view returns (uint256);

    function totalNFTStakedForUser(
        address user,
        address collection
    ) external view returns (uint256);

    function pendingRewardForUser(
        address user
    ) external view returns (PendingRewardForUser[] memory);

    function userInfo(
        address user
    ) external view returns (UserDashboard[] memory);

    function remainRewardByCollection(
        address collection
    ) external view returns (uint256);

    function getUserVaultUnstakeDebt(
        uint256 _poolId,
        address _user,
        uint256 _vaultId
    ) external view returns (uint256);
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";

import "@openzeppelin/contracts/utils/math/SafeCast.sol";

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import {IERC165} from "@openzeppelin/contracts/utils/introspection/IERC165.sol";

import "./interfaces/IMoonCoinStaking.sol";

contract MoonCoinStaking is
    IMoonCoinStaking,
    ReentrancyGuardUpgradeable,
    AccessControlUpgradeable
{
    using SafeCast for uint256;
    using SafeCast for int256;

    bytes4 public constant INTERFACE_ID_ERC721 = 0x80ac58cd;
    bytes4 public constant INTERFACE_ID_ERC1155 = 0xd9b67a26;

    // uint256 public constant
    uint256 public constant SECONDS_PER_YEAR = 86400;

    uint256 public constant SECONDS_PER_MINUTE = 60;

    uint256 public constant SECONDS_PER_HOUR = 3600;

    uint256 public constant SECONDS_PER_MONTH = 3600;

    uint256 public constant SECONDS_PER_3_MONTH = 10800;

    uint256 public constant SECONDS_PER_6_MONTH = 21600;

    uint256 public constant SECONDS_PER_7_DAYS = 604_800;

    // uint256 public constant
    // uint256 public constant SECONDS_PER_YEAR = 31_536_000;

    // uint256 public constant SECONDS_PER_MINUTE = 60;

    // uint256 public constant SECONDS_PER_HOUR = 3600;

    // uint256 public constant SECONDS_PER_MONTH = 2_592_000;

    // uint256 public constant SECONDS_PER_3_MONTH = 7_776_000;

    // uint256 public constant SECONDS_PER_6_MONTH = 15_552_000;

    // uint256 public constant SECONDS_PER_7_DAYS = 604_800;

    mapping(uint256 => mapping(address => mapping(uint256 => UserVaultInfo)))
        public vaultStorage;

    mapping(uint256 => mapping(address => uint256)) private userMaxVaultStorage;

    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");

    bytes32 public constant ADMIN_UNSTAKE = keccak256("ADMIN_UNSTAKE");

    //pool
    mapping(uint256 => PoolInfo) public pools;
    uint256 public poolLength;

    address public MOONCOINADDRESS;

    address public feeReceiver;

    function init_data(address _moonCoinAddress) external initializer {
        __ReentrancyGuard_init();
        __AccessControl_init();
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        MOONCOINADDRESS = _moonCoinAddress;
    }

    function addPool(
        PoolInfo memory payload
    ) external override onlyRole(OPERATOR_ROLE) {
        for (uint256 i = 0; i < payload.timeRanges.length; ) {
            TimeRange memory temp = payload.timeRanges[i];
            if (block.timestamp >= temp.startTimestampHour)
                revert TimeStartIsInThePast();
            if (temp.endTimestampHour <= temp.startTimestampHour)
                revert EndMustGreaterThanStart();

            if (
                temp.startTimestampHour % SECONDS_PER_HOUR != 0 ||
                temp.endTimestampHour % SECONDS_PER_HOUR != 0
            ) {
                revert MustBeHour();
            }

            if (
                i != payload.timeRanges.length - 1 &&
                temp.endTimestampHour >
                payload.timeRanges[i + 1].startTimestampHour
            ) {
                revert TimeRangeInvalidData();
            }
            pools[poolLength].timeRanges.push(temp);

            unchecked {
                i++;
            }
        }

        uint256 totalTokenHaveToBePaid = _rewardBy(
            payload.timeRanges,
            payload.timeRanges[0].startTimestampHour,
            payload.timeRanges[payload.timeRanges.length - 1].endTimestampHour
        );

        IERC20(MOONCOINADDRESS).transferFrom(
            msg.sender,
            address(this),
            totalTokenHaveToBePaid
        );

        // require(payload.collection)
        pools[poolLength].collection = payload.collection;
        // if (
        //     payload.period != SECONDS_PER_7_DAYS &&
        //     payload.period != SECONDS_PER_MONTH &&
        //     payload.period != SECONDS_PER_3_MONTH &&
        //     payload.period != SECONDS_PER_6_MONTH &&
        //     payload.period != SECONDS_PER_YEAR
        // ) revert PeriodWrong();
        pools[poolLength].period = payload.period;

        uint8 checkingData = _check721and1155(payload.collection);

        if (checkingData == 1) pools[poolLength].isErc721 = true;
        else if (checkingData == 0) revert CollectionNotERC721orERC1155();
        emit AddPoolSuccessful(poolLength, payload);
        poolLength++;
    }

    function stake(
        uint256 poolId,
        uint256[] memory tokensId,
        uint256[] memory amount
    ) external override {
        // poolId must exist
        if (tokensId.length != amount.length) revert InputStakeForTokenWrong();
        if (poolId >= poolLength) revert PoolNotExists();

        PoolInfo memory poolTemp = pools[poolId];

        //get totalStake
        uint256 totalStakingVault;
        if (poolTemp.isErc721) {
            for (uint256 i = 0; i < amount.length; ) {
                // if (poolTemp.isErc721)
                require(amount[i] == 1, "revert amount not right");
                unchecked {
                    i++;
                }
            }
            totalStakingVault = amount.length;
        } else {
            for (uint256 i = 0; i < amount.length; ) {
                totalStakingVault += amount[i];
                unchecked {
                    i++;
                }
            }
        }

        // stake in timerages
        require(
            _checkStakingInRange(poolTemp.timeRanges),
            "Not stake in right time"
        );

        _updatePool(poolId);

        // transfer NFT
        _transferNftManager(
            poolTemp.collection,
            poolTemp.isErc721,
            msg.sender,
            address(this),
            tokensId,
            amount
        );

        // update User And Pool
        _userStake(poolId, msg.sender, tokensId, amount, totalStakingVault);

        emit StakeSuccessful(
            poolId,
            msg.sender,
            userMaxVaultStorage[poolId][msg.sender] - 1,
            pools[poolId].totalShares,
            tokensId,
            amount
        );
    }

    function unstake(
        uint256 poolId,
        uint256 vaultId
    ) external payable override {
        //precheck
        if (poolId >= poolLength) revert PoolNotExists();
        if (vaultId >= userMaxVaultStorage[poolId][msg.sender])
            revert VaultIndexWrong();

        _updatePool(poolId);

        PoolInfo memory poolTemp = pools[poolId];

        UserVaultInfo storage userVaultDetails = vaultStorage[poolId][
            msg.sender
        ][vaultId];

        require(
            msg.value == userVaultDetails.unstakeDebt,
            "MoonStaking: must pay operator unstake fee"
        );
        require(
            feeReceiver != address(0),
            "MoonStaking: fee recevier address cannot be address zero"
        );
        (bool success, ) = payable(feeReceiver).call{value: msg.value}("");
        require(success, "Failed to send Ether");

        require(userVaultDetails.tokensId.length > 0, "Not data");

        _transferNftManager(
            poolTemp.collection,
            poolTemp.isErc721,
            address(this),
            msg.sender,
            userVaultDetails.tokensId,
            userVaultDetails.amount
        );

        _unstake(poolId, msg.sender, vaultId);
        uint256 length = userVaultDetails.amount.length;

        uint256 amountUnstake;

        for (uint256 i = 0; i < length; ) {
            userVaultDetails.tokensId.pop();
            amountUnstake += userVaultDetails.amount[length - i - 1];
            userVaultDetails.amount.pop();
            unchecked {
                i++;
            }
        }

        emit UserWithDrawVaultSuccessful(
            poolId,
            msg.sender,
            vaultId,
            amountUnstake
        );

        _harvest(poolId, msg.sender, vaultId);
    }

    function operatorAutoUnstake(
        uint256 poolId,
        uint256 vaultId,
        address user
    ) external override onlyRole(ADMIN_UNSTAKE) {
        uint256 initGas = gasleft();
        //precheck

        if (poolId >= poolLength) revert PoolNotExists();
        if (vaultId >= userMaxVaultStorage[poolId][user])
            revert VaultIndexWrong();

        _updatePool(poolId);

        UserVaultInfo storage userVaultDetails = vaultStorage[poolId][user][
            vaultId
        ];

        if (userVaultDetails.userShares == 0) revert UserHaveNoShare();
        _unstake(poolId, user, vaultId);

        //admin unstake required have to be out of period
        if (
            userVaultDetails.startStake + pools[poolId].period > block.timestamp
        ) revert AdminWithdrawNotInPeriod();

        // store gas used
        uint256 gasPrice;
        assembly {
            gasPrice := gasprice()
        }

        userVaultDetails.unstakeDebt = gasPrice * (initGas - gasleft() + 43000);
    }

    function setMoonCoin(
        address _moonCoinAddress
    ) public override onlyRole(OPERATOR_ROLE) {
        require(
            _moonCoinAddress != address(0),
            "MoonCoin: address cannot be zero"
        );
        MOONCOINADDRESS = _moonCoinAddress;
    }

    function setFeeReceiver(
        address _feeReceiver
    ) public override onlyRole(OPERATOR_ROLE) {
        require(_feeReceiver != address(0), "MoonCoin: address cannot be zero");
        feeReceiver = _feeReceiver;
    }

    function harvest(uint256 _poolId, uint256 _vaultId) external override {
        PoolInfo storage pool = pools[_poolId];
        UserVaultInfo storage userVaultDetails = vaultStorage[_poolId][
            msg.sender
        ][_vaultId];
        if (userVaultDetails.startStake + pool.period > block.timestamp)
            revert RewardStillLocked();
        _harvest(_poolId, msg.sender, _vaultId);
    }

    function emergencyWithdrawMoonCoin()
        external
        override
        onlyRole(OPERATOR_ROLE)
    {
        uint256 balance = IERC20(MOONCOINADDRESS).balanceOf(msg.sender);
        IERC20(MOONCOINADDRESS).transfer(msg.sender, balance);
    }

    function pendingRewards(
        uint256 _poolId,
        address _user,
        uint256 _vaultId
    ) public view override returns (uint256) {
        PoolInfo memory pool = pools[_poolId];
        UserVaultInfo memory userVaultDetails = vaultStorage[_poolId][_user][
            _vaultId
        ];

        uint256 previousTimestampHour = (block.timestamp / SECONDS_PER_HOUR) *
            SECONDS_PER_HOUR;

        uint256 rewardsSinceLastCalculated = _rewardBy(
            pool.timeRanges,
            pool.lastUpdated,
            previousTimestampHour
        );
        uint256 accumulatedRewardsPerShare = pool.accumulatedRewardsPerShare;

        if (
            block.timestamp > pool.lastUpdated + SECONDS_PER_HOUR &&
            pool.totalShares != 0
        ) {
            accumulatedRewardsPerShare =
                accumulatedRewardsPerShare +
                rewardsSinceLastCalculated /
                pool.totalShares;
        }

        return
            ((userVaultDetails.userShares * accumulatedRewardsPerShare)
                .toInt256() - userVaultDetails.rewardDebt).toUint256();
    }

    function getPoolDetail(
        uint256 _poolId
    ) public view override returns (PoolInfo memory) {
        return pools[_poolId];
    }

    function getPoolsList() public view override returns (PoolInfo[] memory) {
        PoolInfo[] memory poolsList = new PoolInfo[](poolLength);
        for (uint256 i = 0; i < poolLength; i++) {
            poolsList[i] = getPoolDetail(i);
        }
        return poolsList;
    }

    function totalPendingReward(
        address user
    ) public view override returns (uint256) {
        uint256 totalReward;
        for (uint256 i = 0; i < poolLength; i++) {
            for (uint256 j = 0; j < userMaxVaultStorage[i][user]; j++) {
                totalReward = totalReward + pendingRewards(i, user, j);
            }
        }

        return totalReward;
    }

    function totalPendingRewardForCollection(
        address user,
        address collection
    ) public view override returns (uint256) {
        uint256 totalReward;
        for (uint256 i = 0; i < poolLength; i++) {
            if (pools[i].collection == collection) {
                for (uint256 j = 0; j < userMaxVaultStorage[i][user]; j++) {
                    totalReward = totalReward + pendingRewards(i, user, j);
                }
            }
        }
        return totalReward;
    }

    function totalNFTStakedForUser(
        address user,
        address collection
    ) external view override returns (uint256) {
        uint256 totalNFT;
        for (uint256 i = 0; i < poolLength; i++) {
            if (pools[i].collection == collection) {
                for (uint256 j = 0; j < userMaxVaultStorage[i][user]; j++) {
                    UserVaultInfo memory vaultTemp = vaultStorage[i][user][j];
                    for (uint256 k = 0; k < vaultTemp.amount.length; k++) {
                        totalNFT = totalNFT + vaultTemp.amount[k];
                    }
                }
            }
        }
        return totalNFT;
    }

    function pendingRewardForUser(
        address user
    ) external view override returns (PendingRewardForUser[] memory) {
        // PendingRewardForUser[] memory results = new PendingRewardForUser[]()
        uint256 length;
        for (uint256 i = 0; i < poolLength; i++) {
            length += userMaxVaultStorage[i][user];
        }
        PendingRewardForUser[] memory results = new PendingRewardForUser[](
            length
        );

        uint256 index;

        for (uint256 i = 0; i < poolLength; i++) {
            // length += userMaxVaultStorage[i][user];
            for (uint256 j = 0; j < userMaxVaultStorage[i][user]; j++) {
                // totalReward = totalReward + pendingRewards(i, user, j);
                results[index] = PendingRewardForUser(
                    i,
                    j,
                    pendingRewards(i, user, j)
                );
                index++;
            }
        }
        return results;
    }

    function userInfo(
        address user
    ) external view override returns (UserDashboard[] memory) {
        uint256 length;
        for (uint256 i = 0; i < poolLength; i++) {
            length += userMaxVaultStorage[i][user];
        }
        UserDashboard[] memory results = new UserDashboard[](length);

        uint256 index;

        for (uint256 i = 0; i < poolLength; i++) {
            for (uint256 j = 0; j < userMaxVaultStorage[i][user]; j++) {
                // UserVaultInfo memory userVaultDetail = getVaultStorageDetail(i, user, j);
                UserVaultInfo memory userVaultDetail = vaultStorage[i][user][j];
                results[index] = UserDashboard(
                    i,
                    j,
                    userVaultDetail.tokensId,
                    userVaultDetail.amount,
                    pendingRewards(i, user, j)
                );
                index++;
            }
        }
        return results;
    }

    function remainRewardByCollection(
        address collection
    ) public view override returns (uint256) {
        uint256 result;
        for (uint256 i = 0; i < poolLength; i++) {
            if (pools[i].collection == collection) {
                result += _rewardBy(
                    pools[i].timeRanges,
                    (block.timestamp / 3600) * 3600,
                    pools[i]
                        .timeRanges[pools[i].timeRanges.length - 1]
                        .endTimestampHour
                );
            }
        }
        return result;
    }

    function getUserVaultUnstakeDebt(
        uint256 _poolId,
        address _user,
        uint256 _vaultId
    ) external view override returns (uint256) {
        UserVaultInfo memory userVaultDetails = vaultStorage[_poolId][_user][
            _vaultId
        ];
        return userVaultDetails.unstakeDebt;
    }

    function _harvest(
        uint256 _poolId,
        address _user,
        uint256 _vaultId
    ) private {
        PoolInfo storage pool = pools[_poolId];
        UserVaultInfo storage userVaultDetails = vaultStorage[_poolId][_user][
            _vaultId
        ];

        _updatePool(_poolId);

        int256 accumulatedMoonCoins = (userVaultDetails.userShares *
            uint256(pool.accumulatedRewardsPerShare)).toInt256();
        uint256 rewardsToBeClaimed = (accumulatedMoonCoins -
            userVaultDetails.rewardDebt).toUint256();

        userVaultDetails.rewardDebt = accumulatedMoonCoins;

        if (rewardsToBeClaimed != 0) {
            IERC20(MOONCOINADDRESS).transfer(_user, rewardsToBeClaimed);
        }

        emit HarvestSuccessful(_poolId, _user, _vaultId, rewardsToBeClaimed);
    }

    function _unstake(uint256 poolId, address user, uint256 vaultId) private {
        UserVaultInfo storage userVaultDetails = vaultStorage[poolId][user][
            vaultId
        ];

        uint256 sharesForUser = userVaultDetails.userShares;

        userVaultDetails.userShares = 0;
        pools[poolId].totalShares -= sharesForUser;

        //check is penalty
        if (
            userVaultDetails.startStake + pools[poolId].period > block.timestamp
        ) {
            userVaultDetails.rewardDebt = 0;
        } else {
            userVaultDetails.rewardDebt -= (sharesForUser *
                pools[poolId].accumulatedRewardsPerShare).toInt256();
        }

        emit UnstakeSuccessful(
            poolId,
            user,
            vaultId,
            pools[poolId].totalShares
        );
    }

    function _userStake(
        uint256 poolId,
        address user,
        uint256[] memory tokensId,
        uint256[] memory amount,
        uint256 shares
    ) private {
        UserVaultInfo storage userVaultDetails = vaultStorage[poolId][user][
            userMaxVaultStorage[poolId][user]
        ];
        userVaultDetails.tokensId = tokensId;
        userVaultDetails.amount = amount;
        userVaultDetails.userShares = shares;
        userVaultDetails.startStake =
            (block.timestamp / SECONDS_PER_HOUR) *
            SECONDS_PER_HOUR;

        userVaultDetails.rewardDebt = (shares *
            pools[poolId].accumulatedRewardsPerShare).toInt256();

        pools[poolId].totalShares += shares;
        userMaxVaultStorage[poolId][user]++;
    }

    function _transferNftManager(
        address collection,
        bool isErc721,
        address from,
        address to,
        uint256[] memory tokensId,
        uint256[] memory amount
    ) private {
        if (isErc721) {
            for (uint256 i = 0; i < amount.length; ) {
                IERC721(collection).transferFrom(from, to, tokensId[i]);
                unchecked {
                    i++;
                }
            }
        } else {
            for (uint256 i = 0; i < amount.length; ) {
                IERC1155(collection).safeTransferFrom(
                    from,
                    to,
                    tokensId[i],
                    amount[i],
                    ""
                );
                unchecked {
                    i++;
                }
            }
        }
    }

    function _updatePool(uint256 poolId) private {
        PoolInfo storage pool = pools[poolId];

        if (block.timestamp < pool.timeRanges[0].startTimestampHour) return;
        if (block.timestamp < pool.lastUpdated + SECONDS_PER_HOUR) return;

        //
        uint256 previousTimestampHour = (block.timestamp / SECONDS_PER_HOUR) *
            SECONDS_PER_HOUR;
        uint256 rewardBy = _rewardBy(
            pool.timeRanges,
            pool.lastUpdated,
            previousTimestampHour
        );
        if (pool.totalShares != 0) {
            pool.accumulatedRewardsPerShare = (pool.accumulatedRewardsPerShare +
                rewardBy /
                pool.totalShares);
        }

        pool.lastUpdated = previousTimestampHour;
    }

    function _check721and1155(address collection) private view returns (uint8) {
        if (IERC165(collection).supportsInterface(INTERFACE_ID_ERC721)) {
            return 1;
        } else if (
            IERC165(collection).supportsInterface(INTERFACE_ID_ERC1155)
        ) {
            return 2;
        }
        return 0;
    }

    function _checkStakingInRange(
        TimeRange[] memory ranges
    ) private view returns (bool) {
        for (uint256 i = 0; i < ranges.length; ) {
            if (
                block.timestamp < ranges[i].endTimestampHour &&
                block.timestamp > ranges[i].startTimestampHour
            ) {
                return true;
            }

            unchecked {
                i++;
            }
        }
        return false;
    }

    function _rewardBy(
        TimeRange[] memory ranges,
        uint256 startTime,
        uint256 endTime
    ) private pure returns (uint256) {
        uint256 totalReward;
        if (
            startTime < ranges[0].startTimestampHour &&
            endTime < ranges[0].startTimestampHour
        ) return 0;
        if (startTime < ranges[0].startTimestampHour)
            startTime = ranges[0].startTimestampHour;
        if (startTime > ranges[ranges.length - 1].endTimestampHour) return 0;
        // return 0;
        for (uint256 i = 0; i < ranges.length; i++) {
            if (startTime > ranges[i].endTimestampHour) {
                startTime = ranges[i + 1].startTimestampHour;
                continue;
            }
            if (endTime > ranges[i].endTimestampHour) {
                totalReward +=
                    (ranges[i].rewardsPerHour *
                        (ranges[i].endTimestampHour - startTime)) /
                    3600;
                if (i == ranges.length - 1) {
                    return totalReward;
                }
                startTime = ranges[i + 1].startTimestampHour;
                continue;
            } else {
                totalReward +=
                    (ranges[i].rewardsPerHour * (endTime - startTime)) /
                    3600;
                return totalReward;
            }
        }
        return totalReward;
    }
}