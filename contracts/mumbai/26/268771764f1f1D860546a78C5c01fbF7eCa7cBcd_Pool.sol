     **/
    function _exitLongPosition(uint256 bpsToWithdraw) public returns (uint256, uint256, uint256) {
        require(bpsToWithdraw > 0, "Error: LP amount cannot be 0");
        uint256 zPosition = lpConversions[msg.sender];
        uint256 calculatedZToRemove = (zPosition*bpsToWithdraw)/10000;
        require(calculatedZToRemove <= zPosition, "Error: Calculating Z position to reduce");
        lpConversions[msg.sender] -= calculatedZToRemove;

        uint256 calculatedXToRemove = (xConversions[msg.sender]*bpsToWithdraw)/10000;
        uint256 calculatedYToRemove = (yConversions[msg.sender]*bpsToWithdraw)/10000;
        uint128 calculatedCollateralToRemove = (collateralDeposits[msg.sender]*uint128(bpsToWithdraw))/10000;
        xConversions[msg.sender] -= calculatedXToRemove;
        yConversions[msg.sender] -= calculatedYToRemove;
        collateralDeposits[msg.sender] -= calculatedCollateralToRemove;
        liquidityCurrentlyConverted -= calculatedZToRemove;
        // Add xAmountHeld and yAmountHeld for leftovers
        // Add liquidity to uniswap
        // TODO: needs to be tested
        (uint128 liquidityProvided, uint xTokensProvided, uint yTokensProvided) = increaseLiquidityCurrentRange(calculatedXToRemove, calculatedYToRemove);
        if (liquidityProvided > calculatedCollateralToRemove) {
            (uint256 amountA, uint256 amountB) = decreaseLiquidityCurrentRange(liquidityProvided - calculatedCollateralToRemove);
            xTokensProvided += amountA;
            yTokensProvided += amountB;
        } // TODO: in the opposite scenario, there might be an issue with calculating the correct liquidity to add. Calculated based on the previous calculation?
        
        if (xTokensProvided < calculatedXToRemove) {
            IERC20(underlyingAssetAddressA).transfer(msg.sender, calculatedXToRemove - xTokensProvided);
        }
        if (yTokensProvided < calculatedYToRemove) {
            IERC20(underlyingAssetAddressB).transfer(msg.sender, calculatedYToRemove - yTokensProvided);
        }
        return (calculatedXToRemove, calculatedYToRemove, calculatedCollateralToRemove);
    }

    function buyAndHold(uint128 lpAmount, address user) internal returns (uint256, uint256) {
        (uint256 amountA, uint256 amountB) = decreaseLiquidityCurrentRange(lpAmount);
        xConversions[user] += amountA;
        yConversions[user] += amountB;
        liquidityCurrentlyConverted += lpAmount;
        return (amountA, amountB);
    }

