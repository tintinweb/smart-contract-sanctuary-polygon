pragma solidity ^0.8.0;

import "./ERC721Drop.sol";
import "./ITrade.sol";

contract ERC721IA is ERC721Drop, ITrade {

    constructor(
        string memory _name,
        string memory _symbol,
        address _royaltyRecipient,
        uint128 _royaltyBps,
        address _primarySaleRecipient,
        uint128 _startToken
    )
        ERC721Drop(
            _name,
            _symbol,
            _royaltyRecipient,
            _royaltyBps,
            _primarySaleRecipient
        )
    {
        _currentIndex = _startToken;
        nextTokenIdToLazyMint = _startToken;

    }

    /// @dev Mapping from uid of a tokenId => trade offeror address => trade offer made on a claimed NFT by the respective offeror.
    mapping(uint256 => mapping(address => TradeOffer)) public tradeoffers;

    /*///////////////////////////////////////////////////////////////
                        Trade Offer logic
    //////////////////////////////////////////////////////////////*/

 


    /// @dev Lets a NFT owners accept an offer for their NFT.
    function acceptTradeOffer(
        uint256 _tokenId,
        address _tokenOwner,
        address _offeror,
        uint256 _tradeTokenId
    ) external {
        // First confirm there is an existing offer to be accepted.
        _verifyValidOffer(_tokenId, _offeror, _tradeTokenId);

        // Check authorization & ownership
        if (msg.sender != _tokenOwner) {
            revert("Not authorized to accept trade offer");
        }

        if (!areYouTheOwner(_tokenOwner, _tokenId)) {
            revert("You Need to own NFT to accept trade offer");
        }

        // Confirm that the offeror still owns the offered NFTs
        _verifyOfferIsHolder(_tokenId, _offeror, _tradeTokenId);

        // Passed all checks time to Remove the accepted offer.
        delete tradeoffers[_tokenId][_tokenOwner];
        emit TradeOfferRemoved(_tokenId, _offeror);

        // Begin the trading of NFTs
        _tradeNFTS(_tokenId, _tokenOwner, _offeror, _tradeTokenId);
    }

    /// @dev Verifies there is existing offer to be accepted
    function _verifyValidOffer(
        uint256 _tokenId,
        address _offeror,
        uint256 _tradeTokenId
    ) internal virtual {
        TradeOffer memory currentOffer = tradeoffers[_tokenId][_offeror];

        /// @notice The acceptance data must match an existing trade offer
        if (currentOffer.tokenId != _tokenId) {
            revert("Not A Valid Offer - tokens do not Match");
        }
        if (currentOffer.offeror != _offeror) {
            revert("Not A Valid Offer - wallets must match");
        }
        if (currentOffer.tradeTokenId != _tradeTokenId) {
            revert("Not A Valid Offer - Trade tokens do not Match");
        }
    }

    /// @dev Verify the Offer is still a holder
    function _verifyOfferIsHolder(
        uint256 _tokenId,
        address _offeror,
        uint256 _tradeTokenId
    ) internal virtual {
        ///  Check each NFT to make sure offeror is still owns NFTs
        if (!areYouTheOwner(_offeror, _tradeTokenId)) {
            /// The offeror no longer owns NFTs, removes offers for being Invalid
            _removeInvalidOffer(_tokenId, _offeror);
            revert("Could not verify offeror is not the current owner");
        }
    }

    /// @dev Removes the Offer if the owner no Longer Owns the NFTs
    function _removeInvalidOffer(
        uint256 _tokenId,
        address _offeror
    ) internal virtual {
        TradeOffer memory targetTradeOffer = tradeoffers[_tokenId][_offeror];
        delete tradeoffers[_tokenId][_offeror];
        emit TradeOfferRemoved(_tokenId, targetTradeOffer.offeror);
    }

    /// @dev Allows the offeror to remove their offer
    function cancelOffer(uint256 _tokenId, address _offeror) external {
        TradeOffer memory targetTradeOffer = tradeoffers[_tokenId][_offeror];

        // only the offeror can cancel the offer
        if (_offeror != _dropMsgSender()) {
            revert("Not Allowed to Cancel Must Be the Offeror");
        }

        delete tradeoffers[_tokenId][_offeror];
        emit TradeOfferRemoved(_tokenId, targetTradeOffer.offeror);
    }

    ///  @dev    Lets a two NFT owers Trade NFTs.
    ///  @notice Must check that
    ///          1. The trade offer exist.
    ///          2. Both parties have agreed to this trade
    ///          before this function is called.
    function _tradeNFTS(
        uint256 _tokenId,
        address _tokenOwner,
        address _offeror,
        uint256 _tradeTokenId
    ) internal {
        /// Transfer the approved offer, from
        safeTransferFrom(_tokenOwner, _offeror, _tokenId);
        safeTransferFrom(_offeror, _tokenOwner, _tradeTokenId);
    }

    /// @dev    Lets a holder of NFT from this collection make an offer
    ///         to trade a NFT with another holder.
    function makeTradeOffer(
        uint256 nftId,
        address offeror,
        uint256 tradeTokenId
    ) external virtual {
        // Offeror is the only one who can make the offer.
        if (msg.sender != offeror) {
            revert("Only owners of tokens are authorized offer");
        }

        // Offeror must own the NFT being offered.

        if (!areYouTheOwner(offeror, tradeTokenId)) {
            revert("Offeror must own the NFT being offered");
        }

        // NFT must be claimed.
        doesTokenExist(uint16(nftId));

        TradeOffer memory newTradeOffer = TradeOffer({
            tokenId: nftId,
            offeror: offeror,
            tradeTokenId: tradeTokenId
        });

        _handleTradeOffer(nftId, newTradeOffer);
    }

    /// @dev Processes a new trade offer on a NFT
    function _handleTradeOffer(
        uint256 _tokenId,
        TradeOffer memory _newTradeOffer
    ) internal {
        tradeoffers[_tokenId][_newTradeOffer.offeror] = _newTradeOffer;

        emit NewTradeOffer(
            _tokenId,
            _newTradeOffer.offeror,
            _newTradeOffer.tradeTokenId
        );
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256 id,
        uint256 amount
    ) internal virtual {}

    function _afterTokenTransfer(
        address operator,
        address from,
        address to,
        uint256 id,
        uint256 amount
    ) internal virtual {}
}

