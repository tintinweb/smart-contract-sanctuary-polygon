     */
    mapping(bytes32 => mapping(address => uint256))
        private supplyClaimedByWallet;

    /// @dev Map of the number of NFTs claimed for free my account.
    mapping(address => uint256) public numClaimedForFree;
    mapping(uint256 => Product) public product;

    // mapping(uint256 => mapping(address => Product)) public product;
    /*///////////////////////////////////////////////////////////////
                            Drop logic
    //////////////////////////////////////////////////////////////*/


    function addProduct(uint256 _productId, uint256 _price) public {
        // Only Contract owner Should be able to call this function
        if (!_canSetClaimConditions()) {
            revert("Not authorized");
        }
        Product memory newProduct = Product({
            productId: _productId,
            price: _price
        });
        product[_productId] = newProduct;
    }



    function getProductPrice(uint256 productId) public view returns (uint256) {
        // Only Contract owner Should be able to call this function
        if (!_canSetClaimConditions()) {
            revert("Not authorized");
        }
        return product[productId].price;
    }

    function updateProductPrice(uint256 _productId, uint256 _price) public {
        // Only Contract owner Should be able to call this function
        if (!_canSetClaimConditions()) {
            revert("Not authorized");
        }
        Product memory updateProduct = Product({
            productId: _productId,
            price: _price
        });
        product[_productId] = updateProduct;
    }

    function removeProduct(uint256 _productId) public {
        // Only Contract owner Should be able to call this function
        if (!_canSetClaimConditions()) {
            revert("Not authorized");
        }
        delete product[_productId];
    }

    function addProductBatch(Product[] calldata _productData, ExecutionType _executionType ) public {
        // Only Contract owner Should be able to call this function
        if (!_canSetClaimConditions()) {
            revert("Not authorized");
        }
        for (uint256 i; i < _productData.length; ) {

            if(_executionType == ExecutionType.Add){

                addProduct(_productData[i].productId, _productData[i].price);
               
            }else if(_executionType == ExecutionType.Update){

                updateProductPrice(_productData[i].productId, _productData[i].price);

            }else if(_executionType == ExecutionType.Remove){
                removeProduct(_productData[i].productId);
            }

             ++i;

        }

    }


    /// @dev Lets an account claim tokens.
    function claim(
        address _receiver,
        uint256 _quantity,
        address _currency,
        uint256 _pricePerToken,
        AllowlistProof calldata _allowlistProof,
        bytes memory _data
    ) public payable virtual override {
        _beforeClaim(
            _receiver,
            _quantity,
            _currency,
            _pricePerToken,
            _allowlistProof,
            ""
        );

        bytes32 activeConditionId = conditionId;
        bool isActiveOverride;
        uint256 claimLimit;

        (, isActiveOverride, claimLimit) = verifyClaim(
            _dropMsgSender(),
            _quantity,
            _currency,
            _pricePerToken,
            _allowlistProof
        );

        // Update contract state.
        claimCondition.supplyClaimed += _quantity;
        supplyClaimedByWallet[activeConditionId][_dropMsgSender()] += _quantity;

        _collectPriceOnClaim(
            address(0),
            _quantity,
            isActiveOverride,
            claimLimit,
            _currency,
            _pricePerToken,
            0
        );

        // Mint the relevant NFTs to claimer.
        uint256 startTokenId = _transferTokensOnClaim(_receiver, _quantity);

        emit TokensClaimed(
            _dropMsgSender(),
            _receiver,
            startTokenId,
            _quantity
        );

        _afterClaim(
            _receiver,
            _quantity,
            _currency,
            _pricePerToken,
            _allowlistProof,
            _data
        );
    }

    function getPrintPrice(
        uint256 productId,
        uint16 quantity
    ) public  returns (uint256 totalPrice) {

        uint256 productPrice = getProductPrice(productId);

        if(productId == 0){ 
            totalPrice += 0; 
        }else{
            totalPrice += productPrice * uint256(quantity);
        }
        return totalPrice;
    }
