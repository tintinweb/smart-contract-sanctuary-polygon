    function doesTokenExist(uint16 _tokenId) public pure returns (uint16){
        // Token must be inside of the collections range.
        if ( _tokenId > 0 &&  _tokenId< 5476) {
           return _tokenId;
        }
        revert NftDoesNotExist(_tokenId); // Error the token doesn't exist
        
    }

    /**
