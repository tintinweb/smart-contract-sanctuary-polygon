abstract contract ColoringManagerState {
    address public coloringManager;

    function _setColoringManager(address _coloringManager) internal {
        require(
            Address.isContract(_coloringManager),
            "CrayonStorage#_setColoringManager: NOT_CONTRACT"
        );
        coloringManager = _coloringManager;
    }
}

library StringLib {
    function isStringEqual(string memory a, string memory b)
        internal
        pure
        returns (bool)
    {
        return keccak256(abi.encodePacked(a)) == keccak256(abi.encodePacked(b));
    }

    function toString(uint256 value_) internal pure returns (string memory) {
        if (value_ == 0) {
            return "0";
        }
        uint256 _iterate = value_;
        uint256 _digits;
        while (_iterate != 0) {
            _digits++;
            _iterate /= 10;
        } // get digits in value_
        bytes memory _buffer = new bytes(_digits);
        while (value_ != 0) {
            _digits--;
            _buffer[_digits] = bytes1(uint8(48 + uint256(value_ % 10)));
            value_ /= 10;
        } // create bytes of value_
        return string(_buffer); // return string converted bytes of value_
    }
}

contract CrayonStorage is
    ERC1155,
    ERC1155Supply,
    ERC1155Burnable,
    AccessControl,
    ICrayonStorage,
    ColoringManagerState,
    Ownable
{
    struct CrayonData {
        string name;
        string colorCode;
        uint256 maxSupply;
    }

    string public name;
    string public symbol;
    uint256 private _currentCrayonId = 0;
    bool private _firstCrayonCreated = false;
    mapping(uint256 => CrayonData) private _crayonData;
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    constructor() ERC1155("") {
        name = "CryptoCrayons";
        symbol = "CRAYONS";
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
    }

    function currentCrayonId() public view override returns (uint256) {
        return _currentCrayonId;
    }

    function crayonData(uint256 crayonId) public view returns (CrayonData memory) {
        return _crayonData[crayonId];
    }

    function numCrayonTypes() public view override returns (uint256) {
        if (!_firstCrayonCreated) {
            return 0;
        } else {
            return _currentCrayonId + 1;
        }
    }

    function grantMinterRole(address _minter, bool isMinter) public onlyRole(DEFAULT_ADMIN_ROLE) {
        if (isMinter) {
            grantRole(MINTER_ROLE, _minter);
        } else {
            revokeRole(MINTER_ROLE, _minter);
        }
    }

    function uri(uint256 _id)
        public
        view
        override(ERC1155)
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(
                    super.uri(_id),
                    StringLib.toString(_id),
                    ".json"
                )
            );
    }

    function supportsInterface(bytes4 interfaceId)
        public
        pure
        override(IERC165, ERC1155, AccessControl)
        returns (bool)
    {
        return
            interfaceId == type(IERC165).interfaceId ||
            interfaceId == type(IERC1155).interfaceId ||
            interfaceId == type(IERC1155MetadataURI).interfaceId ||
            interfaceId == type(IAccessControl).interfaceId ||
            interfaceId == type(ICrayonStorage).interfaceId;
    }

    function setURI(string memory newuri) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _setURI(newuri);
    }

    function create(
        address _initialOwner,
        uint256 _initialSupply,
        uint256 _maxSupply,
        string calldata _name,
        string calldata _colorCode
    ) external onlyRole(DEFAULT_ADMIN_ROLE) returns (uint256) {
        require(
            bytes(_name).length > 0,
            "CrayonStorage#create: NO_NAME_SPECIFIED"
        );
        require(
            bytes(_colorCode).length > 0,
            "CrayonStorage#create: NO_COLOR_CODE_SPECIFIED"
        );

        // verify that there is no crayon with the same color and no crayon with same name
        for (uint256 i = 0; i < _currentCrayonId + 1; i++) {
            CrayonData memory _crayonDataOfId = _crayonData[i];
            require(
                !StringLib.isStringEqual(_name, _crayonDataOfId.name),
                "CrayonStorage#create NAME_EXISTS"
            );
            require(
                !StringLib.isStringEqual(_colorCode, _crayonDataOfId.colorCode),
                "CrayonStorage#create COLOR_CODE_EXISTS"
            );
        }

        CrayonData memory newCrayonData = CrayonData({
            name: _name,
            colorCode: _colorCode,
            maxSupply: _maxSupply
        });

        if (!_firstCrayonCreated) {
            _firstCrayonCreated = true;
            _currentCrayonId = 0;
        } else {
            _currentCrayonId += 1;
        }

        uint _id = _currentCrayonId;
        _crayonData[_id] = newCrayonData;
        _mint(_initialOwner, _id, _initialSupply, abi.encode(newCrayonData));
        return _id;
    }

    function setColoringManager(address _coloringManager)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _setColoringManager(_coloringManager);
    }

    function setApprovalForColorContract(bool approved) public override {
        require(
            coloringManager != address(0),
            "CrayonStorage#setApprovalForColorContact COLOR_CONTRACT_NOT SET"
        );
        _setApprovalForAll(_msgSender(), coloringManager, approved);
    }

    function _crayonExists(uint256 id) internal view {
        require(
            bytes(_crayonData[id].name).length > 0,
            "CrayonStorage#mint CRAYON_ID_DOESNT_EXIST"
        );
    }

    function mint(
        address account,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public override onlyRole(MINTER_ROLE) {
        _crayonExists(id);
        require(
            _crayonData[id].maxSupply >= totalSupply(id) + amount,
            "CrayonStorage#mint NOT_ENOUGH_SUPPLY"
        );
        _mint(account, id, amount, data);
    }

    function mintBatch(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public override onlyRole(MINTER_ROLE) {
        for (uint256 i = 0; i < ids.length; i++) {
            uint256 _id = ids[i];
            _crayonExists(_id);
            require(
                _crayonData[_id].maxSupply >= totalSupply(_id) + amounts[i],
                "CrayonStorage#mintBatch NOT_ENOUGH_SUPPLY"
            );
        }
        _mintBatch(to, ids, amounts, data);
    }

    function burn(
        address account,
        uint256 id,
        uint256 value
    ) public override(ICrayonStorage, ERC1155Burnable) {
        ERC1155Burnable.burn(account, id, value);
    }

    function burnBatch(
        address account,
        uint256[] memory ids,
        uint256[] memory values
    ) public override(ICrayonStorage, ERC1155Burnable) {
        ERC1155Burnable.burnBatch(account, ids, values);
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155, ERC1155Supply) {
        ERC1155Supply._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }
}

// SPDX-License-Identifier: MIT
// An example of a consumer contract that relies on a subscription for funding.
pragma solidity ^0.8.4;

import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";

abstract contract VRFConsumer is VRFConsumerBaseV2 {
  VRFCoordinatorV2Interface COORDINATOR;
  uint64 subId;
  address vrfCoordinator;
  bytes32 keyHash;
  uint16 requestConfirmations = 3;

  constructor(address _vrfCoordinator, bytes32 _keyHash, uint64 _subId) VRFConsumerBaseV2(_vrfCoordinator) {
    COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
    vrfCoordinator = _vrfCoordinator;
    subId = _subId;
    keyHash = _keyHash;
  }

  function _setSubId(uint64 newSubId) internal {
    subId = newSubId;
  }

  function _setKeyHash(bytes32 newKeyHash) internal {
    keyHash = newKeyHash;
  }

  function _setReqConfirmations(uint16 newReqConfirmations) internal {
    requestConfirmations = newReqConfirmations;
  }

  // Assumes the subscription is funded sufficiently.
  function _requestRandomWords(uint32 numWords, uint32 callbackGasLimit) internal returns(uint256) {
    // Will revert if subscription is not set and funded.
    return COORDINATOR.requestRandomWords(
      keyHash,
      subId,
      requestConfirmations,
      callbackGasLimit,
      numWords
    );
  }
}

