import "./Ownable.sol";
import "./interfaces/IColorableOwnershipManager.sol";
import "./CrayonStorage.sol";
import "../VRF/Vrf.sol";

contract CrayonShopV1 is VRFConsumer, Ownable {
    mapping(uint256 => bool) private _claimedTokenIds;
    mapping(uint256 => uint256[]) private _tokenIdRandRequests; // map requestId to array of tokenIds
    uint256[] private _randomNumForTokens = new uint256[](2501); // index is tokenId of mainnet Zilla

    // vrf config vars
    uint256 public maxTokenIdsPerRandReq = 75;
    uint32 internal callbackGasLimit = 2499999;

    IColorableOwnershipManager private _colorableOwnershipManager;
    ICrayonStorage private _crayonStorage;
    address private _rootChainCollection;

    mapping(uint256 => uint256) private _numColorsOfZilla;
    mapping(uint256 => bool) private _wonderpalZillas;
    mapping(uint256 => bool) private _coolCatZillas;

    constructor(
        address _vrfCoordinator,
        bytes32 _keyHash,
        uint64 _subId
    ) VRFConsumer(_vrfCoordinator, _keyHash, _subId) {}

    // set dependencies
    function setColorableOwnershipManager(address colorableOwnershipManager_) external onlyOwner {
        _colorableOwnershipManager = IColorableOwnershipManager(colorableOwnershipManager_);
    }

    function setCrayonStorage(address crayonStorage_) external onlyOwner {
        _crayonStorage = ICrayonStorage(crayonStorage_);
    }

    function setRootChainCollection(address rootChainCollection_) external onlyOwner {
        _rootChainCollection = rootChainCollection_;
    }

    // set collection metadata
    function setNumColorsOfZilla(uint256[] memory tokenIds, uint256[] memory numColors)
        external
        onlyOwner
    {
        uint256 _loopThrough = tokenIds.length;
        require(
            _loopThrough == numColors.length,
            "CrayonShopV1#setNumColorsOfZilla: PARAMS_LENGTH_MISMATCH"
        );
        for (uint256 i = 0; i < _loopThrough; i++) {
            _numColorsOfZilla[tokenIds[i]] = numColors[i];
        }
    }

    function setWonderpalZillas(uint256[] memory tokenIds, bool[] memory isWpZilla)
        external
        onlyOwner
    {
        uint256 _loopThrough = tokenIds.length;
        require(
            _loopThrough == isWpZilla.length,
            "CrayonShopV1#setWonderpalZillas: PARAMS_LENGTH_MISMATCH"
        );
        for (uint256 i = 0; i < _loopThrough; i++) {
            _wonderpalZillas[tokenIds[i]] = isWpZilla[i];
        }
    }

    function setCoolCatsZillas(uint256[] memory tokenIds, bool[] memory isCCZilla)
        external
        onlyOwner
    {
        uint256 _loopThrough = tokenIds.length;
        require(
            _loopThrough == isCCZilla.length,
            "CrayonShopV1#setWonderpalZillas: PARAMS_LENGTH_MISMATCH"
        );
        for (uint256 i = 0; i < _loopThrough; i++) {
            _coolCatZillas[tokenIds[i]] = isCCZilla[i];
        }
    }

    /**
    @dev - used to reduce gas fees of Chainlink VRF callback
     */
    function setRandomNumbersToNonEmpty(uint256 startTokenId, uint256 endTokenId)
        external
        onlyOwner
    {
        for (uint256 i = startTokenId; i < endTokenId + 1; i++) {
            _randomNumForTokens[i] = 1;
        }
    }

    // chainlink vrf setters
    function setMaxTokenIdsPerRandReq(uint256 newMax) external onlyOwner {
        maxTokenIdsPerRandReq = newMax;
    }

    function setSubId(uint64 newSubId) external onlyOwner {
        _setSubId(newSubId);
    }

    function setKeyHash(bytes32 newKeyHash) external onlyOwner {
        _setKeyHash(newKeyHash);
    }

    function setReqConfirmations(uint16 newReqConfirmations) external onlyOwner {
        _setReqConfirmations(newReqConfirmations);
    }

    function requestRandomNumbers(uint256[] memory tokenIds) external onlyOwner {
        _requestRandomNumbers(tokenIds);
    }

    function distributeRandomCrayons(uint256[] memory _tokenIds) external onlyOwner {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            uint256 _tokenId = _tokenIds[i];
            require(_tokenId != 0, "CrayonShopV1#distributeRandomCrayons: NO_TOKEN_ID_MAPPING");
            uint256 randomNum = _randomNumForTokens[_tokenId];
            require(randomNum > 1, "CrayonShopV1#distributeRandomCrayons: RANDOM_NUM_NOT_SET"); // TODO: Uncomment this before deployment
            _distributeRandomCrayonsToTokenId(_tokenId, randomNum);
        }
    }

    function _requestRandomNumbers(uint256[] memory _tokenIds) internal {
        uint256 _tokenIdsLen = _tokenIds.length;
        require(
            _tokenIdsLen <= maxTokenIdsPerRandReq,
            "CrayonShopv1#requestRandomNumbers: TOO_MANY_TOKENIDS"
        );
        for (uint256 i = 0; i < _tokenIdsLen; i++) {
            uint256 _tokenId = _tokenIds[i];
            require(
                _randomNumForTokens[_tokenId] < 2,
                "CrayonShopv1#_requestRandomNumbers: RANDOM_NUM_ALREADY_REQUESTED"
            );
            require(
                _numColorsOfZilla[_tokenId] > 0,
                "CrayonShopv1#requestRandomNumbers: CRAYONS_NOT_YET_CLAIMABLE"
            );
        }
        uint256 _requestId = _requestRandomWords(uint32(_tokenIdsLen), callbackGasLimit);
        _tokenIdRandRequests[_requestId] = _tokenIds;
    }

    function _setRandomNumbers(uint256[] memory _tokenIds, uint256[] memory randomNums) internal {
        uint256 _len = _tokenIds.length;
        require(_len == randomNums.length, "CrayonShopV1#_setRandomNumbers: PARAM_LENGTH_MISMATCH");
        for (uint256 i = 0; i < _len; i++) {
            _randomNumForTokens[_tokenIds[i]] = randomNums[i];
        }
    }

    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords)
        internal
        virtual
        override
    {
        uint256[] memory tokenIds = _tokenIdRandRequests[requestId];
        _setRandomNumbers(tokenIds, randomWords);
    }

    function _distributeRandomCrayonsToTokenId(uint256 _tokenId, uint256 randomNum) internal {
        uint256 _totalNumColors = _numColorsOfZilla[_tokenId];
        uint256 _numColors = _numColorsOfZilla[_tokenId];
        uint256 _numCrayonIds = _crayonStorage.numCrayonTypes();
        require(!_claimedTokenIds[_tokenId], "CrayonShopv1#distributeRandomCrayons: CLAIMED");
        _claimedTokenIds[_tokenId] = true;
        // each index represents the number of a crayonId
        // OG crayonIds are indexed 0 - 11
        uint256[] memory _crayonAmounts = new uint256[](_numCrayonIds);

        // check if the user has a gold crayon
        if (_tokenId <= 1733) {
            _crayonAmounts[11] = 1;
            _totalNumColors++;
        }

        uint256 _randomNum;

        // attempt to get cc crayon
        _randomNum = randomNum % 1000; // get right-most 3 digits
        if (_randomNum < 4 || isCoolCatsZilla(_tokenId)) {
            // 0.4% chance
            _crayonAmounts[9] = 1;
            _numColors--;
        }
        randomNum = randomNum / 1000;

        // attempt to get wp crayon
        _randomNum = randomNum % 1000;
        if (_randomNum < 4 || isWonderpalsZilla(_tokenId)) {
            _crayonAmounts[10] = 1;
            _numColors--;
        }
        randomNum = randomNum / 1000;

        for (uint256 i = 0; i < _numColors; i++) {
            // for each loop, get random number from [0 to 8]
            _randomNum = randomNum % 9;
            _crayonAmounts[_randomNum]++;
            randomNum = randomNum / 10;
        }

        address _owner = _colorableOwnershipManager.ownerOf(_rootChainCollection, _tokenId);
        require(_owner != address(0x0), "NO_OWNER");

        uint256[] memory _ids = new uint256[](_totalNumColors);
        uint256[] memory _amounts = new uint256[](_totalNumColors);
        uint256 j = 0;

        for (uint256 i = 0; i < _numCrayonIds; i++) {
            if (_crayonAmounts[i] > 0) {
                _ids[j] = i;
                _amounts[j] = _crayonAmounts[i];
                j++;
            }
        }

        _crayonStorage.mintBatch(_owner, _ids, _amounts, "0x0");
    }

    // publi view methods
    function claimedTokenIds(uint256 _tokenId) external view returns (bool) {
        return _claimedTokenIds[_tokenId];
    }

    function tokenIdRandRequests(uint256 _requestId) external view returns (uint256[] memory) {
        return _tokenIdRandRequests[_requestId];
    }

    function randomNumForTokens(uint256 _tokenId) public view returns (uint256) {
        return _randomNumForTokens[_tokenId];
    }

    function colorableOwnershipManager() public view returns (address) {
        return address(_colorableOwnershipManager);
    }

    function crayonStorage() public view returns (address) {
        return address(_crayonStorage);
    }

    function rootChainCollection() public view returns (address) {
        return address(_rootChainCollection);
    }

    function getNumColorsOfZilla(uint256 tokenId) public view returns (uint256) {
        return _numColorsOfZilla[tokenId];
    }

    function isWonderpalsZilla(uint256 tokenId) public view returns (bool) {
        return _wonderpalZillas[tokenId];
    }

    function isCoolCatsZilla(uint256 tokenId) public view returns (bool) {
        return _coolCatZillas[tokenId];
    }
}

