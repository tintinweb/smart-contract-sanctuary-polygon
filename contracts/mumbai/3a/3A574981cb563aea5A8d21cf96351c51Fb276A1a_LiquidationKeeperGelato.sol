pragma solidity ^0.8.9;
import "@chainlink/contracts/src/v0.8/interfaces/AutomationCompatibleInterface.sol";
import "./../interfaces/ID4X.sol";

import "./../MixinResolver.sol";
import "hardhat/console.sol";

/**
 * @title Liquidation Keeper
 *
 * This contract gets all positions to be liquidated and then
 * liquidates them automatically using Chainlink Upkeep (see https://docs.chain.link/
 * chainlink-automation/introduction/)
 * This contract contains logic allow Chainlink Automation to
 * determine when to execute smart contract functions.
 * This contract is compatible with the
 * AutomationCompatibleInterface contract and its functions.
 *
 **/
contract LiquidationKeeperGelato is MixinResolver {
    ID4X _d4x;

    bytes32 private constant D4X_CONTRACT = "D4X";

    constructor(address resolver) MixinResolver(resolver) {}

    /**
     * @notice to set the addresses required for the contract
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function resolverAddressesRequired()
        public
        pure
        override
        returns (bytes32[] memory addresses)
    {
        addresses = new bytes32[](1);
        addresses[0] = D4X_CONTRACT;
    }

    /**
     * @dev This function is called when rebuildCache or rebuildCaches functions called
     */
    function _resolveAddressesFinish() internal override {
        _d4x = ID4X(requireAndGetAddress(D4X_CONTRACT));
    }

    /**
     * @notice This function retrieves all positions that need to be
     * liquidated, executed by the keeper
     *
     * @return upkeepNeeded boolean to indicate whether the keeper should call
     * performUpkeep or not.
     * @return performData Array of positions that need to be liquidated in bytes that the
     * keeper should call performUpkeep with
     *
     * @dev This function contains the logic that will be executed
     * off-chain by Chainlink Upkeep to see if performUpkeep should be executed.
     */
    function checkUpkeep()
        external
        view
        returns (bool upkeepNeeded, bytes memory performData)
    {
        uint256[] memory positions = _d4x.getLiquidationPositions();

        return (positions.length > 0, abi.encode(positions));
    }

    /**
     * @notice This function liquidates all positions
     * @dev This function will be executed by Chainlink Upkeep
     * when checkUpkeep function retunrs true.
     */
    function performUpkeep(uint256[] memory positions) external {
        //  uint256[] memory positions = abi.decode(performData, (uint256[]));

        bytes[] memory empty = new bytes[](positions.length);

        _d4x.liquidation(positions, empty);
    }
}

