contract RandomSelectionUpgradeable is Initializable, BusinessAddressesUpgradeable, CollectionUpgradeable {
    using EnumerableSetUpgradeable for *;
    event Deposit(address collection, uint256[] tokenIds);

    EnumerableSetUpgradeable.AddressSet internal _assets;
    mapping(address => EnumerableSetUpgradeable.UintSet) internal _tokenIds;

    function _selectRandomElementsAndSend(address asset_, uint256 k_, address recipient_) internal {
        uint256 length;
        uint256 seed;
        uint256 tokenIndex;
        address treasury = address(this);

        unchecked {
            seed = block.timestamp + block.basefee + block.gaslimit + block.prevrandao;
        }

        if (asset_ == address(0)) {
            address asset;
            for (uint256 i = 0; i < k_; ) {
                // random collection
                length = _assets.length();
                asset = _assets.at(_getRandomNumber(seed + i, length));

                // random tokenId
                length = _tokenIds[asset].length();
                if (length == 1) {
                    _assets.remove(asset);
                    tokenIndex = 0;
                } else {
                    tokenIndex = _getRandomNumber(seed + i, length);
                }

                _safeTransferFromIndex(asset, treasury, recipient_, tokenIndex);

                unchecked {
                    ++i;
                }
            }
        } else {
            for (uint256 i = 0; i < k_; ) {
                tokenIndex = _getRandomNumber(seed + i, _tokenIds[asset_].length());
                _safeTransferFromIndex(asset_, treasury, recipient_, tokenIndex);
                unchecked {
                    ++i;
                }
            }
        }
    }

    function _safeTransferFromIndex(address asset_, address from_, address recipient_, uint256 tokenIndex_) internal {
        uint256 tokenId = _tokenIds[asset_].at(tokenIndex_);
        _tokenIds[asset_].remove(tokenId);
        SafeTransferLib.safeTransferFrom(asset_, from_, recipient_, tokenId);
    }

    function _getRandomNumber(uint256 seed_, uint256 upper_) private pure returns (uint256) {
        uint256 seed = uint256(keccak256(abi.encodePacked(seed_ + upper_)));
        return seed % upper_;
    }

    function deposit(address collection_, uint256[] calldata tokenIds) external {
        address operator = msg.sender;
        address receiver = address(this);
        _onlyBusiness(operator);
        _onlyAccepted(collection_);
        _assets.add(collection_);

        uint256 length = tokenIds.length;
        for (uint i = 0; i < length; ) {
            _tokenIds[collection_].add(tokenIds[i]);
            SafeTransferLib.safeTransferFrom(collection_, operator, receiver, tokenIds[i]);
            unchecked {
                ++i;
            }
        }

        emit Deposit(collection_, tokenIds);
    }

