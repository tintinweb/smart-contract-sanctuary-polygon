import { OPERATOR_ROLE, HUNDER_PERCENT_IN_BPS } from "../libraries/Constants.sol";

contract FeeSplitterUpgradeable is IFeeSplitterUpgradeable, BaseUpgradeable {
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;

    EnumerableSetUpgradeable.AddressSet internal _feeRecipients;
    mapping(address => uint256) internal _percents;

    function __FeeSplitterUpgradeable_init(
        address[] calldata recipients_,
        uint256[] calldata percents_
    ) internal onlyInitializing {
        __FeeSplitterUpgradeable_init_unchained(recipients_, percents_);
    }

    function __FeeSplitterUpgradeable_init_unchained(
        address[] calldata recipients_,
        uint256[] calldata percents_
    ) internal onlyInitializing {
        _configFees(recipients_, percents_);
    }

    function configFees(
        address[] calldata recipients_,
        uint256[] calldata percents_
    ) external override onlyRole(OPERATOR_ROLE) {
        _configFees(recipients_, percents_);
    }

    function _slittingFee(address token_, uint256 amount_) internal {
        uint256 length = _feeRecipients.length();
        uint256 transferAmount;
        address recipient;
        // address sender = _msgSender();

        for (uint256 i = 0; i < length; ) {
            recipient = _feeRecipients.at(i);
            transferAmount = (amount_ * _percents[recipient]) / HUNDER_PERCENT_IN_BPS;
            if (token_ == address(0)) {
                SafeTransferLib.safeTransferETH(recipient, transferAmount);
            }
            // else {
            //     SafeTransferLib.safeTransferFrom(token_, sender, recipient, transferAmount);
            // }
            unchecked {
                ++i;
            }
        }
    }

    function _configFees(address[] calldata recipients_, uint256[] calldata percents_) internal {
        uint256 length = recipients_.length;
        if (percents_.length != length) revert LengthMisMatch();

        for (uint256 i = 0; i < length; ) {
            if (recipients_[i] == address(0)) revert InvalidRecipient();
            if (percents_[i] != 0) {
                _feeRecipients.add(recipients_[i]); // whether exists or not
                _percents[recipients_[i]] = percents_[i];
            } else {
                _feeRecipients.remove(recipients_[i]);
            }

            unchecked {
                i++;
            }
        }

        emit FeeUpdated();
    }

    function viewFees() external view override returns (address[] memory, uint256[] memory fees) {
        uint256 length = _feeRecipients.length();
        fees = new uint256[](length);

        for (uint256 i = 0; i < length; ) {
            fees[i] = _percents[_feeRecipients.at(i)];
            unchecked {
                ++i;
            }
        }
        return (_feeRecipients.values(), fees);
    }
}

