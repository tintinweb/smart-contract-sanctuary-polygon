import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol";
import "@openzeppelin/contracts/interfaces/IERC2981.sol";
import "@openzeppelin/contracts/interfaces/IERC1271.sol";
import "@openzeppelin/contracts/utils/cryptography/SignatureChecker.sol";
import "@openzeppelin/contracts/utils/Address.sol";

import "hardhat/console.sol";

contract FanpageMarketplaceV3 is ReentrancyGuard, Pausable, FanpageAccessControl, ERC1155Holder {
    using SafeMath for uint256;

    bytes4 private constant _ERC2981InterfaceId = 0x2a55205a;
    bytes4 private constant _ERC1271MagicValue = 0x1626ba7e;
    address private nftContract;

    event TokenListed(
        uint256 indexed saleId,
        uint256 indexed tokenId,
        uint256 amount,
        address seller,
        uint256 price
    );

    event TokenDeListed(
        uint256 indexed saleId,
        uint256 indexed tokenId
    );

    event TokenPurchased(
        uint256 indexed saleId,
        uint256 indexed tokenId,
        uint256 amount,
        address buyer,
        address seller,
        uint256 price
    );

    event TokenClaimed(
        address from,
        address to,
        uint256 indexed tokenId,
        string serial,
        uint256[] quantities,
        uint256[] usdPrices
    );

    event TokenTransferred(
        address from,
        address to,
        uint256 indexed tokenId,
        string serial,
        uint256 quantity,
        uint256 usdPrice
    );

    mapping(uint256 => SaleOffer) private _saleOffersMap;

    constructor(address _nftContract) {
        nftContract = _nftContract;
    }

    /////////////////
    // Sale Offer ///
    /////////////////

    struct SaleOffer {
        uint saleId;
        uint256 tokenId;
        uint256 amount;
        address payable seller;
        uint256 price;
    }

    /* Places an item for sale on the marketplace, by creating a Sale Offer and mapping it to active offers */
    function createFixedPriceSaleOffer(
        uint256 _saleId,
        uint256 _tokenId,
        uint256 _amount,
        uint256 _price,
        bytes memory _data
    ) public nonReentrant whenNotPaused
    {
        require(_saleId != 0, "SaleID should be valid");
        require(_saleOffersMap[_saleId].tokenId == 0, "SaleID already exists");
        require(_price > 0, "Price must be at least 1 wei");

        _saleOffersMap[_saleId] = SaleOffer(
        _saleId,
        _tokenId,
        _amount,
        payable(msg.sender),
        _price
        );

        IERC1155(nftContract).safeTransferFrom(msg.sender, address(this), _tokenId, _amount, _data);
        emit TokenListed(_saleId, _tokenId, _amount, msg.sender, _price);
    }

    /* Claim token for buyers, once purchase is complete  */
    function claimToken(
        address _from,
        address _signer,
        uint256 _tokenId,
        bytes32 hash,
        bytes memory _signature,
        bytes32 _serial,
        StorageLibrary.Ownership[] memory _ownershipCost
    ) public nonReentrant onlyClaimsAdmin whenNotPaused
    {
        require(_tokenId != 0, "TokenID should be valid");
        require(_ownershipCost.length > 0, "Quantity of claim invalid");
        require(_from != address(0), "From Address must be valid");
        require(_signer != address(0), "Recipient must be valid");

        //serial number check
        require(FanpageCollectibleFacetV3(nftContract).checkSerial(_tokenId, _serial), "Serial number mismatch");

        if (Address.isContract(_signer)) {
            require(IERC1271(_signer).isValidSignature(hash, _signature) == _ERC1271MagicValue, "Account and Signature mismatch");
        } else {
            require(SignatureChecker.isValidSignatureNow(_signer, hash, _signature), "Account and Signature mismatch");
        }

        //register cost+quantity in chain
        FanpageCollectibleFacetV3(nftContract).registerOwnershipValue(_from, _signer, _tokenId, _serial, _ownershipCost);

        //get full volume of tokens
        uint256 fullQuantity = 0;
        uint256[] memory quantities = new uint256[](_ownershipCost.length);
        uint256[] memory usdPrices = new uint256[](_ownershipCost.length);
        for (uint256 i = 0; i < _ownershipCost.length; i++) {
            fullQuantity += _ownershipCost[i].quantity;
            quantities[i] = _ownershipCost[i].quantity;
            usdPrices[i] = _ownershipCost[i].usdPrice;
        }

        IERC1155(nftContract).safeTransferFrom(_from, _signer, _tokenId, fullQuantity, _signature);
        emit TokenClaimed(_from, _signer, _tokenId, string(abi.encodePacked(_serial)), quantities, usdPrices);
    }

    /* Transfer token in secondary routes, once initial purchase/claim is complete  */
    function transferToken(
        address _recipient,
        uint256 _tokenId,
        uint256 _quantity,
        bytes32 _serial,
        bytes memory _data
    ) public nonReentrant whenNotPaused
    {
        address _from = msg.sender;
        require(_tokenId != 0, "TokenID should be valid");
        require(_recipient != address(0), "Recipient must be valid");
        require(_recipient != _from, "Recipient cannot be sender");

        //serial number check
        require(FanpageCollectibleFacetV3(nftContract).checkSerial(_tokenId, _serial), "Serial number mismatch");
        //validate balance and ownership
        uint256 balanceOfAccount = IERC1155(nftContract).balanceOf(_from, _tokenId);
        require(balanceOfAccount != 0, "Only the owner can transfer assets");
        require(balanceOfAccount >= _quantity, "Sender does not own required quantities for transfer");

        //remove ownership
        FanpageCollectibleFacetV3(nftContract).deregisterOwnership(_from, _tokenId, _quantity, _serial);
        IERC1155(nftContract).safeTransferFrom(_from, _recipient, _tokenId, _quantity, _data);
        emit TokenTransferred(_from, _recipient, _tokenId, string(abi.encodePacked(_serial)), _quantity, 0);
    }

    /* Delist an active Token from Sales list */
    function delistFixedPriceSaleOffer(
        uint256 _saleId,
        bytes memory _data
    ) public nonReentrant whenNotPaused {
        require(_saleOffersMap[_saleId].saleId != 0, "Sale Offer not found");

        address seller = _saleOffersMap[_saleId].seller;
        require(msg.sender == seller, "Only the seller can delist an Sale Offer");

        IERC1155(nftContract).safeTransferFrom(
            address(this),
            msg.sender,
            _saleOffersMap[_saleId].tokenId,
            _saleOffersMap[_saleId].amount,
            _data);

        delete _saleOffersMap[_saleId];
        emit TokenDeListed(_saleId, _saleOffersMap[_saleId].tokenId);
    }

    /* Get all the Sale Offers listed in the Contract */
    function getSaleOffer(uint256 _saleId) public view returns (SaleOffer memory) {
        require(_saleOffersMap[_saleId].saleId != 0, "Sale Offer not found");

        SaleOffer memory offer = _saleOffersMap[_saleId];
        return offer;
    }

    /* Purchase an item listed for sale in the marketplace*/
    /* Transfers ownership of the item, as well as funds between parties */
    function closeFixedPriceSaleOffer(
        uint256 _saleId,
        bytes memory _data
    ) public payable nonReentrant whenNotPaused {
        require(_saleOffersMap[_saleId].saleId != 0, "Sale Offer not found");

        uint price = _saleOffersMap[_saleId].price;
        require(msg.value == price, "Value does not match the listing price");

        if (ERC165(nftContract).supportsInterface(_ERC2981InterfaceId)) {
            (address receiver, uint256 royaltyAmount) = IERC2981(nftContract).royaltyInfo(_saleId, msg.value);
            address payable receiverWallet = payable(receiver);
            receiverWallet.transfer(royaltyAmount);
            _saleOffersMap[_saleId].seller.transfer(msg.value - royaltyAmount);
        } else {
            _saleOffersMap[_saleId].seller.transfer(msg.value);
        }

        IERC1155(nftContract).safeTransferFrom(
            address(this),
            msg.sender,
            _saleOffersMap[_saleId].tokenId,
            _saleOffersMap[_saleId].amount,
            _data);

        delete _saleOffersMap[_saleId];

        emit TokenPurchased(
            _saleId,
            _saleOffersMap[_saleId].tokenId,
            _saleOffersMap[_saleId].amount,
            msg.sender,
            _saleOffersMap[_saleId].seller, price
        );
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(AccessControl, ERC1155Receiver) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    // Admins can pause the Contracts for an emergency
    function pause() public onlyFanPageAdmin {
        _pause();
    }

    // Admins can unpause the Contracts after an emergency
    function unpause() public onlyFanPageAdmin {
        _unpause();
    }
}

// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/AccessControl.sol";

contract FanpageAccessControl is AccessControl {
    bytes32 public constant MINTER_ROLE = keccak256("FANPAGE_MINTER");
    bytes32 public constant CLAIMS_ADMIN_ROLE = keccak256("FANPAGE_CLAIMS_ADMIN");
    bytes32 public constant FANPAGE_ADMIN_ROLE = keccak256("FANPAGE_ADMIN");

    constructor () {
        // Contract Owner is set as the Admin, Minter, Claim admin & fanpage admin
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(MINTER_ROLE, msg.sender);
        _setupRole(CLAIMS_ADMIN_ROLE, msg.sender);
        _setupRole(FANPAGE_ADMIN_ROLE, msg.sender);
    }

    /// Restricted to members of the admin role
    modifier onlyAdmin() {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "Sender does not have Admin role");
        _;
    }

    /// Restricted to members of the minter role
    modifier onlyMinter() {
        require(hasRole(MINTER_ROLE, msg.sender), "Sender does not have Minter role");
        _;
    }

    /// Restricted to members of the Claim admin role
    modifier onlyClaimsAdmin() {
        require(hasRole(CLAIMS_ADMIN_ROLE, msg.sender), "Sender does not have Claims Admin role");
        _;
    }

    /// Restricted to members of the fanpage admin role
    modifier onlyFanPageAdmin() {
        require(hasRole(FANPAGE_ADMIN_ROLE, msg.sender), "Sender does not have fanpage Admin role");
        _;
    }
}

