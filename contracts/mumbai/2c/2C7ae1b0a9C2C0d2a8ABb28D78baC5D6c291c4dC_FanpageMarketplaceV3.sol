pragma abicoder v2;
pragma solidity ^0.8.9;

import {StorageLibrary} from "../libraries/StorageLibrary.sol";

import "../../FanpageAccessControl.sol";

import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Burnable.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Pausable.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/interfaces/IERC2981.sol";

import "hardhat/console.sol";
import {ERC165Storage} from "@solidstate/contracts/introspection/ERC165.sol";

contract FanpageCollectibleFacetV3 is
    Context,
    FanpageAccessControl,
    ERC1155Burnable,
    ERC1155Pausable,
    IERC2981
{
    using ERC165Storage for ERC165Storage.Layout;
    address immutable contractOwner = msg.sender;

    // Emitted on every mint
    event Minted(
        address recipient,
        uint256 tokenId,
        string serial,
        string tokenUri,
        uint256 quantity,
        address royaltyRecipient,
        uint32 royaltyPercentage
    );

    constructor(string memory _name, string memory _uri) ERC1155(_uri) {
        StorageLibrary.setName(_name);

        StorageLibrary.setName("FanpageTest123");
    }

    function initAccess() external {
        // Contract Owner is set as the Admin, Minter, Claim admin & fanpage admin
        _setupRole(DEFAULT_ADMIN_ROLE, contractOwner);
        _setupRole(MINTER_ROLE, contractOwner);
        _setupRole(CLAIMS_ADMIN_ROLE, contractOwner);
        _setupRole(FANPAGE_ADMIN_ROLE, contractOwner);

        ERC165Storage.layout().setSupportedInterface(
            type(IERC165).interfaceId,
            true
        );
        ERC165Storage.layout().setSupportedInterface(
            type(IERC1155).interfaceId,
            true
        );

        ERC165Storage.layout().setSupportedInterface(
            type(IERC2981).interfaceId,
            true
        );
    }

    function uri(
        uint256 _tokenId
    ) public view override returns (string memory) {
        string memory _tokenURI = StorageLibrary.getURI(_tokenId);

        // return token specific URI if available; else return base URI
        if (bytes(_tokenURI).length > 0) {
            return _tokenURI;
        }
        return super.uri(_tokenId);
    }

    function mintAndDeposit(
        address _recipient,
        uint256 _tokenId,
        string memory _tokenURI,
        uint256 _quantity,
        bytes memory _data,
        address _royaltyRecipient,
        uint32 _royaltyPercentage,
        bytes32 _serial
    ) external onlyMinter whenNotPaused {
        require(_tokenId != 0, "Token ID should be valid");
        require(_royaltyPercentage <= 10, "Royalty cannot be greater than 10%");
        require(
            _royaltyPercentage == 0 ||
                _royaltyRecipient == address(_royaltyRecipient),
            "Recipient has to be valid if royalty is greater than 0"
        );

        _mint(_recipient, _tokenId, _quantity, _data);
        StorageLibrary.setTokenURL(_tokenId, _tokenURI);
        StorageLibrary.setSerialNumber(_tokenId, _serial);
        if (_royaltyPercentage != 0) {
            StorageLibrary.setRoyaltyInfo(
                _tokenId,
                _recipient,
                _royaltyPercentage
            );
        }
        emit Minted(
            _recipient,
            _tokenId,
            string(abi.encodePacked(_serial)),
            _tokenURI,
            _quantity,
            _royaltyRecipient,
            _royaltyPercentage
        );
    }

    function checkSerial(
        uint256 _tokenId,
        bytes32 _serial
    ) public view virtual returns (bool) {
        require(_tokenId != 0, "Token ID should be valid");
        require(_serial != 0, "Serial should be valid");

        // return true if serial available; else return false
        if (StorageLibrary.getSerialNumber(_tokenId) == _serial) {
            return true;
        }
        return false;
    }

    function royaltyInfo(
        uint256 _tokenId,
        uint256 _salePrice
    ) external view returns (address receiver, uint256 royaltyAmount) {
        uint256 percentage = StorageLibrary.getRoyaltyPercentage(_tokenId);
        address recipient = StorageLibrary.getRoyaltyRecipient(_tokenId);

        if (percentage != 0) {
            return (recipient, (percentage * _salePrice) / 100);
        }
        return (address(0), 0);
    }

    function registerOwnershipValue(
        address _from,
        address _signer,
        uint256 _tokenId,
        bytes32 _serial,
        StorageLibrary.Ownership[] memory _ownership
    ) public onlyClaimsAdmin whenNotPaused {
        require(_tokenId != 0, "TokenID should be valid");
        require(_ownership.length > 0, "Quantity of claim invalid");
        require(_from != address(0), "From Address must be valid");
        require(_signer != address(0), "Recipient must be valid");

        require(checkSerial(_tokenId, _serial), "Serial number mismatch");
        StorageLibrary.setRegisterOwnership(_signer, _tokenId, _ownership);
    }

    function deregisterOwnership(
        address _from,
        uint256 _tokenId,
        uint256 _quantity,
        bytes32 _serial
    ) public whenNotPaused {
        require(_tokenId != 0, "TokenID should be valid");
        require(_from != address(0), "From Address must be valid");

        //serial number check
        require(checkSerial(_tokenId, _serial), "Serial number mismatch");
        require(
            _from == _msgSender() || isApprovedForAll(_from, _msgSender()),
            "FanPageCollectible: caller is not owner nor approved"
        );
        StorageLibrary.setDeregisterOwnership(_from, _tokenId, _quantity);
    }

    function ownershipCost(
        address account,
        uint256 _tokenId
    ) public view returns (StorageLibrary.Ownership[] memory) {
        require(_tokenId != 0, "TokenID should be valid");
        return StorageLibrary.getOwnershipCost(account, _tokenId);
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        virtual
        override(ERC1155, AccessControl, IERC165)
        returns (bool)
    {
        return
            interfaceId == type(IERC2981).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal virtual override(ERC1155, ERC1155Pausable) {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    /**
     * Override isApprovedForAll to auto-approve proxy contracts
     */
    function isApprovedForAll(
        address _owner,
        address _operator
    ) public view override returns (bool isOperator) {
        // if Proxy Address is detected, auto-return true
        // otherwise, use the default ERC1155.isApprovedForAll()
        return ERC1155.isApprovedForAll(_owner, _operator);
    }

    // Admins can pause the Contracts for an emergency
    function pause() public onlyFanPageAdmin {
        _pause();
    }

    // Admins can unpause the Contracts after an emergency
    function unpause() public onlyFanPageAdmin {
        _unpause();
    }
}

