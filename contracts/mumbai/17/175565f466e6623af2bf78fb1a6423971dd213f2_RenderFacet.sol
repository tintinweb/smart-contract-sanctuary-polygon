                    dirtbike.forks,
                    '" />'

                    // <!-- handlebars -->
                    '<rect x="900" y="500" width="100" height="200" fill="',
                    dirtbike.handlebars,
                    '" />'
                    '<rect x="800" y="500" width="200" height="100" fill="',
                    dirtbike.handlebars,
                    '" />'

                )
            );
    }

