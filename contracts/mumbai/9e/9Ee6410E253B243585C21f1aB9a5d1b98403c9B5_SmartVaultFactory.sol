     */
    function getRoleAdmin(bytes32 role) external view override returns (bytes32) {
        return _getRoleAdmin[role];
    }

    /*
     *  @notice         Grants multiple roles to an account, if not previously granted.
     *  @dev            Caller must have admin role for the `role`.
     *                  Emits {RoleGranted Event}.
     *
     *  @param role     keccak256 hash of the role. e.g. keccak256("TRANSFER_ROLE")
     *                  Note: you can't modify UNLOAD_ROLE and TRANSFER_ROLE. These will always 
     *                  be present for any given account.
     *  @param account  Address of the account to which the role is being granted.
     */
    function modifyRoles(bytes32[] memory roles, address account) public virtual onlyRole(DEFAULT_ADMIN_ROLE) {
        
        require(roles.length > 0, "Roles is empty. Assign at least one role");

        bool lockRoleFound = false;
        bool loadRoleFound = false;
        bool minterRoleFound = false;

        // Check which roles are present in the array
        for(uint256 i = 0; i < roles.length; i++) {
            if(roles[i] == keccak256("LOCK_ROLE")) lockRoleFound = true;
            if(roles[i] == keccak256("LOAD_ROLE")) loadRoleFound = true;
            if(roles[i] == keccak256("MINTER_ROLE")) minterRoleFound = true;
        }

        for(uint256 i = 0; i < roles.length; i++) {
            require(roles[i] != keccak256("NOT_UPGRADEABLE"));
            if(roles[i] == keccak256("MINTER_ROLE")) require(account != address(0));
            if(roles[i] != DEFAULT_ADMIN_ROLE) {
                if(coAllocatedSmartVault) {
                    _checkRole(_getRoleAdmin[roles[i]], msg.sender);
                    _setupRole(roles[i], account);
                } else {
                    revert();
                }
            }
        }

        // Revoke roles that are not present in the array
        if(lockRoleFound == false && hasRole(keccak256("LOCK_ROLE"), account)) {
            revokeRole(keccak256("LOCK_ROLE"), account);
        }
            
        if(loadRoleFound == false && hasRole(keccak256("LOAD_ROLE"), account)) {
            revokeRole(keccak256("LOAD_ROLE"), account);
        }
            
        if(minterRoleFound == false && hasRole(keccak256("MINTER_ROLE"), account)) {
            revokeRole(keccak256("MINTER_ROLE"), account);
        }

        // Always assign UNLOAD_ROLE and TRANSFER_ROLE if not present already
        if(!hasRole(keccak256("UNLOAD_ROLE"), account)) {
            _setupRole(keccak256("UNLOAD_ROLE"), account);
        }

        if(!hasRole(keccak256("TRANSFER_ROLE"), account)) {
            _setupRole(keccak256("TRANSFER_ROLE"), account);
        }

    }

    /**
     *  @notice         Revokes role from an account.
     *  @dev            Caller must have admin role for the `role`.
     *                  Emits {RoleRevoked Event}.
     *
     *  @param role     keccak256 hash of the role. e.g. keccak256("TRANSFER_ROLE")
     *  @param account  Address of the account from which the role is being revoked.
     */
    function revokeRole(bytes32 role, address account) public virtual override {
        _checkRole(_getRoleAdmin[role], msg.sender);
        _revokeRole(role, account);
    }

    /**
     *  @notice         Revokes role from the account.
     *  @dev            Caller must have the `role`, with caller being the same as `account`.
     *                  Emits {RoleRevoked Event}.
     *
     *  @param role     keccak256 hash of the role. e.g. keccak256("TRANSFER_ROLE")
     *  @param account  Address of the account from which the role is being revoked.
     */
    function renounceRole(bytes32 role, address account) public virtual override {
        if (msg.sender != account) {
            revert();
        }
        _revokeRole(role, account);
    }

    /// @dev Sets `adminRole` as `role`'s admin role.
    function _setRoleAdmin(bytes32 role, bytes32 adminRole) internal virtual {
        bytes32 previousAdminRole = _getRoleAdmin[role];
        _getRoleAdmin[role] = adminRole;
        emit RoleAdminChanged(role, previousAdminRole, adminRole);
    }

    /// @dev Checks `role` for `account`. Reverts with a message including the required role.
    function _checkRole(bytes32 role, address account) internal view virtual {
        if (!_hasRole[role][account]) {
            revert(
/*                 string(
                    abi.encodePacked(
                        "Permissions: account ",
                        TWStrings.toHexString(uint160(account), 20),
                        " is missing role ",
                        TWStrings.toHexString(uint256(role), 32)
                    )
                ) */
            );
        }
    }

    /// @dev Checks `role` for `account`. Reverts with a message including the required role.
    function _checkRoleWithSwitch(bytes32 role, address account) internal view virtual {
        if (!hasRoleWithSwitch(role, account)) {
            revert(
/*                 string(
                    abi.encodePacked(
                        "Permissions: account ",
                        TWStrings.toHexString(uint160(account), 20),
                        " is missing role ",
                        TWStrings.toHexString(uint256(role), 32)
                    )
                ) */
            );
        }
    }
}

