        _setupRole(LOAD_ROLE, _defaultAdmin);
        _setupRole(UNLOAD_ROLE, _defaultAdmin);
        _setupRole(LOCK_ROLE, _defaultAdmin);

        // note: see `_beforeTokenTransfer` for TRANSFER_ROLE behaviour.
        _setupRole(TRANSFER_ROLE, address(0));

        // note: see `onlyRoleWithSwitch` for ASSET_ROLE behaviour.
        _setupRole(ASSET_ROLE, address(0));

        // Set the Vault type depending on user input.
        coAllocatedSmartVault = _coAllocatedSmartVault;
        factory = ISmartVaultFactory(_createdByFactory);
        
        emit SmartVaultCreated(
            _msgSender(),
            _name,
            _symbol,
            _coAllocatedSmartVault
        );
    }

    ///@dev required by the OZ UUPS module
    function _authorizeUpgrade(address)
        internal
        override
        onlyRoleWithSwitch(NOT_UPGRADEABLE)
    {}

    /*///////////////////////////////////////////////////////////////
                            Modifiers
    //////////////////////////////////////////////////////////////*/

    modifier onlyRoleWithSwitch(bytes32 role) {
        _checkRoleWithSwitch(role, _msgSender());
        _;
    }

    /*///////////////////////////////////////////////////////////////
                        Generic contract logic
    //////////////////////////////////////////////////////////////*/

    /// @dev Returns the type of the Smart Vault.
    function isCoAllocatedSmartVault() external view returns (bool) {
        return coAllocatedSmartVault;
    }

    /// @dev Returns the version of the contract.
    function contractVersion() external pure returns (uint8) {
        return uint8(VERSION);
    }

    /// @dev Lets the contract receive ether to unwrap native tokens.
    receive() external payable {
        require(msg.sender == nativeTokenWrapper);
    }

    /*///////////////////////////////////////////////////////////////
                        ERC 165 / 721 / 2981 logic
    //////////////////////////////////////////////////////////////*/

    /// @dev Returns the URI for a given tokenId.
    function tokenURI(uint256 _tokenId)
        public
        view
        override
        returns (string memory)
    {
        return getUriOfBundle(_tokenId);
    }

    /// @dev See ERC 165
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC1155ReceiverUpgradeable, ERC721EnumerableUpgradeable)
        returns (bool)
    {
        return
            super.supportsInterface(interfaceId) ||
            interfaceId == type(IERC721Upgradeable).interfaceId ||
            interfaceId == type(IERC1155ReceiverUpgradeable).interfaceId ||
            interfaceId == type(IERC2981Upgradeable).interfaceId;
    }

    /*///////////////////////////////////////////////////////////////
                    Minting logic
    //////////////////////////////////////////////////////////////*/

    /// @dev Mint an empty ERC721 NFT that can later wrap multiple ERC1155, ERC721, ERC20 tokens.
    function mintSmartNFT(address _recipient)
        public
        onlyRoleWithSwitch(MINTER_ROLE)
        returns (uint256 tokenId)
    {
        Token[] memory noTokens;
        tokenId = nextTokenIdToMint;

        if (coAllocatedSmartVault == true) {
            _safeMint(_recipient, tokenId);
            emit SmartNFTCreated(_msgSender(), _recipient, tokenId, noTokens);
            nextTokenIdToMint += 1;
        } else {
            if (nextTokenIdToMint == 0) {
                _safeMint(_recipient, tokenId);
                emit SmartNFTCreated(
                    _msgSender(),
                    _recipient,
                    tokenId,
                    noTokens
                );
                nextTokenIdToMint += 1;
            } else {
                revert();
            }
        }

        // If recipient is not the admin we must inform Factory contract for tracking
        if(!hasRole(DEFAULT_ADMIN_ROLE, _recipient)) {
            factory.addSmartVaultToNonAdmin(address(this), _recipient, tokenId);
        }
    }

    function transferOwnership(address _to) external onlyRoleWithSwitch(DEFAULT_ADMIN_ROLE) {
        
        // If fully allocated vault, use transfer smart NFT to transfer ownership
        require(coAllocatedSmartVault);

        bool isAdminOwner = false;
        
        // Check if admin holds smart NFTs
        uint[] memory tokenList = new uint[](totalSupply());
        uint256 j = 0;
        for (uint256 index = 0; index < totalSupply(); index++) {
            if(_isApprovedOrOwner(msg.sender, index)) {
                isAdminOwner = true;
                tokenList[j] = index;
                j += 1;
            }
        }
        
        // Gives roles to new admin
        _setupRole(DEFAULT_ADMIN_ROLE, _to);
        _setupRole(MINTER_ROLE, _to);
        _setupRole(TRANSFER_ROLE, _to);
        _setupRole(UNLOAD_ROLE, _to);
        _setupRole(LOAD_ROLE, _to);
        _setupRole(LOCK_ROLE, _to);
        
        // Revokes roles from old admin
        _revokeRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _revokeRole(MINTER_ROLE, msg.sender);
        _revokeRole(LOAD_ROLE, msg.sender);
        _revokeRole(LOCK_ROLE, msg.sender);
        
        // If admin is non-holder of smart NFTs, we revoke the roles UNLOAD_ROLE and TRANSFER_ROLE
        if(!isAdminOwner) {
            _revokeRole(TRANSFER_ROLE, msg.sender);
            _revokeRole(UNLOAD_ROLE, msg.sender);
        } else {
            // Inform factory about all tokens the old admin owns and keep roles
            for (uint256 index = 0; index < tokenList.length; index++) {
                factory.addSmartVaultToNonAdmin(address(this), msg.sender, tokenList[index]);   
            }
        }
        
        // Add new owner for the vault
        _setupOwner(_to);

        // Update Factory with new vault owner
        factory.transferSmartVaultOwnership(address(this), msg.sender, _to);

    }

    /*///////////////////////////////////////////////////////////////
                    Loading / Unloading logic
    //////////////////////////////////////////////////////////////*/

    /// @dev Load multiple ERC1155, ERC721, ERC20 tokens into a Smart NFT (bundle already created).
    function loadTokens(Token[] calldata _tokensToLoad, uint256 _smartNFTId)
        external
        payable
        nonReentrant
        onlyRoleWithSwitch(LOAD_ROLE)
    {
        require(_smartNFTId < nextTokenIdToMint);
        require(_isApprovedOrOwner(_msgSender(), _smartNFTId));

        for (uint256 i = 0; i < _tokensToLoad.length; i += 1) {
            require(_tokensToLoad[i].assetContract != address(this));
        }

        if (!hasRole(ASSET_ROLE, address(0))) {
            for (uint256 i = 0; i < _tokensToLoad.length; i += 1) {
                _checkRole(ASSET_ROLE, _tokensToLoad[i].assetContract);
            }
        }

        if (getTokenCountOfBundle(_smartNFTId) > 0) {
            for (uint256 i = 0; i < _tokensToLoad.length; i += 1) {
                _addTokenInBundle(_tokensToLoad[i], _smartNFTId);
                _transferToken(_msgSender(), address(this), _tokensToLoad[i]);
            }
        } else if (getTokenCountOfBundle(_smartNFTId) == 0) {
            _storeTokens(
                _msgSender(),
                _tokensToLoad,
                "bundleCreated",
                _smartNFTId
            );
        } else {
            revert();
        }

        emit TokensLoaded(_msgSender(), _smartNFTId, _tokensToLoad);
    }

    /// @dev Unload from Smart Vault to retrieve one underlying ERC1155, ERC721, ERC20 token.
    function unloadToken(
        uint256 _smartNFTId,
        Token calldata _tokenToRetrieve,
        address _recipient
    ) external nonReentrant onlyRoleWithSwitch(UNLOAD_ROLE) {
        require(_smartNFTId < nextTokenIdToMint);
        require(_isApprovedOrOwner(_msgSender(), _smartNFTId));

        _releaseToken(_recipient, _smartNFTId, _tokenToRetrieve);

        emit TokenUnloaded(
            _msgSender(),
            _recipient,
            _smartNFTId,
            _tokenToRetrieve
        );
    }

    /// @dev Unload from Smart Vault to retrieve all underlying ERC1155, ERC721, ERC20 tokens.
    function unloadAll(uint256 _smartNFTId, address _recipient)
        external
        nonReentrant
        onlyRoleWithSwitch(UNLOAD_ROLE)
    {
        require(_smartNFTId < nextTokenIdToMint);
        require(_isApprovedOrOwner(_msgSender(), _smartNFTId));

        _releaseTokens(_recipient, _smartNFTId);

        emit AllTokensUnloaded(_msgSender(), _recipient, _smartNFTId);
    }

    /*///////////////////////////////////////////////////////////////
                    Locking / Unlocking logic
    //////////////////////////////////////////////////////////////*/

    /// @dev Lock ERC1155, ERC721, ERC20 tokens into Smart NFT
    function lockToken(
        Token calldata _tokenToLock,
        uint256 _smartNFTId,
        uint256 lockedUntil
    ) external payable nonReentrant onlyRoleWithSwitch(LOCK_ROLE) {
        require(_smartNFTId < nextTokenIdToMint);
        require(getTokenCountOfBundle(_smartNFTId) > 0);

        if (!hasRole(ASSET_ROLE, address(0))) {
            _checkRole(ASSET_ROLE, _tokenToLock.assetContract);
        }

        uint256 indexForTokenInBundle = findTokenInBundle(
            _smartNFTId,
            _tokenToLock
        );

        require(indexForTokenInBundle != type(uint256).max);

        if(getTokenCountOfLock(_smartNFTId) > 0) {
            // Lock already exists for this smart nft, I can add tokens to it
            _addTokenInLock(_tokenToLock, _smartNFTId, lockedUntil);
        } else {
            // First lock of this smart nft
            _createTokenLock(_tokenToLock, _smartNFTId, lockedUntil);
        }

        emit TokenLocked(_tokenToLock, _smartNFTId, lockedUntil);
    }

    /*///////////////////////////////////////////////////////////////
                        Getter functions
    //////////////////////////////////////////////////////////////*/

    /// @dev Returns the underlying contents of a wrapped NFT.
    function getWrappedContents(uint256 _tokenId)
        external
        view
        returns (Token[] memory contents)
    {
        uint256 total = getTokenCountOfBundle(_tokenId);
        contents = new Token[](total);

        for (uint256 i = 0; i < total; i += 1) {
            contents[i] = getTokenOfBundle(_tokenId, i);
        }
    }

    /*///////////////////////////////////////////////////////////////
                        Internal functions
    //////////////////////////////////////////////////////////////*/

    /// @dev Returns whether owner can be set in the given execution context.
    function _canSetOwner() internal view override returns (bool) {
        return hasRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    /*///////////////////////////////////////////////////////////////
                        Miscellaneous
    //////////////////////////////////////////////////////////////*/

    /**
     * @dev See {ERC721-_beforeTokenTransfer}.
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal virtual override(ERC721EnumerableUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId, batchSize = 1);

        // if transfer is restricted on the contract, we still want to allow burning and minting
        if (
            !hasRole(TRANSFER_ROLE, address(0)) &&
            from != address(0) &&
            to != address(0)
        ) {
            require(hasRole(TRANSFER_ROLE, from) || hasRole(TRANSFER_ROLE, to));
        }

        if (!coAllocatedSmartVault && nextTokenIdToMint == 1) {
            // Sendint token to another address
            _setupRole(DEFAULT_ADMIN_ROLE, to);
            _setupRole(MINTER_ROLE, to);
            _setupRole(TRANSFER_ROLE, to);
            _setupRole(UNLOAD_ROLE, to);
            _setupRole(LOAD_ROLE, to);
            _setupRole(LOCK_ROLE, to);
            _revokeRole(DEFAULT_ADMIN_ROLE, from);
            _revokeRole(MINTER_ROLE, from);
            _revokeRole(TRANSFER_ROLE, from);
            _revokeRole(UNLOAD_ROLE, from);
            _revokeRole(LOAD_ROLE, from);
            _revokeRole(LOCK_ROLE, from);
            _setupOwner(to);
            // Update Factory with new vault owner
            factory.transferSmartVaultOwnership(address(this), from, to);
        } else {
            if (!coAllocatedSmartVault && nextTokenIdToMint == 0) {
                if(!hasRole(DEFAULT_ADMIN_ROLE, to)) {
                    // Minting directly to EOA: from is 0x - use owner()
                    _setupRole(DEFAULT_ADMIN_ROLE, to);
                    _setupRole(MINTER_ROLE, to);
                    _setupRole(TRANSFER_ROLE, to);
                    _setupRole(UNLOAD_ROLE, to);
                    _setupRole(LOAD_ROLE, to);
                    _setupRole(LOCK_ROLE, to);
                    _revokeRole(DEFAULT_ADMIN_ROLE, owner());
                    _revokeRole(MINTER_ROLE, owner());
                    _revokeRole(TRANSFER_ROLE, owner());
                    _revokeRole(UNLOAD_ROLE, owner());
                    _revokeRole(LOAD_ROLE, owner());
                    _revokeRole(LOCK_ROLE, owner());
                    // Update Factory with new vault owner
                    factory.transferSmartVaultOwnership(address(this), owner(), to);
                    _setupOwner(to);
                }
            } else {
                _setupRole(TRANSFER_ROLE, to);      
                _setupRole(UNLOAD_ROLE, to);
            }
        }

        // If recipient is not the admin we must inform Factory contract for tracking
        if(!hasRole(DEFAULT_ADMIN_ROLE, to)) {
            factory.addSmartVaultToNonAdmin(address(this), to, tokenId);
        }
    }

}