import "./interface/ITokenLock.sol";
import "../lib/CurrencyTransferLib.sol";

interface IERC165Lock {
    function supportsInterface(bytes4 interfaceId) external view returns (bool);
}

/**
 *  @title   Token Lock
 *  @notice  `TokenLock` contract extension allows locking of ERC20/ERC721/ERC1155 and native-token assets
 *           in a data structure, and provides logic for setting/getting IDs and info for locked assets.
 *  @dev     See {ITokenLock}
 */

abstract contract TokenLock is ITokenLock {
    /// @dev Mapping from bundle UID => lock info.
    mapping(uint256 => LockInfo) private lock;

    /// @dev Returns the total number of assets locked in a particular Smart Vault.
    function getTokenCountOfLock(uint256 _bundleId)
        public
        view
        returns (uint256)
    {
        return lock[_bundleId].count;
    }

    /// @dev Returns an asset contained in a particular lock, at a particular index.
    function getTokenOfLock(uint256 _bundleId, uint256 index)
        public
        view
        returns (LockedToken memory)
    {
        return lock[_bundleId].lockedTokens[index];
    }

    /// @dev Returns the timestamp until an asset, at a particular index, will be locked.
    function getLockedUntilOfTokenInLock(uint256 _bundleId, uint256 index)
        public
        view
        returns (uint256)
    {
        return lock[_bundleId].lockedTokens[index].lockedUntil;
    }

    /** @dev    Returns the index of an asset contained in a particular bundle.
     *  @notice If the asset is not found or it is locked, the index is not retrieved.
     */
    function findTokenInLock(
        uint256 _bundleId,
        ITokenBundle.Token memory _tokenToFind
    ) internal view returns (uint256 index) {
        uint256 count = getTokenCountOfLock(_bundleId);
        index = type(uint256).max;

        for (uint256 i = 0; i < count; i += 1) {
            if (
                lock[_bundleId].lockedTokens[i].lockedToken.assetContract ==
                _tokenToFind.assetContract &&
                lock[_bundleId].lockedTokens[i].lockedToken.tokenType ==
                _tokenToFind.tokenType &&
                lock[_bundleId].lockedTokens[i].lockedToken.tokenId ==
                _tokenToFind.tokenId &&
                lock[_bundleId].lockedTokens[i].lockedToken.totalAmount ==
                _tokenToFind.totalAmount
            ) index = i;
        }
    }

    /// @dev Lets the calling contract create a bundle, by passing in a list of tokens and a unique id.
    function _createTokenLock(
        ITokenBundle.Token memory _tokenToLock,
        uint256 _bundleId,
        uint256 lockUntil
    ) internal {
        require(lockUntil > 0);
        require(lockUntil > block.timestamp, "lockUntil must be greater than the current block height");
        require(lock[_bundleId].count == 0);

        _checkToken(_tokenToLock);
        lock[_bundleId].count = 1;
        lock[_bundleId].lockedTokens[0].lockedToken = _tokenToLock;
        lock[_bundleId].lockedTokens[0].lockedUntil = lockUntil;
       
    }

    /// @dev Lets the calling contract add a token to a bundle for a unique bundle id and index.
    function _addTokenInLock(
        ITokenBundle.Token memory _tokenToLock,
        uint256 _bundleId,
        uint256 _lockUntil
    ) internal {
        _checkToken(_tokenToLock);
        uint256 id = lock[_bundleId].count;

        LockedToken memory tokenToLock;
        tokenToLock.lockedToken = _tokenToLock;
        tokenToLock.lockedUntil = _lockUntil;

        lock[_bundleId].lockedTokens[id] = tokenToLock;
        lock[_bundleId].count += 1;
    }

    /// @dev Lets the calling contract remove a token from a unique bundle id.
    function _removeTokenFromLock(uint256 _bundleId, uint256 index) internal {
        delete lock[_bundleId].lockedTokens[index];
        uint256 count = getTokenCountOfLock(_bundleId);
        lock[_bundleId].count = count - 1;
    }

    /// @dev Lets the calling contract update a token in a bundle for a unique bundle id and index.
    function _updateTokenInLock(
        ITokenBundle.Token memory _tokenToBind,
        uint256 _bundleId,
        uint256 _index,
        uint256 _lockUntil
    ) internal {
        require(_index < lock[_bundleId].count);
        _checkToken(_tokenToBind);
        LockedToken memory tokenToLock;
        tokenToLock.lockedToken = _tokenToBind;
        tokenToLock.lockedUntil = _lockUntil;
        lock[_bundleId].lockedTokens[_index] = tokenToLock;
    }

    /// @dev Checks if the type of asset-contract is same as the TokenType specified.
    function _checkToken(ITokenBundle.Token memory _token) internal view {
        if (_token.tokenType == ITokenBundle.TokenType.ERC721) {
            try
                IERC165Lock(_token.assetContract).supportsInterface(0x80ac58cd)
            returns (bool supported721) {
                require(supported721);
            } catch {
                revert();
            }
        } else if (_token.tokenType == ITokenBundle.TokenType.ERC1155) {
            try
                IERC165Lock(_token.assetContract).supportsInterface(0xd9b67a26)
            returns (bool supported1155) {
                require(supported1155);
            } catch {
                revert();
            }
        } else if (_token.tokenType == ITokenBundle.TokenType.ERC20) {
            if (_token.assetContract != CurrencyTransferLib.NATIVE_TOKEN) {
                // 0x36372b07
                try
                    IERC165Lock(_token.assetContract).supportsInterface(
                        0x80ac58cd
                    )
                returns (bool supported721) {
                    require(!supported721);

                    try
                        IERC165Lock(_token.assetContract).supportsInterface(
                            0xd9b67a26
                        )
                    returns (bool supported1155) {
                        require(!supported1155);
                    } catch (bytes memory) {}
                } catch (bytes memory) {}
            }
        }
    }

    /// @dev Lets the calling contract delete a particular bundle.
    function _deleteLock(uint256 _bundleId) internal {
        for (uint256 i = 0; i < lock[_bundleId].count; i += 1) {
            delete lock[_bundleId].lockedTokens[i];
        }
        lock[_bundleId].count = 0;
    }
}

