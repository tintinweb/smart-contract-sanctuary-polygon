pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "../proxy/SmartVaultProxy.sol";
import "../vault/SmartVault.sol";

contract SmartVaultFactory is Initializable, OwnableUpgradeable {
    using CountersUpgradeable for CountersUpgradeable.Counter;
    mapping(uint256 => address) private smartVaults;
    mapping(address => address[]) private smartVaultsPerUser;
    mapping(address => address[]) private smartVaultsPerNonAdmin;
    mapping(address => mapping(address => uint256[])) private tokenIdsPerNonAdminVault;
    CountersUpgradeable.Counter private counter;
    uint256 smartVaultsTotal;
    string private version;
    address implementation;
    address private feeAddress;
    uint256 private fullyAllocatedFee;
    uint256 private coAllocatedFee;

    function initialize(address _smartVault, address _feeAddress, uint256 _fullyAllocatedFee, uint256 _coAllocatedFee, string memory _version) public initializer {
        __Ownable_init();
        version = _version;
        fullyAllocatedFee = _fullyAllocatedFee;
        coAllocatedFee = _coAllocatedFee;
        implementation = _smartVault;
        feeAddress = _feeAddress;
    }

    function versionContract() public view returns (string memory) {
        return version;
    }

    function createSmartVault(
        string memory _name,
        string memory _symbol,
        bool _coAllocated
    ) public payable collectFee(_coAllocated) {
        SmartVaultProxy newSmartVault = new SmartVaultProxy(address(implementation), abi.encodeWithSelector(SmartVault(payable(address(0))).initialize.selector, msg.sender, _name, _symbol, _coAllocated, address(this)));
        uint256 currentCounter = counter.current();
        smartVaults[currentCounter] = address(newSmartVault);
        counter.increment();
        smartVaultsPerUser[msg.sender].push(address(newSmartVault));
        smartVaultsTotal += 1;
    }

    function getSmartVaultsPerUser(address user) public view returns(address[] memory) {
        return smartVaultsPerUser[user];
    }

    function getSmartVaultsPerNonAdmin(address nonAdmin) public view returns (address[] memory) {
        return smartVaultsPerNonAdmin[nonAdmin];
    }

    function getSmartNftsPerNonAdminVault(address nonAdmin, address smartVault) public view returns (uint256[] memory) {
        return tokenIdsPerNonAdminVault[smartVault][nonAdmin];
    }

    function transferSmartVaultOwnership(address smartVault, address from, address to) external {
        for (uint256 index = 0; index < smartVaultsPerUser[from].length; index++) {
            if(smartVaultsPerUser[from][index] == smartVault) {
                smartVaultsPerUser[to].push(smartVault);
                smartVaultsPerUser[from][index] = smartVaultsPerUser[from][smartVaultsPerUser[from].length-1];
                if(index == smartVaultsPerUser[from].length - 1) {
                    delete smartVaultsPerUser[from][smartVaultsPerUser[from].length - 1];
                } else {
                    for(uint i = index; i < smartVaultsPerUser[from].length - 1; i++){
                        smartVaultsPerUser[from][i] = smartVaultsPerUser[from][i + 1];  
                    }
                    delete smartVaultsPerUser[from][smartVaultsPerUser[from].length - 1];
                }
            }    
        }
    }

    function addSmartVaultToNonAdmin(address smartVault, address nonAdmin, uint256 tokenId) external {
        smartVaultsPerNonAdmin[nonAdmin].push(smartVault);
        tokenIdsPerNonAdminVault[smartVault][nonAdmin].push(tokenId);
    }

    function getFeeAddress() public view returns(address) {
        return feeAddress;
    }

    function changeFeeAddress (address _feeAddress) external onlyOwner {
        feeAddress = _feeAddress;
    }

    function getFullyAllocatedFee() public view returns(uint256) {
        return fullyAllocatedFee;
    }

    function changeFullyAllocatedFee (uint256 _fullyAllocatedFee) external onlyOwner {
        fullyAllocatedFee = _fullyAllocatedFee;
    }

    function getCoAllocatedFee() public view returns(uint256) {
        return coAllocatedFee;
    }

    function changeCoAllocatedFee (uint256 _coAllocatedFee) external onlyOwner {
        coAllocatedFee = _coAllocatedFee;
    }

    function getSmartVaultAddress(uint256 id) public view returns(address) {
        return smartVaults[id];
    }

    function getSmartVaultsTotal() public view returns(uint256) {
        return smartVaultsTotal;
    }

    function getCurrentCounter() public view returns(uint256) {
        return counter.current();
    }

    modifier collectFee(bool coAllocated) {
        if(coAllocated) {
            if(coAllocatedFee > 0) {
                require(msg.value >= coAllocatedFee, "insufficient fee for co-allocated smart vault");
                payable(feeAddress).transfer(coAllocatedFee);
            }
        } else {
            if(fullyAllocatedFee > 0) {
                require(msg.value >= fullyAllocatedFee, "insufficient fee for fully-allocated smart vault");
                payable(feeAddress).transfer(fullyAllocatedFee);
            }
        }
        _;
    }

}

