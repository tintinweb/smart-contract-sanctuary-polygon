pragma solidity ^0.8.17;

import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "erc721a-upgradeable/contracts/ERC721AUpgradeable.sol";
import "./interfaces/ICommonProvider.sol";
import "./common/Common.sol";

contract FundingCollection is ERC721AUpgradeable, OwnableUpgradeable, Common {
	ICommonProvider private __provider;
	string private __tokenURI;
	string private __contractURI;
	Option[] private __options;

	struct Option {
		uint256 price;
		uint256 maxSupply; // max allowed number
		uint256 totalSupply; // current supply
	}

	function initialize(
		address _to,
		address _provider,
		string memory _name,
		string memory _symbol,
		string memory _baseTokenURI,
		string memory _baseContractURI
	) public initializerERC721A initializer {
		__tokenURI = _baseTokenURI;
		__contractURI = _baseContractURI;
		__provider = ICommonProvider(_provider);

		__ERC721A_init(_name, _symbol);
		_transferOwnership(_to);
		_setOperator(_to);
	}

	function _startTokenId() internal view virtual override returns (uint256) {
		return 1;
	}

	function _afterTokenTransfers(
		address _from,
		address _to,
		uint256 _startToken,
		uint256 _quantity
	) internal virtual override {
		if (_from != address(0) && _quantity == 1) {
			__provider.onTokenTransfer(address(this), _from, _to, _startToken);
		}
	}

	function _afterTokenMint(
		address _to,
		uint256 _startToken,
		uint256 _quantity,
		uint256 _option
	) private {
		__provider.onTokenMint(address(this), _to, _startToken, _quantity, abi.encode(_option));
	}

	function _baseURI() internal view virtual override returns (string memory) {
		return __tokenURI;
	}

	function setOperator(address operator) external onlyOwner {
		_setOperator(operator);
	}

	function deleteOperator(address operator) external onlyOwner {
		_deleteOperator(operator);
	}

	function setTokenURI(string memory _uri) external onlyOperator {
		__tokenURI = _uri;
	}

	function setContractURI(string memory _uri) external onlyOperator {
		__contractURI = _uri;
	}

	function setProvider(address _newProvider) external {
		require(_msgSender() == address(__provider), "Caller not provider");
		__provider = ICommonProvider(_newProvider);
	}

	function setOptions(
		uint256[] memory _optionPrices,
		uint256[] memory _optionSupply
	) external onlyOperator {
		require(_optionPrices.length == _optionSupply.length, "Incorrect options");

		for (uint256 i = 0; i < _optionPrices.length; ) {
			__options.push(Option(_optionPrices[i], _optionSupply[i], 0));
			unchecked {
				i++;
			}
		}
		__provider.onTokenUpdate(address(this), 0xd7e4f368, "");
	}

	function updateOptions(
		uint256[] memory _optionSupply,
		uint256[] memory _optionPrices
	) external onlyOperator {
		require(_optionSupply.length == _optionPrices.length, "Incorrect input options");
		require(_optionSupply.length == __options.length, "Incorrect output options");

		for (uint256 i = 0; i < __options.length;) {
			uint256 supply = _optionSupply[i];
			Option storage opt = __options[i];

			opt.price = _optionPrices[i];

			if (supply >= opt.totalSupply) {
				opt.maxSupply = supply;
			}
			unchecked {
				i++;
			}
		}
		__provider.onTokenUpdate(address(this), 0x3fe08166, "");
	}

	function publicMint(address _to, uint256 _amount, uint256 _option) external payable {
		require(__options.length != 0, "Options not settled");

		Option storage opt = __options[_option];
		uint256 totalPrice = opt.price * _amount;
		uint256 totalAmount = opt.totalSupply + _amount;

		require(totalPrice == msg.value, "Price not met");
		require(totalAmount <= opt.maxSupply, "Supply overflow");

		(uint256 fee, address feeReceiver) = __provider.fundingCollectionMintFee(
			totalPrice,
			owner()
		);
		if (fee > 0) Address.sendValue(payable(feeReceiver), fee);

		uint256 startTokenId = ERC721AStorage.layout()._currentIndex;

		_mint(_to, _amount);
		opt.totalSupply = totalAmount;
		_afterTokenMint(_to, startTokenId, _amount, _option);
	}

	function contractURI() public view returns (string memory) {
		return __contractURI;
	}

	function contractProvider() public view returns (address) {
		return address(__provider);
	}

	function maxSupply() public view returns (uint256) {
		uint256 count = 0;

		for (uint256 i = 0; i < __options.length; i++) {
			count += __options[i].maxSupply;
		}
		return count;
	}

	function options() public view virtual returns (Option[] memory) {
		return __options;
	}

	function option(uint256 _option) public view virtual returns (Option memory) {
		return __options[_option];
	}

	function withdraw(address _beneficiary, uint256 _amount) external onlyOwner {
		Address.sendValue(payable(_beneficiary), _amount);
	}

	function balance() public view returns (uint256) {
		return address(this).balance;
	}
}

