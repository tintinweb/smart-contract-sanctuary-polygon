pragma solidity ^0.8.17;

import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "erc721a-upgradeable/contracts/ERC721AUpgradeable.sol";
import "./interfaces/ICommonProvider.sol";
import "./common/Common.sol";

contract FanCollection is Common, ERC721AUpgradeable, OwnableUpgradeable {
	ICommonProvider private __provider;
	string private __tokenURI;
	string private __contractURI;

	event Update(address, bytes4, bytes);

	bool public tokenSale;
	uint256 public tokenPrice;
	uint256 public tokenPresalePrice;
	uint256 public tokenPresaleAmount;
	uint256 public tokenPresaleSold;
	uint256 public maxSupply;

	function initialize(
		address _to,
		address _provider,
		string memory _name,
		string memory _symbol,
		string memory _baseTokenURI,
		string memory _baseContractURI,
		uint256 _maxSupply,
		uint256 _tokenPrice,
		bool _tokenSale
	) public initializerERC721A initializer {
		__tokenURI = _baseTokenURI;
		__contractURI = _baseContractURI;
		__provider = ICommonProvider(_provider);

		maxSupply = _maxSupply;
		tokenSale = _tokenSale;
		tokenPrice = _tokenPrice;

		__ERC721A_init(_name, _symbol);
		_transferOwnership(_to);
		_setOperator(_to);
	}

	modifier inSupply(uint256 _amount) virtual {
		require(_amount + totalSupply() <= maxSupply, "Supply limit reached");
		_;
	}

	function setOperator(address operator) external onlyOwner {
		_setOperator(operator);
	}

	function deleteOperator(address operator) external onlyOwner {
		_deleteOperator(operator);
	}

	function _startTokenId() internal view virtual override returns (uint256) {
		return 1;
	}

	function _afterTokenTransfers(
		address _from,
		address _to,
		uint256 _startToken,
		uint256 _quantity
	) internal virtual override {
		if (_from != address(0) && _quantity == 1) {
			__provider.onTokenTransfer(address(this), _from, _to, _startToken);
		}
	}

	function _afterTokenMint(address _to, uint256 _startToken, uint256 _quantity) private {
		__provider.onTokenMint(address(this), _to, _startToken, _quantity, "");
	}

	function _baseURI() internal view virtual override returns (string memory) {
		return __tokenURI;
	}

	function setTokenURI(string memory _uri) external onlyOperator {
		__tokenURI = _uri;
	}

	function setContractURI(string memory _uri) external onlyOperator {
		__contractURI = _uri;
	}

	function contractURI() public view returns (string memory) {
		return __contractURI;
	}

	function contractProvider() public view returns (address) {
		return address(__provider);
	}

	function privateMint(address _to, uint256 _amount) external onlyOperator {
		__mint(_to, _amount, 0);
	}

	function publicMint(address _to, uint256 _amount) external payable {
		require(tokenSale, "Token sale not started");
		__mint(_to, _amount, tokenPrice * _amount);
	}

	function presaleMint(address _to, uint256 _amount) external payable {
		uint256 total = tokenPresaleSold + _amount;
		require(tokenPresaleAmount > 0 && tokenPresaleAmount >= total, "Out of presale limit");

		__mint(_to, _amount, tokenPresalePrice * _amount);
		tokenPresaleSold = total;
	}

	function __mint(address _to, uint256 _amount, uint256 _price) private inSupply(_amount) {
		if (_price > 0) {
			require(msg.value == _price, "Price not met");

			(uint256 fee, address feeReceiver) = __provider.fanCollectionMintFee(_price, owner());
			if (fee > 0) Address.sendValue(payable(feeReceiver), fee);
		}

		uint256 startTokenId = ERC721AStorage.layout()._currentIndex;

		_mint(_to, _amount);
		_afterTokenMint(_to, startTokenId, _amount);
	}

	function setupPresaleMint(uint256 _price, uint256 _amount) external onlyOperator {
		tokenPresaleSold = 0;
		tokenPresalePrice = _price;
		tokenPresaleAmount = _amount;
		__provider.onTokenUpdate(address(this), 0x652cbe46, abi.encode(_price, _amount));
	}

	function setupPublicMint(bool _sale, uint256 _price) external onlyOperator {
		tokenSale = _sale;
		tokenPrice = _price;
		__provider.onTokenUpdate(address(this), 0x1a5addc8, abi.encode(_sale, _price));
	}

	function setProvider(address _newProvider) external {
		require(_msgSender() == address(__provider), "Caller not provider");
		__provider = ICommonProvider(_newProvider);
	}

	function withdraw(address _beneficiary, uint256 _amount) external onlyOwner {
		Address.sendValue(payable(_beneficiary), _amount);
	}

	function balance() public view returns (uint256) {
		return address(this).balance;
	}
}

