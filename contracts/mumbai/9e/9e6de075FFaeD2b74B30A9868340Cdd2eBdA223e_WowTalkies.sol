// SPDX-License-Identifier: MIT
pragma solidity ^0.8.17;

import {ERC1155Upgradeable, Initializable} from "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import {AccessControlUpgradeable} from "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import {ERC2981Upgradeable} from "@openzeppelin/contracts-upgradeable/token/common/ERC2981Upgradeable.sol";
import {ERC1155BurnableUpgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155BurnableUpgradeable.sol";

contract WowTalkiesEarlyAdopterDrop is
    Initializable,
    ERC1155Upgradeable,
    AccessControlUpgradeable,
    ERC2981Upgradeable,
    ERC1155BurnableUpgradeable
{
    string public name;
    string public symbol;
    string private _contractUri;
    bool public tokensBurnable;

    struct SaleItem {
        uint256 supply;
        uint256 sold;
        uint256 erc20Value;
        bool paused;
        bool exists;
    }

    mapping(uint256 => SaleItem) public saleItems;
    // Mapping for user address to token count
    // mapping(address => mapping(uint256 => uint256)) public boughtQuantities;
    bytes32 public constant MARKET_ADMIN_ROLE = keccak256("MARKET_ADMIN_ROLE");

    event TokenBurned(
        address indexed account,
        uint256 indexed tokenId,
        uint256 value
    );

    /**
     * @dev Throws if called by any account other than admins.
     */
    modifier adminOnly() {
        require(
            hasRole(MARKET_ADMIN_ROLE, _msgSender()),
            "Must have market admin role"
        );
        _;
    }

    function initialize(
        string memory _name,
        string memory _symbol,
        // bool _tokensBurnable,
        string memory _contractUrl,
        string memory _tokenUri,
        address _royaltiesAddress,
        uint96 _royalty
    ) external initializer {
        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(MARKET_ADMIN_ROLE, _msgSender());
        _setDefaultRoyalty(_royaltiesAddress, _royalty);
        name = _name;
        symbol = _symbol;
        // _tokensBurnable = tokensBurnable;
        _contractUri = _contractUrl;
        __ERC1155_init(_tokenUri);
        __AccessControl_init();
    }

    function mint(
        address _to,
        uint256 _tokenId,
        uint256 _count
    ) external adminOnly {
        require(saleItems[_tokenId].exists, "Token is not registered.");
        require(!saleItems[_tokenId].paused, "Token is paused.");
        require(
            saleItems[_tokenId].sold + _count <= saleItems[_tokenId].supply,
            "Reached max supply"
        );
        super._mint(_to, _tokenId, _count, "0x");
        saleItems[_tokenId].sold += _count;
        // boughtQuantities[_to][_tokenId] += _count;
    }

    function batchMint(
        address[] calldata _addresses,
        uint256 _tokenId,
        uint256 _count
    ) external adminOnly {
        require(saleItems[_tokenId].exists, "Token is not registered.");
        require(!saleItems[_tokenId].paused, "Token is paused.");
        require(
            saleItems[_tokenId].sold + _count <= saleItems[_tokenId].supply,
            "Reached max supply"
        );
        for (uint256 i = 0; i < _addresses.length; i++) {
            super._mint(_addresses[i], _tokenId, _count, "0x");
            saleItems[_tokenId].sold += _count;
            // boughtQuantities[_addresses[i]][_tokenId] += _count;
        }
    }

    function registerToken(
        uint256 _tokenId,
        uint256 _supply,
        uint256 _erc20Value
    ) external adminOnly {
        require(_supply > 0, "Not enough supply");
        require(saleItems[_tokenId].supply == 0, "Token already registered.");
        saleItems[_tokenId].supply = _supply;
        saleItems[_tokenId].erc20Value = _erc20Value;
        saleItems[_tokenId].paused = false;
        saleItems[_tokenId].exists = true;
    }

    function pauseToken(
        uint256 _tokenId,
        bool _pauseStatus
    ) external adminOnly {
        saleItems[_tokenId].paused = _pauseStatus;
    }

    function setMaxSupply(
        uint256 _tokenId,
        uint256 _newSupply
    ) external adminOnly {
        require(
            _newSupply >= saleItems[_tokenId].supply - saleItems[_tokenId].sold,
            "Invalid supply."
        );
        saleItems[_tokenId].supply = _newSupply;
    }

    function setERC20Value(
        uint256 _tokenId,
        uint256 _erc20Value
    ) external adminOnly {
        saleItems[_tokenId].erc20Value = _erc20Value;
    }

    function setURI(string memory _newUri) external adminOnly {
        _setURI(_newUri);
    }

    function setContractURI(string memory _newUri) external adminOnly {
        _contractUri = _newUri;
    }

    function setDefaultRoyalty(
        address receiver,
        uint96 feeNumerator
    ) public adminOnly {
        _setDefaultRoyalty(receiver, feeNumerator);
    }

    function setTokensBurnable(bool _tokensBurnable) external adminOnly {
        tokensBurnable = _tokensBurnable;
    }

    function contractURI() public view returns (string memory) {
        return _contractUri;
    }

    function getSaleItems(
        uint256 _tokenId
    ) public view returns (SaleItem memory) {
        return saleItems[_tokenId];
    }

    function grantRole(
        bytes32 role,
        address account
    ) public virtual override adminOnly {
        return super._grantRole(role, account);
    }

    function burn(
        address account,
        uint256 tokenId,
        uint256 value
    ) public override {
        require(tokensBurnable, "NFT: tokens burning is disabled");
        require(saleItems[tokenId].exists, "Token is not registered.");
        super.burn(account, tokenId, value);
        emit TokenBurned(account, tokenId, value);
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        virtual
        override(
            ERC1155Upgradeable,
            AccessControlUpgradeable,
            ERC2981Upgradeable
        )
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

