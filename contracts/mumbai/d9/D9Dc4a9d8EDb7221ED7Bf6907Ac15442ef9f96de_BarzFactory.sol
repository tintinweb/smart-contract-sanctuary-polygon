/**
 * @title Barz Factory
 * @dev Contract to easily deploy Barz to a pre-computed address with a single call
 * @author David Yongjun Kim (@Powerstream3604)
 */
contract BarzFactory is IBarzFactory {
    event BarzDeployed(address);

    address immutable diamondCutFacet;
    address immutable accountFacet;
    address immutable entryPoint;
    address immutable diamondLoupeFacet;
    address immutable diamondInit;
    address immutable facetRegistry;

    constructor(
        address _diamondCutFacet,
        address _accountFacet,
        address _entryPoint,
        address _diamondLoupeFacet,
        address _diamondInit,
        address _facetRegistry
    ) {
        diamondCutFacet = _diamondCutFacet;
        accountFacet = _accountFacet;
        entryPoint = _entryPoint;
        diamondLoupeFacet = _diamondLoupeFacet;
        diamondInit = _diamondInit;
        facetRegistry = _facetRegistry;
    }

    function createAccount(
        address _verificationFacet,
        bytes calldata _owner,
        uint256 _salt
    ) external override returns (Barz barz) {
        address addr = getAddress(_verificationFacet, _owner, _salt);
        uint codeSize = addr.code.length;
        if (codeSize > 0) {
            return Barz(payable(addr));
        }
        barz = new Barz{salt: bytes32(_salt)}(
            diamondCutFacet,
            accountFacet,
            _verificationFacet,
            entryPoint,
            diamondLoupeFacet,
            diamondInit,
            facetRegistry,
            _owner
        );
        emit BarzDeployed(address(barz));
    }

    function getAddress(
        address _verificationFacet,
        bytes calldata _owner,
        uint256 _salt
    ) public view override returns (address) {
        bytes memory bytecode = getBytecode(
            diamondCutFacet,
            accountFacet,
            _verificationFacet,
            entryPoint,
            diamondLoupeFacet,
            diamondInit,
            facetRegistry,
            _owner
        );
        bytes32 hash = keccak256(
            abi.encodePacked(
                bytes1(0xff),
                address(this),
                _salt,
                keccak256(bytecode)
            )
        );
        return address(uint160(uint256(hash)));
    }

    function getBytecode(
        address _diamondCutFacet,
        address _accountFacet,
        address _verificationFacet,
        address _entryPoint,
        address _diamondLoupeFacet,
        address _diamondInit,
        address _facetRegistry,
        bytes calldata _ownerPublicKey
    ) public pure override returns (bytes memory) {
        bytes memory bytecode = type(Barz).creationCode;
        return
            abi.encodePacked(
                bytecode,
                abi.encode(
                    _diamondCutFacet,
                    _accountFacet,
                    _verificationFacet,
                    _entryPoint,
                    _diamondLoupeFacet,
                    _diamondInit,
                    _facetRegistry,
                    _ownerPublicKey
                )
            );
    }

    function getCreationCode() public pure override returns (bytes memory) {
        bytes memory bytecode = type(Barz).creationCode;
        return bytecode;
    }
}

