// File: contracts/Airdrop.sol



pragma solidity ^0.8.0;

//   :::  === :::===  :::====     $$  
//   :::  === :::     :::  ===    $$  
//   ===  ===  =====  ===  === $$$$$$$$
//   ===  ===     === ===  ===    $$  
//    ======  ======  =======     $$









contract Airdrop is ReentrancyGuard, Context, Ownable{

    using ABDKMath64x64 for int128;
    
    uint256 private deploymentTimestamp;

    bool private burnClaimed;

    uint256 public remainingClaims = 500;

    uint256 public counter = 0;

    mapping(address => bool) private Claimed;
    mapping(address => bool) private _isWhitelist;
    mapping(address => uint256) private _valDrop;

    mapping(address => uint256) public counters;

    mapping(address => mapping(uint256 => bool)) public claimedNFTs;

    address private _DGENToken = 0xAe60cb11856DE1519C6c51B0157115D64e0048a4;
    address private _YAYOToken =  0xFCc92fEd5F5EE1f1C755f7F1Ffab72102d563095; // Airdrop 1 Token
    address private _SHNOISESToken = 0xD1C1d841d24C1a45cE0556E6f6826594DF190946; // Airdrop 2 Token
    address private _NECKSToken = 0x9758fA8ecB9f5E523e7270fadCB097625EB546FD; // Airdrop 3 Token
    
    IERC20 private _token;
    bool public airdropLive = false;
    
    event AirdropClaimed(address receiver, uint256 amount);
    event WhitelistSetted(address[] recipient, uint256[] amount);

    constructor() {
        deploymentTimestamp = block.timestamp;


    }
    
     
     //Start Airdrop
    function startAirdrop(IERC20 tokenAddress) public onlyOwner{
        require(airdropLive == false, 'Airdrop already started');
        _token = tokenAddress;

        airdropLive = true;
    }
    
     function setWhitelist(address[] calldata recipients, uint256[] calldata amount) external onlyOwner{
        for(uint i = 0; i< recipients.length; i++){
            require(recipients[i] != address(0));
            _valDrop[recipients[i]] = amount[i];
        }
        emit WhitelistSetted(recipients, amount);
    }

    function claimTokens() public nonReentrant {
        require(airdropLive == true, 'Airdrop not started yet');
        require(Claimed[msg.sender] == false, 'Airdrop already claimed!');
        if(_token.balanceOf(address(this)) == 0) { airdropLive = false; return;}
        Claimed[msg.sender] = true;
        uint256 amount = _valDrop[msg.sender] *10**18;
        _token.transfer(msg.sender, amount);
        emit AirdropClaimed(msg.sender, amount);
    }
    
    function withdraw() external onlyOwner {
         require(address(this).balance > 0, 'Contract has no money');
         address payable wallet = payable(msg.sender);
         wallet.transfer(address(this).balance);    
    }

    // After 30 days anyone can call this function to receive 1% of the tokens in the contract.
    // Burns the other 99%. Excludes DGEN holders, who can claim anytime.

    function claimAndBurn() public nonReentrant {

        require(!burnClaimed, "Already Claimed.");
        require(!isTransferWithin30Days(), "Wait 30 days after deployment to use this function.");

        address payable burn = payable(0x0000000000000000000000000000000000000000);

        uint256 exDegenesysBalance = _token.balanceOf(address(this)) - (remainingClaims * 2100000000 *10 **18);
        _token.transfer(burn, exDegenesysBalance);

        burnClaimed = true;
    }

    function isTransferWithin30Days() public view returns (bool) {
        uint256 currentTime = block.timestamp;
        uint256 thirtyDaysInSeconds = 30 days;
        // Check if the current time is within 30 days of the deployment timestamp
        return currentTime <= (deploymentTimestamp + thirtyDaysInSeconds);
    }

    // unlike all other airdrops, can be claimed after 30 days
    function claimDGEN(uint256[] calldata _tokenIds) public nonReentrant {
        counter = 0;
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            require(!claimedNFTs[_DGENToken][_tokenIds[i]], "NFT already claimed");
            require(IERC721(_DGENToken).balanceOf(msg.sender) > 0 && IERC721(_DGENToken).ownerOf(_tokenIds[i]) == msg.sender, "You do not own this NFT");
            
            claimedNFTs[_DGENToken][_tokenIds[i]] = true;

            counter++;

            remainingClaims--;
        }

        uint256 airdropAmt = counter * 2100000000 *10 **18;

        _token.transfer(msg.sender, airdropAmt);
    }


    // Logistic function used to determine airdrop allocation for Longnecks, YAYO, Shnoises and Impulses.
    // This way the supply is less centralized, as each subsequent NFT is worth less.
    function f(uint256 x) public pure returns (uint256) {
        int128 x128 = ABDKMath64x64.fromUInt(x);

        int128 L = ABDKMath64x64.fromInt(2100000000);
        int128 k = ABDKMath64x64.divu(2, 10); // 0.2
        int128 x0 = ABDKMath64x64.fromInt(5);

        int128 exponent = ABDKMath64x64.mul(-k, ABDKMath64x64.sub(x128, x0));
        int128 expValue = ABDKMath64x64.exp(exponent);

        return ABDKMath64x64.toUInt(ABDKMath64x64.div(L, ABDKMath64x64.add(ABDKMath64x64.fromInt(1), expValue)));
    }
    

    function claimMultipleProjects(
        uint256[] calldata yayoTokenIds,
        uint256[] calldata necksTokenIds,
        uint256[] calldata shnoisesTokenIds
    ) public nonReentrant {

        uint256 claimed = counters[msg.sender];

        uint256 totalAirdropAmt = 0;

        // Calculate airdrop amount for YAYO tokens
        calculateAirdropForProject(_YAYOToken, yayoTokenIds);

        // Calculate airdrop amount for Longnecks tokens
        calculateAirdropForProject(_NECKSToken, necksTokenIds);

        // Calculate airdrop amount for Shnoises tokens
        calculateAirdropForProject(_SHNOISESToken, shnoisesTokenIds);

        uint256 totalAllocation = f(counters[msg.sender]) *10 **18 ;
        uint256 claimedAllocation = 0;

        if(claimed > 0) {
            claimedAllocation = f(claimed) *10 **18;
        }

        totalAirdropAmt = totalAllocation - claimedAllocation;

        // Transfer total airdrop amount to the caller
        _token.transfer(msg.sender, totalAirdropAmt);
    }

    function calculateAirdropForProject(address nftAddress, uint256[] memory tokenIds) private {

        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(IERC721(nftAddress).balanceOf(msg.sender) > 0 && IERC721(nftAddress).ownerOf(tokenIds[i]) == msg.sender, "You do not own this NFT");

            if (claimedNFTs[nftAddress][tokenIds[i]] = false) {

                claimedNFTs[nftAddress][tokenIds[i]] = true;

                counters[msg.sender]++;
            }
        }

    }








}