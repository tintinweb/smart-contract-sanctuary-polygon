/// @title Last Remains Character Contract
/// @author Earn Alliance / Last Remains , Elliott Williams, Ho Chi Ho
/// @notice This contract mints various characters from the Last Remains Game that are verifiably random
/// @dev Contract is liable to be updated without warning
/// @custom:security-contact [emailprotected]
contract LastRemainsCharacter is
  Initializable,
  PausableUpgradeable,
  ERC1155SupplyUpgradeable,
  UUPSUpgradeable,
  EIP712Upgradeable,
  OwnableUpgradeable
{
  // chainlink middleman
  IMiddleman public middleman;

  // LRCharPersonas
  ILRCharPersonas public characterPersonas;

  event RequestSent(uint256 requestId, uint256 amt);
  event Minted(address mintedTo, uint256 amt, uint256 rand);

  error Unauthorized();

  modifier onlyMiddleman() {
    if (address(middleman) != msg.sender) revert Unauthorized();
    _;
  }

  function setMiddleman(address _middleman) public onlyOwner {
    middleman = IMiddleman(_middleman);
  }

  function setPersonas(address _characterPersonas) public onlyOwner {
    characterPersonas = ILRCharPersonas(_characterPersonas);
  }

  bytes32 private constant TYPEHASH =
    keccak256("Character(bool isGenesis,uint256 amount,address toAddress)");

  address s_signer; // The Signer must be this address for the message payload for a successful mint

  mapping(uint256 => uint256) public maxSupply;

  mapping(uint256 => string) private _uris;

  mapping(uint256 => CharacterMintRequest) public requestsToAddr;

  uint256 public salePrice; // 0.1337 MATIC

  /// @custom:oz-upgrades-unsafe-allow constructor
  constructor() {
    _disableInitializers();
  }

  /// @notice Initialization of contract, called upon deployment
  /// @dev implements EIP712, is upgradeable, pausable, burn function is custom to save space
  /// @param _signer this address will be used to sign all mints
  /// @param name name of the contract (EIP712 required)
  /// @param version version of the contract (EIP712 required)
  function initialize(
    address _signer,
    string memory name,
    string memory version,
    address _middleman,
    address _characterPersonas
  ) public initializer {
    middleman = IMiddleman(_middleman);
    characterPersonas = ILRCharPersonas(_characterPersonas);

    __ERC1155_init("");
    __Ownable_init();
    __Pausable_init();
    // __ERC1155Burnable_init();
    __ERC1155Supply_init();
    __UUPSUpgradeable_init();
    __EIP712_init(name, version);
    s_signer = _signer;

    salePrice = 133700000000000000; // 0.1337 MATIC
  }

  /// @notice set URI.  Where metadata and images will come from
  /// @param newuri the address that the owner would like the new URI to be
  function setURI(string memory newuri, uint256 tokenId) public onlyOwner {
    _setURI(newuri);
    _uris[tokenId] = newuri;
  }

  // /// TODO: clean up.. simplify
  // function _setURI(string memory newuri, uint256 tokenId) internal virtual {
  //   _uris[tokenId] = newuri;
  // }

  function uri(
    uint256 tokenId
  ) public view virtual override returns (string memory) {
    return _uris[tokenId];
  }

  /// @notice contractURI, OpenSea compliant metadata standard for Character Collections
  /// @return URI that holds the metadata for collection

  function contractURI() public pure returns (string memory) {
    return "https://cdn-ea.earnalliance.com/metadata/lrcharacters";
  }

  /// @notice pause any token transfers including mints and burns
  /// @dev can only be called by owner
  function pause() public onlyOwner {
    _pause();
  }

  /// @notice unpause any token transfers including mints and burns
  /// @dev can only be called by owner
  function unpause() public onlyOwner {
    _unpause();
  }

  /// @notice burn a character, can only be called by owner
  /// @dev used custom call instead of inheritable to optimize contract size
  /// @param from what address's character to burn
  /// @param id what the id of the character to burn is
  /// @param amount how many to burn
  function characterBurn(
    address from,
    uint256 id,
    uint256 amount
  ) public virtual onlyOwner {
    _burn(from, id, amount);
  }

  /// @notice Owner can set Max Supply for each character
  /// @dev returns default value (0) if not set, owner only
  /// @param _id the tokenId of the character to set supply
  /// @param _supply what to set the new supply as
  function setMaxSupply(uint256 _id, uint256 _supply) public onlyOwner {
    maxSupply[_id] = _supply;
  }

  function setCharacterPrice(uint256 newPrice) public onlyOwner {
    salePrice = newPrice;
  }

  /// @notice Mint an LR character, dependent on signature approval and choice of tokenId
  /// @dev param details: https://ethereum.stackexchange.com/a/118415/68000
  /// @param input Interface for isGenesis, amount, address
  /// @param r the x-coordinate of a random curve point
  /// @param s signature proof for r
  /// @param v recovery parameter
  function characterMint(
    CharacterMintRequest calldata input,
    bytes32 r,
    bytes32 s,
    uint8 v
  ) external payable {
    bool g = input.isGenesis;
    address a = input.toAddress;

    require(verify(input, r, s, v), "Invalid sig");
    // require(totalSupply(t) < maxSupply[t], "Supply depleted");
    require(input.amount < 101, "< 100 or less / request");
    if (g) {
      // then is a genesis mint
      require(
        msg.value >= SafeMathUpgradeable.mul(salePrice, input.amount),
        "Price value sent is not correct"
      );
    }

    // TODO: amount can currently only be 1.  Please make it so you can call more than one..
    // this makes callbacks simpler initially, but a loop could fix this.

    // Call middle man for randomness.
    // Middleman calls character for minting in ~4 blocks.
    uint256 requestId = middleman.requestRandomWords(uint32(input.amount));

    requestsToAddr[requestId] = CharacterMintRequest({
      toAddress: a,
      isGenesis: g,
      amount: input.amount
    });

    emit RequestSent(requestId, uint256(input.amount));
  }

  /// @notice Completion of mint process, called by middleman
  /// @dev param details:
  /// @param rands random number received
  /// @param reqId corresponding request id
  function finishCharacterMint(
    uint256[] memory rands,
    uint256 reqId
  ) external onlyMiddleman {
    //  use request id to get address
    address userAddr = requestsToAddr[reqId].toAddress;
    bool isGenesis = requestsToAddr[reqId].isGenesis;
    uint256 amt = uint256(requestsToAddr[reqId].amount);

    // ADD: emit address + randomness?

    //  use number of rands to determine how many characters minted in this request
    //  persona determines tokenId
    for (uint256 i = 0; i < rands.length; i++) {
      uint256 randomNumberForPersona = rands[i];
      // VRF can return 0, this below avoids that plague
      if (randomNumberForPersona == 0) {
        randomNumberForPersona = 1;
      }
      uint256 persona = characterPersonas.getPersona(
        randomNumberForPersona,
        isGenesis
      );

      // Max Supplies should basically be infinite at start, so ignoring supply check at peril of revert
      // NOTE: may be worth looking again, in case a tokenID gets picked that hasn't been properly maxSupplied
      // if (totalSupply(tokenId) < maxSupply[tokenId]) {
      //  // "Supply depleted"
      //  // could also add/subtract one, but prob poor solution
      //   continue;
      // }

      //  mint each to address, 1 per each run
      emit Minted(userAddr, amt, randomNumberForPersona);

      _mint(userAddr, persona, 1, bytes("0x"));
    }
  }

  //   function checkOnRequest(
  //     uint256 requestId
  //   ) returns (uint256 paid, bool fulfilled, uint256[] memory randomWords) {
  //     uint256 req = middleman.requestsForRandomness[1];
  //   }

