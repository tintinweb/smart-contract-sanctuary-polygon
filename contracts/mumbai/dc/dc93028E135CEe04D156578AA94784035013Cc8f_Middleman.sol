  // }

  /// @notice verifies that minting user has accomplished Earn Alliance tasks and has earned mint verification from Earn
  /// @dev compares user's digest to s_signer address to see if they have been validated by the s_signer addr
  /// @param characterMintRequest Interface for isGenesis, amount, address
  /// @return bool true or false, used to determine in characterMint if address has completed tasks for character
  function verify(
    CharacterMintRequest calldata characterMintRequest,
    bytes32 r,
    bytes32 s,
    uint8 v
  ) private view returns (bool) {
    bytes32 digest = _hashTypedDataV4(
      keccak256(
        abi.encode(
          TYPEHASH,
          characterMintRequest.isGenesis,
          characterMintRequest.amount,
          characterMintRequest.toAddress
        )
      )
    );
    // address signer = digest.recover(x.v, x.r, x.s);

    return ecrecover(digest, v, r, s) == s_signer;
  }

