import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";

import "hardhat/console.sol";

import "./LRCharacter.sol";

struct Ceiling {
  uint256 id;
  uint256 val;
}

interface ILRCharPersonas {
  function getPersona(
    uint256 fromRand,
    bool isGenesis
  ) external returns (uint256 personaId);
}

/// @title Earn Alliance MetaBadge Contract
/// @author Earn Alliance, Elliott Williams, Ho Chi Ho
/// @notice This contract mints various badges from the Earn Alliance Ecosystem that are soulbound
/// @dev Contract is liable to be updated without warning
/// @custom:security-contact [emailprotected]
contract LRCharPersonas is
  Initializable,
  PausableUpgradeable,
  UUPSUpgradeable,
  OwnableUpgradeable
{
  ILastRemainsCharacter public minter;

  error Unauthorized();

  event getPersonaEvent(uint256 pers, uint256 tId);

  mapping(uint256 => Ceiling) ceilings;
  uint256 public ceilingCount;

  /// @custom:oz-upgrades-unsafe-allow constructor
  constructor() {
    _disableInitializers();
  }

  modifier onlyMinter() {
    if (address(minter) != msg.sender) revert Unauthorized();
    _;
  }

  /// @notice Initialization of contract, called upon deployment
  /// @dev implements EIP712, is upgradeable, pausable, burn function is custom to save space
  /// @param _minter address of last remains character contract
  function initialize(address _minter) public initializer {
    minter = ILastRemainsCharacter(_minter);
    __Ownable_init();
    __Pausable_init();
    __UUPSUpgradeable_init();
  }

  function setMinter(address _minter) public onlyOwner {
    minter = ILastRemainsCharacter(_minter);
  }

  function gUp(uint256 a) internal pure returns (uint256) {
    return uint256(a + 1337000);
  }

  function createTokenId(
    bool isGenesis,
    uint256 personaId
  ) internal pure returns (uint256 newTokenId) {
    uint256 tokenId = personaId;
    // append genesis
    if (isGenesis == true) {
      tokenId = gUp(tokenId);
      // tokenId = bytes1(shiftLeft(tokenId, 1) | bytes1(0x01));
    }
    return tokenId;
  }

  function makePersona(
    uint256 rand
  ) internal view returns (uint256 basePersona) {
    uint256 cleanedRand = SafeMathUpgradeable.mod(rand, 32000000000);
    for (uint256 i = 0; i < ceilingCount; i++) {
      // console.log(ceilings[i].val);
      // console.log("here in solidity");
      if (cleanedRand < ceilings[i].val) {
        return ceilings[i].id;
      }
    }
  }

  /// @notice get a persona based on the random number returned from Chainlink
  /// @dev may want to consider alterations if we need to delete persona
  /// @param fromRand random number
  /// @return personaId token ID to assign to new mint
  function getPersona(
    uint256 fromRand,
    bool isGenesis
  ) external onlyMinter returns (uint256 personaId) {
    // TODO: do in smaller ints.. at first
    uint256 persona = makePersona(fromRand);
    // if genesis, add to id
    uint256 tokenId = createTokenId(isGenesis, persona);
    emit getPersonaEvent(persona, tokenId);

    return tokenId;
  }

  function addCeiling(
    uint256 id,
    uint256 val
  ) public onlyOwner returns (bool success) {
    // deal with adjusting ceilings instead of just adding new ones
    ceilings[ceilingCount] = Ceiling({ id: id, val: val });
    ceilingCount += 1;

    success = true;
  }

  function getCeiling(uint256 id) public view returns (Ceiling memory) {
    for (uint256 i = 0; i < ceilingCount; i++) {
      if (ceilings[i].id == id) {
        return ceilings[i];
      }
    }
  }

  function findCeilings(
    uint256 val
  ) public view returns (Ceiling[] memory filteredCeilings) {
    Ceiling[] memory ceilingsTemp = new Ceiling[](ceilingCount);
    uint256 count;
    for (uint256 i = 0; i < ceilingCount; i++) {
      if (ceilings[i].val == val) {
        ceilingsTemp[count] = ceilings[i];
        count += 1;
      }
    }

    filteredCeilings = new Ceiling[](count);
    for (uint256 i = 0; i < count; i++) {
      filteredCeilings[i] = ceilingsTemp[i];
    }
  }

  function _authorizeUpgrade(
    address newImplementation
  ) internal virtual override {}
}

