  /// #if_succeeds TokenType.unwrap(t) > 0;
  /// #if_succeeds TokenType.unwrap(t) == 1 ==> ed(t) == Edition.STD && rarity(t) == Rarity.COMMON;
  /// #if_succeeds TokenType.unwrap(t) == 2 ==> ed(t) == Edition.STD && rarity(t) == Rarity.UNCOMMON;
  /// #if_succeeds TokenType.unwrap(t) == 3 ==> ed(t) == Edition.STD && rarity(t) == Rarity.RARE;
  /// #if_succeeds TokenType.unwrap(t) == 4 ==> ed(t) == Edition.FIRST && rarity(t) == Rarity.COMMON;
  /// #if_succeeds TokenType.unwrap(t) == 5 ==> ed(t) == Edition.FIRST && rarity(t) == Rarity.UNCOMMON;
  /// #if_succeeds TokenType.unwrap(t) == 6 ==> ed(t) == Edition.FIRST && rarity(t) == Rarity.RARE;
  /// #if_succeeds TokenType.unwrap(t) < 7;
  function checkTokenType(TokenType t) public pure {
    if (TokenType.unwrap(t) == 0 || TokenType.unwrap(t) >= 7) { revert InvalidTokenType(t); }
  }

  error InvalidTokenType(TokenType typ);

