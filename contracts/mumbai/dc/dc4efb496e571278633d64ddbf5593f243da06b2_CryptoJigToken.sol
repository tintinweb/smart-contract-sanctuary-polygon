  ///     int256(_getAmtBacked(typ)) - int256(old(_getAmtBacked(typ)));
  function _updateCountBackedByMatic(TokenType typ, int32 amt) private {
    if (amt < 0) {
      countBackedByMatic[typ] -= uint32(-amt);
    } else {
      countBackedByMatic[typ] += uint32(amt);
    }
  }
  */

  // Mapping from abi.encodePacked(contractAddr, tokenId) to the token's TokenType
  mapping(bytes => TokenType) private _typeRegistry;

  /* only for testing
  mapping(TokenType => uint256) private _totalSupply;
