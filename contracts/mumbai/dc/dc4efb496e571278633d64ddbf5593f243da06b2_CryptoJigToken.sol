  // a multiplier for token values
  /// #if_updated isBasicTrusted(msg.sender);
  uint32 private _valMult = 1;

  constructor() ERC1155("https://bafybeicojvdj6k2wnqwfjqffmww4kfoamzumkavvuoxyvrci63rfcd4tve.ipfs.dweb.link/metadata/{id}.json") {}

  function onERC1155Received(address operator, address from, uint256 tokenId, uint256 count, bytes calldata data)
      external override returns (bytes4) {
    uint256[] memory tids   = new uint256[](1); tids[0]   = tokenId;
    uint256[] memory counts = new uint256[](1); counts[0] = count;
    _receiveERC1155Batch(operator, from, tids, counts, data);
    return this.onERC1155Received.selector;
  }

  function onERC1155BatchReceived(
    address operator,
    address from,
    uint256[] memory tids,
    uint256[] memory counts,
    bytes calldata data
  ) external override returns (bytes4) {
    _receiveERC1155Batch(operator, from, tids, counts, data);
    return this.onERC1155BatchReceived.selector;
  }

  //  If reading the spec becomes more complicated than reading the code, then what's the point?
  /// #if_succeeds isBasicTrusted(from) && isBasicTrusted(operator) && AUTH_CONTRACT.hasRole(trustedERC1155, msg.sender);
  /// #if_succeeds tids.length == counts.length;
  /// #if_succeeds
  ///   let typUnwrapped := TokenType.unwrap(_decodeTokenType(data)) in
  ///   typUnwrapped > 0 && typUnwrapped < 7;
  /// #if_succeeds
  ///   let typ := old(_decodeTokenType(data)) in
  ///   let cjtid := old(getTokenId(typ)) in
  ///   isBackedType(typ) ==>
  ///     balanceOf(operator, cjtid) == old(balanceOf(operator, cjtid)) + unchecked_sum(counts);
  /// #if_succeeds
  ///   let typ := old(_decodeTokenType(data)) in
  ///   let cjtid := old(getTokenId(typ)) in
  ///   !isBackedType(typ) ==>
  ///     balanceOf(operator, cjtid) == old(balanceOf(operator, cjtid));
  /// #if_succeeds
  ///   let typ := _decodeTokenType(data) in
  ///   forall(uint256 i in tids)
  ///     _tokenTypsEq(typ, _typeRegistry[_getTypeRegistryKey(msg.sender, tids[i])]);
  /// these ought to be forall, but can't be due to scribble limitations
  /// #if_succeeds
  ///   ERC1155(msg.sender).balanceOf(address(this), tids[0]) > counts[0] ==>
  ///     _tokenTypsEq(
  ///       _typeRegistry[_getTypeRegistryKey(msg.sender, tids[0])],
  ///       old(_typeRegistry[_getTypeRegistryKey(msg.sender, tids[0])]));
  function _receiveERC1155Batch(
    address operator,
    address from,
    uint256[] memory tids,
    uint256[] memory counts,
    bytes calldata data
  ) private {
    require(
      isBasicTrusted(from) && isBasicTrusted(operator),
      "CryptoJigToken: Only CryptoJigWorld may send NFTs to CryptoJigToken"
    );

    address contractAddr = msg.sender;
    require(contractAddr != address(this), "CryptoJigToken: Can't send CryptoJigToken its own tokens");
    require(AUTH_CONTRACT.hasRole(trustedERC1155, contractAddr), "CryptoJigToken: reception of unapproved NFT");

    TokenType typ = _decodeTokenType(data);
    checkTokenType(typ);
    uint256 totalCount = _performBatch(contractAddr, tids, counts, typ, _receiveTask);

    if (isBackedType(typ)) {
      _mintUnsafe(operator, totalCount, typ);
    }
  }

  function _decodeTokenType(bytes calldata data) private pure returns (TokenType) {
    return abi.decode(data, (TokenType));
  }

