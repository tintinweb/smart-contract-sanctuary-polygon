  bytes32 public constant trustedERC1155 = keccak256("ERC1155s that may be used to back CryptoJigToken");

  address private constant _osAddr     = 0x2953399124F0cBB46d2CbACD8A89cF0599974963;
  ERC1155 private constant _osContract = ERC1155(_osAddr);
  address private constant _osBurnAddr = 0x000000000000000000000000000000000000dEaD;

  uint8 private constant _stdRareVal  = 15;
  uint8 private constant _1stComVal   = 5;
  uint8 private constant _1stUncomVal = 20;
  uint8 private constant _1stRareVal  = 80;

  event RegisterType(address indexed contractAddr, uint256 indexed tokenId, TokenType indexed typ);
  event UnregisterType(address indexed contractAddr, uint256 indexed tokenId, TokenType indexed typ);
  event MintWithMatic(TokenType indexed typ, uint32 count);
  event MintWithNFTs(TokenType indexed typ, address indexed contractAddr, uint256 indexed tokenId, uint32 count);
  event RedeemForMatic(TokenType indexed typ, uint32 count);
  event RedeemForNFTs(TokenType indexed typ, address indexed contractAddr, uint256 indexed tokenId, uint32 count);
  event MultiplyValue(uint32 mult);

  /**
   * Attempt to redeem contractAddr-tokenId as type specTyp, but it's
   * registered as regTyp
   */
  error InconsistentTypeRedemption(
    address contractAddr,
    uint256 tokenId,
    TokenType specTyp,
    TokenType regTyp
  );

  /**
   * Payment for backed tokens must be an exact multiple of the type value.
   */
  error UnevenPayment(uint256 payment, uint256 typVal, TokenType typ);

  /**
   * multiplyValue must be called with the exact amount of payment to
   * appropriately bolster backing for matic-backed tokens.
   */
  error WrongPayment(uint256 reqdPayment, uint256 actualPayment);

  /**
   * Attempt to mintBacked or redeemForMatic with an unbacked type,
   * or mintUnbacked with a backed type.
   */
  error WrongBackingStatus(TokenType typ);

  /**
   * Attempted to redeem countRequested tokens of TokenType typ,
   * but only actualCount such tokens are appropriately backed.
   */
  error InsufficientCount(uint32 countRequested, uint32 actualCount, TokenType typ);

  // We must record the count backed by matic, per type, to prevent an NFT-backed
  // item leeching the funds of a matic-backed item.
  mapping(TokenType => uint32) public countBackedByMatic;

  /* only for testing
  /// #if_succeeds
  ///   countBackedByMatic[typ] > old(countBackedByMatic[typ]) ==> isBasicTrusted(msg.sender);
  /// #if_succeeds
