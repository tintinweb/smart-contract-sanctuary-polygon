  function _receiveTask(
    address contractAddr,
    uint256 tokenId,
    uint256 count,
    uint256 balance,
    TokenType typ
  ) private {
    bytes memory key = _getTypeRegistryKey(contractAddr, tokenId);

    // Check if there was a balance for this token prior to the transfer.
    if (balance > count) {
      // Since this token was in a pool, we must ensure we don't add it to some other pool.
      TokenType regTyp = _typeRegistry[key];
      require(_tokenTypsEq(regTyp, typ), "CryptoJigToken: Inconsistent type registration");
    } else {
      // _pushTypeRegistryKey(contractAddr, tokenId); only for testing
      _typeRegistry[key] = typ;
      emit RegisterType(contractAddr, tokenId, typ);
    }

    if (isBackedType(typ)) {
      emit MintWithNFTs(typ, contractAddr, tokenId, uint32(count));
    }
  }

  function _tokenTypsEq(TokenType t1, TokenType t2) private pure returns (bool) {
    return TokenType.unwrap(t1) == TokenType.unwrap(t2);
  }

  function _getTypeRegistryKey(address contractAddr, uint256 tokenId) private pure returns (bytes memory) {
    return abi.encodePacked(contractAddr, tokenId);
  }

  function _performBatch(
    address contractAddr,
    uint256[] memory tokenIds,
    uint256[] memory counts,
    TokenType typ,
    function (address, uint256, uint256, uint256, TokenType) task
  ) private returns (uint256) {
    uint256 tidsLen = tokenIds.length;
    require(tidsLen == counts.length, "CryptoJigToken: mismatched batch");

    ERC1155 otherContract = ERC1155(contractAddr);
    uint256[] memory balances;
    {
      address[] memory thisAddrN = new address[](tidsLen);
      for (uint256 i = 0; i < tidsLen; i++) {
        thisAddrN[i] = address(this);
      }
      balances = otherContract.balanceOfBatch(thisAddrN, tokenIds);
    }

    uint256 totalCount = 0;
    for (uint256 i = 0; i < tidsLen; i++) {
      uint256 tid = tokenIds[i];
      uint256 count = counts[i];
      uint256 balance = balances[i];
      require(count != 0, "CryptoJigToken: no count");
      task(contractAddr, tid, count, balance, typ);
      totalCount += count;
    }

    return totalCount;
  }

  function setURI(string memory uri) external onlyBasicTrusted {
    _setURI(uri);
  }

  /// TFW the spec is cleaner, more readable, and more efficient than the impl ...
  /// #if_succeeds  isBackedType(typ) ==> v >= 5 ether;
  /// #if_succeeds !isBackedType(typ) ==> v == 0;
  /// #if_succeeds TokenType.unwrap(typ) == 1 ==> v == 0;
  /// #if_succeeds TokenType.unwrap(typ) == 2 ==> v == 0;
  /// #if_succeeds TokenType.unwrap(typ) == 3 ==> v == _valMult * 15 ether;
  /// #if_succeeds TokenType.unwrap(typ) == 4 ==> v == _valMult *  5 ether;
  /// #if_succeeds TokenType.unwrap(typ) == 5 ==> v == _valMult * 20 ether;
  /// #if_succeeds TokenType.unwrap(typ) == 6 ==> v == _valMult * 80 ether;
  function getTypeVal(TokenType typ) public view returns (uint256 v) {
    uint256 val;
    if (ed(typ) == Edition.STD) {
      val = rarity(typ) == Rarity.RARE ? _stdRareVal : 0;
    } else if (rarity(typ) == Rarity.COMMON) {
      val = _1stComVal;
    } else if (rarity(typ) == Rarity.UNCOMMON) {
      val = _1stUncomVal;
    } else {
      val = _1stRareVal;
    }
    // Note that 'ether' is a numerical unit, so `1 ether` is actually 1 matic
    return val * _valMult * 1 ether;
  }

  /// #if_succeeds b == (ed(typ) == Edition.FIRST || rarity(typ) == Rarity.RARE);
  function isBackedType(TokenType typ) public pure returns (bool b) {
    return TokenType.unwrap(typ) >= 3;
  }

  function hasRegisteredType(address contractAddr, uint256 tokenId) public view returns (bool) {
    return ERC1155(contractAddr).balanceOf(address(this), tokenId) > 0;
  }

  function getRegisteredType(address contractAddr, uint256 tokenId) external view returns (TokenType) {
    if (!hasRegisteredType(contractAddr, tokenId)) { return TokenType.wrap(0); }
    bytes memory key = _getTypeRegistryKey(contractAddr, tokenId);
    return _typeRegistry[key];
  }

  function _beforeTokenTransfer(
      address op,
      address from,
      address to,
      uint256[] memory tokenIds,
      uint256[] memory counts,
      bytes memory data) internal override {
    require(to != _osBurnAddr, "CryptoJigToken: burn through a 'redeem' function - can't redeem by sending to 0xDEAD");
    super._beforeTokenTransfer(op, from, to, tokenIds, counts, data);
  }

  function getTokenId(TokenType typ) public pure returns (uint256) {
    return TokenType.unwrap(typ);
  }

  /// #if_succeeds _valMult > old(_valMult);
  /// #if_succeeds _valMult == old(_valMult) * mult;
  /// #if_succeeds msg.value == mult * old(_getAmtBacked()) - old(_getAmtBacked());
  /// #if_succeeds _getAmtBacked() == old(_getAmtBacked()) + msg.value;
  function multiplyValue(uint32 mult) external payable onlyBasicTrusted {
    require(mult > 1, "CryptoJigToken: multiplier must be increasing");
    uint256 payment = msg.value;
    uint256 reqdPayment = _getAmtBacked() * (mult - 1);
    if (payment != reqdPayment) { revert WrongPayment(reqdPayment, payment); }
    emit MultiplyValue(mult);
    _valMult *= mult;
  }

  function _getAmtBacked() private view returns (uint256) {
    return
      _getAmtBacked(mkTokenType(Edition.STD, Rarity.RARE)) +
      _getAmtBacked(mkTokenType(Edition.FIRST, Rarity.COMMON)) +
      _getAmtBacked(mkTokenType(Edition.FIRST, Rarity.UNCOMMON)) +
      _getAmtBacked(mkTokenType(Edition.FIRST, Rarity.RARE));
  }

  function _getAmtBacked(TokenType typ) private view returns (uint256) {
    uint256 typVal = getTypeVal(typ);
    return countBackedByMatic[typ] * typVal;
  }

  /// #if_succeeds tokenIds.length == counts.length;
  /// these ought to be forall, but can't be due to scribble limitations
  /// #if_succeeds
  ///   old(counts[0] <= ERC1155(contractAddr).balanceOf(address(this), tokenIds[0]) &&
  ///   _sumCounts(tokenIds[0], tokenIds, counts) <= ERC1155(contractAddr).balanceOf(address(this), tokenIds[0]));
  /// #if_succeeds
  ///   counts[0] == old(ERC1155(contractAddr).balanceOf(address(this), tokenIds[0])) ==>
  ///     TokenType.unwrap(_typeRegistry[_getTypeRegistryKey(contractAddr, tokenIds[0])]) == 0;
  /// #if_succeeds
  ///   !isBackedType(old(_typeRegistry[_getTypeRegistryKey(contractAddr, tokenIds[0])]));
  /// #if_succeeds
  ///   old(_sumCounts(tokenIds[0], tokenIds, counts) < ERC1155(contractAddr).balanceOf(address(this), tokenIds[0])) ==>
  ///     _tokenTypsEq(
  ///        old(_typeRegistry[_getTypeRegistryKey(msg.sender, tokenIds[0])]),
  ///            _typeRegistry[_getTypeRegistryKey(msg.sender, tokenIds[0])]);
  /// #if_succeeds
  ///   let ctr := old(ERC1155(contractAddr)) in
  ///   let tid := old(tokenIds[0]) in
  ///   ctr.balanceOf(msg.sender, tid) ==
  ///     old(ctr.balanceOf(msg.sender, tid)) + _sumCounts(tid, tokenIds, counts);
  /// #if_succeeds
  ///   let ctr := old(ERC1155(contractAddr)) in
  ///   let tid := old(tokenIds[0]) in
  ///   ctr.balanceOf(address(this), tid) ==
  ///     old(ctr.balanceOf(address(this), tid)) - _sumCounts(tid, tokenIds, counts);
  function withdrawUnbacked(address contractAddr, uint256[] calldata tokenIds, uint256[] calldata counts)
      external onlyBasicTrusted {
    _performBatch(contractAddr, tokenIds, counts, mkTokenType(Edition.STD, Rarity.COMMON), _withdrawTask);
    address redeemer = msg.sender;
    ERC1155(contractAddr).safeBatchTransferFrom(address(this), redeemer, tokenIds, counts, "");
  }

  function _withdrawTask(
    address contractAddr,
    uint256 tokenId,
    uint256 count,
    uint256 balance,
    TokenType
  ) private {
    bytes memory key = _getTypeRegistryKey(contractAddr, tokenId);
    TokenType regTyp = _typeRegistry[key];
    if (balance < count || TokenType.unwrap(regTyp) == 0 /* if a previous withdraw unreg'd */) {
      revert InsufficientCount(uint32(count), uint32(balance), regTyp);
    }
    if (isBackedType(regTyp)) { revert WrongBackingStatus(regTyp); }

    // There are several apparent vulnerabilities in batch operations, stemming
    // from the possibility for tokenIds to contain duplicates. However, the
    // checks should ensure that an attack of this nature will either eventually
    // revert, or produce the same black-box result as if the arrays were de-duped.
    // This is a bit subtle, but it comes from the fact that _typeRegistry is
    // only deleted when balance == count, and count cannot be 0, so trying to
    // break _typeRegistry will result in safeBatchTransferFrom reverting.
    if (balance == count) {
      _unregisterType(key, contractAddr, tokenId, regTyp);
    }
  }

  /// #if_succeeds !isBackedType(typ);
  /// #if_succeeds balanceOf(recipient, getTokenId(typ)) ==
  ///                old(balanceOf(recipient, getTokenId(typ))) + count;
  function mintUnbacked(address recipient, uint32 count, TokenType typ) external onlyBasicTrusted {
    checkTokenType(typ);
    if (isBackedType(typ)) { revert WrongBackingStatus(typ); }
    _mintUnsafe(recipient, count, typ);
  }

  /// #if_succeeds isBackedType(typ);
  /// #if_succeeds _getAmtBacked() - old(_getAmtBacked()) == msg.value;
  /// #if_succeeds _getAmtBacked(typ) - old(_getAmtBacked(typ)) == msg.value;
  /// #if_succeeds balanceOf(recipient, getTokenId(typ)) - old(balanceOf(recipient, getTokenId(typ)))
  ///                == msg.value / getTypeVal(typ);
  /// #if_succeeds msg.value % getTypeVal(typ) == 0;
  function mintBacked(address recipient, TokenType typ) external payable onlyBasicTrusted {
    uint256 oldAmtBacked = _getAmtBacked();
    checkTokenType(typ);
    if (!isBackedType(typ)) { revert WrongBackingStatus(typ); }
    uint256 typVal = getTypeVal(typ);
    uint256 payment = msg.value;
    require(payment != 0, "CryptoJigToken: no payment");
    // downcasting like this is unsafe, but whatever
    uint32 count = (uint32)(payment / typVal);
    if (payment % typVal != 0) {
      revert UnevenPayment(payment, typVal, typ);
    }
    countBackedByMatic[typ] += count;
    emit MintWithMatic(typ, count);
    /// #assert _getAmtBacked() - oldAmtBacked == count * getTypeVal(typ);
    _mintUnsafe(recipient, count, typ);
  }

  /// #if_succeeds
  ///   isBasicTrusted(msg.sender) || AUTH_CONTRACT.hasRole(trustedERC1155, msg.sender);
  function _mintUnsafe(address recipient, uint256 count, TokenType typ) private {
    // _totalSupply[typ] += count; only for testing
    _mint(recipient, getTokenId(typ), count, "");
  }

  /// #if_succeeds isBackedType(typ);
  /// #if_succeeds countBackedByMatic[typ] == old(countBackedByMatic[typ]) - count;
  /// #if_succeeds old(_getAmtBacked(typ)) - _getAmtBacked(typ) == count * getTypeVal(typ);
  /// #if_succeeds old(_getAmtBacked()) - _getAmtBacked() == count * getTypeVal(typ);
  /// #if_succeeds old(_getAmtBacked()) - _getAmtBacked() == old(address(this).balance) - address(this).balance;
  /// #if_succeeds old(_getAmtBacked()) - _getAmtBacked() == msg.sender.balance - old(msg.sender.balance);
  /// #if_succeeds balanceOf(msg.sender, getTokenId(typ)) == old(balanceOf(msg.sender, getTokenId(typ))) - count;
  function redeemForMatic(TokenType typ, uint32 count) external {
    checkTokenType(typ);
    if (!isBackedType(typ)) { revert WrongBackingStatus(typ); }
    require(count != 0, "CryptoJigToken: no count");

    uint32 _countBackedByMatic = countBackedByMatic[typ];
    if (count > _countBackedByMatic) { revert InsufficientCount(uint32(count), _countBackedByMatic, typ); }

    address redeemer = msg.sender;
    _burnUnsafe(redeemer, typ, count);
    countBackedByMatic[typ] -= count;
    (bool paySuccess, ) = redeemer.call{value: count * getTypeVal(typ)}("");
    require(paySuccess, "CryptoJigToken: redemption payment failed");
    emit RedeemForMatic(typ, count);
  }

  /// #if_succeeds tokenIds.length == counts.length;
  /// #if_succeeds balanceOf(msg.sender, getTokenId(typ)) ==
  ///   old(balanceOf(msg.sender, getTokenId(typ))) - unchecked_sum(counts);
  /// these ought to be forall, but can't be due to scribble limitations
  /// #if_succeeds
  ///   _tokenTypsEq(typ, old(_typeRegistry[_getTypeRegistryKey(contractAddr, tokenIds[0])]));
  /// #if_succeeds
  ///   counts[0] <= old(ERC1155(contractAddr).balanceOf(address(this), tokenIds[0])) &&
  ///   _sumCounts(tokenIds[0], tokenIds, counts) <= old(ERC1155(contractAddr).balanceOf(address(this), tokenIds[0]));
  /// #if_succeeds
  ///   old(ERC1155(contractAddr).balanceOf(address(this), tokenIds[0])) == counts[0] ==>
  ///     TokenType.unwrap(_typeRegistry[_getTypeRegistryKey(contractAddr, tokenIds[0])]) == 0;
  /// #if_succeeds
  ///   ERC1155(contractAddr).balanceOf(address(this), tokenIds[0]) > 0 ==>
  ///     _tokenTypsEq(typ, _typeRegistry[_getTypeRegistryKey(contractAddr, tokenIds[0])]);
  /// #if_succeeds
  ///   let ctr := old(ERC1155(contractAddr)) in
  ///   let tid := old(tokenIds[0]) in
  ///   ctr.balanceOf(msg.sender, tid) ==
  ///     old(ctr.balanceOf(msg.sender, tid)) + _sumCounts(tid, tokenIds, counts);
  /// #if_succeeds
  ///   let ctr := old(ERC1155(contractAddr)) in
  ///   let tid := old(tokenIds[0]) in
  ///   ctr.balanceOf(address(this), tid) ==
  ///     old(ctr.balanceOf(address(this), tid)) - _sumCounts(tid, tokenIds, counts);
  function redeemForNFTs(
      TokenType typ,
      address contractAddr,
      uint256[] calldata tokenIds,
      uint256[] calldata counts
  ) external {
    checkTokenType(typ);
    uint256 totalCount = _performBatch(contractAddr, tokenIds, counts, typ, _redeemTask);
    address redeemer = msg.sender;
    _burnUnsafe(redeemer, typ, totalCount);
    ERC1155(contractAddr).safeBatchTransferFrom(address(this), redeemer, tokenIds, counts, "");
  }

  function _burnUnsafe(address redeemer, TokenType typ, uint256 count) private {
    // _totalSupply[typ] -= count; only for testing
    _burn(redeemer, getTokenId(typ), count);
  }

  function _redeemTask(
    address contractAddr,
    uint256 tokenId,
    uint256 count,
    uint256 balance,
    TokenType typ
  ) private {
    if (balance < count) { revert InsufficientCount(uint32(count), uint32(balance), typ); }

    bytes memory key = _getTypeRegistryKey(contractAddr, tokenId);
    TokenType regTyp = _typeRegistry[key];
    if (!_tokenTypsEq(regTyp, typ)) {
      revert InconsistentTypeRedemption(contractAddr, tokenId, typ, regTyp);
    }

    emit RedeemForNFTs(typ, contractAddr, tokenId, uint32(count));
    if (balance == count) {
      _unregisterType(key, contractAddr, tokenId, regTyp);
    }
  }

  function _unregisterType(bytes memory key, address contractAddr, uint256 tokenId, TokenType typ) private {
    emit UnregisterType(contractAddr, tokenId, typ);
    // get a gas refund
    _typeRegistry[key] = TokenType.wrap(0);
  }
}