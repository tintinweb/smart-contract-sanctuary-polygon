contract MagikMakers is
ERC721A,
ERC721ABurnable,
ERC721AQueryable,
OwnableRoles,
ERC2981
 {

    constructor(
        string memory name,
        string memory symbol,
        string memory metadata
    ) ERC721A(name, symbol) {
        _normalUri = metadata;
        _initializeOwner(msg.sender);
        _grantRoles(0x8AB5496a45c92c36eC293d2681F1d3706eaff85D,1); // Grant Dev Admin roles
        _setDefaultRoyalty(0x16b3606003597134070f90365DD99913bF38d6c1, 1000); //Set 10% Royalty default.
    }

    event TokenURIChanged(uint256 indexed tokenId, string newURI);
    event DistributedViaBatch(address indexed recipient, uint256 amount);

    // ============================================================= //
    //                          Constant                             //
    // ============================================================= //
   
    //-------------Role based Permission------------//

    //@dev ADMIN_ROLE for performing admin actions.
    uint256 public constant EXTENSION_ROLE = _ROLE_3; 
    //@dev SUPPORT_ROLE  for performing maintaince  interactions.
    uint256 public constant SUPPORT_ROLE = _ROLE_2; 
    //@dev MINTER_ROLE every minter module must have in order to mint new tokens. 
    uint256 public constant MINTER_ROLE = _ROLE_1; 
    //@dev ADMIN_ROLE for performing admin actions.
    uint256 public constant ADMIN_ROLE = _ROLE_0;


    // ============================================================= //
    //                           STORAGE                             //
    // ============================================================= //


    // Distribution Status
    bool public distributionIsLive = true ; 
    //Supply Limits & Locks
    uint256 public _maxSupply = 500;
    bool private _maxSupplyLocked; 
    // Hubspot Safe Address
    address public hubspotSafe = 0x16b3606003597134070f90365DD99913bF38d6c1; 
    // Authorized Contract for potential burn/migration functionality
    address private _authorizedContract;
    // Address that will link [Redacted]
    address public magicContractAddress;
    //Metadata Types
    string public _normalUri;

	mapping (uint256 => string) customURIs;

    // Limit mint to 1 per address - maybe change to
    mapping(address => uint256) public _mintCounter;

    // ============================================================= //
    //                       METADATA CNTRL                          //
    // ============================================================= //

    /**
     * @notice Flexible URI controls that defaults to base token. Allows for indiviudal token specific modification/upgradability.
     */

    function setNewTokenURI(uint256 typeOfURI, uint256 tokenId, string calldata newURI) external onlyRolesOrOwner(ADMIN_ROLE | SUPPORT_ROLE){
        if(typeOfURI == 0) 
            _normalUri = newURI;
        else 
            customURIs[tokenId] = newURI;
            emit TokenURIChanged(tokenId, newURI);
    }

    //Returns the URI for a specfic Token
    //Returns concatenated base URI if no custom URI is set
    function tokenURI(uint256 tokenId) public view virtual override(ERC721A,IERC721A) returns (string memory) {
    require(_exists(tokenId), "URIQueryForNonexistentToken");

    if (bytes(customURIs[tokenId]).length != 0) {
        return customURIs[tokenId];
    } else {
        string memory baseURI = _normalUri;
        return bytes(baseURI).length != 0 ? string(abi.encodePacked(baseURI, _toString(tokenId), ".json")) : '';
    }
    }

    //Clears customURI of a token and restores to normalURI
    function resetURI(uint256 tokenId) external onlyRolesOrOwner(ADMIN_ROLE | SUPPORT_ROLE) {
    customURIs[tokenId] = "";
    }

    function _startTokenId() internal view virtual override returns (uint256) {
    return 1;
    }

    
    // ============================================================= //
    //                      DISTRIBUTION  CNTRL                      //
    // ============================================================= //

    //
    function adminMint(address to, uint256 amount) external onlyRolesOrOwner(ADMIN_ROLE) {
        require(totalSupply() + amount <= _maxSupply, "Quantity exceeds supply");
        _safeMint(to,amount);
    }

    // Primary Distribution method 
    function distribute(address to) external onlyRolesOrOwner(MINTER_ROLE)  {
        require(distributionIsLive, "Distibution is not open right now");
        require(totalSupply() + 1 <= _maxSupply, "Quantity exceeds supply");
        require(checkMints(to) < 1 , "User has already minted");
        _safeMint(to, 1);
        _mintCounter[to] = _mintCounter[to] + 1;

    }

    // Batch Distribution method - will not increment mintcounter but will abide by total supply limiations.
    function distributeBatch(address[] calldata recipients, uint256[] calldata amounts) external onlyRolesOrOwner(ADMIN_ROLE)  {
    require(recipients.length == amounts.length, "Recipients and amounts length mismatch");
    
         // Calculate the total amount to be minted
         uint256 totalAmount = 0;
          for (uint256 i = 0; i < amounts.length; i++) {
        totalAmount += amounts[i];
        }
                //check
        // Check that the total amount doesn't exceed the available supply
            require(totalSupply() + totalAmount <= _maxSupply, "Quantity exceeds supply");

            for (uint256 i = 0; i < recipients.length; i++) {
            _safeMint(recipients[i], amounts[i]);

             emit DistributedViaBatch(recipients[i], amounts[i]);

        }
    }

    //Batch Disperse - will disperse 1 token to each address in the array, abides by mintcouner and total supply limitations.
    function batchDisperse(address[] memory recipients) onlyRolesOrOwner(ADMIN_ROLE) external {
    // Calculate the total amount to be minted
    uint256 totalAmount = 0;
    for (uint256 i = 0; i < recipients.length; i++) {
        if (checkMints(recipients[i]) < 1) {
            totalAmount += 1;
        }
    }

    // Check that the total amount doesn't exceed the available supply
    require(totalSupply() + totalAmount <= _maxSupply, "Quantity exceeds supply");

    for (uint256 i = 0; i < recipients.length; i++) {
        address recipient = recipients[i];
        if (checkMints(recipient) < 1) {
            _safeMint(recipient, 1);
            _mintCounter[recipient] = 1;
            }
        }
    }

    //backup distribution method for future proofing, will not increment mintcounter but will abide by total supply limiations.
   function extendibleDistribute (address to, uint256 amount) external onlyRolesOrOwner(EXTENSION_ROLE) {
        require(totalSupply() + amount <= _maxSupply, "Quantity exceeds supply");
        _safeMint(to,amount);
    }



    // Toggle Distribution Status
    function toggleDistribution() public onlyRolesOrOwner(ADMIN_ROLE) {
        distributionIsLive = !distributionIsLive;
    }


    // ============================================================= //
    //                         SUPPLY  CNTRL                         //
    // ============================================================= //

    //Set Max Supply
    function setMaxSupply(uint256 supply) external onlyRolesOrOwner(ADMIN_ROLE) {
        require(!_maxSupplyLocked, "Max supply is locked");
        _maxSupply = supply;
    }

    // Locks maximum supply forever
    function lockMaxSupply() external onlyRolesOrOwner(ADMIN_ROLE) {
        _maxSupplyLocked = true;
    }

    function checkMints(address user) public view returns(uint256){
         return _mintCounter[user];
    }

    // Only the owner of the token and its approved operators, 
    // and the authorized contract can call this function.
    function burn(uint256 tokenId) public virtual override {
        // Avoid unnecessary approvals for the authorized contract
        bool approvalCheck = msg.sender != _authorizedContract;
        _burn(tokenId, approvalCheck);
    }

    // ============================================================= //
    //                         ACCESS  CNTRL                         //
    // ============================================================= //

    // Additional Role controls for permissioned users
    // if action = 1 = grant role, if else revoke
    function AdjustRoles(address user, uint256 roles, uint256 action) public onlyRolesOrOwner(ADMIN_ROLE) {
        if (action == 1)
        _grantRoles(user, roles);
        else
        _removeRoles(user, roles);
    }

    // Authoried Contract for burn/migration functionality
    function setAuthorizedContract(address authorizedContract) public onlyRolesOrOwner(ADMIN_ROLE) {
        _authorizedContract = authorizedContract;
    }

    function initializeRelayers(address[] calldata relayers) external onlyRolesOrOwner(ADMIN_ROLE) {
    // Iterate through the array of addresses
    for (uint256 i = 0; i < relayers.length; i++) {
        // Grant role 2 to each address in the array
        _grantRoles(relayers[i], 2);
    }
}
    // ============================================================= //
    //                           IERC2981                            //       
    // ============================================================= //   

    /**
    @notice Sets the contract-wide royalty info.
     */
    function setRoyaltyInfo(
        address receiver,
        uint96 feeBasisPoints
    ) external onlyOwner{
        _setDefaultRoyalty(receiver, feeBasisPoints);
    }


    // =============================================================
    //                            IERC165
    // =============================================================

    /**
     * @dev Returns true if this contract implements the interface defined by
     * `interfaceId`. See the corresponding
     * [EIP section](https://eips.ethereum.org/EIPS/eip-165#how-interfaces-are-identified)
     * to learn more about how these ids are created.
     *
     * This function call must use less than 30000 gas.
     */
    function supportsInterface(
        bytes4 interfaceId
    ) public view virtual override(ERC721A, IERC721A, ERC2981) returns (bool) {
        // The interface IDs are constants representing the first 4 bytes
        // of the XOR of all function selectors in the interface.
        // See: [ERC165](https://eips.ethereum.org/EIPS/eip-165)
        // (e.g. `bytes4(i.functionA.selector ^ i.functionB.selector ^ ...)`)
        return
            ERC721A.supportsInterface(interfaceId) ||
            ERC2981.supportsInterface(interfaceId);
    }

    // =============================================================
    //                     SECRET CONTROLS
    // =============================================================

    // IYKYK
    function setMagicContract(address magicAddress) external onlyRolesOrOwner(ADMIN_ROLE) {
        magicContractAddress = magicAddress;
    }

}


