pragma solidity ^0.8.0;

import "./Factory.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import {IUniswapV2Router02} from "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import {IProjectsManager} from "./interfaces/IProjectsManager.sol";

contract ProjectsManager is IProjectsManager, ERC721URIStorage, AccessControl {
    using Counters for Counters.Counter;
    using Strings for uint256;


    Counters.Counter private _ProjectsIds;
    mapping(uint256 => ProjectDetails) private _projectDetails;
    mapping(uint256 => TokenDetails) private _tokenDetails;
    mapping(address => bool) private _whitelist;
    Factory public tokenFactory;

    //access variables
	///@notice role required to launch projects
    ///@notice this will be used to grants permission to the protocol voting system
	bytes32 public constant LAUNCHER = keccak256("MINTER");
	///@notice Role required to manipulate admin functions
	bytes32 public constant ADMIN = keccak256("ADMIN");

    event CreateProject(
        uint256 indexed projectID,
        string name,
        string description,
        address indexed owner
    );

    event LaunchToken(
        uint256 indexed projectID,
        address  indexed tokenAddress,
        string name,
        string symbol,
        uint256 initialSupply,
        uint8 decimals
    );

    constructor(
        string memory name,
        string memory symbol
    ) ERC721(
        name,
        symbol
    ) {
        _grantRole(ADMIN, msg.sender);
    }

    ///@dev necessary override in order to use both ERC721 and AccessControl
    function supportsInterface(bytes4 interfaceId)
	public
	view
	virtual
	override(ERC721, AccessControl)
	returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    /**
     * @notice Create a new project in the Protocol. By default the project starts unlaunched.
     * @notice It will only be launched after win a votation in the protocol votation system
     * @param owner The owner of the new project
     * @param name Project name
     * @param description Project description
     */
    function createProject(
        address owner,
        string memory name,
        string memory description
        )
        public
        override
        returns (uint256)
    {
        _ProjectsIds.increment();
        uint256 newItemId = _ProjectsIds.current();
        ProjectDetails storage project = _projectDetails[newItemId];
        project.name = name;
        project.description = description;
        project.owner = owner;
        project.launched = false;

        _mint(owner, newItemId);
        // _setTokenURI(newItemId, _getProjectURI(project));

        emit CreateProject(newItemId, name, description, owner);
        return newItemId;
    }

    /**
     * @notice Get project metadata URI
     * @param projectID Project identifier
     */
    function getProjectURI(uint256 projectID) public override view returns(string memory) {
        ProjectDetails memory projectData = _projectDetails[projectID];

        bytes memory dataURI = abi.encodePacked(
            '{',
                '"name":"', projectData.name, '",',
                '"description":"', projectData.description, '",',
                '"owner":"', Strings.toHexString(uint256(uint160(projectData.owner))), '",',
                '"status":"', projectData.launched ? 'launched' : 'notLaunched', '",',
                '"token":"', Strings.toHexString(uint256(uint160(projectData.token))) , '"'
            '}'
        );

        return string(
            abi.encodePacked(
                "data:application/json;base64",
                Base64.encode(dataURI)
            )
        );
    }

    /**
    * @notice launch project to be able to assign its token
    * @dev switch the project launched state to true
    * @param projectID project identifier
     */
    function launchProject(uint256 projectID) public override  {
        require(hasRole(LAUNCHER, msg.sender), 'address does not have permissions to launch projects');
        ProjectDetails storage project = _projectDetails[projectID];
        project.launched = true;
    }


    /**
     * @notice launch project token
     * @param projectID project id
     * @param name token name
     * @param symbol token symbol
     * @param decimals token decimals
     * @param initialSupply initial supply
     */
    function launchToken(
        uint256 projectID,
        string memory name,
        string memory symbol,
        uint8 decimals,
        uint256 initialSupply
    ) public override returns(address) {
        ProjectDetails storage project = _projectDetails[projectID];
        TokenDetails storage tokenDetails = _tokenDetails[projectID];
        require(project.owner == msg.sender, "Manager: unauthorized caller");
        require(project.launched == true, "Manager: project not launched yet");
        require(address(tokenFactory) != address(0), "Manager: token factory not setted yet");
        address token = tokenFactory.deployNewERC20Token(name, symbol, decimals, initialSupply, project.owner);
        project.token = token;
        tokenDetails.totalSupply = initialSupply * 10 ** uint256(decimals);
        tokenDetails.decimals = decimals;

        emit LaunchToken(projectID, token, name, symbol, initialSupply, decimals);
        return token;
    }

    /**
     * @notice Create a liquidity pool of the project token and the network native token
     * @notice Sender should have already given the contract allowance of at least amountTokenDesired
     * @param projectID project id
     * @param liquidityRouter router contract used to create the liquidity pool
     * @param amountTokenDesired amount of contribution token
     * @param amountTokenMin min amount of contribution token
     * @param amountEthMin min amount of ETH 
     * @param deadline time to unlock liquidity
     */
    function createTokenLiquidity(
        uint256 projectID,
        address liquidityRouter,
        uint256 amountTokenDesired,
        uint256 amountTokenMin,
        uint256 amountEthMin,
        uint256 deadline
    )  public override payable {
        ProjectDetails storage project = _projectDetails[projectID];
        TokenDetails storage tokenDetails = _tokenDetails[projectID];
        require(project.token != address(0), "Manager: Token not launched yet");
        require(project.owner == msg.sender, "Manager: unauthorized caller");
        require(tokenDetails.poolLiquidity == 0, "Manager: Liquidity pool already created");
        ERC20(project.token).transferFrom(project.owner, address(this), amountTokenDesired);
        ERC20(project.token).approve(liquidityRouter, amountTokenDesired);
        (
            uint256 amountToken,
            uint256 amountETH,
            uint256 liquidity
        ) = IUniswapV2Router02(liquidityRouter).addLiquidityETH{
                value: msg.value
            }(
            project.token,
            amountTokenDesired,
            amountTokenMin,
            amountEthMin,
            msg.sender,
            deadline
        );
        
        tokenDetails.liquidityTokenAmount = amountToken;
        tokenDetails.liquidityEthAmount = amountETH;
        tokenDetails.poolLiquidity = liquidity;
    }

    /**
    * @notice Gets if the project was already launched
    * @param projectID project identifier
    * @return boolean indicates the project state
    */
    function projectIsLaunched(uint256 projectID) public override view returns(bool) {
        ProjectDetails memory project = _projectDetails[projectID];

        return project.launched;
    }

    /**
     * @notice get project token contract address if exists
     * @dev if the project does not have a token, returns address 0
     * @param projectID project identifier
     */
    function getProjectToken(uint256 projectID) public override  view returns (address) {
        return _projectDetails[projectID].token;
    }
    
    /**
     * @notice Gets the number of projects created in the protocol
     */
    function getProjectsCount() public override view returns(uint256) {
        return _ProjectsIds.current();
    }


    /**
     * @notice Remove from whitelist of addresses allowed to launch projects
     */
    function addToWhitelist(address[] calldata toAddAddresses) external override {
        require(hasRole(ADMIN, msg.sender), 'address does not have permissions to perform this action');
        for (uint i = 0; i < toAddAddresses.length; i++) {
            _grantRole(LAUNCHER, toAddAddresses[i]);
        }
    }

    /**
     * @notice Remove from whitelist of addresses allowed to launch projects
     */
    function removeFromWhitelist(address[] calldata toRemoveAddresses) external override {
        require(hasRole(ADMIN, msg.sender), 'address does not have permissions to perform this action');
        for (uint i = 0; i < toRemoveAddresses.length; i++) {
            _revokeRole(LAUNCHER, toRemoveAddresses[i]);
        }
    }

    function isInWhitelist(address _who) external override view returns(bool) {
        return hasRole(LAUNCHER, _who);
    }


    
    function setFactory(address _factory) public override {
        require(hasRole(ADMIN, msg.sender), 'Manager: unauthorized caller');
        tokenFactory = Factory(_factory);
    }

    /**
     * @notice get project token details
     * @param projectID project unique identifier
     */
    function getTokenDetails(uint256 projectID) public override view returns (TokenDetails memory tokenDetails) {
        tokenDetails = _tokenDetails[projectID];
    }

    /**
     * @notice get Project details
     * @param projectID project unique identifier
     */
    function getProjectDetails(uint256 projectID) public override view returns (ProjectDetails memory projectDetails) {
        projectDetails = _projectDetails[projectID];
    }
}

