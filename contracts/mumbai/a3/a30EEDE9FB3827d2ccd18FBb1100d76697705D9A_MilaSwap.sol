    struct internalVariables {
        address milaToken;
        address stableToken;
        address vault;
        uint milaPrice;
        uint fee;
    }

    internalVariables public sysVars;
    function setVars(
        address _milaToken,
        address _stableToken,
        address _vault,
        uint _milaPrice,
        uint _fee
    ) external onlyRole(EDITOR_ROLE) {
        if (_milaToken != address(0)) {
            sysVars.milaToken = _milaToken;
        }
        if (_stableToken != address(0)) {
            sysVars.stableToken = _stableToken;
        }
        if (_vault != address(0)) {
