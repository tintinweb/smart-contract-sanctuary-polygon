pragma solidity >=0.8.0 <0.9.0;

contract NFTRaffle is AccessControl, ReentrancyGuard, VRFConsumerBaseV2, ConfirmedOwner {
    Config public config;
    VRFCoordinatorV2Interface private coordinator;
    using SafeERC20 for IERC20;
    TokenInterface public token;

    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR");

    address public TokenContract;
    address public constant BurnAddress = address(0);// Dead address for token burn

    mapping(address => bool) public adminList;

    mapping(uint64 => RaffleStruct) public raffleMap;
    mapping(uint64 => ticketsBought[]) public ticketsMap;
    mapping(uint256 => uint64) public randomRaffleMap;
    mapping(address => mapping(uint64 => uint16)) public purchasedTickets;
    mapping(uint64 => Participant[]) public participants;

    event RaffleCreate(uint64 indexed _raffleId, address indexed _nftAddress, uint32 indexed _nftId);
    event RaffleSold(uint64 indexed _raffleId, uint32 indexed _ticketId, address indexed _owner);
    event RaffleClaim(uint64 indexed _raffleId, address indexed _user, CLAIM_TYPE _type);
    event RaffleStatus(uint64 indexed _raffleId, STATUS indexed status);
    event ConfigChanged(uint32 _fee, address _feeReceiver);

    modifier onlyAdmin() {
        require(msg.sender == owner() || adminList[msg.sender], "only admins can create raffle!");
        _;
    }

    // MUMBAI:
    // Coordinator: 0x7a1BaC17Ccc5b313516C5E16fb24f7659aA5ebed
    // KeyHash: 0x4b09e658ed251bcafeebbc69400383d49f344ace09b9576fe248bb02c003fe9f
    // MAINNET:
    // Coordinator: 0xAE975071Be8F8eE67addBC1A82488F1C24858067
    // KeyHash: 0x6e099d640cde6de9d40ac749b4b594126b0169747122711109c9985d47751f93
    constructor(uint64 _subId, address _vrfCoordinator, bytes32 _keyHash, address _tokenContract) VRFConsumerBaseV2(_vrfCoordinator) ConfirmedOwner(msg.sender) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

        config = Config({
        subId : _subId,
        raffleIndex : 0,
        vrfGasLimit : 2500000,
        vrfConfirms : 3,
        maxSupply : 150,
        maxPurchaseLimit : 100,
        treasuryWallet : msg.sender,
        burnFee : 2000, //2%
        keyHash : _keyHash
        });
        coordinator = VRFCoordinatorV2Interface(_vrfCoordinator);

        adminList[msg.sender] = true;

        TokenContract = _tokenContract;
        token = TokenInterface(_tokenContract);
    }

    // NFT Approve is required before calling
    function createRaffle(address _nftAddress, uint32 _nftId, uint32 _price, uint16 _totalSupply, uint32 _expirationTime) external onlyAdmin nonReentrant returns (uint64) {
        if (_totalSupply < 2 || _totalSupply > config.maxSupply) revert("SupplyLimited");

        // The maximum number of holder conditions can only be set to 3, and cannot be set to empty

        IERC721 asset = IERC721(_nftAddress);
        asset.transferFrom(msg.sender, address(this), _nftId);

        RaffleStruct memory raffle = RaffleStruct({
        nftAddress : _nftAddress,
        nftId : _nftId,
        expirationTime : _expirationTime,
        totalSupply : _totalSupply,
        currentSupply : 0,
        creator : msg.sender,
        price : _price,
        randomIndex : 0,
        winner : address(0),
        settled : false,
        status : STATUS.NORMAL
        });
        raffleMap[++config.raffleIndex] = raffle;

        emit RaffleCreate(config.raffleIndex, _nftAddress, _nftId);
        return config.raffleIndex;
    }

    function buyTicket(uint64 _raffleId, uint16 _qty, uint256 _price) external nonReentrant {
        ticket(_raffleId, _qty, msg.sender, _price);
    }

    function presentTicket(uint64 _raffleId, uint16 _qty, address _receiptAddress, uint256 _price) external nonReentrant {
        ticket(_raffleId, _qty, _receiptAddress, _price);
    }


    //need to token allowance before sending to this contract
    function ticket(uint64 _raffleId, uint16 _qty, address _receiptAddress, uint256 _price) internal {
        RaffleStruct memory raffle = raffleMap[_raffleId];
        if (raffle.status != STATUS.NORMAL) revert("Wrong status");
        if (msg.sender == raffle.creator) revert("Disallow creator");
        if (_price != uint256(raffle.price) * _qty * 1e18) revert("Incorrect amount");
        if (_qty == 0 || _qty > raffle.totalSupply - raffle.currentSupply || purchasedTickets[_receiptAddress][_raffleId] + _qty > config.maxPurchaseLimit) revert("Wrong entries");

        // check holder only
        uint32 ticketId = uint32(ticketsMap[_raffleId].length);
        ticketsBought memory ticketBought = ticketsBought({
        owner : _receiptAddress,
        currentIndex : raffle.currentSupply + _qty,
        ticketNum : _qty,
        ticketId : ticketId
        });
        ticketsMap[_raffleId].push(ticketBought);

        Participant memory part = Participant({
        owner : _receiptAddress,
        ticketAmount : purchasedTickets[_receiptAddress][_raffleId] + _qty
        });

        if (checkParticipant(_raffleId, _receiptAddress)) {
            updateParticipant(_raffleId, _receiptAddress, purchasedTickets[_receiptAddress][_raffleId] + _qty);
        } else {
            participants[_raffleId].push(part);
        }

        raffleMap[_raffleId].currentSupply += _qty;

        purchasedTickets[_receiptAddress][_raffleId] += _qty;

        // fee management
        uint256 fee = _price * config.burnFee / 10000;
        IERC20(TokenContract).safeTransferFrom(msg.sender, config.treasuryWallet, fee);
        uint256 burnAmount = _price - fee;
        token.burn(msg.sender, burnAmount);

        if (raffle.currentSupply == raffle.totalSupply) {
            draw(_raffleId);
        }
        emit RaffleSold(_raffleId, ticketId, msg.sender);
    }

    function updateParticipant(uint64 _raffleId, address _owner, uint16 _ticketAmount) internal {
        Participant[] storage part = participants[_raffleId];
        for (uint256 i = 0; i < part.length; i++) {
            if (part[i].owner == _owner) {
                part[i].ticketAmount = _ticketAmount;
                break;
            }
        }
    }

    function checkParticipant(uint64 _raffleId, address _owner) internal view returns (bool) {
        Participant[] memory part = participants[_raffleId];
        for (uint256 i = 0; i < part.length; i++) {
            if (part[i].owner == _owner) {
                return true;
            }
        }
        return false;
    }

    // should add consumer on chainlink website before draw
    function draw(uint64 _raffleId) public nonReentrant {
        RaffleStruct memory raffle = raffleMap[_raffleId];

        // Unsold entries cannot draw
        if (raffle.status != STATUS.NORMAL || raffle.currentSupply == 0) revert("Wrong status");

        if (raffle.currentSupply == raffle.totalSupply || block.timestamp > uint256(raffle.expirationTime)) {
            if (raffle.currentSupply == raffle.totalSupply) {
                requestRandomNumber(_raffleId);
                raffleMap[_raffleId].status = STATUS.DRAWING;
                winnerClaim(_raffleId);
            } else {
                if (msg.sender != owner() || adminList[msg.sender] == false) revert("Not admin");

                requestRandomNumber(_raffleId);
                raffleMap[_raffleId].status = STATUS.DRAWING;
                winnerClaim(_raffleId);
            }

        } else {
            // Not sold out and under the deadline
            revert("Not sold out & deadline");
        }

        emit RaffleStatus(_raffleId, STATUS.DRAWING);
    }

    function cancelRaffle(uint64 _raffleId) external nonReentrant {
        RaffleStruct memory raffle = raffleMap[_raffleId];

        if (msg.sender != raffle.creator) revert("Not owned");
        if (raffle.currentSupply > 0) revert("Already sold");

        IERC721 asset = IERC721(raffle.nftAddress);
        asset.transferFrom(address(this), msg.sender, raffle.nftId);

        raffleMap[_raffleId].status = STATUS.CANCELLED;
        emit RaffleStatus(_raffleId, STATUS.CANCELLED);
    }

    function requestRandomNumber(uint64 _raffleId) internal {
        uint256 requestId = coordinator.requestRandomWords(config.keyHash, config.subId, config.vrfConfirms, config.vrfGasLimit, 1);
        randomRaffleMap[requestId] = _raffleId;
    }

    function fulfillRandomWords(uint256 _requestId, uint256[] memory _randomWords) internal override {
        uint64 raffleId = randomRaffleMap[_requestId];
        RaffleStruct memory raffle = raffleMap[raffleId];

        // random index in [1 - sold numbers]
        uint256 randomIndex = (_randomWords[0] % raffle.currentSupply) + 1;
        raffleMap[raffleId].randomIndex = uint16(randomIndex);

        raffleMap[raffleId].status = STATUS.DRAWN;

        emit RaffleStatus(raffleId, STATUS.DRAWN);
    }

    function winnerClaim(uint64 _raffleId) internal {
        RaffleStruct memory raffle = raffleMap[_raffleId];

        address winner = calRandomWinner(_raffleId, raffle.randomIndex);

        IERC721 asset = IERC721(raffle.nftAddress);
        asset.transferFrom(address(this), winner, raffle.nftId);

        raffleMap[_raffleId].status = STATUS.CLAIMED;
        raffleMap[_raffleId].winner = winner;
        emit RaffleClaim(_raffleId, winner, CLAIM_TYPE.WINNER);
    }

    function calRandomWinner(uint64 _raffleId, uint256 _randomIndex) public view returns (address) {
        uint256 position = findUpperBound(ticketsMap[_raffleId], _randomIndex);
        return ticketsMap[_raffleId][position].owner;
    }

    /// https://docs.openzeppelin.com/contracts/3.x/api/utils#Arrays-findUpperBound-uint256---uint256-
    function findUpperBound(ticketsBought[] memory array, uint256 randomIndex) internal pure returns (uint256) {
        if (array.length == 0) return 0;

        uint256 low = 0;
        uint256 high = array.length;

        while (low < high) {
            uint256 mid = Math.average(low, high);
            if (array[mid].currentIndex > randomIndex) {
                high = mid;
            } else {
                low = mid + 1;
            }
        }

        if (low > 0 && array[low - 1].currentIndex == randomIndex) {
            return low - 1;
        } else {
            return low;
        }
    }


    function getParticipants(uint64 _raffleId) external view returns (Participant[] memory) {
        return participants[_raffleId];
    }


    // Verifying the signer role
    function verifySignature(uint64 _raffleId, ClaimData memory _claimData) internal view returns (bool) {
        bytes32 messagehash = keccak256(abi.encodePacked(_raffleId, _claimData.platformFeeOff, _claimData.linkFee, address(this), getChainID()));
        address signer = ECDSA.recover(ECDSA.toEthSignedMessageHash(messagehash), _claimData.signature);
        return hasRole(OPERATOR_ROLE, signer);
    }

    function setBurnFee(uint32 _fee, address payable _treasuryWallet) external onlyRole(DEFAULT_ADMIN_ROLE) {
        config.burnFee = _fee;
        config.treasuryWallet = _treasuryWallet;
        emit ConfigChanged(_fee, _treasuryWallet);
    }

    function setMaximum(uint16 _totalSupply, uint16 _maxPurchaseLimit) external onlyRole(DEFAULT_ADMIN_ROLE) {
        config.maxSupply = _totalSupply;
        config.maxPurchaseLimit = _maxPurchaseLimit;
    }

    function setChainlink(uint64 _subId, bytes32 _keyHash, uint32 _vrfGasLimit, uint16 _vrfConfirms) external onlyRole(DEFAULT_ADMIN_ROLE) {
        config.subId = _subId;
        config.keyHash = _keyHash;
        config.vrfGasLimit = _vrfGasLimit;
        config.vrfConfirms = _vrfConfirms;
    }

    function setAdmin(address _admin, bool _status) external onlyRole(DEFAULT_ADMIN_ROLE) {
        adminList[_admin] = _status;
    }

    function getTicketsBought(uint64 _raffleId) external view returns (ticketsBought[] memory) {
        return ticketsMap[_raffleId];
    }

    function getRaffles() external view returns (RaffleStruct[] memory) {
        RaffleStruct[] memory raffles = new RaffleStruct[](config.raffleIndex);
        for (uint64 i = 0; i < config.raffleIndex; i++) {
            raffles[i] = raffleMap[i + 1];
        }
        return raffles;
    }

    function getChainID() private view returns (uint256 id) {
        assembly {
            id := chainid()
        }
    }

    function accidentFix() external onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 balance = IERC20(TokenContract).balanceOf(address(this));
        IERC20(TokenContract).safeTransfer(config.treasuryWallet, balance);
    }
}