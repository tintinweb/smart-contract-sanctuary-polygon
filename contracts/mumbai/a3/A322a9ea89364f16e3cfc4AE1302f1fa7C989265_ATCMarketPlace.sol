// SPDX-License-Identifier: MIT
pragma solidity ^0.8.17;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

import "./interfaces/IATCNFT.sol";

contract ATCMarketPlace is Ownable{

    /**
     * -----------------------------------------------------------------------------------------------------
     *                                      ATRIBUTOS
     * -----------------------------------------------------------------------------------------------------
     */

    //Relaciona cada venta con su ID.
    //El ID de las ventas empieza en 1.
    mapping (uint256 => Sale) public sales;
    //Relaciona el ID del NFT con el ID de la venta en la que esta siendo vendido.
    mapping(uint256 => uint256) NFTtoSale;

    //Counter para generar los IDs de las ventas.
    using Counters for Counters.Counter;
    Counters.Counter counterIDs;
    //Counter para saber cuantas ventas abiertas hay.
    //Aumenta cuando se abre una venta. Disminuye cuando se ejecuta/cancela una venta.
    Counters.Counter counterOpenSales;

    //Instalcia del contrato ERC20 - ATCcoin.
    IERC20 ATCcoin;
    //Instalcia del contrato ERC721 - ATCNFT.
    IATCNFT ATCNFT;
    //Numero de decimales del token ERC20 - ATCcoin.
    uint256 ATCcoinDecimals = 100;

    //Enumeracion con los posibles estados de una venta.
    // enum SaleStatus{
    //     open,
    //     cancelled,
    //     executed
    // }
    //Variables tipo string para los posibles estados de una venta.
    string statusOpen = "open";
    string statusCancelled = "cancelled";
    string statusExecuted = "executed";

    //Struct con el contenido de una venta.
    //Cada instancia de este struct es una nueva venta.
    struct Sale{
        //Address del dueÃ±o del NFT. Quien va a recibir el dinero.
        address owner;
        //ID del token NFT a la venta.
        uint256 tokenID;
        //Precio a pagar para comprar el NFT.
        uint256 price;
        //Estado de la venta.
        string status; 
        //Tipo del token a la venta.
        uint8 typeToken;
    }

    /**
     * -----------------------------------------------------------------------------------------------------
     *                                      MODIFIERS
     * -----------------------------------------------------------------------------------------------------
     */
    
    //Comprobar que el ID del token existe, que el NFT ha sido minteado.
    modifier TokenExists(uint256 _tokenID){
        require(ATCNFT.ownerOf(_tokenID) != address(0), "TokenExists - el NFT no existe.");
        _;
    }

    //Comprueba que el usuario que realiza la llamada es el propietario del NFT.
    modifier UserOwnsToken(uint256 _tokenID){
        require(ATCNFT.ownerOf(_tokenID) == msg.sender, "CallerOwnsToken - usuario no es el propietario del NFT.");
        _;
    }

    //Comprueba que la venta existe.
    modifier SaleExists(uint256 _saleID){
        require(_saleID>=1 && _saleID<=counterIDs.current(),"SaleExists - no existe el identificador de venta");
        _;
    }

    //Comprueba que el usuario que realiza la llamada es el propietario de la venta.
    modifier UserOwnsSale(uint256 _saleID){
        require(sales[_saleID].owner == msg.sender, "UserOwnsSale - usuario no es el propietario de la venta.");
        _;
    }

    modifier SaleIsOpen(uint256 _saleID){
        require(keccak256(bytes(sales[_saleID].status)) == keccak256(bytes(statusOpen)), "SaleIsOpen - la venta no esta abierta.");
        _;
    }
    
    /**
     * -----------------------------------------------------------------------------------------------------
     *                                      CONSTRUCTOR
     * -----------------------------------------------------------------------------------------------------
     */
    constructor(address _ATCcoinAddress, address _ATCNFTAddress){
        ATCcoin = IERC20(_ATCcoinAddress);
        ATCNFT = IATCNFT(_ATCNFTAddress);
    }

    /**
     * -----------------------------------------------------------------------------------------------------
     *                                      FUNCIONES
     * -----------------------------------------------------------------------------------------------------
     */

    function getSale(uint256 index) public view SaleExists(index) returns(Sale memory){
        return sales[index];
    }

    function getCounterCurrent() public view returns (uint256){
        return counterIDs.current();
    }

    function getOpenSales() public view returns (Sale[] memory){
        Sale[] memory ret = new Sale[](counterOpenSales.current());
        //Variable para ir rellenando el array que va a ser devuelto.abi
        uint8 indexRet = 0;
        //Empieza en 1 porque los IDs de las ventas empiezan en 1.
        for(uint i = 1; i <= counterIDs.current(); i++){
            if(keccak256(bytes(sales[i].status)) == keccak256(bytes(statusOpen))){
                ret[indexRet] = sales[i];
                indexRet++;
            }
        }
        return ret;
    }

    //Devuelve las ventas abiertas que pertenezcan a alguno de los tipos.
    //Si el parametro es true se incluye el tipo.
    //_type1 == true => incluye las ventas abiertas de NFTs del tipo 1. Asi con los tres tipos.
    function getOpenSalesByTypes(bool _type1, bool _type2, bool _type3)public view returns(Sale[] memory){
        
    }

    function openSale(uint256 _tokenID, uint256 _price) public TokenExists(_tokenID) UserOwnsToken(_tokenID){
        ATCNFT.transferFrom(msg.sender,address(this), _tokenID);
        counterIDs.increment();
        uint8 tokenType = ATCNFT.getTypeOfToken(_tokenID);
        console.log(tokenType);
        sales[counterIDs.current()] = Sale(msg.sender,_tokenID,_price*ATCcoinDecimals,statusOpen,tokenType);
        NFTtoSale[_tokenID] = counterIDs.current();
        //Se aumenta el contrador de ventas abiertas.
        counterOpenSales.increment();
    }

    function buyTokenFromSale(uint256 _tokenID) public{
        uint256 saleID = NFTtoSale[_tokenID];
        require(keccak256(bytes(sales[saleID].status)) == keccak256(bytes(statusOpen)) , "buyTokenFromSale - la venta no esta disponible.");
        address formerOwner = sales[saleID].owner;
        uint256 price = sales[saleID].price;
        require(ATCcoin.balanceOf(msg.sender) >= price, "buyTokenFromSale - saldo de ATCC insuficiente.");
        require(ATCcoin.allowance(msg.sender,address(this)) >= price, "buyTokenFromSale - el MarketPlace no tiene permiso para transferir los fondos del usuario.");
        require(ATCcoin.transferFrom(msg.sender, formerOwner, price), "buyTokenFromSale - ha ocurrido un fallo al transferir los ATCC.");
        sales[saleID].owner = msg.sender;
        sales[saleID].status = statusExecuted;
        ATCNFT.transferFrom(address(this), msg.sender, _tokenID);
        //Se disminuye el contrador de ventas abiertas.
        counterOpenSales.decrement();
    }

    function cancelSale(uint256 _tokenID) public TokenExists(_tokenID) UserOwnsToken(_tokenID){
        uint256 saleID = NFTtoSale[_tokenID];
        require(keccak256(bytes(sales[saleID].status)) == keccak256(bytes(statusOpen)), "cancelTokenSale - la cancelacion no esta disponible");
        sales[saleID].status = statusCancelled;
        ATCNFT.transferFrom(address(this), sales[saleID].owner, _tokenID);
        //Se disminuye el contrador de ventas abiertas.
        counterOpenSales.decrement();
    }

    //Modifica el precio de una venta
    function modifyPriceSale(uint256 _saleID, uint256 _newPrice) public SaleExists(_saleID) UserOwnsToken(_saleID) SaleIsOpen(_saleID){
        //Se obtiene la venta que corresponde con el ID, se accede al precio y se actualiza con el nuevo.
        sales[_saleID].price = _newPrice*ATCcoinDecimals;
    }

}

