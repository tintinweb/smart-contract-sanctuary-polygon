pragma solidity ^0.8.7;

import "./Collection-6.sol";

contract CollectionFactory6 {
  event CollectionCreated(address indexed collection, string slug);

  // CREATOR FUNCTIONS

  function createCollection(
    string calldata name,
    string calldata symbol,
    string calldata slug_,
    address signer_,
    address _gallery
  ) external returns (address collection) {
    collection = address(new Collection6(msg.sender, name, symbol, slug_, signer_, _gallery));
    emit CollectionCreated(collection, slug_);
  }
}

