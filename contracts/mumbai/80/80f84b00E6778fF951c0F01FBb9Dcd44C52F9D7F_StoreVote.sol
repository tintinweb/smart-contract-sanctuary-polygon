pragma solidity 0.8.17;

import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "../ERC1155Vote.sol";
import "hardhat/console.sol";

contract StoreVote is ERC1155Vote {
    address public owner;
    uint256[] public tokenIds;
    IERC1155 public shopManager;
    ProductCandidate[] public productCandidates;

    struct ProductCandidate {
        address owner;
        uint256 cost;
        uint256 sellPrice;
        string imageUrl;
        uint256 votedPoints;
    }

    struct Proposal {
        uint256[] candidateIds;
        uint256 endTime;
        uint256 startTime;
    }

    Proposal[] public proposals;
    mapping(uint256 => mapping(address => bool)) public votes;

    mapping(uint256 => uint256) private _multipliers;
    mapping(uint256 => bool) private _validTokenIds;
    mapping(uint => address[]) public candidateVoters;

    event ProposalCreated(uint256 proposalId);
    event CandidateAdded(uint256 proposalId, uint256 candidateId);
    event Voted(uint256 proposalId, uint256 candidateId, address voter);
    modifier onlyOwner() {
        require(msg.sender == owner, "Only the owner can call this function");
        _;
    }

    constructor(address _shopManager) {
        owner = msg.sender;
        shopManager = IERC1155(_shopManager);
    }

    function createProposal() public onlyOwner {
        uint256[] memory candidateIds; // Define an empty array of type uint256
        uint256 endTime = getEndTime(block.timestamp);
        uint256 startTime = getNextMonday(block.timestamp);
        // Define the proposal
        Proposal memory newProposal;
        newProposal.candidateIds = candidateIds;
        newProposal.endTime = endTime;
        newProposal.startTime = startTime;

        // Add the new proposal to the array of proposals
        proposals.push(newProposal);

        // Emit the ProposalCreated event with the id of the new proposal
        emit ProposalCreated(proposals.length - 1);
    }

    function getProposal(
        uint256 proposalId
    ) public view returns (uint256[] memory, uint256, uint256) {
        require(proposalId < proposals.length, "Invalid proposalId");
        Proposal memory proposal = proposals[proposalId];
        return (proposal.candidateIds, proposal.startTime, proposal.endTime);
    }

    function getNextMonday(uint timestamp) public pure returns (uint) {
        uint dayOfWeek = (timestamp / 1 days + 4) % 7;
        uint nextMonday = timestamp + (8 - (dayOfWeek % 7)) * 1 days;
        uint secondsPassedToday = timestamp % 1 days;
        return nextMonday - secondsPassedToday;
    }

    function getEndTime(uint timestamp) public pure returns (uint) {
        uint nextMonday = getNextMonday(timestamp);
        uint endTime = nextMonday + 6 days + 23 hours + 59 minutes;
        return endTime;
    }

    function addProductCandidate(
        uint256 proposalId,
        address _owner,
        uint256 _cost,
        uint256 _sellPrice,
        string memory _imageUrl
    ) public onlyOwner {
        require(proposalId < proposals.length, "Invalid proposalId");
        Proposal storage proposal = proposals[proposalId];
        uint256 candidateId = productCandidates.length;
        productCandidates.push(
            ProductCandidate({
                owner: _owner,
                cost: _cost,
                sellPrice: _sellPrice,
                imageUrl: _imageUrl,
                votedPoints: 0
            })
        );
        proposal.candidateIds.push(candidateId);

        emit CandidateAdded(proposalId, candidateId);
    }

    function vote(uint256 proposalId, uint256 candidateId) public {
        require(proposalId < proposals.length, "Invalid proposalId");
        ProductCandidate storage candidate = productCandidates[candidateId];
        Proposal storage proposal = proposals[proposalId];
        require(
            block.timestamp >= proposal.startTime &&
                block.timestamp <= proposal.endTime,
            "Voting is not currently open for this proposal"
        );
        require(candidateId < productCandidates.length, "Invalid candidateId");
        require(
            !votes[proposalId][msg.sender],
            "You already voted for this proposal"
        );

        uint256 totalBalance = 0;

        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 id = tokenIds[i];
            uint256 balance = shopManager.balanceOf(msg.sender, id);
            uint256 multiplier = _multipliers[id];
            candidate.votedPoints += balance * multiplier;
            totalBalance += balance;
        }

        if (totalBalance == 0) {
            candidate.votedPoints += 1;
        }

        votes[proposalId][msg.sender] = true;
        candidateVoters[candidateId].push(msg.sender);
        emit Voted(proposalId, candidateId, msg.sender);
    }

    function addTokenId(uint256 tokenId) public onlyOwner {
        tokenIds.push(tokenId);
        _validTokenIds[tokenId] = true;
        _multipliers[tokenId] = 1; // Default multiplier
    }

    function setMultiplier(
        uint256 tokenId,
        uint256 _multiplier
    ) public onlyOwner {
        require(_validTokenIds[tokenId], "Invalid tokenId");
        _multipliers[tokenId] = _multiplier;
    }

    function getCandidateVoters(
        uint256 candidateId
    ) public view returns (address[] memory) {
        return candidateVoters[candidateId];
    }

    function getProposalsInPeriod(
        uint256 _startTime,
        uint256 _endTime
    ) public view returns (uint256[] memory) {
        uint256[] memory proposalIds;
        uint256 count = 0;
        for (uint256 i = 0; i < proposals.length; i++) {
            if (
                proposals[i].startTime >= _startTime &&
                proposals[i].endTime <= _endTime
            ) {
                proposalIds[count] = i;
                count++;
            }
        }
        return proposalIds;
    }

    function getProposalsNextWeek() public view returns (uint256[] memory) {
        uint256 nextMonday = getNextMonday(block.timestamp);
        uint256 nextSunday = nextMonday + 6 days;
        return getProposalsInPeriod(nextMonday, nextSunday);
    }

    function getProposalsThisWeek() public view returns (uint256[] memory) {
        uint256 thisMonday = getThisMonday(block.timestamp);
        uint256 thisSunday = thisMonday + 6 days;
        return getProposalsInPeriod(thisMonday, thisSunday);
    }

    function getThisMonday(uint timestamp) public pure returns (uint) {
        uint dayOfWeek = (timestamp / 1 days + 4) % 7;
        uint thisMonday = timestamp - (dayOfWeek % 7) * 1 days;
        uint secondsPassedToday = timestamp % 1 days;
        return thisMonday - secondsPassedToday;
    }

    function changeProposalPeriod(
        uint256 proposalId,
        uint256 _startTime,
        uint256 _endTime
    ) public onlyOwner {
        require(proposalId < proposals.length, "Invalid proposalId");
        Proposal storage proposal = proposals[proposalId];
        proposal.startTime = _startTime;
        proposal.endTime = _endTime;
    }
}

