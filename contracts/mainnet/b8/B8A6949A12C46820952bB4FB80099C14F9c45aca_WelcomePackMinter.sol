pragma solidity 0.8.4;

import "./ERC20PresetMinterPauserPermit.sol";

contract ResourceERC20 is ERC20PresetMinterPauserPermit {
    uint256 public immutable maxSupply;

    constructor(
        address owner,
        string memory name,
        string memory symbol,
        uint256 _maxSupply
    ) ERC20PresetMinterPauserPermit(owner, name, symbol) {
        maxSupply = _maxSupply * 10**18;
    }

    function mint(address to, uint256 amount) public override {
        uint256 remaining = maxSupply - totalSupply();
        require(remaining >= amount, "ResourceERC20: Max supply reached");
        super.mint(to, amount);
    }
}