pragma solidity ^0.8.4;

import "../resources/ResourceERC20.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/draft-IERC20Permit.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

interface ICrafter {
    function craft(address to, uint256 recipeId) external returns (bytes32);
}

/*
 * @title WelcomePackMinter
 * @notice WelcomePackMinter is an utility contract that helps mints of
 * welcome packs, from whitelisted addresses.
 */
contract WelcomePackMinter is AccessControl, ReentrancyGuard, EIP712 {
    using SafeERC20 for IERC20;

    uint256 public boosterPackId;
    uint256 public boosterPackQuantity;
    uint256 private maxMintableResource = 3092 * 10**18;

    /// @dev mapping nonce => consumed
    mapping(uint256 => bool) nonces;

    struct WelcomePackRequest {
        address to;
        uint256 deadline;
        uint256 nonce;
        CraftIngredient[] ingredients;
        bytes signature;
    }

    struct CraftIngredient {
        address token0;
        uint256 amount0Max;
        address token1;
        uint256 amount1Max;
        uint256 amount;
    }

    IUniswapV2Factory public immutable factory;
    ICrafter public immutable crafter;

    uint256 public maxPurchasesPerUser = 1;
    mapping(address => uint256) public purchaseCounts;

    /// @notice MINTER_ROLE is required to request welcome packs to be purchased.
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    string private constant SIGNING_DOMAIN = "WelcomePack";
    string private constant SIGNATURE_VERSION = "2";

    event Purchased(address indexed receiver);

    constructor(
        address owner,
        uint256 _boosterPackId,
        uint256 _boosterPackQuantity,
        ICrafter _crafter,
        IUniswapV2Factory _factory
    ) EIP712(SIGNING_DOMAIN, SIGNATURE_VERSION) {
        _setupRole(DEFAULT_ADMIN_ROLE, owner);

        boosterPackId = _boosterPackId;
        boosterPackQuantity = _boosterPackQuantity;
        crafter = _crafter;
        factory = _factory;
    }

    /**
     * Mint WelcomePack.
     * @param request welcome pack request
     */
    function mint(WelcomePackRequest calldata request) external nonReentrant {
        address signer = _verify(request);
        require(hasRole(MINTER_ROLE, signer), "WelcomePackMinter: Signature invalid or unauthorized");

        // ensure deadline still valid
        require(block.timestamp <= request.deadline, "Expired deadline");
        // ensure nonce not already consumed
        require(!nonces[request.nonce], "Nonce consumed");
        // ensure user can still make purchases
        require(purchaseCounts[request.to] < maxPurchasesPerUser, "Too many purchases");

        // keep track of purchase
        nonces[request.nonce] = true;
        purchaseCounts[request.to]++;

        // mint booster packs resources
        CraftIngredient[] memory boosterPackIngredients = request.ingredients;
        mintBoostersResources(boosterPackIngredients, boosterPackQuantity);
        // craft boosters
        craftBoosters(request.to, boosterPackId, boosterPackQuantity, boosterPackIngredients);

        // emit event
        emit Purchased(request.to);
    }

    function setBoosterPackId(uint256 _boosterPackId) external onlyRole(DEFAULT_ADMIN_ROLE) {
        boosterPackId = _boosterPackId;
    }

    function setBoosterPackQuantity(uint256 _boosterPackQuantity) external onlyRole(DEFAULT_ADMIN_ROLE) {
        boosterPackQuantity = _boosterPackQuantity;
    }

    function setMaxMintableResource(uint256 _maxMintableResource) external onlyRole(DEFAULT_ADMIN_ROLE) {
        maxMintableResource = _maxMintableResource;
    }

    function setMaxPurchasesPerUser(uint256 _maxPurchasesPerUser) external onlyRole(DEFAULT_ADMIN_ROLE) {
        maxPurchasesPerUser = _maxPurchasesPerUser;
    }

    function mintBoostersResources(CraftIngredient[] memory ingredients, uint256 quantity) internal {
        for (uint256 i = 0; i < ingredients.length; i++) {
            require(
                ingredients[i].amount0Max <= maxMintableResource && ingredients[i].amount1Max <= maxMintableResource,
                "Invalid ingredients"
            );
            IERC20 token0 = IERC20(ingredients[i].token0);
            IERC20 token1 = IERC20(ingredients[i].token1);
            IUniswapV2Pair lp = IUniswapV2Pair(factory.getPair(address(token0), address(token1)));
            (uint256 amount0, uint256 amount1) = _computeLpAmounts(lp, ingredients[i].amount * quantity);
            require(amount0 <= ingredients[i].amount0Max * quantity, "token0 exceeds max amount");
            require(amount1 <= ingredients[i].amount1Max * quantity, "token1 exceeds max amount");
            ResourceERC20(ingredients[i].token0).mint(address(lp), amount0);
            ResourceERC20(ingredients[i].token1).mint(address(lp), amount1);
            lp.mint(address(this));
        }
    }

    function craftBoosters(
        address to,
        uint256 recipeId,
        uint256 quantity,
        CraftIngredient[] memory ingredients
    ) internal {
        for (uint256 q = 0; q < quantity; q++) {
            for (uint256 i = 0; i < ingredients.length; i++) {
                IUniswapV2Pair lp = IUniswapV2Pair(factory.getPair(ingredients[i].token0, ingredients[i].token1));
                uint256 lpBalance = lp.balanceOf(address(this));
                IERC20(address(lp)).safeTransfer(address(crafter), lpBalance / (quantity - q));
            }
            crafter.craft(to, recipeId);
        }
    }

    function _computeLpAmounts(IUniswapV2Pair pair, uint256 amount) private view returns (uint256, uint256) {
        (uint112 reserve0, uint112 reserve1, ) = pair.getReserves();
        uint256 totalSupply = pair.totalSupply();
        uint256 kLast = pair.kLast();

        if (factory.feeTo() != address(0)) {
            if (kLast != 0) {
                uint256 rootK = sqrt(uint256(reserve0) * reserve1);
                uint256 rootKLast = sqrt(kLast);
                if (rootK > rootKLast) {
                    uint256 numerator = totalSupply * (rootK - rootKLast);
                    uint256 denominator = rootK * 9 + rootKLast;
                    totalSupply = totalSupply + (numerator / denominator);
                }
            }
        }

        uint256 amount0 = (amount * reserve0) / totalSupply;
        uint256 amount1 = (amount * reserve1) / totalSupply;

        return (amount0, amount1);
    }

    function sqrt(uint256 y) internal pure returns (uint256 z) {
        if (y > 3) {
            z = y;
            uint256 x = y / 2 + 1;
            while (x < z) {
                z = x;
                x = (y / x + x) / 2;
            }
        } else if (y != 0) {
            z = 1;
        }
    }

    string private constant WELCOME_PACK_REQUEST_TYPE =
        "WelcomePackRequest(address to,uint256 deadline,uint256 nonce,CraftIngredient[] ingredients)";
    string private constant CRAFT_INGREDIENT_TYPE =
        "CraftIngredient(address token0,uint256 amount0Max,address token1,uint256 amount1Max,uint256 amount)";
    bytes32 private constant CRAFT_INGREDIENT_TYPE_HASH = keccak256(bytes(CRAFT_INGREDIENT_TYPE));
    bytes32 private constant TYPE_HASH = keccak256(abi.encodePacked(WELCOME_PACK_REQUEST_TYPE, CRAFT_INGREDIENT_TYPE));

    function _hash(WelcomePackRequest calldata request) internal view returns (bytes32) {
        bytes32 ingredientsHash = hashIngredients(request.ingredients);
        return
            _hashTypedDataV4(
                keccak256(abi.encode(TYPE_HASH, request.to, request.deadline, request.nonce, ingredientsHash))
            );
    }

    function _verify(WelcomePackRequest calldata request) internal view returns (address) {
        bytes32 digest = _hash(request);
        return ECDSA.recover(digest, request.signature);
    }

    /// @dev keccak256("");
    bytes32 private constant _EMPTY_ARRAY_KECCAK256 =
        0xc5d2460186f7233c927e7db2dcc703c0e500b653ca82273b7bfad8045d85a470;

    function hashIngredients(CraftIngredient[] memory ingredients) internal pure returns (bytes32 hash) {
        uint256 arrayLength = ingredients.length;
        if (arrayLength == 0) {
            return _EMPTY_ARRAY_KECCAK256;
        }

        bytes32[] memory hashArray = new bytes32[](arrayLength);
        for (uint256 i = 0; i < arrayLength; i++) {
            hashArray[i] = keccak256(
                abi.encode(
                    CRAFT_INGREDIENT_TYPE_HASH,
                    ingredients[i].token0,
                    ingredients[i].amount0Max,
                    ingredients[i].token1,
                    ingredients[i].amount1Max,
                    ingredients[i].amount
                )
            );
        }
        // solhint-disable-next-line no-inline-assembly
        assembly {
            hash := keccak256(add(hashArray, 32), mul(arrayLength, 32))
        }
    }
}

