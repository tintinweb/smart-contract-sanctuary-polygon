        _setDefaultRoyalty(multisigWallet, royaltyFee);
    }

    function _baseURI() internal view override returns (string memory) {
        return sharedMetadataUri;
    }

    function _startTokenId() internal pure override returns (uint256) {
        return 1;
    }

    function _burn(uint256 tokenId) internal override(ERC721A, ERC721ARoyalty) {
        super._burn(tokenId);
    }

    function burn(uint256 tokenId) public virtual {
        _burn(tokenId, true);
    }
    
    function buyersAmount() public view returns (uint256) {
        return buyers.length;
    }

    function mint(uint256 _mintAmount) public payable priceCheck(_mintAmount) supplyCheck(_mintAmount) nonReentrant {
        require(!isPaused, 'The contract is paused!');
        _safeMint(_msgSender(), _mintAmount);
        buyerPendingPaidTokens[_msgSender()] = buyerPendingPaidTokens[_msgSender()] + _mintAmount;
        buyers.push(_msgSender());
        (bool isSaleOK, ) = multisigWallet.call{value: msg.value}('');
        require(isSaleOK);
    }
    
    function tokenURI(uint256 tokenId) public view virtual override(ERC721A, IERC721A) returns (string memory) {
        require(_exists(tokenId), 'ERC721Metadata: URI query for nonexistent token');
        return sharedMetadataUri;
    }

    function claimPendingUserPLS(uint256 _amount, address buyer) public onlyContract {
        buyerPendingPaidTokens[buyer] = buyerPendingPaidTokens[buyer] - _amount;
        buyerClaimedTokens[buyer] = buyerClaimedTokens[buyer] + _amount;
        
        emit BuyerPendingTokensDistributed(buyer, _amount);
    }

    function mintForTeam(address _teamAddress) public onlyOwner {
        require(!isTeamAlreadyMinted, 'Team already minted their supply!');
        _safeMint(_teamAddress, teamAllocation);
        isTeamAlreadyMinted = true;
    }

    function mintForAddress(address _to, uint256 _amount) public onlyOwner {
        require((totalSupply() + _amount) <= maxSupply, 'Max supply exceeded!');
        _safeMint(_to, _amount);
    }

    function setPaused(bool _state) public onlyOwner {
        isPaused = _state;
    }

    function setTeamAllocation(uint256 _newAmount) public onlyOwner {
        teamAllocation = _newAmount;
    }

    function setMaxWallet(uint256 _newAmount) public onlyOwner {
        maxWallet = _newAmount;
    }

    function setMintPrice(uint256 _newPrice) public onlyOwner {
        mintPrice = _newPrice;
    }

    function setRoyaltyFee(uint96 _newFee) public onlyOwner {
        royaltyFee = _newFee;
    }

    function setMultisigWallet(address _newAddress) public onlyOwner {
        multisigWallet = payable(_newAddress);
    }

    function setContractWallet(address _newAddress) public onlyOwner {
        contractWallet = _newAddress;
    }

    function setMaxSupply(uint256 _newSupply) public onlyOwner {
        maxSupply = _newSupply;
    }

    function setSharedMetadataUri(string memory _newURI) public onlyOwner {
        sharedMetadataUri = _newURI;
    }

    function rescueTrappedFunds(address _token) public onlyOwner {
        uint256 contractTokenBalance = IERC20(_token).balanceOf(address(this));
        uint256 contractETHBalance = address(this).balance;

        if(contractTokenBalance > 0) {
            IERC20(_token).transfer(multisigWallet, contractTokenBalance);
        }

        if(contractETHBalance > 0) {
            (bool isSendOK, ) = multisigWallet.call{value: contractETHBalance}('');
            require(isSendOK);
        }
    }

    function supportsInterface(
        bytes4 interfaceId
    ) public view virtual override(ERC721A, IERC721A, ERC721ARoyalty) returns (bool) {
        return 
            ERC721A.supportsInterface(interfaceId) || 
            ERC721ARoyalty.supportsInterface(interfaceId);
    }
}