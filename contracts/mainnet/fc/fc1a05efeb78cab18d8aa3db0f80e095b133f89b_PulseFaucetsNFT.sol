    mapping(address => uint256) public buyerPendingPaidTokens;
    mapping(address => uint256) public buyerClaimedTokens;
    address[] public buyers;

    modifier priceCheck(uint256 _mintAmount) {
        require(msg.value >= mintPrice * _mintAmount, 'Insufficient funds!');
        _;
    }

    modifier supplyCheck(uint256 _mintAmount) {
        require(_mintAmount > 0, 'Invalid mint amount!');        
        require((balanceOf(_msgSender()) + _mintAmount) <= maxWallet, 'Max wallet exceeded!');
        require((totalSupply() + _mintAmount) <= maxSupply, 'Max supply exceeded!');
        _;
