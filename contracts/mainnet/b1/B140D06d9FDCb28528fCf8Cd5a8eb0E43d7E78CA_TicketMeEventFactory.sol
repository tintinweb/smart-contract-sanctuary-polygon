import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "./ITicketMeEvent.sol";

/**
 * @title TicketMe Event Factory
 * @author TicketMe
 */
contract TicketMeEventFactory is Ownable {
    using Clones for address;

    address public masterContract;
    address public priceFeederContract;
    address public operatingAddress;
    mapping(string => address) eventAddress;

    /**
     * @dev Constractor of TicketMe Event Factory contract.
     * @param _masterContract Initial setting of master contract address.
     * @param _priceFeederContract Initial setting of price feeder contract address.
     * @param _operatingAddress Initial setting of operating address.
     */
    constructor(address _masterContract, address _priceFeederContract, address _operatingAddress) {
        setMasterContract(_masterContract);
        setPriceFeederContract(_priceFeederContract);
        setOperatingAddress(_operatingAddress);
    }

    /**
     * @dev Deploy event contract.
     */
    function deploy(ITicketMeEvent.Event memory _eventData, ITicketMeEvent.HolderRestriction memory _holderRestriction) public onlyOwner {
        address deployedAddress = masterContract.clone();
        eventAddress[_eventData.eventId] = deployedAddress;
        ITicketMeEvent deployedContract = ITicketMeEvent(deployedAddress);
        deployedContract.initialize(_eventData, priceFeederContract, _holderRestriction);

        deployedContract.transferOwnership(operatingAddress);
    }

    /**
     * @dev Returns event contract address.
     * @param _eventId Event id
     */
    function getAddress(string memory _eventId) public view returns (address) {
        return eventAddress[_eventId];
    }

    /**
     * @dev Setter of master contract address.
     * @param _masterContract master contract address
     */
    function setMasterContract(address _masterContract) public onlyOwner {
        masterContract = _masterContract;
    }

    /**
     * @dev Setter of price feeder contract address.
     * @param _priceFeederContract price feeder contract address
     */
    function setPriceFeederContract(address _priceFeederContract) public onlyOwner {
        priceFeederContract = _priceFeederContract;
    }

    /**
     * @dev Setter of operating address.
     * @param _operatingAddress operating address
     */
    function setOperatingAddress(address _operatingAddress) public onlyOwner {
        operatingAddress = _operatingAddress;
    }
}