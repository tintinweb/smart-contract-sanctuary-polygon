    function burnTokens(
        address _to,
        uint256 _tokenId,
        uint256 _mintAmount
    ) external {
        require(
            _mintAmount != 0,
            "Super Mint Converter: mint amount must be > 0"
        );

        uint256 requestId = i_vrfCoordinator.requestRandomWords(
            i_keyHash,
            i_subscriptionId,
            REQUEST_CONFIRMATIONS,
            i_callbackGasLimit,
            NUM_WORDS
        );

        s_requests[requestId] = RequestStatus({
            randomWord: 0,
            exists: true,
            fulfilled: false,
            burner: msg.sender,
            to: _to,
            tokenId: _tokenId,
            mintAmount: _mintAmount
        });

        lastRequestId = requestId;

        pgerc1155.safeTransferFrom(
            msg.sender,
            receiver,
            _tokenId,
            _mintAmount,
            "0x"
        );

        emit TokenBurned(msg.sender, requestId, _tokenId, _to, _mintAmount);
    }

    /**
     * @notice Burn multiple tokens and initiate a random word request
     * @dev This function allows burning multiple tokens with corresponding amounts and initiating a random word request.
     * @param _to The address to which the burned tokens will be transferred.
     * @param _tokenIds The array of token IDs to be burned.
     * @param _mintAmounts The array of amounts corresponding to the token IDs to be burned.
     */
    function burnTokensMultiple(
        address _to,
        uint256[] calldata _tokenIds,
        uint256[] calldata _mintAmounts
    ) external {
        require(
            _tokenIds.length > 0,
            "Super Mint Converter: tokenIds.length must be > 0"
        );
        require(
            _tokenIds.length == _mintAmounts.length,
            "Super Mint Converter: Arrays need to have the same lengths"
        );

        uint256 requestId = i_vrfCoordinator.requestRandomWords(
            i_keyHash,
            i_subscriptionId,
            REQUEST_CONFIRMATIONS,
            i_callbackGasLimit,
            NUM_WORDS
        );

        s_requestsMultiple[requestId] = RequestStatusMultiple({
            randomWord: 0,
            exists: true,
            fulfilled: false,
            burner: msg.sender,
            to: _to,
            tokenIds: _tokenIds,
            mintAmounts: _mintAmounts
        });

        lastRequestId = requestId;

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            pgerc1155.safeTransferFrom(
                msg.sender,
                receiver,
                _tokenIds[i],
                _mintAmounts[i],
                "0x"
            );
        }

        emit TokenBurnedMultiple(
            msg.sender,
            requestId,
            _tokenIds,
            _to,
            _mintAmounts
        );
    }

    /**
     * @notice Burn a Super Token and initiate a random word request
     * @dev This function allows burning a Super Token, verifying the provided signature, and initiating a random word request.
     * @param _burnSuperToken The BurnSuperToken calldata struct containing the necessary information for burning the Super Token.
     * @param _signature The signature to be verified.
     */
    function burnSuperToken(
        BurnSuperToken calldata _burnSuperToken,
        bytes calldata _signature
    ) external {
        require(
            hasRole(SIGNER_ROLE, _verify(_burnSuperToken, _signature)),
            "SuperMintConverter: Invalid signature"
        );

        uint256 requestId = i_vrfCoordinator.requestRandomWords(
            i_keyHash,
            i_subscriptionId,
            REQUEST_CONFIRMATIONS,
            i_callbackGasLimit,
            NUM_WORDS
        );

        s_requestsSuperMint[requestId] = RequestStatusSuperMint({
            randomWord: 0,
            exists: true,
            fulfilled: false,
            burner: msg.sender,
            to: _burnSuperToken.to,
            tokenId: _burnSuperToken.tokenId,
            mintAmount: 1,
            superIds: _burnSuperToken.superIds
        });

        lastRequestId = requestId;

        pgerc1155.safeTransferFrom(
            msg.sender,
            receiver,
            _burnSuperToken.tokenId,
            1,
            "0x"
        );

        for (uint256 i = 0; i < _burnSuperToken.superIds.length; i++) {
            pgerc1155_super.safeTransferFrom(
                msg.sender,
                receiver,
                _burnSuperToken.superIds[i],
