            }
            while (true) {
                ptr--;
                /// @solidity memory-safe-assembly
                assembly {
                    mstore8(ptr, byte(mod(value, 10), _SYMBOLS))
                }
                value /= 10;
                if (value == 0) break;
            }
            return buffer;
        }
    }

    /**
     * @dev Converts a `uint256` to its ASCII `string` hexadecimal representation.
     */
    function toHexString(uint256 value) internal pure returns (string memory) {
        unchecked {
            return toHexString(value, Math.log256(value) + 1);
        }
    }

    /**
     * @dev Converts a `uint256` to its ASCII `string` hexadecimal representation with fixed length.
     */
    function toHexString(uint256 value, uint256 length) internal pure returns (string memory) {
        bytes memory buffer = new bytes(2 * length + 2);
        buffer[0] = "0";
        buffer[1] = "x";
        for (uint256 i = 2 * length + 1; i > 1; --i) {
            buffer[i] = _SYMBOLS[value & 0xf];
            value >>= 4;
        }
        require(value == 0, "Strings: hex length insufficient");
        return string(buffer);
    }

    /**
     * @dev Converts an `address` with fixed length of 20 bytes to its not checksummed ASCII `string` hexadecimal representation.
     */
    function toHexString(address addr) internal pure returns (string memory) {
        return toHexString(uint256(uint160(addr)), _ADDRESS_LENGTH);
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.18;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

contract MariposaNFT is ERC1155, Ownable, ReentrancyGuard, AccessControl {
    // Collection identity
    string public constant name = "Mariposa NFTs";
    string public constant symbol = "MRPS";
    
    // Access roles 
    bytes32 public constant BANK_ROLE = keccak256("BANK_ROLE");
    bytes32 public constant NFT_ROLE = keccak256("NFT_ROLE");
    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");

    // 
    bytes32 private wlMerkleRoot;

    // will store the minters to prevent double minting during WL
    mapping (address => bool) public senderhasMinted;

    // WETH address on blockchains
    IERC20 private iWETH;

    using Strings for uint;

    // Project steps
    enum Step {
        none,
        mintWL,
        mintPublic,
        soldOut
    }

    // Project wide data
    struct project {
        Step step;
        uint mintPrice;
        bool revealed;
        bool metadataLocked;
        uint8 maxId;
        uint8 nextMint;
        uint8 collectionSize;
        uint8 maxWLMints;
    }

    // Item specific data
    struct item {
        string name; 
        string description;
        string uri; // Media CID
        string artist;
        string rarity;
        string series;
        uint maxSupply; // 0 => unlimited
        uint maxPerAddress; // 0 => unlimited
        uint currentSupply;
    }

    // The project
    project public MRPSProject;

    // Project NFTs
    mapping (uint => item) public MRPSitems;

    // Modifier : needs metadataLocked false
    modifier ifMetadataUnlocked() {
        require(MRPSProject.metadataLocked == false,
                "Metadata are locked");
        _;
    }

    // Modifier : Need an existing Id (collection + OG)
    modifier ifIdExists(uint _id) {
        require(_id >= 1 &&
                _id <= MRPSProject.maxId,
                "Nonexistent id");
        _;
    }

    // Event : An NFT from the collection was minted
    event CollectionItemMinted(uint id, address who);

    // Event : project step changed
    event StepChanged(Step step);

    // Event MetadataLocked
    event MetadataLocked();

    // Event Collection revealed
    event CollectionRevealed();

    // Event : Withdrawal successful
    event Withdrawal(address account);

    // Event : OG Minted to an address
    event OGMintedTo(address to);

    // ipfs://bafkreibzh5ccti4jxf62mzgd4vk7ofy3ofkizifadjger7jeib5lk73jze/
    // Mumbai : 0xA6FA4fB5f76172d178d61B04b0ecd319C5d1C0aa
    // Polygon Mainnet : 0x7ceB23fD6bC0adD59E62ac25578270cFf1b9f619
    // Mumbai Fake WETH : 0xAae88739aB434dE726ad6f19b209F1A2DE80a79e
    constructor(string memory _nfturi, address _wethAddr, address _otherAdmin) ERC1155(_nfturi) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MANAGER_ROLE, msg.sender);
        _grantRole(NFT_ROLE, msg.sender);
        _grantRole(BANK_ROLE, msg.sender);
        _grantRole(DEFAULT_ADMIN_ROLE, _otherAdmin);
        _grantRole(MANAGER_ROLE, _otherAdmin);
        _grantRole(NFT_ROLE, _otherAdmin);
        _grantRole(BANK_ROLE, _otherAdmin);

        iWETH = IERC20(_wethAddr);

        // Initialize Project
        MRPSProject.step = Step.none;
        MRPSProject.mintPrice = 5 * 10 ** 16; // 0.05 ether
        MRPSProject.revealed = false;
        MRPSProject.metadataLocked = false;
        MRPSProject.nextMint = 2; // 1 is minted in the construstor 
        MRPSProject.collectionSize = 25;
        MRPSProject.maxWLMints = 19; // Il doit rester 5 NFTs minimum pour la vente publique

        // Create the NFTs data (Collection + OGs)
        addNFT(item("Mariposa NFT", "", _nfturi, "", unicode"Légendaire", "Collection", 1, 0, 0));
        for (uint i = 2; i<=MRPSProject.collectionSize ; ++i) {
            addNFT(item("Mariposa NFT", "", _nfturi, "", "Commun", "Collection", 1, 0, 0));
        }
        addNFT(item("HS - MARIPOSA", unicode"Cette chrysalide a été offerte aux artistes et aux artisans du projet *Mariposa*, pour l’ouverture d’une **Maison d’Accueil pour les femmes victimes de violences**. Numéro SOLIDARITÉ FEMMES : 3919 (Appel anonyme et gratuit)", _nfturi, "Calypso", unicode"Spécial", unicode"Chrysalide", 0, 0, 0));
        addNFT(item("HS - MARIPOSAS", unicode"Ce SFT est offert aux premiers soutiens du projet *Mariposa*, pour l’ouverture d’une **Maison d’Accueil pour les femmes victimes de violences**. Numéro SOLIDARITÉ FEMMES : 3919 (Appel anonyme et gratuit)",_nfturi,"Calypso",unicode"Spécial",unicode"Présents", 0, 0, 0));

        // Mint auction NFT
        _mint(msg.sender, 1, 1, "");
    }

    // Mint using WETH
    // Corresponding WETH amount must be approved first
    function publicmint(bytes32[] memory _proof) public nonReentrant {
        // Get Next id available
        uint _id = MRPSProject.nextMint;

        // is id part of the collection ?
        // OG is not publicly mintable
        require(_id >= 1 &&
                _id <= MRPSProject.collectionSize,
                "This NFT is not mintable");

        // is mint active ?
        require(MRPSProject.step == Step.mintPublic ||
                MRPSProject.step == Step.mintWL,
                "Mint is not open");

        // Was the price set ?
        require(MRPSProject.mintPrice > 0,
                "Mint price has not been set yet !");

        // Is whitelist the current step ?
        if (MRPSProject.step == Step.mintWL) {
            // Is the caller in the whitelist ?
            require(addressIsOnWL(msg.sender, _proof) == true,
                    "Address is not on the whitelist");
            // Has the sender already mint ?
            require(senderhasMinted[msg.sender] == false, 
                    "Address has already minted");
            // Max number of mints during WL reached ?        
            require(MRPSProject.nextMint <= MRPSProject.maxWLMints,
                    "Max mints during WL reached");
        }

        // Check this item totalSupply
        require(MRPSitems[_id].maxSupply == 0 ||
                MRPSitems[_id].currentSupply < MRPSitems[_id].maxSupply,
                "Max supply reached for this NFT");
        
        // Is the max NFT per address reached for this NFT ?
        require(MRPSitems[_id].maxPerAddress == 0 ||
                balanceOf(msg.sender, _id) < MRPSitems[_id].maxPerAddress,
                "You can't own more of this NFT on this address");

        // Does the transfer work ?
        require(iWETH.transferFrom(msg.sender, address(this), MRPSProject.mintPrice),
                "WETH transfer failed");

        // Mint the NFT 
        _mint(msg.sender, _id, 1, "");

        // update current supply of this NFT
        MRPSitems[_id].currentSupply++;

        // Increase next Mint ID
        MRPSProject.nextMint++;

        // Remember this minter 
        senderhasMinted[msg.sender] = true;

        emit CollectionItemMinted (_id, msg.sender);
    }

    // Allow / Disallow minting the collection's NFTs
    function setStep(Step _step) external onlyRole(NFT_ROLE) {
        MRPSProject.step = _step;
        emit StepChanged(_step);
    }

    // reveal the collection
    function reveal() external onlyRole(NFT_ROLE) {
        MRPSProject.revealed = true;
        emit CollectionRevealed();
    }

    // Lock the collection metadata
    function lockMetadata() external onlyRole(NFT_ROLE) {
        MRPSProject.metadataLocked = true;
        emit MetadataLocked();
    }

    // set the mint Price
    function setMintPrice (uint256 _price) external onlyRole(NFT_ROLE) {
        MRPSProject.mintPrice = _price;
    }

    // Sets the root of the WL addresses Merkle Tree
    function setWLMerkleRoot (bytes32 _root) external onlyRole(MANAGER_ROLE) {
        wlMerkleRoot = _root;
    }

    // Check if an address is on the whitelist
    function addressIsOnWL(address account, bytes32[] memory _proof) internal view returns (bool) {
        bytes32 leaf = keccak256(bytes.concat(keccak256(abi.encode(account))));
        return MerkleProof.verify(_proof, wlMerkleRoot, leaf);
    }

    // handle Items uri retrieval
    function uri(uint256 _id) override public view ifIdExists(_id) returns (string memory) {
        if ( ! MRPSProject.revealed ) {
            // The original uri
            // Unrevealed image with no metadata
            return string(
                abi.encodePacked(
                    'data:application/json;base64,',
                    Base64.encode(
                        bytes(
                            string(
                                abi.encodePacked(
                                    '{"name": "', MRPSitems[_id].name,
                                    '", "image": "', MRPSitems[MRPSProject.collectionSize+1].uri,
                                    '", "description": "',
                                    '", "attributes": [',
                                    ']}'
                                )
                            )
                        )
                    )
                )
            );
        } else {
            // A Dynamic JSON Data URL
            // Actual art and specific metadata
            string memory localDescription = MRPSitems[_id].description;
            if (_id >= 1 && _id <= MRPSProject.collectionSize ) {
                localDescription = string(abi.encodePacked(
                    unicode"Créé par *",MRPSitems[_id].artist,unicode"*, ce papillon monarque s’envole pour aider l’association **Accueil Service** à ouvrir une **Maison d’Accueil pour les femmes victimes de violences**. Numéro SOLIDARITÉ FEMMES : 3919 (Appel anonyme et gratuit)"
                ));
            }
            return string(
                abi.encodePacked(
                    'data:application/json;base64,',
                    Base64.encode(
                        bytes(
                            string(
                                abi.encodePacked(
                                    '{"name": "', MRPSitems[_id].name,
                                    '", "image": "', MRPSitems[_id].uri,
                                    '", "description": "', localDescription,
                                    '", "attributes": [',
                                        '{"trait_type": "Artiste", "value": "', MRPSitems[_id].artist, '"},',
                                        unicode'{"trait_type": "Rareté", "value": "', MRPSitems[_id].rarity, '"},',
                                        unicode'{"trait_type": "Série", "value": "', MRPSitems[_id].series, '"}',
                                    ']}'
                                )
                            )
                        )
                    )
                )
            );
        }
    }

    // Add an item to the project
    function addNFT (item memory _newItem)
        public 
        nonReentrant
        onlyRole(NFT_ROLE)
        returns (uint256)
    {
        MRPSProject.maxId++;
        MRPSitems[MRPSProject.maxId] = _newItem;
        return MRPSProject.maxId;
    }

    // Add multiples item to the project
    function addNFTs (item[] memory _newItems)
        public 
        nonReentrant
        onlyRole(NFT_ROLE)
        returns (uint256)
    {
        for (uint i = 0 ; i<_newItems.length ; i++) {
            MRPSProject.maxId++;
            MRPSitems[MRPSProject.maxId] = _newItems[i];
        }
        return MRPSProject.maxId;
    }

    // Update items metadata
    function updateMetadata (uint[] memory ids, string[] memory _names,
                             string[] memory _descriptions, string[] memory _uris,
                             string[] memory _artists, string[] memory _rarity,
                             string[] memory _series) external onlyRole(NFT_ROLE) ifMetadataUnlocked {
        for (uint i = 0 ; i<ids.length ; i++) {
            require(ids[i] >= 1 && ids[i] <= MRPSProject.maxId, "Nonexistent id");
            if (bytes(_names[i]).length > 0) MRPSitems[ids[i]].name = _names[i];
            if (bytes(_descriptions[i]).length > 0) MRPSitems[ids[i]].description = _descriptions[i];
            if (bytes(_uris[i]).length > 0) MRPSitems[ids[i]].uri = _uris[i];
            if (bytes(_artists[i]).length > 0) MRPSitems[ids[i]].artist = _artists[i];
            if (bytes(_rarity[i]).length > 0) MRPSitems[ids[i]].rarity = _rarity[i];
            if (bytes(_series[i]).length > 0) MRPSitems[ids[i]].series = _series[i];
        }
    }

    // Computes the total supply
    function totalSupply() external view returns (uint) {
        uint supply = 0;
        for (uint id = 1; id <= MRPSProject.maxId; id++) {
            supply += MRPSitems[id].currentSupply;
        }
        return supply;
    }

    // Withdraw all the WETH to the current owner's address
    function withdrawFunds() external onlyRole(BANK_ROLE) {
        require (iWETH.balanceOf(address(this)) > 0, "Empty balance");
        require (iWETH.transfer(msg.sender, iWETH.balanceOf(address(this))), "Withdrawal failed");
        emit Withdrawal(msg.sender);
    } 

    function airdrop(uint256 _id, address[] memory _to) public onlyRole(MANAGER_ROLE) {
        require(_id > MRPSProject.collectionSize, "Pas d'airdrop des 1/1");
        for (uint i = 0; i < _to.length; i++) {
            _mint(_to[i], _id, 1, "");
            MRPSitems[_id].currentSupply += 1;
        }
    }

    // The following functions are overrides required by Solidity.
    function supportsInterface(bytes4 interfaceId) public view override(ERC1155, AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}