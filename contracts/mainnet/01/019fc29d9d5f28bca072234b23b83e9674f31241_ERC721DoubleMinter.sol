// File contracts/ERC721DoubleMinter.sol

pragma solidity ^0.8.0;







contract ERC721DoubleMinter is Context, Ownable, Pausable, ReentrancyGuard {
    using Counters for Counters.Counter;

    struct Round {
        uint256 supply;
        uint256 price;
    }

    address private _bank_1;
    address private _bank_2;
    IERC721Mintable private _nft_1;
    IERC721Mintable private _nft_2;
    ITether private _priceToken;
    mapping(uint256 => Round) private _rounds_1;
    mapping(uint256 => Round) private _rounds_2;
    uint256 private _numOfRounds_1;
    uint256 private _numOfRounds_2;
    mapping(address => uint256) private _referalsByAddress;
    mapping(uint256 => address) private _referalsById;
    Counters.Counter private _referalIdTracker;
    uint256 private _referalFraction;

    event ReferalAdded(address indexed referalAddress, uint256 indexed id);
    event ReferalPaid(
        uint256 indexed collection,
        address indexed referalAddress,
        uint256 indexed amount
    );

    constructor(
        uint256[] memory roundSupplies_1,
        uint256[] memory roundPrices_1,
        uint256[] memory roundSupplies_2,
        uint256[] memory roundPrices_2,
        uint256 referalFraction,
        address nft_1,
        address nft_2,
        address priceToken,
        address bank_1,
        address bank_2
    ) {
        _nft_1 = IERC721Mintable(nft_1);
        _nft_2 = IERC721Mintable(nft_2);
        _priceToken = ITether(priceToken);
        _bank_1 = bank_1;
        _bank_2 = bank_2;
        _setRounds(roundSupplies_1, roundPrices_1, 1);
        _setRounds(roundSupplies_2, roundPrices_2, 2);
        _setReferalFraction(referalFraction);
        _referalIdTracker.increment();
    }

    function mint(
        uint256 referalId,
        uint256 tokens,
        uint256 collection
    ) external whenNotPaused nonReentrant {
        address referalAddress = _referalsById[referalId];
        for (uint256 i = 0; i < tokens; i++) {
            uint256 price = currentPrice(collection);
            uint256 referalFee = _referalFee(price);
            uint256 shareFraction;
            if (collection == 1) {
                if (_nft_1.totalSupply() < 52) {
                    shareFraction = 500;
                } else {
                    shareFraction = 925;
                }
            } else {
                shareFraction = 1000;
            }

            if (referalAddress != address(0)) {
                uint256 priceAfterReferal = price - referalFee;
                uint256 price_1 = ((price - referalFee) * shareFraction) / 1000;
                uint256 price_2 = priceAfterReferal - price_1;
                _priceToken.transferFrom(
                    _msgSender(),
                    referalAddress,
                    referalFee
                );
                _priceToken.transferFrom(_msgSender(), _bank_1, price_1);
                if (price_2 != 0) {
                    _priceToken.transferFrom(_msgSender(), _bank_2, price_2);
                }
                emit ReferalPaid(collection, referalAddress, referalFee);
            } else {
                uint256 price_1 = (price * shareFraction) / 1000;
                uint256 price_2 = price - price_1;
                _priceToken.transferFrom(_msgSender(), _bank_1, price_1);
                if (price_2 != 0) {
                    _priceToken.transferFrom(_msgSender(), _bank_2, price_2);
                }
            }
            if (collection == 1) {
                _nft_1.mint(_msgSender());
            } else {
                _nft_2.mint(_msgSender());
            }
        }
    }

    function addReferal(
        address referal
    ) external whenNotPaused returns (uint256) {
        uint256 referalId = _referalIdTracker.current();
        _referalsById[referalId] = referal;
        _referalsByAddress[referal] = referalId;
        _referalIdTracker.increment();
        emit ReferalAdded(referal, referalId);
        return referalId;
    }

    function getReferalIdByAddress(
        address referal
    ) external view returns (uint256) {
        return _referalsByAddress[referal];
    }

    function nextReferalId() external view returns (uint256) {
        return _referalIdTracker.current();
    }

    function setRounds(
        uint256[] memory roundSupplies,
        uint256[] memory roundPrices,
        uint256 collection
    ) external onlyOwner {
        _setRounds(roundSupplies, roundPrices, collection);
    }

    function setBank(address bank, uint256 index) external onlyOwner {
        if (index == 1) {
            _bank_1 = bank;
        } else {
            _bank_2 = bank;
        }
    }

    function _setReferalFraction(uint256 referalFraction) private {
        require(
            referalFraction <= _feeDenominator(),
            "ERC721Minter: referal fee will exceed sale price"
        );
        _referalFraction = referalFraction;
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function currentPrice(uint256 collection) public view returns (uint256) {
        uint256 currentRoundIndex = _currentRoundIndex(collection);
        if (collection == 1) {
            return _rounds_1[currentRoundIndex].price;
        } else {
            return _rounds_2[currentRoundIndex].price;
        }
    }

    function _setRounds(
        uint256[] memory roundSupplies,
        uint256[] memory roundPrices,
        uint256 collection
    ) private {
        uint256 roundSuppliesLength = roundSupplies.length;
        uint256 roundPricesLength = roundPrices.length;
        require(
            roundSuppliesLength == roundPricesLength,
            "ERC721Minter: Must have same number of round supplies and round prices"
        );
        if (collection == 1) {
            _numOfRounds_1 = roundSuppliesLength;
            for (uint256 i = 0; i < _numOfRounds_1; i++) {
                _rounds_1[i] = Round(roundSupplies[i], roundPrices[i]);
            }
        } else {
            _numOfRounds_2 = roundSuppliesLength;
            for (uint256 i = 0; i < _numOfRounds_2; i++) {
                _rounds_2[i] = Round(roundSupplies[i], roundPrices[i]);
            }
        }
    }

    function _currentRoundIndex(
        uint256 collection
    ) private view returns (uint256 roundIndex) {
        if (collection == 1) {
            uint256 nftSupply = _nft_1.totalSupply();
            for (uint256 i = 0; i < _numOfRounds_1; i++) {
                if (nftSupply < _rounds_1[i].supply) {
                    return i;
                }
            }
        } else {
            uint256 nftSupply = _nft_2.totalSupply();
            for (uint256 i = 0; i < _numOfRounds_2; i++) {
                if (nftSupply < _rounds_2[i].supply) {
                    return i;
                }
            }
        }
        require(
            false,
            "ERC721Minter: More tokens minted than allowed in rounds"
        );
    }

    function _referalFee(uint256 price) private view returns (uint256) {
        return (price * _referalFraction) / _feeDenominator();
    }

    function _feeDenominator() private pure returns (uint96) {
        return 10000;
    }
}