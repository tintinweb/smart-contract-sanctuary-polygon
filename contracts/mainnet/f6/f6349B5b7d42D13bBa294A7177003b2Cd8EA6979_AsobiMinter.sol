import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./interface/INFT.sol";

// SPDX-License-Identifier: UNLICENSED
pragma solidity =0.8.19;

contract AsobiMinter is Ownable, ReentrancyGuard {
    using Counters for Counters.Counter;
    using Strings for uint256;

    address public immutable nft;
    Counters.Counter private _totalRules;

    mapping(uint256 => ClaimRules) public claimRules;

    struct ClaimRules {
        uint256 start;
        uint256 end;
        bytes32 merkleRoot;
        string ipfsClaim;
    }

    constructor(address nft_) {
        nft = nft_;
    }

    function totalRules() public view returns (uint256) {
        return _totalRules.current();
    }

    function verify(
        uint256 claimId,
        uint256 nftId,
        address receiver,
        bytes32[] memory claimProof
    ) public view returns (bool) {
        ClaimRules memory data = claimRules[claimId];

        require(
            data.start < block.timestamp && data.end > block.timestamp,
            "AsobiMinter : Invalid verify action"
        );

        bytes32 leaf = keccak256(abi.encodePacked(receiver, nftId.toString()));

        return MerkleProof.verify(claimProof, data.merkleRoot, leaf);
    }

    function mint(
        address[] memory receivers,
        uint256[] memory ids
    ) external onlyOwner nonReentrant {
        require(
            receivers.length == ids.length,
            "AsobiMinter : lenght must same"
        );

        for (uint256 a; a < receivers.length; a++) {
            INFT(nft).lazyMint(receivers[a], ids[a]);
        }
    }

    function createClaimRules(
        ClaimRules memory data
    ) external onlyOwner nonReentrant {
        uint256 index = totalRules();
        _totalRules.increment();

        claimRules[index] = data;
    }

    function claim(
        uint256 claimId,
        uint256 nftId,
        bytes32[] memory claimProof
    ) external nonReentrant {
        require(
            verify(claimId, nftId, _msgSender(), claimProof) == true,
            "AsobiMinter : Invalid verify action"
        );

        INFT(nft).lazyMint(_msgSender(), nftId);
    }
}

