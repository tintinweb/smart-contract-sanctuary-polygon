pragma solidity ^0.8.18;

contract LfiNftUpgrade is Initializable, UUPSUpgradeable, ERC1155Upgradeable {
    event safeMinted(uint256 tokenId, uint256 emissionDate);
    uint256 public totalSupply;
    uint256 public mintCount;
    uint256 public burnCount;
    uint256 public marketPrice;
    uint256 supply;
    string private _name;
    string private _symbol;
    uint256 tokenid;
    mapping(uint256 => address) public nftOwner;
    mapping(address => bool) public adminList;
    mapping(uint256 => string) public softwareNftInfo;
    mapping(address => uint256[]) private nftHolderInfo;

    constructor() {
        _disableInitializers();
    }

    function initialize() public initializer {
        __ERC1155_init("");
        __Ownable_init();
        __UUPSUpgradeable_init();
    }

    function safeMint(string[] calldata _fileUri, address[] calldata _nftHolder)
        public
    {
        require(
            owner() == msg.sender || adminList[msg.sender] == true,
            "address cannot mint"
        );
        require(
            _nftHolder.length == _fileUri.length,
            "address and fileUri length not match"
        );
        for (uint256 i = 0; i < _nftHolder.length; i++) {
            uint256 emissionDate = block.timestamp;
            softwareNftInfo[mintCount] = _fileUri[i];
            _mint(_nftHolder[i], mintCount, 1, "");
            nftOwner[mintCount] = _nftHolder[i];
            nftHolderInfo[_nftHolder[i]].push(mintCount);
            emit safeMinted(mintCount, emissionDate);
            mintCount++;
            totalSupply++;
        }
    }

    function getNFtHolding(address owner)
        public
        view
        returns (uint256[] memory)
    {
        return nftHolderInfo[owner];
    }

    function burnNft(address[] memory _nftHolder, uint256[] memory id) public {
          require(_nftHolder.length == id.length, "Array length mismatch");

        require(
            owner() == msg.sender || adminList[msg.sender] == true,
            "address cannot mint"
        );
        for (uint256 i = 0; i < _nftHolder.length; i++) {
         _burn(_nftHolder[i], id[i], 1);
            delete nftOwner[id[i]] ;
            burnCount += 1;
            totalSupply -= 1;
            address userAddr = _nftHolder[i];
            uint userValue = id[i];
            uint[] storage array =nftHolderInfo[userAddr];
            for (uint j = 0; j < array.length; j++) {
                if (array[j] == userValue) {
                    array[j] = array[array.length - 1];
                    array.pop();
                    break;
                }

        }
    
        }

    }

    
    function updateDiffMetadata(uint256[] memory id , string [] memory _fileUri ) public {
        require( owner() == msg.sender ||adminList[msg.sender] == true  , "address cannot mint");
        require(id.length == _fileUri.length ,"address and fileUri length not match"); 
        for (uint256 i = 0; i < id.length; i++) {
         softwareNftInfo[id[i]] = _fileUri[i]; 
        }
}
    function updateCommonMetadata(uint256[] memory id , string  memory _fileUri ) public {
        require( owner() == msg.sender ||adminList[msg.sender] == true  , "address cannot mint");
        for (uint256 i = 0; i < id.length; i++) {
         softwareNftInfo[id[i]] = _fileUri; 
        }
}
    function addNRemoveAdmin(address[] memory _addr, bool flag)
        public
        onlyOwner
    {
        for (uint256 i; i < _addr.length; i++) {
            adminList[_addr[i]] = flag;
        }
    }

    function updateMarketPrice(uint256 _marketPrice) public onlyOwner {
        marketPrice = _marketPrice;
    }

    function addStackingContract(address _stackingContractAddress)
        public
        override
        onlyOwner
    {
        stakingContract = _stackingContractAddress;
    }

    function getNftContractInfo()
        public
        view
        returns (
            uint256 _carryingCapacity,
            uint256 _mintCount,
            uint256 _burnCount,
            uint256 _totalSupply,
            uint256 _marketPrice
        )
    {
        _carryingCapacity = carryingCapacity();
        _mintCount = mintCount;
        _burnCount = burnCount;
        _totalSupply = totalSupply;
        _marketPrice = marketPrice;
        return (
            _carryingCapacity,
            _mintCount,
            _burnCount,
            _totalSupply,
            _marketPrice
        );
    }

    function changeNameSymbol(string memory name_, string memory symbol_)
        public
        onlyOwner
    {
        _name = name_;
        _symbol = symbol_;
    }

    function name() public view virtual returns (string memory) {
        return _name;
    }

    function symbol() public view virtual returns (string memory) {
        return _symbol;
    }

    function getMintCount() public view returns (uint256) {
        return mintCount;
    }

    function carryingCapacity() public view returns (uint256) {
        return 15 * (totalSupply / 250000);
    }

    // maybe removable
    function getTotalSupply() public view returns (uint256) {
        require(mintCount >= burnCount, "invalid");
        return totalSupply;
    }

    function _authorizeUpgrade(address _newImplementation)
        internal
        override
        onlyOwner
    {}
}