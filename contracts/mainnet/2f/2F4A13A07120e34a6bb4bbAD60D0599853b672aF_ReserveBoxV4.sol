contract ReserveBoxV4 is
    Initializable,
    MetaContextUpgradeable,
    IERC721ReceiverUpgradeable,
    IReserveBoxV4,
    ReentrancyGuardUpgradeable
{
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;
    //using EnumerableMapUpgradeable for EnumerableMapUpgradeable.UintToUintMap;
    //using EnumerableMapUpgradeable for EnumerableMapUpgradeable.UintToAddressMap;
    using EnumerableMapUpgradeable for EnumerableMapUpgradeable.AddressToUintMap;

    struct StakeItem {
        uint256 tokenId;
        uint256 startTime;
        uint256 endTime;
    }

    struct StakeRecord {
        address user;
        //changing on stake|unstake
        uint256 cytCount;
        //user reserved number of box for all seasons : increment on reserve box
        uint256 numberOfBoxes;
        //changing on stake|unstake
        uint256 maxBoxAmount;
        //tokenIds[]: changing on stake|unstake
        EnumerableSetUpgradeable.UintSet tokenIds;
        //tokenId=>StakeItem : changing on stake|unstake
        mapping(uint256 => StakeItem) items;
    }

    struct SeasonData {
        uint256 sid;
        uint256 startTime;
        uint256 endTime;
        uint256 eggPrice;
        uint256 boxPrice;
        uint256 boxSupply;
        uint256 lockDuration;
        //address[] : all users for this season
        EnumerableSetUpgradeable.AddressSet users;
        //address=>numberOfBoxes : every user's number of boxes for this season
        EnumerableMapUpgradeable.AddressToUintMap userReserveBoxCount;
    }
    bytes32 public constant NEW_SEASON_ROLE = keccak256("NEW_SEASON_ROLE");
    address private _selfAddress;
    address private _mysteryBoxAddress;
    address private _crazyTokenAddress;
    address private _crazyTicketNFTAddress;
    address private _fundsVaultAddress;

    uint32[][] private _reserveAmountCondition;

    uint256 private _crazyBoxPrice;
    uint256 private _crazyEggPrice;
    uint256 private _boxSupply;
    uint256 private _tokenLockDuration;
    uint256 private _seasonDuration;
    //auto increment id for every season
    CountersUpgradeable.Counter private _seasonIdTracker;
    //all users for all seasons
    EnumerableSetUpgradeable.AddressSet _allStakeUsers;
    //all staked reocrds for all users
    mapping(address => StakeRecord) private _stakeRecords;
    //all seasons data
    mapping(uint256 => SeasonData) private _seasonData;
    uint256 public currentSeasonId;

    function initialize(
        address _box,
        address _cyt,
        address _crazy
    ) public initializer {
        __ReserveBox_init(_box, _cyt, _crazy);
    }

    function __ReserveBox_init(
        address _box,
        address _cyt,
        address _crazy
    ) internal initializer {
        _selfAddress = address(this);
        _mysteryBoxAddress = _box;
        _crazyTokenAddress = _cyt;
        _crazyTicketNFTAddress = _crazy;

        _reserveAmountCondition = [
            [uint32(0), 0, 0],
            [uint32(1), 3000, 3],
            [uint32(2), 15000, 15],
            [uint32(3), 30000, 25],
            [uint32(4), 75000, 40],
            [uint32(5), 150000, 60],
            [uint32(6), 300000, 80],
            [uint32(7), 600000, 100]
        ];
        //default lock 3 days, after that can be unstaked.
        _tokenLockDuration = 3600 * 24 * 3;
        // sid started from 1.
        _seasonIdTracker.increment();
        // default season duration is 7 days.

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(NEW_SEASON_ROLE, _msgSender());
        __MetaContext_init(address(this));
        __ReentrancyGuard_init();
    }

    modifier hasInited() {
        require(_crazyTokenAddress != address(0), "invalid crazyTokenAddress.");
        require(_mysteryBoxAddress != address(0), "invalid mysteryBoxAddress.");
        require(
            _crazyTicketNFTAddress != address(0),
            "invalid crazyTicketNFTAddress."
        );
        require(_crazyBoxPrice != 0, "box price must be inited.");
        require(_crazyEggPrice != 0, "egg price must be inited.");
        require(_boxSupply != 0, "box supply must be inited.");
        require(_seasonDuration != 0, "season duration must be inited.");
        require(_tokenLockDuration != 0, "token lock duration must be inited.");
        _;
    }

    //-----------------------------------------------------------------------------
    // MetaContextUpgradeable.sol
    //-----------------------------------------------------------------------------
    function _msgSender()
        internal
        view
        virtual
        override(MetaContextUpgradeable)
        returns (address sender)
    {
        if (isTrustedForwarder(msg.sender)) {
            // The assembly code is more direct than the Solidity version using `abi.decode`.
            assembly {
                sender := shr(96, calldataload(sub(calldatasize(), 20)))
            }
        } else {
            return super._msgSender();
        }
    }

    function _msgData()
        internal
        view
        virtual
        override(MetaContextUpgradeable)
        returns (bytes calldata)
    {
        if (isTrustedForwarder(msg.sender)) {
            return msg.data[:msg.data.length - 20];
        } else {
            return super._msgData();
        }
    }

    //-----------------------------------------------------------------------------
    // public funciton : IERC721Receiver
    //-----------------------------------------------------------------------------
    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes calldata
    ) public view override returns (bytes4) {
        console.log("onERC721Received");
        console.log("operator:", operator);
        console.log("from:", from);
        console.log("tokenId:", tokenId);
        return this.onERC721Received.selector;
    }

    function getVersion() public pure returns (string memory) {
        return "22.07.0315";
    }

    //-----------------------------------------------------------------------------
    // public funciton : stake
    //-----------------------------------------------------------------------------
    function stake(uint256 tokenId) public override hasInited nonReentrant {
        address user = _msgSender();
        _stake(user, tokenId);
    }

    function unstake(uint256 tokenId) public override hasInited nonReentrant {
        address user = _msgSender();
        _unstake(user, tokenId);
    }

    function unstakeAll() public override hasInited nonReentrant {
        address user = _msgSender();
        _unstakeAll(user);
    }

    // enumerable token list of stated.
    function totalOfUserStaked(address user)
        public
        view
        override
        returns (uint256)
    {
        StakeRecord storage record = _getStakeRecord(user);
        uint256 count = 0;
        count = record.tokenIds.length();
        return count;
    }

    function tokenOfUserByIndex(address user, uint256 index)
        public
        view
        override
        returns (uint256)
    {
        StakeRecord storage record = _getStakeRecord(user);
        uint256 count = 0;
        count = record.tokenIds.length();
        require(index >= count, "index out of range.");
        uint256 tokenId = record.tokenIds.at(index);
        return tokenId;
    }

    function totalOfStakedAll() public view override returns (uint256) {
        uint256 _totalStaked = 0;
        uint256 numberOfUsers = _allStakeUsers.length();
        for (uint256 i = 0; i < numberOfUsers; i++) {
            address user = _allStakeUsers.at(i);
            StakeRecord storage record = _getStakeRecord(user);
            uint256 count = record.tokenIds.length();
            _totalStaked += count;
        }
        return _totalStaked;
    }

    //-----------------------------------------------------------------------------
    // public funciton : reserve box
    //-----------------------------------------------------------------------------
    function getCrazyBoxPrice() public view override returns (uint256) {
        return _crazyBoxPrice;
    }

    function getCrazyEggPrice() public view override returns (uint256) {
        return _crazyEggPrice;
    }

    function getUserMaxReserveBoxAmount(address user)
        public
        view
        override
        returns (uint256)
    {
        StakeRecord storage record = _getStakeRecord(user);
        uint256 count = 0;
        count = record.maxBoxAmount;
        return count;
    }

    function getUserNumberOfBoxes(address user)
        public
        view
        override
        returns (uint256)
    {
        StakeRecord storage record = _getStakeRecord(user);
        uint256 count = 0;
        count = record.numberOfBoxes;
        return count;
    }

    function reserveBox(uint256 amount) public override hasInited nonReentrant{
        require(amount > 0, "amount must be greater than 0.");
        require(
            amount <= _boxSupply,
            "amount must be less than or equal to box supply."
        );
        address user = _msgSender();
        _reserveBox(user, amount);
    }

    function buyEgg(uint256 amount) public override hasInited nonReentrant {
        require(amount > 0, "Amount must be greater than 0");
        address user = _msgSender();
        _buyEgg(user, amount);
    }

    //----------------------------------------------------------------------------------
    // admin role functions : onlyRole(DEFAULT_ADMIN_ROLE)
    //----------------------------------------------------------------------------------
