    }

    function setCrazyTokenAddress(address addr)
        public
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _crazyTokenAddress = addr;
    }

    function getCrazyTokenAddress() public view override returns (address) {
