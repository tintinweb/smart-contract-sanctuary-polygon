    }

    function setCrazyBoxPrice(uint256 price)
        public
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _crazyBoxPrice = price;
    }

    function setCrazyEggPrice(uint256 price)
        public
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _crazyEggPrice = price;
    }

    function setBoxSupply(uint256 supply)
        public
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _boxSupply = supply;
    }

    function getBoxSupply() public view override returns (uint256) {
