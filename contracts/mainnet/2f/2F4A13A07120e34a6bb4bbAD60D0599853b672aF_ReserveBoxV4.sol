    }

    // admin role functions : onlyRole(NEW_SEASON_ROLE)
    function setNextSeason()
        public
        override
        onlyRole(NEW_SEASON_ROLE)
        hasInited
    {
        if (_isSeasonEnd()) {
            SeasonData storage season = _newSeasonData();
            currentSeasonId = season.sid;
            emit NewSeason(
                season.sid,
                season.startTime,
                season.endTime,
                season.boxSupply
            );
        } else {
            console.log("Season is not end, nothing to do...");
        }
    }

    //-----------------------------------------------------------------------------
    // internal function : stake record
    //-----------------------------------------------------------------------------
    function _stake(address user, uint256 tokenId) internal {
        require(tokenId != 0, "tokenId must not be 0.");
        // entrust to IERC721Receiver.
        _safeEntrust(user, tokenId);
        emit Stake(user, tokenId);
    }

    function _unstake(address user, uint256 tokenId) internal {
        require(tokenId != 0, "tokenId is invalid");
        require(_isLockExpired(user, tokenId), "token is locked not expired");
        _safeUnEntrust(user, tokenId);
        emit Unstake(user, tokenId);
    }

    function _unstakeAll(address user) internal {
        require(user != address(0), "user is invalid");
        uint256[] memory ids = _getUserStakedTokenAll(user);
        for (uint256 i = 0; i < ids.length; i++) {
            _safeUnEntrust(user, ids[i]);
        }
    }

    function _safeEntrust(address user, uint256 tokenId) internal {
        address from = user;
        address to = _selfAddress;

        _beforeTokenEntrust(from, to, tokenId);

        IERC721Upgradeable(_crazyTicketNFTAddress).safeTransferFrom(
            from,
            to,
            tokenId
        );

        _afterTokenEntrust(from, to, tokenId);
    }

    function _safeUnEntrust(address user, uint256 tokenId) internal {
        address from = _selfAddress;
        address to = user;
        _beforeTokenEntrust(from, to, tokenId);

        IERC721Upgradeable(_crazyTicketNFTAddress).safeTransferFrom(
            from,
            to,
            tokenId
        );

        _afterTokenEntrust(from, to, tokenId);
    }

    function _beforeTokenEntrust(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual {}

    function _afterTokenEntrust(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual {
        if (to == _selfAddress) {
            //stake
            _addTokenToStakeRecord(from, tokenId);
            _addUserToSeasonData(_getCurrentSeasonId(), from);
        } else if (from == _selfAddress) {
            //unstake
            _removeTokenFromStakeRecord(to, tokenId);
        }
    }

    //-----------------------------------------------------------------------------
    // internal function : reserve box
    //-----------------------------------------------------------------------------
    function _reserveBox(address user, uint256 amount) internal {
        require(!_isSeasonEnd(), "season end.");
        require(!_isBoxSupplySoldOut(), "box was sold out.");
        StakeRecord storage record = _getStakeRecord(user);
        require(record.maxBoxAmount >= amount, "Insufficient quota1.");
        uint256 userRemainReserveBoxAmount = _getUserNumberOfBoxes(
            currentSeasonId,
            user
        );
        require(
            userRemainReserveBoxAmount + amount <= record.maxBoxAmount,
            "Insufficient quota2."
        );
        //1. payment for reserve box amount
        _paymentBox(user, amount);
        //2. mint box to user
        ICrazyMysteryBox(_mysteryBoxAddress).mintTo(user, BoxType.BOX, amount);
        //3. update user's numberOfBoxes
        record.numberOfBoxes += amount;
        //4. update season's user numberOfBoxes
        _addUserBoxAmountToSeasonData(currentSeasonId, user, amount);
        //5. emit a ReserveBox event
        emit ReserveBox(user, amount);
    }

    function _buyEgg(address user, uint256 amount) internal {
        //0. payment of buy the `amount` egg fee.
        _paymentEgg(user, amount);
        //1. mint box to user
        ICrazyMysteryBox(_mysteryBoxAddress).mintTo(user, BoxType.EGG, amount);
    }

    function _paymentBox(address buyer, uint256 amount) internal {
        uint256 founds = _crazyBoxPrice * amount;
        require(founds > 0, "Founds must be greater than 0");
        uint256 balance = IERC20(_crazyTokenAddress).balanceOf(buyer);
        console.log("price = %d", _crazyBoxPrice);
        console.log(
            "[%d],erc20 balance = %d, founds = %d",
            amount,
            balance,
            founds
        );
        require(balance >= founds, "Not enough founds");
        IERC20(_crazyTokenAddress).transferFrom(buyer, _selfAddress, founds);
    }

    function _paymentEgg(address buyer, uint256 amount) internal {
        require(amount > 0, "Amount must be greater than 0");
        uint256 founds = _crazyEggPrice * amount;
        require(founds > 0, "Founds must be greater than 0");
        uint256 balance = IERC20(_crazyTokenAddress).balanceOf(buyer);
        console.log("price = %d", _crazyEggPrice);
        console.log(
            "[%d],erc20 balance = %d, founds = %d",
            amount,
            balance,
            founds
        );
        require(balance >= founds, "Not enough founds");
        IERC20(_crazyTokenAddress).transferFrom(buyer, address(this), founds);
    }

    //-----------------------------------------------------------------------------
    // internal function : stake data I/O add|delete|modify|query
    //-----------------------------------------------------------------------------
    function _newStakeRecord(address user)
        internal
        returns (StakeRecord storage)
    {
        StakeRecord storage record = _stakeRecords[user];
        record.user = user;
        record.cytCount = 0;
        record.numberOfBoxes = 0;
        record.maxBoxAmount = 0;
        return record;
    }

    function _getStakeRecord(address user)
        internal
        view
        returns (StakeRecord storage)
    {
        StakeRecord storage record = _stakeRecords[user];
        return record;
    }

    function _isLockExpired(address user, uint256 tokenId)
        internal
        view
        returns (bool)
    {
        StakeRecord storage record = _getStakeRecord(user);
        StakeItem memory item = record.items[tokenId];
        require(item.endTime > 0, "token is not locked(staked)");
        return item.endTime <= block.timestamp;
    }

    function _getCytCountByTokenId(uint256 tokenId)
        internal
        view
        returns (uint256)
    {
        uint256 icount = 0;
        (, , , icount) = ICrazyTicketNFT(_crazyTicketNFTAddress)
            .getTicketDetail(tokenId);
        return icount;
    }

    function _getUserStakedTokenAll(address user)
        internal
        view
        returns (uint256[] memory)
    {
        StakeRecord storage record = _getStakeRecord(user);
        uint256 count = record.tokenIds.length();
        uint256[] memory ids = new uint256[](count);
        for (uint256 i = 0; i < count; i++) {
            uint256 tokenId = record.tokenIds.at(i);
            ids[i] = tokenId;
        }
        return ids;
    }

    function _addTokenToStakeRecord(address user, uint256 tokenId)
        internal
        returns (StakeRecord storage)
    {
        require(user != address(0), "user is invalid");
        require(tokenId != 0, "tokenId is invalid");
        //1.get or create new stake record.
        StakeRecord storage record = _getStakeRecord(user);
        require(record.items[tokenId].tokenId == 0, "token is already staked.");
        //2.insert StakeItem to items
        record.items[tokenId] = StakeItem({
            tokenId: tokenId,
            startTime: block.timestamp,
            endTime: block.timestamp + _tokenLockDuration
        });
        //3.update token's numberOfCytPoint
        record.cytCount += _getCytCountByTokenId(tokenId);
        console.log("cytCount:", record.cytCount);
        //4.update user's maxBoxAmount by new cytCount
        record.maxBoxAmount = _calcMaxBoxAmount(record.cytCount);
        console.log("maxBoxAmount:", record.maxBoxAmount);
        //5.push tokenId to token list
        record.tokenIds.add(tokenId);
        //6.insert user to user list
        _allStakeUsers.add(user);
        return record;
    }

    function _removeTokenFromStakeRecord(address user, uint256 tokenId)
        internal
    {
        require(user != address(0), "user is invalid");
        require(tokenId != 0, "tokenId is invalid");
        //1.get exist stake record.
        StakeRecord storage record = _getStakeRecord(user);
        require(record.items[tokenId].tokenId != 0, "token is not staked.");
        //2.remove StakeItem from items
        delete record.items[tokenId];
        //3.update token's numberOfCytPoint
        uint256 cytCount = _getCytCountByTokenId(tokenId);
        require(cytCount > 0, "tokenId's cytCount is invalid1.");
        require(record.cytCount >= cytCount, "tokenId's cytCount is invalid2.");
        record.cytCount -= cytCount;
        //4.update user's maxBoxAmount by new cytCount
        record.maxBoxAmount = _calcMaxBoxAmount(record.cytCount);
        //5.remove tokenId from token list
        record.tokenIds.remove(tokenId);
    }

    //-----------------------------------------------------------------------------
    // internal function : season data I/O add|delete|modify|query
    //-----------------------------------------------------------------------------
    function _newSeasonData() internal returns (SeasonData storage) {
        uint256 sid = _seasonIdTracker.current();
        SeasonData storage data = _seasonData[sid];
        data.sid = sid;
        data.startTime = block.timestamp;
        data.endTime = block.timestamp + _seasonDuration;
        data.eggPrice = _crazyEggPrice;
        data.boxPrice = _crazyBoxPrice;
        data.boxSupply = _boxSupply;
        data.lockDuration = _tokenLockDuration;

        _seasonIdTracker.increment();
        return data;
    }

    function _getCurrentSeasonId() internal view returns (uint256) {
        return currentSeasonId;
    }

    function _getSeasonData(uint256 sid)
        internal
        view
        returns (SeasonData storage)
    {
        SeasonData storage data = _seasonData[sid];
        require(data.sid == sid, "invalid sid.");
        return data;
    }

    function _addUserToSeasonData(uint256 sid, address user) internal {
        require(sid != 0, "sid is invalid");
        require(user != address(0), "user is invalid");
        SeasonData storage data = _getSeasonData(sid);
        data.users.add(user);
    }

    function _addUserBoxAmountToSeasonData(
        uint256 sid,
        address user,
        uint256 amount
    ) internal {
        SeasonData storage data = _getSeasonData(sid);
        (, uint256 oldNumberOfBoxes) = data.userReserveBoxCount.tryGet(user);
        data.userReserveBoxCount.set(user, oldNumberOfBoxes + amount);
    }

    function _getBoxSales(uint256 sid) internal view returns (uint256) {
        uint256 totalSales = 0;
        bool isExist = false;
        uint256 count = 0;
        SeasonData storage data = _getSeasonData(sid);
        uint256 numberOfUsers = data.users.length();
        for (uint256 i = 0; i < numberOfUsers; i++) {
            address user = data.users.at(i);
            (isExist, count) = data.userReserveBoxCount.tryGet(user);
            if (isExist) {
                totalSales += count;
            }
        }
        return totalSales;
    }

    function _getBoxSupply(uint256 sid) internal view returns (uint256) {
        SeasonData storage data = _getSeasonData(sid);
        return data.boxSupply;
    }

    function _getBoxInventory() internal view returns (uint256) {
        uint256 boxSales = _getBoxSales(currentSeasonId);
        uint256 curBoxSupply = _getBoxSupply(currentSeasonId);
        return curBoxSupply - boxSales;
    }

    function _getUserNumberOfBoxes(uint256 sid, address user)
        internal
        view
        returns (uint256)
    {
        SeasonData storage data = _getSeasonData(sid);
        uint256 count = 0;
        bool isExist = false;
        (isExist, count) = data.userReserveBoxCount.tryGet(user);
        if (isExist) {
            return count;
        }
        return 0;
    }

    //-----------------------------------------------------------------------------
    // internal function : reserve box condition judgment
    //-----------------------------------------------------------------------------
    function _calcMaxBoxAmount(uint256 cytCount)
        internal
        view
        returns (uint256)
    {
        uint256 _amount = 0;
        for (uint256 i = 1; i < _reserveAmountCondition.length; i++) {
            uint256 temp = _reserveAmountCondition[i][1] * 1e18;
            console.log("temp:", temp);
            if (cytCount < temp) {
                _amount = _reserveAmountCondition[i - 1][2];
                console.log("_amount:", _amount);
                break;
            }
        }
        return _amount;
    }

    function _isSeasonEnd() internal view returns (bool) {
        return block.timestamp >= _seasonData[currentSeasonId].endTime;
    }

    function _isBoxSupplySoldOut() internal view returns (bool) {
        uint256 inventory = _getBoxInventory();
        return inventory == 0;
    }
}

