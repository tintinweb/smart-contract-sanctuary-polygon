import {BaseContractV2} from '../core/BaseContractV2.sol';
import {IERC20} from '../interfaces/IERC20.sol';
import {IStrategy} from './IStrategy.sol';

abstract contract Strategy is IStrategy, BaseContractV2 {
    bytes32 public constant PORTFOLIO_MANAGER = keccak256('PORTFOLIO_MANAGER');

    address public portfolioManager;
    IERC20 public usdc;
    uint256 public reinvestRate; // 3000 = 3000 / 10000 = 30%

    function __Strategy_init(
        address portofolioManager_,
        address usdc_,
        uint256 reinvestRate_
    ) internal initializer {
        __BaseContract_init();
        portfolioManager = portofolioManager_;
        grantRole(PORTFOLIO_MANAGER, portfolioManager);
        usdc = IERC20(usdc_);
        reinvestRate = reinvestRate_;
    }

    modifier onlyPortfolioManager() {
        require(
            hasRole(PORTFOLIO_MANAGER, msg.sender),
            'Restricted to PORTFOLIO_MANAGER'
        );
        _;
    }

    // --- setters

    function setPortfolioManager(address value_) public onlyAdmin {
        require(value_ != address(0), 'Zero address not allowed');

        revokeRole(PORTFOLIO_MANAGER, portfolioManager);
        grantRole(PORTFOLIO_MANAGER, value_);

        portfolioManager = value_;
        emit PortfolioManagerUpdated(value_);
    }

    function setReinvestRate(uint256 rate_) public onlyAdmin {
        reinvestRate = rate_;
        emit ReinvestRateUpdated(rate_);
    }

    // --- logic
    function claimAndReinvest(address to_)
        external
        override
        onlyPortfolioManager
        returns (uint256)
    {
        uint256 totalUsdc = _claimAndReinvest(to_);
        emit ClaimReward(totalUsdc);
        return totalUsdc;
    }

    function _claimAndReinvest(address to_)
        internal
        virtual
        returns (uint256 usdcAmountClaimed_);

    function invest(address token_, uint256 amount_)
        external
        override
        returns (uint256 usdcAmount_)
    {
        IERC20(token_).transferFrom(msg.sender, address(this), amount_);
        usdcAmount_ = _invest(token_, amount_);

        emit Invest(usdcAmount_);
        emit InvestToken(token_, amount_, usdcAmount_);
    }

    function _invest(address, uint256) internal virtual returns (uint256);

    function withdraw(address token_, uint256 amount_)
        external
        override
        onlyPortfolioManager
        returns (uint256)
    {
        uint256 result = _withdraw(token_, amount_);
        emit Withdraw(token_, result);
        return result;
    }

    function _withdraw(address token_, uint256 amount_)
        internal
        virtual
        returns (uint256);

    function netAssetValue() public view virtual override returns (uint256) {
        return 0;
    }

    uint256[50] private __gap;
}

