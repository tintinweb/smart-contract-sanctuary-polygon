contract PenroseHedgeLpStrategy is Strategy {
    uint8 public constant E_MODE_CATEGORY_ID = 0;
    uint256 public constant AAVE_INTEREST_RATE_MODE = 2; // Variable: 2
    uint16 public constant REFERRAL_CODE = 0;
    uint256 public constant ORACLE_PRICE_DECIMAL = 1e8;

    IERC20 public constant aUsdc =
        IERC20(0x625E7708f30cA75bfd92586e17077590C60eb4cD);
    IERC20 public constant wmatic =
        IERC20(0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270);
    IERC20 public constant vWmatic =
        IERC20(0x4a1c3aD6Ed28a636ee1751C69071f6be75DEb8B8);
    IERC20 public constant dyst =
        IERC20(0x39aB6574c289c3Ae4d88500eEc792AB5B947A5Eb);
    IERC20 public constant pen =
        IERC20(0x9008D70A5282a936552593f410AbcBcE2F891A97);

    IDystopiaRouter public constant dystRouter =
        IDystopiaRouter(0xbE75Dd16D029c6B32B7aD57A0FD9C1c20Dd2862e);
    IPenUserProxy public constant penrose =
        IPenUserProxy(0xc9Ae7Dac956f82074437C6D40f67D6a5ABf3E34b);
    IPenLens public constant penLens =
        IPenLens(0x1432c3553FDf7FBD593a84B3A4d380c643cbf7a2);

    // Aave
    IAavePoolAddressesProvider public constant aavePoolAddressesProvider =
        IAavePoolAddressesProvider(0xa97684ead0e402dC232d5A977953DF7ECBaB3CDb);
    uint256 public targetHealthFactor;
    uint256 public liquidationThreshold;

    IDystopiaPair public dystPair;

    struct UnderlyingAssets {
        uint256 usdcBalance;
        uint256 aaveCollateralUsdc;
        uint256 wmaticDebt;
        uint256 wmaticDebtUsdc;
        uint256 usdcInLp;
        uint256 maticInLp;
        uint256 maticInLpUsdc;
    }

    struct Delta {
        uint256 aaveCollateralUsd;
        uint256 aaveWithdrawUsd;
        uint256 aaveBorrowMaticNeeded;
        uint256 aaveRepayMaticNeeded;
        uint256 lpDeposit;
        uint256 lpWithdraw;
    }

    function initialize(
        uint256 targetHealthFactor_,
        uint256 liquidationThreshold_
    ) public initializer {
        __Strategy_init(
            // 0xE8c7574f7355920B254a91658D04074e1C0FdeF4, // portfolio manager
            msg.sender,
            0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174, // usdc
            3000 // 30%
        );
        targetHealthFactor = targetHealthFactor_;
        liquidationThreshold = liquidationThreshold_;
        dystPair = IDystopiaPair(
            dystRouter.pairFor(address(usdc), address(wmatic), false)
        );
    }

    function _claimAndReinvest(address to_)
        internal
        virtual
        override
        returns (uint256 usdcClaimed_)
    {
        address rewardAddress = penLens.stakingRewardsByDystPool(
            address(dystPair)
        );
        penrose.claimStakingRewards(rewardAddress);

        uint256 dystAmount = dyst.balanceOf(address(this));
        uint256 penAmount = pen.balanceOf(address(this));

        // convert reward to usdc
        usdcClaimed_ += _convertDystToUsdc(dystAmount);
        usdcClaimed_ += _convertPenToUsdc(penAmount);

        // Send reward usdc
        uint256 reinvestAmount = (usdcClaimed_ * reinvestRate) / 10000;
        usdc.transfer(to_, usdcClaimed_ - reinvestAmount);

        // reinvest
        _invest(address(usdc), reinvestAmount);
    }

    function _convertDystToUsdc(uint256 amount_) internal returns (uint256) {
        if (amount_ < 1e18) {
            return 0;
        }
        IDystopiaRouter.Route[] memory dystRoute = new IDystopiaRouter.Route[](
            2
        );
        dystRoute[0] = IDystopiaRouter.Route({
            from: address(dyst),
            to: address(wmatic),
            stable: false
        });
        dystRoute[1] = IDystopiaRouter.Route({
            from: address(wmatic),
            to: address(usdc),
            stable: false
        });
        uint256 amountOut = dystRouter.getAmountsOut(amount_, dystRoute)[2];
        dyst.approve(address(dystRouter), amount_);
        uint256[] memory swapDystResults = dystRouter.swapExactTokensForTokens(
            amount_,
            amountOut,
            dystRoute,
            address(this),
            block.timestamp
        );
        return swapDystResults[2];
    }

    function _convertPenToUsdc(uint256 amount_) internal returns (uint256) {
        if (amount_ < 1e18) {
            return 0;
        }
        IDystopiaRouter.Route[] memory penRoute = new IDystopiaRouter.Route[](
            2
        );
        penRoute[0] = IDystopiaRouter.Route({
            from: address(pen),
            to: address(wmatic),
            stable: false
        });
        penRoute[1] = IDystopiaRouter.Route({
            from: address(wmatic),
            to: address(usdc),
            stable: false
        });
        uint256 amountOut = dystRouter.getAmountsOut(amount_, penRoute)[2];
        pen.approve(address(dystRouter), amount_);
        uint256[] memory swapResults = dystRouter.swapExactTokensForTokens(
            amount_,
            amountOut,
            penRoute,
            address(this),
            block.timestamp
        );
        return swapResults[2];
    }

    function _invest(address token_, uint256 amount_)
        internal
        override
        returns (uint256)
    {
        require(token_ == address(usdc), 'only support usdc');
        _rebalance(0);
        return amount_;
    }

    function _rebalance(uint256 withdrawAmount_) internal {
        Delta memory delta = getDelta(withdrawAmount_);
        if (delta.aaveCollateralUsd > 1e5) {
            if (usdc.balanceOf(address(this)) < delta.aaveCollateralUsd) {
                uint256 liquidtyRequired = ((delta.aaveCollateralUsd -
                    usdc.balanceOf(address(this))) * dystPair.totalSupply()) /
                    usdc.balanceOf(address(dystPair));
                _removeLiquidity(liquidtyRequired);
            }

            usdc.approve(address(aavePool()), delta.aaveCollateralUsd);
            aavePool().supply(
                address(usdc),
                delta.aaveCollateralUsd,
                address(this),
                0
            );
        } else if (delta.aaveWithdrawUsd > 1e5) {
            aavePool().withdraw(
                address(usdc),
                delta.aaveWithdrawUsd,
                address(this)
            );
        }
        if (delta.aaveBorrowMaticNeeded > 1e17) {
            aavePool().borrow(
                address(wmatic),
                delta.aaveBorrowMaticNeeded,
                AAVE_INTEREST_RATE_MODE,
                0,
                address(this)
            );
        } else if (delta.aaveRepayMaticNeeded > 1e17) {
            if (wmatic.balanceOf(address(this)) < delta.aaveRepayMaticNeeded) {
                uint256 liquidtyRequired = ((delta.aaveRepayMaticNeeded -
                    wmatic.balanceOf(address(this))) * dystPair.totalSupply()) /
                    wmatic.balanceOf(address(dystPair));
                _removeLiquidity(liquidtyRequired);
            }
            wmatic.approve(address(aavePool()), delta.aaveRepayMaticNeeded);
            aavePool().repay(
                address(wmatic),
                delta.aaveRepayMaticNeeded,
                AAVE_INTEREST_RATE_MODE,
                address(this)
            );
        }

        // add liquidity
        uint256 usdcBalance = usdc.balanceOf(address(this));
        uint256 wmaticBalance = wmatic.balanceOf(address(this));

        if (usdcBalance < 1e6 || wmaticBalance < 1e18) {
            return;
        }

        usdc.approve(address(dystRouter), usdcBalance);
        wmatic.approve(address(dystRouter), wmaticBalance);
        (, , uint256 liquidity) = dystRouter.addLiquidity(
            address(usdc),
            address(wmatic),
            false,
            usdcBalance,
            wmaticBalance,
            1,
            1,
            address(this),
            block.timestamp
        );

        // stake
        dystPair.approve(address(penrose), liquidity);
        penrose.depositLpAndStake(address(dystPair), liquidity);
    }

    function _removeLiquidity(uint256 liquidity)
        internal
        returns (uint256 usdcAmount, uint256 wmaticAmount)
    {
        // unstake
        penrose.unstakeLpAndWithdraw(address(dystPair), liquidity);

        // remove liquidity
        dystPair.approve(address(dystPair), liquidity);
        return
            dystRouter.removeLiquidity(
                address(usdc),
                address(wmatic),
                false,
                liquidity,
                1,
                1,
                address(this),
                block.timestamp
            );
    }

    function getDelta(uint256 withdrawAmount_)
        public
        view
        returns (Delta memory delta)
    {
        UnderlyingAssets memory assets = underlyingAssets();
        uint256 nav = _netAssetValue(assets);
        nav = nav - withdrawAmount_;
        console.log('nav: {} ', nav);

        (, uint256 maticPrice) = getAssetsPrices();
        uint256 targetUsdcInLp = (nav * 1e18) /
            (1e18 + ((targetHealthFactor * 10000) / liquidationThreshold));
        uint256 targetWmaticInLp = _usdcToMatic(targetUsdcInLp, maticPrice);
        uint256 targetUsdcInAave = nav - targetUsdcInLp;

        if (targetUsdcInAave > assets.aaveCollateralUsdc) {
            delta.aaveCollateralUsd =
                targetUsdcInAave -
                assets.aaveCollateralUsdc;
            // delta.aaveWithdrawUsd = 0;
        } else {
            // delta.aaveCollateralUsd = 0;
            delta.aaveWithdrawUsd =
                assets.aaveCollateralUsdc -
                targetUsdcInAave;
        }

        if (targetUsdcInLp > assets.maticInLpUsdc) {
            delta.aaveBorrowMaticNeeded = _usdcToMatic(
                targetUsdcInLp - assets.maticInLp,
                maticPrice
            );
        } else {
            delta.aaveRepayMaticNeeded = _usdcToMatic(
                assets.maticInLpUsdc - targetUsdcInLp,
                maticPrice
            );
        }

        console.log('[DELTA] target usdc in Lp: {}', targetUsdcInLp);
        console.log('[DELTA] target Wmatic in Lp: {}', targetWmaticInLp);
        console.log('[DELTA] target usdc in aave: {}', targetUsdcInAave);
        console.log('[DELTA] aave collateral usd: {}', delta.aaveCollateralUsd);
        console.log('[DELTA] aave withdraw usd: {}', delta.aaveWithdrawUsd);
        console.log(
            '[DELTA] aave borrow matic needed: {}',
            delta.aaveBorrowMaticNeeded
        );
        console.log(
            '[DELTA] aave repay matic needed: {}',
            delta.aaveRepayMaticNeeded
        );
    }

    function getAssetsPrices()
        public
        view
        returns (uint256 usdcPrice, uint256 maticPrice)
    {
        IAaveOracle oracle = IAaveOracle(
            aavePoolAddressesProvider.getPriceOracle()
        );
        address[] memory assets = new address[](2);
        assets[0] = address(usdc);
        assets[1] = address(wmatic);
        uint256[] memory prices = oracle.getAssetsPrices(assets);
        usdcPrice = prices[0];
        maticPrice = prices[1];
    }

    function _usdcToMatic(uint256 usdcAmount_, uint256 maticPrice_)
        internal
        pure
        returns (uint256)
    {
        return (usdcAmount_ * 1e12 * ORACLE_PRICE_DECIMAL) / maticPrice_;
    }

    function netAssetValue() public view virtual override returns (uint256) {
        UnderlyingAssets memory assets = underlyingAssets();
        return _netAssetValue(assets);
    }

    function _netAssetValue(UnderlyingAssets memory assets)
        internal
        pure
        returns (uint256)
    {
        return
            assets.usdcBalance +
            assets.aaveCollateralUsdc -
            assets.wmaticDebtUsdc +
            assets.usdcInLp +
            assets.maticInLpUsdc;
    }

    function underlyingAssets()
        public
        view
        returns (UnderlyingAssets memory assets)
    {
        (, uint256 wmaticPrice) = getAssetsPrices();
        assets.usdcBalance = usdc.balanceOf(address(this));
        assets.aaveCollateralUsdc = aUsdc.balanceOf(address(this));

        assets.wmaticDebt = vWmatic.balanceOf(address(this));
        assets.wmaticDebtUsdc = _maticToUsdc(assets.wmaticDebt, wmaticPrice);

        address userProxy = penLens.userProxyByAccount(address(this));
        address penStakingAddress = penLens.stakingRewardsByDystPool(
            address(dystPair)
        );
        uint256 poolBalance = IERC20(penStakingAddress).balanceOf(userProxy);
        (uint256 maticReserve, uint256 usdcReserve, ) = dystPair.getReserves();
        assets.usdcInLp = (poolBalance * usdcReserve) / dystPair.totalSupply();
        assets.maticInLp =
            (poolBalance * maticReserve) /
            dystPair.totalSupply();
        assets.maticInLpUsdc = _maticToUsdc(assets.maticInLp, wmaticPrice);

        console.log('[ASSETS] matic in Lp: {}', assets.maticInLp);
        console.log('[ASSETS] matic in usdc in Lp: {}', assets.maticInLpUsdc);
        console.log('[ASSETS] usdc in Lp: {}', assets.usdcInLp);
        console.log('[ASSETS] usdc in contract: {}', assets.usdcBalance);
        console.log('[ASSETS] usdc in Aave: {}', assets.aaveCollateralUsdc);
        console.log('[ASSETS] matic debt in Aave: {}', assets.wmaticDebt);
    }

    function _maticToUsdc(uint256 maticAmount_, uint256 maticPrice_)
        internal
        pure
        returns (uint256)
    {
        return (maticAmount_ * maticPrice_) / (1e12 * ORACLE_PRICE_DECIMAL);
    }

    function aavePool() public view returns (IAavePool) {
        return IAavePool(aavePoolAddressesProvider.getPool());
    }

    function aaveHealthFactor() public view returns (uint256) {
        (, , , , , uint256 currentHealthFactor) = aavePool().getUserAccountData(
            address(this)
        );
        return currentHealthFactor;
    }

    function _withdraw(address, uint256) internal override returns (uint256) {
        revert('not implemented');
    }
}

