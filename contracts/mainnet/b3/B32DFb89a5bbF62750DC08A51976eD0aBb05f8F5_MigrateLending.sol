pragma solidity >= 0.8.11;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "../Lending/ILendingControl.sol";
import "../Lending/ILending.sol";

contract MigrateLending is AccessControl, ILending {
    struct localInfo {
        address user;
        uint256 id;
    }

    bytes32 public constant DEV_ROLE = keccak256("DEV_ROLE");
    address private immutable usdt; //USDT Mainnet

    ILendingControl private oldLControl;
    ILendingControl private newLControl;

    constructor(address _oldLControl, address _newLControl) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(DEV_ROLE, msg.sender);
        _setupRole(DEV_ROLE, 0x30268390218B20226FC101cD5651A51b12C07470);
        oldLControl = ILendingControl(_oldLControl);
        newLControl = ILendingControl(_newLControl);
        usdt = 0xc2132D05D31c914a87C6611C10748AEb04B58e8F;
    }

    modifier onlyDev() {
        if (!hasRole(DEV_ROLE, msg.sender)) {
            revert("Not enough Permissions");
        }
        _;
    }

    function migrateInfo(address[] calldata _user, uint256[] calldata _lendingId) public onlyDev {
        Data memory info;
        localInfo memory vars;
        uint256 length = _user.length;
        if (length != _lendingId.length) {
            revert("array mismatch");
        }
        for (uint256 i = 0; i < length;) {
            vars.user = _user[i];
            vars.id = _lendingId[i];

            (
                info.amount,
                info.depositTime,
                address(0),
                info.lastCalcTime,
                info.payPerSecond,
                info.withdrawTime,
                info.pendRewards,
                info.daysLeft
            );
            info = getInfo(vars.user, vars.id);
            newLControl.addInfo(vars.id, vars.user);
            newLControl.updatePosition(vars.user, vars.id, info, usdt);

            unchecked {
                ++i;
            }
        }
    }

    function getInfo(address _user, uint256 _id) internal view returns (Data memory) {
        Data memory info;
        uint256 _depositTime;
        (info.amount, _depositTime, info.token, info.lastCalcTime, info.payPerSecond, info.withdrawTime) =
            oldLControl.getLenderInfo(_user, _id);
        info.depositTime = safeCastToUint96(_depositTime);
        (info.claimed, info.claimTime,) = oldLControl.getRewardsClaimed(_user, _id);
        info.lastSetClaimRewards = oldLControl.getLastSetClaimRewards(_user, _id);
        info.pendRewards = oldLControl.getPendingRewards(_user, _id);
        info.daysLeft = oldLControl.getDaysLeft(_user, _id);
        return info;
    }

    function migrateRewards(address _user, uint256 _request, uint8 _flag) public onlyDev {
        localInfo memory vars;
        vars.user = _user;
        vars.id = _request;
        uint96 _lastCalcTime;

        (,,, _lastCalcTime,,) = oldLControl.getLenderInfo(vars.user, vars.id);

        (uint256 _amount, uint256 _rewards,, uint96 _date) = oldLControl.getRequest(vars.user, vars.id);
        newLControl.updateRequest(vars.user, _amount, _rewards, _flag, _date, _lastCalcTime);
    }

    function safeCastToUint96(uint256 value) private pure returns (uint96) {
        require(value <= type(uint96).max, "SafeCast: Value doesn't fit in 96 bits");
        return uint96(value);
    }
}

