pragma solidity 0.8.16;

import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "./utils/VRFConsumerBaseV2Upgradeable.sol";
import "./IUniversusSkillTreeNFT.sol";

interface ILootBoxNFT is IERC721Upgradeable, IERC721ReceiverUpgradeable {
    function mint(address _to, uint8 _tier) external;

    function burn(uint256 tokenId) external;

    function lockForReveal(address _owner, uint256 _tokenId) external;

    function tokenTierMap(uint256 _tokenId) external view returns (uint8);
}

interface IUniversusNFT {
    function mint(
        address _to,
        string[] calldata _traits,
        string[] calldata _values
    ) external;
}

contract ShipReveal is
    IERC721ReceiverUpgradeable,
    Initializable,
    PausableUpgradeable,
    AccessControlEnumerableUpgradeable,
    ReentrancyGuardUpgradeable,
    VRFConsumerBaseV2Upgradeable
{
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    uint16 public constant INVERSE_BASIS_POINT = 10000;
    bytes4 private constant _ERC721_RECEIVED = 0x150b7a02;

    VRFCoordinatorV2Interface public COORDINATOR;
    LinkTokenInterface public LINK;
    uint64 public subscriptionId;
    uint32 public callbackGasLimit;
    uint32 public numWords;
    uint16 public requestConfirmations;
    bytes32 public keyHash;

    IUniversusSkillTreeNFT public nft;
    string[] public rarities;
    TypeWithClass[] public shipTypes;
    mapping(uint8 => uint16[]) public rarityOdds;
    mapping(address => bool) public authorizedLootBoxes;
    mapping(address => uint8) public maxTier;
    mapping(uint256 => RevealRequest) public requests;
    mapping(address => EnumerableSetUpgradeable.UintSet) private activeRequestsByAccount;

    enum RequestStatus {
        REQUESTED,
        SEEDS_RECEIVED,
        COMPLETED
    }

    struct RevealRequest {
        uint256 requestId;
        address owner;
        ILootBoxNFT lootBox;
        uint256 boxId;
        uint256 tokenId;
        uint256 timestamp;
        uint256[] words;
        string[] traits;
        string[] value;
        RequestStatus status;
    }
    struct TypeWithClass {
        string itemType;
        string itemClass;
    }

    event BoxOpened(
        uint256 requestId,
        uint256 boxId,
        uint256 typeWithClassIndex,
        uint256 rarityIndex,
        uint256 tokenId
    );
    event SeedReceived(uint256 requestId, uint256[] randomWords);
    event NewOpenRequest(ILootBoxNFT lootbox, uint256 boxId, address sender);

    // each nft contract will have a set of tiers and a destination nft address
    // each tier will have three randomizable props: Class, Type and Rarity
    // open will burn the box nft and mint a ship or android
    // odds and props are set by lootbox address
    function initialize(
        ILootBoxNFT _promoLootBox,
        ILootBoxNFT _shipLootBox,
        IUniversusSkillTreeNFT _nft,
        address _vrfCoordinator,
        bytes32 _keyHash,
        uint64 _subscriptionId,
        address _operator
    ) public initializer {
        __Pausable_init();
        __AccessControl_init();
        __VRFConsumerBaseV2_init(_vrfCoordinator);

        COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
        keyHash = _keyHash;
        subscriptionId = _subscriptionId;
        callbackGasLimit = 300000;
        requestConfirmations = 3;
        numWords = 2;

        nft = _nft;
        rarities = ["common", "uncommon", "exotic", "rare", "epic", "mythic"];

        TypeWithClass[15] memory tempShipTypes = [
            TypeWithClass({itemType: "pace", itemClass: "recon"}),
            TypeWithClass({itemType: "drone", itemClass: "recon"}),
            TypeWithClass({itemType: "huntress", itemClass: "recon"}),
            TypeWithClass({itemType: "fade", itemClass: "recon"}),
            TypeWithClass({itemType: "spectator", itemClass: "recon"}),

            TypeWithClass({itemType: "ripper", itemClass: "battleship"}),
            TypeWithClass({itemType: "archmage", itemClass: "battleship"}),
            TypeWithClass({itemType: "armada", itemClass: "battleship"}),
            TypeWithClass({itemType: "invader", itemClass: "battleship"}),
            TypeWithClass({itemType: "outcast", itemClass: "battleship"}),

            TypeWithClass({itemType: "emissary", itemClass: "cargo"}),
            TypeWithClass({itemType: "loader", itemClass: "cargo"}),
            TypeWithClass({itemType: "glide", itemClass: "cargo"}),
            TypeWithClass({itemType: "protector", itemClass: "cargo"}),
            TypeWithClass({itemType: "pioneer", itemClass: "cargo"})
        ];

        for (uint256 i; i < tempShipTypes.length; i++) {
            shipTypes.push(tempShipTypes[i]);
        }

        rarityOdds[0] = [7900, 9400, 9890, 9990, 10000, 10001];
        rarityOdds[1] = [750, 6000, 9350, 9850, 10000, 10001];
        rarityOdds[2] = [0, 0, 3400, 8400, 9900, 10000];
        rarityOdds[3] = [0, 0, 0, 0, 6000, 10000];

        authorizedLootBoxes[address(_promoLootBox)] = true;
        authorizedLootBoxes[address(_shipLootBox)] = true;

        maxTier[address(_promoLootBox)] = 0;
        maxTier[address(_shipLootBox)] = 3;

        _grantRole(DEFAULT_ADMIN_ROLE, _operator);
        _grantRole(PAUSER_ROLE, _operator);
    }

    function prepare(ILootBoxNFT _lootBox, uint256 _boxId)
        external
        nonReentrant
        whenNotPaused
    {
        (uint256 balance,,,) = COORDINATOR.getSubscription(subscriptionId);
        require(
            balance >= 1 ether,
            "Insufficient subscription balance"
        );

        require(
            authorizedLootBoxes[address(_lootBox)],
            "LootBox not authorized"
        );

        require(
            _lootBox.tokenTierMap(_boxId) <= maxTier[address(_lootBox)],
            "Tier not authorized"
        );

        // ownerOf() will also check whether token exists
        require(
            _lootBox.ownerOf(_boxId) == msg.sender,
            "Only token owners can open boxes"
        );

        // will transfer token id to this contract
        _lootBox.lockForReveal(msg.sender, _boxId);

        require(
            _lootBox.ownerOf(_boxId) == address(this),
            "Token lock failed"
        );

        // request VRF seed
        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );

        RevealRequest memory request = RevealRequest({
            requestId: requestId,
            owner: msg.sender,
            lootBox: _lootBox,
            boxId: _boxId,
            tokenId: 0,
            timestamp: block.timestamp,
            words: new uint256[](0),
            traits: new string[](0),
            value: new string[](0),
            status: RequestStatus.REQUESTED
        });

        requests[requestId] = request;
        activeRequestsByAccount[msg.sender].add(requestId);

        emit NewOpenRequest(_lootBox, _boxId, msg.sender);
    }

    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory _randomWords
    ) internal override {
        requests[_requestId].words = _randomWords;
        requests[_requestId].status = RequestStatus.SEEDS_RECEIVED;

        emit SeedReceived(_requestId, _randomWords);
    }

    function open(uint256 _requestId)
        external
        nonReentrant
        whenNotPaused
    {
        RevealRequest storage request = requests[_requestId];

        require(request.timestamp != 0, "No such open request");
        require(request.owner == msg.sender, "Sender is not owner");
        require(
            request.status == RequestStatus.SEEDS_RECEIVED,
            "Invalid request status"
        );

        request.status = RequestStatus.COMPLETED;

        uint8 boxTier = request.lootBox.tokenTierMap(request.boxId);

        // draw type and class (symmetric odds)
        uint256 typeWithClassIndex = _drawRandom(
            request.words[0],
            shipTypes.length
        );
        // draw rarity (asymmetric odds)
        uint256 rarityIndex = _drawWeightedIndex(
            rarityOdds[boxTier],
            request.words[1]
        );

        request.lootBox.burn(request.boxId);

        request.value = [
            shipTypes[typeWithClassIndex].itemClass,
            shipTypes[typeWithClassIndex].itemType,
            rarities[rarityIndex]
        ];

        request.traits = [
            "class",
            "type",
            "rarity"
        ];
        // mint nft to sender
        nft.mint(request.owner, request.traits, request.value);

        request.tokenId = nft.getLastMintedId();

        activeRequestsByAccount[msg.sender].remove(_requestId);

        emit BoxOpened(
            _requestId,
            request.boxId,
            typeWithClassIndex,
            rarityIndex,
            request.tokenId
        );
    }

    function _drawWeightedIndex(uint16[] memory _probabilities, uint256 _seed)
        internal
        pure
        returns (uint256 index)
    {
        uint16 value = uint16(_seed % INVERSE_BASIS_POINT) + 1; // 1 to 10000

        for (uint256 i = _probabilities.length; i > 0; i--) {
            uint16 probability = _probabilities[i - 1];

            if (value > probability) break;

            index = i - 1;
        }
    }

    function _drawRandom(uint256 _seed, uint256 _max)
        internal
        pure
        returns (uint256)
    {
        return _seed % _max;
    }

    function pause() external onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() external onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function getRequest(
        uint256 _id
    )
        external view returns (RevealRequest memory request) {
            request = requests[_id];
        }

    function getActiveRequestsByAccount(
        address _account
    ) external view returns (uint256[] memory) {
        uint256 length = activeRequestsByAccount[_account].length();
        uint256[] memory activeRequests = new uint256[](length);

        for (uint256 i; i < length; i++) {
            activeRequests[i] = activeRequestsByAccount[_account].at(i);
        }

        return activeRequests;
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external pure returns (bytes4) {
        return _ERC721_RECEIVED;
    }
}

