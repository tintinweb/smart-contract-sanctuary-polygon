import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";

interface IUniversusSkillTreeNFT is IERC721Upgradeable {

    struct Skill {
        uint8 id;
        uint8 level;
        string class;
        string abilityType;
        string name;
    }

    function mint(
        address _to,
        string[] calldata _traits,
        string[] calldata _values
    ) external;

    function exists(
        uint256 _tokenId
    )
        external
        returns (bool);

    function setTokenSkill(uint256 _tokenId, uint8 _skillId, uint8 _amount) external;

    function getSkill(uint8 _skillId) external view returns (Skill memory);

    function getTokenSkill(uint256 _tokenId, uint8 _skillId) external view returns (uint8);

    function skillTreeSize() external view returns (uint256);

    function getLastMintedId()
        external
        view
        returns (uint256);

    function getAttribute(
        uint _tokenId,
        string memory _name
    )
        external
        view
        returns (string memory);

}

