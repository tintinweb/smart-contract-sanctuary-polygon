pragma solidity ^0.8.0;

/**
 * @title Balancer wMATIC-stMATIC Strategy
 * @notice Investment strategy for investing assets via Balancer Strategy
 */
import { SafeERC20 } from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import { SafeMath } from "@openzeppelin/contracts/utils/math/SafeMath.sol";

import { StableMath } from "../../utils/StableMath.sol";
import { OvnMath } from "../../utils/OvnMath.sol";

import "../../exchanges/BalancerExchange.sol";
import "../../swapper/ISwapper.sol";
import "../../utils/Helpers.sol";
import { IERC20, InitializableAbstractStrategy } from "../../utils/InitializableAbstractStrategy.sol";
import "hardhat/console.sol";

import "../../interfaces/balancer/MerkleOrchard.sol";
import "../../interfaces/balancer/interfaces/IVault.sol";
import "../../interfaces/balancer/interfaces/IBalancerPool.sol";
import "../../interfaces/IGauge.sol";


interface IBalancerHelpers {
    function queryExit(
        bytes32 poolId,
        address sender,
        address recipient,
        IVault.ExitPoolRequest memory request
    ) external returns (uint256 bptIn, uint256[] memory amountsOut);
}


contract BalancerWMATICSTMATICStrategy is InitializableAbstractStrategy, BalancerExchange   {
    using StableMath for uint256;
    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    using OvnMath for uint256;

    IERC20 public token0;
    IERC20 public token1;
    IERC20 public balToken;
    IBalancerPool public lp;
    bytes32 public poolId;

    uint256 token0denominator;
    uint256 token1denominator;
    uint256 lpDenominator;

    IVault public balancerVault;
    IGauge public gauge;
    
    ISwapper internal swapper;

    uint256[] decimals;
    uint256[] minThresholds;

    uint256 public constant BASIS_POINTS_FOR_SLIPPAGE = 4;
    
    IBalancerHelpers public helper;

    /**
     * Initializer for setting up strategy internal state. This overrides the
     * InitializableAbstractStrategy initializer as MeshSwap strategies don't fit
     * well within that abstraction.
     @param _vaultAddress Address of Vault
     @param _rewardTokenAddresses Addresses of Reward Tokens
     @param _assets Addresses of supported asset tokens
     @param _pToken Address of the LP token of the strategy
     @param _gauge Address of the Gauge of the strategy
     @param _helper BalancerHelpers
     @param _swapper Address of the XStable swapper
     */
    function initialize(
        address _vaultAddress,    // VaultProxy address
        address[] calldata _rewardTokenAddresses, // Bal address
        address[] calldata _assets, // wMATIC+stMATIC
        address _pToken, // Balancer Strategy Pair
        address _gauge, // Balancer Strategy Gauge
        address _helper, 
        address _swapper // XStabl Swapper
    ) external onlyGovernor initializer {
        token0 = IERC20(_assets[0]); // wMatic
        token1 = IERC20(_assets[1]); // stMatic
        lp = IBalancerPool(_pToken);
        balToken = IERC20(_rewardTokenAddresses[0]); // bal token

        gauge = IGauge(_gauge);
        swapper = ISwapper(_swapper);
        helper = IBalancerHelpers(_helper);
        
        decimals.push(Helpers.getDecimals(_assets[0]));
        decimals.push(Helpers.getDecimals(_assets[1]));
        decimals.push(Helpers.getDecimals(_pToken));

        token0denominator = 10 ** Helpers.getDecimals(_assets[0]);
        token1denominator = 10 ** Helpers.getDecimals(_assets[1]);
        lpDenominator = 10 ** Helpers.getDecimals(_pToken);

        super._initialize(
            _vaultAddress,
            _rewardTokenAddresses,
            _assets
        );
        
        minThresholds.push(10 ** 14);
        minThresholds.push(10**decimals[0]); // 1 token0
        minThresholds.push(10**decimals[1]); // 2 token1
    }


    /**
    @dev Function to set Balancer Vault & Pool Id
    @param _vault Address of Balancer Vault
    @param _poolId Address of Pool Id
     */
    function setBalancerVaultAndPool(address _vault, bytes32 _poolId) external onlyGovernor {
        balancerVault = IVault(_vault);
        setBalancerVault(_vault);
        poolId = _poolId;
    }

    function setHelper(address _helper) external onlyGovernor {
        helper = IBalancerHelpers(_helper);
    }

    /**
    @dev Function to return the current reserve ratio in form 100:X
    @return (uint256,uint256) - ratios of reserves
     */
    function getRatios() public view override returns (uint256,uint256) {
        (uint256 reserve0, uint256  reserve1,) = getReserves();
        return (10**20, reserve0.scaleBy(18,  decimals[0]).mul(10**20).div(reserve1.scaleBy(18, decimals[1])) );
    }

    /**
    @dev Function to return the actual reserves of the sPool
    @return (uint256,uint256) - actual reserves ratios of the sPool
     */
    function getReserves() internal view returns (uint256,uint256,uint256) {
        (uint256 reserve0,,,) = balancerVault.getPoolTokenInfo(poolId, token0);
        (uint256 reserve1,,,) = balancerVault.getPoolTokenInfo(poolId, token1);
        require(reserve0 > (10 ** (decimals[0] - 3)) && reserve1 > (10 ** (decimals[1] - 3)), "Reserves too low");
        return (reserve0, reserve1, lp.totalSupply());
    }

    /**
    @dev Internal - function to deposit the assets to the sPools
    @param _assets Assets which are to be supported by the strategy. Ex: MATIC-stMATIC
    @param _amounts Corresponding amounts of each asset
     */
    function _deposit(
        address[] memory _assets,
        uint256[] memory _amounts
    )  internal {
        require(supportsAsset(_assets), "AS NS");
        uint256[] memory amounts = new uint256[](2);
        amounts[0] = _amounts[0];
        amounts[1] = _amounts[1];

        if (_amounts[0].scaleBy(18, decimals[0]) < _amounts[1].scaleBy(18, decimals[1])) {
            amounts[1] = swapper.getAmountOut(address(token0), address(token1), _amounts[0]);
            if (amounts[1] > _amounts[1]) {
                amounts[1] = _amounts[1];
                amounts[0] = swapper.getAmountOut(address(token1), address(token0), _amounts[0]);
            }
        } else if (_amounts[0].scaleBy(18, decimals[0]) > _amounts[1].scaleBy(18, decimals[1])) {
            amounts[0] = swapper.getAmountOut(address(token1), address(token0), _amounts[0]);
            if (amounts[0] > _amounts[0]) {
                amounts[0] = _amounts[0];
                amounts[1] = swapper.getAmountOut(address(token0), address(token1), _amounts[0]);
            }
        } else {
            amounts[1] = swapper.getAmountOut(address(token0), address(token1), _amounts[0]);
        }
        (IERC20[] memory tokens,,) = balancerVault.getPoolTokens(poolId);
        IAsset[] memory assets = new IAsset[](3);
        uint256[] memory maxAmountsIn = new uint256[](3);
        uint256[] memory amountsIn = new uint256[](3);
        for (uint256 i; i < tokens.length; i++) {
            assets[i] = IAsset(address(tokens[i]));
            if (tokens[i] == token0) {
                // if wMATIC -> set amounts
                maxAmountsIn[i] = _amounts[0];
                amountsIn[i] = _amounts[0];
            } else if (tokens[i] == token1) {
                // if stMATIC -> set amounts
                maxAmountsIn[i] = _amounts[1];
                amountsIn[i] = _amounts[1];
            } else {
                // put other tokens 0.
                maxAmountsIn[i] = 0;
                amountsIn[i] = 0;
            }
            // console.log("Balancer Strategy - %s, %s, %s", i, amountsIn[i], maxAmountsIn[i]);
        }

        token0.approve(address(balancerVault), maxAmountsIn[0]);
        token1.approve(address(balancerVault), maxAmountsIn[1]);
        uint256[] memory _actualLiquidity = new uint256[](2);
        _actualLiquidity[0] = amountsIn[0];
        _actualLiquidity[1] = amountsIn[1];
        bytes memory userData = abi.encode(1, _actualLiquidity, 0);
        IVault.JoinPoolRequest memory request = IVault.JoinPoolRequest(assets, maxAmountsIn, userData, false);
        balancerVault.joinPool(poolId, address(this), address(this), request);
        _stake(lp.balanceOf(address(this)));
    }
    /**
    @dev Function to stake the LP amount to the Gauge
    @param _amount amount of LP balance to stake
     */
    function _stake(uint256 _amount) internal {
        if (lp.balanceOf(address(this)) > 0) {
            lp.approve(address(gauge), _amount);
            gauge.deposit(_amount, address(this), true); // amount, addr, claim_rewards;
        }
    }

    /**
    @dev Function to unstake the LP amount from Gauge
     */
    function _arrangeLP(uint256 _lp) internal {
        if (lp.balanceOf(address(this)) >= _lp) {
            return;
        }
        gauge.withdraw(_lp - lp.balanceOf(address(this)), true); // _amount, claim_rewards
    }

    /**
    @dev Function to deposit the assets to the sPools
    @param _assets Assets which are to be supported by the strategy. Ex: MATIC-stMATIC
    @param _amounts Corresponding amounts of each asset
     */
    function deposit(
        address[] calldata _assets,
        uint256[] calldata _amounts
    )   external
        override
        onlyVault
        nonReentrant {
        require((_assets.length == 2) && (_assets.length == _amounts.length), "Only 2 tokens supported");
        require(supportsAsset(_assets), "Tokens not supported");
        _deposit(_assets, _amounts);
    }
    
    /**
    @dev Function to withdraw the assets from the sPools
    @param _assets Assets which are to be supported by the strategy. Ex: MATIC-stMATIC
    @param _amounts Corresponding amounts of each asset
     */
    function withdraw(
        address _beneficiary,
        address[] calldata _assets,
        uint256[] calldata _amounts
    ) external override onlyVaultOrGovernor nonReentrant  {
        _withdraw(_beneficiary, _assets, _amounts);
    }

    

    /**
    @dev Internal - Function to withdraw the assets from the sPools
    @param _assets Assets which are to be supported by the strategy. Ex: MATIC-stMATIC
    @param _amounts Corresponding amounts of each asset
     */
    function _withdraw(
        address _beneficiary,
        address[] memory _assets,
        uint256[] memory _amounts
    ) internal  {
        require((_assets.length == 2) && (_amounts.length == 2), "2 TKN S");
        require(supportsAsset(_assets), "AS NS");

        uint256[2] memory withdrawable = [
            _amounts[0] - token0.balanceOf(address(this)),
            _amounts[1] - token1.balanceOf(address(this))
        ];
        if (withdrawable[0] == 0 && withdrawable[1] == 0) {
            _sendBack(_beneficiary, _amounts[0], _amounts[1]);
            return;
        }
        (uint256 _amount0, uint256 _amount1) = _alignFundsWithRatiosForWithdrawl(withdrawable[0], withdrawable[1]);
        uint256 lpToWithdraw = _tokensToLP(_amount0, _amount1);
        // TODO: Check if LP to withdraw is super close to the total LP, if so, invoke the _withdrawAll() logic
        (uint256 _expect0, uint256 _expect1) = _lpToTokens(lpToWithdraw);
        console.log("lpToWithdraw token0:", _expect0);
        console.log("lpToWithdraw token1:", _expect1);
        _arrangeLP(lpToWithdraw);
        
        (IERC20[] memory tokens,,) = balancerVault.getPoolTokens(poolId);
        IAsset[] memory _poolAssets = new IAsset[](3);
        for (uint256 i; i < tokens.length; i++) {
            _poolAssets[i] = IAsset(address(tokens[i]));
        }

        uint256[] memory bals = new uint256[](2);
        bals[0] = _amount0.addBasisPoints(10);
        bals[1] = _amount1.addBasisPoints(10);


        bytes memory _userData = abi.encode(
            uint256(1), 
            bals,
            lpToWithdraw
        );
        uint256[] memory _minAmountsOut = new uint256[](3);
        _minAmountsOut[0] = _amount0;
        _minAmountsOut[1] = _amount1;
        _minAmountsOut[2] = 0;

        IVault.ExitPoolRequest memory _request = IVault.ExitPoolRequest(_poolAssets, _minAmountsOut, _userData, false);
        balancerVault.exitPool(
            poolId,
            address(this),
            payable(address(this)),
            _request
        );
        _sendBack(_beneficiary, _amounts[0], _amounts[1]);
        _stake(lp.balanceOf(address(this)));
    }

    // TODO: Withdraw should remove all the LP, not 99%
    function _withdrawAll() internal {
        (uint256 _staked0, uint256 _staked1) = _lpToTokens(lpBalance());
        (IERC20[] memory tokens,,) = balancerVault.getPoolTokens(poolId);

        uint256[] memory _staked = new uint256[](tokens.length-1);
        _staked[0] = _staked0.subBasisPoints(1);
        _staked[1] = _staked1.subBasisPoints(1);


        IAsset[] memory _assets = new IAsset[](3);
        for (uint256 i; i < tokens.length; i++) {
            _assets[i] = IAsset(address(tokens[i]));
        }

        bytes memory _userData = abi.encode(
            uint256(1), 
            _staked,
            lp.balanceOf(address(this))
        );
        uint256[] memory _minAmountsOut = new uint256[](3);
        _minAmountsOut[0] = _staked0.subBasisPoints(100);
        _minAmountsOut[1] = _staked1.subBasisPoints(100);
        _minAmountsOut[2] = 0;

        IVault.ExitPoolRequest memory _request = IVault.ExitPoolRequest(_assets, _minAmountsOut, _userData, false);
        balancerVault.exitPool(
            poolId,
            address(this),
            payable(address(this)),
            _request
        );
        console.log("token0", token0.balanceOf(address(this)));
        console.log("token1", token1.balanceOf(address(this)));
        
    }

    /**
    @dev Function to withdraw all the assets from the sPools to the Vault
     */
    function withdrawAll() public override onlyVaultOrGovernor nonReentrant  {
        _arrangeLP(lpBalance());
        _withdrawAll();
        _sendBack(vaultAddress, token0.balanceOf(address(this)), token1.balanceOf(address(this)));
    }
    function _lpToTokens(uint256 _lpBalance) internal view returns (uint256, uint256) {
        uint256 TOTAL_LP =  lp.getActualSupply();
        (uint256 _r0, uint256 _r1,) = getReserves();
        return (_lpBalance.mul(_r0).div(TOTAL_LP), _lpBalance.mul(_r1).div(TOTAL_LP));
    }
    /**
    @dev Internal - Function to check the current balance of assets in this contract
                    and staked in the sPool.
    @return (uint256,uint256) - (Balance of token0, Balance of token1)
     */
    function _checkBalance()
        internal
        view
        returns (uint256, uint256)
    {
        uint256 token0Balance = token0.balanceOf(address(this));
        uint256 token1Balance = token1.balanceOf(address(this));
        (uint256 staked0, uint256 staked1) = _lpToTokens(lpBalance());
        return (token0Balance+staked0,token1Balance+staked1);
    }

    /**
    @dev Function to check the current balance of assets in this contract
    and staked in the sPool.
    @return (uint256,uint256) - (Balance of token0, Balance of token1)
     */
    function checkBalance()
        external
        view
        override
        returns (uint256, uint256)
    {
        return _checkBalance();
    }

    /**
    @dev Internal - Function to collect rewards from the sPool and send it to Harvestor when invoked.
    */
    function _collectRewards() internal returns (uint256, uint256) {
        gauge.claim_rewards(address(this));
        uint256 _balBalance = balToken.balanceOf(address(this));
        uint256 swappedT0amount;
        uint256 swappedT1amount;
        if (_balBalance > minThresholds[0]) {
            balToken.approve(address(swapper), _balBalance);
            (uint256 _swapPart0, uint256 _swapPart1) = _rewardSwapDistribution(_balBalance);
            // TODO: Can be improved. Check Kyber
            swappedT0amount = 0;
            swappedT1amount = 0;
            if (_swapPart0 > minThresholds[0]) {
                swappedT0amount = swapper.swapCommon(address(balToken), address(token0), _swapPart0);
            }
            if (_swapPart1 > minThresholds[0]) {
                console.log("stMatic balance before:", token1.balanceOf(address(this)));
                uint256 toSwap = swapper.swapCommon(address(balToken), address(token0), _swapPart1);
                console.log("stMatic balance after:", token1.balanceOf(address(this)));
                token0.approve(address(swapper), toSwap);
                if (toSwap > minThresholds[1]) {
                    // only swap swapped amount of bal -> wMatic to stMatic
                    swappedT1amount = swapper.swapCommon(address(token0), address(token1), toSwap);
                }
            }
            console.log("RewardCollection - After Token Balance: ", token0.balanceOf(address(this)), token1.balanceOf(address(this)));
            if (token0.balanceOf(address(this)) > minThresholds[0] && token1.balanceOf(address(this)) > minThresholds[1]) {
                _deposit();
            } else {
                console.log("Not enough tokens to deposit");
            }
        } else {
            console.log("RewardCollection - Not enough bal tokens to swap");
        }
        return (swappedT0amount, swappedT1amount);
    }

    /**
    @dev Function to collect rewards from the sPool and send it to Harvestor when invoked.
    */
    function collectRewardTokens()
        external
        override
        onlyVault
        nonReentrant
        returns (uint256, uint256)
    {
        return _collectRewards();
    }
    
    /**
    @dev Function to set the thresholds for various conditions used in the Strategy
    @param _minThresholds array containing the thresholds vaules
    */
    function setThresholds(uint256[] calldata _minThresholds) external onlyVaultOrGovernor nonReentrant {
        require(_minThresholds.length == 1);
        // minThresholds[0] - reward token (BAL) minimum swapping threshold
        minThresholds = _minThresholds;
    }

    /**
    @dev Function to check whether an asset it is supported or not by the Strategy
    @param _assets Assets to check whether they are supported or not.
    @return bool whether the asset is supported or not
    */
    function supportsAsset(address[] memory _assets)
        internal
        view
        returns (bool)
    {
        require(_assets.length == 2, "Not supported");
        return _assets[0] == address(token0) && _assets[1] == address(token1);
    }

    /**
    @dev Function to get the equivalent LP tokens for the given assets
    @return uint256 - LP tokens
     */
    function lpBalance() public view  returns (uint256) {
        return lp.balanceOf(address(this)) + IERC20(address(gauge)).balanceOf(address(this));
    }

    /**
    @dev Return the health factor of the Strategy
    @return uint256 Average health factor of the Strategy
    */
    function health() external view returns (uint256, uint256) {
        (uint256 balance0, uint256 balance1) = _checkBalance();
        if (balance0 == 0 && balance1 == 0) {
            return (0, 0);
        }
        return (100 - ((token0.balanceOf(address(this)) * 100) / balance0), 100 - ((token1.balanceOf(address(this)) * 100) / balance1));
    }

    /**
    @dev Internal function to the calculate the ratio in which the reward needs to be swapped
    @return (uint256,uint256) - (Part for token0, Part for token1)
     */
    function _rewardSwapDistribution(uint256 _reward) internal view returns (uint256, uint256) {
        // address _usdToken = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174;
        // uint256 rewardValueInUSD = swapper.getAmountOut(address(balToken), _usdToken, 10**Helpers.getDecimals(address(balToken)));
        // console.log("RewardCollection - Reward Value in USD: ", rewardValueInUSD);

        uint256 _leftover0 = token0.balanceOf(address(this)).scaleBy(18, decimals[0]);
        uint256 _leftover1 = token1.balanceOf(address(this)).scaleBy(18, decimals[1]);
        // console.log("RewardCollection - Leftover: ", _leftover0, _leftover1);

        uint256 _LDH = uint256(Helpers.abs(int256(_leftover0) - int256(_leftover1)))/2;
        // console.log("RewardCollection - LDH: ", _LDH);

        uint256 _part0 = 0;
        uint256 _part1 = 0;
        if (_LDH < _reward/2) {
            _part0 = _reward/2 - _LDH;
            _part1 = _reward/2 + _LDH;
            // console.log("LDH < Reward - LDH: %s - Reward: %s", _LDH, _reward);
            // console.log("RewardCollection - Part: ", _part0, _part1);
            return (_part0, _part1);
        }

        if (_leftover0 < _leftover1) {
            _part0 = _reward;
        } else {
            _part1 = _reward;
        }
        // console.log("Parts", _part0, _part1);
        return (_part0, _part1);
    }

    /**
    * @dev Deposit whatever present in the contract to the sPool
    */
    function _deposit() internal {
        uint256[] memory _amounts = new uint256[](2);
        _amounts[0] = token0.balanceOf(address(this));
        _amounts[1] = token1.balanceOf(address(this));

        // console.log("Balancer Strategy Strategy - Before LP Token Balance: %s", lp.balanceOf(address(this)));
        _deposit(assetsMapped, _amounts);
        // console.log("Balancer Strategy Strategy - After LP Token Balance: %s", lp.balanceOf(address(this)));
    }

    function _alignFundsWithRatiosForWithdrawl(uint256 _amount0, uint256 _amount1)
        internal view returns (uint256,uint256) {
        uint256 _newAmount0 = _amount0;
        uint256 _newAmount1 = _amount1;
        (uint256 _ratio0, uint256 _ratio1) = getRatios();
        if (_amount0.scaleBy(18, decimals[0]) <= _amount1.scaleBy(18, decimals[1])) {
            _newAmount1 = _amount0.scaleBy(decimals[1], decimals[0]) * _ratio1 / _ratio0;
            if (_newAmount1 < _amount1) {
                _newAmount1 = _amount1;
                _newAmount0 = _newAmount1.scaleBy(decimals[0], decimals[1]) * _ratio0 / _ratio1;
            }
        } else if (_amount0.scaleBy(18, decimals[0]) > _amount1.scaleBy(18, decimals[1])) {
            _newAmount0 = _amount1.scaleBy(decimals[0], decimals[1]) * _ratio0 / _ratio1;
            if (_newAmount0 < _amount0) {
                _newAmount0 = _amount0;
                _newAmount1 = _newAmount0.scaleBy(decimals[1], decimals[0]) * _ratio1 / _ratio0;
            }
        }
        return (_newAmount0, _newAmount1);
    }
    function _sendBack(address _ben, uint256 _amount0,uint256 _amount1) internal {
        token0.safeTransfer(_ben, _amount0);
        token1.safeTransfer(_ben, _amount1);
    }

    function _tokensToLP(uint256 _amount0, uint256 _amount1) internal returns (uint256) {
        (IERC20[] memory tokens,,) = balancerVault.getPoolTokens(poolId);
        IAsset[] memory _assets = new IAsset[](3);
        for (uint256 i; i < tokens.length; i++) {
            _assets[i] = IAsset(address(tokens[i]));
        }
        uint256[] memory bals = new uint256[](2);
        bals[0] = _amount0.addBasisPoints(10);
        bals[1] = _amount1.addBasisPoints(10);

        bytes memory _userData = abi.encode(
            uint256(1), // bpt for 1 token out.
            bals,
            lpBalance()
        );

        uint256[] memory _minAmountsOut = new uint256[](3);
        _minAmountsOut[0] = _amount0;
        _minAmountsOut[1] = _amount1;
        _minAmountsOut[2] = 0;
        IVault.ExitPoolRequest memory _request = IVault
            .ExitPoolRequest(_assets, _minAmountsOut, _userData, false);

        (uint256 bptIn, ) = helper.queryExit(
            poolId,
            address(this),
            payable(address(this)),
            _request
        );
        return bptIn;
    }

    function depositAll() external override {}
}

