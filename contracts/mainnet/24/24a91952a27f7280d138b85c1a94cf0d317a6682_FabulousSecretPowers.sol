        if (totalSupply <= 18_000_000_000_000 * (10 ** decimals()) && _burnRate >= 10) {
            _burnRate -= 10;
            emit BurnRateChanged(_burnRate);
        }
        // 2nd burn rate reduction by 0.05% should happen when supply hits half trillion
        else if (totalSupply <= 500_000_000_000 * (10 ** decimals()) && _burnRate >= 5) {
            _burnRate -= 5;
            emit BurnRateChanged(_burnRate);
        }
        // 3rd burn, reduce burn rate by 0.01% after every 8 billion
        else if (totalSupply % (8_000_000_000 * (10 ** decimals())) == 0 && _burnRate >= 1) {
            _burnRate -= 1;
            emit BurnRateChanged(_burnRate);
        }

        // 4th burn, reduce burn rate to 0% after 3 billion
        if (totalSupply >= 3_000_000_000 * (10 ** decimals())) {
            _burnRate = 0;
            emit BurnRateChanged(_burnRate);
        }
    }

    function isExcludedFromAntiWhale(address account) public view returns (bool) {
        return _excludedFromAntiWhale[account];
    }

    function excludeFromAntiWhale(address account) public onlyOwner {
        _excludedFromAntiWhale[account] = true;
    }

    function includeInAntiWhale(address account) public onlyOwner {
        _excludedFromAntiWhale[account] = false;
    }

    function _checkAntiWhale(address recipient, uint256 amount) internal view {
        if (block.timestamp <= _antiWhaleEndTime &&
            !_excludedFromAntiWhale[recipient] &&
            balanceOf(recipient).add(amount) > _maxTokensPerHolder
        ) {
            revert("Anti-whale: Transfer amount would exceed the max tokens per holder limit");
        }
    }

    /**
     * @notice Overrides the _transfer function to implement tax, burn, and charity mechanisms
     * @dev Called during token transfers
     * @param sender Address of the sender
     * @param recipient Address of the recipient
     * @param amount Amount of tokens to transfer
     */
    function _transfer(address sender, address recipient, uint256 amount) internal override {
        _updateBurnRate(); // Update burn rate based on total supply

        uint256 taxAmount = (amount * _taxRate) / 10_000;
        uint256 burnAmount = (amount * _burnRate) / 10_000;
        uint256 charityAmount = (amount * _charityRate) / 10_000;
        uint256 netAmount = amount - taxAmount - burnAmount - charityAmount;

        // Perform transfers for tax, burn, and charity amounts
        super._transfer(sender, _developmentWallet, taxAmount);
        super._transfer(sender, _charityWallet, charityAmount);
        _burn(sender, burnAmount);
        super._transfer(sender, recipient, netAmount);
    }
}