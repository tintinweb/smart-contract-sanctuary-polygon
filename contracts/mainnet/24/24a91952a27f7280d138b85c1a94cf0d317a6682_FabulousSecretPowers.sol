/**
 * @title TokenLock
 * @dev A contract to handle token locking and timed unlocking
 */
contract TokenLock is Ownable, ReentrancyGuard {
    using SafeMath for uint256;

    ERC20 private _token;
    // Emit event when a new lock is added or when tokes are withdran
    /**
     * @notice Events for key operations
     * @dev emit an event when a new lock is added or when tokens are withdrawn
     */
    event LockAdded(address indexed beneficiary, uint256 amount, uint256 unlockTime);
    event TokensWithdrawn(address indexed beneficiary, uint256 amount);

    /**
     * @notice Token lock structure to store locked tokens and unlock time
     * @dev Struct containing locked amount and unlock time
     */
    struct Lock {
        uint256 amount;
        uint256 unlockTime;
    }

    /**
     * @notice Mapping of beneficiary addresses to array of locks
     * @dev Beneficiary address => Array of Lock structs
     */
    mapping(address => Lock[]) private _locks;

    /**
     * @notice Constructor sets the token address for locking and unlocking
     * @param token Address of the token
     */
    constructor(ERC20 token) {
        _token = token;
    }

    /**
     * @notice Adds a new lock for the beneficiary
     * @dev Only callable by the contract owner
     * @param beneficiary Address of the beneficiary
     * @param amount Amount of tokens to lock
     * @param unlockTime Unix timestamp when tokens will be unlocked
     */
    function addLock(address beneficiary, uint256 amount, uint256 unlockTime) external onlyOwner {
        _locks[beneficiary].push(Lock(amount, unlockTime));
        emit LockAdded(beneficiary, amount, unlockTime);
    }

    /**
     * @notice Allows beneficiary to withdraw unlocked tokens
     * @dev Fails if no tokens are available for withdrawal
     */
    function withdrawTokens() external nonReentrant {
        uint256 totalAmount = 0;

        for (uint256 i = 0; i < _locks[msg.sender].length; i++) {
            if (block.timestamp >= _locks[msg.sender][i].unlockTime) {
                totalAmount = totalAmount.add(_locks[msg.sender][i].amount);
                _locks[msg.sender][i].amount = 0;
            }
        }

        require(totalAmount > 0, "No tokens available for withdrawal");
        _token.transfer(msg.sender, totalAmount);
        emit TokensWithdrawn(msg.sender, totalAmount);
    }
} 

