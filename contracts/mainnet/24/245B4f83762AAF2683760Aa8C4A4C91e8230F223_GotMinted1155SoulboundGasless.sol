    address private trustedForwarder;

    ///@dev bool to reveal token uri
    bool public isRevealed;

    /*///////////////////////////////////////////////////////////////
                                Mappings
    //////////////////////////////////////////////////////////////*/

    /// @dev Owner of the contract (purpose: OpenSea compatibility)
    address private _owner;

    ///@dev allowList mapping for TokenId
    mapping(uint256 => address[]) internal allowList;

    ///@dev tokenIdtoCost mapping from TokenId => cost
    mapping(uint256 => uint256) private tokenIdtoCost;

    ///@dev tokenIdtoTokenMaxSupply mapping from TokenId => TokenMaxSupply
    mapping(uint256 => uint256) private tokenIdtoTokenMaxSupply;

    ///@dev tokenIdToTokenURI mapping from TokenId => TokenURI
    mapping(uint256 => string) internal tokenIdToTokenURI;

    ///@dev NewToBurnable mapping from TokenId => NewToBurnable
    mapping(uint256 => uint256[]) internal NewToBurnable;

    ///@dev Transferable Mapping from TokenId => bool
    mapping(uint256 => bool) private isTransferableMapping;

    mapping(uint256 => bool) private isAllowListRequiredMapping;

    ///@dev Array of token ids user is allowed to mint
    mapping(address => uint256[]) private allowListIDsByAddress;


    /*///////////////////////////////////////////////////////////////
                            Events
    //////////////////////////////////////////////////////////////*/

    event SingleToSingle(address indexed to_, uint256 indexed id_, uint256 amount_);

    event ManyToSingle(address indexed to_, uint256[] indexed ids_, uint256[] amounts_);

    event SingleToMany(address[] indexed tos_, uint256 indexed id_, uint256 amount_);

    event ManyToMany(address[] indexed tos_, uint256[] indexed ids_, uint256[] amounts_);

    event Transfer(address indexed from, address indexed to, uint256 indexed id, uint256 amount);

    event Mint(address indexed to_, uint256 indexed id_, uint256 amount_);

    /*///////////////////////////////////////////////////////////////
                    Initializer logic
    //////////////////////////////////////////////////////////////*/

    function initialize() external initializer {
        // Initialize this contract's state.
        __AccessControl_init();
        _setRoleAdmin(ADMIN_ROLE, ADMIN_ROLE);
        _setRoleAdmin(MINTER_ROLE, ADMIN_ROLE);
        _setupRole(ADMIN_ROLE, _msgSender());
        _setupRole(MINTER_ROLE, _msgSender());
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        name = "GotMinted";
        symbol = "MINTED";
        _owner = _msgSender();
        trustedForwarder = 0xc82BbE41f2cF04e3a8efA18F7032BDD7f6d98a81;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor()
        ERC2771ContextUpgradeable(0xc82BbE41f2cF04e3a8efA18F7032BDD7f6d98a81)
    {
         
    }

