    @dev Lets a contract admin (account with `DEFAULT_ADMIN_ROLE`) set setUriTokenIdToToken.
    */
    function setUri(uint256 tokenId_, string memory uri_)
        external
        onlyRole(ADMIN_ROLE)
    {
        tokenIdToTokenURI[tokenId_] = uri_;
    }

    /**
    @notice the uriBase should take the form of "ipfs://CID/".  
    */ 
     function setUriBatch(uint256 tokenIdStart_, uint256 tokenIdEnd_, string memory uriBase_)
        external
        onlyRole(ADMIN_ROLE)
    {
        string memory ext = ".json";

        for(uint256 i = tokenIdStart_; i < tokenIdEnd_ + 1; i++){

             tokenIdToTokenURI[i] = string(abi.encodePacked(uriBase_, _toString(i), ext));

        }
    }

    ///@dev Lets a contract admin (account with `DEFAULT_ADMIN_ROLE`) set setTokenMaxSupply.
    function setTokenMaxSupply(uint256 tokenId_, uint256 maxSupply_)
        external
        onlyRole(ADMIN_ROLE)
    {
        require(
            maxSupply_ >= totalSupply(tokenId_),
            "max supply must be greater than current supply"
