    /*///////////////////////////////////////////////////////////////
                    AllowList Functionality
    //////////////////////////////////////////////////////////////*/

     function toggleAllowList(uint256 tokenId, bool _bool) external onlyRole(ADMIN_ROLE) {
        isAllowListRequiredMapping[tokenId] = _bool;
    }

     function isAllowListRequired(uint256 tokenId_) external view returns (bool) {
        return isAllowListRequiredMapping[tokenId_];
    }

    ///@dev Lets a contract admin (account with `DEFAULT_ADMIN_ROLE`) set allowlist for a tokenId.
    function createAllowList(uint256 tokenId, address[] calldata users)
        external
        onlyRole(ADMIN_ROLE)
    {
        address[] memory _users = allowList[tokenId];

        for (uint256 i = 0; i < _users.length; i++) {
            allowListIDsByAddress[_users[i]].removeElement(tokenId);
        }
        for (uint256 i = 0; i < users.length; i++) {
            allowListIDsByAddress[users[i]].push(tokenId);
        }

        delete allowList[tokenId];
        allowList[tokenId] = users;
    }

    ///@return `true` if user is on allowlist and `false` if otherwise.
    function isOnAllowlist(uint256 tokenId, address user)
        public
        view
        returns (bool)
    {
        address[] memory users = getAllowList(tokenId);

        for (uint256 i = 0; i < users.length; i++) {
            if (users[i] == user) {
                return true;
            }
        }
        return false;
    }

