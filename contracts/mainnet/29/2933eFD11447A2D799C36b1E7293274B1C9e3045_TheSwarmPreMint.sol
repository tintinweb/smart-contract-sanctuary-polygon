// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import '@openzeppelin/contracts/token/ERC721/ERC721.sol';
import '@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol';
import '@openzeppelin/contracts/utils/cryptography/ECDSA.sol';

contract TheSwarmPreMint is ERC721, ERC721Enumerable, Ownable {
    using SafeERC20 for IERC20;
    using ECDSA for bytes32;

    /// @notice The maximum number of NFTs that can be minted.
    uint256 public immutable SUPPLY_CAP;

    /// @notice The ERC20 token used for payment.
    IERC20 public immutable PAYMENT_TOKEN;

    /// @notice The price of an NFT.
    uint256 public immutable salePrice;

    /// @notice The start timestamp of the sale.
    uint256 public immutable saleBegin;

    /// @notice The end timestamp of the sales period.
    uint256 public saleEnd;

    /// @notice The address authorized to sign messages for minting.
    address public mintAuthority;

    /// @notice The address that collects the funds raised.
    address public paymentReceiver;

    /// @notice The number of NFTs minted for each address during the sale.
    mapping(address => uint256) public whitelistMintCounts;

    string internal _tokenURI;

    constructor(
        uint256 _supplyCap,
        address _paymentToken,
        address _paymentReceiver,
        address _mintAuthority,
        uint256 _salePrice,
        uint256 _saleBegin,
        uint256 _saleEnd
    ) ERC721('TestReceiptNFT', 'rcptNFT') {
        SUPPLY_CAP = _supplyCap;

        PAYMENT_TOKEN = IERC20(_paymentToken);
        paymentReceiver = _paymentReceiver;

        mintAuthority = _mintAuthority;

        salePrice = _salePrice;

        saleBegin = _saleBegin;
        saleEnd = _saleEnd;
    }

    /// @dev Make NFT soulbound
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal override(ERC721, ERC721Enumerable) {
        require(from == address(0), 'This soulbound NFT cannot be transferred.');
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    /**
     * @notice Sets the sale end timestamp.
     * @param _saleEnd The new sale end timestamp.
     * Only the contract owner can call this function.
     */
    function setSaleEnd(uint256 _saleEnd) external onlyOwner {
        saleEnd = _saleEnd;
    }

    /**
     * @notice Sets the address authorized to sign messages for minting.
     * @param _mintAuthority The new address authorized to sign messages for minting.
     * Only the contract owner can call this function.
     */
    function setMintAuthority(address _mintAuthority) external onlyOwner {
        mintAuthority = _mintAuthority;
    }

    /**
     * @notice Sets address that collects the funds raised.
     * @param _paymentReceiver The new address that collects the funds raised.
     * Only the contract owner can call this function.
     */
    function setPaymentReceiver(address _paymentReceiver) external onlyOwner {
        paymentReceiver = _paymentReceiver;
    }

    /**
     * @notice Sets the token URI for the token metadata.
     * @param newTokenURI The new token URI.
     * Only the contract owner can call this function.
     */
    function setTokenURI(string calldata newTokenURI) external onlyOwner {
        _tokenURI = newTokenURI;
    }

    /**
     * @notice Mints quantity nfts for the private sale.
     * @param user Address to which the nfts are minted.
     * @param quantity Amount of nfts to mint for private sale.
     * Only the contract owner can call this function.
     */
    function mintPrivateSale(address user, uint256 quantity) external onlyOwner {
        batchMint(user, quantity, 0);
    }

    /**
     * @notice Mints NFTs for the whitelist sale based on the provided quantity and cap.
     * @param quantity The number of NFTs to mint.
     * @param cap The cap on the number of NFTs that can be minted by the caller.
     * @param signature The signature of the signed message.
     * Whitelist sale must be active, the signature must be valid, and the caller must not have already claimed the maximum cap.
     */
    function mintWhitelist(uint256 quantity, uint256 cap, bytes calldata signature) external {
        require(isSaleActive(), 'Sale not active');

        // Verify mintAuthority signature
        bytes32 valueHash = keccak256(abi.encode(block.chainid, address(this), msg.sender, cap));
        require(mintAuthority == valueHash.toEthSignedMessageHash().recover(signature), 'Invalid signed message');

        // Try to mint quantity amount of nfts
        uint256 mintCount = batchMint(msg.sender, quantity, salePrice);

        // Verify cap limitation per account given actual mint count
        uint256 totalMintCount = whitelistMintCounts[msg.sender] + mintCount;
        require(totalMintCount <= cap, 'Cannot mint above cap');
        whitelistMintCounts[msg.sender] = totalMintCount;
    }

    /**
     * @notice Mints a batch of NFTs, taking payment for the minted NFTs.
     * @param to The address to receive the minted NFTs.
     * @param quantity The number of NFTs to mint.
     * @param price The price in payment tokens for each minted NFT.
     * @return mintCount The number of minted NFTs may be less if there are not enough available.
     * Payment tokens are transferred from the caller to the contract.
     */
    function batchMint(address to, uint256 quantity, uint256 price) private returns (uint256 mintCount) {
        uint256 supplyBefore = totalSupply();
        require(supplyBefore < SUPPLY_CAP, 'Mint cap reached');

        uint256 supplyAfter = Math.min(supplyBefore + quantity, SUPPLY_CAP);

        if (price > 0) {
            PAYMENT_TOKEN.safeTransferFrom(msg.sender, paymentReceiver, price * (supplyAfter - supplyBefore));
        }

        for (uint256 i = supplyBefore; i < supplyAfter; i++) {
            _safeMint(to, i);
        }

        return supplyAfter - supplyBefore;
    }

    function isSaleActive() private view returns (bool) {
        uint256 timestamp = block.timestamp;
        return (timestamp >= saleBegin && timestamp < saleEnd);
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        _requireMinted(tokenId);

        return _tokenURI;
    }

    // The following functions overrides required by solidity
    function supportsInterface(bytes4 interfaceId) public view override(ERC721, ERC721Enumerable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    // Function to rescue ERC20 tokens wrongly sent directly to this contract
    function rescueERC20(address token) external onlyOwner {
        IERC20(token).safeTransfer(owner(), IERC20(token).balanceOf(address(this)));
    }
}