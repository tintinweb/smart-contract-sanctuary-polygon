pragma solidity ^0.8.4;

import './solidity-stringutils/strings.sol';

import '@openzeppelin/contracts/token/common/ERC2981.sol';
import '@openzeppelin/contracts/token/ERC1155/ERC1155.sol';
import '@openzeppelin/contracts/utils/math/SafeMath.sol';
import '@openzeppelin/contracts/utils/Strings.sol';
import '@openzeppelin/contracts/utils/Address.sol';

import './openzeppelin/contracts/access/Ownable.sol';
import './openzeppelin/contracts/token/ERC1155/extensions/IERC1155Supply.sol';

import './opensea/common/meta-transactions/ContentMixin.sol';
import './opensea/common/meta-transactions/NativeMetaTransaction.sol';

contract OwnableDelegateProxy {}

contract ProxyRegistry {
    mapping(address => OwnableDelegateProxy) public proxies;
}

/**
 * @title ERC1155Tradable
 * ERC1155Tradable - ERC1155 contract that whitelists an operator address, has mint functionality,
 * and supports useful standards from OpenZeppelin, like exists(), name(), symbol(), and totalSupply()
 */
abstract contract ERC1155Tradable is
    ContextMixin,
    ERC1155,
    ERC2981,
    NativeMetaTransaction,
    Ownable,
    IERC1155Supply
{
    using Strings for string;
    using Strings for uint256;
    using strings for *;
    using SafeMath for uint256;
    using Address for address;

    struct TokenOwnership {
        address addr;
        uint64 startTimestamp;
    }

    address public proxyRegistryAddress;
    mapping(address => bool) public sharedProxyAddresses;
    uint256 public sharedProxyAddressesCount;
    mapping(uint256 => bool) private mintResults;
    /**
     * Recording token holders for future airdrop or other benefits.
     * key: tokenId, value: holder EOA & hold start time.
     */
    mapping(uint256 => TokenOwnership) private ownerships;
    // Contract name
    string public name;
    // Contract symbol
    string public symbol;

    constructor(
        string memory _name,
        string memory _symbol,
        string memory _baseURI
    ) ERC1155(_baseURI) {
        require(startsWith(_baseURI, 'ipfs://'), '_uri is not IPFS');
        require(endsWith(_baseURI, '/'), '_uri is not ends with "/"');
        name = _name;
        symbol = _symbol;
        proxyRegistryAddress = address(0);
        if (super.getChainId() == 80001) {
            // Mumbai - ERC-1155 on Polygon
            addSharedProxyAddress(0x53d791f18155C211FF8b58671d0f7E9b50E596ad);
        } else {
            // MainNet or other - ERC-1155 on Polygon
            addSharedProxyAddress(0x207Fa8Df3a17D96Ca7EA4f2893fcdCb78a304101);
        }
        _initializeEIP712(name);
        // Royalty is informed by this setting to NFT markets.
        // But for now, OpenSea uses royalty setting of contractURI() metadata.
        // royalty receiver == contract creator
        // royalty == 10%
        _setDefaultRoyalty(owner(), 1000);
    }

    /**
     * May be used for emergency maintenance.
     */
    function setURI(string memory _baseURI) public onlyOwner {
        require(startsWith(_baseURI, 'ipfs://'), '_uri is not IPFS');
        require(endsWith(_baseURI, '/'), '_uri is not ends with "/"');
        super._setURI(_baseURI);
    }

    /**
     * If _str is not empty, returns true.
     */
    function hasText(string memory _str) private pure returns (bool) {
        // We have to convert string to bytes to check for existence.
        return bytes(_str).length > 0;
    }

    function startsWith(string memory _str, string memory _prefix)
        private
        pure
        returns (bool)
    {
        strings.slice memory slicee = _str.toSlice();
        return slicee.startsWith(_prefix.toSlice());
    }

    function endsWith(string memory _str, string memory _sufix)
        private
        pure
        returns (bool)
    {
        strings.slice memory slicee = _str.toSlice();
        return slicee.endsWith(_sufix.toSlice());
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC1155, ERC2981)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function setDefaultRoyalty(address receiver, uint96 feeNumerator)
        public
        onlyOwner
    {
        _setDefaultRoyalty(receiver, feeNumerator);
    }

    /**
     * @dev Allows owner to change the proxy registry.
     */
    function setProxyRegistryAddress(address _address) public onlyOwner {
        proxyRegistryAddress = _address;
    }

    /**
     * @dev Allows owner to add a shared proxy address.
     */
    function addSharedProxyAddress(address _address) public onlyOwner {
        bool backup = sharedProxyAddresses[_address];
        sharedProxyAddresses[_address] = true;
        if (backup == false) {
            sharedProxyAddressesCount += 1;
        }
    }

    /**
     * @dev Allows owner to remove a shared proxy address.
     */
    function removeSharedProxyAddress(address _address) public onlyOwner {
        bool backup = sharedProxyAddresses[_address];
        delete sharedProxyAddresses[_address];
        if (backup == true) {
            sharedProxyAddressesCount -= 1;
        }
    }

    function uri(uint256 _id) public view override returns (string memory) {
        require(exists(_id), 'ERC1155Tradable#uri: NONEXISTENT_TOKEN');
        return concatURI(_id);
    }

    function concatURI(uint256 _id) private view returns (string memory) {
        string memory baseURI = super.uri(0);
        if (hasText(baseURI)) {
            return string(abi.encodePacked(baseURI, _id.toString()));
        } else {
            return '';
        }
    }

    /**
     * @dev Returns the total supply for a token ID.
     * Each token's total supply is up to 1.
     * @param _id uint256 ID of the token to query
     * @return uint256 amount of token in existence
     */
    function totalSupply(uint256 _id) public view override returns (uint256) {
        if (exists(_id)) {
            return 1;
        } else {
            return 0;
        }
    }

    /**
     * @dev Creates a new token type and assigns 1 as initial supply to an address
     * NOTE: remove onlyOwner if you want third parties to create new tokens on
     *       your contract (which may change your IDs)
     * NOTE: The token id must be passed. This allows lazy creation of tokens or
     *       creating NFTs by setting the id's high bits with the method
     *       described in ERC1155 or to use ids representing values other than
     *       successive small integers. If you wish to create ids as successive
     *       small integers you can either subclass this class to count onchain
     *       or maintain the offchain cache of identifiers recommended in
     *       ERC1155 and calculate successive ids from that.
     * @param _initialOwner address of the first owner of the token
     * @param _id The id of the token to create (must not currenty exist).
     * @return _id The newly created token ID
     */
    function mint(address _initialOwner, uint256 _id)
        public
        onlyOwner
        returns (uint256)
    {
        require(_id <= 20064, 'token _id is over max token id');
        // With this require, each token's total supply will be up to 1.
        require(!exists(_id), 'token _id already exists');
        string memory _uri = concatURI(_id);
        emit URI(_uri, _id);
        uint256 amount = 1;
        _mint(_initialOwner, _id, amount, '0x00');
        mintResults[_id] = true;
        ownerships[_id] = TokenOwnership(
            _initialOwner,
            uint64(block.timestamp)
        );
        return _id;
    }

    /**
     * Mint tokens.
     * Each token's total supply is up to 1.
     * Suppose the images are prepared on IPFS before this process.
     */
    function mintBatch(
        address to,
        uint256 startId,
        uint256 endId
    ) public onlyOwner {
        require(startId <= 20064, 'startId is out of bound 20064');
        require(endId <= 20064, 'endId is out of bound 20064');
        require(startId <= endId, 'endId is smaller than startId');
        // Check array size needed.
        uint256 size = 0;
        for (uint256 id = startId; id <= endId; id += 1) {
            if (!exists(id)) {
                size += 1;
            }
        }
        uint256[] memory ids = new uint256[](size);
        uint256[] memory amounts = new uint256[](size);
        bool hasAmount = false;
        uint256 pos = 0; // insert position
        for (uint256 id = startId; id <= endId; id += 1) {
            if (!exists(id)) {
                ids[pos] = id;
                amounts[pos] = 1;
                pos += 1;
                hasAmount = true;
            }
        }
        require(
            hasAmount,
            'No amounts to mintBatch! All the indicated tokens have already been minted!'
        );
        super._mintBatch(to, ids, amounts, '0x00');
        for (uint256 i = 0; i < ids.length; i += 1) {
            uint256 id = ids[i];
            mintResults[id] = true;
            ownerships[id] = TokenOwnership(to, uint64(block.timestamp));
        }
    }

    /**
     * Whitelist OpenSea proxy contract for easy trading.
     */
    function _isProxyForUser(address _user, address _address)
        internal
        view
        returns (bool)
    {
        if (sharedProxyAddresses[_address]) {
            return true;
        }
        if (!proxyRegistryAddress.isContract()) {
            // NULL Address, invalid Address or EOA.
            return false;
        }
        // For now, there doesn't seem to be a 'ProxyRegistry' with an interface called 'proxies'.
        // However, in the OpenSea sample code, it was written as if something called 'ProxyRegistry' was planned to exist.
        // So we left this code just in case.
        // Perhaps sharedProxyAddresses in OpenSea's shared contracts can be used in a similar way to 'proxies'.
        // But relying directly on other contracts can be risky, so we don't use that approach.
        ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);
        return address(proxyRegistry.proxies(_user)) == _address;
    }

    /**
     * Override isApprovedForAll to whitelist user's OpenSea proxy accounts to enable gas-free listings.
     */
    function isApprovedForAll(address _owner, address _operator)
        public
        view
        override
        returns (bool isOperator)
    {
        if (_isProxyForUser(_owner, _operator)) {
            return true;
        }
        return super.isApprovedForAll(_owner, _operator);
    }

    function _safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) internal override {
        // If transfer caller is not owner nor approved, error will occur before this line.
        super._safeTransferFrom(from, to, id, amount, data);
        // If insufficient balance for transfer is detected, error will occur before this line.
        if (amount > 0 && from != to) {
            ownerships[id] = TokenOwnership(to, uint64(block.timestamp));
        }
    }

    function _safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override {
        // If transfer caller is not owner nor approved, error will occur before this line.
        super._safeBatchTransferFrom(from, to, ids, amounts, data);
        // If insufficient balance for transfer is detected, error will occur before this line.
        uint64 time = uint64(block.timestamp);
        for (uint256 i = 0; i < ids.length; ++i) {
            uint256 id = ids[i];
            // amount is up to 1.
            uint256 amount = amounts[i];
            if (amount > 0 && from != to) {
                ownerships[id] = TokenOwnership(to, time);
            }
        }
    }

    function ownershipOf(uint256 tokenId)
        internal
        view
        returns (TokenOwnership memory)
    {
        require(
            exists(tokenId),
            'ERC1155Tradable: owner query for nonexistent token'
        );
        TokenOwnership memory ownership = ownerships[tokenId];
        if (ownership.addr != address(0)) {
            return ownership;
        }
        revert('ERC1155Tradable: unable to determine the owner of token');
    }

    /**
     * @dev See {IERC721-ownerOf}.
     */
    function ownerOf(uint256 tokenId) public view returns (address) {
        return ownershipOf(tokenId).addr;
    }

    function ownerStartTimeOf(uint256 tokenId) public view returns (uint64) {
        return ownershipOf(tokenId).startTimestamp;
    }

    /**
     * @dev Returns whether the specified token exists.
     * @param _id uint256 ID of the token to query the existence of
     * @return bool whether the token exists
     */
    function exists(uint256 _id) public view override returns (bool) {
        return mintResults[_id];
    }

    /**
     * This is used instead of msg.sender as transactions won't be sent by the original token owner, but by OpenSea.
     */
    function _msgSender() internal view override returns (address sender) {
        return ContextMixin.msgSender();
    }
}

