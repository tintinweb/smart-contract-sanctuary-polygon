pragma solidity ^0.8.4;

import './ERC1155Tradable.sol';

contract CryptoCasinoTarot is ERC1155Tradable {
    /**
     * Contract name, contract symbol, baseURI should be passed.
     * baseURI should be end with '/'.
     */
    constructor()
        ERC1155Tradable(
            'CryptoCasinoTarot',
            'CCT',
            'ipfs://QmT38n3xaUF1mrR7PRBK8azzpMtrD8PWjSWWg9kyYooPLH/'
        )
    {}

    /**
     * Contract metadata URI.
     * OpenSea doesn't define contractURI() interface.
     * But if we define it, OpenSea will not be able to recognize this concrete method for some reason.
     * This URI should be unique URI, otherwise opensea doesn't load your metadata.
     */
    function contractURI() public pure returns (string memory) {
        // crypto.casino.tarot.opensea.contract.metadata.json
        return 'ipfs://QmYw5PNMjAfdzhki32kfYurRNHefmTLjVzasvri6ViEbQ7';
    }
}

