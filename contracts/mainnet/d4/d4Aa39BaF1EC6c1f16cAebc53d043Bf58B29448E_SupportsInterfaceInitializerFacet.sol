        UNICORN_BREEDING_ALLOWED,                                       //7
        UNICORN_HATCHING_ALLOWED,                                       //8
        UNICORN_EVOLVING_ALLOWED,                                       //9
        UNICORN_AIRLOCK_IN_ALLOWED,                                     //10
        UNICORN_AIRLOCK_OUT_ALLOWED,                                    //11

        LAND_AIRLOCK_IN_ALLOWED,                                        //12
