pragma solidity ^0.8.0;
import {LibDiamond} from "LibDiamond.sol";
import {IERC173} from "IERC173.sol";
import {IPermissionProvider} from "IPermissionProvider.sol";

import "IERC165.sol";

contract SupportsInterfaceInitializerFacet {
    function initializeSupportsInterface() external {
        LibDiamond.DiamondStorage storage ds = LibDiamond.diamondStorage();
        LibDiamond.enforceIsContractOwner();
        ds.supportedInterfaces[type(IERC165).interfaceId] = true;               // 0x01ffc9a7
        ds.supportedInterfaces[type(IERC173).interfaceId] = true; 
        ds.supportedInterfaces[type(IPermissionProvider).interfaceId] = true;
    }
}

