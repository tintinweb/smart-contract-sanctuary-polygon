    enum Permission {               // WARNING: This list must NEVER be re-ordered
        FARM_ALLOWED,                                                   //0
        JOUST_ALLOWED,              //  Not in use yet                  //1
        RACE_ALLOWED,               //  Not in use yet                  //2
        PVP_ALLOWED,                //  Not in use yet                  //3
