        uint256 tokenId
        ) external payable whenNotPaused() nonReentrant() noSlave(_singletonArray(tokenId)) {
            console.log("Function BodY");
        _maxMintCheck(amount, tokenId);
        console.log("MaxMintCheck passed");
        _paymentCheck(getMintPrice(tokenId) * amount);
        console.log("PaymentCheck passed");
        _supplyCheck(amount, tokenId, 0, false);
        console.log("SupplyCheck passed");
        _mint(msg.sender, tokenId, amount, "0");
        }

    function mintBatch (
        uint256[] calldata amounts, 
        uint256[] calldata tokenIDs
        ) external payable whenNotPaused() nonReentrant() noSlave(tokenIDs) {
            uint256 paymentDue = 0;
            for(uint256 i = 0; i < tokenIDs.length; i++) {
                uint256 amount = amounts[i];
                _maxMintCheck(amount, tokenIDs[i]);
                _supplyCheck(amount, tokenIDs[i], 0, false);
                paymentDue += getMintPrice(tokenIDs[i]) * amount;
            }
            _paymentCheck(paymentDue);
            _mintBatch(msg.sender, tokenIDs, amounts, "0");
        }

    function burn (
        uint256 amount, 
