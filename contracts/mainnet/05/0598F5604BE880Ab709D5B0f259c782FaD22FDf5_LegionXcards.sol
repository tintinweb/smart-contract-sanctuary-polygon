    }

 function _claim(
        uint256 dropAmount,
        bytes32 root, 
         uint256 tokenIDsFromStartingID,
        address recipient, 
        bytes32[] calldata _merkleProof
    ) internal {
          bytes32 leaf = keccak256(bytes.concat(keccak256(abi.encode(recipient, dropAmount, tokenIDsFromStartingID, msg.value))));
          // check if the params result in a valid proof
          if(!MerkleProof.verify(_merkleProof, root, leaf)) revert InvalidProof();
          // going into storage, doing it here instead of in the modifier, because we need the values down the line
            uint256 startingID = getStartingID(root);
          // Is the claim on?
            if(startingID == 0) revert RootNotClaimable(root);
            // Hasn't the address claimed yet?
            if(hasClaimed(root, recipient)) revert AlreadyClaimed(root, recipient);
            _supplyCheck(dropAmount, startingID, tokenIDsFromStartingID, true);
            _setClaimedValue(root, recipient, 1); //set Claimed for this drop/root
            if(tokenIDsFromStartingID == 0) {
                console.log("minting single token");
                console.log("startingID", startingID);
                console.log("dropAmount", dropAmount);
                console.log("recipient", recipient);
                // if tokenIDsFromStartingID is 0, mint only startingID
                _mint(recipient, startingID, dropAmount, "0");

            } else {
                console.log("minting batch");
                // set up relevant variables for mintBatch
                uint256[] memory tokenIDs = new uint256[](tokenIDsFromStartingID + 1);
                for(uint256 i = 0; i < tokenIDsFromStartingID + 1; i++) {
                    tokenIDs[i] = startingID + i;
                }
                uint256[] memory amounts = new uint256[](tokenIDsFromStartingID + 1);
                for(uint256 i = 0; i < tokenIDsFromStartingID + 1; i++) {
                    amounts[i] = dropAmount;
                }
                _mintBatch(recipient, tokenIDs, amounts, "0");
            }   

      }


