        } else _supplyCheck(amount, tokenId, 0, true); // Previous route already includes a supply check due to param check
        if(slaveMode) _setMaxSupply(tokenId + 1000, 1);
        _mint(recipient, tokenId, amount, "0");
    } 


    function batchAdMintSlave(address[][] memory recipients, uint256[][] memory amounts, uint256[] memory tokenIds) external onlyOwner {
