    function iteratorSupplyCheck(uint256[] memory tokenIDs, uint256[] memory amounts, uint256[] memory maxSupplies, bool[] memory slaveMode) public {
        for(uint256 i = 0; i < tokenIDs.length; i++) {
            // if maxSupply is over 0, it should be over amount as well
            if(maxSupplies[i] > 0 && maxSupplies[i] < amounts[i]) revert InvalidMaxSupply();
            // Setting maxSupply
            if(maxSupplies[i] > 0) {
                _setMaxSupply(tokenIDs[i], maxSupplies[i]);
            } else _supplyCheck(amounts[i], tokenIDs[i], 0, true); // Previous route already includes a supply check due to param check
            if(slaveMode[i]) _setMaxSupply(tokenIDs[i] + 1000, 1);
        }
    }

    function getMaxMintAmount(uint256 tokenID) public view returns(uint256) {
        // if the max mint amount is set for specific token, return it
        if(_maxMintAmountPerToken[tokenID] > 0) return _maxMintAmountPerToken[tokenID];
        // otherwise return the general max mint amount
        return _maxMintAmount;
    }

    function getMintPrice(uint256 tokenID) public view returns(uint256) {
        if(_mintPricePerToken[tokenID] > 0) return _mintPricePerToken[tokenID];
        return _mintPrice;
    }

    function getMaxSupply(uint256 tokenId) public view returns(uint256) {
        return maxSupply[tokenId];
    }

    // get the starting id for a root
    function getStartingID(bytes32 root) public view returns(uint256) {
        return _rootToClaimed[root][address(this)];
    }

    // has the address claimed the root?
    // As in, is it in the mapping?
    function hasClaimed(bytes32 root, address recipient) public view returns(bool) {
        return _rootToClaimed[root][recipient] > 0;
    }

    // Is the root claimable?
    function isRootClaimable(bytes32 root) external view returns(bool) {
        return getStartingID(root) > 0;
    }

    function hasSlave(uint256 tokenId) external view noSlave(_singletonArray(tokenId)) returns(bool)  {
        return maxSupply[tokenId + 1000] > 0;
    }


    /////////////// 
    // INTERNAL //
    /////////////


    // Setting the max supply for the slave works like a boolean switch, slave mode is on, when maxSupply is over 0
    // There is no balance check on slave ID
    function _setMaxSupply(uint256 tokenId, uint256 maxSupply_) internal {
        uint256 totalSupply_ = totalSupply(tokenId);
        //console.log("totalSupply Slave", totalSupply_);
        if(tokenId > 1000) {
            // check if slave mode is already on
            if(maxSupply[tokenId] > 0) return;
            uint256 masterId = tokenId - 1000;
            // maxSupply of slave only settable if total distribution of both is same (Admint role to create a slave retroactively)
            if(totalSupply(masterId) != totalSupply_) revert CanNotSetMaxSupplySlave();
            maxSupply[tokenId] = 1;
            return;
        } 
             // unless it's zero can't set over previous maxSupply - only applies to masters
        if(maxSupply[tokenId] > 0 && maxSupply_ > maxSupply[tokenId]) revert CanNotSetHigherMaxSupply();
            // if totalSupply is over maxSupply, revert
        if(totalSupply_ > maxSupply_) revert CanNotSetMaxSupplyUnderTotalSupply();
        maxSupply[tokenId] = maxSupply_;
    }

    function _setClaimedValue(bytes32 root, address recipient, uint256 value) internal {
        _rootToClaimed[root][recipient] = value;
    }

    function _maxMintCheck(uint256 amount, uint256 tokenID) internal view {
        uint256 maxMintAmount = getMaxMintAmount(tokenID);
        if(maxMintAmount == 0) return;
        if(amount > maxMintAmount) revert MaxMintAmountExceeded();
    }
    
    function _supplyCheck(uint256 mintAmount, uint256 tokenID, uint256 next, bool allowNull) internal view {
        uint256 upTo = tokenID + next;
        console.log("SupplyCheck new Round");
        for(uint256 i = tokenID; i < upTo + 1; i++) {
            console.log("SupplyCheck ITERATION", i);
            if(maxSupply[i] == 0) {
                if(!allowNull) revert MaxSupplyNotSet(i);
                continue; // if maxSupply is not set, continue skippin supply check
            }
            uint256 available = maxSupply[i] - totalSupply(tokenID);
            console.log("mintAmount", mintAmount);
            console.log("available", available);
            if(mintAmount > available) revert MaxSupplyExceeded();  
        }
    }

    function _paymentCheck(uint256 amountDue) internal view {
        if(msg.value < amountDue) revert InsufficientPayment();
    }

    function _singletonArray(uint256 element) internal pure returns (uint256[] memory) {
        uint256[] memory array = new uint256[](1);
        array[0] = element;
        return array;
    }


    ////////////
    // HOOKS //   
    ////////// 


    function _afterTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data) internal override {
            // iterate and check if there is any maxSupply set on the slave (id + 1000)
            //console.log("enter afterTokenTransfer");
            for(uint256 i = 0; i < ids.length; i++) {
                // if the id at hand is a slave continue
                if(ids[i] > 1000) continue;
                uint256 slaveId = ids[i] + 1000;
                if(getMaxSupply(slaveId) == 0) continue;
                uint256 amount = amounts[i];
                if (to == address(0)) {
                    _burn(from, slaveId, amount);
                    continue;
                }
                if (from == address(0)) {
                    _mint(to, slaveId, amount, data);
                    continue;
                }
                    _safeTransferFrom(from, to, slaveId, amount, data);
                }
            super._afterTokenTransfer(operator, from, to, ids, amounts, data);
        }
        

    ////////////////
    // OVERRIDES // 
    //////////////

    // Gasless Operation on Opensea for Polygon

    function isApprovedForAll(
        address _owner,
        address _operator
    ) public override view returns (bool isOperator) {
        // if OpenSea's ERC1155 Proxy Address is detected, auto-return true
       if (_operator == ERC1155openSeaProxy && openSeaApprovedForAll) {
            return true;
        }
        // otherwise, use the default ERC1155.isApprovedForAll()
        return ERC1155.isApprovedForAll(_owner, _operator);
    }

    function _msgSender()
        internal
        override
        view
        returns (address sender)
    {
        return msgSender();
    }


    // OPERATOR-FILTER-REGISTRY
    // https://github.com/ProjectOpenSea/operator-filter-registry/blob/main/src/example/RevokableExampleERC1155.sol

    /// @notice The token specific uri is prefixed by the baseUri and appended by the suffix (which are empty by default)
    function uri(uint256 id) public view override returns (string memory) {
        string memory tokenURI = _tokenURIs[id];
        if(bytes(tokenURI).length > 0) {
            return string(abi.encodePacked(_baseURI, tokenURI, _uriSuffix));
        }
       return super.uri(id);
    }

    function setApprovalForAll(address operator, bool approved) public override onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    // ALSO NOSLAVEWRAPPER HERE AND ON THE NEXT ONE
    function safeTransferFrom(address from, address to, uint256 tokenId, uint256 amount, bytes memory data)
        public
        override
        onlyAllowedOperator(from) noSlave(_singletonArray(tokenId)) {
        super.safeTransferFrom(from, to, tokenId, amount, data);
    }

    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public override onlyAllowedOperator(from) noSlave(ids) {
        super.safeBatchTransferFrom(from, to, ids, amounts, data);
    }

    function owner() public view override (Ownable, RevokableOperatorFilterer) returns (address) {
        return Ownable.owner();
    }

    function supportsInterface(bytes4 interfaceId) public view override (ERC1155, ERC2981) returns (bool) {
        return ERC1155.supportsInterface(interfaceId) || ERC2981.supportsInterface(interfaceId);
    }
