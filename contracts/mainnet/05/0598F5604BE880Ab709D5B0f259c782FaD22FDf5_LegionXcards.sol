    /// @dev Mints a slave retroactively. Checks supply, and if the recipient holds  the same amount of the master
    /// @param recipients the recipients of the slave tokens
    /// @param amounts the amount of slave tokens to be minted
    /// @param tokenId the SLAVE token ID
    function adMintSlave(address[] memory recipients, uint256[] memory amounts, uint256 tokenId) public onlyOwner {
        // if tokenId is not a slave, revert
        if(tokenId < 1000) revert IdIsNotASlave();
        // if maxSupply set on slave, revert - slave mode already on
        if(maxSupply[tokenId] > 0) revert CanNotMintSlaveSlaveModeAlreadyOn();
        // Get the total supply for the master
        uint256 totalSupply_ = totalSupply(tokenId - 1000);
        // if totalSupply is 0, revert - master not minted yet
        if(totalSupply_ == 0) revert CanNotMintSlaveMasterNotYetMinted();
        uint256 amount = 0;
        for(uint256 i = 0; i < recipients.length; i++) {
            amount += amounts[i]; //Probably double but no state involved
            if(amount > totalSupply_) revert CanNotMintSlaveMasterMismatch();
            // check that the amount corresponds with the recipients balance of the master
            uint256 masterBalance = balanceOf(recipients[i], tokenId - 1000);
            if(masterBalance != amounts[i]) revert CanNotMintSlave(masterBalance, tokenId, recipients[i]);
            _mint(recipients[i], tokenId, amounts[i], '0x00');
            // set the maxSupply of the slave to 1
            //
        }
        if(amount != totalSupply_) revert CanNotMintSlaveMasterMismatch();  
        _setMaxSupply(tokenId, 1);
    }


