pragma solidity ^0.8.0;


/// @title Interface defining a contract which can be staked upon a "tile" in the mission control
interface IMissionControlStakeable {

    /**
    * @notice Event emitted when a token is staked
    * @param _userAddress address of the staker
    * @param _stakeable Address to either the IMCStakeable implementation or the possibly the underlying token?
    * @param _tokenId Token id which has been staked
    */
    event TokenClaimed(address _userAddress, address _stakeable, uint _tokenId);
    /**
    * @notice Event emitted when a token is unstaked
    * @param _userAddress address of the staker
    * @param _stakeable Address to either the IMCStakeable implementation or the possibly the underlying token?
    * @param _tokenId Token id which has been unstaked
    */
    event TokenReturned(address _userAddress, address _stakeable, uint _tokenId);

    /**
    * @notice Event emitted when the mission control is set
    * @param missionControl The address of the mission control
    */
    event MissionControlSet(address missionControl);
    /**
    * @notice Resets the seed of a staked token. A cheaper alternative to onCollect when you do not need the number of harvested tokens
    * @param _userAddress The players address
    * @param _nonce Tile-staking nonce
    */
    function reset(address _userAddress, uint _nonce) external;

    /**
    * @notice Function to check the number of tokens ready to be harvested from this tile
    * @param _userAddress The players address
    * @param _nonce Tile-staking nonce
    * @return _amount Number of tokens that can be collected
    * @return _retTokenId The tokenId of the tokens that can be collected
    */
    function checkTile(address _userAddress, uint _nonce) external view returns (uint _amount, uint _retTokenId);

    /**
    * @notice Used to fetch new seed time upon collection some resources
    * @param _userAddress The players address
    * @param _nonce Tile-staking nonce
    * @return _amount Number of tokens that can be collected
    * @return _retTokenId The tokenId of the tokens that can be collected
    * @dev It is vital for this function to update the rng
    */
    function onCollect(address _userAddress, uint _nonce) external returns (uint _amount, uint _retTokenId);

    /**
    * @notice Used to see if the token can be used as the base of a tile, or if it is meant to be staked upon another token
    * @param _tokenId The id of the token
    * @return _isBase Whether this token is a base or not
    */
    function isBase(uint _tokenId) view external returns (bool _isBase);

    function isRaidable(uint _tokenId) view external returns (bool _isRaidable);

    /**
    * @notice transfers ownership of a token from the player to the stakeable contract
    * @param _currentOwner Address of the current owner
    * @param _tokenId The id of the token
    * @param _nonce Tile-staking nonce
    * @param _underlyingToken todo
    * @param _underlyingTokenId todo
    */
    function onStaked(address _currentOwner, uint _tokenId, uint _nonce, address _underlyingToken, uint _underlyingTokenId) external;


    /**
    * @notice transfers ownership of a token from the stakeable contract back to the player
    * @param _newOwner Address of the soon-to-be owner
    * @param _nonce Tile-staking nonce
    */
    function onUnstaked(address _newOwner, uint _nonce) external;

    function checkTimestamp(address _userAddress, uint _nonce) external view returns (uint _timestamp);

    /**
    * @notice returns the remaining time until a tile is filled
    * @param _userAddress The players address
    * @param _nonce Tile-staking nonce
    * @param _tokenId id of the token. Not used for now
    * @return _timeLeft The time left, -1 if the time can not be predicted
    */
    function timeLeft(address _userAddress, uint _nonce, uint _tokenId) external view returns(int _timeLeft);

}

pragma solidity ^0.8.0;

import "./IMissionControl.sol";
import "./IMissionControlStakeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./IAssetManager.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "hardhat/console.sol";

/// @title Implementation of IMissionControl
contract MissionControl is IMissionControl, OwnableUpgradeable, EIP712Upgradeable {
    struct Coords {
        int256 x;
        int256 y;
        int256 z;
    }

    /**
     * @notice Event emitted when an NFT is placed on a tile
     * @param user The users address
     * @param tokenAddress The NFT address
     * @param tokenId The id of the token
     * @param x x-coordinate
     * @param y y-coordinate
     * @param z z-coordinate
     */
    event NFTPlaced(
        address user,
        address tokenAddress,
        uint256 tokenId,
        int256 x,
        int256 y,
        int256 z
    );

    /**
     * @notice Event emitted when an NFT is removed from a tile
     * @param user The users address
     * @param tokenAddress The address of the removed token
     * @param underlyingAddress The address of the underlying (base) token, zero address if there is none
     * @param tokenId The token ID of the removed token
     * @param underlyingTokenId The tokenId of the underlying (base) token, zero if there is none
     * @param x x-coordinate
     * @param y y-coordinate
     * @param z z-coordinate
     */
    event NFTRemoved(
        address user,
        address tokenAddress,
        address underlyingAddress,
        uint256 tokenId,
        uint256 underlyingTokenId,
        int256 x,
        int256 y,
        int256 z
    );

    /**
     * @notice Emitted when a loot crate is found
     * @param user The user that found the crate
     */
    event LootCrateFound(address user);

    /**
     * @notice Event emitted when the whitelist is set
     * @param token The token address
     * @param whitelisted Whether the token is whitelisted or not
     */
    event WhitelistSet(address token, bool whitelisted);

    /**
     * @notice Event emitted when the mint is set
     * @param newMint The address of the mint
     */
    event MintSet(address newMint);

    /**
     * @notice Event emitted when the radius is set
     * @param newRadius The new radius
     */
    event RadiusSet(uint256 newRadius);

    event Raid(address defender, uint256 raidId, uint256 booty, uint256 timeStamp);

    // Mapping storing the information regarding staked tokens
    mapping(address => mapping(int256 => mapping(int256 => TileElement[2]))) tiles;
    mapping(address => uint256) stakeNonces;
    mapping(uint256 => bool) raidSpent;
    // Mapping of whitelisted tokens
    mapping(address => bool) whiteList;
    // Mapping of addresses allowed to sign raids
    mapping(address => bool) signers;

    // The contract responsible for minting rewards
    IAssetManager mint;
    // The currently allowed max radius of the mission control
    uint256 allowedRadius;
    // raid params
    int256 raidTimeout;

    // Constants
    uint256 constant WASTE_ID = uint256(IAssetManager.AssetIds.Waste);
    bytes32 private constant RAID_MESSAGE =
        keccak256("RaidMessage(address _defender,uint256 _timestamp,uint256 _raidId)");

    // out of order so that i can upgrade on testnet for now
    mapping(address => address) tokenAliases;
    bool public enableSignature;
    mapping(address => uint256) public nonces;
    bool public paused;
    Coords[] public coords;
    // This allows players to get a sig and then use it later but it's no big deal
    bytes32 private constant COLLECT_MESSAGE =
        keccak256("CollectMessage(address _user,uint256 _nonce)");

    /**
     * @notice Modifier to hinder calls with invalid coordinates
     * @param _x X-coordinate of tile
     * @param _y Y-coordinate of tile
     * @param _z Z-coordinate of tile
     */
    modifier validCoordinates(
        int256 _x,
        int256 _y,
        int256 _z
    ) {
        int256 sum = _x + _y + _z;
        uint256 rad = (abs(_x) + abs(_y) + abs(_z)) / 2;
        require(
            rad <= allowedRadius && rad > 0 && sum == 0,
            "MISSION_CONTROL: Invalid coordinates"
        );
        _;
    }

    /**
     * @notice Modifier to hinder calls with invalid coordinates, except for out of bounds
     * @param _x X-coordinate of tile
     * @param _y Y-coordinate of tile
     * @param _z Z-coordinate of tile
     */
    modifier validCoordinates_noMaxRad(
        int256 _x,
        int256 _y,
        int256 _z
    ) {
        int256 sum = _x + _y + _z;
        uint256 rad = (abs(_x) + abs(_y) + abs(_z)) / 2;
        require(rad > 0 && sum == 0, "MISSION_CONTROL: Invalid coordinates");
        _;
    }

    /**
     * @notice Modifier to only allow whitelisted tokens
     * @param _tokenAddress The address of the token to be checked against the whitelist
     */
    modifier onWhitelist(address _tokenAddress) {
        require(
            whiteList[_tokenAddress] || whiteList[tokenAliases[_tokenAddress]],
            "MISSION_CONTROL: Not whitelisted"
        );
        _;
    }

    /**
     *
     */
    modifier checkSignature(
        uint8 _v,
        bytes32 _r,
        bytes32 _s
    ) {
        if (enableSignature) {
            address signer = ECDSA.recover(
                _hashTypedDataV4(
                    keccak256(abi.encode(COLLECT_MESSAGE, msg.sender, nonces[msg.sender]++))
                ),
                _v,
                _r,
                _s
            );
            require(signers[signer], "MISSION_CONTROL: INVALID_SIGNATURE");
        }
        _;
    }

    modifier whenNotPaused() {
        require(!paused, "MISSION_CONTROL: PAUSED");
        _;
    }

    /**
     * @notice Initialiser for this contract
     * @param _radius The mission control radius
     * @param _mint The address of the mint
     */
    function initialize(uint256 _radius, address _mint) external initializer {
        allowedRadius = _radius;
        mint = IAssetManager(_mint);
        __Ownable_init();
        __EIP712_init("Mission Control", "1");
    }

    /// @inheritdoc IMissionControl
    function placeNFT(
        int256 _x,
        int256 _y,
        int256 _z,
        uint256 _tokenId,
        address _tokenAddress
    ) external override whenNotPaused {
        __placeNFT(_x, _y, _z, _tokenId, _tokenAddress);
    }

    /// @inheritdoc IMissionControl
    function placeNFTs(PlaceOrder[] memory _placeOrders) external override whenNotPaused {
        for (uint256 i; i < _placeOrders.length; i++) {
            __placeNFT(
                _placeOrders[i].x,
                _placeOrders[i].y,
                _placeOrders[i].z,
                _placeOrders[i].tokenId,
                _placeOrders[i].tokenAddress
            );
        }
    }

    /// @inheritdoc IMissionControl
    function removeNFT(int256 _x, int256 _y, int256 _z) external override {
        __removeNFT(_x, _y, _z);
    }

    /// @inheritdoc IMissionControl
    function removeNFTs(PlaceOrder[] memory _placeOrders) external override {
        for (uint256 i; i < _placeOrders.length; i++) {
            __removeNFT(_placeOrders[i].x, _placeOrders[i].y, _placeOrders[i].z);
        }
    }

    /// @inheritdoc IMissionControl
    function checkTile(
        address _user,
        int256 _x,
        int256 _y,
        int256 _z
    ) external view override validCoordinates(_x, _y, _z) returns (uint256, uint256) {
        TileElement memory tile = tiles[_user][_x][_y][__zeroOrOne(_x, _y, _user)];
        return IMissionControlStakeable(tile.stakeable).checkTile(_user, tile.nonce);
    }

    /** @inheritdoc IMissionControl
     * @dev This code is absolute cancer, but is this way to only require 1 cross contract call
     */
    function collectFromTiles(
        CollectOrder[] memory _orders,
        uint8 _v,
        bytes32 _r,
        bytes32 _s
    ) external override checkSignature(_v, _r, _s) whenNotPaused {
        uint256 arrayLen = _orders.length;
        uint256[] memory ids = new uint256[](arrayLen);
        uint256[] memory amounts = new uint256[](arrayLen);
        uint256 cId;
        uint256 cAmount;
        // I hate this loop
        for (uint256 i; i < _orders.length; i++) {
            (cAmount, cId) = _handle_collect(_orders[i].x, _orders[i].y, _orders[i].z);
            ids[i] = cId;
            amounts[i] = cAmount;
        }
        // Reusing cId, cAmount to save on variables here
        mint.trustedBatchMint(msg.sender, ids, amounts);
    }

    function collectFromTilesAndRemove(
        uint8 _v,
        bytes32 _r,
        bytes32 _s
    ) external checkSignature(_v, _r, _s) {
        uint256 arrayLen = coords.length;
        uint256[] memory ids = new uint256[](arrayLen);
        uint256[] memory amounts = new uint256[](arrayLen);
        uint256 cId;
        uint256 cAmount;
        // I hate this loop
        for (uint256 i; i < arrayLen; i++) {
            TileElement[2] memory tileElements = tiles[msg.sender][coords[i].x][coords[i].y];

            if (tileElements[1].nonce != 0 || tileElements[0].nonce != 0) {
                (cAmount, cId) = _handle_collect(coords[i].x, coords[i].y, coords[i].z);
                ids[i] = cId;
                amounts[i] = cAmount;
            }

            if (tileElements[1].nonce != 0) {
                __removeNFT(coords[i].x, coords[i].y, coords[i].z);
                __removeNFT(coords[i].x, coords[i].y, coords[i].z);
            }

            if (tileElements[1].nonce == 0 && tileElements[0].nonce != 0) {
                __removeNFT(coords[i].x, coords[i].y, coords[i].z);
            }
        }
        mint.trustedBatchMint(msg.sender, ids, amounts);
    }

    /// @inheritdoc IMissionControl
    function notifyRaided(
        address _defender,
        uint256 _timestamp,
        uint256 _raidId,
        CollectOrder[] calldata _orders,
        uint8 _v,
        bytes32 _r,
        bytes32 _s
    ) external override {
        address signer = ECDSA.recover(
            _hashTypedDataV4(keccak256(abi.encode(RAID_MESSAGE, _defender, _timestamp, _raidId))),
            _v,
            _r,
            _s
        );
        require(signers[signer], "MISSION_CONTROL: INVALID_SIGNATURE");
        require(
            int256(block.timestamp) - int256(_timestamp) < raidTimeout,
            "MISSION_CONTROL: Signature has timed out"
        );
        require(!raidSpent[_raidId], "MISSION_CONTROL: Raid already registered");
        uint256 totalAmount;
        uint256 cAmount;
        for (uint256 i = 0; i < _orders.length; i++) {
            (cAmount, ) = _handle_raid_collect(_defender, _orders[i].x, _orders[i].y);
            totalAmount += cAmount;
        }
        raidSpent[_raidId] = true;
        emit Raid(_defender, _raidId, totalAmount, _timestamp);
    }

    /// @inheritdoc IMissionControl
    function checkStakedOnTile(
        address _user,
        int256 _x,
        int256 _y,
        int256 _z
    )
        public
        view
        override
        validCoordinates(_x, _y, _z)
        returns (TileElement memory _base, TileElement memory _top)
    {
        if (tiles[_user][_x][_y][1].nonce != 0) {
            _top = tiles[_user][_x][_y][1];
            _base = tiles[_user][_x][_y][0];
        } else if (tiles[_user][_x][_y][0].nonce != 0) {
            _base = tiles[_user][_x][_y][0];
        }
    }

    /// @inheritdoc IMissionControl
    function timeLeft(
        address _user,
        int256 _x,
        int256 _y,
        int256 _z
    ) external view override validCoordinates(_x, _y, _z) returns (int256 _timeLeft) {
        TileElement memory tile = tiles[_user][_x][_y][__zeroOrOne(_x, _y, _user)];
        _timeLeft = IMissionControlStakeable(tile.stakeable).timeLeft(_user, tile.nonce, 1);
    }

    /**
     * Private functions
     */

    /**
     * @notice Calculates the absolute value of an integer
     * @param _value The integer
     * @return Its absolute value
     */
    function abs(int256 _value) private pure returns (uint256) {
        return _value < 0 ? uint256(-_value) : uint256(_value);
    }

    /**
     * @notice Handles the placement of a tile if it is classified as a base
     * @param _x X-coordinate of tile
     * @param _y Y-coordinate of tile
     * @param _z Z-coordinate of tile
     * @param _tokenId Token ID of NFT
     * @param _stakeable Interface towards the token contract
     */
    function _handleBasePlacement(
        int256 _x,
        int256 _y,
        int256 _z,
        uint256 _tokenId,
        address _stakeable,
        uint256 _nonce
    ) private {
        require(tiles[msg.sender][_x][_y][0].nonce == 0, "MISSION_CONTROL: Already a base on tile");
        IMissionControlStakeable(_stakeable).onStaked(msg.sender, _tokenId, _nonce, address(0), 0);
        // Could use this variable above but i think this saves on case in case of revert on the above line?
        TileElement storage tileElement = tiles[msg.sender][_x][_y][0];
        tileElement.nonce = _nonce;
        tileElement.tokenId = _tokenId;
        tileElement.stakeable = _stakeable;
    }

    /**
     * @notice Handles the placement of a tile if it is classified as a top
     * @param _x X-coordinate of tile
     * @param _y Y-coordinate of tile
     * @param _z Z-coordinate of tile
     * @param _tokenId Token ID of NFT
     * @param _stakeable Interface towards the token contract
     */
    function _handleTopPlacement(
        int256 _x,
        int256 _y,
        int256 _z,
        uint256 _tokenId,
        address _stakeable,
        uint256 _nonce
    ) private {
        require(
            tiles[msg.sender][_x][_y][0].nonce != 0 && tiles[msg.sender][_x][_y][1].nonce == 0,
            "MISSION_CONTROL: Invalid placement"
        );
        IMissionControlStakeable(_stakeable).onStaked(
            msg.sender,
            _tokenId,
            _nonce,
            tiles[msg.sender][_x][_y][0].stakeable,
            tiles[msg.sender][_x][_y][0].tokenId
        );
        TileElement storage tileElement = tiles[msg.sender][_x][_y][1];
        tileElement.nonce = _nonce;
        tileElement.tokenId = _tokenId;
        tileElement.stakeable = _stakeable;
    }

    /**
     * @notice Handles overarching logic for placing a nft on a tile
     * @param _x X-coordinate of tile
     * @param _y Y-coordinate of tile
     * @param _z Z-coordinate of tile
     * @param _tokenId Token ID of NFT
     * @param _tokenAddress Address of NFT, which is presumed to implement the IMissionControlStakeable interface
     */
    function __placeNFT(
        int256 _x,
        int256 _y,
        int256 _z,
        uint256 _tokenId,
        address _tokenAddress
    ) private validCoordinates(_x, _y, _z) onWhitelist(_tokenAddress) {
        // If the sender is not the owner of the token, the transaction should revert here
        address tokenAddress = _tokenAddress;
        if (tokenAliases[_tokenAddress] != address(0)) {
            tokenAddress = tokenAliases[_tokenAddress];
        }
        if (IMissionControlStakeable(tokenAddress).isBase(_tokenId)) {
            _handleBasePlacement(_x, _y, _z, _tokenId, tokenAddress, ++stakeNonces[msg.sender]);
        } else {
            _handleTopPlacement(_x, _y, _z, _tokenId, tokenAddress, ++stakeNonces[msg.sender]);
        }

        emit NFTPlaced(msg.sender, tokenAddress, _tokenId, _x, _y, _z);
    }

    /**
     * @notice Handles the logic for unstaking a token
     * @param _x X-coordinate of tile
     * @param _y Y-coordinate of tile
     * @param _z Z-coordinate of tile
     */
    function __removeNFT(
        int256 _x,
        int256 _y,
        int256 _z
    ) private validCoordinates_noMaxRad(_x, _y, _z) {
        TileElement[2] storage tileElements = tiles[msg.sender][_x][_y];
        uint256 zeroOrOne = __zeroOrOne(_x, _y, msg.sender);
        _handleReturn(tileElements[zeroOrOne]);
        if (zeroOrOne == 1) {
            IMissionControlStakeable(tileElements[0].stakeable).reset(
                msg.sender,
                tileElements[0].nonce
            );
        }
        emit NFTRemoved(
            msg.sender,
            tileElements[zeroOrOne].stakeable,
            zeroOrOne == 1 ? tileElements[0].stakeable : address(0),
            tileElements[zeroOrOne].tokenId,
            zeroOrOne == 1 ? tileElements[0].tokenId : 0,
            _x,
            _y,
            _z
        );
    }

    function _handleReturn(TileElement storage element) private {
        IMissionControlStakeable(element.stakeable).onUnstaked(msg.sender, element.nonce);
        element.nonce = 0;
        element.stakeable = address(0);
        element.tokenId = 0;
    }

    /**
     * @notice Used to check whether there is a base, top or no token staked on a tile
     * @param _x x-coordinate
     * @param _y y-coordinate
     * @return zeroOrOne 1 if there is a top, 0 if there is a base, reverts if there is none
     */
    function __zeroOrOne(
        int256 _x,
        int256 _y,
        address _sender
    ) internal view returns (uint256 zeroOrOne) {
        if (tiles[_sender][_x][_y][1].nonce != 0) {
            zeroOrOne = 1;
        } else if (tiles[_sender][_x][_y][0].nonce != 0) {
            zeroOrOne = 0;
        } else {
            revert("MISSION_CONTROL: No token staked on coordinates");
        }
    }

    function _handle_collect(
        int256 _x,
        int256 _y,
        int256 _z
    ) private validCoordinates(_x, _y, _z) returns (uint256, uint256) {
        TileElement memory tileElement = tiles[msg.sender][_x][_y][__zeroOrOne(_x, _y, msg.sender)];
        return
            IMissionControlStakeable(tileElement.stakeable).onCollect(
                msg.sender,
                tileElement.nonce
            );
    }

    function _handle_raid_collect(
        address _defender,
        int256 _x,
        int256 _y
    ) private validCoordinates(_x, _y, -(_x + _y)) returns (uint256, uint256) {
        TileElement memory tileElement = tiles[_defender][_x][_y][__zeroOrOne(_x, _y, _defender)];
        IMissionControlStakeable iStakeable = IMissionControlStakeable(tileElement.stakeable);
        if (!iStakeable.isRaidable(tileElement.tokenId)) {
            return (0, WASTE_ID);
        }
        return iStakeable.onCollect(msg.sender, tileElement.nonce);
    }

    /**
     * Admin functions
     */

    /**
     * @notice Allows the owner to set a token to whitelisted
     * @param _token Address of token
     * @param _whitelisted Whether it should be whitelisted
     */
    function setWhitelist(address _token, bool _whitelisted) external onlyOwner {
        whiteList[_token] = _whitelisted;
        emit WhitelistSet(_token, _whitelisted);
    }

    /**
     * @notice Sets an alias for a stakeable
     * @param _token Token address
     * @param _alias An alias for it
     */
    function setStakeableAlias(address _token, address _alias) external onlyOwner {
        tokenAliases[_alias] = _token;
    }

    /**
     * @notice Allows the owner to set the mint
     * @param _mint The address of the mint
     */
    function setMint(address _mint) external onlyOwner {
        mint = IAssetManager(_mint);
        emit MintSet(_mint);
    }

    /**
     * @notice Allows the owner to set the maximum radius
     * @param _radius The radius
     */
    function setRadius(uint256 _radius) external onlyOwner {
        require(_radius > 0, "Radius can't be zero");
        allowedRadius = _radius;
        emit RadiusSet(_radius);
    }

    function setRaidParams(int256 _timeout) external onlyOwner {
        require(_timeout > 0, "MISSION_CONTROL: Raid timeout must be >0");
        raidTimeout = _timeout;
    }

    function setRaidSigner(address _address, bool _isSigner) external onlyOwner {
        signers[_address] = _isSigner;
    }

    function addCoords(Coords[] memory _coords) external onlyOwner {
        for (uint256 i; i < _coords.length; i++) {
            coords.push(_coords[i]);
        }
    }

    function setPaused(bool _paused) external onlyOwner {
        paused = _paused;
    }

    /// @inheritdoc IMissionControl
    function checkLastUpdated(
        address _user,
        int256 _x,
        int256 _y,
        int256 _z
    ) external view override validCoordinates(_x, _y, _z) returns (uint256 _timestamp) {
        uint256 zeroOrOne = __zeroOrOne(_x, _y, _user);
        _timestamp = IMissionControlStakeable(tiles[_user][_x][_y][zeroOrOne].stakeable)
            .checkTimestamp(_user, tiles[_user][_x][_y][zeroOrOne].nonce);
    }

    function setSignature(bool _requiresSignature) external onlyOwner {
        enableSignature = _requiresSignature;
    }
}

