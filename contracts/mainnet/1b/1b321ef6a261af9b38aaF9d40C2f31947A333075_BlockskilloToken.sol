import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

contract BlockskilloToken is
    Initializable,
    ERC20Upgradeable,
    OwnableUpgradeable,
    AccessControlUpgradeable,
    ReentrancyGuardUpgradeable
{
    using SafeMathUpgradeable for uint256;

    uint256 private constant _maxSupply = 300000000;
    bytes32 private constant _ADMIN_ROLE = keccak256("ADMIN_ROLE");
    uint256 private constant _privateSaleQuotaPercentage = 40;
    uint256 private constant _promoterQuotaPercentage = 10;
    uint256 private constant _teamQuotaPercentage = 10;
    uint256 private constant _studentsQuotaPercentage = 20;
    uint256 private constant _facultyQuotaPercentage = 10;
    uint256 private constant _hiringManagersQuotaPercentage = 5;
    uint256 private constant _communityContributionQuotaPercentage = 5;
    uint256 _unlockedQuota;
    address supplyAddress;
    enum Profile {
        Student,
        Team,
        Promoter,
        Faculty,
        HiringManager,
        Community,
        PrivateSale
    }

    mapping(uint256 => uint256) public profileToQuota;
    mapping(uint256 => uint256) public percentToMultiplier;

    event TokensDistributed(
        uint256 currentUnlockedQuota,
        uint256 unlockedQuota,
        uint256 profileQuota,
        address indexed recipient,
        uint256 amount
    );
    event CheckBalance(address indexed addr, uint256 bal);

    modifier onlyAuth(uint256 index) {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, _msgSender()) ||
                (hasRole(_ADMIN_ROLE, _msgSender()) && index != 6),
            "Not authorized"
        );
        _;
    }

    function initialize(uint256 _amount, address _admin) public initializer {
        supplyAddress = msg.sender;
        _unlockedQuota = 0;
        __Context_init_unchained();
        __ERC20_init("Blockskillo", "BSO");
        __Ownable_init();
        __AccessControl_init();
        __ReentrancyGuard_init();

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(_ADMIN_ROLE, _msgSender());
        setAdminRole(_admin);

        percentToMultiplier[100] = 100;
        percentToMultiplier[75] = 75;
        percentToMultiplier[50] = 50;
        percentToMultiplier[25] = 10;
        _mint(supplyAddress, _maxSupply * 10**decimals());
        unlockTokens(_amount);
    }

    function setAdminRole(address account) private onlyOwner {
        grantRole(_ADMIN_ROLE, account);
    }

    function getBalance(address addr) external {
        emit CheckBalance(addr, balanceOf(addr));
    }

    function getTotalSupply() external view onlyOwner returns (uint256) {
        return balanceOf(supplyAddress);
    }

    function unlockTokens(uint256 amount) public onlyOwner {
        require(amount > 0, "Invalid amount");
        _unlockedQuota = amount;

        uint256 promoterQuota = _unlockedQuota
            .mul(_promoterQuotaPercentage)
            .div(100);
        uint256 teamQuota = _unlockedQuota.mul(_teamQuotaPercentage).div(100);
        uint256 studentQuota = _unlockedQuota.mul(_studentsQuotaPercentage).div(
            100
        );
        uint256 facultyQuota = _unlockedQuota.mul(_facultyQuotaPercentage).div(
            100
        );
        uint256 hiringManagerQuota = _unlockedQuota
            .mul(_hiringManagersQuotaPercentage)
            .div(100);
        uint256 communityQuota = _unlockedQuota
            .mul(_communityContributionQuotaPercentage)
            .div(100);
        uint256 privateSaleQuota = _unlockedQuota
            .mul(_privateSaleQuotaPercentage)
            .div(100);

        if (profileToQuota[uint256(Profile.Promoter)] > 0) {
            profileToQuota[uint256(Profile.Promoter)] += promoterQuota;
        } else {
            profileToQuota[uint256(Profile.Promoter)] = promoterQuota;
        }

        if (profileToQuota[uint256(Profile.Team)] > 0) {
            profileToQuota[uint256(Profile.Team)] += teamQuota;
        } else {
            profileToQuota[uint256(Profile.Team)] = teamQuota;
        }

        if (profileToQuota[uint256(Profile.Student)] > 0) {
            profileToQuota[uint256(Profile.Student)] += studentQuota;
        } else {
            profileToQuota[uint256(Profile.Student)] = studentQuota;
        }

        if (profileToQuota[uint256(Profile.Faculty)] > 0) {
            profileToQuota[uint256(Profile.Faculty)] += facultyQuota;
        } else {
            profileToQuota[uint256(Profile.Faculty)] = facultyQuota;
        }

        if (profileToQuota[uint256(Profile.HiringManager)] > 0) {
            profileToQuota[
                uint256(Profile.HiringManager)
            ] += hiringManagerQuota;
        } else {
            profileToQuota[uint256(Profile.HiringManager)] = hiringManagerQuota;
        }

        if (profileToQuota[uint256(Profile.Community)] > 0) {
            profileToQuota[uint256(Profile.Community)] += communityQuota;
        } else {
            profileToQuota[uint256(Profile.Community)] = communityQuota;
        }

        if (profileToQuota[uint256(Profile.PrivateSale)] > 0) {
            profileToQuota[uint256(Profile.PrivateSale)] += privateSaleQuota;
        } else {
            profileToQuota[uint256(Profile.PrivateSale)] = privateSaleQuota;
        }
    }

    function distributeTokens(
        uint256 index,
        address recipient,
        uint256 amount
    ) external onlyAuth(index) nonReentrant {
        require(amount < profileToQuota[index], "Invalid amount");
        require(recipient != address(0), "Invalid Address");
        require(supplyAddress != address(0), "Invalid Profile");

        uint256 spentTokens = (_maxSupply.mul(10**decimals()) -
            (balanceOf(supplyAddress)));
        uint256 currentUnlockedQuota = _unlockedQuota.mul(10**decimals()) -
            spentTokens;
        uint256 percent = currentUnlockedQuota.mul(100).div(
            _unlockedQuota.mul(10**decimals())
        );

        uint256 multiplier;

        if (percent > 75) {
            multiplier = percentToMultiplier[100];
        } else if (percent > 50) {
            multiplier = percentToMultiplier[75];
        } else if (percent > 25) {
            multiplier = percentToMultiplier[50];
        } else {
            multiplier = percentToMultiplier[25];
        }

        uint256 approvedAmount = amount.mul(multiplier).div(100);

        require(approvedAmount <= type(uint128).max, "Overflow error"); // Overflow check

        _transfer(supplyAddress, recipient, approvedAmount.mul(10**decimals()));

        profileToQuota[index] = profileToQuota[index].sub(approvedAmount);

        emit TokensDistributed(
            currentUnlockedQuota.div(10**decimals()),
            _unlockedQuota,
            profileToQuota[index],
            recipient,
            approvedAmount
        );
    }

    function burnTokens(uint256 amount) external onlyOwner {
        require(amount > 0, "Invalid amount");
        require(amount <= balanceOf(msg.sender), "Insufficient balance");
        _burn(msg.sender, amount);
    }

    function rescueToken(
        address Address,
        uint256 _amount,
        uint256 profileIndex
    ) external onlyOwner {
        require(_amount > 0, "Invalid amount");
        require(Address != address(0), "Invalid Address");
        require(
            profileIndex <= 6,
            "Profile Index should within be 0 to 6 range"
        );
        profileToQuota[profileIndex] += _amount;
        _transfer(Address, msg.sender, _amount.mul(10**decimals()));
    }

    function getQuotaBalances() external view onlyOwner returns (uint256[] memory) {
        uint256[] memory balances = new uint256[](7);
        balances[uint256(Profile.Promoter)] = profileToQuota[
            uint256(Profile.Promoter)
        ];
        balances[uint256(Profile.Team)] = profileToQuota[uint256(Profile.Team)];
        balances[uint256(Profile.Student)] = profileToQuota[
            uint256(Profile.Student)
        ];
        balances[uint256(Profile.Faculty)] = profileToQuota[
            uint256(Profile.Faculty)
        ];
        balances[uint256(Profile.HiringManager)] = profileToQuota[
            uint256(Profile.HiringManager)
        ];
        balances[uint256(Profile.Community)] = profileToQuota[
            uint256(Profile.Community)
        ];
        balances[uint256(Profile.PrivateSale)] = profileToQuota[
            uint256(Profile.PrivateSale)
        ];

        return balances;
    }
