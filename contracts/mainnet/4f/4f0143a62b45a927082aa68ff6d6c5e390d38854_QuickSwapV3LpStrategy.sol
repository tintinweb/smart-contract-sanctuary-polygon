pragma solidity >=0.8.0 <0.9.0;

import 'hardhat/console.sol';

import {IERC20} from '../interfaces/IERC20.sol';

import {IQuickSwapV3Pool} from '../connectors/quickswap/IQuickSwapV3Pool.sol';
import {INonfungiblePositionManager} from '../connectors/quickswap/INonfungiblePositionManager.sol';
import {IFarmingCenter} from '../connectors/quickswap/IFarmingCenter.sol';
import {IDragonLair} from '../connectors/quickswap/IDragonLair.sol';

import {ISwapHelper} from '../interfaces/ISwapHelper.sol';

import {Math} from '@openzeppelin/contracts/utils/math/Math.sol';
import {LiquidityAmounts} from '../libraries/LiquidityAmounts.sol';
import {TickMath} from '../libraries/TickMath.sol';
import {FixedPoint192} from '../libraries/FixedPoint192.sol';

import {Strategy} from './Strategy.sol';

contract QuickSwapV3LpStrategy is Strategy {
    event Rebalance(
        int24 oldTickLower,
        int24 oldTickUpper,
        int24 newTickLower,
        int24 newTickUpper
    );
    event TickConfigUpdated(int24 tickLower, int24 tickUpper);

    address public constant USDC = 0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48;
    IERC20 public constant usdt =
        IERC20(0xc2132D05D31c914a87C6611C10748AEb04B58e8F);
    IERC20 public constant mai =
        IERC20(0xa3Fa99A148fA48D14Ed51d610c367C61876997F1);
    IERC20 public constant quick =
        IERC20(0xB5C064F955D8e7F38fE0460C556a72987494eE17);
    IDragonLair public constant dQuick =
        IDragonLair(0x958d208Cdf087843e9AD98d23823d32E17d723A1);
    IERC20 public constant qi =
        IERC20(0x580A84C73811E1839F75d86d75d88cCa0c241fF4);

    // USDT/MAI
    IQuickSwapV3Pool private constant pool =
        IQuickSwapV3Pool(0x5B1fD9Fa139353C5D9eaf459E4BD2f7A6B4812Bb);
    INonfungiblePositionManager public constant positionManager =
        INonfungiblePositionManager(0x8eF88E4c7CfbbaC1C163f7eddd4B578792201de6);
    IFarmingCenter public constant farmingCenter =
        IFarmingCenter(0x7F281A8cdF66eF5e9db8434Ec6D97acc1bc01E78);

    ISwapHelper private constant swapHelper =
        ISwapHelper(0x2Bd1b50F990A31b167c5e0354De106b542190897);

    uint256 public nftId;
    uint256 public farmingTokenId;
    uint256 public lastNetAssetValue;
    int24 public tickLower;
    int24 public tickUpper;

    struct UnderlyingAssets {
        uint256 usdtBalance;
        uint256 maiBalance;
        uint256 maiBalanceUsdt;
        uint256 usdtInLp;
        uint256 maiInLp;
        uint256 maiInLpUsdt;
        uint256 usdtFee;
        uint256 maiFee;
        uint256 maiFeeUsdt;
        uint256 quickReward;
        uint256 quickRewardUsdc;
    }

    struct Delta {
        int24 currentTick;
        uint160 currentSqrtP;
        int24 tickLower;
        uint160 lowerSqrtP;
        int24 tickUpper;
        uint160 upperSqrtP;
        bool tickUpdated;
    }

    function initialize(int24 tickLower_, int24 tickUpper_) public initializer {
        __Strategy_init(
            // 0xE8c7574f7355920B254a91658D04074e1C0FdeF4, // portfolio manager
            msg.sender,
            0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174, // usdc
            3000 // 30%
        );
        tickLower = tickLower_;
        tickUpper = tickUpper_;
    }

    function setTickConfig(int24 tickLower_, int24 tickUpper_)
        public
        onlyManager
    {
        tickLower = tickLower_;
        tickUpper = tickUpper_;
        _claimAndReinvest(portfolioManager);
        emit TickConfigUpdated(tickLower, tickUpper);
    }

    function _claimAndReinvest(address to_)
        internal
        virtual
        override
        returns (uint256 usdcClaimed_)
    {
        return _claimAndRebalance(to_, 9900);
    }

    function _claimAndRebalance(address to_, uint256 slippage_)
        internal
        returns (uint256 usdcClaimed_)
    {
        uint160 currentSqrtP = currentSqrtPrice();
        UnderlyingAssets memory assets = underlyingAssets();
        (
            assets.maiFee,
            assets.usdtFee,
            assets.quickReward,

        ) = _collectRewards();
        assets.maiFeeUsdt = _maiToUsdt(assets.maiFee, currentSqrtP);
        if (assets.quickReward > 0) {
            dQuick.leave(assets.quickReward);
            assets.quickReward = quick.balanceOf(address(this));
            quick.approve(address(swapHelper), assets.quickReward);
            assets.quickRewardUsdc = swapHelper.swapQuickToUsdc(
                assets.quickReward
            );
            console.log('[claim] quick claimed: %s', assets.quickReward);
            emit ClaimRewardToken(
                address(quick),
                assets.quickReward,
                assets.quickRewardUsdc
            );
        }
        console.log('[claim] usdt claimed: %s', assets.usdtFee);
        console.log('[claim] mai claimed: %s', assets.maiFee);
        emit ClaimRewardToken(address(usdt), assets.usdtFee, assets.usdtFee);
        emit ClaimRewardToken(address(mai), assets.maiFee, assets.maiFeeUsdt);

        usdcClaimed_ =
            assets.usdtFee +
            assets.maiFeeUsdt +
            assets.quickRewardUsdc;
        int256 revenue = int256(_netAssetValue(assets)) -
            int256(lastNetAssetValue);
        console.log('[claim] total claimed $: %s', usdcClaimed_);
        console.log('[claim] revenue:');
        console.logInt(revenue);
        uint256 reinvestUsdcAmount = Math.mulDiv(
            assets.quickRewardUsdc,
            reinvestRate,
            10000
        );
        uint256 rewardUsdcAmount = assets.quickRewardUsdc - reinvestUsdcAmount;
        if (rewardUsdcAmount > 0) {
            console.log('[claim] reward payout: %s', rewardUsdcAmount);
            usdc.transfer(to_, rewardUsdcAmount);
        }
        if (reinvestUsdcAmount > 0) {
            usdc.approve(address(swapHelper), reinvestUsdcAmount);
            swapHelper.swapUsdcToUsdt(reinvestUsdcAmount);
        }
        _rebalance(9900);

        uint256 nav = netAssetValue();
        emit NetAssetValueUpdated(lastNetAssetValue, nav);
        console.log('NAV before: %s, after: %s', lastNetAssetValue, nav);
        if (revenue > 0) {
            lastNetAssetValue = nav;
        }
        emit Invest(assets.usdtFee + assets.maiFeeUsdt + reinvestUsdcAmount);
        emit InvestToken(address(usdt), assets.usdtFee, assets.usdtFee);
        emit InvestToken(address(mai), assets.maiFee, assets.maiFeeUsdt);
        emit PayoutReward(nav, revenue, rewardUsdcAmount);
    }

    function _collectRewards()
        internal
        returns (
            uint256 maiAmount_,
            uint256 usdtAmount_,
            uint256 quickAmount_,
            uint256 qiAmount_
        )
    {
        if (nftId == 0) {
            return (0, 0, 0, 0);
        }
        INonfungiblePositionManager.CollectParams
            memory params = INonfungiblePositionManager.CollectParams({
                tokenId: nftId,
                recipient: address(this),
                amount0Max: type(uint128).max,
                amount1Max: type(uint128).max
            });
        (maiAmount_, usdtAmount_) = farmingCenter.collect(params);
        farmingCenter.collectRewards(_incentiveKey(), nftId);
        quickAmount_ = farmingCenter.claimReward(
            address(dQuick),
            address(this),
            0,
            type(uint256).max
        );
        qiAmount_ = farmingCenter.claimReward(
            address(qi),
            address(this),
            0,
            type(uint256).max
        );
    }

    function _incentiveKey()
        internal
        pure
        returns (IFarmingCenter.IncentiveKey memory key)
    {
        return
            IFarmingCenter.IncentiveKey({
                rewardToken: address(dQuick),
                bonusRewardToken: address(qi),
                pool: address(pool),
                startTime: 1665193197,
                endTime: 4104559500
            });
    }

    function _invest(address token_, uint256 amount_)
        internal
        override
        returns (uint256)
    {
        require(token_ == address(usdt) || token_ == address(mai), 'ns');
        emit ClaimReward(_claimAndRebalance(portfolioManager, 9990));
        if (token_ == address(usdt)) {
            return amount_;
        }
        if (token_ == address(mai)) {
            return _maiToUsdt(amount_, currentSqrtPrice());
        }
        return 0;
    }

    function rebalance() public {
        emit ClaimReward(_claimAndRebalance(portfolioManager, 9990));
    }

    function _rebalance(uint256 slippage_)
        internal
        returns (uint256 liquidity_)
    {
        Delta memory delta = getDelta();
        if (delta.tickUpdated) {
            (int24 tickLower, int24 tickUpper, uint128 liq, , ) = lpPos();
            _removeLiquidity(liq);
            emit Rebalance(
                tickLower,
                tickUpper,
                delta.tickLower,
                delta.tickUpper
            );
        }

        uint256 usdtAmount = usdt.balanceOf(address(this));
        uint256 maiAmount = mai.balanceOf(address(this));
        int256 price = int256(_usdtToMai(1e6, delta.currentSqrtP));
        console.log('[delta] usdt price: ');
        console.logInt(price);
        (uint256 maiRatio, uint256 usdtRatio) = LiquidityAmounts
            .getAmountsForLiquidity(
                delta.currentSqrtP,
                delta.lowerSqrtP,
                delta.upperSqrtP,
                1e18
            );
        console.log('[rebalance] liquidity ratio: %s %s', usdtRatio, maiRatio);
        // console.log('[rebalance] a: ');
        console.logInt(1e6 + (price * int256(usdtRatio)) / int256(maiRatio));
        int256 deltaMai = int256(
            ((price *
                (int256(usdtAmount) -
                    int256(Math.mulDiv(maiAmount, usdtRatio, maiRatio)))) /
                (1e6 + (price * int256(usdtRatio)) / int256(maiRatio)))
        );
        console.log('[rebalance] delta mai:');
        console.logInt(deltaMai);

        uint256 amountIn = 0;
        if (deltaMai < -1e5) {
            // mai -> usdt
            amountIn = uint256(-deltaMai);
            mai.approve(address(swapHelper), amountIn);
            swapHelper.swapMaiToUsdt(
                amountIn,
                Math.mulDiv(
                    _maiToUsdt(amountIn, delta.currentSqrtP),
                    slippage_,
                    10000
                )
            );
        } else if (deltaMai >= 1e5) {
            // usdt -> mai
            amountIn = _maiToUsdt(uint256(deltaMai), delta.currentSqrtP);
            console.log('amount in: %s', amountIn);
            usdt.approve(address(swapHelper), amountIn);
            swapHelper.swapUsdtToMai(
                amountIn,
                Math.mulDiv(amountIn, slippage_, 10000)
            );
        }

        usdtAmount = usdt.balanceOf(address(this));
        maiAmount = mai.balanceOf(address(this));
        if (usdtAmount > 1e5 && maiAmount > 1e17) {
            if (nftId == 0) {
                // mint new nft
                liquidity_ = _mintNft(delta);
            } else {
                // add to current liquidity
                console.log(
                    '[rebalance] add liquidity, usdt: %s mai: %s',
                    usdtAmount,
                    maiAmount
                );
                mai.approve(address(positionManager), maiAmount);
                usdt.approve(address(positionManager), usdtAmount);
                INonfungiblePositionManager.IncreaseLiquidityParams
                    memory params = INonfungiblePositionManager
                        .IncreaseLiquidityParams({
                            tokenId: nftId,
                            amount0Desired: maiAmount,
                            amount1Desired: usdtAmount,
                            amount0Min: Math.mulDiv(maiAmount, 9000, 10000),
                            amount1Min: Math.mulDiv(usdtAmount, 9000, 10000),
                            deadline: block.timestamp
                        });
                (liquidity_, maiAmount, usdtAmount) = positionManager
                    .increaseLiquidity(params);
                farmingCenter.exitFarming(_incentiveKey(), nftId, false);
                farmingCenter.withdrawToken(nftId, address(this), '');
                _enterFarming();
                console.log(
                    '[rebalance] added liquidity: %s usdt: %s mai: %s',
                    liquidity_,
                    usdtAmount,
                    maiAmount
                );
            }
        }
    }

    function _removeLiquidity(uint128 liquidity_)
        internal
        returns (uint256 maiAmount_, uint256 usdtAmount_)
    {
        farmingCenter.exitFarming(_incentiveKey(), nftId, false);
        farmingCenter.withdrawToken(nftId, address(this), '');
        (uint256 amount0, uint256 amount1) = _lpUnderlyingAmounts(liquidity_);
        INonfungiblePositionManager.DecreaseLiquidityParams
            memory params = INonfungiblePositionManager
                .DecreaseLiquidityParams({
                    tokenId: nftId,
                    liquidity: liquidity_,
                    amount0Min: (amount0 * 9900) / 10000,
                    amount1Min: (amount1 * 9900) / 10000,
                    deadline: block.timestamp
                });
        (maiAmount_, usdtAmount_) = positionManager.decreaseLiquidity(params);
        console.log(
            '[rebalance] remove liquidity: %s, %s',
            usdtAmount_,
            maiAmount_
        );
        (maiAmount_, usdtAmount_, , ) = _collectRewards();
        (, , uint128 liq, , ) = lpPos();
        if (liq == 0) {
            positionManager.burn(nftId);
            nftId = 0;
        }
    }

    function _mintNft(Delta memory delta_)
        internal
        returns (uint128 liquidity)
    {
        uint256 usdtAmount = usdt.balanceOf(address(this));
        uint256 maiAmount = mai.balanceOf(address(this));
        console.log('[rebalance] mint nft:  %s, %s', usdtAmount, maiAmount);
        usdt.approve(address(positionManager), usdtAmount);
        mai.approve(address(positionManager), maiAmount);
        INonfungiblePositionManager.MintParams
            memory params = INonfungiblePositionManager.MintParams({
                token0: address(mai),
                token1: address(usdt),
                tickLower: delta_.tickLower,
                tickUpper: delta_.tickUpper,
                amount0Desired: maiAmount,
                amount1Desired: usdtAmount,
                amount0Min: Math.mulDiv(maiAmount, 9000, 10000),
                amount1Min: Math.mulDiv(usdtAmount, 9000, 10000),
                recipient: address(this),
                deadline: block.timestamp
            });
        (nftId, liquidity, usdtAmount, maiAmount) = positionManager.mint(
            params
        );
        _enterFarming();
        console.log(
            '[rebalance] mint nft: %s, %s, %s',
            nftId,
            maiAmount,
            usdtAmount
        );
    }

    function _enterFarming() internal {
        positionManager.safeTransferFrom(
            address(this),
            address(farmingCenter),
            nftId,
            ''
        );
        farmingCenter.enterFarming(_incentiveKey(), nftId, 0, false);
    }

    function getDelta() public view returns (Delta memory delta) {
        (delta.currentSqrtP, delta.currentTick, , , , , ) = pool.globalState();
        (delta.tickLower, delta.tickUpper, , , ) = lpPos();
        if (delta.tickLower != tickLower || delta.tickUpper != tickUpper) {
            delta.tickLower = tickLower;
            delta.tickUpper = tickUpper;
            delta.tickUpdated = true;
        }
        delta.lowerSqrtP = TickMath.getSqrtRatioAtTick(delta.tickLower);
        delta.upperSqrtP = TickMath.getSqrtRatioAtTick(delta.tickUpper);
        console.log('[delta] current tick range:');
        console.logInt(delta.currentTick);
        console.logInt(delta.tickLower);
        console.logInt(delta.tickUpper);
    }

    function netAssetValue() public view virtual override returns (uint256) {
        UnderlyingAssets memory assets = underlyingAssets();
        return _netAssetValue(assets);
    }

    function _netAssetValue(UnderlyingAssets memory assets)
        internal
        pure
        returns (uint256)
    {
        return
            assets.usdtBalance +
            assets.maiBalanceUsdt +
            assets.usdtInLp +
            assets.maiInLpUsdt +
            assets.usdtFee +
            assets.maiFeeUsdt;
    }

    function underlyingAssets()
        public
        view
        returns (UnderlyingAssets memory assets)
    {
        uint160 currentSqrtP = currentSqrtPrice();
        assets.usdtBalance = usdt.balanceOf(address(this));
        assets.maiBalance = mai.balanceOf(address(this));
        assets.maiBalanceUsdt = _maiToUsdt(assets.maiBalance, currentSqrtP);
        (, , uint128 liquidity, , ) = lpPos();
        (assets.maiInLp, assets.usdtInLp) = _lpUnderlyingAmounts(liquidity);
        assets.maiInLpUsdt = _maiToUsdt(assets.maiInLp, currentSqrtP);
        console.log('[ASSETS] usdt in Lp: %s', assets.usdtInLp);
        console.log('[ASSETS] mai in Lp: %s', assets.maiInLp);
        console.log('[ASSETS] mai $ in Lp: %s', assets.maiInLpUsdt);
        console.log('[ASSETS] usdt balance: %s', assets.usdtBalance);
        console.log('[ASSETS] mai balance: %s', assets.maiBalance);
        console.log('[ASSETS] mai $ in contract: %s', assets.maiBalanceUsdt);
    }

    function _lpUnderlyingAmounts(uint128 liquidity_)
        internal
        view
        returns (uint256 amount0_, uint256 amount1_)
    {
        uint160 currentSqrtP = currentSqrtPrice();
        (int24 tl, int24 tu, , , ) = lpPos();
        (amount0_, amount1_) = LiquidityAmounts.getAmountsForLiquidity(
            currentSqrtP,
            TickMath.getSqrtRatioAtTick(tl),
            TickMath.getSqrtRatioAtTick(tu),
            liquidity_
        );
    }

    function lpPos()
        public
        view
        returns (
            int24 tickLower_,
            int24 tickUpper_,
            uint128 liquidity_,
            uint128 tokensOwed0_,
            uint128 tokensOwed1_
        )
    {
        if (nftId > 0)
            (
                ,
                ,
                ,
                ,
                tickLower_,
                tickUpper_,
                liquidity_,
                ,
                ,
                tokensOwed0_,
                tokensOwed1_
            ) = positionManager.positions(nftId);
    }

    function currentSqrtPrice() public view returns (uint160 currentSqrtP) {
        (currentSqrtP, , , , , , ) = pool.globalState();
    }

    function _maiToUsdt(uint256 maiAmount_, uint256 sqrtPrice_)
        internal
        pure
        returns (uint256)
    {
        return (maiAmount_ * sqrtPrice_) / (FixedPoint192.Q192 / sqrtPrice_);
    }

    function _usdtToMai(uint256 usdtAmount_, uint256 sqrtPrice_)
        internal
        pure
        returns (uint256)
    {
        return (usdtAmount_ * (FixedPoint192.Q192 / sqrtPrice_)) / sqrtPrice_;
    }

    function _withdraw(address, uint256) internal override returns (uint256) {
        revert();
    }
}

