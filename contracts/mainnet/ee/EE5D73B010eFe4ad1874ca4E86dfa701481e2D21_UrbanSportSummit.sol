abstract contract ERC1155Tradable is ERC1155, Ownable, Split {
    using ECDSA for bytes32;

    address public royaltyReceiver; // This contract
    address public creator; // Brand
    address public eShot;
    mapping (uint256 => uint256) public tokenSupply;
    mapping (uint256 => uint256) public tokenMaxSupply;
    mapping (uint256 => string) public metadataHash; //SHA256
    struct Supply {
        mapping (uint256 => bool) supplies;
    }
    mapping (uint256 => Supply) supplyIDs;
    // Contract name
    string public name;
    // Contract symbol
    string public symbol;
    uint16 public royaltyBasisPointsSecondary; // 225 == 2.25% of royalties
    uint16 royaltyBasisPointsEshotSecondary;
    uint16 royaltyBasisPointsEshotPrimary;
    uint256 primarySales;
    
    event mintEmit(address mintedTo, uint256 tokenID, uint256[] supplyID);

    constructor(
      address _eShot,
      address _creator,
      string memory _name,
      string memory _symbol,
      string memory _uri,
      uint16 _royaltyBasisPointsSecondary,
      uint16 _royaltyEshotSecondary,
      uint16 _royaltyEshotPrimary
      )
      ERC1155(_uri) {
      eShot = _eShot;
      creator = _creator;
      name = _name;
      symbol = _symbol;
      royaltyBasisPointsSecondary = _royaltyBasisPointsSecondary;
      royaltyBasisPointsEshotSecondary = _royaltyEshotSecondary;
      royaltyBasisPointsEshotPrimary = _royaltyEshotPrimary;
      royaltyReceiver = address(this);
    }

    /**
     * @dev Set a max supply and a hash to the given id token
     * @param _id the ID of the token
     * @param _maxSupply The maximum amount of supplies allowed
     * @param _metadataHash The hash of the metadata for the token
     */
    function setToken(uint256 _id, uint256 _maxSupply, string calldata _metadataHash) external onlyOwner{
      require(tokenSupply[_id] == 0, "token already exists");
      tokenMaxSupply[_id] = _maxSupply;
      metadataHash[_id] = _metadataHash;
    }

    /**
     * @dev Returns true if the interface sent is supported
     * @param _interfaceId the ID of the interface to compare
     */
    function supportsInterface(bytes4 _interfaceId) public view virtual override returns (bool) {
        return
            _interfaceId == type(IERC2981).interfaceId ||
            _interfaceId == type(IERC1155).interfaceId ||
            _interfaceId == type(IERC1155MetadataURI).interfaceId ||
            super.supportsInterface(_interfaceId);
    }

    /**
     * @dev Updates the royalties reciever and basis points
     * @param _royaltyReceiver Is the address of the new royalty reciever
     * @param _royaltyBasisPoints Is the "percentage" of royalty desired
     */
    function updateRoyaltyInfo(address _royaltyReceiver, uint16 _royaltyBasisPoints) external onlyOwner {
        royaltyReceiver = _royaltyReceiver;
        royaltyBasisPointsSecondary = _royaltyBasisPoints;
    }

    /**
     * @dev Gives information on how much to send to the receivers address
     * @param _tokenId Is mandatory parameter but not used
     * @param _price Is the price of the token
     */
    function royaltyInfo(uint256 _tokenId, uint256 _price) external view returns (address receiver, uint256 royaltyAmount) {
        _tokenId = 0;
        receiver = royaltyReceiver;
        royaltyAmount = getPercentageOf(_price, royaltyBasisPointsSecondary);
    }

    /**
      * @dev Returns the total quantity minted for a token ID
      * @param _id uint256 ID of the token to query
      * @return amount of token in existence
      */
     function totalSupply(uint256 _id) external view returns (uint256) {
       return tokenSupply[_id];
     }

    function uri(uint256 _id) override public view returns (string memory) {
      require(_exists(_id), "ERC1155Tradable#uri: NONEXISTENT_TOKEN");
      return string(abi.encodePacked(super.uri(_id), metadataHash[_id]));
    }


    /**
     * @dev Mints a new token with a number of supply
     * @param _id ID of the token to mint
     * @param _supply Number of supplies available
     */
    function factoMint(uint256 _id, uint256 _supply, uint256[] calldata _supplyID) internal {
      require(tokenSupply[_id] + _supply <= tokenMaxSupply[_id], "mint exceeds max supply");
      require(_supplyID.length == _supply, "Bad supply");
      for (uint256 i = 0; i < _supplyID.length; i++) {
        require(!supplyIDs[_id].supplies[_supplyID[i]], "Already minted");
        supplyIDs[_id].supplies[_supplyID[i]] = true;
      }
      _mint(msg.sender, _id, _supply, "");
      tokenSupply[_id] += _supply;
      emit mintEmit(msg.sender, _id, _supplyID);
    }

    /**
     * @dev Mints a new token with a number of supply
     * @param _id ID of the token to mint
     * @param _supply Number of supplies available
     */
    function mint(uint256 _id, uint256 _supply, uint256[] calldata _supplyID) public onlyOwner {
      factoMint(_id, _supply, _supplyID);
    }

    /**
     * @dev Mints a new token with a number of supply
     * @param _ids ID of the token to mint
     * @param _supply Number of supplies available
     */
    function mintBatch(uint256[] memory _ids, uint256[] calldata _supply, uint256[][] calldata _supplyIDs) external onlyOwner {
      for (uint256 i = 0; i < _ids.length; i++) {
        require(_supplyIDs[i].length == _supply[i], "Bad supply");
        require(tokenSupply[_ids[i]] + _supply[i] <= tokenMaxSupply[_ids[i]], "mint exceeds max supply");
        for (uint256 j = 0; j < _supplyIDs[i].length; j++) {
          require(!supplyIDs[_ids[i]].supplies[_supplyIDs[i][j]], "Already minted");
          supplyIDs[_ids[i]].supplies[_supplyIDs[i][j]] = true;
        }
        tokenSupply[_ids[i]] += _supply[i];
      }
      _mintBatch(msg.sender, _ids, _supply, "");
    }

    /**
    * @dev Authorizes by signature a wallet to transfer a token supply from the contract owner to himself.
    * @param _amountDue is the amount required for the transaction
    * @param _id is the ID of the token
    * hon hon oui oui baguette
    * @param _supply is the number of supplies to transfer
    * @param _signature is a signed message by the owner of the contract
    */
    function lazyTransfer(uint256 _amountDue, uint256 _id, uint256 _supply, bytes calldata _signature) external payable {
      require(msg.value == _amountDue, "Incorrect amount.");
      require(creator == ECDSA.toEthSignedMessageHash(abi.encodePacked(address(this), _id, _supply, _amountDue, msg.sender)).recover(_signature), "Signer address mismatch.");
      require(tokenSupply[_id] + _supply <= tokenMaxSupply[_id], "mint exceeds max supply");
      _safeTransferFrom(creator, msg.sender, _id, _supply, "");
      sale();
    }

    /**
    * @dev Authorizes by signature a wallet to mint a token supply to himself.
    * @param _amountDue is the amount required for the transaction
    * @param _id is the ID of the token
    * @param _supply is the number of supplies to mint
    * @param _signature is a signed message by the owner of the contract
    */
    function lazyMint(uint256 _amountDue, uint256 _id, uint256 _supply, bytes calldata _signature, uint256[] calldata _supplyID) external payable {
      require(msg.value == _amountDue, "Incorrect amount.");
      require(creator == ECDSA.toEthSignedMessageHash(abi.encodePacked(address(this), _id, _supplyID, _amountDue, msg.sender)).recover(_signature), "Signer address mismatch.");
      require(tokenSupply[_id] + _supply <= tokenMaxSupply[_id], "mint exceeds max supply");
      factoMint(_id, _supply, _supplyID);
      sale();
    }

    /**
     * @dev Registers the amount sent for a secondary sale
     */
    receive() external payable {
    }

    /**
     * @dev Registers the amount sent for a primary sale
     */
    function sale() public payable {
      primarySales += msg.value;
    }

    /**
    * @dev Sends back the money from the contract to the owner of the contract and the creator
    */
    function withdraw() public onlyOwner{
      uint256 totalPrimary = primarySales;
      primarySales = 0; // Prevent reentry
      splitIt(totalPrimary, royaltyBasisPointsEshotPrimary, royaltyBasisPointsEshotSecondary, creator, eShot);
    }

    /**
    * @dev Returns whether the specified token exists by checking to see if it has a creator
    * @param _id uint256 ID of the token to query the existence of
    * @return bool whether the token exists
    */
    function _exists(uint256 _id) internal view returns (bool) {
      return tokenSupply[_id] != 0;
    }
}

