abstract contract Split {

    /**
     * @notice Split an amount with the roalties given.
     * IF PARAM _amountPrimary IS PRIMARY SALE RESET VALUE AFTER CALL TO THIS FUNCTION
     * @param _amountPrimary - the amount of primary sales
     * @param _sharePrimary - the share of primary sales to split with
     * @param _royaltySecondary - the royalty of secondary sales to split with
     * @param _creator - the creator of the nft address to send the crypto to
     * @param _eShot - the address of eShot to send crypto to
     */
    function splitIt(uint256 _amountPrimary, uint16 _sharePrimary, uint16 _royaltySecondary, address _creator, address _eShot) internal {
      require (_amountPrimary <= address(this).balance);
      uint256 eShotShare;
      eShotShare = getPercentageOf(_amountPrimary, _sharePrimary);
      eShotShare += getPercentageOf(address(this).balance - _amountPrimary, _royaltySecondary);
      require (eShotShare <= address(this).balance);
      payable(_creator).transfer((address(this).balance - eShotShare));
      payable(_eShot).transfer(eShotShare);
    }

    /**
     * @dev Sends back the amount due
     * @param _amount Is the price of the token
     * @param _basisPoints Is the "percentage" of royalty
     */
    function getPercentageOf(uint256 _amount, uint16 _basisPoints) internal pure returns (uint256 value) {
        value = (_amount * _basisPoints) / (100 * 100);
    }
}