pragma solidity ^0.8.18;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "./mason/utils/Administrable.sol";
import "operator-filter-registry/src/DefaultOperatorFilterer.sol";

contract FloorPack is DefaultOperatorFilterer, ERC1155, Administrable {
  string public name;
  string public symbol;

  constructor(
    string memory _name,
    string memory _symbol,
    string memory _uri
  ) ERC1155(_uri) {
    _setURI(_uri);
    name = _name;
    symbol = _symbol;
  }

  function setURI(string memory uri) public onlyOperatorsAndOwner() {
    _setURI(uri);
  }

  function airdropSingle(uint256 tokenId, address to) external onlyOperatorsAndOwner() {
    _mint(to, tokenId, 1, "");
  }

  function airdropBatch(uint256[] memory tokenIds, address to) external onlyOperatorsAndOwner() {
    uint256[] memory amounts = new uint256[](tokenIds.length);
    for (uint256 i = 0; i < tokenIds.length;) {
      amounts[i] = 1;
      unchecked {
        ++i;
      }
    }
    _mintBatch(to, tokenIds, amounts, "");
  }

  function supportsInterface(bytes4 interfaceId) public view virtual override(ERC1155, AccessControlEnumerable) returns (bool) {
    return super.supportsInterface(interfaceId);
  }

}

