        }
    }

    function mintExistingPlushies(address owner, uint256 id) internal {
        _mint(owner, id, 1, "");
        _breedCapacities[id] = _breedCapacity;
        _tokenTypes[id] = 1;
        _owners[id] = msg.sender;

        _isPlushie[id] = true;

        _og[id] = true;

        _tokenIds.increment();
        _plushieTotal.increment();
    }

    function getTokenId() public view returns (uint256) {
        return _tokenIds.current();
    }

    function _decrementPlushieBreedingCapacity(uint256 plushieId) private {
        require(_breedCapacities[plushieId] > 0, "This Plushie has no breeding capacity.");
        _breedCapacities[plushieId] -= 1;
    }

    function ascension(uint256 plushieId, address owner) public {
        require(_startAscension, "Ascension not released.");
        require(super.balanceOf(owner, plushieId) == 1, "Only owner may ascend a Plushie.");
        require(msg.sender == _breedingContract, "Only specified breeding contract may ascend a Plushie.");

        uint256 newItemId = _tokenIds.current();

        _mint(owner, newItemId, 1, "");
        _breedCapacities[newItemId] = _breedCapacity;
        _tokenTypes[newItemId] = 1;
        _owners[newItemId] = msg.sender;

        _isPlushie[newItemId] = true;

        _og[newItemId] = false;

        _tokenIds.increment();
        _plushieTotal.increment();

        _decrementPlushieBreedingCapacity(plushieId);
    }

    function mintPlushie(address recipient, uint256 amount)
    public
    payable
    returns (uint256)
    {
        require(!_mintStopped, "No more mint requests is allowed.");
        require(_totalPlushiesBought[msg.sender] + amount <= _maxMintPerAccount, "You're purchase exceeds the maximum amount of Mints per Account.");
        require(_plushieTotal.current() + amount - 1 < _totalPlushieSupply, "Request exceed total Plushie supply.");
        require(msg.value == _priorityList.plushiePrice(msg.sender) * amount, "Sent value must be exactly the mint cost.");

        for (uint256 i = 0; i < amount; i++) {
            uint256 newItemId = _tokenIds.current();

            _mint(recipient, newItemId, 1, "");
            _breedCapacities[newItemId] = _breedCapacity;
            _tokenTypes[newItemId] = 1;
            _owners[newItemId] = msg.sender;

            _isPlushie[newItemId] = true;
            _priorityList.boughtAPlushie(msg.sender);

            _og[newItemId] = true;

            _tokenIds.increment();
            _plushieTotal.increment();
        }

        _totalPlushiesBought[msg.sender] += amount;

        _beneficiary.transfer(msg.value);

        return 0;
    }

    function setNFTForSale(uint256 _tokenId, uint256 _price) public {
        require(_tokenIds.current() > _tokenId, "Token doesn`t exists.");
        require(_owners[_tokenId] == msg.sender, "Only the token owner can put it for sale.");
        require(!_saleData[_tokenId].forSale, "Token already on sale.");
        require(super.balanceOf(_owners[_tokenId], _tokenId) == 1, "Old owner does not own this token anymore.");

        _saleData[_tokenId].forSale = true;
        _saleData[_tokenId].price = _price;
        _saleData[_tokenId].seller = msg.sender;
    }

    function cancelNFTSale(uint256 _tokenId) public {
        require(_tokenIds.current() > _tokenId, "Token doesn`t exists.");
        require(_owners[_tokenId] == msg.sender, "Only the token owner can cancel the sale.");
        require(_saleData[_tokenId].forSale, "Token not on sale.");
        require(super.balanceOf(_owners[_tokenId], _tokenId) == 1, "Old owner does not own this token anymore.");

        _saleData[_tokenId].forSale = false;
        _saleData[_tokenId].price = 0;
    }

    function buyNFT(uint256 _tokenId) public payable {
        require(_saleData[_tokenId].forSale, "Token not on sale.");
        require(msg.value == _saleData[_tokenId].price, "Sent value must be exactly the buy price.");
        require(_owners[_tokenId] != msg.sender, "This is your token cancel the sale instead of buying.");
        require(super.balanceOf(_owners[_tokenId], _tokenId) == 1, "Old owner does not own this token anymore.");

        require(_saleData[_tokenId].seller == _owners[_tokenId], "Sale inputed by old seller.");

        // We bypass the owner verification since it was available for sale, we do that enable 
        // buy and sell operations with a single transaction.

        _safeTransferFrom(_owners[_tokenId], msg.sender, _tokenId, 1, "");

        payable(_owners[_tokenId]).transfer(msg.value);

        _owners[_tokenId] = msg.sender;
        _saleData[_tokenId].forSale = false;
        _saleData[_tokenId].price = 0;
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public virtual override {
        super.safeTransferFrom(from, to, id, amount, data);
        // If token was for sale and was sold on another marketplace, setting it to not for sale
        if (_saleData[id].forSale) {
            _saleData[id].forSale == false;
            _saleData[id].price = 0;
        }
        _owners[id] = to;
    }

    function uri(uint256 _tokenId) override public pure returns (string memory) {
        return string(
            abi.encodePacked(
                "https://storagenftsjson.blob.core.windows.net/jsonplushie/", Strings.toString(_tokenId), ".json")
        );
    }

    function stopMint() public onlyOwner{
        _mintStopped = true;
    }

    function restartMint() public onlyOwner {
        _mintStopped = false;
    }

    function getAllData() public view returns (TokenMetadata[] memory) {
        TokenMetadata[] memory tokenMetadata = new TokenMetadata[](_tokenIds.current());
        for (uint256 i = 0; i < _tokenIds.current(); i++) {
            tokenMetadata[i] = TokenMetadata({
                tokenId : i,
                tokenType : _tokenTypes[i],
                uri : uri(i),
                owner : _owners[i],
                contractAddress : address(this),
                forSale : _saleData[i].forSale,
                price : _saleData[i].price,
                og : _og[i],
                ascensionCount : _breedCapacities[i]
            });
        }

        return tokenMetadata;
    }

}

