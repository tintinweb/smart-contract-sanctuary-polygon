        return _contract_uri;
    }

    function _msgSender() internal view virtual override (Context, ERC2771Context) returns (address) {
        return ERC2771Context._msgSender();
    }

    function _msgData() internal view virtual override (Context, ERC2771Context) returns (bytes calldata) {
        return ERC2771Context._msgData();
    }

    //Overrides to store list of token ids
    function _mint(address to, uint256 id, uint256 amount, bytes memory data) internal virtual override {
        if (!_token_ids_exists[id])
        {
           _token_ids.push(id);
           _token_ids_exists[id] = true;
        }
        super._mint(to, id, amount, data);
    }

    function _mintBatch(address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data) internal virtual override{
        for (uint256 i = 0; i < ids.length; i++) {
            if (!_token_ids_exists[ids[i]])
            {
                _token_ids.push(ids[i]);
                _token_ids_exists[ids[i]] = true;
            }
        }
        super._mintBatch(to, ids, amounts, data);
    }

    //Return list of TokenBalance for an account
    function balanceOfAll(address account) public view returns (TokenBalance[] memory) {
        require(account != address(0), "address zero is not a valid owner");

        TokenBalance[] memory allBalances = new TokenBalance[](_token_ids.length);
        uint256 balanceCount = 0;
        for (uint256 i = 0; i < _token_ids.length; i++) {
            if (balanceOf(account, _token_ids[i]) > 0) {
                allBalances[balanceCount] = TokenBalance(_token_ids[i], balanceOf(account, _token_ids[i]));
                balanceCount++;
            }
        }

        TokenBalance[] memory trimmedResult = new TokenBalance[](balanceCount);
        for (uint256 i = 0; i < balanceCount; i++) {
            trimmedResult[i] = allBalances[i];
        }
        return trimmedResult;
    }

    struct NFTVoucher {
        uint256 voucherId;
        uint256 tokenId;
        uint256 amount;
        bytes   signature;
    }

    function redeem(address redeemer, NFTVoucher calldata voucher, bytes memory data) external returns (uint256) {
        require(!_vouchersUsed[voucher.voucherId], "Voucher already used");
        require (voucher.amount > 0, "Amount need to be > 0");
        _vouchersUsed[voucher.voucherId] = true;

        // make sure signature is valid and get the address of the signer
        address signer = _verify(voucher);

        // make sure that the signer is authorized to mint NFTs
        require(hasRole(MINTER_ROLE, signer), "Signature invalid or unauthorized");

        // mint to redeemer
        _mint(redeemer, voucher.tokenId, voucher.amount, data);

        return voucher.tokenId;
    }

    function redeemBatch(address redeemer, NFTVoucher[] calldata vouchers, bytes memory data) external {

        uint256[] memory ids = new uint256[](vouchers.length);
        uint256[] memory amounts = new uint256[](vouchers.length);

        for (uint256 i = 0; i < vouchers.length; i++) {
            NFTVoucher calldata voucher = vouchers[i];

            require(!_vouchersUsed[voucher.voucherId], "Voucher already used");
            require (voucher.amount > 0, "Amount need to be > 0");
            _vouchersUsed[voucher.voucherId] = true;

            // make sure signature is valid and get the address of the signer
            address signer = _verify(voucher);

            // make sure that the signer is authorized to mint NFTs
            require(hasRole(MINTER_ROLE, signer), "Signature invalid or unauthorized");

            ids[i] = voucher.tokenId;
            amounts[i] = voucher.amount;
        }

        // mint to redeemer
        _mintBatch(redeemer, ids, amounts, data);
    }

    /// @notice Returns a hash of the given NFTVoucher, prepared using EIP712 typed data hashing rules.
    /// @param voucher An NFTVoucher to hash.
    function _hash(NFTVoucher calldata voucher) internal view returns (bytes32) {
        return _hashTypedDataV4(keccak256(abi.encode(
            keccak256("NFTVoucher(uint256 voucherId,uint256 tokenId,uint256 amount)"),
            voucher.voucherId,
            voucher.tokenId,
            voucher.amount
        )));
    }

    /// @notice Returns the chain id of the current blockchain.
    /// @dev This is used to workaround an issue with ganache returning different values from the on-chain chainid() function and
    ///  the eth_chainId RPC method. See https://github.com/protocol/nft-website/issues/121 for context.
    function getChainID() external view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }

    /// @notice Verifies the signature for a given NFTVoucher, returning the address of the signer.
    /// @dev Will revert if the signature is invalid. Does not verify that the signer is authorized to mint NFTs.
    /// @param voucher An NFTVoucher describing an unminted NFT.
    function _verify(NFTVoucher calldata voucher) internal view returns (address) {
        bytes32 digest = _hash(voucher);
        return ECDSA.recover(digest, voucher.signature);
    }
}