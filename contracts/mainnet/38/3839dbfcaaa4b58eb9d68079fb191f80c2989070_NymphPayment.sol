pragma solidity ^0.8.17;

import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableMapUpgradeable.sol";
import "./Nymph.sol";

 struct NymphManagersData {
    mapping (address => EnumerableMapUpgradeable.AddressToUintMap) managers;

}


library NymphManagersLib {
    using SafeMathUpgradeable for uint;
    using EnumerableMapUpgradeable for EnumerableMapUpgradeable.AddressToUintMap; 

    function addManager(NymphManagersData storage self, address author, address addr, uint amount) internal returns (bool) {
        uint count  = self.managers[author].length();
        address[] memory addrs = new address[](count);
        uint[] memory percents = new uint[](count);  
        uint total = 0;
        for (uint i=0; i < count; i++) {
          (addrs[i],percents[i]) = self.managers[author].at(i);
          total = total.add(percents[i]);
       }  
        require(total<=100, "Maximum percents is high");
        self.managers[author].set(addr,amount);
        return true;
    }

    function delManager(NymphManagersData storage self, address author, address addr) internal returns (bool) {  
        require(self.managers[author].contains(addr), "not manager");
        self.managers[author].remove(addr);
        return true;
    }

    function getManager(NymphManagersData storage self, address author) internal view returns (address[] memory, uint[] memory) {
       uint count  = self.managers[author].length();
       address[] memory addrs = new address[](count);
       uint[] memory percents = new uint[](count);      

       for (uint i=0; i < count; i++) {
          (addrs[i],percents[i]) = self.managers[author].at(i);
       }
        return (addrs,percents);
    }
   
    function payManager(NymphManagersData storage self, address NYMCoin, address author, uint amount) internal returns (uint) {
        require(NYMCoin != address(0),"Contract NYM not set");
        uint remain   = amount;
        uint percent  = amount.div(100);
        uint count  = self.managers[author].length();
        address manager_addr;
        uint manager_percent = 0; 
        uint manager_amount = 0;     

        for (uint i = 0; i < count; i++) {
            (manager_addr,manager_percent) = self.managers[author].at(i);
            manager_amount = percent.mul(manager_percent);
            Nymph(NYMCoin).move(author,manager_addr,manager_amount);
            remain = remain.sub(manager_amount);
        }
        return remain;
    }
}

