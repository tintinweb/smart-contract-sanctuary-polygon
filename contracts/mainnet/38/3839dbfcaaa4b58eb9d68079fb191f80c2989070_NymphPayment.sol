pragma solidity ^0.8.17;

import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";

contract Nymph is Initializable, ERC20Upgradeable, AccessControlUpgradeable, UUPSUpgradeable {
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant CONTRACT_ROLE = keccak256("CONTRACT_ROLE");
    bytes32 public constant BURNER_ROLE = keccak256("BURNER_ROLE");
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
    
    event LogContractSet(address indexed newContract);
    event LogStableSet(address indexed stableContract);
    event LogStableDel(address indexed stableContract);
    event LogDepositMade(address indexed fromAddress, address indexed toAddress, address indexed stable, uint amount);
    event LogDepositWithdraw(address indexed fromAddress, address indexed toAddress, address indexed stable, uint amount);
    event LogTransfer(address indexed accountFrom, address indexed accountTo, uint amount);

    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;
    using SafeMathUpgradeable for uint;
    
    // contract for deposit and withdraw
    address private delegated;
   
    // list stableconins
    EnumerableSetUpgradeable.AddressSet private stables;

    // new in contract v 1.3 //
    uint private commissionRate;
    address private commissionHolder;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize() initializer public {
        __ERC20_init("Nymph Stablecoin", "NYM");
        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        _grantRole(BURNER_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);
    }

    function mint(address to, uint256 amount) public onlyRole(MINTER_ROLE) {
        _mint(to, amount);
    }

    function burn(address from, uint256 amount) public onlyRole(BURNER_ROLE) {
        _burn(from, amount);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        onlyRole(UPGRADER_ROLE)
        override
    {}

    function setContract(address newContract) public onlyRole(DEFAULT_ADMIN_ROLE) {  
         delegated = newContract;
         _grantRole(CONTRACT_ROLE, delegated);
         emit LogContractSet(newContract);
    }

    function getContract() public view returns (address) {
         return delegated;
    }

    function setStableCoin(address stable) public onlyRole(DEFAULT_ADMIN_ROLE) {
         if(EnumerableSetUpgradeable.contains(stables,stable)) {
           require(ERC20Upgradeable(stable).balanceOf(address(this)) > 0,"Stable alrady set and balance not 0");  
         }
        EnumerableSetUpgradeable.add(stables, stable);  
        emit LogStableSet(stable);
    }

    function delStableCoin(address stable) public onlyRole(DEFAULT_ADMIN_ROLE) {
         if(EnumerableSetUpgradeable.contains(stables,stable)) {
          require(ERC20Upgradeable(stable).balanceOf(address(this)) == 0,"Stable not removed, balance not 0");  
         }
         EnumerableSetUpgradeable.remove(stables, stable);         
         emit LogStableDel(stable);
    }


    function getStableCoin(address stable) public view returns (bool) {
         return EnumerableSetUpgradeable.contains(stables,stable);
    }

    function getStables() public view returns (address[] memory, uint[] memory, string[] memory, uint[] memory){
      uint len = EnumerableSetUpgradeable.length(stables);
      address[] memory addrs = new address[](len);
      uint[] memory bals = new uint[](len);      
      string[] memory names = new string[](len);
      uint[] memory decimals = new uint[](len);
      for (uint i=0; i < len; i++) {
         addrs[i] = EnumerableSetUpgradeable.at(stables,i);
         bals[i] = ERC20Upgradeable(addrs[i]).balanceOf(address(this));
         names[i] = ERC20Upgradeable(addrs[i]).symbol();
         decimals[i] = ERC20Upgradeable(addrs[i]).decimals();

    }
       return (addrs,bals,names,decimals);
    }

    function getStablesBalance(address stable) public view returns (uint) {
         return ERC20Upgradeable(stable).balanceOf(address(this));
    }
   
   // in stable decimals
    function deposit(address stable, uint256 amount) public returns (bool) { 
        require(amount >= 10000, "amount to small for deposit");
        require(EnumerableSetUpgradeable.contains(stables,stable),"no stable set for deposit"); 
        ERC20Upgradeable(stable).transferFrom(address(msg.sender), address(this), amount);
        uint under = 0;
        if (ERC20Upgradeable(stable).decimals() < this.decimals()) {
            under = this.decimals()-ERC20Upgradeable(stable).decimals();
        }
        amount = amount.mul(10 ** under);
        _mint(address(msg.sender), amount);
        if (commissionRate > 0) {
            uint commission = amount.div(100);
            commission = commission.mul(commissionRate).div(100);
          // send commission  
           _transfer(address(msg.sender), commissionHolder, commission);
           amount = amount.sub(commission);
        }  
        
        emit LogDepositMade(address(msg.sender), address(this), stable, amount);
        return true;
    }
    
    // in nym decimals
    function withdraw(address stable, uint256 amount) public returns (bool){
        require(EnumerableSetUpgradeable.contains(stables,stable),"no stable set for withdraw"); 
        _burn(address(msg.sender), amount);
        uint under = 0;
        if (ERC20Upgradeable(stable).decimals() < this.decimals()) {
            under = this.decimals()-ERC20Upgradeable(stable).decimals();
        }
        amount = amount.div((10 ** under));
        ERC20Upgradeable(stable).transfer(address(msg.sender), amount); 
        emit LogDepositWithdraw( address(this), address(msg.sender), stable, amount);  
        return true;
    }

    function depositTo(address user, address stable, uint256 amount) public onlyRole(CONTRACT_ROLE) returns (bool) {
        require(amount >= 10000, "amount to small for deposit");
        require(EnumerableSetUpgradeable.contains(stables,stable),"no stable set for depositTo"); 
        ERC20Upgradeable(stable).transferFrom(address(msg.sender), address(this), amount);
        uint under = 0;
        if (ERC20Upgradeable(stable).decimals() < this.decimals()) {
            under = this.decimals()-ERC20Upgradeable(stable).decimals();
        }
        amount = amount.mul(10 ** under);
        _mint(user, amount);
        if (commissionRate > 0) {
            uint commission = amount.div(100);
            commission = commission.mul(commissionRate).div(100);
            // send commission  
            _transfer(address(msg.sender), commissionHolder, commission);
             amount = amount.sub(commission);
        }
        emit LogDepositMade(user, address(this), stable, amount);
        return true;
    }

    function withdrawTo(address user, address stable, uint256 amount) public onlyRole(CONTRACT_ROLE) returns (bool) {
        require(EnumerableSetUpgradeable.contains(stables,stable),"no stable set for withdrawTo"); 
        _burn(user, amount);
        uint under = 0;
        if (ERC20Upgradeable(stable).decimals() < this.decimals()) {
            under = this.decimals()-ERC20Upgradeable(stable).decimals();
        }
        amount = amount.div((10 ** under));
        ERC20Upgradeable(stable).transfer(user, amount);  
        emit LogDepositWithdraw(address(this), user, stable, amount); 
        return true;
    }

    function move(address from, address to, uint256 amount) public onlyRole(CONTRACT_ROLE) {
       _transfer(from, to, amount);
       emit LogTransfer(from, to, amount);
