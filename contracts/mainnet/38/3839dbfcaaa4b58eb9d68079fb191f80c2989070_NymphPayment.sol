pragma solidity ^0.8.17;

import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableMapUpgradeable.sol";
import "./Nymph.sol";
import "./NymphManagersLib.sol";

contract NymphPayment is Initializable, AccessControlUpgradeable, UUPSUpgradeable {
    using SafeMathUpgradeable for uint;
    using EnumerableMapUpgradeable for EnumerableMapUpgradeable.AddressToUintMap;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;

    bytes32 public constant CONTRACT_ROLE = keccak256("CONTRACT_ROLE");
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
  
    address private owner;
    address private NYM;
    address private Swapper;

    mapping (address => mapping(address => uint)) private deposites;
    mapping (address => address) private referer;
    mapping (address => bool) private referer_set;

    mapping (address => uint) private platform_percent;
    
    mapping (address => address) private nft;
    mapping (address => bool) private  nft_set;
    mapping (address => uint) private nft_percent;

    // new in contract v 1.2 //
    uint private commissionRate;
    address private commissionHolder;
    // storage for library
    NymphManagersData managers;
   // new in contract v 1.3 //
    mapping (address => EnumerableSetUpgradeable.AddressSet) private referals;

    // event for EVM logging
    event OwnerSet(address indexed oldOwner, address indexed newOwner);
    event LogDepositMade(address indexed accountAddress, uint amount);
    event LogDepositWithdraw(address indexed accountAddress, uint amount);
    event LogPayment(address indexed accountFrom, address indexed accountTo, uint amount);
    event LogTransfer(address indexed accountFrom, address indexed accountTo, uint amount);
    event LogAllocate(address indexed accountFrom, address indexed accountTo, uint amount);
    event LogRelease(address indexed accountFrom, address indexed accountTo, uint amount);
    event ContractSet(address indexed oldContract,address indexed newContract);

/// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize() initializer public {
        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);
        owner = msg.sender; 
        emit OwnerSet(address(0), owner);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        onlyRole(UPGRADER_ROLE)
        override
    {}

    function changeOwner(address newOwner) public {
        require(msg.sender == owner,"Only owner can change owner");
        owner = newOwner;
        emit OwnerSet(owner, newOwner);
       
    }
 
    function getOwner() external view returns (address) {
        return owner;
    }

    function changeContract(address newContract) public onlyRole(DEFAULT_ADMIN_ROLE) {
        address _NYM = NYM;
        NYM = newContract;
        emit ContractSet(_NYM,newContract);      
    }
 
    function getContract() external view returns (address) {
        return NYM;
    }

    function deposit(address stablecoin, uint amount) public {
       // address user = msg.sender;
        require(NYM!=address(0),"Contract NYM not set");
        require(Nymph(NYM).getStableCoin(stablecoin),"NYM/Error stable not exists");
        require(ERC20Upgradeable(stablecoin).transferFrom(msg.sender, address(this), amount),"Stablecoin/Error transfer");
 
        require(ERC20Upgradeable(stablecoin).approve(NYM,type(uint256).max),"NYM/Error approve");
        require(Nymph(NYM).depositTo(msg.sender, stablecoin, amount),"NYM/Error deposite");
        emit LogDepositMade(msg.sender, amount);
    }

    function depositOwner(address user, address stablecoin, uint amount) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(NYM!=address(0),"Contract NYM not set");
        require(Nymph(NYM).getStableCoin(stablecoin),"NYM/Error stable not exists");
        require(ERC20Upgradeable(stablecoin).transferFrom(user, address(this), amount),"Stablecoin/Error transfer");
  
        ERC20Upgradeable(stablecoin).approve(NYM,type(uint256).max);
        require(Nymph(NYM).depositTo(user, stablecoin, amount),"NYM/Error deposite");
        emit LogDepositMade(user, amount);
    }


    function withdraw(address stablecoin,uint amount) public {
     //   address user = msg.sender;
        require(NYM!=address(0),"Contract NYM not set");
        require(Nymph(NYM).getStableCoin(stablecoin),"NYM/Error stable not exists");
        require(amount <= _balanceOf(msg.sender),"not enough funds for withdraw");

        require(Nymph(NYM).withdrawTo(msg.sender,stablecoin, amount),"NYM/Error transfer");
        emit LogDepositWithdraw(msg.sender, amount);
    }

    function withdrawOwner(address user, address stablecoin,uint amount) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(NYM!=address(0),"Contract NYM not set");
        require(Nymph(NYM).getStableCoin(stablecoin),"NYM/Error stable not exists");
        require(amount <= _balanceOf(user),"not enough funds for withdraw");

        require(Nymph(NYM).withdrawTo(user,stablecoin, amount),"NYM/Error transfer");
        emit LogDepositWithdraw(user, amount);
    }

    function getBalance() public view returns (uint remainingBal) {
          require(NYM!=address(0),"Contract NYM not set");
          return _balanceOf(msg.sender);
    }

    function userBalance(address addr) public onlyRole(DEFAULT_ADMIN_ROLE) view returns (uint256) {
          require(NYM!=address(0),"Contract NYM not set");
          return _balanceOf(addr);
    }

    function totalBalance() public onlyRole(DEFAULT_ADMIN_ROLE) view returns (uint256) {
        require(NYM!=address(0),"Contract NYM not set");
        return Nymph(NYM).totalSupply();
    }

    // wrappers
     function _balanceOf(address addr) internal virtual view returns (uint256) {
         return Nymph(NYM).balanceOf(addr);
    } 

    function _move(address fromAddr, address toAddr, uint amount) internal virtual {
         Nymph(NYM).move(fromAddr,toAddr,amount);
    }    

    function _pay_to_referers(address fromAddr, address toAddr, uint amount) internal virtual returns (uint) {
       // address my        = address(this);
        uint percent      = amount.div(100);
        uint p_ref_author = percent.mul(25);
        uint p_ref_user   = percent.mul(5);
        uint p_ref_sub    = percent.mul(125).div(100);
        uint total        = 0;
      
      // pay to user referers
      if (referer_set[fromAddr]) {
            address ref_u = referer[fromAddr];
            _move(address(this),ref_u,p_ref_user);
            total = total.add(p_ref_user);
            if (referer_set[ref_u]) {
               address ref_us = referer[ref_u];
               _move(address(this),ref_us,p_ref_sub);
               total = total.add(p_ref_sub);
            }
        }
        // pay author referers
        if (referer_set[toAddr]) {
            address ref_a = referer[toAddr];
            _move(address(this),ref_a,p_ref_author);
            total = total.add(p_ref_author);
            if (referer_set[ref_a]) {
               address ref_as = referer[ref_a];
               _move(address(this),ref_as,p_ref_sub);
               total = total.add(p_ref_sub);
            }
        }
        return total;
    }

    function _pay(address fromAddr, address toAddr, uint amount) internal virtual {
        require(amount >= 10000, "amount to small for pay");
      //  address my = address(this);
        uint percent      = amount.div(100);
        uint discount     = 25;
             discount     = discount.sub(platform_percent[toAddr]);
        uint for_platform = percent.mul(discount);
        uint for_author   = amount.sub(for_platform);
 
   
/*      if (commissionRate > 0) {
          // send commission  
          _move(fromAddr,commissionHolder,commissionRate);
        }
        // reserve sum 
        _move(fromAddr,address(this),amount);
 */       
        // pay to referers
        if (for_platform > 0 ) {
            uint total = _pay_to_referers(fromAddr, toAddr, for_platform);
            for_platform = for_platform.sub(total);
        }
        // discount
       /* if (for_platform >= discount )
        {
            for_platform = for_platform.sub(discount);
            for_author = for_author.add(discount);
        } else {
            for_author = for_author.add(for_platform);
            for_platform = 0;
        }
        */
       // send to author
       _move(address(this),toAddr,for_author);
       // send to managers
       NymphManagersLib.payManager(managers, NYM, toAddr, for_author);
       if (for_platform > 0 ) {
          _move(address(this),owner,for_platform);
       } 
       emit LogPayment(fromAddr, toAddr, amount); 
    }    

    function pay(address fromAddr, address toAddr, uint amount) public onlyRole(DEFAULT_ADMIN_ROLE) returns (uint remainingBal) {
        require((amount+commissionRate) <= _balanceOf(fromAddr), "not enough funds for pay");
        if (commissionRate > 0) {
          // send commission  
          _move(fromAddr,commissionHolder,commissionRate);
        }
        // reserve sum 
        _move(fromAddr,address(this),amount);
        _pay(fromAddr,toAddr,amount);
        return _balanceOf(fromAddr); 
    }

    function transfer(address fromAddr, address toAddr, uint amount) public onlyRole(DEFAULT_ADMIN_ROLE) returns (uint remainingBal) {
        require((amount+commissionRate) <= _balanceOf(fromAddr), "not enough funds for transfer");
        if (commissionRate > 0) {
          // send commission  
          _move(fromAddr,commissionHolder,commissionRate);
        }
        _move(fromAddr,toAddr,amount);
        emit LogTransfer(fromAddr, toAddr, amount);
        return _balanceOf(fromAddr); 
    }
  
    function allocate(address fromAddr, address toAddr, uint amount) public onlyRole(DEFAULT_ADMIN_ROLE) returns (uint remainingBal) {
        require((amount+commissionRate) <= _balanceOf(fromAddr), "not enough funds for allocate");
       // reserve allocate sum on contract
       // address my = address(this);
        if (commissionRate > 0) {
          // send commission  
          _move(fromAddr,commissionHolder,commissionRate);
        }
        // reserve sum 
        _move(fromAddr,address(this),amount+commissionRate);
        deposites[fromAddr][toAddr] = deposites[fromAddr][toAddr].add(amount+commissionRate);
        emit LogAllocate(fromAddr, toAddr, amount+commissionRate);
        return _balanceOf(fromAddr); 
    } 
    

    function release(address fromAddr, address toAddr, uint amount) public onlyRole(DEFAULT_ADMIN_ROLE) returns (uint remainingBal) {
       require((amount+commissionRate) <= deposites[fromAddr][toAddr] , "not enough funds for release");
       deposites[fromAddr][toAddr] = deposites[fromAddr][toAddr].sub(amount+commissionRate);  
       if (commissionRate > 0) {
          // send commission  
          _move(address(this),commissionHolder,commissionRate);
        }
       _pay(fromAddr,toAddr,amount);
       uint ret = deposites[fromAddr][toAddr];
     //  address my = address(this);
       deposites[fromAddr][toAddr] = 0;
       _move(address(this),fromAddr,ret);     
       return _balanceOf(fromAddr);  
    }  

    function refund(address fromAddr, address toAddr) public onlyRole(DEFAULT_ADMIN_ROLE) returns (uint remainingBal) {
       require((commissionRate) <= deposites[fromAddr][toAddr] , "not enough funds for commision");
       deposites[fromAddr][toAddr] = deposites[fromAddr][toAddr].sub(commissionRate);   
        if (commissionRate > 0) {
          // send commission  
          _move(address(this),commissionHolder,commissionRate);
        }    
       uint ret = deposites[fromAddr][toAddr];
       deposites[fromAddr][toAddr] = 0;
       _move(address(this),fromAddr,ret);     
       return _balanceOf(fromAddr);  
    }     

    function setReferer(address addr, address ref) public onlyRole(DEFAULT_ADMIN_ROLE) returns (address) {
        require(addr != address(0) , "addr not set");
        require(ref != address(0) , "ref not set");
        referer[addr] = ref;
        referer_set[addr] = true;
        referals[ref].add(addr);
        return referer[addr];
    }

    function getReferer(address addr) public onlyRole(DEFAULT_ADMIN_ROLE) view returns (address ref) {
        return referer[addr];
    }


    function isReferer(address addr) public onlyRole(DEFAULT_ADMIN_ROLE) view returns (bool ref) {
        return referer_set[addr];
    }

    function delReferer(address addr) public onlyRole(DEFAULT_ADMIN_ROLE) returns (address) {
        referer_set[addr] = false;
        referals[referer[addr]].remove(addr);
        return referer[addr];
    }

    function changeReferer(address oldAddr, address newAddr) public onlyRole(DEFAULT_ADMIN_ROLE) returns (uint) {
        require(oldAddr != address(0) , "oldAddr not set");
        require(newAddr != address(0) , "newAddr not set");
    /*    if (commissionRate > 0) {
          // send commission  
          _move(oldAddr,commissionHolder,commissionRate);
        }
    */    

        uint count = referals[oldAddr].length();
        address addr;
        for (uint i=0; i < count; i++) {
            addr = referals[oldAddr].at(i);
            referals[newAddr].add(addr);  
            referer[addr] = newAddr;
            referer_set[addr] = true; 
        }  
        for (uint i=0; i < count; i++) {
            addr = referals[newAddr].at(i);
            referals[oldAddr].remove(addr);
        }
        return referals[newAddr].length();
    }

    function listReferer(address addr) public onlyRole(DEFAULT_ADMIN_ROLE) view returns (address[] memory) {
        uint count  = referals[addr].length();
        address[] memory addrs = new address[](count);

        for (uint i=0; i < count; i++) {
          addrs[i] = referals[addr].at(i);
        }
        return addrs;
    }


    function setDiscount(address addr, uint amount) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(amount <= 25, "Discount is high");
        platform_percent[addr] = amount; 
    }

    function getDiscount(address addr) public onlyRole(DEFAULT_ADMIN_ROLE) view returns (uint) {
        return platform_percent[addr];
    }

    function setCommission(address addr, uint amount) public onlyRole(DEFAULT_ADMIN_ROLE) {
        commissionHolder = addr;
        commissionRate = amount; 
    }

    function getCommission() public onlyRole(DEFAULT_ADMIN_ROLE) view returns (address,uint) {
        return (commissionHolder,commissionRate);
    }

    function addManager(address author, address addr, uint amount) public onlyRole(DEFAULT_ADMIN_ROLE) {
        NymphManagersLib.addManager(managers, author, addr, amount);
    }

    function delManager(address author, address addr) public onlyRole(DEFAULT_ADMIN_ROLE) {  
         NymphManagersLib.delManager(managers, author, addr);
    }

    function getManager(address author) public onlyRole(DEFAULT_ADMIN_ROLE) view returns (address[] memory, uint[] memory) {
        return NymphManagersLib.getManager(managers, author);
    }    
}

