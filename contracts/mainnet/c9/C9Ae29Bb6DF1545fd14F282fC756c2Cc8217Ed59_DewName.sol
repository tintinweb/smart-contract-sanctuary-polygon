pragma solidity ^0.8.13;

address constant CANONICAL_OPERATOR_FILTER_REGISTRY_ADDRESS = 0x000000000000AAeB6D7670E522A718067333cd4E;
address constant CANONICAL_CORI_SUBSCRIPTION = 0x3cc6CddA760b79bAfa08dF41ECFA224f810dCeB6;

// SPDX-License-Identifier: Unlicensed

pragma solidity ^0.8.0;
pragma abicoder v2;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import '@openzeppelin/contracts/security/ReentrancyGuard.sol';
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import '@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol';
import '@openzeppelin/contracts/utils/cryptography/ECDSA.sol';
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "operator-filter-registry/src/IOperatorFilterRegistry.sol";
import {CANONICAL_OPERATOR_FILTER_REGISTRY_ADDRESS} from "operator-filter-registry/src/lib/Constants.sol";
import "./FeeDistributor.sol";

contract DewName is ERC721Enumerable, Ownable, ReentrancyGuard {
    using SafeERC20 for IERC20;

    /// @dev Emitted when an operator is not allowed.
    error OperatorNotAllowed(address operator);

    IOperatorFilterRegistry public constant OPERATOR_FILTER_REGISTRY =
        IOperatorFilterRegistry(CANONICAL_OPERATOR_FILTER_REGISTRY_ADDRESS);

    uint8 constant STAGE_INITIAL = 0;
    uint8 constant STAGE_RESERVED = 1;
    uint8 constant STAGE_TWO_LETTER = 2;
    uint8 constant STAGE_THREE_LETTER = 3;
    uint8 constant STAGE_PUBLIC = 4;

    uint256 constant MAX_NAME_LENGTH = 42;

    string public baseURI;
    uint8 public stage;
    bytes32 public merkleRoot;
    mapping(address => bool) public signers;

    FeeDistributor public feeDistributor;

    uint256 public totalStaked;
    // Mapping from token ID to bool
    mapping(uint256 => bool) public stakedTokens;
    // Mapping from name to token ID
    mapping(string => uint256) public mintedNames;

    struct MintData {
        uint256 salt;
        uint256 deadline;
        string name;
        bytes32[] merkleProof;
        uint256 amount;
        // signature
        bytes32 r;
        bytes32 s;
        uint8 v;
    }

    struct BurnData {
        uint256 salt;
        uint256 deadline;
        string name;
        uint256 feeAmount;
        uint256 treasuryAmount;
        // signature
        bytes32 r;
        bytes32 s;
        uint8 v;
    }

    struct ForceBurnData {
        string name;
        uint256 feeAmount;
        uint256 treasuryAmount;
    }

    event NewBaseUri(string uri);
    event NewStagte(uint8 stage);
    event MerkleRootUpdate(bytes32 merkleRoot);
    event SignerUpdate(address signer, bool isRemoval);
    event FeeDistributorUpdate(address feeDistributor);
    event TokenStaked(address indexed owner, uint256 tokenId);
    event TokenUnstaked(address indexed owner, uint256 tokenId, address operator);
    event Minted(
        address indexed owner,
        uint256 tokenId,
        uint256 rewardsAmount,
        uint256 teamAmount,
        uint256 treasuryAmount
    );
    event Burned(
        address indexed owner,
        uint256 tokenId,
        uint256 rewardsAmount,
        uint256 treasuryAmount,
        address operator,
        address receiver
    );
    event Withdraw(address token, uint256 amount, address operator, address receiver);
    event WithdrawERC721(address token, uint256 tokenId, address operator, address receiver);

    constructor(
        string memory name_, 
        string memory symbol_, 
        string memory baseUri_, 
        FeeDistributor feeDistributor_
    ) ERC721(name_, symbol_) {
        baseURI = baseUri_;
        feeDistributor = feeDistributor_;
        stage = STAGE_INITIAL;
    }

    /**
     * @dev A helper function to check if an operator is allowed.
     */
    modifier onlyAllowedOperator(address from) virtual {
        // Allow spending tokens from addresses with balance
        // Note that this still allows listings and marketplaces with escrow to transfer tokens if transferred
        // from an EOA.
        if (from != msg.sender) {
            _checkFilterOperator(msg.sender);
        }
        _;
    }

    /**
     * @dev A helper function to check if an operator approval is allowed.
     */
    modifier onlyAllowedOperatorApproval(address operator) virtual {
        _checkFilterOperator(operator);
        _;
    }

    /**
     * @dev A helper function to check if an operator is allowed.
     */
    function _checkFilterOperator(address operator) internal view virtual {
        // Check registry code length to facilitate testing in environments without a deployed registry.
        if (Address.isContract(address(OPERATOR_FILTER_REGISTRY))) {
            // under normal circumstances, this function will revert rather than return false, but inheriting contracts
            // may specify their own OperatorFilterRegistry implementations, which may behave differently
            if (!OPERATOR_FILTER_REGISTRY.isOperatorAllowed(address(this), operator)) {
                revert OperatorNotAllowed(operator);
            }
        }
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function setBaseURI(string memory uri) external onlyOwner {
        baseURI = uri;
        emit NewBaseUri(uri);
    }

    function updateStage(uint8 newStage) external onlyOwner {
        require(stage < STAGE_PUBLIC, 'DewName: fail to set new stage');
        stage += 1;
        require(stage == newStage, 'DewName: fail to set new stage');
        emit NewStagte(newStage);
    }

    function updateMerkleRoot(bytes32 merkleRoot_) external onlyOwner {
        merkleRoot = merkleRoot_;
        emit MerkleRootUpdate(merkleRoot);
    }

    function updateSigners(address[] memory toAdd, address[] memory toRemove) external onlyOwner {
        for (uint256 i = 0; i < toAdd.length; i++) {
            signers[toAdd[i]] = true;
            emit SignerUpdate(toAdd[i], false);
        }
        for (uint256 i = 0; i < toRemove.length; i++) {
            delete signers[toRemove[i]];
            emit SignerUpdate(toRemove[i], true);
        }
    }

    function updateFeeDistributor(FeeDistributor feeDistributor_) external onlyOwner {
        feeDistributor = feeDistributor_;
        emit FeeDistributorUpdate(address(feeDistributor));
    }

    function stake(uint256 tokenId) external nonReentrant {
        require(ownerOf(tokenId) == msg.sender, 'DewName: Not your token');
        require(!stakedTokens[tokenId], 'DewName: Already staked');
        _stake(msg.sender, tokenId);
    }

    function stakeTokens(uint256[] memory tokens) external nonReentrant {
        for (uint256 i = 0; i < tokens.length; i++) {
            uint256 tokenId = tokens[i];
            if (ownerOf(tokenId) == msg.sender && !stakedTokens[tokenId]) {
                _stake(msg.sender, tokenId);
            }
        }
    }

    function _stake(address operator, uint256 tokenId) internal {
        stakedTokens[tokenId] = true;
        totalStaked += 1;
        emit TokenStaked(operator, tokenId);
    }

    function unstake(uint256 tokenId) external nonReentrant {
        require(ownerOf(tokenId) == msg.sender, 'DewName: Not your token');
        require(stakedTokens[tokenId], 'DewName: Not staked');
        _unstake(msg.sender, tokenId);
    }

    function unstakeTokens(uint256[] memory tokens) external nonReentrant {
        for (uint256 i = 0; i < tokens.length; i++) {
            uint256 tokenId = tokens[i];
            if (ownerOf(tokenId) == msg.sender && stakedTokens[tokenId]) {
                _unstake(msg.sender, tokenId);
            }
        }
    }

    function _unstake(address operator, uint256 tokenId) internal {
        delete stakedTokens[tokenId];
        totalStaked -= 1;
        emit TokenUnstaked(ownerOf(tokenId), tokenId, operator);
    }

    function mint(uint256 tokenId, MintData calldata data) external payable nonReentrant {
        require(data.deadline > block.timestamp, 'DewName: deadline reached');
        require(mintedNames[data.name] == 0, 'DewName: Minted dew name');
        require(data.amount == msg.value, 'DewName: Wrong price');
        _canMint(msg.sender, data);
        _verifyMintSignature(tokenId, data);
        (uint256 rewardsAmount, uint256 teamAmount, uint256 treasuryAmount) = feeDistributor
            .deposit{value: data.amount}();
        _safeMint(msg.sender, tokenId);
        emit Minted(msg.sender, tokenId, rewardsAmount, teamAmount, treasuryAmount);
        mintedNames[data.name] = tokenId;
    }

    function burn(uint256 tokenId, BurnData calldata data) external nonReentrant {
        require(data.deadline > block.timestamp, 'DewName: deadline reached');
        require(_exists(tokenId), 'DewName: Token not exists');
        require(ownerOf(tokenId) == msg.sender, 'DewName: Not your token');
        _verifyBurnSignature(tokenId, data);
        _burn(tokenId, msg.sender, data.feeAmount, data.treasuryAmount, data.name);
    }

    function _burn(
        uint256 tokenId, 
        address to, 
        uint256 feeAmount, 
        uint256 treasuryAmount, 
        string memory name
    ) internal {
        require(mintedNames[name] == tokenId, 'DewName: Name not exists');
        address owner = _ownerOf(tokenId);
        if (stakedTokens[tokenId]) {
            _unstake(msg.sender, tokenId);
        }
        feeDistributor.withdraw(to, feeAmount, treasuryAmount);
        _burn(tokenId);
        emit Burned(owner, tokenId, feeAmount, treasuryAmount, msg.sender, to);
        delete mintedNames[name];
    }

    function setApprovalForAll(
        address operator, 
        bool approved
    ) public override(ERC721, IERC721) onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    function approve(
        address operator, 
        uint256 tokenId
    ) public override(ERC721, IERC721) onlyAllowedOperatorApproval(operator) {
        super.approve(operator, tokenId);
    }

    function transferFrom(
        address from, 
        address to, 
        uint256 tokenId
    ) public override(ERC721, IERC721) onlyAllowedOperator(from) {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from, 
        address to, 
        uint256 tokenId
    ) public override(ERC721, IERC721) onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from, 
        address to, 
        uint256 tokenId, 
        bytes memory data
    ) public override(ERC721, IERC721) onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId, data);
    }

    function _beforeTokenTransfer(
        address from, 
        address to, 
        uint256 tokenId, 
        uint256 batchSize
    ) internal virtual override {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
        require(!stakedTokens[tokenId], 'DewName: Token transfer while staked');
    }

    function _canMint(address user, MintData memory data) internal view {
        bool canMint = false;
        uint len = bytes(data.name).length;
        if (stage == STAGE_RESERVED) {
            require(balanceOf(user) == 0, 'DewName: Already minted');
            // Compute the node and verify the merkle proof
            bytes32 node = keccak256(abi.encodePacked(user, data.name));
            canMint = MerkleProof.verify(data.merkleProof, merkleRoot, node);
            require(canMint, 'DewName: Invalid proof');
        } else if (stage == STAGE_TWO_LETTER) {
            canMint = len == 2;
        } else if (stage == STAGE_THREE_LETTER) {
            canMint = len == 3;
        } else if (stage == STAGE_PUBLIC) {
            canMint = len <= MAX_NAME_LENGTH;
        } else {
            revert('DewName: Invalid stage');
        }
        require(canMint, 'DewName: Invalid name');
    }

    function _verifyMintSignature(uint256 tokenId, MintData memory data) internal view {
        bytes32 hash = keccak256(
            abi.encode(data.salt, data.deadline, tokenId, data.name, data.amount)
        );
        address signer = ECDSA.recover(hash, data.v, data.r, data.s);
        require(signers[signer], 'DewName: Signature error');
    }

    function _verifyBurnSignature(uint256 tokenId, BurnData memory data) internal view {
        bytes32 hash = keccak256(
            abi.encode(
                data.salt, 
                data.deadline, 
                tokenId, 
                data.name, 
                data.feeAmount, 
                data.treasuryAmount
            )
        );
        address signer = ECDSA.recover(hash, data.v, data.r, data.s);
        require(signers[signer], 'DewName: Signature error');
    }

    // safety
    function emergencyBurn(
        uint256 tokenId, 
        ForceBurnData calldata data, 
        address to
    ) external onlyOwner nonReentrant {
        require(_exists(tokenId), 'DewName: Token not exists');
        _burn(tokenId, to, data.feeAmount, data.treasuryAmount, data.name);
    }

    function emergencyWithdraw(address to, IERC20[] memory tokens) external onlyOwner nonReentrant {
        uint256 ethBalance = address(this).balance;
        if (ethBalance > 0) {
            Address.sendValue(payable(to), ethBalance);
            emit Withdraw(address(0), ethBalance, msg.sender, to);
        }
        for (uint256 i = 0; i < tokens.length; i++) {
            IERC20 token = tokens[i];
            uint256 balance = token.balanceOf(address(this));
            if (balance > 0) {
                token.safeTransfer(to, balance);
                emit Withdraw(address(token), balance, msg.sender, to);
            }
        }
    }

    function emergencyWithdrawERC721(
        address to, 
        IERC721[] memory tokens, 
        uint256[] memory tokenIds
    ) external onlyOwner nonReentrant {
        require(tokens.length == tokenIds.length, 'DewName: length check');
        for (uint256 i = 0; i < tokens.length; i++) {
            IERC721 token = tokens[i];
            uint256 tokenId = tokenIds[i];
            if (token.ownerOf(tokenId) == address(this)) {
                token.safeTransferFrom(address(this), to, tokenId);
                emit WithdrawERC721(address(token), tokenId, msg.sender, to);
            }
        }
    }

}

