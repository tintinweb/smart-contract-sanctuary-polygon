import "@openzeppelin/contracts-upgradeable/metatx/ERC2771ContextUpgradeable.sol";
import "hardhat/console.sol";

contract CryptoDadsMarketplaceV2 is
    OwnableUpgradeable,
    ERC721HolderUpgradeable,
    ERC1155HolderUpgradeable,
    ReentrancyGuardUpgradeable,
    EIP712Upgradeable,
    ERC2771ContextUpgradeable
{
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using StringsUpgradeable for string;

    /// @notice seed message for bid encryption
    bytes32 private constant BID_MESSAGE =
        keccak256(
            "BidMessage(address buyer,uint256 price,address nftToken,uint256 tokenId,uint256 amount,bool is721,uint256 nonce)"
        );

    /// @notice nonce for (buyer => nftToken => tokenId)
    mapping(address => mapping(address => mapping(uint256 => uint256)))
        public nonces;

    /// @notice items sold
    uint256 public itemsSold;

    /// @notice raffles run
    uint256 public rafflesDrawn;

    /// @notice last item id
    uint256 public lastItemId;

    /// @notice last raffle id
    uint256 public lastRaffleId;

    /// @notice erc20 payment token
    IERC20Upgradeable public paymentToken;

    /// @notice Whitelists, NFTs, and Merch raffle structure
    struct RaffleItem {
        string name;
        string description;
        string itemType; /* WL, NFT, MERCH */
        mapping(address => bool) participants;
        uint256 participantsCount;
        address[] addresses;
        address[] winners;
        uint256 maxWinners;
        uint256 maxParticipants;
        uint256 price;
        uint256 endTimestamp;
        uint256 shuffleEntropy;
        string imageURI;
        string externalURL;
    }

    /// @notice lastRaffleId => raffleItem structure
    mapping(uint256 => RaffleItem) public raffleItems;

    /// @notice address => user raffleIds
    mapping(address => uint256[]) private raffleEntries;

    /// @notice Marketplace items structure
    struct MarketItem {
        address seller;
        address nftToken;
        uint256 tokenId;
        uint256 amount;
        string name;
        string description;
        string externalURL;
        string itemType; /* ERC721, ERC1155, RAFFLE, WL, EXPERIENCES, etc */
        uint256 price;
        string imageURI;
    }

    /// @notice itemId => marketItem structure
    mapping(uint256 => MarketItem) public marketItems;

    /// @notice whitelisted nfts
    mapping(address => bool) public whitelistedTokens;

    modifier onlyWhitelistedNFT(address token) {
        require(
            whitelistedTokens[token],
            "CryptoDadsMarketplace: NOT_WHITELISTED"
        );
        _;
    }

    /// @notice gnosis token safe
    address tokenSafe;

    /// @notice moderators (mainly backend) to handle unlisting
    mapping(address => bool) public moderators;

    modifier onlyModerator() {
        require(
            moderators[_msgSender()],
            "CryptoDadsMarketplace: NON_MODERATOR"
        );
        _;
    }

    /** EVENTS **/

    /// @notice event emitted when seller lists item on sale
    event ItemListed(
        address indexed seller,
        address indexed nftToken,
        uint256 indexed tokenId,
        uint256 amount,
        string name,
        string itemType,
        uint256 price
    );

    /// @notice event emitted when seller unlists item from sale
    event ItemUnlisted(uint256 indexed itemId);

    /// @notice event emitted when seller updates the item price on sale
    event ItemPriceUpdated(uint256 indexed itemId, uint256 price);

    /// @notice event emitted when item is sold
    event ItemSold(
        uint256 indexed itemId,
        address indexed seller,
        address indexed buyer,
        uint256 price,
        address nftToken,
        string name,
        uint256 tokenId
    );

    /// @notice event emitted when a raffle is created
    event RaffleCreated(
        string indexed name,
        string indexed itemType,
        uint256 indexed raffleId,
        uint256 price,
        string nameStr,
        string itemTypeStr
    );

    /// @notice event emitted when a raffle is joined
    event RaffleJoined(
        uint256 indexed raffleId,
        address indexed participant,
        string nameStr
    );

    /// @notice event emitted when a raffle is won
    event RaffleWinners(
        string indexed name,
        string indexed itemType,
        uint256 indexed raffleId,
        address[] winners,
        string nameStr,
        string itemTypeStr
    );

    /// @notice event emitted when bid is accepted
    event BidAccepted(
        address indexed seller,
        address indexed buyer,
        uint256 price,
        address nftToken,
        uint256 tokenId,
        uint256 amount,
        bool is721
    );

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor(address _forwarder) ERC2771ContextUpgradeable(_forwarder) {}

    function initialize(address _token, address _tokenSafe) public initializer {
        require(_token != address(0), "CryptoDadsMarketplace: ZERO_TOKEN");
        paymentToken = IERC20Upgradeable(_token);
        moderators[_msgSender()] = true;
        tokenSafe = _tokenSafe;

        __Ownable_init();
        __ERC721Holder_init();
        __ERC1155Holder_init();
        __ReentrancyGuard_init();
        __EIP712_init("CryptoDads Marketplace", "1.0");
    }

    /**
     * @notice whitelist nft token for trade
     * @dev only owner call this function
     * @param nftToken nft token address
     * @param approved approved status
     */
    function setWhitelistedToken(address nftToken, bool approved)
        external
        onlyModerator
    {
        require(nftToken != address(0), "CryptoDadsMarketplace: INVALID_NFT");
        whitelistedTokens[nftToken] = approved;
    }

    /**
     * @dev sets token safe for erc20 payment token transfers
     * @param _tokenSafe address of safe
     */
    function setTokenSafe(address _tokenSafe) public onlyModerator {
        tokenSafe = payable(_tokenSafe);
    }

    /**
     * @dev set moderator address by owner
     * @param moderator address of moderator
     * @param approved true to add, false to remove
     */
    function setModerator(address moderator, bool approved) external onlyOwner {
        require(
            moderator != address(0),
            "CryptoDadsMarketplace: INVALID_MODERATOR"
        );
        moderators[moderator] = approved;
    }

    /** RAFFLES **/

    /**
     * @notice to create raffle item
     * @dev moderators call this function
     * @param _name The name of the raffle
     * @param _description The description of the raffle
     * @param _imageURI Image uri for raffle image
     * @param _externalURL External URL for raffle link
     * @param _itemType The raffle item type (WL, ERC721, MERCH, etc.)
     * @param _maxParticipants Max participants in raffle
     * @param _maxWinners Max allowable winners
     * @param _endTimestamp End of raffle timestamp (UNIX)
     * @param _price Price
     */
    function createRaffle(
        string memory _name,
        string memory _description,
        string memory _imageURI,
        string memory _externalURL,
        string memory _itemType,
        uint256 _maxParticipants,
        uint256 _maxWinners,
        uint256 _endTimestamp,
        uint256 _price
    ) external onlyModerator returns (uint256) {
        lastRaffleId += 1;

        RaffleItem storage raffle = raffleItems[lastRaffleId];

        raffle.name = _name;
        raffle.description = _description;
        raffle.imageURI = _imageURI;
        raffle.externalURL = _externalURL;
        raffle.itemType = _itemType;
        raffle.maxParticipants = _maxParticipants;
        raffle.maxWinners = _maxWinners;
        raffle.price = _price;
        raffle.endTimestamp = _endTimestamp;

        emit RaffleCreated(
            _name,
            _itemType,
            lastRaffleId,
            _price,
            _name,
            _itemType
        );

        return lastRaffleId;
    }

    /**
     * @notice to update raffle item
     * @dev moderators call this function
     * @param _name The name of the raffle
     * @param _description The description of the raffle
     * @param _imageURI Image uri for raffle image
     * @param _externalURL External URL for raffle link
     * @param _itemType The raffle item type (WL, ERC721, MERCH, etc.)
     * @param _maxParticipants Max participants in raffle
     * @param _maxWinners Max allowable winners
     * @param _endTimestamp End of raffle timestamp (UNIX)
     * @param _price Price
     */
    function updateRaffleItem(
        uint256 _raffleId,
        string memory _name,
        string memory _description,
        string memory _imageURI,
        string memory _externalURL,
        string memory _itemType,
        uint256 _maxParticipants,
        uint256 _maxWinners,
        uint256 _endTimestamp,
        uint256 _price
    ) external onlyModerator {
        require(
            lastRaffleId >= _raffleId,
            "CryptoDadsMarketplace: NON_EXISTENT"
        );
        RaffleItem storage raffle = raffleItems[_raffleId];

        raffle.name = _name;
        raffle.description = _description;
        raffle.imageURI = _imageURI;
        raffle.externalURL = _externalURL;
        raffle.itemType = _itemType;
        raffle.maxParticipants = _maxParticipants;
        raffle.maxWinners = _maxWinners;
        raffle.price = _price;
        raffle.endTimestamp = _endTimestamp;
    }

    /**
     * @notice to update price and end time for a raffle
     * @dev sellers call this function
     * @param _raffleId raffle id
     * @param _price new price of raffle
     * @param _endTimestamp new end timestamp
     * ---
     * Added V4
     */
    function updateRafflePriceAndTimestamp(
        uint256 _raffleId,
        uint256 _price,
        uint256 _endTimestamp
    ) external onlyModerator {
        require(
            lastRaffleId >= _raffleId,
            "CryptoDadsMarketplace: NON_EXISTENT"
        );
        RaffleItem storage raffle = raffleItems[_raffleId];
        raffle.price = _price;
        raffle.endTimestamp = _endTimestamp;
    }

    function canRunRaffle(uint256 _raffleId) external view returns (bool) {
        require(
            lastRaffleId >= _raffleId,
            "CryptoDadsMarketplace: NON_EXISTENT"
        );
        RaffleItem storage raffle = raffleItems[_raffleId];
        return
            (raffle.winners.length == 0 ||
                raffle.participantsCount <= raffle.maxParticipants) &&
            block.timestamp < raffle.endTimestamp;
    }

    function determineRaffleWinners(uint256 _raffleId) internal {
        require(
            lastRaffleId >= _raffleId,
            "CryptoDadsMarketplace: NON_EXISTENT"
        );

        RaffleItem storage raffle = raffleItems[_raffleId];

        require(
            raffle.winners.length == 0,
            "CryptoDadsMarketplace: RAFFLE_ALREADY_RUN"
        );

        uint256 shuffleEntropy = getPseudorandom(_raffleId) %
            raffle.addresses.length;

        // Generate int array equal to length addresses and randomly shuffle it with entropy
        uint256[] memory shuffledParticipants = shuffle(
            raffle.addresses.length,
            shuffleEntropy
        );

        raffle.shuffleEntropy = shuffleEntropy;

        // Set winners from shuffle
        if (raffle.addresses.length < raffle.maxWinners) {
            // Everyones a winner!
            for (uint256 i = 0; i < raffle.addresses.length; i++) {
                raffle.winners.push(raffle.addresses[i]);
            }
        } else {
            for (uint256 i = 0; i < raffle.maxWinners; i++) {
                raffle.winners.push(
                    raffle.addresses[shuffledParticipants[i] - 1]
                );
            }
        }

        // Emit raffle event winner
        emit RaffleWinners(
            raffle.name,
            raffle.itemType,
            _raffleId,
            raffle.winners,
            raffle.name,
            raffle.itemType
        );

        rafflesDrawn++;
    }

    function joinRaffle(uint256 id) public {
        address participant = _msgSender();
        RaffleItem storage raffle = raffleItems[id];

        require(
            IERC20Upgradeable(paymentToken).balanceOf(participant) >=
                raffle.price,
            "CryptoDadsMarketplace: NOT_ENOUGH_FUNDS"
        );

        require(
            raffle.winners.length == 0,
            "CryptoDadsMarketplace: NO_WINNERS"
        );

        require(
            raffle.participantsCount < raffle.maxParticipants,
            "CryptoDadsMarketplace: MAX_PARTICIPANTS"
        );

        require(
            block.timestamp <= raffle.endTimestamp,
            "CryptoDadsMarketplace: TOO_LATE"
        );

        // Transfer tokens to marketplace owner (vault)
        IERC20Upgradeable(paymentToken).transferFrom(
            participant,
            payable(tokenSafe),
            raffle.price
        );

        raffle.participants[participant] = true;
        raffle.addresses.push(participant);
        raffle.participantsCount++;
        raffleEntries[participant].push(id);

        emit RaffleJoined(id, participant, raffle.name);
    }

    // https://gist.github.com/cleanunicorn/d27484a2488e0eecec8ce23a0ad4f20b
    function shuffle(uint256 size, uint256 entropy)
        public
        pure
        returns (uint256[] memory)
    {
        uint256[] memory result = new uint256[](size);

        // Initialize array.
        for (uint256 i = 0; i < size; i++) {
            result[i] = i + 1;
        }

        // Set the initial randomness based on the provided entropy.
        bytes32 random = keccak256(abi.encodePacked(entropy));

        // Set the last item of the array which will be swapped.
        uint256 last_item = size - 1;

        // We need to do `size - 1` iterations to completely shuffle the array.
        for (uint256 i = 1; i < size - 1; i++) {
            // Select a number based on the randomness.
            uint256 selected_item = uint256(random) % last_item;

            // Swap items `selected_item <> last_item`.
            uint256 aux = result[last_item];
            result[last_item] = result[selected_item];
            result[selected_item] = aux;

            // Decrease the size of the possible shuffle
            // to preserve the already shuffled items.
            // The already shuffled items are at the end of the array.
            last_item--;

            // Generate new randomness.
            random = keccak256(abi.encodePacked(random));
        }

        return result;
    }

    /// @notice Manually runs a raffle by raffle id
    function runRaffleWinnersManually(uint256 _raffleId)
        external
        onlyModerator
    {
        determineRaffleWinners(_raffleId);
    }

    /// @notice Manually sets raffle winners by raffle id, if needed to be done offchain
    function setRaffleWinnersManually(
        uint256 _raffleId,
        address[] calldata addresses
    ) external onlyModerator {
        require(
            lastRaffleId >= _raffleId,
            "CryptoDadsMarketplace: NON_EXISTENT"
        );

        RaffleItem storage raffle = raffleItems[_raffleId];

        // Set winners from shuffle
        for (uint256 i = 0; i < addresses.length; i++) {
            raffle.winners.push(addresses[i]);
        }

        // Emit raffle event winner
        emit RaffleWinners(
            raffle.name,
            raffle.itemType,
            _raffleId,
            raffle.winners,
            raffle.name,
            raffle.itemType
        );

        rafflesDrawn++;
    }

    /* Good enough for now, we can switch to chainlink vrf2 when support for upgradeable contracts is easier */
    function getPseudorandom(uint256 _raffleId) private view returns (uint256) {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        address(this),
                        block.difficulty,
                        block.number,
                        block.timestamp,
                        blockhash(block.number - 1),
                        _raffleId
                    )
                )
            );
    }

    function isParticipating(address addr, uint256 _raffleId)
        external
        view
        returns (bool)
    {
        require(
            lastRaffleId >= _raffleId,
            "CryptoDadsMarketplace: NON_EXISTENT"
        );
        RaffleItem storage raffle = raffleItems[_raffleId];
        return raffle.participants[addr];
    }

    function myRaffles() public view returns (uint256[] memory) {
        return raffleEntries[_msgSender()];
    }

    function getRaffleDrawingDetails(uint256 _raffleId)
        public
        view
        returns (
            address[] memory,
            address[] memory,
            uint256
        )
    {
        require(
            lastRaffleId >= _raffleId,
            "CryptoDadsMarketplace: NON_EXISTENT"
        );
        return (
            raffleItems[_raffleId].addresses,
            raffleItems[_raffleId].winners,
            raffleItems[_raffleId].shuffleEntropy
        );
    }

    /** MARKETPLACE **/

    /**
     * @notice to list item on sale
     * @dev seller(nft owner)s call this function
     * @param nftToken nft token address (either 721 or 1155)
     * @param tokenId  token id of nft token
     * @param amount amount of nfts (valid only when nftToken is 1155 = !is721)
     * @param name The name of the item
     * @param description The description of the item
     * @param imageURI The image uri for display purposes
     * @param externalURL The external URL of the item
     * @param itemType The type of item being sold
     * @param price amount of paymentToken to purchase nft
     * emit {ItemListed} event
     */
    function listItem(
        address nftToken,
        uint256 tokenId,
        uint256 amount,
        string memory name,
        string memory description,
        string memory imageURI,
        string memory externalURL,
        string memory itemType,
        uint256 price
    ) external onlyModerator {
        require(price > 0, "CryptoDadsMarketplace: ZERO_PRICE");
        lastItemId += 1;
        marketItems[lastItemId] = MarketItem(
            _msgSender(),
            nftToken,
            tokenId,
            amount,
            name,
            description,
            externalURL,
            itemType,
            price,
            imageURI
        );
        emit ItemListed(
            _msgSender(),
            nftToken,
            tokenId,
            amount,
            name,
            itemType,
            price
        );
    }

    /**
     * @notice to unlist item from sale
     * @dev sellers call this function
     * @param itemId market item id
     * emit {ItemUnlisted} event
     */
    function unlistItem(uint256 itemId) external onlyModerator {
        MarketItem storage item = marketItems[itemId];
        require(
            _msgSender() == item.seller || moderators[_msgSender()],
            "CryptoDadsMarketplace: NOT_SELLER"
        );
        emit ItemUnlisted(itemId);
        delete marketItems[itemId];
        itemsSold++;
    }

    /**
     * @notice to update image uri for item
     * @dev sellers call this function
     * @param itemId market item id
     * @param newImageURI new image of item
     * emit {ItemPriceUpdated} event
     */
    function updateItemImage(uint256 itemId, string memory newImageURI)
        external
        onlyModerator
    {
        MarketItem storage item = marketItems[itemId];
        require(
            _msgSender() == item.seller,
            "CryptoDadsMarketplace: NOT_SELLER"
        );
        item.imageURI = newImageURI;
    }

    /**
     * @notice to update image uri for item
     * @dev sellers call this function
     * @param itemId market item id
     * @param newName new name of item
     * emit {ItemPriceUpdated} event
     */
    function updateItemName(uint256 itemId, string memory newName)
        external
        onlyModerator
    {
        MarketItem storage item = marketItems[itemId];
        require(
            _msgSender() == item.seller,
            "CryptoDadsMarketplace: NOT_SELLER"
        );
        item.name = newName;
    }

    /**
     * @notice to update price of item on sale
     * @dev sellers call this function
     * @param itemId market item id
     * @param newPrice new price of item
     * emit {ItemPriceUpdated} event
     */
    function updateItemPrice(uint256 itemId, uint256 newPrice)
        external
        onlyModerator
    {
        MarketItem storage item = marketItems[itemId];
        require(
            _msgSender() == item.seller,
            "CryptoDadsMarketplace: NOT_SELLER"
        );
        require(newPrice > 0, "CryptoDadsMarketplace: ZERO_PRICE");
        item.price = newPrice;
        emit ItemPriceUpdated(itemId, newPrice);
    }

    /**
     * @notice to purchase item on sale
     * @dev buyers call this function
     * @param itemId market item id
     * emit {ItemSold} event
     */
    function purchaseItem(uint256 itemId) external nonReentrant {
        MarketItem storage item = marketItems[itemId];

        require(item.price > 0, "CryptoDadsMarketplace: INVALID_ITEM");

        _proceedPayment(item.seller, _msgSender(), item.price);

        emit ItemSold(
            itemId,
            item.seller,
            _msgSender(),
            item.price,
            item.nftToken,
            item.name,
            item.tokenId
        );

        delete marketItems[itemId];
        itemsSold++;
    }

    /**
     * @notice to accept bid from buyers to nft which is not listed on sale
     * @dev seller(nft owner)s call this function
     * @param buyer address of buyer who placed the bid
     * @param price amount of paymentToken to purchase nft
     * @param nftToken nft token address (either 721 or 1155)
     * @param tokenId  token id of nft token
     * @param amount amount of nfts (valid only when nftToken is 1155 = !is721)
     * @param is721 specify whether nftToken is 721 or not
     * param {v,r,s} bid signature info which are fetched from backend
     * emit {BidAccepted} event
     */
    function acceptBid(
        address buyer,
        uint256 price,
        address nftToken,
        uint256 tokenId,
        uint256 amount,
        bool is721,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external onlyWhitelistedNFT(nftToken) {
        uint256 nonce = nonces[buyer][nftToken][tokenId];
        bytes32 structHash = keccak256(
            abi.encode(
                BID_MESSAGE,
                buyer,
                price,
                nftToken,
                tokenId,
                amount,
                is721,
                nonce
            )
        );
        bytes32 hash = _hashTypedDataV4(structHash);
        address signer = ECDSAUpgradeable.recover(hash, v, r, s);
        require(signer == buyer, "CryptoDadsMarketplace: INVALID_SIGNATURE");

        address _buyer = buyer;
        uint256 _price = price;
        address _nftToken = nftToken;
        uint256 _tokenId = tokenId;
        uint256 _amount = amount;
        bool _is721 = is721;

        if (is721) {
            IERC721Upgradeable(_nftToken).safeTransferFrom(
                _msgSender(),
                _buyer,
                _tokenId
            );
        } else {
            IERC1155Upgradeable(_nftToken).safeTransferFrom(
                _msgSender(),
                _buyer,
                _tokenId,
                _amount,
                "0x"
            );
        }

        _proceedPayment(_msgSender(), _buyer, _price);

        emit BidAccepted(
            _msgSender(),
            _buyer,
            _price,
            _nftToken,
            _tokenId,
            _amount,
            _is721
        );
    }

    /**
     * @notice to proceed payment from buyer to seller
     * @dev only internal usage
     * @param seller token payee
     * @param buyer token payer
     * @param price amount of paymentToken to purchase nft
     */
    function _proceedPayment(
        address seller,
        address buyer,
        uint256 price
    ) private {
        paymentToken.safeTransferFrom(buyer, seller, price);
    }

    /**
     * @notice Gets open raffle items
     * @dev Integrators call this function
     */
    function getOpenRaffleItems() external view returns (uint256[] memory) {
        uint256 unraffledItems = lastRaffleId - rafflesDrawn;
        uint256[] memory liveItems = new uint256[](unraffledItems);
        uint256 index = 0;
        for (uint256 i = 0; i <= lastRaffleId; i++) {
            RaffleItem storage item = raffleItems[i];
            if (item.winners.length == 0) {
                liveItems[index++] = i;
            }
        }
        return liveItems;
    }

    /**
     * @notice Get market items listed
     * @dev Integrators call this function
     */
    function getListedMarketItemIds() external view returns (uint256[] memory) {
        // There seems to be an issue with the math being off on lastItemId - itemsSold
