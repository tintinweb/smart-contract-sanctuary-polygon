     */
    function redeemer(uint256 _id) public view returns (address) {
        if (newRedeemer[_id] != address(0)) {
            return newRedeemer[_id];
        } else if (oldCellar.redeemer(_id) != address(0)) {
            return oldCellar.redeemer(_id);
        } else if (v2TastingToken.redeemer(_id) != address(0)) {
            return v2TastingToken.redeemer(_id);
        }
        return newRedeemer[_id];
    }

    /**
     * @dev Returns bottle data for particular token id
     */
    function bottleData(uint256 _id) public view returns (Bottle memory) {
        if (newBottleData[_id].maxTastingTokens != 0) {
            return newBottleData[_id];
        } else if (oldCellar.bottleData(_id).maxTastingTokens != 0) {
            return oldCellar.bottleData(_id);
        } else if (v2DigitalCork.bottleData(_id).maxTastingTokens != 0) {
            return v2DigitalCork.bottleData(_id);
        }
        return newBottleData[_id];
    }

    /**
     * @notice Admin only function to mint new Certificates of Authenticity
     * @dev Requires CORK_MINTER
     * @param _tokenId ID of new bottle
     * @param _dst Recipient of CoA token
     * @param _tastingTokens Number of tasting tokens that can be generated once redeemed
     */
    function mintCoA(
        uint256 _tokenId,
        address _dst,
        uint256 _tastingTokens
    ) external nonReentrant whenNotPaused onlyRole(CORK_MINTER_ROLE) {
        // Bottles must have nonzero tasting tokens available
        if (_tastingTokens == 0) {
            revert MustSupplyNonzeroTastingTokens();
        }
        // Don't allow multiple tokens with same ID after redemption
        if (bottleData(_tokenId).maxTastingTokens != 0) {
            revert DuplicateCorkId();
        }
        newBottleData[_tokenId] = Bottle(_tastingTokens, 0);
        if (!v3DigitalCork.mintCoA(_tokenId, _dst)) {
            revert MintingFailed();
        }
    }

    /**
     * @notice Function to mark a bottle as consumed and start the process of issuing tasting tokens
     * @dev Requires caller to be CORK_REDEEMER
     * @param _tokenId ID of new bottle
     * @param _soldierDst Recipient of soldier (empty bottle) token
     */
    function redeemCoA(
        uint256 _tokenId,
        address _soldierDst
    ) external nonReentrant whenNotPaused onlyRole(CORK_REDEEMER_ROLE) {
        // If burning for redemption, start counter for tasting token minting
        if (v3TastingToken.balanceOf(_soldierDst, _tokenId) > 0) {
            revert DuplicateTastingToken();
        }
        // Revert if burning fails
        if (!v3DigitalCork.burnCoA(_tokenId)) {
            revert BurningFailed();
        }
        if (!v3TastingToken.redeemCoA(_tokenId, _soldierDst)) {
            revert RedemptionFailed();
        }
        newRedeemer[_tokenId] = _soldierDst;
        // Updating max tasting tokens in new cellar along with redemption until
        newBottleData[_tokenId].maxTastingTokens = bottleData(_tokenId)
            .maxTastingTokens;
        newBottleData[_tokenId].redemptionUntil =
            block.timestamp +
            redemptionPeriod;
    }

    /**
     * @notice Function to mint tasting tokens during redemption period
     * @dev Requires caller to be TT_MINTER
     * @param _tokenId ID of new bottle
     * @param _dsts Array of recipients
     */
    function mintTastingTokens(
        uint256 _tokenId,
        address[] memory _dsts
    ) external nonReentrant whenNotPaused onlyRole(TT_MINTER_ROLE) {
        Bottle memory b = bottleData(_tokenId);
        // Only allow during redemption period
        if (block.timestamp > b.redemptionUntil) {
            revert RedemptionPeriodOver();
        }
        // Check how many have been redeemed already
        uint256 _mintedSupply = v3TastingToken.totalSupply(_tokenId);
        if (b.maxTastingTokens - _mintedSupply < _dsts.length) {
            revert InsufficientSupplyRemaining();
        }
        if (
            !v3TastingToken.mintTastingTokens(
                _tokenId,
                redeemer(_tokenId),
                _dsts
            )
        ) {
            revert MintingFailed();
        }
    }

    function setV2DigitalCork(
        IV2DVinDigitalCork _digitalCork
    ) external onlyOwner {
        v2DigitalCork = _digitalCork;
    }

    function setV2TastingToken(
        IV2DVinTastingToken _tastingToken
    ) external onlyOwner {
        v2TastingToken = _tastingToken;
    }

    function setOldDigitalCellar(
        IDVinDigitalCellar _cellar
    ) external onlyOwner {
        oldCellar = _cellar;
    }

    /**
     * @notice Set DVin Digital Cork address
     * @param _digitalCork new DVin Digital Cork address
     */
    function setV3DigitalCork(
        IDVinDigitalCork _digitalCork
    ) external onlyOwner {
        v3DigitalCork = _digitalCork;
    }

    /**
     * @notice Set DVin Tasting Token address
     * @param _tastingToken new DVin Tasting Token address
     */
    function setV3TastingToken(
        IDVinTastingToken _tastingToken
    ) external onlyOwner {
        v3TastingToken = _tastingToken;
    }

    /**
     * @notice Set tasting token redemption period
     * @param _period Redemption period in seconds
     */
    function setRedemptionPeriod(uint256 _period) external onlyOwner {
        redemptionPeriod = _period;
    }
}