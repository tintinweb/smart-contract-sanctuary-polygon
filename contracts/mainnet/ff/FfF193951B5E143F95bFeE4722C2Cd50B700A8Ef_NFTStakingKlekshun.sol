// File: contracts/NFTStakingKlekshun.sol


pragma solidity ^0.8.18;






contract NFTStakingKlekshun is Ownable, IERC721Receiver {
    event Received();

    function onERC721Received(
        address _operator,
        address _from,
        uint256 _tokenId,
        bytes calldata _data
    ) external override returns (bytes4) {
        _operator;
        _from;
        _tokenId;
        _data;
        emit Received();
        return 0x150b7a02;
    }

    using Strings for uint256;

    enum Step {
        Off,
        On
    }

    Step public Steps;
    struct StakeInfo {
        uint256[] tokenIds;
        uint256[] stakingTimestamps;
    }
    struct ContractRule {
        uint256 limit1;
        uint256 limit2;
        uint256 reward1;
        uint256 reward2;
    }

    IERC20 public rewardToken;
    IERC721A[] public stakeableNFTs;
    uint256 public timeUnit = 86400; // 1 day in seconds

    mapping(address => mapping(uint256 => StakeInfo)) internal stakerInfo;
    mapping(uint256 => ContractRule) public contractRules;

    constructor(IERC20 _rewardToken, IERC721A[] memory _stakeableNFTs) {
        rewardToken = _rewardToken;
        stakeableNFTs = _stakeableNFTs;
    }

    modifier callerIsUser() {
        require(tx.origin == msg.sender, "The caller is another contract");
        _;
    }

    // STAKING FUNCTION
    function stake(uint256 _contractIndex, uint256[] calldata _tokenIds)
        external
        callerIsUser
    {
        require(Steps == Step.On, "Staking is disabled.");
        IERC721A nftCollection = stakeableNFTs[_contractIndex];

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            require(
                nftCollection.ownerOf(_tokenIds[i]) == msg.sender,
                "You don't own this NFT"
            );
            nftCollection.safeTransferFrom(
                msg.sender,
                address(this),
                _tokenIds[i]
            );

            stakerInfo[msg.sender][_contractIndex].tokenIds.push(_tokenIds[i]);
            stakerInfo[msg.sender][_contractIndex].stakingTimestamps.push(
                block.timestamp
            );
        }
    }

    // UNSTAKING FUNCITON
    function unstake(uint256 _contractIndex, uint256[] calldata _tokenIds)
        external
    {
        require(Steps == Step.On, "Unstaking is disabled.");
        StakeInfo storage stakeInfo = stakerInfo[msg.sender][_contractIndex];
        IERC721A nftCollection = stakeableNFTs[_contractIndex];

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            uint256 index = findTokenIndex(stakeInfo.tokenIds, _tokenIds[i]);
            require(index < stakeInfo.tokenIds.length, "NFT not staked");

            uint256 stakingTimestamp = stakeInfo.stakingTimestamps[index];
            if (block.timestamp >= stakingTimestamp + timeUnit) {
                uint256 reward = getRewards(msg.sender, _contractIndex);
                rewardToken.transfer(msg.sender, reward);
                stakeInfo.stakingTimestamps[index] = block.timestamp;
            }

            nftCollection.safeTransferFrom(
                address(this),
                msg.sender,
                _tokenIds[i]
            );

            removeTokenFromStakeInfo(stakeInfo, index);
        }
    }

    // FUNCTIONS FOR REWARDS CALCULATION
    function checkRewards(uint256 _contract, uint256 nftCount)
        public
        view
        returns (uint256 totalReward)
    {
        ContractRule storage rule = contractRules[_contract];
        uint256 reward1NFTs;
        uint256 reward2NFTs;

        if (nftCount <= rule.limit1) {
            reward1NFTs = nftCount;
            reward2NFTs = 0;
        } else if (nftCount > rule.limit1 && nftCount <= rule.limit2) {
            reward1NFTs = rule.limit1;
            reward2NFTs = nftCount - rule.limit1;
        } else {
            reward1NFTs = rule.limit1;
            reward2NFTs = rule.limit2 - rule.limit1;
        }

        totalReward =
            (reward1NFTs * rule.reward1) +
            (reward2NFTs * rule.reward2);
        return totalReward;
    }

    function claimRewards(uint256 _contract) external {
        uint256 rewards = getRewards(msg.sender, _contract);

        require(rewards > 0, "No rewards to claim");
        rewardToken.transfer(msg.sender, rewards);
    }

    function getRewards(address user, uint256 _contract)
        internal
        returns (uint256)
    {
        StakeInfo storage stakeInfo = stakerInfo[user][_contract];
        uint256 rewards = 0;

        for (uint256 i = 0; i < stakeInfo.tokenIds.length; i++) {
            uint256 stakingTimestamp = stakeInfo.stakingTimestamps[i];
            if (block.timestamp >= stakingTimestamp + timeUnit) {
                uint256 totalReward = checkRewards(
                    _contract,
                    stakeInfo.tokenIds.length
                );
                uint256 reward = ((block.timestamp - stakingTimestamp) /
                    timeUnit) *
                    totalReward;
                rewards += reward;
                stakeInfo.stakingTimestamps[i] = block.timestamp;
            }
        }

        return rewards;
    }

    // EXTERNAL FUNCTION FOR CHECKING USER REWARDS
    function earnedRewards(uint256 _contract, address _address)
        external
        view
        returns (uint256)
    {
        StakeInfo storage stakeInfo = stakerInfo[_address][_contract];
        uint256 rewards = 0;

        for (uint256 i = 0; i < stakeInfo.tokenIds.length; i++) {
            uint256 stakingTimestamp = stakeInfo.stakingTimestamps[i];
            if (block.timestamp >= stakingTimestamp + timeUnit) {
                uint256 totalReward = checkRewards(
                    _contract,
                    stakeInfo.tokenIds.length
                );
                uint256 reward = ((block.timestamp - stakingTimestamp) /
                    timeUnit) *
                    totalReward;
                rewards += reward;
            }
        }

        return rewards;
    }

    // UTILITY FUNCTIONS
    function findTokenIndex(uint256[] memory tokenIds, uint256 tokenId)
        internal
        pure
        returns (uint256)
    {
        uint256 index = tokenIds.length;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            if (tokenIds[i] == tokenId) {
                index = i;
                break;
            }
        }
        return index;
    }

    function removeTokenFromStakeInfo(
        StakeInfo storage stakeInfo,
        uint256 index
    ) internal {
        require(index < stakeInfo.tokenIds.length, "Index out of bounds");

        stakeInfo.tokenIds[index] = stakeInfo.tokenIds[
            stakeInfo.tokenIds.length - 1
        ];
        stakeInfo.stakingTimestamps[index] = stakeInfo.stakingTimestamps[
            stakeInfo.stakingTimestamps.length - 1
        ];

        stakeInfo.tokenIds.pop();
        stakeInfo.stakingTimestamps.pop();
    }

    // FUNCTION TO CHECK THE OWNED NFTs OF A USER (STAKED AND UNSTAKED)
    function getOwnedTokens(address user, uint256 _contractIndex)
        external
        view
        returns (uint256[] memory, uint256[] memory)
    {
        IERC721A stakeableNFT = stakeableNFTs[_contractIndex];
        uint256 totalSupply = stakeableNFT.totalSupply();
        uint256[] memory tempUnstaked = new uint256[](totalSupply);
        uint256 unstakedIndex = 0;

        for (uint256 i = 1; i <= totalSupply; i++) {
            if (_tokenExists(stakeableNFT, i)) {
                address owner = stakeableNFT.ownerOf(i);

                if (owner == user) {
                    tempUnstaked[unstakedIndex] = i;
                    unstakedIndex++;
                }
            }
        }

        uint256[] memory staked = stakerInfo[user][_contractIndex].tokenIds;
        uint256[] memory unstaked = new uint256[](unstakedIndex);

        for (uint256 i = 0; i < unstakedIndex; i++) {
            unstaked[i] = tempUnstaked[i];
        }

        return (staked, unstaked);
    }

    function _tokenExists(IERC721A nft, uint256 tokenId)
        internal
        view
        returns (bool)
    {
        try nft.ownerOf(tokenId) returns (address) {
            return true;
        } catch {
            return false;
        }
    }

    function setContractRule(
        uint256 _contract,
        uint256 _limit1,
        uint256 _limit2,
        uint256 _reward1,
        uint256 _reward2
    ) external onlyOwner {
        contractRules[_contract] = ContractRule(
            _limit1,
            _limit2,
            _reward1 * 10**18,
            _reward2 * 10**18
        );
    }

    function updateStakeableNFTs(uint256 _index, IERC721A _newNFT)
        external
        onlyOwner
    {
        if (_index < stakeableNFTs.length) {
            stakeableNFTs[_index] = _newNFT;
        } else {
            stakeableNFTs.push(_newNFT);
        }
    }
    
    function setStep(uint256 _step) external onlyOwner {
        Steps = Step(_step);
    }

    function checkTokenBalance() external view returns (uint256) {
        return rewardToken.balanceOf(address(this));
    }

    function withdrawRewardToken() external onlyOwner {
        rewardToken.transfer(msg.sender, rewardToken.balanceOf(address(this)));
    }
}