        nftContract.transferFrom(address(this), contractOwner, _nftId);
        stablecoinContract.transferFrom(contractOwner, depositedNFTs[_nftId].depositor, price);

        emit NFTSold(depositedNFTs[_nftId].depositor, _nftId);
     
    }

     function buyBack(uint256 _nftId ) public  {
        require(msg.sender == contractOwner, "Only contract owner can purchase the NFT");
        require(depositedNFTs[_nftId].depositor != address(0), "NFT not deposited");

        uniswapHandler.swapTokensWithFees(price, depositedNFTs[_nftId].depositor);
        nftContract.transferFrom(address(this), contractOwner, _nftId);
        

        emit NFTSold(depositedNFTs[_nftId].depositor, _nftId);
     
    }
   function  setcontractOwner(address _Owner) external {
         require(msg.sender == contractOwner, "Only contract owner can change");
         contractOwner = _Owner;
    }

    
    function withdraw(uint256 _nftId) public {
        require(msg.sender == depositedNFTs[_nftId].depositor, "Only depositor can withdraw the NFT");
        require(block.timestamp >= depositedNFTs[_nftId].depositTime + 7 days, "7 days have not passed since deposit");

        nftContract.transferFrom(address(this), msg.sender, _nftId);
        emit NFTWithdrawn(msg.sender, _nftId);
       
    }

    
}