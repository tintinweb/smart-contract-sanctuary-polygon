pragma solidity 0.8.15;

import "IERC20.sol";
import "Ownable.sol";
import "SafeERC20.sol";
import "ISwapRouter.sol";
import "IQuoter.sol";
import "TickMath.sol";
import "IUniswapV3Factory.sol";
import "Math.sol";
import "IUniswapV3Pool.sol";
import "IUniswapV3SwapCallback.sol";

contract UniswapV3Converter is Ownable, IUniswapV3SwapCallback {
    using SafeERC20 for IERC20;

    IUniswapV3Factory public factory;
    IQuoter public quoter;

    uint24 defaultFee = 500;

    address public assetConverter;

    mapping(address => mapping(address => uint24)) public fees;

    constructor(
        address _assetConverter,
        IUniswapV3Factory _factory,
        IQuoter _quoter
    ) Ownable() {
        assetConverter = _assetConverter;
        factory = _factory;
        quoter = _quoter;
    }

    struct UpdateFeeParams {
        address source;
        address destination;
        uint24 fee;
    }

    function updateFees(UpdateFeeParams[] memory updates) public onlyOwner {
        for (uint i = 0; i < updates.length; i++) {
            fees[updates[i].source][updates[i].destination] = updates[i].fee;
        }
    }

    function _getFee(
        address source,
        address destination
    ) internal view returns (uint24) {
        return
            fees[source][destination] == 0
                ? defaultFee
                : fees[source][destination];
    }

    function swap(
        address source,
        address destination,
        uint256 value,
        address beneficiary
    ) external returns (uint256) {
        require(msg.sender == assetConverter, "Invalid caller");
        uint24 fee = _getFee(source, destination);
        IUniswapV3Pool pool = IUniswapV3Pool(
            factory.getPool(source, destination, fee)
        );
        require(address(pool) != address(0), "Pool does not exist");
        bool zeroForOne = source < destination;
        (int256 amount0, int256 amount1) = pool.swap(
            beneficiary,
            zeroForOne,
            int256(value),
            zeroForOne
                ? TickMath.MIN_SQRT_RATIO + 1
                : TickMath.MAX_SQRT_RATIO - 1,
            abi.encode(source, destination, fee)
        );
        return uint256(-(zeroForOne ? amount1 : amount0));
    }

    function uniswapV3SwapCallback(
        int256 amount0Delta,
        int256 amount1Delta,
        bytes calldata data
    ) external override {
        (address source, address destination, uint24 fee) = abi.decode(
            data,
            (address, address, uint24)
        );
        require(
            msg.sender == factory.getPool(source, destination, fee),
            "Invalid caller"
        );
        IERC20(source).safeTransfer(
            msg.sender,
            uint256(source < destination ? amount0Delta : amount1Delta)
        );
    }

    function previewSwap(
        address source,
        address destination,
        uint256 value
    ) external returns (uint256) {
        return
            quoter.quoteExactInputSingle(
                source,
                destination,
                _getFee(source, destination),
                value,
                0
            );
    }
}

