        address oldAddress,
        address newAddress,
        DaoType oldDaoType,
        DaoType newDaoType
    );



    function initialize(
        ERC1155Creator _tokenAddress,
        address _daoAddress,
        DaoType _daoType
    ) public initializer{
        __AccessControlEnumerable_init();

        tokenAddress = _tokenAddress;
        daoAddress = _daoAddress;
        daoType = _daoType;


        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

        emit RevolutionOpenCreated(
            address(tokenAddress),
            daoAddress,
            daoType
        );
    }


    /**
        * @dev Creates a new drop. Percent units in bps (1/100 of a percent). governanceSplitPercents and splitPercents must each add up to 10000 (100%). 
        * @param uri URI for the drop
        * @param dropStartTime Start timestamp for the drop
        * @param dropEndTime End timestamp for the drop
        * @param dropPrice Price for each edition
        * @param splitAddresses Addresses to split the drop proceeds between
        * @param splitPercents Percentages to split the drop proceeds between
        * @param daoSplit Percentage of the proceeds to go toward the dao and governance emission 
        * @param governanceSplitPercents Percentages to split the drop between
     */
    function createDrop(
        string memory uri,
        uint256 dropStartTime,
        uint256 dropEndTime,
        uint256 dropPrice,
        address[] memory splitAddresses,
        uint16[] memory splitPercents,
        uint16 daoSplit,
        uint16[] memory governanceSplitPercents
    ) public returns (uint256) {
        require(bytes(uri).length > 0, "URI must not be empty");
        //require(dropTime > 0, "Drop time must be greater than 0");
        //require(creator != address(0x0), "Creator must not be 0x0");
        require(daoSplit >= minDaoSplit, "DAO split too small");
        require(splitAddresses.length == splitPercents.length && splitAddresses.length == governanceSplitPercents.length, "array lengths do not match");
        //make sure splitPercents and daosplit all add up to 10000
        uint16 totalSplit = 0;
        for (uint16 i = 0; i < splitPercents.length; i++) {
            totalSplit += splitPercents[i];
        }
        require(totalSplit == 10000, "Split percentages must add up to 10000");

        totalSplit = 0;
        for (uint16 i = 0; i < governanceSplitPercents.length; i++) {
            totalSplit += governanceSplitPercents[i];
        }
        require(totalSplit == 10000, "Governance split percentages must add up to 10000");

        address[] memory receivers = new address[](1);
        receivers[0] = msg.sender;
        uint256[] memory amounts = new uint256[](1);
        amounts[0] = 0;
        string[] memory uris = new string[](1);
        uris[0] = uri;

        uint256 tokenId = tokenAddress.mintBaseNew(receivers, amounts, uris)[0];
        drops[tokenId] = Drop(
            dropStartTime,
            dropEndTime,
            dropPrice,
            splitAddresses,
            splitPercents,
            daoSplit,
            governanceSplitPercents
        );
        emitDropCreatedEvent(tokenId, uri, dropStartTime, dropEndTime, dropPrice, splitAddresses, splitPercents, daoSplit, governanceSplitPercents);
        return tokenId;
    }

    //because of stack too deep error
    function emitDropCreatedEvent(uint256 tokenId, string memory uri, uint256 dropStartTime, uint256 dropEndTime, uint256 dropPrice, address[] memory splitAddresses, uint16[] memory splitPercents, uint16 daoSplit, uint16[] memory governanceSplitPercents) internal {
        emit DropCreated(
            address(tokenAddress),
            tokenId,
            uri,
            dropStartTime,
            dropEndTime,
            dropPrice,
            splitAddresses,
            splitPercents,
            daoSplit,
            governanceSplitPercents
        );
    }
    //function to buy a drop
    function buyDrop(
        uint256 tokenId,
        uint256 amount,
        address to
    ) public nonReentrant payable {
        //require drop exists
        require(drops[tokenId].dropEndTime != 0, "Drop does not exist");
        require(
            msg.value == drops[tokenId].dropPrice * amount,
            "Incorrect amount sent"
        );
        //require current time to be greater than drop start time 
        require(
            block.timestamp > drops[tokenId].dropStartTime,
            "Drop has not started yet"
        );
        //require current time to be less than drop time
        require(
            block.timestamp < drops[tokenId].dropEndTime,
            "Drop has already ended"
        );
        //require amount to be greater than 0
        require(amount > 0, "Amount must be greater than 0");

        if (to == address(0x0)) {
            to = msg.sender;
        }

        address[] memory receivers = new address[](1);
        receivers[0] = to;
        uint256[] memory amounts = new uint256[](1);
        amounts[0] = amount;
        uint256[] memory tokenIds = new uint256[](1);
        tokenIds[0] = tokenId;

        tokenAddress.mintBaseExisting(receivers, tokenIds, amounts);

        uint256 revolutionFee = (msg.value * revolutionPercentFee) / 10000;
        uint256 remainingAmount = msg.value - revolutionFee;
        uint256 daoAmount = (remainingAmount * drops[tokenId].daoSplit) / 10000;

        remainingAmount -= daoAmount;
        //send funds to drops.splitAddresses
        uint256 totalSentToAddresses = 0;
        for (uint256 i = 0; i < drops[tokenId].splitAddresses.length; i++) {
            (bool splitTransferSuccess, ) = drops[tokenId]
                .splitAddresses[i]
                .call{value: (remainingAmount * drops[tokenId].splitPercents[i]) / 10000}("");
            require(splitTransferSuccess, "Transfer to split address failed.");
            totalSentToAddresses +=
                (remainingAmount * drops[tokenId].splitPercents[i]) /
                10000;
        }

        daoAmount += remainingAmount - totalSentToAddresses;


        handleDaoPayment(tokenId, daoAmount, revolutionFee, to);
        

        emit DropPurchased(address(tokenAddress), tokenId, amount, to);
    }
    function handleDaoPayment(uint256 tokenId, uint256 daoAmount, uint256 revolutionFee, address to) internal{
        if (daoType == DaoType.REVOLUTION_VRGDA) {
            address[] memory vrgdaReceivers = new address[](1 + drops[tokenId].splitAddresses.length);
            for (uint256 i = 0; i < drops[tokenId].splitAddresses.length; i++) {
                vrgdaReceivers[i] = drops[tokenId].splitAddresses[i];
            }
            vrgdaReceivers[drops[tokenId].splitAddresses.length] = to;

            uint256[] memory vrgdaSplits = new uint256[](1 + drops[tokenId].splitAddresses.length);
            uint16 totalPercent = 0;
            for (uint256 i = 0; i < drops[tokenId].governanceSplitPercents.length; i++) {
                vrgdaSplits[i] = uint16((uint256(drops[tokenId].governanceSplitPercents[i]) * uint256(buyerGovernanceSplit))/10000/100);
                totalPercent += uint16((uint256(drops[tokenId].governanceSplitPercents[i]) * uint256(buyerGovernanceSplit))/10000/100);
            }
            vrgdaSplits[drops[tokenId].splitAddresses.length] = 100 - totalPercent;
            
            ITokenEmitter(daoAddress).buyToken{value: daoAmount}(
                vrgdaReceivers,
                vrgdaSplits
            );
        } else if (daoType == DaoType.SIMPLE_TREASURY) {
            (bool daoTransferSuccess, ) = daoAddress.call{value: daoAmount}("");
            require(daoTransferSuccess, "Transfer to DAO treasury failed.");
        }

        (bool revolutionTransferSuccess, ) = revolutionTreasury.call{value: revolutionFee}("");
        require(revolutionTransferSuccess, "Transfer to revolution treasury failed.");
    }
    function getDrop(uint256 tokenId) public view returns (Drop memory) {
        return drops[tokenId];
    }

