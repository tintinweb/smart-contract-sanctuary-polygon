pragma solidity ^0.8.15;

import "../../../interfaces/ISiloManager.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";
import "../../../interfaces/ISiloFactory.sol";
import "../../../interfaces/ISilo.sol";
import "./interfaces/IAutoCreator.sol";

contract SiloManagerFactoryV2 is Ownable {
    using EnumerableMap for EnumerableMap.UintToAddressMap;

    EnumerableMap.UintToAddressMap private autoTypes;

    address public siloFactory;

    mapping(address => mapping(uint => address)) public userToManager;

    mapping(uint256 => uint256) public managerCount;

    uint96 public riskBuffer = 15000; //based off a number 10000 -> ∞
    uint96 public rejoinBuffer = 30000;
    uint96 public bufferPerSilo = 750;

    event SiloManagerCreate(address owner, address manager, uint256 upkeepId);

    event SiloManagerCancel(address owner, address manager, uint256 upkeepId);

    event SiloManagerFund(
        address owner,
        address manager,
        uint256 upkeepId,
        uint256 amount
    );

    event SiloManagerWithdraw(
        address owner,
        address manager,
        uint256 upkeepId,
        uint256 amount
    );

    constructor(address _siloFactory) {
        siloFactory = _siloFactory;
    }

    function addAutoType(address _creator) external onlyOwner {
        uint256 _autoType = IAutoCreator(_creator).autoType();
        autoTypes.set(_autoType, _creator);
    }

    function setAutoType(
        uint256 _autoType,
        address _creator
    ) external onlyOwner {
        autoTypes.set(_autoType, _creator);
    }

    function removeAutoType(uint256 _autoType) external onlyOwner {
        autoTypes.remove(_autoType);
    }

    function udpateSiloFactory(address _siloFactory) external onlyOwner {
        siloFactory = _siloFactory;
    }

    function adjustSiloBuffer(uint96 _buffer) external onlyOwner {
        bufferPerSilo = _buffer;
    }

    function adjustRiskBuffer(uint96 _buffer) external onlyOwner {
        require(_buffer > 10000, "Risk Buffer not valid");
        riskBuffer = _buffer;
    }

    function adjustRejoinBuffer(uint96 _buffer) external onlyOwner {
        require(_buffer > 10000, "Rejoin Buffer not valid");
        rejoinBuffer = _buffer;
    }

    function getAutoCreator(uint256 _autoType) public view returns (address) {
        return autoTypes.get(_autoType);
    }

    function getAutoTypesSize() public view returns (uint256) {
        return autoTypes.length();
    }

    function getAutoTypeAt(
        uint256 index
    ) public view returns (uint256 autoType, address creator) {
        return autoTypes.at(index);
    }

    function autoTypeInfo()
        public
        view
        returns (uint256[] memory autos, address[] memory creators)
    {
        uint size = autoTypes.length();
        creators = new address[](size);
        autos = new uint256[](size);

        for (uint i = 0; i < size; ) {
            (uint256 autoType, address creator) = autoTypes.at(i);
            autos[i] = autoType;
            creators[i] = creator;

            unchecked {
                i++;
            }
        }
    }

    function checkManager(
        address _owner,
        address _manager,
        uint256 _autoType
    ) public view returns (bool) {
        address manager = userToManager[_owner][_autoType];

        if (_manager != address(0) && manager == _manager) {
            return true;
        }

        return false;
    }

    function createAutoManager(
        uint256 autoType,
        uint256 _amount
    ) public payable {
        address creator = autoTypes.get(autoType);
        require(creator != address(0), "wrong creator");

        mapping(uint256 => address) storage managers = userToManager[
            msg.sender
        ];

        require(managers[autoType] == address(0), "already manager");

        bytes memory inputData = abi.encode(_amount, msg.sender);

        (bool success, bytes memory data) = payable(creator).call{
            value: msg.value
        }(abi.encodeWithSignature("createAutoManager(bytes)", inputData));

        require(success, "error create manager");
        address newManager = abi.decode(data, (address));

        managers[autoType] = newManager;

        managerCount[autoType] += 1;
    }

    function addFund(uint autoType, uint256 _amount) public payable {
        address creator = autoTypes.get(autoType);
        require(creator != address(0), "wrong creator");

        address manager = userToManager[msg.sender][autoType];

        require(manager != address(0), "no manager");

        bytes memory inputData = abi.encode(manager, _amount);

        (bool success, ) = payable(creator).call{value: msg.value}(
            abi.encodeWithSignature("addFund(bytes)", inputData)
        );

        require(success, "error add fund");
    }

    function cancelAuto(uint autoType) public {
        address creator = autoTypes.get(autoType);
        require(creator != address(0), "wrong creator");

        mapping(uint256 => address) storage managers = userToManager[
            msg.sender
        ];

        require(managers[autoType] != address(0), "no manager");

        bool checkCancel = possibleCancel(msg.sender, autoType);

        require(checkCancel, "since deposited high risk silo");

        bytes memory inputData = abi.encode(managers[autoType]);

        (bool success, bytes memory data) = creator.call(
            abi.encodeWithSignature("cancelAuto(bytes)", inputData)
        );

        require(success, "error cancel manager");

        bool _flag = abi.decode(data, (bool));

        if (_flag) {
            managers[autoType] = address(0);
        }
    }

    function withdrawFund(uint autoType, bool _type) public {
        address creator = autoTypes.get(autoType);
        require(creator != address(0), "wrong creator");

        mapping(uint256 => address) storage managers = userToManager[
            msg.sender
        ];

        require(managers[autoType] != address(0), "no manager");

        bytes memory inputData = abi.encode(managers[autoType], _type);

        (bool success, bytes memory data) = creator.call(
            abi.encodeWithSignature("withdrawFund(bytes)", inputData)
        );

        require(success, "error withdrawFund");

        bool _flag = abi.decode(data, (bool));

        if (_flag) {
            managers[autoType] = address(0);
        }
    }

    function setUserRiskBuffer(uint96 _buffer) external {
        require(_buffer >= 10000, "Risk Buffer not valid");
        address manager = userToManager[msg.sender][1];
        require(manager != address(0), "User does not have a manager");
        uint256 id = ISiloManager(manager).upkeepId();
        require(id != 0, "Manager not approved");
        ISiloManager(manager).setCustomRiskBuffer(_buffer);
    }

    function setUserRejoinBuffer(uint96 _buffer) external {
        require(_buffer >= 10000, "Rejoin Buffer not valid");
        address manager = userToManager[msg.sender][1];
        require(manager != address(0), "User does not have a manager");
        uint256 id = ISiloManager(manager).upkeepId();
        require(id != 0, "Manager not approved");
        ISiloManager(manager).setCustomRejoinBuffer(_buffer);
    }

    function possibleCancel(
        address _user,
        uint256 autoType
    ) public view returns (bool) {
        uint256 siloID;
        ISilo Silo;
        ISiloFactory SiloFactory = ISiloFactory(siloFactory);
        uint256 balance = SiloFactory.balanceOf(_user);
        for (uint256 i; i < balance; ) {
            siloID = SiloFactory.tokenOfOwnerByIndex(_user, i);
            Silo = ISilo(SiloFactory.siloMap(siloID));
            if (Silo.highRiskAction() && Silo.autoType() == autoType) {
                if (Silo.deposited()) {
                    return false;
                }
            }
            unchecked {
                i++;
            }
        }
        return true;
    }

    function getAutoStatus(
        address _user,
        uint256 _autoType
    ) public view returns (AutoStatus) {
        address manager = userToManager[_user][_autoType];

        if (manager != address(0)) {
            address creator = getAutoCreator(_autoType);

            bool approved = IAutoCreator(creator).managerApproved(_user);

            if (!approved) {
                return AutoStatus.NOT;
            }

            uint256 balance = IAutoCreator(creator).getAutoManagerBalance(
                manager
            );

            uint256 min = IAutoCreator(creator).getAutoMinThreshold(manager);

            uint256 highRisk = (min * ISiloManager(manager).getRiskBuffer()) /
                uint96(10000);

            if (balance >= highRisk) {
                return AutoStatus.HIGH;
            } else if (balance >= min) {
                return AutoStatus.NORMAL;
            } else if (balance < min) {
                return AutoStatus.MANUAL;
            }
            return AutoStatus.APPROVED;
        }

        return AutoStatus.NOT;
    }
}

