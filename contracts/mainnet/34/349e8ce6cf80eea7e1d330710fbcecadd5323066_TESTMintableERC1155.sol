    constructor(string memory uri_, address childChainManager) //DNFT,( 0xb5505a6d998549090530911180f38ac5130101c6(testnet) , 0xA6FA4fB5f76172d178d61B04b0ecd319C5d1C0aa(mainnet))
        ERC1155(uri_)
    {
        _setupContractId("ChildERC1155");
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(DEPOSITOR_ROLE, childChainManager);
        admin =_msgSender();
        _initializeEIP712(uri_);
        
        setChainlinkToken(0xb0897686c545045aFc77CF20eC7A532E3120E0F1); //LINK mainnet Address
        oracle = 0x188b71C9d27cDeE01B9b0dfF5C1aff62E8D6F434;
            setChainlinkOracle(oracle);
        
        jobId = "a84b561bd8f64300a0832682f208321f";
        fee = 0.07 * 10 ** 18; // (Varies by network and job)
    }

    // This is to support Native meta transactions
    // never use msg.sender directly, use _msgSender() instead
    function _msgSender()
        internal
        override
        view
        returns (address  sender)
    {
        return ContextMixin.msgSender();
    }


 
function OraclelinkFee(address tokenaddress, uint256 _fee, uint256 decimal, address _oracle, string memory _jobId, string memory _path, string memory _url) external only(DEFAULT_ADMIN_ROLE){
        setChainlinkToken(tokenaddress); //LINK mainnet Address
       oracle = _oracle;
        setChainlinkOracle(_oracle);
        jobId = bytes32(bytes(_jobId));
        fee = _fee * 10 ** decimal; 
        path = _path;
        url = _url;
    }


    
    function msgSenderS()
        external
        
        view
        returns (address  sender)
    {
       return _msgSender();
    }

    /**
     * @notice called when tokens are deposited on root chain
     * @dev Should be callable only by ChildChainManager
     * Should handle deposit by minting the required tokens for user
     * Make sure minting is done only by this function
     * @param user user address for whom deposit is being done
     * @param depositData abi encoded ids array and amounts array
     */
    function deposit(address user, bytes calldata depositData)
        external
        override
        only(DEPOSITOR_ROLE)
        nonReentrant()
    {
        (
            uint256[] memory ids,
            uint256[] memory amounts,
            bytes memory data
        ) = abi.decode(depositData, (uint256[], uint256[], bytes));

        require(
            user != address(0),
            "ChildMintableERC1155: INVALID_DEPOSIT_USER"
        );

        _mintBatch(user, ids, amounts, data);
    }
 /**
     * Create a Chainlink request to retrieve API response, find the target
     * data, then multiply by 1000000000000000000 (to remove decimal places from data).
     */
    function UpdateURI(uint256 tokenid) public only(DEFAULT_ADMIN_ROLE) returns (bytes32 requestId)  
    {
     UpdateId = tokenid;
        
        
        Chainlink.Request memory request = buildChainlinkRequest(jobId, address(this), this.fulfillrequest.selector); 
        // Set the URL to perform the GET request on
        request.add("get",  string(abi.encodePacked(bytes(url),'',bytes(Strings.toString(tokenid)))));
       
        request.add("path", path);
        
        return sendOperatorRequest( request, fee);

    }
    //0x885e39866ca644DCF32f785f8B86Fb074f41CcAc
    
    /**
     * Receive the response in the form of uint256
     */ 
    function fulfillrequest(bytes32 _requestId,  bytes memory  bytesData) public recordChainlinkFulfillment(_requestId) 
    {
        
        
          string memory data = string(abi.encodePacked(bytes(baseURI),'',bytesData));
            uint256 index = tokenversion[UpdateId]+1;
        tokenversion[UpdateId] = index;  
        versionURI[UpdateId][index] = string(data);
           _setURI(UpdateId, data);
           UpdateId = 0;

        }
        
        

    

    function bytes32ToString(bytes32 _bytes32) internal pure returns (string memory)  {
        uint8 i = 0;
        while(i < 32 && _bytes32[i] != 0) {
            i++;
        }
        bytes memory bytesArray = new bytes(i);
        for (i = 0; i < 32 && _bytes32[i] != 0; i++) {
            bytesArray[i] = _bytes32[i];
        }
        return string(bytesArray);
    }

    function setBaseURL(string memory _url)
        public only(DEFAULT_ADMIN_ROLE)   
        
    {
        
        baseURI = string(abi.encodePacked(_url));
 
    }



 

  function OldVersionMag(uint256 tokenid, uint256 version) public  view returns(string memory)    {
        require(msg.sender!= address(0) && balanceOf(msg.sender ,tokenid)>0 , "Not Owner");

        return versionURI[tokenid][version];
        
    }


  function BuyRestrictioncheck(uint256 tokenid, address useraddress) public  view returns(bool)    { 
        require(useraddress!= address(0) && msg.sender == admin, "Access Denied for Null Address");
        if (buyLimits[tokenid][useraddress] == true && balanceOf(useraddress,tokenid)>0) {
            return false;
        }
        else {
            return true;
        }

        
        
    }

  function totalVersionMag(uint256 tokenid) public  view returns(uint256 )    {

        return tokenversion[tokenid];
        
    }

    /**
     * @notice called when user wants to withdraw single token back to root chain
     * @dev Should burn user's tokens. This transaction will be verified when exiting on root chain
     * @param id id to withdraw
     * @param amount amount to withdraw
     */
    function withdrawSingle(uint256 id, uint256 amount) external  nonReentrant(){
        _burn(_msgSender(), id, amount);
    }

    /**
     * @notice called when user wants to batch withdraw tokens back to root chain
     * @dev Should burn user's tokens. This transaction will be verified when exiting on root chain
     * @param ids ids to withdraw
     * @param amounts amounts to withdraw
     */
    function withdrawBatch(uint256[] calldata ids, uint256[] calldata amounts )
        external nonReentrant()
    {
        _burnBatch(_msgSender(), ids, amounts);
    }

    /**
     * @notice See definition of `_mint` in ERC1155 contract
     * @dev This implementation only allows admins to mint tokens
     * but can be changed as per requirement
     */
    function mint(
        address account,
        uint256 id,
        uint256 amount,
        bytes calldata data
    ) external only(DEFAULT_ADMIN_ROLE)  {
        _mint(account, id, amount, data);
        
    }

    /**
     * @notice See definition of `_mintBatch` in ERC1155 contract
     * @dev This implementation only allows admins to mint tokens
     * but can be changed as per requirement
     */
    function mintBatch(
        address to,
        uint256[] calldata ids,
        uint256[] calldata amounts,
        bytes calldata data
    ) external only(DEFAULT_ADMIN_ROLE) nonReentrant(){
        _mintBatch(to, ids, amounts, data);
    }
}