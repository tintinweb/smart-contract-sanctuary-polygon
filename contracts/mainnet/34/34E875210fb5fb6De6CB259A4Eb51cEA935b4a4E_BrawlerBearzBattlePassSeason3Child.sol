/**************************************************
 * @title BrawlerBearzBattlePassSeason3Child
 * @author @scottybmitch
 **************************************************/

contract BrawlerBearzBattlePassSeason3Child is
    IBrawlerBearzBattlePassSeason3Child,
    IBrawlerBearzBattlePassEvents,
    FxBaseChildTunnel,
    ERC2771Context,
    AccessControl,
    NativeMetaTransaction,
    ReentrancyGuard
{
    /// @dev Sync actions
    bytes32 public constant MINTED = keccak256("MINTED");
    bytes32 public constant REWARDS_CLAIM = keccak256("REWARDS_CLAIM");

    /// @dev Roles
    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");
    bytes32 public constant BATTLE_PASS_MANAGER =
        keccak256("BATTLE_PASS_MANAGER");

    // @dev Flag to be able to claim rewards
    bool public canClaim = false;

    /// @dev Token id to battle pass info
    mapping(uint256 => BattlePass) public passes;

    /// @dev Pass id to rewards structure
    mapping(uint256 => BattlePassMetadata) public passesMetadata;

    /// @dev Address to reward items
    mapping(address => uint256[]) public rewardsToClaim;

    /// @dev Address to owned pass token ids
    mapping(address => uint256[]) public ownedPasses;

    constructor(address _fxChild, address _trustedForwarder)
        FxBaseChildTunnel(_fxChild)
        ERC2771Context(_trustedForwarder)
    {
        // Setup roles
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(OWNER_ROLE, _msgSender());
        _setupRole(BATTLE_PASS_MANAGER, _msgSender());
    }

    // ========================================
    // Reward integrations
    // ========================================

    /**
     * @notice Claims all eligible pass items
     * @param _address - The address of claimer
     * @param _tokenId - The tokenId to claim
     */
    function claimBattlePassRewards(address _address, uint256 _tokenId) public {
        require(canClaim, "!claim");
        BattlePass storage currentPass = passes[_tokenId];
        for (uint256 i = 0; i < currentPass.itemIds.length; i++) {
            rewardsToClaim[_address].push(currentPass.itemIds[i]);
        }
    }

    /**
     * @notice Claims all eligible pass items
     * @param _address - The address of claimer
     */
    function claimAllBattlePassRewards(address _address) external {
        uint256[] storage battlePasses = ownedPasses[_address];
        uint256 passTokenId;
        BattlePass memory currentPass;
        for (uint256 i = 0; i < battlePasses.length; i++) {
            passTokenId = battlePasses[i];
            claimBattlePassRewards(_address, passTokenId);
        }
    }

    /**
     * @notice Gets the current rewards the caller can claim
     * @param _address - The address of claimer
     */
    function getClaimableRewards(address _address)
        external
        view
        returns (uint256[] memory)
    {
        return rewardsToClaim[_address];
    }

    // ========================================
    // Pass ownership
    // ========================================

    /**
     * @notice Tricks collab.land and other ERC721 balance checkers
     * @param _address - the user to get the balance of.
     */
    function balanceOf(address _address) external view returns (uint256) {
        return ownedPasses[_address].length;
    }

    /**
     * @dev Returns pass token ids
     * @param _address - address to lookup
     */
    function getPassOwnerTokens(address _address)
        external
        view
        returns (uint256[] memory)
    {
        return ownedPasses[_address];
    }

    /**
     * @dev Returns pass for a given token id
     * @param _tokenId - The token id of the battle pass
     */
    function getBattlePass(uint256 _tokenId)
        external
        view
        returns (BattlePass memory)
    {
        return passes[_tokenId];
    }

    /**
     * @notice Decrease battle pass tournament entries
     * @param _tokenId The token id of the battle pass
     * @param _amount The amount to decrease tournament entries
     */
    function decreaseBattlePassEntries(uint256 _tokenId, uint256 _amount)
        external
        onlyRole(BATTLE_PASS_MANAGER)
    {
        require(
            _amount <= passes[_tokenId].tournamentEntries,
            "Not enough tournament entries"
        );
        passes[_tokenId].tournamentEntries -= _amount;
    }

    // ========================================
    // Internals
    // ========================================

    /**
     * @dev Persists the tracking data for the battle pass
     * @param user - a user address
     * @param baseTokenId - a token id
     * @param passType - a pass type
     * @param amount - the amount of passes minted
     */
    function _sync(
        address user,
        uint256 baseTokenId,
        uint256 passType,
        uint256 amount
    ) internal {
        for (uint256 i; i < amount; ) {
            uint256 tokenId = baseTokenId + i;

            // Add pass to owner
            ownedPasses[user].push(tokenId);

            // Set up battle pass metadata with snapshot of the metadata
            // Generate random number
            uint256 seed = getPseudoRandomNumber(tokenId);

            // Generate int array equal to length addresses and randomly shuffle it with entropy
            uint256[] memory shuffledCommon = shuffle(
                passesMetadata[passType].commonIds.length - 1,
                seed % 100000
            );

            uint256[] memory shuffledRares = shuffle(
                passesMetadata[passType].rareIds.length - 1,
                seed % 10000
            );

            uint256[] memory shuffledEpics = shuffle(
                passesMetadata[passType].epicIds.length - 1,
                seed % 1000
            );

            uint256[] memory shuffledLegendaries = shuffle(
                passesMetadata[passType].legendaryIds.length - 1,
                seed % 100
            );

            /*
                Common Items (6x)
                Rare Items (3x)
                Epic Items (2x)
                Legendary or Epic Item (1x)
            */

            uint256[] memory itemIds = new uint256[](12);

            uint256 c = 0;
            uint256 r = 0;
            uint256 e = 0;

            // Get commons
            for (; c < 6; ) {
                itemIds[c] = passesMetadata[passType].commonIds[
                    shuffledCommon[c]
                ];
                unchecked {
                    ++c;
                }
            }

            // Get rares
            for (; r < 3; ) {
                itemIds[c + r] = passesMetadata[passType].rareIds[
                    shuffledRares[r]
                ];
                unchecked {
                    ++r;
                }
            }

            // Get epics
            for (; e < 2; ) {
                itemIds[c + r + e] = passesMetadata[passType].epicIds[
                    shuffledEpics[e]
                ];
                unchecked {
                    ++e;
                }
            }

            // Get legendary or epic
            itemIds[c + r + e] = passesMetadata[passType].legendaryIds[
                shuffledLegendaries[0]
            ];

            // Derive individualized item set based on base configuration
            passes[tokenId] = BattlePass({
                typeOf: passType,
                createdAt: block.timestamp,
                itemIds: itemIds,
                tournamentEntries: 3
            });

            unchecked {
                ++i;
            }
        }
    }

    function shuffle(uint256 size, uint256 entropy)
        public
        pure
        returns (uint256[] memory)
    {
        uint256[] memory result = new uint256[](size);

        // Initialize array.
        for (uint256 i = 0; i < size; i++) {
            result[i] = i + 1;
        }

        // Set the initial randomness based on the provided entropy.
        bytes32 random = keccak256(abi.encodePacked(entropy));

        // Set the last item of the array which will be swapped.
        uint256 last_item = size - 1;

        // We need to do `size - 1` iterations to completely shuffle the array.
        for (uint256 i = 1; i < size - 1; i++) {
            // Select a number based on the randomness.
            uint256 selected_item = uint256(random) % last_item;

            // Swap items `selected_item <> last_item`.
            uint256 aux = result[last_item];
            result[last_item] = result[selected_item];
            result[selected_item] = aux;

            // Decrease the size of the possible shuffle
            // to preserve the already shuffled items.
            // The already shuffled items are at the end of the array.
            last_item--;

            // Generate new randomness.
            random = keccak256(abi.encodePacked(random));
        }

        return result;
    }

    function getPseudoRandomNumber(uint256 tokenId)
        private
        view
        returns (uint256)
    {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        block.number,
                        block.difficulty,
                        block.timestamp,
                        tokenId
                    )
                )
            );
    }

    // ========================================
    // Portal integrations
    // ========================================

    /**
     * @notice Sets/updates the address for the root tunnel
     * @param _fxRootTunnel - the fxRootTunnel address
     */
    function setFxRootTunnel(address _fxRootTunnel)
        external
        override
        onlyRole(OWNER_ROLE)
    {
        fxRootTunnel = _fxRootTunnel;
    }

    /**
     * @notice Sets/updates whether claiming is live
     * @param _canClaim - the toggle value
     */
    function setCanClaim(bool _canClaim)
        external
        override
        onlyRole(OWNER_ROLE)
    {
        canClaim = _canClaim;
    }

    /// @notice withdraws claimable rewards (ETH)
    function withdrawRewards() external nonReentrant {
        address to = _msgSender();
        uint256[] storage rewards = rewardsToClaim[to];
        _sendMessageToRoot(abi.encode(REWARDS_CLAIM, abi.encode(to, rewards)));
        delete rewardsToClaim[to];
    }

    /// @dev TEST
    function _processMessageTest(
        uint256,
        address sender,
        bytes memory message
    ) external onlyRole(OWNER_ROLE) {
        (bytes32 syncType, bytes memory syncData) = abi.decode(
            message,
            (bytes32, bytes)
        );
        if (syncType == MINTED) {
            (
                address from,
                uint256 tokenId,
                uint256 passType,
                uint256 amount
            ) = abi.decode(syncData, (address, uint256, uint256, uint256));
            _sync(from, tokenId, passType, amount);
        } else {
            revert("INVALID_SYNC_TYPE");
        }
    }

    /**
     * @notice Process message received from FxChild
     * @param sender root message sender
     * @param message bytes message that was sent from Root Tunnel
     */
    function _processMessageFromRoot(
        uint256,
        address sender,
        bytes memory message
    ) internal override validateSender(sender) {
        (bytes32 syncType, bytes memory syncData) = abi.decode(
            message,
            (bytes32, bytes)
        );
        if (syncType == MINTED) {
            (
                address from,
                uint256 tokenId,
                uint256 passType,
                uint256 amount
            ) = abi.decode(syncData, (address, uint256, uint256, uint256));
            _sync(from, tokenId, passType, amount);
        } else {
            revert("INVALID_SYNC_TYPE");
        }
    }

    // ========================================
    // Native meta transactions
    // ========================================

    function _msgSender()
        internal
        view
        virtual
        override(ERC2771Context, Context)
        returns (address)
    {
        return ERC2771Context._msgSender();
    }

    function _msgData()
        internal
        view
        virtual
        override(ERC2771Context, Context)
        returns (bytes calldata)
    {
        return ERC2771Context._msgData();
    }

    // ========================================
    // Admin Management
    // ========================================

    /**
     * @notice MANUAL OVERRIDE - Set battle pass
     * @param _pass The battle pass settings
     */
    function manualBattlePassOverride(uint256 _tokenId, BattlePass memory _pass)
        external
        onlyRole(BATTLE_PASS_MANAGER)
    {
        passes[_tokenId] = _pass;
    }

    /**
     * @notice MANUAL OVERRIDE - Sets the current rewards the caller can claim
     * @param _address - The address of claimer
     */
    function manuallySetRewards(address _address, uint256[] memory _tokenIds)
        external
        onlyRole(BATTLE_PASS_MANAGER)
    {
        rewardsToClaim[_address] = _tokenIds;
    }

    /**
     * @notice MANUAL OVERRIDE - Sets the metadata for a particular pass id
     * @param _pass - The pass id for metadata
     * @param _metadata - The metadata tuple
     */
    function setPassesMetadata(
        uint256 _pass,
        BattlePassMetadata memory _metadata
    ) public onlyRole(BATTLE_PASS_MANAGER) {
        passesMetadata[_pass] = _metadata;
    }
}

