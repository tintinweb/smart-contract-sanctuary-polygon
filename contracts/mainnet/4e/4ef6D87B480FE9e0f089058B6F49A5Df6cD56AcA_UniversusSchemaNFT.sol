pragma solidity 0.8.16;

import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/Base64Upgradeable.sol";
import "./UniversusBaseNFT.sol";
import "./ERC721GenericMetadata.sol";
import "./GenericTypedMessage.sol";

contract UniversusSchemaNFT is
    UniversusBaseNFT,
    ERC721GenericMetadata,
    GenericTypedMessage
{
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using StringsUpgradeable for uint256;
    using StringsUpgradeable for uint8;
    using Base64Upgradeable for bytes;

    bytes32 private constant _AUTHORIZED_MINT_TYPEHASH = keccak256("Authorization(address signer,address account,string[] traits,string[] values,uint256 deadline,bytes32 authorization)");

    CountersUpgradeable.Counter private tokenIdTracker;

    struct Metadata {
        uint256 id;
        string[] schema;
        Trait[] attributes;
    }

    struct Authorization {
        bytes32 authorization;
        address account;
        string[] traits;
        string[] values;
        uint32 deadline;
        address signer;
        uint8 v;
        bytes32 r;
        bytes32 s;
    }

    function initialize(
        string memory _name,
        string memory _symbol,
        string memory _uri,
        string[] memory _initialTraits,
        address _operator
    ) public initializer {
        __UniversusBaseNFT_initialize(_name, _symbol, _uri);
        __ERC721Enumerable_init();
        __GenericTypedMessage_init();
        __Pausable_init();
        __AccessControl_init();
        __ERC721Burnable_init();
        __ERC721GenericMetadata_init(_initialTraits);

        _grantRole(DEFAULT_ADMIN_ROLE, _operator);
    }

    function mint(
        address _to,
        string[] calldata _traits,
        string[] calldata _values
    )
        public
        onlyRole(MINTER_ROLE)
    {
        _mint(_to, _traits, _values);
    }

    function _mint(
        address _to,
        string[] calldata _traits,
        string[] calldata _values
     )
        internal
    {
        uint256 tokenId = tokenIdTracker.current();

        super._mint(_to, tokenId);

        _assemble(tokenId, _traits, _values);

        tokenIdTracker.increment();
    }

    function authorizedMint(
        Authorization calldata _authorization
    )
        external
        whenNotPaused
    {
        require(hasRole(MINTER_ROLE, _authorization.signer), "UNXResources: signer is not a MINTER");
        require(_authorization.traits.length == _authorization.values.length, "UNXResources: traits and values sizes mismatch");
        require(_verifySignature(_authorization), "UNXResources: invalid authorization");

        _mint(
            _authorization.account,
            _authorization.traits,
            _authorization.values
        );
    }

    function _verifySignature(
        Authorization memory _authorization
    )
        internal
        returns (bool)
    {
        bytes32[] memory traits = new bytes32[](_authorization.traits.length);
        bytes32[] memory values = new bytes32[](_authorization.values.length);

        for (uint256 i; i < traits.length; ++i) {
            traits[i] = keccak256(abi.encodePacked(_authorization.traits[i]));
            values[i] = keccak256(abi.encodePacked(_authorization.values[i]));
        }

        bytes32 structHash = keccak256(
            abi.encode(
                _AUTHORIZED_MINT_TYPEHASH,
                _authorization.signer,
                _authorization.account,
                keccak256(abi.encodePacked(traits)),
                keccak256(abi.encodePacked(values)),
                _authorization.deadline,
                _authorization.authorization
            )
        );

        return _verifyTypedMessage(
            structHash,
            _authorization.authorization,
            _authorization.deadline,
            _authorization.signer,
            _authorization.v,
            _authorization.r,
            _authorization.s
        );
    }

    function addTraits(
        uint256 _tokenId,
        string[] calldata _traits,
        string[] calldata _values
    )
      public
      onlyRole(ADMIN_ROLE)
    {
      require(_exists(_tokenId), "UniversusSchemaNFT: no such token");

      _assemble(_tokenId, _traits, _values);
    }

    function setTrait(
        uint256 _tokenId,
        string memory _trait,
        string memory _value
    )
        external
        onlyRole(ADMIN_ROLE)
    {
        _setTrait(_tokenId, _trait, _value);
    }

    function getAttribute(
        uint _tokenId,
        string memory _name
    )
        external
        view
        returns (string memory)
    {
        return _getAttribute(_tokenId, _name);
    }

    function metadata(uint256 _tokenId)
        external
        view
        returns (Metadata memory)
    {
        return _metadata(_tokenId);
    }

    function metadataBatch(
        uint256[] calldata _tokenIds
    )
        external
        view
        returns (Metadata[] memory batch)
    {
        batch = new Metadata[](_tokenIds.length);

        for (uint256 i; i < _tokenIds.length; ++i) {
            batch[i] = _metadata(_tokenIds[i]);
        }
    }

    function _metadata(
        uint256 _tokenId
    )
        internal
        view
        returns (Metadata memory)
    {
        require(_exists(_tokenId), "UniversusSchemaNFT: no such token");

        return _assembleMetadata(_tokenId);
    }

    function _assembleMetadata(uint256 _tokenId)
        internal
        view
        returns (Metadata memory meta)
    {
        meta.id = _tokenId;
        meta.attributes = attributes(_tokenId);
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "./utils/Roles.sol";

contract UniversusBaseNFT is
    Initializable,
    Roles,
    ERC721Upgradeable,
    ERC721EnumerableUpgradeable,
    PausableUpgradeable,
    AccessControlEnumerableUpgradeable,
    ERC721BurnableUpgradeable
{
    string internal baseTokenURI;

    function __UniversusBaseNFT_initialize(
        string memory _name,
        string memory _symbol,
        string memory _uri
    )
        internal
        onlyInitializing
    {
        __ERC721_init(_name, _symbol);
        __ERC721Enumerable_init();
        __Pausable_init();
        __AccessControl_init();
        __ERC721Burnable_init();

        baseTokenURI = _uri;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseTokenURI;
    }

    function pause() external onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() external onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function updateBaseTokenURI(
        string calldata _newBaseTokenURI
    )
      external
      onlyRole(ADMIN_ROLE)
    {
      baseTokenURI = _newBaseTokenURI;
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        whenNotPaused
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function collectionOf(
        address _account
    )
        external
        view
        returns (uint256[] memory)
    {
        uint256 balance = balanceOf(_account);

        uint256[] memory collection = new uint256[](balance);

        for (uint256 i; i < balance; ++i) {
            collection[i] = tokenOfOwnerByIndex(_account, i);
        }

        return collection;
    }

    // The following functions are overrides required by Solidity.

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable, AccessControlEnumerableUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

