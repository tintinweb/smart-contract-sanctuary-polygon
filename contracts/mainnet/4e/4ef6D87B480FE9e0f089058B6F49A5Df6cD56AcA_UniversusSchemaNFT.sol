import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./utils/ModifiedEnumerableMapUpgradeable.sol";

/**
 * @title Generic metadata contract to store nft attributes
 * @notice This contract allows for a version-controlled attributes, which are saved
 * as an schema in the allowedTraits mapping. NFTs are then saved under their current
 * version.
 * The attributes reading is tolerant to missing attributes or values. Overall, updating
 * a schema should work fine only when reducing the number of traits (that is, "deleting")
 * a trait. For updates that rename or add traits, the best course of action is to create
 * a new version and migrate items to the new schema
 * To update an item's attributes, one should call assemble() again.
 * @notice empty traits will be returned as an empty array position, you should handle them
 * off-chain
 */
contract ERC721GenericMetadata is Initializable {

    using Strings for uint256;
    using ModifiedEnumerableMapUpgradeable for ModifiedEnumerableMapUpgradeable.StringToBytesMap;

    string public currentVersion;
    mapping(string => string[]) public allowedTraits;
    mapping(uint256 => ModifiedEnumerableMapUpgradeable.StringToBytesMap) internal tokens;

    struct Trait {
        string key;
        string value;
    }

    /**
     * @dev initializes the storage with a v1 schema
     * @param _initialTraits array of trait names
     */
    function __ERC721GenericMetadata_init(
        string[] memory _initialTraits
    )
        internal
        onlyInitializing
    {
        _setSchema("v1", _initialTraits);
    }

    /**
     * @dev sets the schema of a version, either new or existing ones
     * @param _version version name
     * @param _traits array of trait names
     */
    function _setSchema(
        string memory _version,
        string[] memory _traits
    )
        internal
    {
        require(abi.encodePacked(_version).length > 0, "ERC721GenericMetadata: invalid schema name");
        require(
            keccak256(abi.encodePacked(currentVersion)) != keccak256(abi.encodePacked(_version)), "ERC721GenericMetadata: version name already in use"
        );

        currentVersion = _version;

        for (uint8 i = 0; i < _traits.length; i++) {
            allowedTraits[currentVersion].push(_traits[i]);
        }
    }

    /**
     * @dev stores traits of an item. This method is tolerant for missing traits
     * and traits outside schema
     * @param _tokenId the item id
     * @param _traits array of trait names
     * @param _values array of trait values
     */
    function _assemble(
        uint256 _tokenId,
        string[] memory _traits,
        string[] memory _values
    )
        internal
    {
        require(_traits.length == _values.length, "ERC721GenericMetadata: traits and values sizes mismatch");

        for (uint256 i = 0; i < _traits.length; i++) {
            tokens[_tokenId].set(_traits[i], abi.encodePacked(_values[i]));
        }

        tokens[_tokenId].set("version", abi.encodePacked(currentVersion));
    }

    function _setTrait(
        uint256 _tokenId,
        string memory _trait,
        string memory _value
    )
        internal
    {
        tokens[_tokenId].set(_trait, abi.encodePacked(_value));
    }

    function _getAttribute(
        uint _tokenId,
        string memory _name
    )
        internal
        view
        returns (string memory)
    {
        return string(tokens[_tokenId].get(_name));
    }

    /**
     * @dev returns the item attributes if present in version schema
     * @param _tokenId the item id
     * @return Trait[] item attributes
     */
    function attributes(
        uint256 _tokenId
    )
        public
        view
        returns (Trait[] memory)
    {
        (bool versionFound, bytes memory v) = tokens[_tokenId].tryGet("version");
        require(versionFound, "ERC721GenericMetadata: item not found");

        string memory version = string(v);
        string[] memory traits = allowedTraits[version];

        Trait[] memory attr = new Trait[](traits.length + 1);

        for (uint8 i = 0; i < traits.length; i++) {
            (bool found, bytes memory val) = tokens[_tokenId].tryGet(traits[i]);
            if (found) attr[i] = Trait(traits[i], string(val));
        }

        // adds schema version
        (bool traitFound, bytes memory value) = tokens[_tokenId].tryGet("version");
        if (traitFound) attr[attr.length - 1] = Trait("version", string(value));

        return attr;
    }

    /**
     * @dev returns allowed traits under a version
     * @param _version the version name
     * @return string[] attribute names
     */
    function schema(
        string calldata _version
    )
        public
        view
        returns (string[] memory)
    {
        return allowedTraits[_version];
    }

}

