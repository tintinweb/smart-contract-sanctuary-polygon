/**************************************************
 * @title BrawlerBearzFactionWarz
 * @author @scottybmitch
 **************************************************/

contract BrawlerBearzFactionWarz is
    EIP712Upgradeable,
    ERC2771ContextUpgradeable,
    AccessControlUpgradeable,
    IBrawlerBearzFactionWarz,
    MinimalForwarderUpgradeable,
    ReentrancyGuardUpgradeable
{
    using StringsUpgradeable for string;
    using SafeMathUpgradeable for uint256;

    uint256 private constant MAX_INT =
        115792089237316195423570985008687907853269984665640564039457584007913129639935;

    /// @dev Role types
    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");

    bytes32 public constant BATTLE_MODERATOR_ROLE =
        keccak256("BATTLE_MODERATOR_ROLE");

    bytes32 public constant BATTLE_EXECUTOR_ROLE =
        keccak256("BATTLE_EXECUTOR_ROLE");

    bytes32 private constant BATTLE_JOIN_MESSAGE =
        keccak256(
            "BattleJoin(uint256 tokenId,uint256 str,uint256 end,uint256 intel,uint256 lck,uint256 hp,uint256 level)"
        );

    bytes32 private constant BATTLE_END_MESSAGE =
        keccak256(
            "DivisionBattleEnded(uint256 divisionId,uint256 battleId,address winnerAddress,uint256 winnerTokenId,uint256 loserTokenId)"
        );

    /// @notice nonce for claim requests
    uint256 private requestNonce;

    /// @notice Trusted signer
    address private trustedSigner;

    /// @notice Accelerator Price
    uint256 public acceleratorPrice;

    /// @notice Min level
    uint256 public minLevel;

    /// @notice The token contract
    IERC20Rewards public rewardsToken;

    /// @notice The staking contract
    IBrawlerBearzStakeChildSupport public stakeContract;

    /// @notice The battle pass contract
    IBrawlerBearzBattlePassChild public battlePassContract;

    /// @notice The version of the contract for typed domain signatures
    string public version;

    /// @notice Ability to pause any create or join actions in the arena
    bool public isPaused;

    /// @notice Last division id
    uint256 public lastDivisionId;

    /// @notice Last battle id
    uint256 public lastBattleId;

    /// @dev Tournament id to config
    mapping(uint256 => DivisionConfig) public divisions;

    /// @dev Tournament id to battle snapshot
    mapping(uint256 => BearSnapshot[]) public snapshots;

    /*
     * @dev Tournament id to token id to win-loss record,
     * leverage `snapshots` to get all token ids involved
     */
    mapping(uint256 => mapping(uint256 => TournamentResult))
        public resultTracking;

    /// @dev Tournament id to joined token id joined
    mapping(uint256 => mapping(uint256 => bool)) public divisionLookup;

    /// @dev Battle id to division battle
    mapping(uint256 => Battle) public battleLookup;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor(address _trustedForwarder)
        ERC2771ContextUpgradeable(_trustedForwarder)
    {}

    modifier isTokenOwner(address owner, uint256 tokenId) {
        require(
            _checkTokenOwnership(owner, tokenId),
            "Token must be staked by owner"
        );
        _;
    }

    modifier hasEntries(uint256 tokenId) {
        // TODO
        _;
    }

    modifier isNotPaused() {
        if (isPaused) {
            revert("Underground is in maintenance mode");
        }
        _;
    }

    function initialize(
        address _token,
        address _stakeContract,
        address _battlePassContract
    ) public initializer {
        __AccessControl_init();
        __ReentrancyGuard_init();
        __EIP712_init("Brawler Bearz Faction Warz", "1.0");

        // Roles
        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(OWNER_ROLE, _msgSender());
        _grantRole(BATTLE_MODERATOR_ROLE, _msgSender());
        _grantRole(BATTLE_EXECUTOR_ROLE, _msgSender());

        // Integration contracts
        rewardsToken = IERC20Rewards(_token);
        stakeContract = IBrawlerBearzStakeChildSupport(_stakeContract);
        battlePassContract = IBrawlerBearzBattlePassChild(_battlePassContract);

        // Set trusted signer
        trustedSigner = _msgSender();

        acceleratorPrice = 500 ether;
        minLevel = 18;

        // Create divisions

        // IronBearz
        divisions[1] = DivisionConfig({
            id: 1,
            entriesStartAt: 1682092800,
            entriesEndAt: 1682956800,
            minLevel: minLevel,
            maxLevel: 9999
        });

        // GeoScapez
        divisions[2] = DivisionConfig({
            id: 2,
            entriesStartAt: 1682092800,
            entriesEndAt: 1682956800,
            minLevel: minLevel,
            maxLevel: 9999
        });

        // PawPunkz
        divisions[3] = DivisionConfig({
            id: 3,
            entriesStartAt: 1682092800,
            entriesEndAt: 1682956800,
            minLevel: minLevel,
            maxLevel: 9999
        });

        // TechHeadz
        divisions[4] = DivisionConfig({
            id: 4,
            entriesStartAt: 1682092800,
            entriesEndAt: 1682956800,
            minLevel: minLevel,
            maxLevel: 9999
        });

        lastDivisionId = 4;

        version = "1.0";
    }

    function getJoinSigner(
        StatSnapshot calldata _snapshot,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public view returns (address) {
        bytes32 structHash = keccak256(
            abi.encode(
                BATTLE_JOIN_MESSAGE,
                _snapshot.tokenId,
                _snapshot.str,
                _snapshot.end,
                _snapshot.intel,
                _snapshot.lck,
                _snapshot.hp,
                _snapshot.level
            )
        );
        bytes32 hash = _hashTypedDataV4(structHash);
        address signer = ECDSAUpgradeable.recover(hash, v, r, s);
        return signer;
    }

    /*
     * @notice Joins a division, snapshot of the bear is taken
     * @param _battlePassTokenId - The battle pass token id being used
     * @param _id - The id of the division being entered
     * @param _tokenId - The bear token id
     * @param _snapshot - The bear snapshot
     * @param _items - The bear items
     * @param _tokenAmount - The token amount
     * @param v
     * @param r
     * @param s
     */
    function joinWithAccelerator(
        uint256 _battlePassTokenId,
        uint256 _id,
        uint256 _tokenId,
        StatSnapshot calldata _snapshot,
        DynamicItems calldata _items,
        uint256 _tokenAmount,
        uint8 v,
        bytes32 r,
        bytes32 s
    )
        external
        override
        isNotPaused
        isTokenOwner(_msgSender(), _tokenId)
        nonReentrant
    {
        require(
            _tokenAmount >= acceleratorPrice,
            "Not enough token to go accelerate"
        );
        rewardsToken.burn(_msgSender(), _tokenAmount);
        _join(_battlePassTokenId, _id, _tokenId, _snapshot, _items, v, r, s);
    }

    function _checkBattlePassOwner(address owner, uint256 _battlePassTokenId)
        internal
        returns (bool)
    {
        uint256[] memory passesOwned = battlePassContract.getPassOwnerTokens(
            _msgSender()
        );
        bool found = false;
        uint256 lookupIndex = 0;
        while (!found && lookupIndex < passesOwned.length) {
            found = passesOwned[lookupIndex] == _battlePassTokenId;
            unchecked {
                ++lookupIndex;
            }
        }
        return found;
    }

    /*
     * @notice Joins a division, snapshot of the bear is taken
     * @param _battlePassTokenId - The battle pass token id being used
     * @param _id - The id of the division being entered
     * @param _tokenId - The bear token id
     * @param _snapshot - The bear snapshot
     * @param _items - The bear items
     * @param v
     * @param r
     * @param s
     */
    function join(
        uint256 _battlePassTokenId,
        uint256 _id,
        uint256 _tokenId,
        StatSnapshot calldata _snapshot,
        DynamicItems calldata _items,
        uint8 v,
        bytes32 r,
        bytes32 s
    )
        public
        override
        isNotPaused
        isTokenOwner(_msgSender(), _tokenId)
        nonReentrant
    {
        _join(_battlePassTokenId, _id, _tokenId, _snapshot, _items, v, r, s);
    }

    /*
     * @notice Joins a division, snapshot of the bear is taken
     * @param _battlePassTokenId - The battle pass token id being used
     * @param _id - The id of the division being entered
     * @param _tokenId - The bear token id
     * @param _snapshot - The bear snapshot
     * @param _items - The bear items
     * @param v
     * @param r
     * @param s
     */
    function _join(
        uint256 _battlePassTokenId,
        uint256 _id,
        uint256 _tokenId,
        StatSnapshot calldata _snapshot,
        DynamicItems calldata _items,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) internal {
        require(_id <= lastDivisionId, "Division does not exist");
        require(_snapshot.tokenId == _tokenId, "Snapshot has invalid token id");

        // Verify snapshot & items
        address signer = getJoinSigner(_snapshot, v, r, s);

        require(
            signer == trustedSigner,
            "BrawlerBearzFactionWarz: INVALID_SIGNATURE"
        );

        DivisionConfig storage config = divisions[_id];

        require(
            block.timestamp > config.entriesStartAt,
            "Tournament entries period not started."
        );

        require(
            block.timestamp < config.entriesEndAt,
            "Tournament entries period expired."
        );

        require(
            !divisionLookup[_id][_tokenId],
            "Token already joined division."
        );

        require(
            _checkBattlePassOwner(_msgSender(), _battlePassTokenId),
            "Battle pass not owned by user"
        );

        uint256 currentLevel = _snapshot.level;

        require(
            currentLevel >= config.minLevel && currentLevel <= config.maxLevel,
            "Level not high enough"
        );

        // Decrease battle pass entries
        battlePassContract.decreaseBattlePassEntries(_battlePassTokenId, 1);

        BearSnapshot memory instance = BearSnapshot({
            bear: StatSnapshot({
                tokenId: _snapshot.tokenId,
                str: _snapshot.str,
                end: _snapshot.end,
                intel: _snapshot.intel,
                lck: _snapshot.lck,
                hp: _snapshot.hp,
                level: currentLevel
            }),
            items: DynamicItems({
                backgroundId: _items.backgroundId,
                weaponId: _items.weaponId,
                armorId: _items.armorId,
                faceArmorId: _items.faceArmorId,
                eyewearId: _items.eyewearId,
                miscId: _items.miscId,
                headId: _items.headId
            })
        });

        // Add bear snapshot to division and update tracking
        snapshots[_id].push(instance);

        // Entered
        divisionLookup[_id][_tokenId] = true;

        emit DivisionJoined(_id, _tokenId);
    }

    function getBattleSigner(
        Battle memory _battle,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public view returns (address) {
        bytes32 structHash = keccak256(
            abi.encode(
                BATTLE_END_MESSAGE,
                _battle.divisionId,
                _battle.battleId,
                _battle.winnerAddress,
                _battle.winnerTokenId,
                _battle.loserTokenId
            )
        );
        bytes32 hash = _hashTypedDataV4(structHash);
        address signer = ECDSAUpgradeable.recover(hash, v, r, s);
        return signer;
    }

    /*
     * @notice Sets and determines the battle winner from a trusted outsider signer
     * @param _battle - The battle struct
     * @param v
     * @param r
     * @param s
     */
    function setBattleWinner(
        Battle memory _battle,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external override onlyRole(BATTLE_EXECUTOR_ROLE) {
        uint256 divisionId = _battle.divisionId;
        uint256 battleId = _battle.battleId;
        uint256 winnerTokenId = _battle.winnerTokenId;
        uint256 loserTokenId = _battle.loserTokenId;
        address winnerAddress = _battle.winnerAddress;

        require(
            divisionLookup[divisionId][winnerTokenId],
            "Winner token Id is not in the division"
        );

        require(
            divisionLookup[divisionId][loserTokenId],
            "Loser token Id is not in the division"
        );

        address signer = getBattleSigner(_battle, v, r, s);

        require(
            signer == trustedSigner,
            "BrawlerBearzFactionWarz: INVALID_SIGNATURE"
        );

        // Add battle to lookup
        battleLookup[battleId] = _battle;

        // Add to winner record
        resultTracking[divisionId][winnerTokenId].wins++;

        // Add to loser record
        resultTracking[divisionId][loserTokenId].losses++;

        // Verify signature is from a trusted signer
        emit DivisionBattleEnded(
            divisionId,
            battleId,
            winnerAddress,
            winnerTokenId,
            loserTokenId
        );
    }

    /*
     * @notice Checks the ownership of a particular owner and token id
     * @param _owner - The owner address to check
     * @param _tokenId - The token id to check
     */
    function _checkTokenOwnership(address _owner, uint256 _tokenId)
        internal
        returns (bool)
    {
        uint256 tokenBalance = stakeContract.balanceOf(_owner);
        uint256[] memory ownedTokens = stakeContract.getStakedTokens(_owner);
        uint256 currentToken;
        for (uint256 i = 0; i < tokenBalance; i++) {
            currentToken = ownedTokens[i];
            if (currentToken == _tokenId) {
                return true;
            }
        }
        return false;
    }

    /*
     * @notice Updates the division config
     * @param _id - The division id
     * @param _config - The config tuple to set for the division
     */
    function updateDivision(uint256 _id, DivisionConfig calldata _config)
        external
        override
        onlyRole(OWNER_ROLE)
    {
        divisions[_id] = _config;
    }

    /**
     * @notice Sets the cost and min level of xp accelerator
     * @param _acceleratorPrice - The price to use the accelerator
     * @param _minLevel - The min level for the accelerator
     */
    function setAccelerator(uint256 _acceleratorPrice, uint256 _minLevel)
        public
        onlyRole(OWNER_ROLE)
    {
        acceleratorPrice = _acceleratorPrice;
        minLevel = _minLevel;
    }

    /*
     * @notice Sets the stake contract reference
     * @param _stakeContract - The address of the staking contract
     */
    function setStakeContract(address _stakeContract)
        external
        override
        onlyRole(OWNER_ROLE)
    {
        stakeContract = IBrawlerBearzStakeChildSupport(_stakeContract);
    }

    /*
     * @notice Sets whether the arena is paused
     * @param _isPaused - The status of the arena
     */
    function setPaused(bool _isPaused) external override onlyRole(OWNER_ROLE) {
        isPaused = _isPaused;
    }

    /*
     * @notice Sets the trusted signer
     * @param _trustedSigner - The address of the trusted signer
     */
    function setTrustedSigner(address _trustedSigner)
        external
        override
        onlyRole(OWNER_ROLE)
    {
        trustedSigner = _trustedSigner;
    }

    /*
     * @notice Gets division by id
     * @param _divisionId - The division id
     */
    function getDivision(uint256 _divisionId)
        external
        view
        override
        returns (DivisionConfig memory)
    {
        return divisions[_divisionId];
    }

    /// @notice Gets all divisions
    function getDivisions()
        external
        view
        override
        returns (DivisionConfig[] memory)
    {
        DivisionConfig[] memory divisionBatch = new DivisionConfig[](
            lastDivisionId
        );
        for (uint256 i = 0; i < lastDivisionId; i++) {
            divisionBatch[i] = divisions[i + 1];
        }
        return divisionBatch;
    }

    /*
     * @notice Gets division entries by id
     * @param _divisionId - The division id
     */
    function getDivisionEntries(uint256 _divisionId)
        external
        view
        override
        returns (BearSnapshot[] memory)
    {
        return snapshots[_divisionId];
    }

    /*
     * @notice Gets specific battle by id
     * @param _battleId - The battle id
     */
    function getBattle(uint256 _battleId)
        external
        view
        override
        returns (Battle memory)
    {
        return battleLookup[_battleId];
    }

    /*
     * @notice Gets specific battles form multiple ids
     * @param _battleIds - The battle ids
     */
    function getBattleBatch(uint256[] calldata _battleIds)
        external
        view
        override
        returns (Battle[] memory)
    {
        require(_battleIds.length > 0, "0");
        Battle[] memory battleBatch = new Battle[](_battleIds.length);
        for (uint256 i = 0; i < _battleIds.length; i++) {
            battleBatch[i] = battleLookup[_battleIds[i]];
        }
        return battleBatch;
    }

    /*
     * @notice Gets the current w/l record for a particular division and token id
     * @param _divisionId - The division id
     * @param _tokenId - The token id
     */
    function getTokenResult(uint256 _divisionId, uint256 _tokenId)
        external
        view
        override
        returns (TournamentResult memory)
    {
        return resultTracking[_divisionId][_tokenId];
    }

    function _msgSender()
        internal
        view
        virtual
        override(ERC2771ContextUpgradeable, ContextUpgradeable)
        returns (address)
    {
        return ERC2771ContextUpgradeable._msgSender();
    }

    function _msgData()
        internal
        view
        virtual
        override(ERC2771ContextUpgradeable, ContextUpgradeable)
        returns (bytes calldata)
    {
        return ERC2771ContextUpgradeable._msgData();
    }

    function versionRecipient() external view returns (string memory) {
        return "1.0";
    }
}

