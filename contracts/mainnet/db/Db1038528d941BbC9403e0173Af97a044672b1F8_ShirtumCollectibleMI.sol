import "erc721a/contracts/ERC721A.sol";  
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import './libraries/TransferHelper.sol';
import "./interfaces/IShiPrice.sol";

error CantBeZeroAddress();
error OnlyOwnerOrMinter();
error BatchSmallerThan100();
error OnlyNFTOwner();
error IncorrectTargetLevel();
error ShiPriceIs0();
error ForbiddenBeforEndDate();
error ForbiddenBeforOneMonthAfterEndDate();

contract ShirtumCollectibleMI is ERC721A, Ownable, AccessControl, ReentrancyGuard {

    using Strings for uint256;
    
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");

    address public constant _burnPool = 0x000000000000000000000000000000000000dEaD;

    IShiPrice private shiPriceContract;

    uint256 constant MIN_LEVEL = 1;
    uint256 constant MAX_LEVEL = 4;

    string private baseURI;

    address private shiToken;
    
    bool private burnShiOnNFTBurn;
    uint private burnFee;
    
    uint constant endDate = 1702684799; //15/12/2023 23:59:59 GMT    
    
    //ID -> Level -> SHI paid to level up
    mapping (uint256 => mapping(uint256 => uint256)) public nftBalance;

    //ID -> Level
    mapping (uint256 => uint256) private nftLevel;

    //Level -> USD needed to upgrade
    mapping(uint256 => uint256) public levels;

    event NFTLevelUpgraded(uint indexed tokenId, uint indexed prevLevel, uint indexed currentLevel, uint price, uint shiPrice);
    event NFTLevelDowngraded(uint indexed tokenId, uint indexed prevLevel, uint indexed currentLevel, uint price, uint burnAmount);
    event AdminWithdraw(address indexed user, uint indexed amount);
    event NFTBurnt(address indexed user, uint nftLevel);

    function supportsInterface(bytes4 interfaceId) public view override(ERC721A, AccessControl) returns (bool) {
        return            
            interfaceId == type(IAccessControl).interfaceId || 
            super.supportsInterface(interfaceId);
    }

    constructor(string memory _baseUri, address _SHIToken, uint _burnFee, address _shiPriceContract)
    ERC721A("SW3 Collectibles 2023 - Mariano Izco", "SW3 Collec MI") 
    {
        if(_shiPriceContract == address(0)) revert CantBeZeroAddress();
        baseURI = _baseUri;
        shiToken = _SHIToken;
        burnFee = _burnFee;
        levels[2] = 100; //Silver
        levels[3] = 200; //Gold
        levels[4] = 400; //Diamond

        shiPriceContract = IShiPrice(_shiPriceContract);        
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(OWNER_ROLE,msg.sender);
    } 

    function _startTokenId() internal override pure returns (uint256) {
        return 1;
    }

    function totalMinted() external view returns(uint256){
        return _totalMinted();
    }

    function mint(address[] calldata _to) external {
        if(!hasRole(OWNER_ROLE, msg.sender) && !hasRole(MINTER_ROLE, msg.sender)) revert OnlyOwnerOrMinter();
        if(_to.length > 100) revert BatchSmallerThan100();
        
        for (uint8 i = 0; i < uint8(_to.length); i++){
            _safeMint(_to[i], 1);
        }
    }

    function upgradeNFT(uint256 tokenId, uint targetLevel) external nonReentrant() {
        if(ownerOf(tokenId) != msg.sender) revert OnlyNFTOwner();
        if(targetLevel > MAX_LEVEL) revert IncorrectTargetLevel();

        uint currentLevel = getNftLevel(tokenId);
                
        if(targetLevel <= currentLevel) revert IncorrectTargetLevel();

        uint shiPrice = getShiPrice();
        if(shiPrice == 0) revert ShiPriceIs0();
        
        uint price;
        for (uint i = currentLevel + 1; i <= targetLevel; i++){
            uint levelPriceInShi = (levels[i] * 10**18 / shiPrice) * 1000000;
            nftBalance[tokenId][i] = levelPriceInShi;
            price += levelPriceInShi;
        }
        
        TransferHelper.safeTransferFrom(shiToken,msg.sender,address(this),price);
        
        nftLevel[tokenId] = targetLevel;

        emit NFTLevelUpgraded(tokenId, currentLevel, nftLevel[tokenId],price,shiPrice);
    }

    function downgradeNFT(uint256 tokenId, uint targetLevel) external nonReentrant() {
        if(ownerOf(tokenId) != msg.sender) revert OnlyNFTOwner();
        if(targetLevel < MIN_LEVEL) revert IncorrectTargetLevel();

        uint currentLevel = getNftLevel(tokenId);
        if(targetLevel >= currentLevel) revert IncorrectTargetLevel();        
        
        uint price;
        for (uint i = currentLevel; i > targetLevel; i--){
            price += nftBalance[tokenId][i];
            nftBalance[tokenId][i] = 0;
        }
        
        uint burnAmount = price * burnFee / 100;

        TransferHelper.safeTransfer(shiToken,_burnPool,burnAmount);

        TransferHelper.safeTransfer(shiToken,msg.sender,(price - burnAmount));
        
        nftLevel[tokenId] = targetLevel;

        emit NFTLevelDowngraded(tokenId, currentLevel, nftLevel[tokenId],price,burnAmount);
    }

    function burn(uint _tokenId) external {
        require(ownerOf(_tokenId) == msg.sender || getApproved(_tokenId) == msg.sender,"Only the owner can burn the NFT");
        if(block.timestamp < endDate) revert ForbiddenBeforEndDate();
        
        uint _nftLevel = getNftLevel(_tokenId);
        uint balance = getNftBalance(_tokenId);
        uint burnAmount;
        if(burnShiOnNFTBurn && balance > 0){
            burnAmount = balance * burnFee / 100;
            TransferHelper.safeTransfer(shiToken,_burnPool,burnAmount);
        }
        super._burn(_tokenId);
        TransferHelper.safeTransfer(shiToken,msg.sender,balance - burnAmount);
        emit NFTBurnt(msg.sender,_nftLevel);
    }

    function adminBurn(uint _tokenId) external onlyRole(OWNER_ROLE){        
        if(block.timestamp < (endDate + 30 days)) revert ForbiddenBeforOneMonthAfterEndDate();
        
        address owner = ownerOf(_tokenId);
        uint _nftLevel = getNftLevel(_tokenId);
        uint balance = getNftBalance(_tokenId);
        uint burnAmount;
        if(burnShiOnNFTBurn && balance > 0){
            burnAmount = balance * burnFee / 100;
            TransferHelper.safeTransfer(shiToken,_burnPool,burnAmount);
        }
        
        super._burn(_tokenId);
        TransferHelper.safeTransfer(shiToken,owner,balance - burnAmount);
        
        emit NFTBurnt(owner,_nftLevel);
    }
    
    function adminWithdraw() external onlyRole(OWNER_ROLE){
        if(block.timestamp < (endDate + 30 days)) revert ForbiddenBeforOneMonthAfterEndDate();        
        uint contractBalance = IERC20(shiToken).balanceOf(address(this));
        if(contractBalance > 0){
            TransferHelper.safeTransfer(shiToken,msg.sender,contractBalance);

            emit AdminWithdraw(msg.sender,contractBalance);
        }
    }

    /**Returns the price needed to upgrade upto target level, expressed in $SHI in WEI */
    function getUpgradePrice(uint currentLevel, uint targetLevel) public view returns(uint){
        if(targetLevel > MAX_LEVEL) revert IncorrectTargetLevel();
        if(targetLevel < currentLevel) revert IncorrectTargetLevel();
        uint price;
        for (uint i = currentLevel + 1; i <= targetLevel; i++){
            price += levels[i];
        }
        return (price * 10**18 / getShiPrice()) * 1000000;
    }

    /**Returns the cost to downgrade upto target level, expressed in $SHI in WEI */
    function getDowngradeCost(uint tokenId, uint targetLevel) public view returns(uint _price, uint _burnAmount){
        if(targetLevel < MIN_LEVEL) revert IncorrectTargetLevel();
        uint currentLevel = getNftLevel(tokenId);
        if(targetLevel > currentLevel) revert IncorrectTargetLevel();
        uint price;        
        for (uint i = currentLevel; i > targetLevel; i--){
            price += nftBalance[tokenId][i];            
        }
        
        uint burnAmount = price * burnFee / 100;
        return (price - burnAmount,burnAmount);
    }

    /** READ */
    function getNftLevel(uint _tokenId) public view returns(uint256){
        return nftLevel[_tokenId] == 0 ? MIN_LEVEL : nftLevel[_tokenId];
    }

    function getNftBalance(uint _tokenId) public view returns(uint256){
        uint balance;
        for (uint i = MIN_LEVEL + 1; i <= MAX_LEVEL; i++){
            balance += nftBalance[_tokenId][i];
        }
        return balance;
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        if (!_exists(tokenId)) revert URIQueryForNonexistentToken();
        
        uint _nftLevel = getNftLevel(tokenId);
        
        return string(abi.encodePacked(_baseURI(), tokenId.toString(),"-",_nftLevel.toString(),'.json'));
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }    
    
    function getBurnFee() external view returns(uint){
        return burnFee;
    }

    function getShiPrice() public view returns(uint){        
        return IShiPrice(shiPriceContract).getShiPrice();
    }

    function getLevels() external pure returns(uint, uint){
        return (MIN_LEVEL,MAX_LEVEL);
    }

    function getShiToken() external view returns(address){
        return shiToken;
    }

    /** WRITE */
    function toggleEnableBurnShiOnNftBurn() external onlyRole(OWNER_ROLE){
        burnShiOnNFTBurn = !burnShiOnNFTBurn;
    }

    function setBaseURI(string memory newUri) external onlyRole(OWNER_ROLE){
        baseURI = newUri;
    }

    function setPriceContract(address _newContract) external onlyRole(OWNER_ROLE){
        if(_newContract == address(0)) revert CantBeZeroAddress();
        shiPriceContract = IShiPrice(_newContract);
    }

    function setLevelPrice(uint _level, uint price) external onlyRole(OWNER_ROLE){        
        require(_level > MIN_LEVEL && _level <= MAX_LEVEL, "Level not allowed");

        levels[_level] = price;
    }

    function setBurnFee(uint _burnFee) external onlyRole(OWNER_ROLE) {
        burnFee = _burnFee;
    }

    function setShiToken(address _shiToken) external onlyRole(OWNER_ROLE) {
        shiToken = _shiToken;
    }

    receive () external payable {
      revert();
    }

}

