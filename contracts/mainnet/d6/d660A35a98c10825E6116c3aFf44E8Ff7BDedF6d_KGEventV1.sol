/// @title KGScratchCard NFT
/// @author kuannnn
/// @notice A NFT contract for KGScratchCard

contract KGScratchCard is ERC721A, Ownable, AccessControl, ContextMixin, NativeMetaTransaction {
    using SafeERC20 for IERC20;
    address public QUEST3_NFT_ADDRESS;

    string public initialCardURI;
    string public winingCardURI;
    string public losingCardURI;

    string public CONTRACT_URI;

    uint256 public EVENT_START_TIMESTAMP;
    uint256 public EVENT_END_TIMESTAMP;

    uint256 public immutable MAX_CARDS = 7554; // 6666 + 888
    uint256 public immutable MAX_CARD_PER_USER = 2;
    uint256 public immutable REWARD = 10_000_000; // 10 USDT

    // @dev cardStatus 0: minted, 1: win, 2: lose
    mapping(uint256 => uint256) public cardStatus;
    // @notice record the quest3 token id
    // @dev quest3TokensMintedStatus true: minted, false: available to mint
    mapping(uint256 => bool) public quest3TokensMintedStatus;

    // @notice USDT address on Polygon
    address public immutable USDT_ADDRESS = 0xc2132D05D31c914a87C6611C10748AEb04B58e8F;

    /// @notice A role for mint operators
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    constructor() ERC721A("KryptoGO Lucky Bag 2023", "KGLB2023") {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _initializeEIP712("KryptoGO Lucky Bag 2023");
        QUEST3_NFT_ADDRESS = 0xA5b04895EF597Ebbd055666E693d3d03D7454bbD; // quest3 NFT address
    }

    // Modifiers
    // ------------------------------------------------------------------------
    modifier validEvent() {
        require(block.timestamp >= EVENT_START_TIMESTAMP && EVENT_START_TIMESTAMP > 0, "Event not started yet");
        require(EVENT_END_TIMESTAMP == 0 || block.timestamp <= EVENT_END_TIMESTAMP, "Event ended");
        _;
    }

    modifier limitedCardsPerUser(address _to, uint256 _amount) {
        require(_numberMinted(_to) + _amount <= MAX_CARD_PER_USER, "Max cards per user reached");
        _;
    }

    modifier limitedEdition() {
        require(_totalMinted() < MAX_CARDS, "Max supply reached");
        _;
    }

    modifier cardOwner(address _owner, uint256 _tokenId) {
        require(ownerOf(_tokenId) == _owner, "Card not owned by this address");
        _;
    }

    modifier cardExists(uint256 _tokenId) {
        require(_exists(_tokenId), "Token does not exist");
        _;
    }

    modifier cardNotScratched(uint256 _tokenId) {
        require(cardStatus[_tokenId] == 0, "The card has already been scratched");
        _;
    }

    modifier validStatus(uint256 _cardStatus) {
        require(_cardStatus == 1 || _cardStatus == 2, "Invalid card status");
        _;
    }

    modifier validParticipant(address _participantAddress) {
        require(
            IERC721(QUEST3_NFT_ADDRESS).balanceOf(_participantAddress) > 0,
            "You need to own at least 1 Quest3 NFT"
        );
        _;
    }

    // Events
    // ------------------------------------------------------------------------
    event CardScratched(uint256 indexed tokenId, uint256 cardStatus);

    // inherritence functions
    // ------------------------------------------------------------------------

    /// @dev Start from tokenId 1 to reduce gas usage of the first mint
    /// @inheritdoc ERC721A
    function _startTokenId() internal view virtual override returns (uint256) {
        return 1;
    }

    /// @inheritdoc ERC721A
    function supportsInterface(bytes4 interfaceId) public view override(ERC721A, AccessControl) returns (bool) {
        return ERC721A.supportsInterface(interfaceId) || AccessControl.supportsInterface(interfaceId);
    }

    /// @dev Returns `true` if `account` has been granted `role` or adminRole of `role`.
    function hasRole(bytes32 role, address account) public view virtual override returns (bool) {
        bytes32 adminRole = getRoleAdmin(role);
        return super.hasRole(role, account) || super.hasRole(adminRole, account);
    }

    // Minting info functions
    // ------------------------------------------------------------------------

    /// @notice Get the total amount of minted tokens, including the ones that are burned
    function totalMinted() public view returns (uint256) {
        return _totalMinted();
    }

    /// @param _tokenIds The tokenIds of the quest3 NFTs
    /// @dev Returns a boolean array of the quest3 NFTs minted status
    function checkQuest3TokenIfMinted(uint256[] memory _tokenIds) public view returns (bool[] memory) {
        bool[] memory mintedResults = new bool[](_tokenIds.length);
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            mintedResults[i] = quest3TokensMintedStatus[_tokenIds[i]];
        }

        return mintedResults;
    }

    // Mint functions
    // ------------------------------------------------------------------------

    /// @notice Mint a single token
    /// @param _to The address to mint the token to
    function mintCards(
        address _to,
        uint256 _amount,
        uint256[] memory _tokenIds
    ) external onlyRole(MINTER_ROLE) validEvent limitedEdition validParticipant(_to) limitedCardsPerUser(_to, _amount) {
        require(_amount == _tokenIds.length, "Amount and tokenIds length mismatch");

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            if (quest3TokensMintedStatus[_tokenIds[i]] == true) {
                revert("Some quest3 NFTs have been used to minted KGScratchCard");
            }

            quest3TokensMintedStatus[_tokenIds[i]] = true;
        }
        _mint(_to, _amount);
    }

    function scratchCards(
        address _owner,
        uint256[] calldata _tokenIds,
        uint256[] calldata _cardStatus
    ) external onlyRole(MINTER_ROLE) {
        require(_tokenIds.length == _cardStatus.length, "TokenIds and cardStatus length mismatch");
        unchecked {
            for (uint256 i = 0; i < _tokenIds.length; ++i) {
                _scratchCard(_owner, _tokenIds[i], _cardStatus[i]);
            }
        }
    }

    function _scratchCard(
        address _owner,
        uint256 _tokenId,
        uint256 _cardStatus
    ) internal cardExists(_tokenId) cardOwner(_owner, _tokenId) cardNotScratched(_tokenId) validStatus(_cardStatus) {
        require(ownerOf(_tokenId) == _owner, "Not the owner of the card");
        cardStatus[_tokenId] = _cardStatus;

        if (_cardStatus == 1) {
            IERC20(USDT_ADDRESS).safeTransfer(_owner, REWARD);
        }

        emit CardScratched(_tokenId, _cardStatus);
    }

    /**
     * Withdraw all USDT from the contract. Only the admin can call this.
     */
    function withdrawAllUSDT() external onlyRole(DEFAULT_ADMIN_ROLE) {
        IERC20(USDT_ADDRESS).safeTransfer(msg.sender, IERC20(USDT_ADDRESS).balanceOf(address(this)));
    }

    // URI functions
    // ------------------------------------------------------------------------

    function setCardURIs(
        string calldata _initialCardURI,
        string calldata _winingCardURI,
        string calldata _losingCardURI
    ) public onlyOwner {
        initialCardURI = _initialCardURI;
        winingCardURI = _winingCardURI;
        losingCardURI = _losingCardURI;
    }

    /// @dev This is the function for opensea to get the contract URI
    /// @return The contract URI for the collection
    function contractURI() public view returns (string memory) {
        return CONTRACT_URI;
    }

    /// @notice Set the URI for collection's metadata
    /// @param _contractURI The URI for contract level metadata
    function setContractURI(string calldata _contractURI) external onlyRole(DEFAULT_ADMIN_ROLE) {
        CONTRACT_URI = _contractURI;
    }

    function setQuest3Address(address _address) external onlyRole(DEFAULT_ADMIN_ROLE) {
        QUEST3_NFT_ADDRESS = _address;
    }

    function tokenURI(uint256 _tokenId) public view virtual override returns (string memory) {
        require(_exists(_tokenId), "URI query for nonexistent token");

        uint256 _cardStatus = cardStatus[_tokenId];

        if (_cardStatus == 1) {
            return winingCardURI;
        }
        if (_cardStatus == 2) {
            return losingCardURI;
        }
        return initialCardURI;
    }

    // Timestamp functions
    // ------------------------------------------------------------------------

    /// @notice Set the timestamps for the event
    /// @param _start The start timestamp for the event
    /// @param _end The end timestamp for the event, if set to 0 and also set `_period` to 0, the event will be open ended
    /// @param _period The period of the event. It won't work if `_end` is none 0 value.

    function setEventTimeStamp(
        uint256 _start,
        uint256 _end,
        uint256 _period
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        EVENT_START_TIMESTAMP = _start;

        if (_end == 0 && _period > 0) {
            EVENT_END_TIMESTAMP = EVENT_START_TIMESTAMP + _period * 1 days;
        }

        if (_end > 0) {
            require(_end > _start, "Must provide valid end timestamp");

            EVENT_END_TIMESTAMP = _end;
        }
    }

    /**
     * This is used instead of msg.sender as transactions won't be sent by the original token owner, but by OpenSea.
     */
    function isApprovedForAll(address _owner, address _operator) public view override returns (bool) {
        if (_operator == address(0x58807baD0B376efc12F5AD86aAc70E78ed67deaE)) {
            return true;
        }

        return super.isApprovedForAll(_owner, _operator);
    }

    /**
     * This is used instead of msg.sender as transactions won't be sent by the original token owner, but by OpenSea.
     */
    function _msgSender() internal view override returns (address sender) {
        return ContextMixin.msgSender();
    }
}

