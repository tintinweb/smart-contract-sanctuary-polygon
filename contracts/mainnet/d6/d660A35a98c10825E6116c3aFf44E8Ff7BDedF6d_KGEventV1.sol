    function airdropSingle(
        address _to,
        uint256 _tokenId,
        uint256 _amount
    ) external onlyRole(MINTER_ROLE) validEvent limitedEdition(_tokenId, _amount) {
        require(_amount > 0, "Must mint at least one token");
        _mint(_to, _tokenId, _amount, "");
    }

    /// @notice Airdrop tokens to multiple addresses at once
