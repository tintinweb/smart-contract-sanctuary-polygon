error NotTheTokenOwner(uint256 tokenID, address sender, address tokenOwner);

/// @title NFT Staking Contract
/// @notice Staking VaultX tokens in the ERC721 non-fungible token interface
contract VaultxCryptoVault is
    ERC721,
    ERC721Enumerable,
    ERC721Burnable,
    Ownable,
    Pausable,
    ReentrancyGuard
{
    using Counters for Counters.Counter;
    using Strings for uint256;

    /// @notice Emitted once when a stake is minted
    /// @param recipient The address of the user who minted the stake
    /// @param amount The amount VaultX of tokens staked
    /// @param secondsInTerm The length of the stake measured in seconds
    /// @param tokenId  The ID of the stake
    event Created(
        address recipient,
        uint256 amount,
        uint256 secondsInTerm,
        uint256 tokenId
    );

    /// @notice Emitted when stake is burned at full term
    /// @param owner The address of the user who minted the stake
    /// @param amountReturned How many tokens were unlocked
    /// @param tokenId The ID of the sy nc stake
    event Matured(address owner, uint256 amountReturned, uint256 tokenId);

    /// @notice Emitted when stake is burned before full term and a penalty is levied
    /// @param tokenId The ID of the stake
    /// @param owner The address of the owner
    /// @param orginalVaultXAmount The amount of VaultX tokens staked
    /// @param vaultXPenaltyFee The amount of VaultX tokens deducted as a penalty
    /// @param originalLockedTokenAmount The amount of locked tokens staked
    /// @param lockedTokenPenaltyFee The amount of locked tokens deducted as a penalty
    event Penalized(
        address owner,
        uint256 tokenId,
        uint256 orginalVaultXAmount,
        uint256 vaultXPenaltyFee,
        uint256 originalLockedTokenAmount,
        uint256 lockedTokenPenaltyFee
    );

    /// @dev The ID of the next token that will be minted. Skips 0
    Counters.Counter private _tokenIdCounter;

    /// @dev Total number of ALL Stakes created.
    Counters.Counter public totalCRYPTOVAULTS;

    /// @dev Total number of Stakes that have been matured.
    Counters.Counter public totalMatured;

    /// @dev Total number of Stakes that have been matured early.
    Counters.Counter public totalPenalized;

    /// @dev Vault is a struct that contains all the information about a stake
    struct Vault {
        uint256 id;
        address lockedTokenAddress;
        uint256 lockedTokenAmount;
        uint256 lockedVaultXAmount;
        uint256 vaultXInterestRate;
        uint256 vaultXRewardedOnMaturity;
        uint256 createdAt;
        uint256 termLength;
        bool isMatured;
    }

    struct LockRecord {
        uint256 timestamp;
        uint256 totalSupply;
        uint256 totalLocked;
    }

    struct MinimalLockRecord {
        uint256 timestamp;
        uint256 totalLocked;
    }

    //The VaultX token address. VaultX (VX) is contained in every Stake and is minted to provide interest
    address vaultXTokenAddress;

    mapping(uint256 tokenId => Vault) public vaults;

    /// CONSTANTS

    /// @dev PERCENTAGE_PRECISION Static factor to Divide percentages by to get the real multiplier.
    uint256 public constant PERCENTAGE_PRECISION = 10000;
    /// @dev STARTING_TIME The time the contract was deployed.
    uint256 public STARTING_TIME = block.timestamp;
    /// @dev MINIMUM_BASE_INTEREST_RATE   10 is equal to 0.1%, the minimum value base interest rate can be.
    uint256 public constant MINIMUM_BASE_INTEREST_RATE = 10;
    /// @dev BASE_INTEREST_RATE 2000 is equal to 20%, the maximum value base interest rate can be.
    uint256 public constant BASE_INTEREST_RATE = 2000;

    /// @dev Bonus interest awarded to user on creating lucky and extra lucky Cbonds.
    uint16[] public LUCKY_EXTRAS = [100, 500, 1000];
    uint256 public constant ONE_YEAR = 360 days;
    /// @dev Min duration of a stake in days which is a solidity constant measured in seconds, controls how short someone can create a cbond & where the bonus rate starts from
    uint256 public MIN_DURATION = 10 days;
    /// @dev Max duration of a stake in days which is a solidity constant measured in seconds
    uint256 public MAX_DURATION = 1080 days;
    /// @dev Max rate at which the bonus tops out for the longest stake duration
    uint256 public maxBonusInterestRate = 15000;
    /// @dev The exponent used to calculate the bonus rate
    UD60x18 public bonusRateExponent = ud(1.8e18);

    mapping(uint256 => LockRecord) public lockRecords;

    //Read only tracking variables (not used within the smart contract)
    mapping(uint256 => uint256) public cbondsMaturingByDay; //Mapping of days to number of cbonds maturing that day.

    string private _baseTokenURI;
    // minimal amount of vaultX to create a cbond
    uint256 public vaultXMinimum = 100 * (10 ** 18);
    // maximum amount of vaultX to create a cbond
    uint256 public vaultXMaximum = 1000000 * (10 ** 18);
    // maximum penalty fee
    uint256 public maxPenaltyFee = 6000;
    // minimum penalty fee
    uint256 public minPenaltyFee = 2000;
    // switch to enable/disable lucky bonus
    bool public luckyEnabled = true;
    // % in BPS, if the circulating supply vs total supply goes below this ration the minimum base interest rate will be returned, otherwise its liearly decreased from BASE_INTEREST_RATE
    uint256 public minCirculatingRatio = 8000;
    // address of the treasury
    address public treasuryAddress;
    // mapping of tokens that can be used to create a cbond aside from vaultX
    mapping(address => bool) public approvedTokens;

    // Oracle public priceChecker; //Used to determine the ratio in price between VaultX and a given Token. The value returned should not significantly affect user incentives and does not need to be guaranteed not to be exploitable by the user. Contract can be upgraded.

    /// @notice Constructor is run on cntract creation but it's bytecode is not stored
    /// @dev The tokenID is incremented by 1 to skip 0 on contract creation
    /// @param _vaultXTokenAddress is the address of the vaultX erc-20 token contract
    constructor(
        address _vaultXTokenAddress,
        address[] memory _approvedTokens,
        address _treasuryAddress,
        string memory baseURI
    ) ERC721("CRYPTOVAULT", "CVS") {
        vaultXTokenAddress = _vaultXTokenAddress;
        treasuryAddress = _treasuryAddress;
        _setBaseURI(baseURI);
        for (uint256 i = 0; i < _approvedTokens.length; i++) {
            approvedTokens[_approvedTokens[i]] = true;
        }

        _tokenIdCounter.increment();
    }

    /// @notice Checks if the NFT Exists, and if the msg.sender is the owner
    /// @param tokenId The ID of the token to check
    modifier onlyTokenOwner(uint256 tokenId) {
        require(_exists(tokenId), "Token does not exist");
        if (msg.sender != ownerOf(tokenId)) {
            revert NotTheTokenOwner({
                sender: msg.sender,
                tokenID: tokenId,
                tokenOwner: ownerOf(tokenId)
            });
        }
        _;
    }

    /// @notice Returns principle and mints VaultX to pay back initial amount plus rewards.
    /// @dev User gets their orginal VaultX back and is minted some interest
    /// @param tokenId The ID of the stake to mature
    function matureStake(
        uint256 tokenId
    ) public onlyTokenOwner(tokenId) nonReentrant {
        Vault memory vault = vaults[tokenId];
        require(
            block.timestamp > vault.createdAt + vault.termLength,
            "Stake term not yet complete"
        );
        require(vault.createdAt != 0, "Stake does not exist");
        require(vault.isMatured == false, "Stake already matured");
        // Update the vault
        vaults[tokenId].isMatured = true;
        // Return originally staked amount of VaultX
        require(
            IERC20(vaultXTokenAddress).transfer(
                msg.sender,
                vault.lockedVaultXAmount
            ),
            "transfer must succeed"
        );

        // Return Locked tokens
        require(
            IERC20(vault.lockedTokenAddress).transfer(
                msg.sender,
                vault.lockedTokenAmount
            ),
            "transfer must succeed"
        );

        // mint the interest in vaultX and make an additional transfer to the user
        IVaultX(vaultXTokenAddress).mint(
            msg.sender,
            vault.vaultXRewardedOnMaturity - vault.lockedVaultXAmount
        );

        // Update read only counters
        totalMatured.increment();
        emit Matured(msg.sender, vault.vaultXRewardedOnMaturity, tokenId);

        recordVaultXSupply();

        //burn the nft
        _burn(tokenId);
    }

    /// @notice Return principle minus a fee.
    /// @param tokenId The ID of the stake to mature
    /// @dev User gets back their VaultX - a fee that is sent to the treasury address
    function penaltyWithdrawal(
        uint256 tokenId
    ) public onlyTokenOwner(tokenId) nonReentrant {
        Vault memory vault = vaults[tokenId];
        // Only Executable if the maturity is not met.
        require(
            block.timestamp < vault.createdAt + vault.termLength,
            "stake is fully matureable"
        );
        require(vault.isMatured == false, "Stake already matured");
        // Update the vault
        vaults[tokenId].isMatured = true;

        uint256 penaltyFee = penaltyFeeForTokenID(tokenId);

        uint256 vaultxPenaltyFeeAmount = (vault.lockedVaultXAmount *
            penaltyFee) / PERCENTAGE_PRECISION;

        uint256 vaultxRemainder = vault.lockedVaultXAmount -
            vaultxPenaltyFeeAmount;

        require(
            IERC20(vaultXTokenAddress).transfer(
                treasuryAddress,
                vaultxPenaltyFeeAmount
            ),
            "transfer must succeed"
        );

        require(
            IERC20(vaultXTokenAddress).transfer(msg.sender, vaultxRemainder),
            "transfer must succeed"
        );

        uint256 lockTokenFeeAmount = 0;
        if (vault.lockedTokenAddress != address(0)) {
            lockTokenFeeAmount =
                (vault.lockedTokenAmount * penaltyFee) /
                PERCENTAGE_PRECISION;

            uint256 lockTokenRemainder = vault.lockedTokenAmount -
                lockTokenFeeAmount;

            require(
                IERC20(vault.lockedTokenAddress).transfer(
                    treasuryAddress,
                    lockTokenFeeAmount
                ),
                "transfer must succeed"
            );

            require(
                IERC20(vault.lockedTokenAddress).transfer(
                    msg.sender,
                    lockTokenRemainder
                ),
                "transfer must succeed"
            );
        }

        totalMatured.increment();
        totalPenalized.increment();
        emit Penalized(
            msg.sender,
            tokenId,
            vault.lockedVaultXAmount,
            vaultxPenaltyFeeAmount,
            vault.lockedTokenAmount,
            lockTokenFeeAmount
        );

        recordVaultXSupply();

        //burn the nft
        _burn(tokenId);
    }

    /// @notice Public function for creating a new Cbond
    /// @dev When stake is successful users receive an NFT that represents their stake
    /// @param lockTokenAddress The address of the token to lock
    /// @param lockTokenAmount The amount of tokens to lock
    /// @param vaultxAmount The amount of VaultX to lock
    /// @param secondsInTerm The length of the Stake as measured in seconds, which must meet the min and max requirements
    /// @param recipient The address of the recipient of the stake NFT
    /// @return tokenId The ID of the newly created stake NFT
    function createStake(
        address lockTokenAddress,
        uint256 lockTokenAmount,
        uint256 vaultxAmount,
        uint256 secondsInTerm,
        address recipient
    ) external payable nonReentrant returns (uint256) {
        if (lockTokenAddress != address(0)) {
            require(
                approvedTokens[lockTokenAddress],
                "Specified Token not approved"
            );
        }
        return
            _createStake(
                lockTokenAddress,
                lockTokenAmount,
                vaultxAmount,
                secondsInTerm,
                recipient
            );
    }

    /// @dev User specifies an amount of VaultX, this is transferred from their account to this contract
    /// @param lockTokenAddress The address of the token to lock
    /// @param lockedTokenAmount The amount of tokens to lock
    /// @param vaultXReceived The amount of VaultX tokens to use to create the PolyStake ^18
    /// @param secondsInTerm The length of the Stake as measured in seconds, which must meet the min and max requirements
    /// @param recipient The creator of the stake
    /// @return tokenId The ID of the newly created stake NFT
    function _createStake(
        address lockTokenAddress,
        uint256 lockedTokenAmount,
        uint256 vaultXReceived,
        uint256 secondsInTerm,
        address recipient
    ) private returns (uint256) {
        // Ensure it Passes Creation Requirements
        require(
            secondsInTerm >= MIN_DURATION && secondsInTerm <= MAX_DURATION,
            "Invalid term length provided"
        );
        // Check the amount of VaultX tokens is within limits
        require(
            vaultXReceived >= vaultXMinimum && vaultXReceived <= vaultXMaximum,
            "Stake not within limits"
        );

        // Get Stake's Serial Number, and prep the counter for the next one
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();

        (uint256 interestRate, uint256 totalReturn) = getCVaultTotalReturn(
            tokenId,
            vaultXReceived,
            secondsInTerm
        );

        // Receive the VaultX Tokens
        require(
            IERC20(vaultXTokenAddress).transferFrom(
                msg.sender,
                address(this),
                vaultXReceived
            ),
            "transfer must succeed"
        );

        // Receive the  Locked Tokens
        if (lockTokenAddress != address(0) && lockedTokenAmount > 0) {
            require(
                IERC20(lockTokenAddress).transferFrom(
                    msg.sender,
                    address(this),
                    lockedTokenAmount
                ),
                "transfer must succeed"
            );
        }

        // Create the Vault
        vaults[tokenId] = Vault({
            id: tokenId,
            createdAt: block.timestamp,
            termLength: secondsInTerm,
            lockedVaultXAmount: vaultXReceived,
            lockedTokenAddress: lockTokenAddress,
            lockedTokenAmount: lockedTokenAmount,
            vaultXRewardedOnMaturity: totalReturn,
            vaultXInterestRate: interestRate,
            isMatured: false
        });

        //update global counters
        cbondsMaturingByDay[getDay(block.timestamp + secondsInTerm)] =
            cbondsMaturingByDay[getDay(block.timestamp + secondsInTerm)] +
            1;

        totalCRYPTOVAULTS.increment();

        recordVaultXSupply();

        //Create NFT
        _safeMint(recipient, tokenId);

        //Fire Event
        emit Created(recipient, vaultXReceived, secondsInTerm, tokenId);
        return tokenId;
    }

    /// @notice Creates a metadata string from a token id. Should not be used onchain.
    /// @param tokenId The ID of the stake to generate metadata string
    /// @return urlString string of the ending half of the MetaData URL
    function putTogetherMetadataString(
        uint256 tokenId
    ) public view returns (string memory) {
        Vault memory vault = vaults[tokenId];
        // vault does not exist
        if (vault.createdAt == 0) return "";
        return
            string(
                abi.encodePacked(
                    "?tokenId=",
                    tokenId.toString(),
                    "&vaultXAmount=",
                    vault.lockedVaultXAmount.toString(),
                    "&mPayout=",
                    vault.vaultXRewardedOnMaturity.toString(),
                    "&vaultXRate=",
                    vault.vaultXInterestRate.toString(),
                    "&startTime=",
                    vault.createdAt.toString(),
                    "&termLength=",
                    vault.termLength.toString(),
                    "&lockedTokenAddress=",
                    Strings.toHexString(vault.lockedTokenAddress),
                    "&lockedTokenAmount=",
                    vault.lockedTokenAmount.toString(),
                    "&isMatured=",
                    vault.isMatured ? "true" : "false"
                )
            );
    }

    /**
     * @dev See {IERC721Metadata-tokenURI}.
     */
    function tokenURI(
        uint256 tokenId
    ) public view override(ERC721) returns (string memory) {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        //Build the String from memory on Read Request instead of storing on chain to save gas on token creation.
        string memory _tokenURI = putTogetherMetadataString(tokenId);

        // If both are set, concatenate the baseURI and tokenURI (via abi.encodePacked). This is the only conditional that should ever fire so let's check it first
        if (bytes(_tokenURI).length > 0) {
            return string(abi.encodePacked(_baseTokenURI, _tokenURI));
        }

        // If there is no base URI, return the token URI.
        if (bytes(_baseTokenURI).length == 0) {
            return _tokenURI;
        }

        // If there is a baseURI but no tokenURI, concatenate the tokenID to the baseURI.
        return string(abi.encodePacked(_baseTokenURI, tokenId.toString()));
    }

