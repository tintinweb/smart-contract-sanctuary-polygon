    /*
    Following two functions work immediately after all the Cbond variables except the interest rate have been set, will be inaccurate other times. To be used as part of Cbond creation.
  */

    /// @notice Parent function that calculates total interest rate and a total return of VaultX tokens after bond is fully matured
    /// @param tokenId The ID of the token to get the interest rate for
    /// @param vaultXAmount The amount of VaultX tokens used to create the PolyStake ^18
    /// @param duration The duration of the bond in seconds
    function getCVaultTotalReturn(
        uint256 tokenId,
        uint256 vaultXAmount,
        uint256 duration
    ) public view returns (uint256 interestRate, uint256 totalReturn) {
        interestRate = calculateTotalInterestRate(
            duration,
            getLuckyExtra(tokenId)
        );
        totalReturn =
            (vaultXAmount * (PERCENTAGE_PRECISION + interestRate)) /
            PERCENTAGE_PRECISION;
    }

    /// @notice Returns extra lucky bonus for a given token ID
    /// @param tokenId The ID of the token to get the lucky extra for
    /// @return luckyExtra The lucky extra for the token ID
    function getLuckyExtra(uint256 tokenId) internal view returns (uint256) {
        if (luckyEnabled) {
            if (tokenId % 1000 == 777) {
                return LUCKY_EXTRAS[2];
            }
            if (tokenId % 100 == 77) {
                return LUCKY_EXTRAS[1];
            }
            if (tokenId % 10 == 7) {
                return LUCKY_EXTRAS[0];
            }
        }
        return 0;
    }

    /// @notice Calculates the bonus interest rate for a given stake duration
    /// @dev the bonus interest rate is increased with the duration of the stake
    /// @dev the bonus increases linearly from 0% to MAXIMUM_BONUS_INTEREST_RATE over the duration of the stake
    /// @param stakeDurationInSeconds The duration of the stake in seconds
    /// @return bonusInterestRate The bonus interest rate for the stake duration
    function calculateBonusInterestRate(
        uint256 stakeDurationInSeconds
    ) public view returns (uint256) {
        UD60x18 stakeDuration = ud(stakeDurationInSeconds);
        UD60x18 maxDuration = ud(MAX_DURATION);
        UD60x18 durationRatio = stakeDuration.div(maxDuration);

        UD60x18 bonusInterest = durationRatio.pow(bonusRateExponent).mul(
            ud(maxBonusInterestRate)
        );

        uint256 bonusInterestRate = bonusInterest.intoUint256();

        return bonusInterestRate;
    }

    /// @notice Calculates the base interest rate
    /// @dev the base interest rate is increased with the circulating supply of VaultX
    /// @dev the base interest rate cannot be lower than MINIMUM_BASE_INTEREST_RATE or higher than MAXIMUM_BASE_INTEREST_RATE
    /// @param stakeDurationInSeconds The duration of the stake in seconds
    /// @return baseInterestRate The base interest rate
    function getBaseInterestRate(
        uint256 stakeDurationInSeconds
    ) public view returns (uint256) {
        uint256 totalSupply = IERC20(vaultXTokenAddress).totalSupply();
        uint256 lockedSupply = IERC20(vaultXTokenAddress).balanceOf(
            address(this)
        );
        uint256 baseInterestRate = BASE_INTEREST_RATE;
        uint256 circulatingSupply = totalSupply - lockedSupply;
        uint256 circulatingRatio = (circulatingSupply * 10000) / totalSupply; // Ratio in basis points (bps)

        if (circulatingRatio <= minCirculatingRatio) {
            // Less than min ratio of total supply circulating
            return MINIMUM_BASE_INTEREST_RATE;
        } else if (circulatingRatio <= 10000) {
            // Between min amount and 100% of total supply circulating
            uint256 decrease = (BASE_INTEREST_RATE *
                (10000 - circulatingRatio)) / (10000 - minCirculatingRatio);
            baseInterestRate -= decrease;
        }
        uint256 calculatedBaseInterestRate = (baseInterestRate *
            stakeDurationInSeconds) / ONE_YEAR;
        return calculatedBaseInterestRate;
    }

    /// @notice Calculates the total interest rate for a given stake duration
    /// @dev the total interest rate is the sum of the base interest rate and the bonus interest rate
    /// @param stakeDurationInSeconds The duration of the stake in seconds
    /// @param luckyExtra The extra interest rate for lucky Cbond
    /// @return totalInterestRate The total interest rate for the stake duration
    function calculateTotalInterestRate(
        uint256 stakeDurationInSeconds,
        uint256 luckyExtra
    ) public view returns (uint256) {
        uint256 baseInterestRate = getBaseInterestRate(stakeDurationInSeconds);
        uint256 bonusInterestRate = calculateBonusInterestRate(
            stakeDurationInSeconds
        );
        uint256 totalInterestRate = baseInterestRate + bonusInterestRate;
        return totalInterestRate + luckyExtra;
    }

    /// @notice Calculates the total interest rate for a given stake duration if it were to be updated
    /// @param duration The duration of the stake in seconds
    /// @param luckyExtra The extra interest rate for lucky Cbond
    /// @return totalInterestRate The total interest rate for the stake duration
    function getCVaultInterestRateIfUpdated(
        uint256 duration,
        uint256 luckyExtra
    ) public view returns (uint256) {
        return calculateTotalInterestRate(duration, luckyExtra);
    }

    /// @notice Records the current VaultX supply in the vaultXSupplyChange
    function recordVaultXSupply() private {
        // Set the maximum number of snapshots
        uint256 maxSnapshots = 720;

        // Calculate the current snapshot ID using mod
        uint256 snapshotId = (block.timestamp / 1 days) % maxSnapshots;

        uint256 totalVaulXSupply = IERC20(vaultXTokenAddress).totalSupply();
        // Calculate the effective total supply
        uint256 lockedAmount = IERC20(vaultXTokenAddress).balanceOf(
            address(this)
        );

        LockRecord storage lockRecord = lockRecords[snapshotId];

        // Check if the effective total supply has changed since the last snapshot
        if (lockRecord.totalLocked != lockedAmount) {
            lockRecord.totalLocked = lockedAmount;
            lockRecord.totalSupply = totalVaulXSupply;
            lockRecord.timestamp = block.timestamp;
        }
    }

    /// @notice Return all 720 lock records
    function getLockRecords() public view returns (MinimalLockRecord[] memory) {
        MinimalLockRecord[] memory records = new MinimalLockRecord[](720);
        for (uint i = 0; i < 720; i++) {
            records[i] = MinimalLockRecord(
                lockRecords[i].timestamp,
                lockRecords[i].totalLocked
            );
        }
        return records;
    }

    /// @notice Convience function to show the current penalty fee for a given stake by ID
    function penaltyFeeForTokenID(
        uint256 tokenId
    ) public view returns (uint256) {
        Vault memory vault = vaults[tokenId];
        uint256 penaltyFee = maxPenaltyFee -
            ((maxPenaltyFee - minPenaltyFee) *
                (block.timestamp - vault.createdAt)) /
            vault.termLength;
        return penaltyFee;
    }

    /// @notice Paginated function to get all vaults for a given address
    /// @param user The address of the user
    /// @param limit The number of vaults to return
    /// @param offset The offset to start from
    /// @return vaultsByAddress An array of vaults for the given address
    function getVaultsByAddress(
        address user,
        uint16 limit,
        uint16 offset
    ) public view returns (Vault[] memory) {
        uint256 balance = balanceOf(user);

        if (offset >= balance) {
            return new Vault[](0);
        }

        // If offset + limit exceeds balance, adjust limit
        if (offset + limit > balance) {
            limit = uint16(balance - offset);
        }

        Vault[] memory vaultsByAddress = new Vault[](limit);

        for (uint256 i = 0; i < limit; i++) {
            vaultsByAddress[i] = vaults[tokenOfOwnerByIndex(user, offset + i)];
        }
        return vaultsByAddress;
    }

