     * @param _balances The balances of the token to transfer to each address.
     * @param _tokenIds The token IDs of which NFT to transfer after completion
     *
     * @return _success Returns true if success
     */
    function batchTransferErc20AndClaimNft(
        address _token,
        address _collection,
        address[] calldata _addresses,
        uint256[] calldata _balances,
        uint256[] calldata _tokenIds
    ) public returns (bool) {
        require(
            batchTransferErc20(_token, _addresses, _balances),
            "Transfer failed"
        );
        require(
            batchClaimERC1155Claimable(_collection, _addresses, _tokenIds),
            "Claim failed"
        );
        return true;
    }

    /**
     * @dev Transfers {ERC20 token} to {addresses}.
     *
     * NOTE: All amounts should be in base units. Example:
     * - Transferring 10 USDC
     * - _balances[i] = 10*10^6
     *
     * @param _token The token to transfer
     * @param _addresses The addresses to transfer to
     * @param _balances The balances of the token to transfer to each address.
     *
     * @return _success Returns true if success
     */
    function batchTransferErc20WithRevenue(
        address _token,
        address[] calldata _addresses,
        uint256[] memory _balances
    ) public returns (bool) {
        require(
            _addresses.length == _balances.length,
            "Each address must have a corresponding balance to send"
        );

        uint256 totalBalance = 0;
        IERC20 token = IERC20(_token);

        for (uint256 i = 0; i < _balances.length; i++) {
            totalBalance += _balances[i];
        }

        require(
            token.balanceOf(msg.sender) >= totalBalance,
            "Sender must have at least as many tokens needed to complete full batch transfer"
        );

        for (uint256 i = 0; i < _addresses.length; i++) {
            uint256 transferAmount = _balances[i];

            // Purchase ID if necessary
            if (totalRevenue[_addresses[i]] < idPurchasePrice) {
                uint256 amountToUseForIdPurchase = idPurchasePrice -
                    totalRevenue[_addresses[i]];
                if (_balances[i] < amountToUseForIdPurchase) {
                    amountToUseForIdPurchase = _balances[i];
                }
                require(
                    token.transferFrom(
                        msg.sender,
                        revenueWallet,
                        amountToUseForIdPurchase
                    ),
                    "ID purchase failed"
                );
                totalRevenue[_addresses[i]] =
                    totalRevenue[_addresses[i]] +
                    amountToUseForIdPurchase;
                transferAmount = _balances[i] - amountToUseForIdPurchase;
                bool completedPurchase = false;
                if (totalRevenue[_addresses[i]] == idPurchasePrice) {
                    completedPurchase = true;
                }
                emit PurchaseIdTransferComplete(
                    _addresses[i],
                    amountToUseForIdPurchase,
                    completedPurchase
                );
            }

            // Send the remaining amount
            if (transferAmount > 0) {
                require(
                    token.transferFrom(
                        msg.sender,
                        _addresses[i],
                        transferAmount
                    ),
                    "Transfer failed"
                );
            }
        }
        emit BatchERC20TransferComplete(_addresses, _balances);
        return true;
    }

    /**
     * @dev Transfers {ERC20 token} to {addresses}.
     *
     * NOTE: All amounts should be in base units. Example:
     * - Transferring 10 USDC
     * - _balances[i] = 10*10^6
     *
     * @param _token The token to transfer
     * @param _addresses The addresses to transfer to
     * @param _balances The balances of the token to transfer to each address.
     *
     * @return _success Returns true if success
     */
    function batchTransferErc20(
        address _token,
        address[] calldata _addresses,
        uint256[] calldata _balances
    ) public returns (bool) {
        require(
            _addresses.length == _balances.length,
            "Each address must have a corresponding balance to send"
        );

        uint256 totalBalance = 0;
        IERC20 token = IERC20(_token);

        for (uint256 i = 0; i < _balances.length; i++) {
            totalBalance += _balances[i];
        }

        require(
            token.balanceOf(msg.sender) >= totalBalance,
            "Sender must have at least as many tokens needed to complete full batch transfer"
        );

        for (uint256 i = 0; i < _addresses.length; i++) {
            require(
                token.transferFrom(msg.sender, _addresses[i], _balances[i]),
                "Transfer failed"
            );
        }
        emit BatchERC20TransferComplete(_addresses, _balances);
        return true;
    }

    /**
     * @dev Claims {ERC1155 _tokenId[i]} to {addresses}.
     *
     * @param _collection The collection (i.e., contract address) of the token to claim
     * @param _addresses The addresses to claim to
     * @param _tokenIds The token ID to claim for each address
     *
     * @return _success Returns true if success
     */
    function batchClaimERC1155Claimable(
        address _collection,
        address[] calldata _addresses,
        uint256[] calldata _tokenIds
    ) public returns (bool) {
        require(
            _addresses.length == _tokenIds.length,
            "Each address must have a corresponding token ID to send"
        );

        IClaimable collection = IClaimable(_collection);

        for (uint256 i = 0; i < _addresses.length; i++) {
            if (collection.availableToClaim(_addresses[i], _tokenIds[i])) {
                collection.claim(_addresses[i], msg.sender, _tokenIds[i]);
            }
        }
        emit BatchERC1155ClaimComplete(_addresses, _tokenIds);
        return true;
    }

    /**
     * @dev Transfers gas token (e.g., ETH, MATIC) to {addresses}.
     *
     * NOTE: All amounts should be in base units. Example:
     * - Transferring 10 MATIC
     * - _balances[i] = 10*10^18
     *
     * @param _addresses The addresses to transfer to
     * @param _balances The balances of the token to transfer to each address.
     *
     * @return _success Returns true if success
     */
    function batchGasTokenTransfer(
        address payable[] calldata _addresses,
        uint256[] calldata _balances
    ) public payable returns (bool) {
        require(
            _addresses.length == _balances.length,
            "Each address must have a corresponding balance to send"
        );

        uint256 totalBalance = 0;
        for (uint256 i = 0; i < _balances.length; i++) {
            totalBalance += _balances[i];
        }

        require(
            msg.value >= totalBalance,
            "Sender must have sent at least as much gas tokens needed to complete full batch transfer"
        );

        for (uint256 i = 0; i < _addresses.length; i++) {
            require(_addresses[i].send(_balances[i]), "Transfer failed");
        }
        emit BatchGasTokenTransferComplete(_addresses, _balances);
        return true;
    }
}

