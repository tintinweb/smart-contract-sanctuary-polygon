contract TokenBatch {
    event PurchaseIdTransferComplete(
        address purchaser,
        uint256 amount,
        bool completedPurchase
    );
    event BatchERC20TransferComplete(address[] addresses, uint256[] balances);
    event BatchERC1155ClaimComplete(address[] addresses, uint256[] tokenIds);
    event BatchGasTokenTransferComplete(
        address payable[] addresses,
        uint256[] balances
