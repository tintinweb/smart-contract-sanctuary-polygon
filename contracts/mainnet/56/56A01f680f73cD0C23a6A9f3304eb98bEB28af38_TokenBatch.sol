pragma solidity ^0.8.0;

import "./IClaimable.sol";
import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

interface IWMATIC {
    function balanceOf(address account) external view returns (uint256);

    function withdraw(uint wad) external;
}

contract UniswapV3Proxy {
    event TransferTokensIn(address sender, uint256 amount);
    event ApproveProxy(address protocol, uint256 amount);
    event Swap(
        address swapper,
        address fromToken,
        address toToken,
        uint256 amountIn,
        uint256 amountOut
    );
    event WithdrawBaseToken(address baseToken, uint256 amount);
    event ClaimUserJourneyNft(address claimant, uint256 tokenId);

    ISwapRouter public immutable swapRouter;
    IWMATIC public wmatic;

    struct SwapAndClaimNft {
        address tokenIn;
        address tokenOut;
        address collectionAddress;
        address faucetWalletAddress;
        address revenueWalletAddress;
        address recipient;
        uint24 fee;
        uint256 amountIn;
        uint256 amountOutMinimum;
        uint256 claimableTokenId;
        uint160 sqrtPriceLimitX96;
        uint256 tipAmount;
    }

    constructor(ISwapRouter _swapRouter, address _wmaticAddress) {
        swapRouter = _swapRouter;
        wmatic = IWMATIC(_wmaticAddress);
    }

    // todo lend and claim nft
    // todo batch swap brz to usdc and send

    /**
     * @dev Proxy to Uniswap V3's exactInputSingle function, which runs the exchange and withdraws MATIC if requested
     *
     * @param _params parameters for the swapAndClaimNft function
     *
     * @return success whether or not the transaction succeeded
     */
    function swapAndClaimNft(
        SwapAndClaimNft calldata _params
    ) external payable returns (bool success) {
        // todo calculate uniswap metadata onchain

        // If the user requests greater than their max balance, use just their max balance
        uint256 amountIn = _params.amountIn;
        uint256 maxBalance = IERC20(_params.tokenIn).balanceOf(msg.sender);
        if (_params.amountIn > maxBalance) {
            amountIn = maxBalance;
        }

        // Transfer tokens into contract
        // NOTE: You only need to approve/transfer tokens in if you're not using a base token. Otherwise it's included in the msg.value
        if (msg.value == 0) {
            require(
                IERC20(_params.tokenIn).transferFrom(
                    msg.sender,
                    address(this),
                    amountIn
                ),
                "Failed to transfer tokens into proxy"
            );
        }

        emit TransferTokensIn(msg.sender, amountIn);

        // Handle tip
        handleTip(
            _params.tipAmount,
            _params.tokenIn,
            _params.revenueWalletAddress
        );

        // Update amount to swap
        amountIn = amountIn - _params.tipAmount;

        // Approve swap router to take tokens from contract
        // NOTE: You still need to approve WMATIC when swapping, you just don't have to transfer it into the contract
        require(
            IERC20(_params.tokenIn).approve(address(swapRouter), amountIn),
            "Failed to approve proxy on SwapRouter"
        );
        emit ApproveProxy(address(swapRouter), amountIn);

        // Execute swap
        uint256 amountOut = swap(
            _params.tokenIn,
            _params.tokenOut,
            _params.fee,
            amountIn,
            _params.amountOutMinimum,
            _params.sqrtPriceLimitX96
        );

        // If you sent MATIC, withdraw the WMATIC that resulted from this transaction
        if (_params.tokenOut == address(wmatic)) {
            withdraw(_params.recipient);
            return true;
        }

        // Approve ERC20 to transfer back to user
        IERC20(_params.tokenOut).approve(_params.tokenOut, amountOut);
        emit ApproveProxy(_params.tokenOut, amountOut);

        // Transfer received ERC20 back to user
        IERC20(_params.tokenOut).transfer(_params.recipient, amountOut);

        // If you need to claim a user journey NFT, claim the user journey NFT
        IClaimable collection = IClaimable(_params.collectionAddress);
        if (collection.availableToClaim(msg.sender, _params.claimableTokenId)) {
            collection.claim(
                msg.sender,
                _params.faucetWalletAddress,
                _params.claimableTokenId
            );
            emit ClaimUserJourneyNft(msg.sender, _params.claimableTokenId);
        }

        return true;
    }

    // Allows contract to receive base token when withdrawing
    receive() external payable {}

    /**
     * Swaps on Uniswap
     *
     * @param _tokenIn inbound token
     * @param _tokenOut outbound token
     * @param _fee fee tier
     * @param _amountOutMinimum minimum expected amount receied
     * @param _sqrtPriceLimitX96 price
     *
     * @return amountOut amount actually received
     */
    function swap(
        address _tokenIn,
        address _tokenOut,
        uint24 _fee,
        uint256 _amountIn,
        uint256 _amountOutMinimum,
        uint160 _sqrtPriceLimitX96
    ) private returns (uint256) {
        uint256 amountOut = swapRouter.exactInputSingle{value: msg.value}(
            ISwapRouter.ExactInputSingleParams({
                tokenIn: _tokenIn,
                tokenOut: _tokenOut,
                fee: _fee,
                recipient: address(this),
                deadline: block.timestamp,
                amountIn: _amountIn,
                amountOutMinimum: _amountOutMinimum,
                sqrtPriceLimitX96: _sqrtPriceLimitX96
            })
        );
        emit Swap(msg.sender, _tokenIn, _tokenOut, _amountIn, amountOut);
        return amountOut;
    }

    function handleTip(
        uint256 _tipAmount,
        address _tokenIn,
        address _revenueWalletAddress
    ) private returns (bool) {
        if (_tipAmount > 0 && msg.value == 0) {
            require(
                IERC20(_tokenIn).approve(_tokenIn, _tipAmount),
                "Failed to approve tip transfer out"
            );
            // Transfer tip out
            require(
                IERC20(_tokenIn).transfer(_revenueWalletAddress, _tipAmount),
                "Failed to process revenue"
            );
        } else if (_tipAmount > 0 && msg.value > 0) {
            require(
                payable(_revenueWalletAddress).send(_tipAmount),
                "Failed to process base token revenue"
            );
        }

        return true;
    }

    /**
     * @dev withdraws WMATIC into MATIC for the user
     */
    function withdraw(address _recipient) private {
        uint256 balanceOf = wmatic.balanceOf(address(this));
        require(balanceOf > 0, "0 WMATIC balance");

        wmatic.withdraw(balanceOf);
        require(
            payable(_recipient).send(balanceOf),
            "Failed to send unwrapped base token"
        );
        emit WithdrawBaseToken(msg.sender, balanceOf);
    }
}

