    function approve(
        address _approved, 
        uint256 _tokenId
    ) external {
        if (_approved == msg.sender) {
            revert CANNOT_APPROVE_SELF();
        }
        if(_approved == address(0)){
            revert ZERO_ADDRESS();  
        }
        if(_owners[_tokenId] != msg.sender){
            revert INVALID_TOKEN_OWNER();
        } 

        _tokenApprovals[_tokenId] = _approved;

        emit Approval(msg.sender, _approved, _tokenId);
    }

