            _tiers.length != _tierDetails.length
        ){
            revert UNEQUAL_ARRAY_LENGTHS();
        }
        if(
            _tiers.length == 0
        ){
            revert EMPTY_ARRAYS();
        }
        uint i;
        uint length = _tiers.length;
        for (i=0; i<length; ++i){
            if(
                _tiers[i] == 0 ||
                _tierDetails[i].price == 0 ||
                _tierDetails[i].duration == 0 ||
                _tierDetails[i].duration < -1 ||
                _tierDetails[i].supply == 0 
            ){
                revert INVALID_ARRAY_VALUES();
            }
