        maxKeysPerAddress = _maxKeysPerAddress;
        baseTokenURI = _baseTokenURI;
        for(i =0; i<length; ++i){
            isTierValid[_tiers[i]] = true;
            tierDetails[_tiers[i]] = _tierDetails[i];
            totalTiers+=1;
        }
        tokenTraxxAddress = _tokenTraxxAddress;
        _grantRole(DEFAULT_ADMIN_ROLE, _owner);
        _grantRole(PAUSER_ROLE, _owner);
        _grantRole(UPGRADER_ROLE, _owner);
        _grantRole(MAINTAINER_ROLE, _owner);
        _grantRole(MAINTAINER_ROLE, _maintainer);
