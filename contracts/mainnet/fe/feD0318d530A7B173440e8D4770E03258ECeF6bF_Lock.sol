    function safeTransferFrom(
      address _from,
      address _to,
      uint256 _tokenId,
      bytes memory _data
    ) public {
      transferFrom(_from, _to, _tokenId);
      if (!_checkOnERC721Received(_from, _to, _tokenId, _data)) {
        revert NON_COMPLIANT_ERC721_RECEIVER();
      }
    }

    /**
