        }
        if(
            !validPurchaseParameters(_recipients, _quantities, _tiers)
        ){
            revert INVALID_PARAMETERS();
        }
        if(
            !supplyAvailability(_recipients, _quantities, _tiers)
        ){
            revert QUANTITY_EXCEEDS_TOTAL_SUPPLY_OR_MAX_KEY_ALLOWANCE(); 
        }

        for(uint i =0; i<_recipients.length; ++i){
            for (uint j =0; j<_quantities[i]; ++j){
                _createNewKey(_recipients[i], _tiers[i]);
            }
        }        
    }

    /** 
    * @notice Count all NFTs assigned to an owner
    * @dev NFTs assigned to the zero address are considered invalid, and this
    *  function throws for queries about the zero address.
    * @param _owner An address for whom to query the balance
    * @return The number of NFTs owned by `_owner`, possibly zero
    */
    function balanceOf(address _owner) external view returns (uint256){
        if(
            _owner == address(0)
        ){
            revert ZERO_ADDRESS();
        }
        return _balances[_owner];
    }

    /** 
    * @notice Find the owner of an NFT
    * @dev NFTs assigned to zero address are considered invalid, and queries
    *  about them do throw.
    * @param _tokenId The identifier for an NFT
    * @return The address of the owner of the NFT
    */
    function ownerOf(uint256 _tokenId) external view returns (address){
        if(
            _tokenId > tokenId.current() ||
            _owners[_tokenId] == address(0)
        ){
            revert TOKEN_ID_NOT_FOUND();
        }
        return _owners[_tokenId];
    }

    /**
    * @dev Returns the contract URI
    */
    function contractURI() external view returns (string memory) {
        return uri;
    }

    /** 
    * @notice Query if a contract implements an interface
    * @param interfaceId The interface identifier, as specified in ERC-165
    * @dev Interface identification is specified in ERC-165. This function
    *  uses less than 30,000 gas.
    * @return `true` if the contract implements `interfaceID` and
    *  `interfaceID` is not 0xffffffff, `false` otherwise
    */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(AccessControlUpgradeable, IERC165Upgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function pause() external onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() external onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    // Public Functions :

    /**
     * @dev Returns the token URI of a specific token
     * @param _tokenId Id of token which is to be checked.
     * @notice currently we are using a common base token uri for now
     */
    function tokenURI(uint256 _tokenId)
        public
        view
        virtual
        returns (string memory)
    {
        if(
            _tokenId > tokenId.current() ||
            _owners[_tokenId] == address(0)
        ){
            revert TOKEN_ID_NOT_FOUND();
        }
        return baseTokenURI;
    }

    // Internal Functions :

    /**
     * @dev Validates purchase parameters.
     * @param _recipients array of addresses of the recipients of the purchased key
     * @param _quantities is the array of number of keys 
     * @param _tiers is the array of price associated to a key       
     * Note _tiers are sent here so that we can get valid expirationDuration 
     * that are stored in state 
     */
    function validPurchaseParameters(
       address[] memory _recipients,
       uint8[] memory _quantities,
       uint16[] memory _tiers
    ) internal view returns(bool){
        for(uint i = 0; i<_recipients.length; ++i){
            if(
                _recipients[i] == address(0) ||
                _quantities[i] == 0 ||
                isTierValid[_tiers[i]] == false 
            ){
                return false;
            }
        }
        return true;
    }

    /**
    * @dev Checks if quantity is less than total supply.
    * @param _recipients array of addresses of the recipients of the purchased key
    * @param _quantities is the array of number of keys 
    * @param _tiers is the array of tier's associated to a key
    */
    function supplyAvailability(
        address[] memory _recipients,
        uint8[] memory _quantities,
        uint16[] memory _tiers
    ) internal view returns(bool){

        uint16[] memory _tiersArr = new uint16[](_tiers.length);
        uint8[] memory _quantitiesArr = new uint8[](_tiers.length);
        uint8[] memory _quantitiesArr2 = new uint8[](_tiers.length);
        address[] memory _recipientsArr = new address[](_tiers.length);
        
        uint index = 0;

        // stores unique tier numbers in _tiersArr and corresponding quantities in _quantitiesArr
        for (uint i = 0; i < _tiers.length; i++) {
            bool isDuplicate = false;
            for (uint j = 0; j < index; j++) {
                if (_tiers[i] == _tiersArr[j]) {
                    _quantitiesArr[j] += _quantities[i];
                    isDuplicate = true;
                    break;
                }
            }
            if (!isDuplicate) {
                _tiersArr[index] = _tiers[i];
                _quantitiesArr[index] = _quantities[i];
                index++;
            }
        }

        uint index2 = 0;

        // stores unique recipients in _recipientsArr and corresponding quantities in _quantitiesArr2
        for (uint i = 0; i < _tiers.length; i++) {
            bool isDuplicate = false;
            for (uint j = 0; j < index2; j++) {
                if (_recipients[i] == _recipientsArr[j]) {
                    _quantitiesArr2[j] += _quantities[i];
                    isDuplicate = true;
                    break;
                }
            }
            if (!isDuplicate) {
                _recipientsArr[index2] = _recipients[i];
                _quantitiesArr2[index2] = _quantities[i];
                index2++;
            }
        }

        // Resizes the _tiersArr and _quantitiesArr to its actual length
        assembly {
            mstore(_tiersArr, index)
            mstore(_quantitiesArr, index)
            mstore(_recipientsArr, index2)
            mstore(_quantitiesArr2, index2)
        }

        // checks if quantity is within the allowed supply limit of a tier
        for(uint16 i=0; i<index; ++i){
            if(
                _quantitiesArr[i] + quantitySold[_tiersArr[i]] > tierDetails[_tiersArr[i]].supply
            ){
                return false;
            }
        }

        for(uint16 i=0; i<index2; ++i){
            if(
                _balances[_recipientsArr[i]] + _quantitiesArr2[i] > maxKeysPerAddress
            ){
                return false;
            }
        }

        return true;
    }

    /**
    * @dev Validates traxx fees balance and allowance, sends fees to dead address.
    * @param _quantities is the array of number of keys 
    * @param _tiers is the array of tier's associated to a key       
    * Requirement:
    * Adequate balance and allowance for this contract to transfer the fees
    */
    function validateAndTransferFees( 
        uint8[] memory _quantities,
        uint16[] memory _tiers
    ) internal {
        uint256 totalCalculatedPrice;

        for(uint i=0; i<_quantities.length; ++i){
            totalCalculatedPrice += _quantities[i] * tierDetails[_tiers[i]].price;
        }

        if(
            IERC20(tokenTraxxAddress).balanceOf(msg.sender) < totalCalculatedPrice ||
            IERC20(tokenTraxxAddress).allowance(msg.sender, address(this)) < totalCalculatedPrice
        ) {
            revert INSUFFICIENT_BALANCE_OR_ALLOWANCE();
        }

        uint256 balBefore = IERC20(tokenTraxxAddress).balanceOf(msg.sender);

        // send to this address ?
        bool success = IERC20(tokenTraxxAddress).transferFrom(
            msg.sender,
            nullAddress,
            totalCalculatedPrice
        );

        uint256 balAfter = IERC20(tokenTraxxAddress).balanceOf(msg.sender);

        // check if this is of any use ?
        if(
            !(success && balAfter == balBefore - totalCalculatedPrice)
        ) {
            revert TRANSFER_FAILED();
        }
    }

    /**
    * @dev Loops through the array and calls _createNewKey()
    * @param _recipients array of addresses of the recipients of the purchased key
    * @param _quantities is the array of number of keys 
    * @param _tiers is the array of price associated to a key   
    */
    function createNewKeys(
        address[] memory _recipients,
        uint8[] memory _quantities,
        uint16[] memory _tiers        
    ) internal {
        for(uint i =0; i<_recipients.length; ++i){
            for (uint j =0; j<_quantities[i]; ++j){
                _createNewKey(_recipients[i], _tiers[i]);
            }
        }        
    }

    /**
    * Record Creates a new key 
    * @param _recipient address for which a new key is generated
    * @param _tier associated with a key
    */
    function _createNewKey(
        address _recipient,
        uint16 _tier
    ) internal {

        tokenId.increment();
        uint id = tokenId.current();

        uint expirationTimestamp;
        tierDetails[_tier].duration == -1 ? expirationTimestamp = 0 : expirationTimestamp = block.timestamp + uint256(tierDetails[_tier].duration);
        
        uint32 i = _balances[_recipient];
       
        // creates a new key
        Key memory key = Key(id, _tier, expirationTimestamp);
        ownedKeys[_recipient][i] = key;
        _owners[id] = _recipient;

        expirationTime[id] = expirationTimestamp;

        _balances[_recipient] += 1;
        quantitySold[_tier] += 1;    

        emit Transfer(
            address(0),
            _recipient,
            id
        );
        emit keyCreated(
            address(0), 
            _recipient,
            id,
            _tier,
            expirationTimestamp
        );

    }

    /**
    * @param _from address that will send the token
    * @param _to address that will receive the token
    * @param _tokenId the id of the token
    * @dev Internal function to transfer token
    */
    function _transfer(
        address _from,
        address _to,
        uint _tokenId
    ) internal {
        if(
            _owners[_tokenId] != _from
        ){
            revert INVALID_TOKEN_OWNER();
        }

        Key memory key =_removeKey(_from, _tokenId);
        ownedKeys[_to][_balances[_to]] = key;

        _balances[_from] -= 1;
        _balances[_to] += 1;

        delete _tokenApprovals[_tokenId];

        _owners[_tokenId] = _to; 

        emit Transfer(_from, _to, _tokenId);

    }

    /**
    * @param _from address that will remove the token
    * @param _tokenId the id of the token
    * @dev Internal function to remove key from struct
    */
    function _removeKey(
        address _from,
        uint _tokenId
    ) internal returns(Key memory key){
        for(uint32 i=0; i<_balances[_from]; ++i){
            key = ownedKeys[_from][i];
            if(key.tokenId == _tokenId){
                ownedKeys[_from][i] = ownedKeys[_from][_balances[_from]-1];
                delete ownedKeys[_from][_balances[_from]-1];
                return key;
            }
        }
    }

    /**
    * @dev Internal function to invoke `onERC721Received` on a target address
    * The call is not executed if the target address is not a contract
    * @param _from address representing the previous owner of the given token ID
    * @param _to target address that will receive the tokens
    * @param _tokenId uint256 ID of the token to be transferred
    * @param _data bytes optional data to send along with the call
    * @return whether the call correctly returned the expected magic value
    */
    function _checkOnERC721Received(
      address _from,
      address _to,
      uint256 _tokenId,
      bytes memory _data
    ) internal returns (bool) {
        if (!isContract(_to)) {
          return true;
        }
        bytes4 retval = IERC721ReceiverUpgradeable(_to).onERC721Received(
            msg.sender,
            _from,
            _tokenId,
            _data
        );
        return (retval == _ERC721_RECEIVED);
    }

    /**
    * @dev Returns true if `account` is a contract.
    *
    * [IMPORTANT]
    * ====
    * It is unsafe to assume that an address for which this function returns
    * false is an externally-owned account (EOA) and not a contract.
    *
    * Among others, `isContract` will return false for the following
    * types of addresses:
    *
    *  - an externally-owned account
    *  - a contract in construction
    *  - an address where a contract will be created
    *  - an address where a contract lived, but was destroyed
    * ====
    */
    function isContract(
      address account
    ) internal view returns (bool) {
        // This method relies on extcodesize, which returns 0 for contracts in
        // construction, since the code is only stored at the end of the
        // constructor execution.
        uint256 size;
        assembly {
          size := extcodesize(account)
        }
        return size > 0;
    }

    /**
     * @dev Function that should revert when `msg.sender` is not authorized to upgrade the contract. Called by
     * {upgradeTo} and {upgradeToAndCall}.
     * @param newImplementation address of the new implementation
     */
    function _authorizeUpgrade(address newImplementation)
        internal
        onlyRole(UPGRADER_ROLE)
        override
    {}

}

