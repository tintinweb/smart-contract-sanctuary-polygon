    * @param _to address that will receive the token
    * @param _tokenId the id of the token
    */
    function transferFrom(
        address _from,
        address _to,
        uint256 _tokenId
    ) public {
        if(
            _to == address(0) || 
            _from == address(0)
        ){
            revert ZERO_ADDRESS();  
        }
        if(
            msg.sender != _tokenApprovals[_tokenId] && 
            msg.sender != _owners[_tokenId] &&
            !_operatorApprovals[_owners[_tokenId]][msg.sender]
        ){
            revert INVALID_APPROVAL();
        }
        if(
            expirationTime[_tokenId] != 0 &&
            expirationTime[_tokenId] < block.timestamp  
        ){
            revert TOKEN_HAS_EXPIRED();
        }
        _transfer(_from, _to, _tokenId);
    }

    /**
    * @dev Sets approval of a token
    * Approved address is allowed to transfer/burn the token of the sender on their behalf
    * @param _tokenId token id to be approved
    * @param _approved operator address to set the approval
