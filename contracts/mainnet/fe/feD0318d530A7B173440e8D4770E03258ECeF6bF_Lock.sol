    function editMetadata (
        string memory _lockName,
        string memory _lockSymbol,
        string memory _uri,
        string memory _baseTokenURI
    ) external adminOrMaintainer {
        if(
            bytes(_lockName).length == 0 ||
            bytes(_lockSymbol).length == 0 ||
            bytes(_uri).length == 0  ||
            bytes(_baseTokenURI).length == 0
        ){
            revert EMPTY_STRING_PARAMETER();
        }
