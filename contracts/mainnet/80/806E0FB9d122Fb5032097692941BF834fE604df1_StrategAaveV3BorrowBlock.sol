pragma solidity ^0.8.0;

import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {SafeERC20} from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import {IPool} from "@aave/core-v3/contracts/interfaces/IPool.sol";
import {IAaveOracle} from "@aave/core-v3/contracts/interfaces/IAaveOracle.sol";
import {DataTypes} from "@aave/core-v3/contracts/protocol/libraries/types/DataTypes.sol";

import {IStrategStrategyBlock} from "@murphy-labs/strateg-protocol-core/contracts/interfaces/IStrategStrategyBlock.sol";
import {IStrategOracleBlock} from "@murphy-labs/strateg-protocol-core/contracts/interfaces/IStrategOracleBlock.sol";
import {LibBlock} from "@murphy-labs/strateg-protocol-core/contracts/libraries/LibBlock.sol";
import {IStrategAaveV3PositionManager} from "@murphy-labs/strateg-protocol-borrow/contracts/interfaces/IStrategAaveV3PositionManager.sol";

/**
 * @title Aave V3 Borrow Strateg. Block
 * @author Bliiitz
 * @notice Block to use position manager instance to borrow a token against a collateral on Aave V3
 * @custom:block-id AAVE_V3_BORROW
 * @custom:block-type position-manager
 * @custom:block-action Borrow
 * @custom:block-protocol-id AAVE_V3
 * @custom:block-protocol-name Aave v3
 * @custom:block-params-tuple tuple(address positionManager, address collateral, address dept, uint256 tokenInPercent)
 */
contract StrategAaveV3BorrowBlock is IStrategStrategyBlock, IStrategOracleBlock {
    using SafeERC20 for IERC20;

    IPool public immutable pool;
    IAaveOracle public immutable oracle;
    string public ipfsHash;
   
    struct BlockParameters {
        address positionManager;
        address collateral;
        address dept;
        uint256 tokenInPercent;
    }

    
    constructor(
        address _pool,
        address _oracle,
        string memory _ipfsHash
    ) {
        pool = IPool(_pool);
        oracle = IAaveOracle(_oracle);
        ipfsHash = _ipfsHash;
    }

    function enter(uint256 _index) external {

        BlockParameters memory parameters = abi.decode(
            LibBlock.getStrategyStorageByIndex(_index),
            (BlockParameters)
        );

        uint256 collateralAmount = IERC20(parameters.collateral).balanceOf(address(this)) * parameters.tokenInPercent / 10000;
        IStrategAaveV3PositionManager pm = IStrategAaveV3PositionManager(parameters.positionManager);

        IERC20(parameters.collateral).safeApprove(address(pool), collateralAmount);
        pm.borrow(collateralAmount);        
    }

    function exit(uint256 _index, uint256) external {
        BlockParameters memory parameters = abi.decode(
            LibBlock.getStrategyStorageByIndex(_index),
            (BlockParameters)
        );

        bytes memory dynParameters = LibBlock.getDynamicBlockData(_index);

        DataTypes.ReserveData memory reserveData = pool.getReserveData(parameters.dept);
        uint256 deptTokenBal = IERC20(parameters.dept).balanceOf(address(this));
        uint256 repayAmount = IERC20(reserveData.variableDebtTokenAddress).balanceOf(parameters.positionManager);
        
        if(repayAmount <= deptTokenBal)
            IERC20(parameters.dept).safeTransfer(parameters.positionManager, repayAmount);
        else 
            IERC20(parameters.dept).safeTransfer(parameters.positionManager, deptTokenBal);

        IStrategAaveV3PositionManager(parameters.positionManager).repay(dynParameters);
    }
    
    function oracleEnter(OracleResponse memory _before, bytes memory _parameters) external view returns (OracleResponse memory) {
        BlockParameters memory parameters = abi.decode(_parameters, (BlockParameters));
        OracleResponse memory before = _before;
        uint256 collateralBal = LibBlock.findTokenAmount(parameters.collateral, before);

        uint256 amountToDeposit;
        if(collateralBal == 0) {
            collateralBal = IERC20(parameters.collateral).balanceOf(before.vault);
            amountToDeposit =  collateralBal * parameters.tokenInPercent / 10000;
            before = LibBlock.addTokenAmount(parameters.collateral, collateralBal, before);
        } else {
            amountToDeposit =  collateralBal * parameters.tokenInPercent / 10000;
        }

        uint256 borrowAmountFor = IStrategAaveV3PositionManager(parameters.positionManager).borrowAmountFor(amountToDeposit);

        OracleResponse memory _after = LibBlock.removeTokenAmount(parameters.collateral, amountToDeposit, before);
        _after = LibBlock.addTokenAmount(parameters.dept, borrowAmountFor, _after);
        
        return _after;
    }
    
    function oracleExit(OracleResponse memory _before, bytes memory _parameters) external view returns (OracleResponse memory) {
        BlockParameters memory parameters = abi.decode(_parameters, (BlockParameters));
        OracleResponse memory before = _before;
        uint256 deptTokenBal = LibBlock.findTokenAmount(parameters.dept, before);

        if(deptTokenBal == 0) {
            deptTokenBal = IERC20(parameters.dept).balanceOf(before.vault);
            before = LibBlock.addTokenAmount(parameters.dept, deptTokenBal, before);
        }

        DataTypes.ReserveData memory reserveData = pool.getReserveData(parameters.dept);
        uint256 repayAmount = IERC20(reserveData.variableDebtTokenAddress).balanceOf(parameters.positionManager);
        
        uint256 toSendForRepay;
        if(repayAmount <= deptTokenBal) {
            toSendForRepay = repayAmount;
        } else {
            toSendForRepay = deptTokenBal;
        }

        uint256 collateralReturned = IStrategAaveV3PositionManager(parameters.positionManager).returnedAmountAfterRepay(toSendForRepay);

        OracleResponse memory _after = LibBlock.removeTokenAmount(parameters.dept, toSendForRepay, before);
        _after = LibBlock.addTokenAmount(parameters.collateral, collateralReturned, _after);
        
        return _after;
    }
}

