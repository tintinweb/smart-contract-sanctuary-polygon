            toRedeem = IERC20(aaveToken).balanceOf(address(this));
        } else {
            toRedeem = collateral * _allocation * borrowRate / 1e18 / totalAllocation; 
        }
        
        // Make sure there is enough matic on the balance for repayment 
        if (address(this).balance < toRepay){
            (uint256 reserveA, uint256 reserveB, ) = IUniswapV2Pair(baseParams.want)
            .getReserves();
            uint256 toSwap = baseParams.uniV2Router.getAmountIn(toRepay - address(this).balance, reserveB, reserveA);
            // swap for additional Matic
            uint256[] memory swapedAmounts = baseParams.uniV2Router.swapExactTokensForETH(
                        toSwap,
                        0,
                        token1ToToken0,
                        address(this),
                        block.timestamp
                    ); 
            return toSwap;
        }
        require(address(this).balance >= toRepay, "Insufficient balance");
        wrappedTokenGateway.repayETH{value: toRepay}(address(aavePool), toRepay, 2, address(this));
        // Redeem corresponsing collateral amount 
        aavePool.withdraw(baseParams.token1, toRedeem, address(this));
    }

    // Stored in basis points
    // 7500 is 75%
    function setCollateralFactorAdmin(uint256 _rate) public onlyRole(ADMIN_ROLE){
        require(_rate <= getLtv() && _rate > 0, "Wrong rate");
        collateralFactorAdmin = _rate;
    } 

    /**
     * @notice Amount of LPs staked into Masterchef
     */
    function balance() public view returns (uint256 amountWant) {
        (amountWant,) = IMiniApeV2(baseParams.masterchef).userInfo(baseParams.poolId, address(this));
    }

    function getAaveRewards(address _aaveAsset) public view returns (address[] memory, uint256[] memory){
        Helpers.getAaveRewards(_aaveAsset, rewardsController);
    }

    function withdrawAaveRewards() external {
        require (hasRole(DEFAULT_ADMIN_ROLE, msg.sender) ||  hasRole(SERVER_ROLE, msg.sender), "Unauthorized caller");
        (, uint256[] memory rewards1) = Helpers.getAaveRewards(aaveToken, rewardsController);
        (, uint256[] memory rewards2) = Helpers.getAaveRewards(aaveDebtToken, rewardsController);
        if (!_isAccrued(rewards1) && !_isAccrued(rewards2)) revert ("Nothing to compound"); 

        address[] memory assets = new address[](2);
        assets[0] = aaveToken;
        assets[1] = aaveDebtToken;

        (address[] memory rewardsList, uint256[] memory claimedAmounts) = rewardsController.claimAllRewards(assets, address(this));
        for (uint256 i = 0; i < rewardsList.length; i++) {
            if (claimedAmounts[i] != 0){
                IERC20(rewardsList[i]).safeTransfer(treasury, claimedAmounts[i]);
            }
        }
    }

    /// A check for utilization being below kink
    function checkUtilization() public view returns (bool){
        (uint256 optRate, uint256 currRate) = Helpers.getOptimalUtilization(aavePool, baseParams.token0);
        if (currRate < optRate) {
            return true;
        }
    }


    function getLtv() public view returns (uint256 ltv){
        ltv = Helpers.getLtv(aavePool, baseParams.token1);
    }

    function calcMaxBorrow() public view returns (uint256 amountMaxToBorrow){
        // Get the borrower's account awailable liquidity (shown in dollar value by Aave) with ltv taken into account
        (,,uint256 availableBorrowsBase, , , ) = aavePool.getUserAccountData(address(this));
        // Adjust in case the factor is different than the borrowRate set by Admin
        uint256 factor = collateralFactorAdmin * 10000 / getLtv();
        uint256 liquidity = availableBorrowsBase * factor / 10000;
        amountMaxToBorrow = liquidity * 1e18 / Helpers.getOracleTokenPrice(aavePool, baseParams.token0);
    }

    //!!!Refactor logic from token0 token1 to avoid unneeded swaps
    function _placeDeposit(address[] memory pathTokenInToToken1) internal {
        if (msg.sender != router && !hasRole(SERVER_ROLE, msg.sender))
            revert("UnauthorizedCaller");
      
        uint256 tokenBal = 0;

        if(pathTokenInToToken1[0] == baseParams.rewardToken) {
            //autocompaund
            tokenBal = IERC20(pathTokenInToToken1[0]).balanceOf(address(this));
        } else if (isIterationStarted) {
            //return funds from short position
            tokenBal = IERC20(baseParams.token1).balanceOf(address(this)) - pendingDeposits;
        } else {
            //initial stake after starting iteration
            tokenBal = IERC20(baseParams.token1).balanceOf(address(this));
        }

        if (tokenBal == 0) revert("Nothing to deposit");

        if (pathTokenInToToken1.length > 1) {
            baseParams.uniV2Router.swapExactTokensForTokens(
                tokenBal,
                0,
                pathTokenInToToken1,
                address(this),
                block.timestamp
            );
        
        }
        Helpers._swapIn(baseParams.want, baseParams.uniV2Router, baseParams.token1, token1ToToken0);
        uint256 wantBal = IERC20(baseParams.want).balanceOf(address(this));

        IMiniApeV2(baseParams.masterchef).deposit(
            baseParams.poolId,
            wantBal,
            address(this));
       // emit Deposited(wantBal);
    }

    // interestRateMode 2 in borrowETH means the type of borrow debt - variable.
    function doShortCycles(uint256 _amountToCover) internal returns (uint256){
        uint256 shorted = 0;
        while (shorted < _amountToCover) {
            uint256 left = _amountToCover - shorted;
            uint256 maxBorrowAmount = calcMaxBorrow();
            console.log(maxBorrowAmount);
            console.log("Max borrow: ",maxBorrowAmount);
            if (left > maxBorrowAmount){
                wrappedTokenGateway.borrowETH(address(aavePool), maxBorrowAmount, 2, 0);
                // sell borrowed token for stablecoin and supply stable to the market
                swapAndSupply(maxBorrowAmount);
                shorted += maxBorrowAmount;
            } else {
                wrappedTokenGateway.borrowETH(address(aavePool), left, 2, 0);
                swapAndSupply(left);
                shorted += left;
            }
        }
        return shorted;
    }


    function swapAndSupply(uint256 _amount) internal {
        uint256[] memory swapedAmounts = new uint256[](token0ToToken1.length - 1);

        if (IERC20(baseParams.token0).balanceOf(address(this)) == _amount) {
                uint256[] memory swapedAmounts = baseParams.uniV2Router.swapExactTokensForTokens(
                _amount,
                0,
                token0ToToken1,
                address(this),
                block.timestamp
            ); 
        } else if ((address(this).balance) == _amount) {
                uint256[] memory swapedAmounts = baseParams.uniV2Router.swapExactETHForTokens{value: _amount}(
                0,
                token0ToToken1,
                address(this),
                block.timestamp
            ); 
        }
        uint256 toSupply = IERC20(baseParams.token1).balanceOf(address(this)) - pendingDeposits;
        aavePool.supply(baseParams.token1, toSupply, address(this), 0);
    }

