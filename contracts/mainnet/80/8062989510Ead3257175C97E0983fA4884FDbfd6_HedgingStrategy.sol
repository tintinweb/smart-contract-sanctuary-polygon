pragma solidity 0.8.17;

import "../../strategies/HedgingStrategy/HedgingCoreAave.sol";
import "hardhat/console.sol";

contract HedgingStrategy is HedgingCoreAave {
    using SafeERC20 for IERC20;

    struct PendingDeposit {
        address user;
        uint256 amount;
        uint256 iterationNumber;
    }

    //User pending usdc amount
    PendingDeposit[] public userPendingAllocation;

    //User percent of pool in mantissa format. 100% = 100e18
    mapping (address => uint256) public userAllocation;

    address[] internal users;

    function initialize(
        address _router,
        BaseParams memory _baseParams,
        address _aavePool,
        address _aaveToken,
        address _aaveDebtToken,
        address _rewardsController,
        address _wrappedTokenGateway,
        address[] memory _pathRewardToToken1,
        address[] memory _token0ToToken1,
        address[] memory _token0ToBase,
        address[] memory _token1ToBase,
        address[] memory _token1ToToken0) external initializer {
        isIterationStarted = false;
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(ADMIN_ROLE, msg.sender);
        _grantRole(SERVER_ROLE, msg.sender);
        __Pausable_init();
        router = _router;
        baseParams = _baseParams;
        aavePool = IAavePool(_aavePool);
        aaveToken = _aaveToken;
        aaveDebtToken = _aaveDebtToken;
        rewardsController = IRewardsController(_rewardsController);
        wrappedTokenGateway = IWrappedTokenGatewayV3(_wrappedTokenGateway);
        pathRewardToToken1 = _pathRewardToToken1;
        token0ToToken1 = _token0ToToken1;
        token0ToBase = _token0ToBase;
        token1ToBase = _token1ToBase;
        token1ToToken0 = _token1ToToken0;
        IERC20(baseParams.want).safeApprove(
            baseParams.masterchef,
            type(uint256).max
        );
        IERC20(baseParams.token1).safeApprove(
            address(baseParams.uniV2Router),
            type(uint256).max
        );
        IERC20(baseParams.token0).safeApprove(
            address(baseParams.uniV2Router),
            type(uint256).max
        );
        IERC20(baseParams.rewardToken).safeApprove(
            address(baseParams.uniV2Router),
            type(uint256).max
        );
        IERC20(baseParams.token1).safeApprove(address(aavePool), type(uint256).max);
        setCollateralFactorAdmin(getLtv());
    }

    /**
     * @notice Re-invests rewards.
     */

    function autocompound() external whenNotPaused {
        if (claimable() == 0) revert("Nothing to Compound");

        IMiniApeV2(baseParams.masterchef).harvest(baseParams.poolId, address(this));
        uint256 rewardBal = IERC20(baseParams.rewardToken).balanceOf(
            address(this)
        );
        uint256 fee = IERC20(baseParams.rewardToken)
            .balanceOf(address(this)) * baseParams.fee / 10000;
        IERC20(baseParams.rewardToken).safeTransfer(treasury, fee);
        _placeDeposit(pathRewardToToken1);
        // emit Compounded(rewardBal, fee, block.timestamp);
    }

    /**
     * @notice Depositing to the pending balance
     * @param user user address deposit for
     * @param tokenInToToken1 for interface unification
     * @param data any additional data
     */
    function deposit(
    address user,
    address[] memory tokenInToToken1,
    bytes memory data
) public whenNotPaused returns (uint256 pending) {
    require(msg.sender == router, "unallowed");
    _approveTokenIfNeeded(tokenInToToken1[0], address(baseParams.uniV2Router));
    if (tokenInToToken1.length > 1) {
        baseParams.uniV2Router.swapExactTokensForTokens(
            IERC20(tokenInToToken1[0]).balanceOf(address(this)),
            0,
            tokenInToToken1,
            address(this),
            block.timestamp
        );
    }

    pending = IERC20(baseParams.token1).balanceOf(address(this)) - pendingDeposits;
    _deposit(user, pending);
    emit Deposited(pending);
}

function _deposit(
    address user,
    uint256 amount
) private returns (uint256) {
    pendingDeposits += amount;

    for (uint256 i = 0; i < userPendingAllocation.length; i++) {
        if (userPendingAllocation[i].user == user) {
            userPendingAllocation[i].amount += amount;
            return userPendingAllocation[i].amount;
        }
    }

    userPendingAllocation.push(PendingDeposit(user, amount, iterationNumber + 1));
    return amount; 
}


    /**
     * @notice Withdrawing funds from staking and close short position
     * @dev Can only be called by LeechRouter.
     * @param allocation Share of pool in mantissa 18 decimals 100% = 100e18
     * @return tokenOutAmount Amount of the token returned to LeechRouter.
     */
    function withdraw(
        address user,
        uint256 allocation,
        address[] memory token0toTokenOut,
        address[] memory token1toTokenOut,
        bool isMigration
    ) public returns (uint256 tokenOutAmount) {
        require(msg.sender == router, "unallowed");
        require (allocation != 0, "Zero amount");

        if (user != router){
             if (allocation > userAllocation[user])
                revert("Wrong amount");
            userAllocation[user] -= allocation;
            totalAllocation -= allocation;

        }
        (uint256 amount0, uint256 amount1) = _withdrawFromFarm(allocation);

        uint256 swappedToken1;
        if (isShortOpened) {
            swappedToken1 = _closeShort(allocation);
        }

        uint256 bal0 = IERC20(baseParams.token0).balanceOf(address(this));

        uint256 bal1 = amount1 - swappedToken1;

        /// swap left into token out
        if (token0toTokenOut.length > 1 && bal0 !=0) {
                baseParams.uniV2Router.swapExactTokensForTokens(
                    bal0,
                    0,
                    token0toTokenOut,
                    address(this),
                    block.timestamp
            );
        }

        if (token1toTokenOut.length > 1) {
            baseParams.uniV2Router.swapExactTokensForTokens(
                bal1,
                0,
                token1toTokenOut,
                address(this),
                block.timestamp
            );
        }
        address tokenOut = token0toTokenOut[token0toTokenOut.length - 1];

        if (tokenOut == baseParams.token1) {
            tokenOutAmount =
            IERC20(tokenOut).balanceOf(address(this)) -
            pendingDeposits;
        } else {
            tokenOutAmount = IERC20(tokenOut).balanceOf(address(this));
        }

        if (isMigration){
            IERC20(tokenOut).safeTransfer(
            router,
            tokenOutAmount
        );
        }
        emit Withdrawn(allocation, tokenOutAmount);
    }

    function withdrawPendingAsset(address user, uint256 allocation) public {
        // Withdraw pending balance in base token
        require(msg.sender == router || msg.sender == user, "unallowed");

        for (uint256 i = 0; i < userPendingAllocation.length; i++) {
            if (userPendingAllocation[i].user == user) {
                require(
                    userPendingAllocation[i].amount >= allocation,
                    "incuficient balance"
                );
                userPendingAllocation[i].amount -= allocation;
                pendingDeposits -= allocation;
                IERC20(baseParams.token1).safeTransfer(user, allocation);
                return;
            }
        }
    }

    function _withdrawFromFarm(uint256 _allocation) internal returns (uint256 amount0, uint256 amount1){

        (uint256 totalInvestment, ) = IMiniApeV2(baseParams.masterchef).userInfo(
            baseParams.poolId,
            address(this)
        );

        uint widrawAmount = (totalInvestment * _allocation) / totalAllocation;
        IMiniApeV2(baseParams.masterchef).withdraw(baseParams.poolId, widrawAmount, address(this));
        (amount0, amount1) = _swapOut();
    }


    // function withdrawAll() public {
    //     uint256 tokenOutAmount = withdraw(msg.sender, totalAllocation, token0ToBase, token1ToBase, true);
    //     emit WithdrawnAll(totalAllocation, tokenOutAmount);
    // }

    function withdrawToPending() internal {
        address[] memory path = new address[](1);
        path[0] = baseParams.token1;
        withdraw(msg.sender, totalAllocation, token0ToToken1, path, false);
    }



    function startIteration() external onlyRole(SERVER_ROLE){
        if (isIterationStarted) {
            revert("Iteration already started");
        }
        if (!checkUtilization()) revert ("Utilization rate is too high");
        iterationNumber++;
        address[] memory token1 = new address[](1);
        token1[0] = baseParams.token1;
        _placeDeposit(token1);
        _distributeShares();
        lastEnterPrice = Helpers.getLastTokenPrice(baseParams.want, baseParams.token0, baseParams.token1);
        isIterationStarted = true;
    }

    function _distributeShares() internal {
        for (uint256 i = 0; i < userPendingAllocation.length; i++) {
            //user Allocation = total Allocation * user Deposit / total Deposit
            //total allocation = 100e18. 1% = 1e18
            require(
                userPendingAllocation[i].iterationNumber == iterationNumber,
                "err"
            );
            userAllocation[userPendingAllocation[i].user] =
                (100e18 * userPendingAllocation[i].amount) /
                pendingDeposits;
            users.push(userPendingAllocation[i].user);
        }
        delete userPendingAllocation;
        pendingDeposits = 0;
        totalAllocation = 100e18;
    }

    function stopIteration() external onlyRole(SERVER_ROLE) {
        require(isIterationStarted, "Iteration not started");
        withdrawToPending();
        _distributeProfit();
        isIterationStarted = false;
    }

    function _distributeProfit() internal {
        uint256 withdrawnAmount = IERC20(baseParams.token1).balanceOf(address(this)) - pendingDeposits;

        for (uint256 i = 0; i < users.length; i++) {
            uint256 amount = withdrawnAmount * userAllocation[users[i]] / totalAllocation;
            _deposit(users[i], amount);
        }
    }

    /**
     * @notice Pause the contract's activity
     * @dev Only the owner or the controller can pause the contract's activity.
     */
    function pause() external onlyRole(ADMIN_ROLE) {
        _pause();
    }

    /**
     * @notice Unpause the contract's activity
     * @dev Only the owner can unpause the contract's activity.
     */
    function unpause() external onlyRole(ADMIN_ROLE) {
        _unpause();
    }

    /**
     * @notice Sets fee taken by the Leech protocol.
     * @dev Only owner can set the protocol fee.
     * @param _fee Fee value.
     */
    function setFee(uint256 _fee) external onlyRole(ADMIN_ROLE) {
        if (_fee > MAX_FEE) revert("Wrong Amount");
        baseParams.fee = _fee;
    }

    /**
     * @notice Sets the tresury address
     * @dev Only owner can set the treasury address
     * @param _treasury The address to be set.
     */
    function setTreasury(address _treasury) external onlyRole(ADMIN_ROLE) {
        if (_treasury == address(0)) revert("ZeroAddressAsInput");
        treasury = _treasury;
    }


    /**
     * @notice Allows the owner to withdraw stuck tokens from the contract's balance.
     * @dev Only owner can withdraw tokens.
     * @param _token Address of the token to be withdrawn.
     * @param _amount Amount to be withdrawn.
     */
    function inCaseTokensGetStuck(
        address _token,
        uint256 _amount
    ) external onlyRole(ADMIN_ROLE) {
        IERC20(_token).safeTransfer(msg.sender, _amount);
    }

    /**
     * @notice Amount of pending rewards
     */
    function claimable() public view returns (uint256 pendingReward) {
        pendingReward = IMiniApeV2(baseParams.masterchef).pendingBanana(
            baseParams.poolId,
            address(this)
        );
    }
}

