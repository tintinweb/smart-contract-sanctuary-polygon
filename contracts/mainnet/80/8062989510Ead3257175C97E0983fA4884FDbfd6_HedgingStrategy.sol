import "../interfaces/Aave/IRewardsController.sol";
import "../interfaces/Aave/IAavePool.sol";
import "../interfaces/Aave/IAaveProtocolDataProvider.sol";

interface IDecimals {
    function decimals() external view returns (uint8);
}

library Helpers {

    /**
     * @dev Computes the swap amount for the given input.
     * @param investmentA The investment amount for token A.
     * @param reserveA The reserve of token A in the liquidity pool.
     * @param reserveB The reserve of token B in the liquidity pool.
     * @return swapAmount The computed swap amount.
     */
    function _getSwapAmount(
        IUniswapV2Router02 router,
        uint256 investmentA,
        uint256 reserveA,
        uint256 reserveB
    ) public view returns (uint256 swapAmount) {
        uint256 halfInvestment = investmentA / 2;

        uint256 nominator = router.getAmountOut(
            halfInvestment,
            reserveA,
            reserveB
        );

        uint256 denominator = router.quote(
            halfInvestment,
            reserveA + halfInvestment,
            reserveB - nominator
        );
        swapAmount = investmentA - 
            Babylonian.sqrt(
                (halfInvestment * halfInvestment * nominator) / denominator
            );
    }

    function getLastTokenPrice(address _lp, address _token0, address _token1) public view returns (uint256){
        (uint256 reserveA, uint256 reserveB, ) = IUniswapV2Pair(_lp).getReserves();
        uint256 denominator = reserveA * 10 ** IDecimals(_token1).decimals();
        uint256 numerator = reserveB * 10 ** IDecimals(_token0).decimals();
        return numerator / denominator;
    }

    function getOracleTokenPrice(IAavePool _aavePool, address _token0) public view returns (uint256){
        address addressProvider = _aavePool.ADDRESSES_PROVIDER();
        (bool success, bytes memory oracleData) = addressProvider.staticcall(abi.encodeWithSignature("getPriceOracle()"));
        require(success, "Call failed");
        address oracle =  abi.decode(oracleData, (address));
        bytes memory price;
        (success, price) = oracle.staticcall(abi.encodeWithSignature("getAssetPrice(address)", _token0));
        require(success, "Call failed");

        return abi.decode(price, (uint256));
    }

    function getAaveRewards(address _aaveAsset, IRewardsController  _rewardsController) public view returns (address[] memory, uint256[] memory){
        address[] memory rewardList = _rewardsController.getRewardsByAsset(_aaveAsset);
        uint256[] memory amounts = new uint256[](rewardList.length);
        address[] memory assetArray = new address[](1);
        assetArray[0] = _aaveAsset;
        for (uint256 i = 0; i < rewardList.length; i++){
           uint256 amount = _rewardsController.getUserRewards(assetArray, address(this), rewardList[i]);
           amounts[i] = amount;
        }
        return (rewardList, amounts);
    }

     // returns percentage in format percent * 1e25
    // 750000000000000000000000000 is 75%
    function getOptimalUtilization(IAavePool _aavePool, address _token0) public view returns (uint256 optRate, uint256 currRate){
         address addressProvider = _aavePool.ADDRESSES_PROVIDER();
        (bool success, bytes memory providerData) = addressProvider.staticcall(abi.encodeWithSignature("getPoolDataProvider()"));
        require(success, "Call failed");
        address dataProvider =  abi.decode(providerData, (address));
        
        address interestModel = IAaveProtocolDataProvider(dataProvider).getInterestRateStrategyAddress(_token0);
        bytes memory rateData;
        (success, rateData) = interestModel.staticcall(abi.encodeWithSignature("OPTIMAL_USAGE_RATIO()"));
        require(success, "Call failed");
        optRate =  abi.decode(rateData, (uint256));

        (,,uint256 totalAToken,,uint256 totalVariableDebt,,,,,,,) = IAaveProtocolDataProvider(dataProvider).getReserveData(_token0);

        currRate = totalVariableDebt * 1e27 / totalAToken;
    }

    function getLtv(IAavePool _aavePool, address _token1) public view returns(uint256 ltv){
        address addressProvider = _aavePool.ADDRESSES_PROVIDER();
        (bool success, bytes memory ltvDataProvider) = addressProvider.staticcall(abi.encodeWithSignature("getPoolDataProvider()"));
        require(success, "Call failed");
        address dataProvider =  abi.decode(ltvDataProvider, (address));  
        bytes memory data;
        ( , ltv, , , , , , , , ) = IAaveProtocolDataProvider(dataProvider).getReserveConfigurationData(_token1);
    }

        /**
     * @dev Swaps the input token into the liquidity pair.
     */
    function _swapIn(address _lp, IUniswapV2Router02 _uniV2, address _token1, address[] memory _path1To0) public {
        (uint256 reserveA, uint256 reserveB, ) = IUniswapV2Pair(_lp)
            .getReserves();
        uint256 fullInvestment = IERC20(_token1).balanceOf(
            address(this)
        );
         uint256 swapAmountIn = _getSwapAmount(
            _uniV2,
            fullInvestment,
            reserveA,
            reserveB
        );

        uint256[] memory swapedAmounts = 
            _uniV2
            .swapExactTokensForTokens(
                swapAmountIn,
                0,
                _path1To0,
                address(this),
                block.timestamp
            );
            _uniV2.addLiquidity(
            _path1To0[0],
            _path1To0[1],
            fullInvestment - swapAmountIn,
            swapedAmounts[swapedAmounts.length - 1],
            1,
            1,
            address(this),
            block.timestamp 
        );
    }

}

