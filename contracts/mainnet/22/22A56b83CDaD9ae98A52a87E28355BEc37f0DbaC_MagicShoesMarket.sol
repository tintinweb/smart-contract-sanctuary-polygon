import {IMarketplace} from "../interfaces/IMarketplace.sol";
import {IPriceFeed} from "../interfaces/IPriceFeed.sol";

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "hardhat/console.sol";

contract MagicShoesMarket is IMarketplace, ERC721Holder, Ownable {
    /*////////////////////////////////////////////////////////////// MagicShoesMarket
                        STATE VARIABLES
    //////////////////////////////////////////////////////////////*/
    IERC20 public MCT_CONTRACT;

    address public MARKETPLACE_FEE_ADDRESS;
    uint256 public MARKETPLACE_TX_FEE;

    IPriceFeed public maticFeed;
    IPriceFeed public mctFeed;

    uint256 public totalInstantOrders;
    uint256 public totalAuctionOrders;

    mapping(uint256 => InstantOrder) public instantOrder;
    mapping(uint256 => AuctionOrder) public auctionOrder;
    mapping(uint256 => AuctionBid) public winningBid;

    mapping(address => bool) public nftContractApproved;
    mapping(address => uint256) public nftRoyalty;
    mapping(address => address) public nftRoyaltyReceiver;

    /*//////////////////////////////////////////////////////////////
                        CONSTRUCTOR
    //////////////////////////////////////////////////////////////*/

    /// @dev sets the magic shoes nft contract as immutable variable
    /// @notice marketplace only supports trading one magic shoes nft
    /// @notice can be expanded if needed
    constructor(
        IERC20 mctContract_,
        IPriceFeed maticSource_,
        IPriceFeed mctSource_
    ) {
        MCT_CONTRACT = mctContract_;
        maticFeed = maticSource_;
        mctFeed = mctSource_;
    }

    /// @dev allows owner to update the NFT contract & MCT Token contract
    /// @param mctContract_ is the address of the MCT contract
    function updateConfig(IERC20 mctContract_) external onlyOwner {
        MCT_CONTRACT = mctContract_;
    }

    /// @dev allows owner to update the marketplace fee address & fee percentage
    /// @dev feeReceiver_ is the address of the fee receiving address
    /// @dev feePercent_ is the % of fees in whole number
    function updateMarketplaceFeesConfig(
        address feeReceiver_,
        uint256 feePercent_
    ) external onlyOwner {
        MARKETPLACE_FEE_ADDRESS = feeReceiver_;
        MARKETPLACE_TX_FEE = feePercent_;
    }

    /// @dev allows owner to update royalty for listed nft collection. By default it's zero
    /// @dev nftContract_ is the address of the fee receiving address
    /// @dev feeReceiver_ is the address of the fee receiving address
    /// @dev feePercent_ is the % of fees in whole number
    function updateCollectionRoyaltyConfig(
        address nftContract_,
        address feeReceiver_,
        uint256 feePercent_
    ) external onlyOwner {
        nftRoyalty[nftContract_] = feePercent_;
        nftRoyaltyReceiver[nftContract_] = feeReceiver_;
    }

    /*//////////////////////////////////////////////////////////////
                        EXTERNAL FUNCTIONS 
    //////////////////////////////////////////////////////////////*/

    /// @dev see IMarketplace-{createInstantOrder}
    function createInstantOrder(
        address nftContract_,
        uint256 id_,
        uint256 price_
    ) external override returns (uint256 orderId) {
        IERC721 NFT_CONTRACT = IERC721(nftContract_);

        require(price_ > 0, "Marketplace: Zero Price");
        require(
            NFT_CONTRACT.ownerOf(id_) == msg.sender,
            "Marketplace: Invalid Owner"
        );

        NFT_CONTRACT.safeTransferFrom(msg.sender, address(this), id_);
        require(
            NFT_CONTRACT.ownerOf(id_) == address(this),
            "Marketplace: Transfer From Failed"
        );

        totalInstantOrders++;

        orderId = totalInstantOrders;
        instantOrder[orderId] = InstantOrder(
            nftContract_,
            id_,
            price_,
            msg.sender,
            false
        );

        emit InstantOrderCreated(id_, price_, orderId);
    }

    /// @dev see IMarketplace-{cancelInstantOrder}
    function cancelInstantOrder(uint256 orderId_) external override {
        require(
            orderId_ <= totalInstantOrders,
            "Marketplace: Invalid Order ID"
        );

        InstantOrder memory i = instantOrder[orderId_];
        require(i.seller == msg.sender, "Marketplace: Invalid Caller");

        require(
            i.nftContract != address(0),
            "Marketplace: Invalid NFT Contract"
        );
        require(i.id != 0, "Marketplace: Invalid NFT Id");

        IERC721 NFT_CONTRACT = IERC721(i.nftContract);

        delete instantOrder[orderId_];
        NFT_CONTRACT.safeTransferFrom(address(this), i.seller, i.id);

        emit InstantOrderCancelled(orderId_);
    }

    /// @dev see IMarketplace-{executeInstantOrder}
    function executeInstantOrder(
        uint256 orderId_,
        bool isNative_
    ) external payable override {
        require(
            orderId_ <= totalInstantOrders,
            "Marketplace: Invalid Order ID"
        );

        InstantOrder memory i = instantOrder[orderId_];
        require(i.id != 0, "Marketplace: Invalid NFT Id");
        require(i.seller != address(0), "Marketplace: Invalid Seller");
        require(i.price > 0, "Marketplace: Invalid Price");

        if (isNative_) {
            /// @dev validate the incoming native payment
            uint256 x = maticPrice();
            uint256 y = x * msg.value; /// 8 * 18 decimal

            uint256 z = y / 10 ** 18; /// decimal adjusted USD value of matic paid
            require(z >= i.price, "Marketplace: Underpaid");

            /// @dev settle payment to the seller
            _settleSeller(true, i.seller, msg.value, i.nftContract);
        } else {
            /// @dev validate and move the MCT token payment
            uint256 x = mctPrice();
            uint256 y = (i.price * 10 ** 18) / x;

            uint256 balBefore = MCT_CONTRACT.balanceOf(address(this));
            MCT_CONTRACT.transferFrom(msg.sender, address(this), y);
            uint256 balAfter = MCT_CONTRACT.balanceOf(address(this));
            require(balAfter - balBefore >= y, "Marketplace: Invalid Payment");

            _settleSeller(false, i.seller, y, i.nftContract);
        }

        IERC721 NFT_CONTRACT = IERC721(i.nftContract);

        /// @dev free up memory for gas optimization
        /// @dev can store the order details in subgraphs for persistent storage
        delete instantOrder[orderId_];
        NFT_CONTRACT.safeTransferFrom(address(this), msg.sender, i.id);
        emit InstantOrderExecuted(orderId_, isNative_, i.id, msg.sender);
    }

    /// @dev see IMarketplace-{createAuction}
    function createAuction(
        address nftContract_,
        uint256 id_,
        uint256 price_,
        uint256 validTill_
    ) external override returns (uint256 orderId) {
        IERC721 NFT_CONTRACT = IERC721(nftContract_);

        require(price_ > 0, "Marketplace: Zero Price");

        /// @dev minimum auction deadline is 24 hours
        require(
            validTill_ > block.timestamp + 1 days,
            "Marketplace: Invalid Auction Deadline"
        );
        require(
            NFT_CONTRACT.ownerOf(id_) == msg.sender,
            "Marketplace: Invalid Owner"
        );

        NFT_CONTRACT.safeTransferFrom(msg.sender, address(this), id_);
        require(
            NFT_CONTRACT.ownerOf(id_) == address(this),
            "Marketplace: Transfer From Failed"
        );

        totalAuctionOrders++;

        orderId = totalAuctionOrders;
        auctionOrder[orderId] = AuctionOrder(
            nftContract_,
            id_,
            msg.sender,
            validTill_,
            price_,
            msg.sender,
            false
        );

        emit AuctionOrderCreated(id_, price_, validTill_, orderId);
    }

    /// @dev see IMarketplace-{cancelAuctionOrder}
    function cancelAuctionOrder(uint256 orderId_) external override {
        require(
            orderId_ <= totalAuctionOrders,
            "Marketplace: Invalid Order ID"
        );

        AuctionOrder memory i = auctionOrder[orderId_];
        require(i.id != 0, "Marketplace: Invalid NFT ID");
        require(i.seller == msg.sender, "Marketplace: Invalid Caller");
        require(
            block.timestamp < i.validTill,
            "Marketplace: Auction Already Ended"
        );

        IERC721 NFT_CONTRACT = IERC721(i.nftContract);
        /// @dev free up memory for gas optimization
        /// @dev can store the order details in subgraphs for persistent storage
        delete auctionOrder[orderId_];
        NFT_CONTRACT.safeTransferFrom(address(this), i.seller, i.id);

        AuctionBid memory a = winningBid[orderId_];
        delete winningBid[orderId_];

        if (a.isNative) {
            payable(i.highestBidder).transfer(a.amount);
        } else {
            MCT_CONTRACT.transfer(i.highestBidder, a.amount);
        }

        emit AuctionOrderExecuted(orderId_, i.currentPrice, i.isNative);
    }

    /// @dev see IMarketplace-{placeBid}
    function placeBid(
        uint256 orderId_,
        uint256 bidValue_,
        bool isNative_
    ) external payable override {
        require(
            orderId_ <= totalAuctionOrders,
            "Marketplace: Invalid Order ID"
        );

        AuctionOrder storage i = auctionOrder[orderId_];
        require(block.timestamp < i.validTill, "Marketplace: Auction Expired");
        require(
            bidValue_ > i.currentPrice,
            "Marketplace: Insufficient Bid Value"
        );

        if (isNative_) {
            /// @dev validate the incoming native payment
            uint256 x = maticPrice();
            uint256 y = x * msg.value; /// 8 * 18 decimal

            uint256 z = y / 10 ** 18; /// decimal adjusted USD value of matic paid
            require(z >= bidValue_, "Marketplace: Underpaid");
        } else {
            /// @dev validate and move the MCT token payment
            uint256 x = mctPrice();
            uint256 y = (bidValue_ * 10 ** 18) / x;

            uint256 balBefore = MCT_CONTRACT.balanceOf(address(this));
            MCT_CONTRACT.transferFrom(msg.sender, address(this), y);
            uint256 balAfter = MCT_CONTRACT.balanceOf(address(this));

            require(balAfter - balBefore >= y, "Marketplace: Invalid Payment");
        }

        i.highestBidder = msg.sender;
        i.isNative = isNative_;
        i.currentPrice = bidValue_;

        settlePreviousBid(orderId_, bidValue_, isNative_);

        emit BidPlaced(orderId_, bidValue_, isNative_);
    }

    /// @dev see IMarketplace-{executeAuctionOrder}
    function executeAuctionOrder(uint256 orderId_) external override {
        require(
            orderId_ <= totalAuctionOrders,
            "Marketplace: Invalid Order ID"
        );

        AuctionOrder memory i = auctionOrder[orderId_];
        require(i.id != 0, "Marketplace: Invalid NFT ID");
        require(i.seller != address(0), "Marketplace: Invalid Seller");
        require(
            block.timestamp > i.validTill,
            "Marketplace: Auction Still Live"
        );

        IERC721 NFT_CONTRACT = IERC721(i.nftContract);
        /// @dev free up memory for gas optimization
        /// @dev can store the order details in subgraphs for persistent storage
        delete auctionOrder[orderId_];
        NFT_CONTRACT.safeTransferFrom(address(this), i.highestBidder, i.id);

        AuctionBid memory a = winningBid[orderId_];
        delete winningBid[orderId_];

        _settleSeller(a.isNative, i.seller, a.amount, i.nftContract);
        emit AuctionOrderExecuted(orderId_, i.currentPrice, i.isNative);
    }

    /// @dev see IMarketplace-{updatePriceFeed}
    function updatePriceFeed(
        IPriceFeed maticSource_,
        IPriceFeed mctSource_
    ) external onlyOwner {
        require(
            address(maticSource_) != address(0),
            "Marketplace: Invalid Price Feed"
        );
        require(
            address(mctSource_) != address(0),
            "Marketplace: Invalid Price Feed"
        );

        maticFeed = maticSource_;
        mctFeed = mctSource_;

        emit PriceFeedUpdated(address(maticSource_), address(mctSource_));
    }

    /*//////////////////////////////////////////////////////////////
                        INTERNAL HELPER FUNCTIONS
    //////////////////////////////////////////////////////////////*/

    /// @dev returns the price of Matic/USD
    function maticPrice() internal view returns (uint256 price) {
        price = IPriceFeed(maticFeed).currentPrice();
        require(price > 0, "Marketplace: Invalid Price Oracle");
    }

    /// @dev returns the price of MCT/USD
    function mctPrice() internal view returns (uint256 price) {
        price = IPriceFeed(mctFeed).currentPrice();
        require(price > 0, "Marketplace: Invalid Price Oracle");
    }

    /// @dev settles any outstanding previous bid
    function settlePreviousBid(
        uint256 orderId_,
        uint256 newValue_,
        bool isNative_
    ) internal {
        AuctionBid memory a = winningBid[orderId_];

        /// @dev settles any outstanding bid
        if (a.amount > 0) {
            /// @dev free up memeory
            delete winningBid[orderId_];

            if (a.isNative) {
                payable(a.bidder).transfer(a.amount);
            } else {
                MCT_CONTRACT.transfer(a.bidder, a.amount);
            }
        }

        winningBid[orderId_] = AuctionBid(msg.sender, newValue_, isNative_);
    }

    /// @dev settles successful orders
    function _settleSeller(
        bool isNative_,
        address seller_,
        uint256 amount_,
        address nftContract_
    ) internal {
        uint256 marketplaceFees = (amount_ * MARKETPLACE_TX_FEE) / 100;
        uint256 royaltyFees;

        uint256 royalty = nftRoyalty[nftContract_];
        if (royalty > 0) {
            royaltyFees = (amount_ * royalty) / 100;
        }

        uint256 sellerAmount = amount_ - (marketplaceFees + royaltyFees);

        if (isNative_) {
            payable(MARKETPLACE_FEE_ADDRESS).transfer(marketplaceFees);
            payable(seller_).transfer(sellerAmount);

            if (royalty > 0) {
                payable(nftRoyaltyReceiver[nftContract_]).transfer(royaltyFees);
            }
        } else {
            MCT_CONTRACT.transfer(MARKETPLACE_FEE_ADDRESS, marketplaceFees);
            MCT_CONTRACT.transfer(seller_, sellerAmount);

            if (royalty > 0) {
                MCT_CONTRACT.transfer(
                    nftRoyaltyReceiver[nftContract_],
                    royaltyFees
                );
            }
        }
    }
}

