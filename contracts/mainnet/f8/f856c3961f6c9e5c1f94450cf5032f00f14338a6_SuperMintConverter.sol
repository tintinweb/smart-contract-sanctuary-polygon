            "PGERC721: SUPER_MINTER_ROLE required"
        );
        require(!paused, "PGERC721: contract on pause");

        uint256 supply = totalSupply();
        for (uint256 i = 1; i <= _mintAmount; i++) {
            superTokenIds[supply + i] = superId;
            superIdTokenCounter[superId] = superIdTokenCounter[superId] + 1;
            tokenIdSuperPosition[supply + i] = superIdTokenCounter[superId];

            _safeMint(_to, supply + i);
        }
    }

