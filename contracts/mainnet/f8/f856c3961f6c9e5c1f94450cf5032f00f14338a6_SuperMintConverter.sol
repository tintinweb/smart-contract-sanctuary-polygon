     *
     * Access: only the contract owner account
     *
     * @param _newBaseExtension new value
     */
    function setBaseExtension(
        string memory _newBaseExtension
    ) public onlyOwner {
        baseExtension = _newBaseExtension;
    }

    /**
     * Set contract to pause
     *
     * Access: only the contract owner account
     *
