pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";

bytes32 constant AUTHORIZER_ROLE = keccak256("AUTHORIZER_ROLE");
bytes32 constant MINTABLE_DATA_SETTER_ROLE = keccak256("MINTABLE_DATA_SETTER_ROLE");
bytes32 constant FREEMINT_DATA_SETTER_ROLE = keccak256("FREEMINT_DATA_SETTER_ROLE");
bytes32 constant BUNDLE_DATA_SETTER_ROLE = keccak256("BUNDLE_DATA_SETTER_ROLE");

/// @custom:security-contact [email protected]
contract NFTouringRepository is AccessControlEnumerable {
    constructor() {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(AUTHORIZER_ROLE, msg.sender);
        _grantRole(MINTABLE_DATA_SETTER_ROLE, msg.sender);
        _grantRole(FREEMINT_DATA_SETTER_ROLE, msg.sender);
        _grantRole(BUNDLE_DATA_SETTER_ROLE, msg.sender);
    }

    struct MintableData {
        string id;
        uint256 price;
        bytes32[] whitelistsId;
        bytes32[] freemintsId;
        bool isPublic;
        bool isOpenable;
        bool exists;
    }

    // Database ID has to be converted to keccak256 first
    mapping(bytes32 => MintableData) private mintableDataById;

    function setMintableData(bytes32 id, MintableData memory data) public onlyRole(MINTABLE_DATA_SETTER_ROLE) {
        mintableDataById[id] = data;
    }

    function setMintablesData(bytes32[] memory ids, MintableData[] memory datas) public onlyRole(MINTABLE_DATA_SETTER_ROLE) {
        require(ids.length == datas.length, "Arrays 'ids' and 'datas' must have the same length.");

        for (uint256 i = 0; i < ids.length; i++) {
            mintableDataById[ids[i]] = datas[i];
        }
    }

    function deleteMintableData(bytes32 id) public onlyRole(MINTABLE_DATA_SETTER_ROLE) {
        delete mintableDataById[id];
    }

    function getMintableData(bytes32 id) public view returns (MintableData memory) {
        return mintableDataById[id];
    }

    struct FreemintData {
        mapping(address => uint) countByAddress;
        uint addressCount;
        bool exists;
    }

    // Database ID has to be converted to keccak256 first
    mapping(bytes32 => FreemintData) private freemintDataById;

    function setFreemintData(bytes32 id, address[] memory addresses, uint[] memory counts) public onlyRole(FREEMINT_DATA_SETTER_ROLE) {
        require(addresses.length == counts.length, "Arrays 'addresses' and 'count' must have the same length.");

        freemintDataById[id].exists = true;
        freemintDataById[id].addressCount = addresses.length;
        for (uint i = 0; i < addresses.length; i++) {
            freemintDataById[id].countByAddress[addresses[i]] = counts[i];
        }
    }

    function deleteFreemintData(bytes32 id) public onlyRole(FREEMINT_DATA_SETTER_ROLE) {
        delete freemintDataById[id];
    }

    function getFreemintCount(bytes32 freemintId, address claimer) public view returns (uint) {
        return freemintDataById[freemintId].countByAddress[claimer];
    }

    function useFreemint(bytes32 freemintId, address claimer) public onlyRole(FREEMINT_DATA_SETTER_ROLE) {
        freemintDataById[freemintId].countByAddress[claimer]--;
    }

    struct PartialFreemintData {
        uint addressCount;
        bool exists;
    }

    function getFreemintAddressCount(bytes32 freemintId) public view returns (PartialFreemintData memory) {
        return PartialFreemintData({addressCount: freemintDataById[freemintId].addressCount, exists: freemintDataById[freemintId].exists});
    }

    mapping(uint256 => bytes32[]) private bundles;

    function setMintablesInBundle(uint256 tokenId, bytes32[] memory mintablesId) public onlyRole(BUNDLE_DATA_SETTER_ROLE) {
        bundles[tokenId] = mintablesId;
    }

    function getMintablesInBundle(uint256 tokenId) public view onlyRole(BUNDLE_DATA_SETTER_ROLE) returns (bytes32[] memory) {
        return bundles[tokenId];
    }
}

