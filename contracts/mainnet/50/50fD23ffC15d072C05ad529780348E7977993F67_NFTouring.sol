pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155SupplyUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC2981Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "hardhat/console.sol";

import "../../repository/v1/NFTouringRepository.sol";

bytes32 constant INTEGER_SETTER_ROLE = keccak256("INTEGER_SETTER_ROLE");
bytes32 constant STRING_SETTER_ROLE = keccak256("STRING_SETTER_ROLE");
bytes32 constant ADDRESS_SETTER_ROLE = keccak256("ADDRESS_SETTER_ROLE");
bytes32 constant URI_SETTER_ROLE = keccak256("URI_SETTER_ROLE");
bytes32 constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
bytes32 constant MINTER_ROLE = keccak256("MINTER_ROLE");
bytes32 constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
bytes32 constant TOKEN_DATA_SETTER = keccak256("TOKEN_DATA_SETTER");

bytes32 constant REPOSITORY = keccak256("REPOSITORY");
bytes32 constant PAYMENTS_RECIPIENT = keccak256("PAYMENTS_RECIPIENT");
bytes32 constant ROYALTY_RECIPIENT = keccak256("ROYALTY_RECIPIENT");
bytes32 constant OWNER_ADDRESS = keccak256("OWNER_ADDRESS");
bytes32 constant ROYALTY_BASIS_POINTS = keccak256("ROYALTY_BASIS_POINTS");

/// @custom:security-contact [email protected]
contract NFTouring is
    Initializable,
    ERC1155Upgradeable,
    AccessControlEnumerableUpgradeable,
    PausableUpgradeable,
    ERC1155BurnableUpgradeable,
    ERC1155SupplyUpgradeable,
    IERC2981Upgradeable,
    UUPSUpgradeable
{
    using Counters for Counters.Counter;

    Counters.Counter private _tokenIdCounter;
    mapping(uint256 => string) private tokenData;
    mapping(uint256 => string) private tokenURI;
    string public tokenURIBase;

    mapping(bytes32 => uint256) private uniqueIntegers;
    mapping(bytes32 => string) private uniqueStrings;
    mapping(bytes32 => address payable) private uniqueAddresses;
    mapping(bytes32 => address payable[]) private addressesLists;

    // For OpenSea
    function name() public view virtual returns (string memory) {
        return "NFTouring";
    }

    // For OpenSea
    function owner() public view virtual returns (address) {
        return uniqueAddresses[OWNER_ADDRESS];
    }

    // Token Data
    function getTokenData(uint256 tokenId) public view onlyRole(TOKEN_DATA_SETTER) returns (string memory) {
        return tokenData[tokenId];
    }

    function setTokenData(uint256 tokenId, string memory value) public onlyRole(TOKEN_DATA_SETTER) {
        tokenData[tokenId] = value;
    }

    // Strings

    function setUniqueString(bytes32 id, string memory value) public onlyRole(STRING_SETTER_ROLE) {
        uniqueStrings[id] = value;
    }

    function getUniqueString(bytes32 id) public view onlyRole(STRING_SETTER_ROLE) returns (string memory) {
        return uniqueStrings[id];
    }

    // Integers

    function setUniqueInteger(bytes32 id, uint256 value) public onlyRole(INTEGER_SETTER_ROLE) {
        uniqueIntegers[id] = value;
    }

    function getUniqueInteger(bytes32 id) public view onlyRole(INTEGER_SETTER_ROLE) returns (uint256) {
        return uniqueIntegers[id];
    }

    // Addresses

    function getRepository() public view returns (address) {
        return uniqueAddresses[REPOSITORY];
    }

    function setUniqueAddress(bytes32 id, address payable value) public onlyRole(ADDRESS_SETTER_ROLE) {
        uniqueAddresses[id] = value;
    }

    function getUniqueAddress(bytes32 id) public view onlyRole(ADDRESS_SETTER_ROLE) returns (address payable) {
        return uniqueAddresses[id];
    }

    function addAddressToList(bytes32 listId, address payable value) public onlyRole(ADDRESS_SETTER_ROLE) {
        addressesLists[listId].push(value);
    }

    function setAddressList(bytes32 listId, address payable[] memory value) public onlyRole(ADDRESS_SETTER_ROLE) {
        addressesLists[listId] = value;
    }

    function getAddressList(bytes32 listId) public view onlyRole(ADDRESS_SETTER_ROLE) returns (address payable[] memory) {
        return addressesLists[listId];
    }

    function getAddressListLength(bytes32 listId) public view onlyRole(ADDRESS_SETTER_ROLE) returns (uint count) {
        return addressesLists[listId].length;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize() public initializer {
        __ERC1155_init("");
        __AccessControl_init();
        __Pausable_init();
        __ERC1155Burnable_init();
        __ERC1155Supply_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(STRING_SETTER_ROLE, msg.sender);
        _grantRole(INTEGER_SETTER_ROLE, msg.sender);
        _grantRole(ADDRESS_SETTER_ROLE, msg.sender);
        _grantRole(URI_SETTER_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);
        _grantRole(TOKEN_DATA_SETTER, msg.sender);

        tokenURIBase = "";
    }

    function setTokenURI(uint256 tokenId, string memory newURI) public onlyRole(URI_SETTER_ROLE) {
        tokenURI[tokenId] = newURI;
    }

    function setTokenURIBase(string memory newURIBase) public onlyRole(URI_SETTER_ROLE) {
        tokenURIBase = newURIBase;
    }

    function uri(uint256 tokenId) public view override returns (string memory) {
        return string.concat(tokenURIBase, tokenURI[tokenId]);
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function mintOnlyMinter(address account, uint256 id, uint256 amount, bytes memory data) public onlyRole(MINTER_ROLE) {
        _mint(account, id, amount, data);
    }

    function mintBatchOnlyMinter(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public onlyRole(MINTER_ROLE) {
        _mintBatch(to, ids, amounts, data);
    }

    function freemint(string memory mintableId) public payable {
        NFTouringRepository repository = NFTouringRepository(uniqueAddresses[REPOSITORY]);
        NFTouringRepository.MintableData memory mintable = repository.getMintableData(keccak256(abi.encodePacked(mintableId)));
        require(mintable.isPublic, "Can only mint public mintables.");

        if (mintable.whitelistsId.length > 0) {
            bool isWhitelisted = _isWhitelisted(msg.sender, mintable.whitelistsId);
            require(isWhitelisted, "This address isn't on any of this mintable's whitelist.");
        }

        for (uint i = 0; i < mintable.freemintsId.length; i++) {
            uint count = repository.getFreemintCount(mintable.freemintsId[i], msg.sender);

            if (count > 0) {
                repository.useFreemint(mintable.freemintsId[i], msg.sender);
                _mintMintable(msg.sender, mintableId);
                return;
            }
        }

        require(false, "This address is not listed in any othe this mintable's freemint lists.");
    }

    function payToMint(string memory mintableId) public payable {
        NFTouringRepository repository = NFTouringRepository(uniqueAddresses[REPOSITORY]);
        NFTouringRepository.MintableData memory mintable = repository.getMintableData(keccak256(abi.encodePacked(mintableId)));
        require(mintable.isPublic, "Can only mint public mintables.");

        if (mintable.whitelistsId.length > 0) {
            bool isWhitelisted = _isWhitelisted(msg.sender, mintable.whitelistsId);
            require(isWhitelisted, "This address isn't on any of this mintable's whitelist");
        }

        require(mintable.freemintsId.length == 0, "This mintable is part of a freemint program and can't be bought.");

        require(msg.value == mintable.price, "Incorrect price paid.");

        address payable recipient = uniqueAddresses[PAYMENTS_RECIPIENT];
        (bool success, ) = recipient.call{value: msg.value}("");
        require(success, "Payment transfer failed.");

        _mintMintable(msg.sender, mintableId);
    }

    function openBundle(uint256 tokenId) public {
        require(balanceOf(msg.sender, tokenId) != 0, "Only the token owner can open the bundle.");

        NFTouringRepository repository = NFTouringRepository(uniqueAddresses[REPOSITORY]);

        NFTouringRepository.MintableData memory bundleMintable = repository.getMintableData(
            keccak256(abi.encodePacked(tokenData[tokenId]))
        );
        require(bundleMintable.isOpenable, "Bundle is not (yet) openable.");

        bytes32[] memory mintablesHash = repository.getMintablesInBundle(tokenId);

        for (uint256 i = 0; i < mintablesHash.length; i++) {
            NFTouringRepository.MintableData memory mintable = repository.getMintableData(mintablesHash[i]);
            _mintMintable(msg.sender, mintable.id);
        }

        _burn(msg.sender, tokenId, 1);
    }

    function _isWhitelisted(address account, bytes32[] memory whitelistsId) internal view returns (bool) {
        for (uint256 i = 0; i < whitelistsId.length; i++) {
            for (uint256 j = 0; j < addressesLists[whitelistsId[i]].length; j++) {
                if (addressesLists[whitelistsId[i]][j] == account) return true;
            }
        }

        return false;
    }

    event MintMintable(address indexed recipient, uint256 indexed tokenId, string mintableId, uint pricePaid);

    function _mintMintable(address account, string memory mintableId) internal {
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _mint(account, tokenId, 1, "");

        setTokenData(tokenId, mintableId);
        setTokenURI(tokenId, string.concat("?mintableId=", mintableId));
        emit MintMintable(account, tokenId, mintableId, msg.value);
    }

    // EIP2981 standard royalties return.
    function royaltyInfo(uint256, uint256 salePrice) external view override returns (address receiver, uint256 royaltyAmount) {
        address recipient = uniqueAddresses[ROYALTY_RECIPIENT];
        uint256 basisPoints = uniqueIntegers[ROYALTY_BASIS_POINTS];
        return (recipient, (salePrice * basisPoints) / 10000);
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155Upgradeable, ERC1155SupplyUpgradeable) whenNotPaused {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    function _authorizeUpgrade(address newImplementation) internal override onlyRole(UPGRADER_ROLE) {}

    // The following functions are overrides required by Solidity.

    function supportsInterface(
        bytes4 interfaceId
    ) public view override(ERC1155Upgradeable, AccessControlEnumerableUpgradeable, IERC165Upgradeable) returns (bool) {
        return (interfaceId == type(IERC2981Upgradeable).interfaceId || super.supportsInterface(interfaceId));
    }
}

