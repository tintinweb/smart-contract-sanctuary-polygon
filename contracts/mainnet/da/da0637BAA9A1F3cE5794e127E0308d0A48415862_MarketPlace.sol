    function finaliseFixedPriceAuction721(
        address _owner,
        /**
				0:_tokenId,1:_minPrice,2:_auctionType,3:_quantity,4:endTime,5:salt
				 */
        uint256[] memory otherParams,
        bytes memory signature,
        address tokenContract
    ) public {
        require(otherParams[2] == 1, "Auction needs to be Fixed Price");
        require(otherParams[3] == 1, "Cannot have more than one copy for 721");

        address seller = verifySignatureAndQuantityAvailable(
            _owner,
            otherParams,
            signature,
            tokenContract,
            1
        );
        GenericERC721 tokenContract721 = GenericERC721(tokenContract);
        uint256 feeAmt = (otherParams[1] * feePercent) / 100;
        uint256 netSalePrice = _deduceRoyalties721(
            otherParams[0],
            otherParams[1],
            _msgSender(),
            tokenContract721
        );
        weth.transferFrom(_msgSender(), feeAddress, feeAmt);
        weth.transferFrom(_msgSender(), seller, netSalePrice);
        tokenContract721.safeTransferFrom(seller, _msgSender(), otherParams[0]);
        emit LogOrderFinalised721(
            otherParams[2],
            otherParams[0],
            _msgSender(),
            seller
        );
    }

    //For Unlimited and TimedAuctions
    function finaliseERC721Auction(
        address buyer,
        address owner,
        /**
				0:_tokenId,1:_minPrice,2:_auctionType,3:_quantity,4:endTime,5:salt
				 */
        uint256[] memory otherParams,
        bytes memory signature,
        uint256 bidAmount,
        address tokenContract
    )
        public
        OnlySeller(owner)
        AuctionTimeIsOver(otherParams[4])
        returns (bool)
    {
        require(
            otherParams[2] == 2 || otherParams[2] == 3,
            "Only Unlimited and Timed Auctions can be Finalised"
        );
        require(otherParams[3] == 1, "Cannot have more than one copy for 721");
        address seller = verifySignatureAndQuantityAvailable(
            owner,
            otherParams,
            signature,
            tokenContract,
            1
        );
        GenericERC721 tokenContract721 = GenericERC721(tokenContract);
        uint256 feeAmt = (bidAmount * feePercent) / (100 + feePercent);

        uint256 netSalePrice = _deduceRoyalties721(
            otherParams[0],
            bidAmount,
            buyer,
            tokenContract721
        );
        weth.transferFrom(buyer, feeAddress, feeAmt);

        //Transfer amount to seller after deducting loyalties
        weth.transferFrom(buyer, seller, netSalePrice);

        //Transfer NFT to the bidder
        tokenContract721.safeTransferFrom(seller, buyer, otherParams[0]);
        emit LogOrderFinalised721(
            otherParams[2],
            otherParams[0],
            buyer,
            seller
        );
        return true;
    }

    function finaliseFixedPriceAuction1155(
        address _owner,
        /**
				0:_tokenId,1:_minPrice,2:_auctionType,3:_quantity,4:endTime,5:salt
				 */
        uint256[] memory otherParams,
        bytes memory signature,
        uint256 quantity,
        address tokenContract
    ) public {
        require(otherParams[2] == 1, "Auction needs to be Fixed Price");

        address seller = verifySignatureAndQuantityAvailable(
            _owner,
            otherParams,
            signature,
            tokenContract,
            quantity
        );
        GenericERC1155 tokenContract1155 = GenericERC1155(tokenContract);
        uint256 totalAmount = otherParams[1] * quantity;

        uint256 feeAmt = (totalAmount * feePercent) / 100;
        uint256 paramTokenId = otherParams[0];
        uint256 netSalePrice = _deduceRoyalties1155(
            otherParams[0],
            totalAmount,
            _msgSender(),
            tokenContract1155
        );

        //Transfer fees to Fee Address
        weth.transferFrom(_msgSender(), feeAddress, feeAmt);
        //Transfer amount to seller
        weth.transferFrom(_msgSender(), seller, netSalePrice);

        tokenContract1155.safeTransferFrom(
            seller,
            _msgSender(),
            otherParams[0],
            quantity,
            bytes(tokenContract1155.uri(paramTokenId))
        );
        emit LogOrderFinalised1155(
            otherParams[2],
            otherParams[0],
            _msgSender(),
            seller,
            quantity
        );
    }

    function finaliseERC1155Auction(
        address _owner,
        /**
				0:_tokenId,1:_minPrice,2:_auctionType,3:_quantity,4:endTime,5:salt
				 */
        uint256[] memory otherParams,
        address _buyer,
        bytes memory signature,
        uint256 bidAmount,
        uint256 _quantity,
        address tokenContract
    ) public OnlySeller(_owner) AuctionTimeIsOver(otherParams[4]) {
        uint256 auctionId = otherParams[2];
        uint256 quantity = _quantity;
        require(
            auctionId == 2 || auctionId == 3,
            "Only Unlimited and Timed Auctions can be Finalised"
        );
        uint256 tokenId = otherParams[0];
        address seller = verifySignatureAndQuantityAvailable(
            _owner,
            otherParams,
            signature,
            tokenContract,
            quantity
        );
        GenericERC1155 tokenContract1155 = GenericERC1155(tokenContract);
        uint256 totalAmount = bidAmount * quantity;
        address buyer = _buyer;

        uint256 feeAmt = (totalAmount * feePercent) / 100;

        uint256 netSalePrice = _deduceRoyalties1155(
            tokenId,
            totalAmount,
            buyer,
            tokenContract1155
        );

        //Transfer fees to fee Address
        weth.transferFrom(buyer, feeAddress, feeAmt);

        //Transfer amount to seller after deducting loyalties
        weth.transferFrom(buyer, seller, netSalePrice);

        //Transfer NFT to the bidder
        tokenContract1155.safeTransferFrom(
            seller,
            buyer,
            tokenId,
            quantity,
            bytes(tokenContract1155.uri(tokenId))
        );
        emit LogOrderFinalised1155(auctionId, tokenId, buyer, seller, quantity);
    }
}

