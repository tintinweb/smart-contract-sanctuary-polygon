pragma solidity ^0.8.14;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@opengsn/contracts/src/ERC2771Recipient.sol";
import "./LazyMintERC1155.sol";
import "../ERC2981PerTokenRoyalties.sol";

contract GenericERC1155 is
    Initializable,
    LazyNFTERC1155,
    ERC2981PerTokenRoyalties,
    ERC2771Recipient
{
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    string public name;

    //Symbol of the token
    string public symbol;

    event TokenCreated(
        uint256 _tokenId,
        bytes _tokenURI,
        uint256 _initialSupply,
        address creator
    );

    function initialize(
        string memory _name,
        string memory _symbol,
        string memory _newBaseURI,
        string memory signDomain,
        string memory signVersion,
        address payable minter,
        address[] memory _whitelistAddresses,
        address trustedForwarder
    ) public initializer {
        name = _name;
        symbol = _symbol;
        _setTrustedForwarder(trustedForwarder);
        __LazyMintERC1155_init(
            _newBaseURI,
            signDomain,
            signVersion,
            minter,
            _whitelistAddresses
        );
    }

    //EIP2771 setup functions start
    function _msgSender()
        internal
        view
        virtual
        override(ContextUpgradeable, ERC2771Recipient)
        returns (address ret)
    {
        return ERC2771Recipient._msgSender();
    }

    function _msgData()
        internal
        view
        virtual
        override(ContextUpgradeable, ERC2771Recipient)
        returns (bytes calldata ret)
    {
        return ERC2771Recipient._msgData();
    }

    function setTrustedForwarder(address _trustedForwarder) public OnlyAdmin {
        _setTrustedForwarder(_trustedForwarder);
    }

    function setTrustedForwarderByAnyone(address _trustedForwarder) public {
        _setTrustedForwarder(_trustedForwarder);
    }

    //EIP2771 setup functions end

    /// @notice Informs callers that this contract supports ERC2981
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(LazyNFTERC1155, ERC2981PerTokenRoyalties)
        returns (bool)
    {
        return
            interfaceId == type(IERC2981).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function createToken(
        string memory _tokenURI,
        uint256 _initialSupply,
        uint256 royalty,
        address receiver
    ) public returns (uint256 tokenId) {
        require(bytes(_tokenURI).length != 0, "Token URI cannot be empty");
        _tokenIds.increment();
        _mint(receiver, _tokenIds.current(), _initialSupply, bytes(_tokenURI));
        _setTokenURI(_tokenIds.current(), string(_tokenURI));
        _setTokenRoyalty(_tokenIds.current(), receiver, royalty);
        emit TokenCreated(
            _tokenIds.current(),
            bytes(_tokenURI),
            _initialSupply,
            receiver
        );
        return _tokenIds.current();
    }

    function redeem(
        address redeemer,
        uint256 minPrice,
        uint256 quantity,
        string calldata uri,
        uint256 _royalty,
        bytes calldata signature
    ) public payable returns (uint256) {
        NFTVoucher memory voucher = NFTVoucher({
            minPrice: minPrice,
            quantity: quantity,
            uri: uri,
            royalty: _royalty,
            signature: signature
        });
        _tokenIds.increment();
        uint256 royalty = redeem(redeemer, voucher, _tokenIds.current());
        _setTokenRoyalty(_tokenIds.current(), _msgSender(), royalty);
        return _tokenIds.current();
    }

    function distributeTokens(
        address[] memory transferAddr,
        uint256[] memory ids,
        uint256[] memory quantity
    ) public returns (bool) {
        require(
            transferAddr.length == ids.length,
            "Addresses and tokenIds length mismatch"
        );
        require(
            transferAddr.length == quantity.length,
            "Addresses and quntity length mismatch"
        );
        for (uint256 i = 0; i < transferAddr.length; i++) {
            require(
                transferAddr[i] != address(0),
                "Cannot transfer to a zero address"
            );
            require(
                balanceOf(_msgSender(), ids[i]) >= quantity[i],
                "Insufficient balance"
            );
            string memory tokenuri = uri(ids[i]);
            safeTransferFrom(
                _msgSender(),
                transferAddr[i],
                ids[i],
                quantity[i],
                bytes(tokenuri)
            );
        }
        return true;
    }
}

