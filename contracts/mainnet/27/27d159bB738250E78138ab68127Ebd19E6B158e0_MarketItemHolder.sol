    }

    function removeManager(address manager) public onlyOwner {
        _managers[manager] = false;
    }

    function migrateMarketContract(address oldMarketContractAddress, uint256 from_itemId, uint256 to_itemId) public payable nonReentrant {
        require(_managers[msg.sender], "!manager");
        OldMarketContract oldMarketContract = OldMarketContract(oldMarketContractAddress);
        for (uint i = from_itemId; i <= to_itemId; i++) {
            OldMarketContract.MarketItem memory oldMarketItem = oldMarketContract.fetchMarketItem(i);
            MarketItem memory item = idToMarketItem[i];
            if (item.itemId <= 0 && oldMarketItem.itemId > 0) {
                idToMarketItem[i] = MarketItem(
                    i,
                    oldMarketItem.isErc721,
                    State(uint(oldMarketItem.state)),
                    oldMarketItem.nftContract,
                    oldMarketItem.erc20Address,
                    oldMarketItem.tokenId,
                    oldMarketItem.amount,
                    oldMarketItem.price,
                    oldMarketItem.seller
                );
                if (_itemIds < i) {
                    _itemIds = i;
                }
            }

        }
    }


    function createMarketItem(bool isErc721, address nftContract, uint256 tokenId, uint256 price, uint256 amount, address erc20Address) external returns (MarketItem memory) {
        require(_managers[msg.sender], "!manager");
        _itemIds = _itemIds.add(1);
        uint256 itemId = _itemIds;
        idToMarketItem[itemId] = MarketItem(
            itemId,
            isErc721,
            State.INITIATED,
            nftContract,
            erc20Address,
            tokenId,
            amount,
            price,
            payable(msg.sender)
        );
        return fetchMarketItem(itemId);
    }

    function updateMarketItem(uint256 itemId, uint256 price) external returns (MarketItem memory) {
        require(_managers[msg.sender], "!manager");
        MarketItem memory item = idToMarketItem[itemId];
        item.price = price;
        idToMarketItem[item.itemId] = item;
        return fetchMarketItem(itemId);
    }

    function cancelMarketItem(uint256 itemId) external returns (MarketItem memory) {
        require(_managers[msg.sender], "!manager");
        MarketItem memory item = idToMarketItem[itemId];
        item.state = State.CANCELLED;
        idToMarketItem[item.itemId] = item;
        return fetchMarketItem(itemId);
    }

    function createMarketSale(uint256 itemId, uint256 remainAmount) external returns (MarketItem memory) {
        require(_managers[msg.sender], "!manager");
        MarketItem memory item = idToMarketItem[itemId];
        if (remainAmount == 0) {
            item.state = State.SOLD;
        }
        item.amount = remainAmount;
        idToMarketItem[item.itemId] = item;
        return fetchMarketItem(itemId);
    }

    function fetchMarketItem(uint itemId) public view returns (MarketItem memory) {
        return idToMarketItem[itemId];
    }

    function migrateMarketBidContract(address oldBidContractAddress, uint256 from_itemId, uint256 to_itemId) public nonReentrant {
        require(_managers[msg.sender], "!manager");
        OldBidContract oldBidContract = OldBidContract(oldBidContractAddress);
        for (uint i = from_itemId; i <= to_itemId; i++) {
            OldBidContract.MarketBidItem memory oldBidMarketItem = oldBidContract.fetchMarketBidItem(i);
            MarketBidItem memory item = idToMarketBidItem[i];
            if (item.itemId <= 0 && oldBidMarketItem.itemId > 0) {
                idToMarketBidItem[i] = MarketBidItem(
                    i,
                    oldBidMarketItem.isErc721,
                    BidState(uint(oldBidMarketItem.state)),
                    oldBidMarketItem.nftContract,
                    oldBidMarketItem.erc20Address,
                    oldBidMarketItem.tokenId,
                    oldBidMarketItem.amount,
                    oldBidMarketItem.price,
                    oldBidMarketItem.bidder
                );
                if (_bidItemIds < i) {
                    _bidItemIds = i;
                }
            }
        }
    }


    function createMarketBidItem(bool isErc721, address nftContract, uint256 tokenId, uint256 price, uint256 amount, address erc20Address) external returns (MarketBidItem memory) {
        require(_managers[msg.sender], "!manager");
        _bidItemIds = _bidItemIds.add(1);
        uint256 itemId = _bidItemIds;
        idToMarketBidItem[itemId] = MarketBidItem(
            itemId,
            isErc721,
            BidState.INITIATED,
            nftContract,
            erc20Address,
            tokenId,
            amount,
            price,
            payable(msg.sender)
        );
        return fetchMarketBidItem(itemId);

    }

    function updateMarketBidItem(uint256 itemId, uint256 price) external returns (MarketBidItem memory) {
        require(_managers[msg.sender], "!manager");
        MarketBidItem memory item = idToMarketBidItem[itemId];
        item.price = price;
        idToMarketBidItem[item.itemId] = item;
        return fetchMarketBidItem(itemId);
    }

    function cancelMarketBidItem(uint256 itemId) external returns (MarketBidItem memory) {
        require(_managers[msg.sender], "!manager");
        MarketBidItem memory item = idToMarketBidItem[itemId];
        item.state = BidState.CANCELLED;
        idToMarketBidItem[item.itemId] = item;
        return fetchMarketBidItem(itemId);
    }

    function createMarketAccept(uint256 itemId, uint256 remainAmount) external returns (MarketBidItem memory) {
        require(_managers[msg.sender], "!manager");
        MarketBidItem memory item = idToMarketBidItem[itemId];
        if (remainAmount == 0) {
            item.state = BidState.ACCEPTED;
        }
        item.amount = remainAmount;
        idToMarketBidItem[item.itemId] = item;
        return fetchMarketBidItem(itemId);
    }

    function fetchMarketBidItem(uint itemId) public view returns (MarketBidItem memory) {
        return idToMarketBidItem[itemId];
    }

    function withdrawToken(address erc20Address, address to_, uint256 amount_) external onlyOwner {
        IERC20(erc20Address).approve(to_, amount_);
        IERC20(erc20Address).transfer(to_, amount_);
    }

    function withdrawNative() public payable onlyOwner {
        require(payable(msg.sender).send(address(this).balance));
    }


    function testSenderManager() external view returns (address) {
        require(_managers[msg.sender], "!manager");
        return msg.sender;
    }
}