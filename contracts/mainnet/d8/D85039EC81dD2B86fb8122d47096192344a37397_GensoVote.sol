pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract GensoVote is Ownable {
    using Counters for Counters.Counter;

    address public to; // Address to receive the transferred ERC1155 tokens
    address public asset_contract; // Address of the ERC1155 contract
    uint256 public token_id; // ID of the ERC1155 token
    Counters.Counter private voteCounter; // Counter to track the number of votes

    event Vote(bytes32 encryptedData, address indexed voter, uint256 voteCount); // Event to record encrypted data, voter address, and vote count

    constructor() {
        token_id = 1200000000691; // Initial token ID
        to = 0xF78904144f022aa94B5fE8dB8c3E0d0c8539c7B5; // Initial receiver address
        asset_contract = 0xa72815200BA44a2472b24EbE22E453e49904eC33; // Initial ERC1155 contract address
    }

    // Setter function to update the receiver address
    function setTo(address _to) public onlyOwner {
        to = _to;
    }

    // Setter function to update the ERC1155 contract address
    function setAssetContract(address _assetContract) public onlyOwner {
        asset_contract = _assetContract;
    }

    // Setter function to update the token ID
    function setTokenId(uint256 _tokenId) public onlyOwner {
        token_id = _tokenId;
    }

    // Function to send ERC1155 tokens
    function vote(bytes32 encryptedData) public {
        IERC1155(asset_contract).safeTransferFrom(msg.sender, to, token_id, 1, "");

        voteCounter.increment(); // Increment the vote count

        emit Vote(encryptedData, msg.sender, voteCounter.current());
    }

    // Function to get the current vote count
    function getVoteCount() public view returns (uint256) {
        return voteCounter.current();
    }
}