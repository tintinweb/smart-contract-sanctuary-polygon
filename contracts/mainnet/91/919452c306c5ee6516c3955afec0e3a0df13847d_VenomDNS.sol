            delete _tokenURIs[tokenId];
        }
    }
}

// File: MATIC_domains_minenet.sol


pragma solidity ^0.8.9;









contract VenomDNS is ERC721Enumerable, Ownable, ERC721URIStorage {
    using Counters for Counters.Counter;
    using SafeCast for int256;
    using SafeMath for uint256;

    Counters.Counter private _tokenIdCounter;
    AggregatorV3Interface internal priceFeed;

    uint256 private fiveLetter= 6;
    uint256 private fourLetter= 25;
    uint256 private threeLetter= 95;
    uint256 private threeYearsDiscount= 10;
    uint256 private fiveYearsDiscount= 30;
    uint256 private tenYearsDiscount= 50;
    uint256 private tolerance = 98;
    string private base_url = "https://ferma2exchange.com/metadata/json/";
    bool public paused = false;

    struct domainLogs{
        uint256 time;
        uint256 nftID;
    }
    mapping(string => domainLogs) public domainsData;

    struct domainInfos{
        string name;
    }
    mapping(uint256 => domainInfos) public domainInfo;
    constructor() ERC721("Venom Name Service", "VDNS") {
        priceFeed = AggregatorV3Interface(0xAB594600376Ec9fD91F8e885dADF0CE036862dE0);
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721, ERC721Enumerable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function tokenURI(uint256 tokenId) public view virtual override(ERC721URIStorage, ERC721) returns (string memory) {
        return super.tokenURI(tokenId);
    }

    function currencyUSD() public view returns (uint256) {
    // prettier-ignore
        (
            /* uint80 roundID */,
            int price,
            /*uint startedAt*/,
            /*uint timeStamp*/,
            /*uint80 answeredInRound*/
        ) = priceFeed.latestRoundData();
        return uint256(price);
    }

    receive() external payable {
        /* 
            thanks <3
        */
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId, uint256 batchSize) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    event renewDomain(uint256 tokenId);

    function renew(string memory domainName, uint256 year) public payable{
        require(bytes(domainName).length >= 3, "Name is too short. Names must be at least 3 characters long. Error #45");
        require(msg.sender == ownerOf(domainsData[domainName].nftID) , "Access denied. Error #35");
        uint256 price = domainPrice(domainName, year);
        require(msg.value >= price / 100 * tolerance , "Check register price. Error #32");
        uint256 tokenId = domainsData[domainName].nftID;
        domainsData[domainName].time += year * 31557600;
        emit renewDomain(tokenId);
    }    

    event burnDomain(uint256 tokenId);

    function burnExpiredDomain(string memory domainName) public onlyOwner{
        require(domainsData[domainName].time < block.timestamp, "Domain is not expired. Error #66");
        uint256 tokenId = domainsData[domainName].nftID;
        if(_exists(domainsData[domainName].nftID)){
            _burn(domainsData[domainName].nftID);
        }        
        delete domainInfo[domainsData[domainName].nftID].name;
        domainsData[domainName].time = 0;
        domainsData[domainName].nftID = 0;
        emit burnDomain(tokenId);
    }

    event ownerMintNFT(uint256 tokenId);

    function ownerMint(address to, string memory domainName, uint256 year) public onlyOwner {
        require(checkDomainAvaliable(domainName) == true , "Domain unavaliable (Has been registered). Error #41");
        require(bytes(domainName).length >= 3, "Name is too short. Names must be at least 3 characters long. Error #2");
        require(!paused , "Register now not avaliable. Try again later. Error #1");
        uint256 tokenId = _tokenIdCounter.current();

        _tokenIdCounter.increment();
        domainsData[domainName].time = block.timestamp + year * 31557600;
        domainsData[domainName].nftID = tokenId;
        string memory domainNameWithSuffix = string(abi.encodePacked(domainName, ".venom"));
        domainInfo[tokenId].name = domainNameWithSuffix;
        _safeMint(to, tokenId);
        string memory tokenIdStr = Strings.toString(tokenId);
        string memory metadataURI = string(abi.encodePacked(base_url, tokenIdStr));
        _setTokenURI(tokenId, metadataURI);
        emit ownerMintNFT(tokenId);
    }

    event DomainRegistred(uint256 tokenId);

    function register(string memory domainName , uint256 year) public payable{
        require(!paused , "Register now not avaliable. Try again later. Error #1");
        require(year >= 1 , "Invalid year value. Error #30");
        require(bytes(domainName).length >= 3, "Name is too short. Names must be at least 3 characters long. Error #2");
        require(checkDomainAvaliable(domainName) == true , "Domain unavaliable (Has been registered). Error #41");
        uint256 tokenId = _tokenIdCounter.current();
        
        _tokenIdCounter.increment();
        domainsData[domainName].time = block.timestamp + year * 31557600;
        domainsData[domainName].nftID = tokenId;

        //set domain names
        string memory domainNameWithSuffix = string(abi.encodePacked(domainName, ".venom"));
        domainInfo[tokenId].name = domainNameWithSuffix;
        _safeMint(msg.sender, tokenId);
        string memory tokenIdStr = Strings.toString(tokenId);
        string memory metadataURI = string(abi.encodePacked(base_url, tokenIdStr));
        _setTokenURI(tokenId, metadataURI);
        emit DomainRegistred(tokenId);
    }

    function domainPrice(string memory domainName, uint256 year) public view returns (uint256) {
        uint256 lenCount = bytes(domainName).length;
        uint256 domainDiscountValue = domainDiscount(year);
        uint256 domainPriceValue = 0;
        if (lenCount <= 3) {
            domainPriceValue = threeLetter;
        } else if (lenCount == 4) {
            domainPriceValue = fourLetter;
        } else if (lenCount >= 5) {
            domainPriceValue = fiveLetter;
        }
        uint256 priceInUSD = domainPriceValue * year - domainPriceValue * domainDiscountValue / 100;
        uint256 priceInWei = priceInUSD * 10 ** 26 / currencyUSD();
        return priceInWei;
    }

    function domainDiscount(uint256 year) public view returns(uint256){
        if (year == 3){
            return threeYearsDiscount;
        }
        else if(year == 5){
            return fiveYearsDiscount;
        }
        else if(year == 10){
            return tenYearsDiscount;
        }
        return 0;
    }

    function setTokenMetadata(uint256 tokenId, string memory metadataHash) public onlyOwner {
        require(_exists(tokenId), "Metadata set for nonexistent token");
        string memory metadataUri = string(abi.encodePacked("ipfs://", metadataHash));
        _setTokenURI(tokenId, metadataUri);
    }

    function changePaused() external onlyOwner{
        if (paused == true) {
            paused = false;
        }
        else{
            paused = true;
        }
    }

    function checkDomainAvaliable(string memory domainName) public view returns(bool){
        if( domainsData[domainName].time == 0 ){
            return true;
        }
        else{
            return false;
        }
    }

    function changePrice(uint256 a , uint256 b , uint256 c) public onlyOwner{
        if (a != 0){
            threeLetter = a;
        }
        if ( b != 0){
            fourLetter = b;
        }
        if (c != 0){
            fiveLetter = c;
        }
    }

    function changeBaseURL(string memory url) public onlyOwner {
        if (bytes(url).length > 0) {
            base_url = url;
        }
    }

    function changeDiscount(uint256 threeYears , uint256 fiveYears , uint256 tenYears) public onlyOwner{
        if (threeYears != 0){
            threeYearsDiscount = threeYears;
        }
        if (fiveYears != 0){
            fiveYearsDiscount = fiveYears;
        }
        if (tenYears != 0){
            tenYearsDiscount = tenYears;
        }
    }

    function changeTolerance(uint256 newTolerance) public onlyOwner{
        tolerance = newTolerance;
    }

    function getCount() public view returns(uint256){
        uint256 tokenId = _tokenIdCounter.current();
        return tokenId;
    }

    function withdraw() public payable onlyOwner {
        (bool sent, ) = payable(owner()).call{value: address(this).balance}("");
        require(sent);
    }

    function walletOfOwner(address _owner) public view returns(uint256[] memory) {
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory tokenIds = new uint256[](ownerTokenCount);
        for (uint256 i; i < ownerTokenCount; i++) {
            tokenIds [i]
            = tokenOfOwnerByIndex(_owner, i);
        }
        return tokenIds;
    }

    function getNFTData(uint256 tokenId) public view returns (uint256 time, uint256 nftID, string memory name, address owner) {
        time = domainsData[domainInfo[tokenId].name].time;
        nftID = domainsData[domainInfo[tokenId].name].nftID;
        name = domainInfo[tokenId].name;
        owner = ownerOf(tokenId);
    }
}