    function _tokenIssueIds(uint256 tokenId) internal view virtual returns (uint256 issueId, uint256 tier) {
        (tier, ) = SeriesHelper.extractTokenIds(tokenId);
        (issueId, ) = SeriesHelper.extractTokenIds(tier);
    }

    /**
     * @notice Allows to customiase the way token ID is derived for the next token.
     * 
     * @dev By default the `serial` value is returned.
     */
    function _nextTokenId(IssueInfo memory issue, IssueTier memory tier, uint256 serial, bool forceSerial) internal view virtual returns (uint256) {
        if (issue.isTiered) {
            uint256 _baseId = SeriesHelper.combineTokenId(issue.issueId, tier.tier);
            return SeriesHelper.combineTokenId(_baseId, serial);
        } else {
            if (!forceSerial) {
                return _itemsCounters[issue.issueId][tier.tier] + 1;
            } else {
                return serial;
            }
        }
    }

    /**
     * @dev Allows to check whether a given `account` is entitled for pre-sale minting.
     */
    function _hasMintingPass(uint256 issueId, IssuePhase phase, address account) internal view virtual returns (bool);

    function _checkout(address beneficiary, uint256 totalAmount) internal virtual nonReentrant {
        require(msg.value >= totalAmount, "Not enough value sent");

        address _beneficiary = (beneficiary != address(0) ? beneficiary : owner());
        payable(_beneficiary).transfer(totalAmount);
        payable(_msgSender()).transfer(msg.value - totalAmount);
    }

    // The following functions are overrides required by Solidity.

    /**
