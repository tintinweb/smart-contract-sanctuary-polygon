        __BaseCollectibleSeries_init(initialUri, contractUri, issueId, resolverContract, repositoryContract);

        token = _token;
        thresholdBalance = _thresholdBalance;
    }

    /**
     * @dev Allows to ajust the threshold balance needed for community pass.
     *
     * @param balance - The new threshold balance.
     */
    function ajustThresholdBalance(uint256 balance) external onlyPrivileged {
        thresholdBalance = balance;
    }

    /**
     * @dev Allows to check whether a given `account` is entitled for pre-sale minting.
     */
    function _hasMintingPass(uint256, IssuePhase phase, address account) internal view virtual override returns (bool) {
        return (phase == IssuePhase.Presale && token.balanceOf(account) >= thresholdBalance);
    }

