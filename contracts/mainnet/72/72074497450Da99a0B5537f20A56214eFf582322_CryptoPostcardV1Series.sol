        emit SeriesTokensIssued(issueId, tier, depositaries.length);
    }

    /**
     * @notice Items minting function for the on-chain purchases.
     * 
     * @param issueId The ID of the issue to mint.
     * @param seriesId The ID of the issue series to mint.
     * @param amount The amount of items to mint.
     * @param recipient If set, the destination wallet. 
     * @param rounds IDs of exchange rate quotation rounds, [0,0] for latest.
     * @param consentApproved By setting the value to `true` you agree to the Terms and conditions of the service.
     */
    function mintItems(uint256 issueId, uint256 seriesId, uint256 amount, address recipient, uint80[2] calldata rounds, bool consentApproved) public payable virtual nonContract {
        require(consentApproved, "Terms not approved");

        (IssueInfo memory issue, IssueSeries memory series, IssueTier memory tier) = seriesRepository.getIssueSeries(issueId, seriesId);
        
        require(_supportedIssues[issueId] && series.seriesId > 0 && !series.onlyBundled, "Invalid series");
        require(series.isMintable && (issue.issuePhase == _mintingPhase || _hasMintingPass(issueId, issue.issuePhase, _msgSender())), "Sale not active!"); 
        
        require(tier.maxSupply > _itemsCounters[issue.issueId][series.tier] + amount, "Not enough tokens");

        address _recipient = (recipient != address(0) ? recipient : _msgSender());
        require(_itemsCollected[_msgSender()][series.seriesId] + amount <= series.maxMints, "Limit claimed");

        uint256[] memory _ids = new uint256[](amount); 
        uint256[] memory _amounts = new uint256[](amount); 

        uint256 _offset = _seriesCounters[issue.issueId][series.seriesId] + series.offset;
        require(series.limit >= _offset + amount, "Exceeded mint range");

        for (uint256 i = 0; i < amount; i++) {
            _ids[i] = _nextTokenId(issue, tier, _offset + i + 1, false);

            require(!tier.isTangible || !exists(_ids[i]), "ID already minted");

            _amounts[i] = 1;
        }

        (uint256 _tokenPrice,) = seriesRepository.getDerivedSeriesPrice(issueId, seriesId, rounds);
        _checkout(issue.beneficiary, (_tokenPrice * amount));
        _mintBatch(_recipient, _ids, _amounts, "");
        
        unchecked {
            _itemsCounters[issue.issueId][series.tier] += amount;
            _itemsCollected[_msgSender()][series.seriesId] += amount;
            _seriesCounters[issue.issueId][series.seriesId] += amount;
        }

        emit SeriesTokensMinted(_recipient, issueId, series.seriesId, amount);   
    }

    function mintBundle(uint256 issueId, uint256 bundleId, uint256 amount, address recipient, uint80[2] calldata rounds, bool consentApproved) public payable virtual nonContract {
        require(consentApproved, "Terms not approved");
 
        (IssueInfo memory issue, IssueBundle memory bundle) = seriesRepository.getIssueBundle(issueId, bundleId); 
        address _recipient = (recipient != address(0) ? recipient : _msgSender());
        
        require(_supportedIssues[issueId] && bundle.bundleId > 0, "Invalid series bundle");
        require(bundle.isMintable && (issue.issuePhase == _mintingPhase || _hasMintingPass(issueId, issue.issuePhase, _msgSender())), "Sale not active!"); 

        (uint256[] memory _ids, uint256[] memory _amounts) = _assemblyBundle(issue, bundle, amount);

        (uint256 _tokenPrice,) = seriesRepository.getDerivedBundlePrice(issueId, bundleId, rounds);
        _checkout(issue.beneficiary, (_tokenPrice * amount));
        _mintBatch(_recipient, _ids, _amounts, "");

        emit SeriesBundleMinted(_recipient, issueId, bundleId); 
    }

    function _assemblyBundle(IssueInfo memory issue, IssueBundle memory bundle, uint256 amount) internal returns (uint256[] memory _ids, uint256[] memory _amounts) {
        BundledSeries[] memory bundledSeries = bundle.series;

        _ids = new uint256[](amount * bundledSeries.length); 
        _amounts = new uint256[](amount * bundledSeries.length); 

        uint256 _batchIndex = 0;
        for (uint256 i = 0;  i < bundledSeries.length; i++) {
            (, IssueSeries memory series, IssueTier memory tier) = seriesRepository.getIssueSeries(issue.issueId, bundledSeries[i].seriesId);

            require(series.isMintable && series.onlyBundled && tier.maxSupply > _itemsCounters[issue.issueId][series.tier] + bundledSeries[i].amount, "Not enough tokens");
            require(_itemsCollected[_msgSender()][series.seriesId] + bundledSeries[i].amount <= series.maxMints, "Limit claimed");

            uint256 _amount = amount * bundledSeries[i].amount;
            uint256 _offset = _seriesCounters[issue.issueId][series.seriesId] + series.offset;
            require(series.limit >= _offset + _amount, "Exceeded mint range");

            for (uint256 j = 0; j < _amount; j++) {
                uint256 _tokenId = _nextTokenId(issue, tier, _offset + j + 1, false);
                require(!tier.isTangible || !exists(_tokenId), "ID already minted");

                _ids[_batchIndex] = _tokenId;
                _amounts[_batchIndex] = bundledSeries[i].amount;
                _batchIndex++;
            }

            unchecked {
                _itemsCounters[issue.issueId][series.tier] += _amount;
                _itemsCollected[_msgSender()][series.seriesId] += _amount;
                _seriesCounters[issue.issueId][series.seriesId] += _amount;
            }
        }
    }

