pragma solidity ^0.8.0;

import {ERC20} from "solmate/tokens/ERC20.sol";
import {SafeTransferLib} from "solmate/utils/SafeTransferLib.sol";
import {DistributionVaultExternalMerkleUpgradeable} from
    "dappcomposer-contracts-upgradeable/distribute/DistributionVaultExternalMerkleUpgradeable.sol";
import {OwnableUpgradeable} from "openzeppelin-contracts-upgradeable/access/OwnableUpgradeable.sol";

contract FITMDU is OwnableUpgradeable, DistributionVaultExternalMerkleUpgradeable {
    using SafeTransferLib for ERC20;

    function initialize(address token_, address vault_, bytes32 merkleRoot_, string memory uri_) public initializer {
        __Ownable_init();
        __DistributionVaultExternalMerkleUpgradeable_init(token_, vault_, merkleRoot_, uri_);
    }

    // solhint-disable-next-line
    function _authorizeUpdateMerkleRoot() internal view override onlyOwner {}

    // solhint-disable-next-line
    function _authorizeSetVault() internal view override onlyOwner {}
}

