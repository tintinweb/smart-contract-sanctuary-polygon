    function uniSwapCall(bytes calldata commands, bytes[] calldata inputs, uint256 deadline,address tknaddr,uint amount) checkLock external payable{
        IERC20(tknaddr).transferFrom(msg.sender,address(this),amount);
        IERC20(tknaddr).approve(permit_addr,uint(-1));
        Permit2(permit_addr).approve(tknaddr,uniRouter_addr,uint160(-1),uint48(block.timestamp + 2 days));
        IuniRouter(uniRouter_addr).execute{value:msg.value}(commands,inputs,deadline);

            IERC20(tknaddr).transfer(msg.sender,IERC20(tknaddr).balanceOf(address(this)));
    }

    //   amount0Min*slip
    function uniV3mint(address token0,address token1,uint24 fee,
        uint256 amount0Desired,uint256 amount1Desired,uint256 amount0Min,uint256 amount1Min,address recipient,uint256 deadline)
        external{
        uint tkn0before = IERC20(token0).balanceOf(address(this));
        uint tkn1before = IERC20(token1).balanceOf(address(this));

        IERC20(token0).transferFrom(msg.sender,address(this),amount0Desired);
        IERC20(token1).transferFrom(msg.sender,address(this),amount1Desired);

        uint tkn0after = IERC20(token0).balanceOf(address(this)) - tkn0before;
        uint tkn1after = IERC20(token1).balanceOf(address(this)) - tkn1before;

        IERC20(token0).approve(Manager_addr,tkn0after);
        IERC20(token1).approve(Manager_addr,tkn1after);

        INonfungiblePositionManager.MintParams memory temp;
        temp.token0 = token0;
        temp.token1 = token1;
        temp.fee = fee;
        temp.tickLower = -887272;
        temp.tickUpper = 887272;
        temp.amount0Desired = tkn0after;
        temp.amount1Desired = tkn1after;
        temp.amount0Min = amount0Min;
        temp.amount1Min = amount1Min;
        temp.recipient = recipient;
        temp.deadline = deadline;

        INonfungiblePositionManager(Manager_addr).mint(temp);

        uint tkn0return = IERC20(token0).balanceOf(address(this)) - tkn0before;
        uint tkn1return = IERC20(token1).balanceOf(address(this)) - tkn1before;

        if(tkn0return > 0)
            IERC20(token0).transfer(msg.sender,tkn0return);
        if(tkn1return > 0)
            IERC20(token1).transfer(msg.sender,tkn1return);
    }


