    function uniV3DecreaseLiquidity(uint256 tokenId,uint128 liquidity,uint256 amount0Min,uint256 amount1Min,uint256 deadline)
        external{
        
        // Check that the caller is the owner of the NFT
        require(msg.sender == INonfungiblePositionManager(Manager_addr).ownerOf(tokenId), "Not the owner");

        INonfungiblePositionManager.DecreaseLiquidityParams memory temp;
        temp.tokenId = tokenId;
        temp.liquidity = liquidity;
        temp.amount0Min = amount0Min;
        temp.amount1Min = amount1Min;
        temp.deadline = deadline;

        // Call decreaseLiquidity method of positionManager
        INonfungiblePositionManager(Manager_addr).decreaseLiquidity(temp);

        INonfungiblePositionManager.CollectParams memory tempcl;
        tempcl.tokenId = tokenId;
        tempcl.recipient = address(this);
        tempcl.amount0Max = uint128(-1);
        tempcl.amount1Max = uint128(-1);

        INonfungiblePositionManager(Manager_addr).collect(tempcl);

    }
