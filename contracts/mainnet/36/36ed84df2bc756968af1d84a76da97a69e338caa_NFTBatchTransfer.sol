// File: NFTBatchTransfer.sol



pragma solidity ^0.8.17;





contract NFTBatchTransfer is AccessControl, IERC721Receiver {

  bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

  mapping(address => uint256[]) public transferredTokens;

  event TokenTransferred(address indexed from, address indexed to, uint256 indexed tokenId);

  constructor() {
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setRoleAdmin(ADMIN_ROLE, DEFAULT_ADMIN_ROLE);
  }

  function batchTransferWithProof(
    bytes32 merkleRoot, 
    bytes calldata encodedData,
    bytes32[] calldata proofs
  ) external onlyRole(ADMIN_ROLE) {
    require(proofs.length < 50, "Too many proofs");  
    
    bytes32 leaf = keccak256(encodedData);
    require(MerkleProof.verify(proofs, merkleRoot, leaf), "Invalid proof");
    
    // Decode data and transfer tokens
    (address[] memory recipients, uint256[] memory tokenIds) = abi.decode(encodedData, (address[],uint256[]));
    uint256 length = recipients.length;
    require(length == tokenIds.length, "Invalid data");

    for(uint i = 0; i < length; i++) {
      address from = msg.sender;
      address to = recipients[i];
      uint256 id = tokenIds[i];
      IERC721(from).safeTransferFrom(from, to, id);

      emit TokenTransferred(from, to, id);
    }
  }

  function getTransferredTokens(address user) external view returns (uint256[] memory) {
    return transferredTokens[user];
  }

  function withdraw(uint256 tokenId) external {
    require(msg.sender == IERC721Enumerable(msg.sender).ownerOf(tokenId), "Not owner");
    transferredTokens[msg.sender].push(tokenId);
    IERC721Enumerable(msg.sender).safeTransferFrom(address(this), msg.sender, tokenId);
  }

  function revokeRole(bytes32 role, address account) public override onlyRole(ADMIN_ROLE) {
    _revokeRole(role, account);
  }

  function grantRole(bytes32 role, address account) public override onlyRole(ADMIN_ROLE) {
    _grantRole(role, account);
  }

  function supportsInterface(bytes4 interfaceId) public view virtual override(AccessControl) returns (bool) {
    return interfaceId == type(IERC721Receiver).interfaceId || super.supportsInterface(interfaceId);
  }

  function onERC721Received(address, address, uint256, bytes memory) public virtual override returns (bytes4) {
    return this.onERC721Received.selector;
  }

}