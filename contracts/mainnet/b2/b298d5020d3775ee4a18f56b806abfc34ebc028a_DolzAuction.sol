pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@opengsn/contracts/src/BaseRelayRecipient.sol";
import "./DolzAuctionNFT.sol";

contract DolzAuction is Ownable, BaseRelayRecipient {
    string public override versionRecipient = "1.0.0";

    using SafeERC20 for IERC20;

    /// token Currency used for the auction
    /// saleStart Timestamp of the auction start
    /// saleEnd Timestamp of the auction end
    /// supply How many tokens are set to auction
    /// minPrice Lowest bid possible
    /// nbBids Current number of bids set
    /// hashRoot Root hash of the MerkleTree that contains withdrawal informations
    /// rarityCount Number of different rarities available to bid on
    /// totalBidsPerUser Allows to calculate losers refunds easily
    /// bidders List of users that have bidded
    /// bids Array that contains all the bids
    /// rarityInfos Object that contains the amount of NFT to sell by rarity and the end date of each auctions
    address public token;
    address public nftEscrow;
    DolzAuctionNFT nft;
    uint32 public supply;
    // uint16[] minPrices;
    uint32 toReserve;
    uint32 public rarityCount;
    uint256 public startingTokenId;
    uint256 nbBids;
    bytes32 hashRoot;
    bool public fundsWithdrawed;
    mapping(address => uint256) public userRefunded;
    mapping(address => mapping(uint256 => uint256)) totalBidsPerUser;
    mapping(uint256 => bool) public tokensClaimed;
    mapping(address => mapping(uint256 => bool)) bidders;
    Bid[] bids;
    RarityInfos rarityInfos;

    /// Event fired everytime someone Bid.
    /// Used to easily get the full list of bids.
    event BidCreated(
        address bidder,
        uint256 bid,
        uint256 timestamp,
        uint256 bidId,
        uint256 rarityId
    );

    /// Event fired everytime someone update their Bid.
    /// Used to easily get the full list of updates.
    event BidUpdated(
        address bidder,
        uint256 bid,
        uint256 timestamp,
        uint256 bidId,
        uint256 rarityId
    );

    event Vaulted(
        uint256 tokenId,
        uint256 timestamp,
        address vaultAddress
    );

    /// Event fired everytime someones withdraw something 
    /// Used to fill the database easily 
    event Withdrawed(
        uint256[] tokenIds,
        uint256 refundAmount,
        address claimer
    );

    /// Event fired everytime a hashroot is set
    event HashRootSet(
        uint256 timestamp
    );

    /// @notice Used as parameter for the withdraw function.
    /// Allows to access all datas quickly.
    struct WithdrawObject {
        uint256[] tokenIDs;
        uint256 refundAmount;
        address bidder;
    }

    /// @notice A basic bid structure.
    /// Has a bid ID which correspond to the current nbBids.
    /// This allows us to quickly update the BidCreated event list
    /// based on the BidUpdated list.
    struct Bid {
        address bidder;
        uint256 value;
        uint256 timestamp;
        uint256 bidId;
        uint256 rarityId;
    }

    struct RarityInfos {
        uint256[] raritySupplies;
        uint256[] rarityStartDates;
        uint256[] rarityEndDates;
        uint16[] minPrices;
    }

    // MODIFIERS

    /// @param _rarityId Rarity which we have to do the verification on
    modifier saleIsOpen(uint256 _rarityId) {
        require(
            block.timestamp >= rarityInfos.rarityStartDates[_rarityId] && block.timestamp < rarityInfos.rarityEndDates[_rarityId],
            "DolzAuction: Sales are not open"
        );
        _;
    }

    /// @param _rarityId Rarity which we have to do the verification on
    modifier saleIsEnded(uint256 _rarityId) {
        require(
            block.timestamp >= rarityInfos.rarityEndDates[_rarityId],
            "DolzAuction: Sales are still in progress"
        );
        _;
    }

    /// Check if all auctions are ended
    modifier allSalesEnded() {
        bool allEnded = true;
        for(uint32 i = 0; i < rarityCount; i++){
            if(rarityInfos.rarityEndDates[i] > block.timestamp){
                allEnded = false;
            }
        }
        require(allEnded == true, "DolzAuction: not all sales are ended");
        _;
    }

    /// @param _owner Ownership will be transferred to this address.
    /// @param _token Currency used for the auction
    /// @param _rarityInfos Object that contains Supplies and Auction End Date for each rarities
    constructor(
        address _owner,
        address _token,
        address _nft,
        uint32[4] memory _numbers,
        RarityInfos memory _rarityInfos,
        address _nftEscrow
    ) {
        transferOwnership(_owner);
        nft = DolzAuctionNFT(_nft);
        token = _token;
        supply = _numbers[0];
        rarityCount = _numbers[1];
        toReserve = _numbers[2];
        startingTokenId = _numbers[3];
        rarityInfos = _rarityInfos;
        nbBids = 0;
        nftEscrow = _nftEscrow;
        fundsWithdrawed = false;
    }

    /// @notice Verify if a proof is inside the MerkleTree.
    /// @param _merkleproof Proof built off-chain
    /// @param _hashRoot MerkleTree root
    /// @param _leaf Leaf that we're looking for inside the tree
    /// @dev Returns true or false whether the leaf is inside the tree or not
    function _verify(bytes32[] calldata _merkleproof, bytes32 _hashRoot, bytes32 _leaf) public pure returns (bool) {
        if (MerkleProof.verify(_merkleproof, _hashRoot, _leaf)) {
            return true;
        } else {
            return false;
        }
    }

    /// @param bidder Address of the wallet we're looking for
    /// @dev Returns true or fals whether the user has already bidded or not
    function alreadyBidded(address bidder, uint256 _rarityId) public view returns (bool) {
        return bidders[bidder][_rarityId];
    }

    /// @notice Update bid amount and timestamp based on his Id
    /// @param _bidId Index of the bid we want to update.
    /// @param _newAmount Bid amount after update
    /// @dev Emit a BidUpdated event so we can quickly access to the updated node
    function updateBid(uint256 _bidId, uint256 _newAmount, uint256 _rarityId) public saleIsOpen(_rarityId) {
        require(
            _newAmount - bids[_bidId].value > 0,
            "DolzAuction: wrong amount sent"
        );
        require(
            _newAmount >= rarityInfos.minPrices[_rarityId] * 10 / 100, 
            "DolzAuction: Can't add less than 10% of your current bid"
        );
        require(
            _msgSender() == bids[_bidId].bidder,
            "DolzAuction: Not the bidder"
        );
        uint256 toPay = _newAmount - bids[_bidId].value;
        bids[_bidId].value = _newAmount;
        bids[_bidId].timestamp = block.timestamp;
        IERC20(token).safeTransferFrom(_msgSender(), address(this), toPay * 1e18);
        totalBidsPerUser[_msgSender()][_rarityId] += toPay;
        emit BidUpdated(_msgSender(), _newAmount, block.timestamp, _bidId, _rarityId);
    }

    /// @notice Set a bid for the use with the given _amount
    /// @param _amount Amount of the bid
    /// @dev Emit a BidCreated event so we can quickly access the bid list
    function bid(uint256 _amount, uint256 _rarityId) external saleIsOpen(_rarityId) {
        require(
            _amount >= rarityInfos.minPrices[_rarityId],
            "DolzAuction: Bid is lower than minimum price"
        );
        require(
            (_rarityId >= 0 && _rarityId < rarityCount),
            "DolzAuction: Trying to bid on unknown rarity"
        );

        if (!alreadyBidded(_msgSender(), _rarityId)) {
            bidders[_msgSender()][_rarityId] = true;
        }
        bids.push(Bid(_msgSender(), _amount, block.timestamp, nbBids, _rarityId));
        IERC20(token).safeTransferFrom(_msgSender(), address(this), _amount * 1e18);
        totalBidsPerUser[_msgSender()][_rarityId] += _amount;
        emit BidCreated(_msgSender(), _amount, block.timestamp, nbBids, _rarityId);
        nbBids++;
    }

    /// @notice Transfer unsold tokens to given address so we can use them later
    /// @param _vault Address where to stock the NFTs
    /// @param _tokenIds Ids to transfer
    function transferToVault(address _vault, uint256[] calldata _tokenIds) external onlyOwner{
        for(uint256 i = 0; i < _tokenIds.length; i++){
            ERC721A(nft).safeTransferFrom(nftEscrow, _vault, _tokenIds[i] + startingTokenId);
            emit Vaulted(_tokenIds[i], block.timestamp, _vault);
        }
    }

    /// @notice Transfer token if some user bids are in the top bids and currencies if some user bids didn't make it to the top bidders
    /// @param _withdrawObject Object that contains the datas needed for the transfers to be made
    /// @param _leaf Bytes32 hash that should be apart of the MerkleTree
    /// @param _proof Bytes32 Array of hashes that is build off-chain based on the _leaf and used to check if _leaf is in the MerkleTree
    function withdraw(WithdrawObject calldata _withdrawObject, bytes32 _leaf, bytes32[] calldata _proof) public {
        bytes32 leaf = keccak256(
            abi.encodePacked(
                _msgSender(),
                _withdrawObject.tokenIDs,
                _withdrawObject.refundAmount
            )
        );
        require(
            leaf == _leaf,
            "DolzAuction: Wrong values provided for withdrawal"
        );
        require(
            _verify(_proof, hashRoot, _leaf),
            "DolzAuction: Nothing to withdraw has been found"
        );

        emit Withdrawed(_withdrawObject.tokenIDs, _withdrawObject.refundAmount, _msgSender());
        
        if (_withdrawObject.refundAmount > 0) {
            if(userRefunded[_msgSender()] < _withdrawObject.refundAmount) {
                uint256 toPay = _withdrawObject.refundAmount - userRefunded[_msgSender()];
                userRefunded[_msgSender()] = _withdrawObject.refundAmount;
                require(
                    IERC20(token).transfer(
                        address(_withdrawObject.bidder),
                        toPay * 1e18
                    ),
                    "DolzAuction: transfer failed"
                );
            }
        }
        if (_withdrawObject.tokenIDs.length != 0) {
            for (uint256 i = 0; i < _withdrawObject.tokenIDs.length; i++) {
                if(!tokensClaimed[_withdrawObject.tokenIDs[i]]){
                    tokensClaimed[_withdrawObject.tokenIDs[i]] = true;
                    if(startingTokenId != 0){
                        ERC721A(nft).safeTransferFrom(
                            nftEscrow,
                            _msgSender(),
                            _withdrawObject.tokenIDs[i] + startingTokenId
                        );
                    } else {
                        ERC721A(nft).safeTransferFrom(
                            nftEscrow,
                            _msgSender(),
                            _withdrawObject.tokenIDs[i]
                        );
                    }
                }
            }
        }
    }

    // TODO Claim dolz based on wins and lose
    function withdrawFunds(uint256 amount) public onlyOwner allSalesEnded(){
        IERC20(token).transfer(
            nftEscrow,
            amount * 1e18
        );
    }

    function emergencyWithdraw() public onlyOwner allSalesEnded(){
        uint256 toTransfer = IERC20(token).balanceOf(address(this));
        IERC20(token).transfer(
            nftEscrow,
            toTransfer
        );
    }

    // GETTERS
    function getNbBids() public view returns (uint256) {
        return nbBids;
    }

    function getTotalBiddedPerUser(address _bidder) public view returns(uint256) {
        uint256 toReturn = 0;
        for(uint256 i; i < rarityCount; i++){
            toReturn += totalBidsPerUser[_bidder][i];
        }
        return toReturn;
    }

    function getTotalBiddedPerUserPerRarity(address _bidder, uint256 _rarityId) public view returns(uint256) {
        return totalBidsPerUser[_bidder][_rarityId];
    }

    function getHashRoot() public view returns (bytes32) {
        return hashRoot;
    }

    function getNftAddress() public view returns (address) {
        return address(nft);
    }

    function getBalance(address _wallet) public view returns (uint256) {
        return ERC721A(nft).balanceOf(_wallet);
    }

    function getSaleSettings() public view returns (RarityInfos memory) {
        RarityInfos memory toReturn = rarityInfos;
        return (toReturn);
    }

    function getOwnershipOfToken(uint256 _id) public view returns (address) {
        return (ERC721A(nft).ownerOf(_id));
    }

    function getRaritySupplies() public view returns (uint256[] memory ){
        return rarityInfos.raritySupplies;
    }

    function getBids() public view returns(Bid[] memory){
        return bids;
    }
    
    function getTokenIsClaimed(uint256[] calldata _tokenIds) public view returns(bool[] memory){
        require(_tokenIds.length > 0, "DolzAuction: Nothing to check");
        bool[] memory ret = new bool[](_tokenIds.length);

        for (uint256 i = 0; i < _tokenIds.length; i++){
            if(_tokenIds[i] >= 0 && _tokenIds[i] < supply){
                if(!tokensClaimed[_tokenIds[i]]){
                    ret[i] = false;
                } else {
                    ret[i] = true;
                }
                ret[i] = tokensClaimed[_tokenIds[i]];
            }
        }

        return(ret);
    }

    // SETTTERS
    function setHashRoot(bytes32 _hashRoot) public onlyOwner {
        emit HashRootSet(block.timestamp);
        hashRoot = _hashRoot;
    }

    function _msgSender() internal view override(Context, BaseRelayRecipient)
        returns (address sender) {
        sender = BaseRelayRecipient._msgSender();
    }

    function _msgData() internal view override(Context, BaseRelayRecipient)
        returns (bytes calldata) {
        return BaseRelayRecipient._msgData();
    }

}

