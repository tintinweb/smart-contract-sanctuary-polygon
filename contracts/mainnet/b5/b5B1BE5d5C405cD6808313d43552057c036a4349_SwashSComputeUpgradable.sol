pragma solidity 0.8.6;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/SignatureCheckerUpgradeable.sol";

contract SwashSwapBaseUpgradeable is OwnableUpgradeable {

    event TokenAdded(string name, address tokenAddress, bool isNative);
    event TokenRemoved(string name);
    event Withdrawn(address reciever, uint256 amount, address tokenAddress);
    event WithdrawnNative(address reciever, uint256 amount);

    struct SignerOb {
        bytes signature;
        address signer;
    }

    struct TokenStruct {
        string name;
        address tokenAddress;
        bool isNative;
    }
    //info about DataProduct, user can request a data product and backend will determine the price and then sign it
    struct DataProduct {
        string requestHash;
        uint256 price;
        bytes signature;
        uint256 timeStamp;
    }

    struct  ProductOb {
        string requestHash;
        uint256 price;
        string productType;
        uint256 timeStamp;
    }

    struct LicenseToBuy {
        string tokenName;
        address tokenAddress;
        bool isNative;
        uint256 price;
    }

    address public swashTokenAddress;
    address public baseTokenAddress;
    address[] public swashToBaseTokenPath;

    string[] public tokenList;
    mapping(string => TokenStruct) public tokenMap;

    address public uniswapRouter;
    uint256 public SLIPPAGE;
    uint256 public timeWindow;

    address[] public _signersList;
    mapping(address => bool) public _signersMap;

    uint public purchaseTimeWindow;

    constructor(){}

    function baseInitialize(
        address _swashTokenAddress,
        address _baseTokenAddress,
        address[] memory _swashToBaseTokenPath,
        uint _purchaseTimeWindow
    ) public initializer {
        __Ownable_init();
        swashTokenAddress = _swashTokenAddress;
        baseTokenAddress = _baseTokenAddress;
        swashToBaseTokenPath = _swashToBaseTokenPath;
        timeWindow = 600;
        SLIPPAGE = 5;
        purchaseTimeWindow = _purchaseTimeWindow;
    }

    //getter setters
    //===============================================================================================

    function setSwashTokenAddress(address _swashTokenAddress) public onlyOwner {
        swashTokenAddress = _swashTokenAddress;
    }

    function setSwashToBaseTokenPath(address[] memory _swashToBaseTokenPath) public onlyOwner {
        swashToBaseTokenPath = _swashToBaseTokenPath;
    }

    function setSlippage(uint256 slippage) public onlyOwner {
        SLIPPAGE = slippage;
    }

    function setTimeWindow(uint256 _timeWindow) public onlyOwner {
        timeWindow = _timeWindow;
    }

    function setPurchaseTimeWindow(uint256 _purchaseTimeWindow) public onlyOwner {
        purchaseTimeWindow = _purchaseTimeWindow;
    }

    function setBaseTokenAddress(address _baseTokenAddress) public onlyOwner {
        baseTokenAddress = _baseTokenAddress;
    }

    function resetTokenList() external onlyOwner {
        for (uint idx = tokenList.length; idx > 0; --idx) {
            delete tokenMap[tokenList[idx - 1]];
            tokenList.pop();
        }
    }

    function addTokenList(
        string[] memory _nameList,
        address[] memory _addressList,
        bool[] memory _isNativeList
    ) external onlyOwner {
        require(
            _nameList.length == _addressList.length,
            "pe01"
        );

        require(
            _nameList.length == _isNativeList.length,
            "pe02"
        );

        for (uint idx = 0; idx < _nameList.length; ++idx)
        {
            addToken(_nameList[idx], _addressList[idx], _isNativeList[idx]);
        }

    }

    function addToken(
        string memory _name,
        address _address,
        bool isNative
    ) public onlyOwner {
        require(
            keccak256(abi.encodePacked(_name)) != keccak256(abi.encodePacked("")),
            "pe03"
        );

        require(
            keccak256(abi.encodePacked(tokenMap[_name].name)) == keccak256(abi.encodePacked("")),
            "pe04"
        );

        tokenList.push(_name);
        tokenMap[_name] = TokenStruct(_name, _address, isNative);

        emit TokenAdded(_name, _address, isNative);
    }

    function removeToken(
        string memory _name
    ) external onlyOwner {
        require(
            tokenMap[_name].tokenAddress != address(0),
            "pe05"
        );

        uint256 len = tokenList.length;
        uint8 idx = 0;
        for (; idx < len - 1; ++idx) {
            if (keccak256(abi.encodePacked(tokenList[idx])) == keccak256(abi.encodePacked(_name))) {
                break;
            }
        }
        tokenList[idx] = tokenList[len - 1];
        tokenList.pop();

        delete tokenMap[_name];

        emit TokenRemoved(_name);
    }

    function getTokenOb(string memory tokenName) public view returns (TokenStruct memory){
        return tokenMap[tokenName];
    }

    function getTokenObList() public view returns (TokenStruct[] memory){
        TokenStruct[] memory ret = new TokenStruct[](tokenList.length);
        for(uint index = 0; index < tokenList.length;++index){
            ret[index]=tokenMap[tokenList[index]];
        }
        return ret;
    }

    function setUniswapRouter(address _uniswapRouter) public onlyOwner {
        uniswapRouter = _uniswapRouter;
    }

    //****************************************** converts ***********************************************************

    function getAmountsIn(uint amountOut, address[] calldata path) internal view returns (uint amount){
        return IUniswapV2Router02(uniswapRouter).getAmountsIn(amountOut, path)[0];
    }

    function priceInSwash(uint256 priceInBaseToken) public view returns (uint256){
        return IUniswapV2Router02(uniswapRouter).getAmountsIn(priceInBaseToken, swashToBaseTokenPath)[0];
    }

    function priceInSpecificToken(uint256 priceInSwashToken, address[] calldata path) public view returns (uint256){
        return IUniswapV2Router02(uniswapRouter).getAmountsIn(priceInSwashToken, path)[0];
    }

    //Withdraw
    //===============================================================================================

    function withdraw(address _tokenAddress) external onlyOwner {
        uint256 balance = IERC20(_tokenAddress).balanceOf(address(this));
        IERC20(_tokenAddress).transfer(msg.sender, balance);
        emit Withdrawn(msg.sender, balance, _tokenAddress);
    }

    function withdrawNative() external onlyOwner payable {
        uint256 balance = address(this).balance;
        //        bool sent = msg.sender.send(balance);
        (bool sent, bytes memory data) = msg.sender.call{value : balance}("");

        require(sent, "pe14");

        emit WithdrawnNative(msg.sender, balance);
    }

    //Sign and signers
    //===============================================================================================
    function addSignerList(address[] memory signers) external onlyOwner {
        for (uint idx = 0; idx < signers.length; ++idx) {
            addSigner(signers[idx]);
        }
    }

    function addSigner(address signer) public onlyOwner {

        require(signer != address(0), "pe17");
        require(!_signersMap[signer], "pe18");

        _signersList.push(signer);
        _signersMap[signer] = true;

    }

    function removeSignerList(address[] memory signers) external onlyOwner {
        for (uint idx = 0; idx < signers.length; ++idx) {
            removeSigner(signers[idx]);
        }
    }

    function removeSigner(address signer) public onlyOwner {
        require(_signersMap[signer], "pe19");

        uint256 len = _signersList.length;
        uint8 idx = 0;
        for (; idx < len - 1; ++idx) {
            if (_signersList[idx] == signer) {
                break;
            }
        }
        _signersList[idx] = _signersList[len - 1];
        _signersList.pop();

        delete _signersMap[signer];

    }

    function getSignersList() public view returns (address[] memory) {
        return _signersList;
    }

    function createHash(ProductOb memory productOb) private view returns (bytes32) {
        uint256 chainId;
        assembly {
            chainId := chainid()
        }
        return keccak256(abi.encodePacked(productOb.requestHash, productOb.productType, productOb.price, productOb.timeStamp, chainId));
    }

    function isValidSignature(address signer, bytes32 hash, bytes memory signature) private view returns (bool) {
        return SignatureCheckerUpgradeable.isValidSignatureNow(signer, ECDSAUpgradeable.toEthSignedMessageHash(hash), signature);
    }

    modifier SignatureVerifier(ProductOb memory productOb, bytes memory signature, address signer) {

        require(_signersMap[signer], "pe20");
        require((block.timestamp - productOb.timeStamp) < purchaseTimeWindow, "p22");
        bytes32 inputHash = createHash(productOb);
        require(isValidSignature(signer, inputHash, signature), "pe21");
        _;
    }

}