pragma solidity 0.8.6;

import "./SwashSwapBaseUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";

contract SwashSComputeUpgradable is SwashSwapBaseUpgradeable {

    event DataProductPurchased(ProductOb productOb, bytes signature, address signer, address sender);

    address[] public dataProductPurchasedUsersList;
    mapping(address => DataProduct[]) public dataProductPurchasedUsersMap;

    constructor(){}

    function initialize(
        address _swashTokenAddress,
        address _baseTokenAddress,
        address[] memory _swashToBaseTokenPath,
        uint _purchaseTimeWindow
    ) public initializer {
        baseInitialize(_swashTokenAddress, _baseTokenAddress, _swashToBaseTokenPath, _purchaseTimeWindow);
    }

    //****************************************** UserLicense ***********************************************************

    function buyDataProductWithUniswapErc20(ProductOb memory productOb,
        bytes memory signature, address signer, string memory from_token_name, address[] calldata path) public {

        TokenStruct memory tokenOb = getTokenOb(from_token_name);

        require(tokenOb.tokenAddress != address(0) || tokenOb.isNative, "pe11");

        uint swashNeeded = priceInSwash(productOb.price);

        changeTokenAndPurchaseDataProduct(productOb, SignerOb(signature, signer), tokenOb.tokenAddress, swashNeeded, path);
    }

    function changeTokenAndPurchaseDataProduct(ProductOb memory productOb, SignerOb memory signerOb,
        address from_token, uint256 swashNeeded, address[] calldata path) private
    SignatureVerifier(productOb, signerOb.signature, signerOb.signer) {


        {
            uint256 deadline = block.timestamp + timeWindow;
            uint256 minSwashNeeded = (1000 * swashNeeded - swashNeeded * SLIPPAGE) / 1000;
            uint tokenNeeded = from_token != address(swashTokenAddress) ? priceInSpecificToken(minSwashNeeded, path) : minSwashNeeded;
            if (from_token != address(0)) {
                IERC20(from_token).transferFrom(msg.sender, address(this), tokenNeeded);
            }

            if (from_token == address(0)) {
                IUniswapV2Router02(uniswapRouter).swapExactETHForTokens{value : tokenNeeded}(minSwashNeeded, path,
                    address(this), deadline)[1];
            }
            else if (from_token != address(swashTokenAddress)) {
                IERC20(from_token).approve(uniswapRouter, productOb.price);
                IUniswapV2Router02(uniswapRouter).swapExactTokensForTokens(tokenNeeded, minSwashNeeded, path,
                    address(this), deadline)[1];
            }
        }
        //purchaseDataProduct(requestHash, time, price, signature, signer, productType);
        {
            DataProduct[] memory dp = dataProductPurchasedUsersMap[msg.sender];
            for (uint i = 0; i < dp.length; ++i) {
                require(keccak256(abi.encodePacked(dp[i].requestHash)) != keccak256(abi.encodePacked(productOb.requestHash)) ||
                    keccak256(abi.encodePacked(dp[i].timeStamp)) != keccak256(abi.encodePacked(productOb.timeStamp)), 'pe13');
            }
            dataProductPurchasedUsersMap[msg.sender].push(DataProduct(productOb.requestHash, productOb.price, signerOb.signature, productOb.timeStamp));

            emit DataProductPurchased(productOb, signerOb.signature, signerOb.signer, msg.sender);
        }
    }

    function isDataProductPurchased(ProductOb memory productOb, address sender) public view returns (bool)
    {
        DataProduct[] memory dp = dataProductPurchasedUsersMap[sender];
        for (uint i = 0; i < dp.length; ++i) {
            if (keccak256(abi.encodePacked(dp[i].requestHash)) == keccak256(abi.encodePacked(productOb.requestHash)) ||
                keccak256(abi.encodePacked(dp[i].timeStamp)) == keccak256(abi.encodePacked(productOb.timeStamp))) return true;
        }
        return false;
    }


    function convertPrice(uint256 price, string[] memory tokenListIn, address[][] calldata paths) external
    view returns (LicenseToBuy[] memory){

        string[] memory tokenNameList = tokenListIn;

        LicenseToBuy[] memory retList = new LicenseToBuy[](tokenNameList.length);
        require(
            tokenListIn.length == paths.length,
            "pe15"
        );

        for (uint idx = 0; idx < tokenListIn.length; ++idx) {

            LicenseToBuy memory licenseToBuy;
            licenseToBuy.tokenName = (tokenListIn[idx]);
            retList[idx] = licenseToBuy;
            address[] calldata path = paths[idx];
            uint256 priceInToken = getAmountsIn(priceInSwash(price), path);
            licenseToBuy.price = (priceInToken);
        }
        return retList;
    }

}

