        require(_tradingFree || _allowedTraders[from] || _allowedTraders[to] || _allowedTraders[_msgSender()], "NOT_ALLOWED_TO_TRADE");
        _controlCenter.beforeCardTransfer(_msgSender(), from, to, address(this), tokenId);
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
