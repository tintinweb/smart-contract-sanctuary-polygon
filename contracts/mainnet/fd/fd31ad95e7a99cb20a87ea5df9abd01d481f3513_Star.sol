        if (msg.data.length >= 20 && isTrustedForwarder(msg.sender)) {
            return msg.data[0:msg.data.length-20];
        } else {
            return msg.data;
        }
    }


    /***************************
     ******** Private **********
     ***************************/

    function _mint(address to, string calldata statId, uint8 rarity) private returns(uint256 nftId) {
        
        // Get nft id
        uint256 currentId = _tokenIdCounter.current();

        // Safe Mint
        _safeMint(to, currentId);

        // Generate Card
        Cards.Card storage card = _items[currentId];
        card.nftId = currentId;
        card.statId = statId;
        card.rarity = rarity;
        card.index = _statIdBalance[statId];
        _nftIdsByStatId[statId][_statIdBalance[statId]] = currentId;
        _statIdBalance[statId]++;
        _stock[rarity]++;

        emitEvent(to, currentId, statId, rarity);

        // Minted, so increment counter
        _tokenIdCounter.increment();


        return currentId;
    }

    function _removeNftAt(uint256 index, string memory statId) private {
        require(index < _statIdBalance[statId], "STATID_INDEX_OUT_OF_BOUNDS");
        _nftIdsByStatId[statId][index] = _nftIdsByStatId[statId][_statIdBalance[statId] - 1];
        _items[_nftIdsByStatId[statId][_statIdBalance[statId] - 1]].index = index;
        delete _nftIdsByStatId[statId][_statIdBalance[statId] - 1];
    }

    function emitEvent(address to, uint256 nftId, string calldata statId, uint8 rarity) internal virtual;

}


