        if (cardsCount == 0) {
            // Return an empty array
            return new Cards.Card[](0);
        } else {
            Cards.Card[] memory result = new Cards.Card[](cardsCount);
            uint256 resultIndex = 0;
            uint256 cardId;

            for (cardId = 0; cardId < cardsCount; cardId++) {
                result[resultIndex] = _items[
                    tokenOfOwnerByIndex(_owner, cardId)
                ];
                resultIndex++;
            }

            return result;
        }
    }

    /// @param _owner The owner whose NFTs we are interested in.
    /// @dev This method MUST NEVER be called by smart contract code. It
    ///  returns a dynamic array, which is only supported for web3 calls, and
    ///  not contract-to-contract calls.
    function tokensOfOwner(address _owner) external view returns (uint256[] memory ownerTokens) {
        uint256 tokenCount = balanceOf(_owner);

        if (tokenCount == 0) {
            // Return an empty array
            return new uint256[](0);
        } else {
            uint256[] memory result = new uint256[](tokenCount);
            uint256 resultIndex = 0;
            uint256 tokenId;

            for (tokenId = 0; tokenId < tokenCount; tokenId++) {
                result[resultIndex] = tokenOfOwnerByIndex(_owner, tokenId);
                resultIndex++;
            }

            return result;
        }
    }
    
    function baseURI() external view returns (string memory) {
        return _baseUri;
    }


    /***************************
     ********* Public **********
     ***************************/

    function mint(address to, string calldata statId, uint8 rarity) public virtual onlyRole(MINTER_ROLE) returns(uint256 nftId) {
        return _mint(to, statId, rarity);
    }

    function tokenURI(uint256 tokenId) public view override(ERC721, ERC721URIStorage) returns (string memory) {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721, ERC721Enumerable, AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    /***************************
     ******** Internal *********
     ***************************/

    function initBaseUri() internal virtual;

    function _baseURI() internal view override returns (string memory) {
        return _baseUri;
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override(ERC721, ERC721Enumerable) {
