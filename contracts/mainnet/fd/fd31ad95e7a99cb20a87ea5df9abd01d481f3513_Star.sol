            return new Cards.Card[](0);
        } else {
            Cards.Card[] memory result = new Cards.Card[](cardsCount);
            uint256 resultIndex = 0;
            uint256 cardId;

            for (cardId = 0; cardId < cardsCount; cardId++) {
                result[resultIndex] = _items[
                    _nftIdsByStatId[_statId][cardId]
                ];
                resultIndex++;
            }

            return result;
        }
    }

    /// @param _owner The owner whose NFTs we are interested in.
