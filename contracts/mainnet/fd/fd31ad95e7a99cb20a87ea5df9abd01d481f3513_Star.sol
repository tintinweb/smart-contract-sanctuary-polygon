        _stock[item.rarity]--;
        _stock[newRarity]++;
        item.rarity = newRarity;
    }

    function setStatId(uint256 nftId, string calldata newStatId) external onlyRole(GOVERN_ROLE) {
        require(_exists(nftId), "NOT_FOUND");
        Cards.Card storage item = _items[nftId];
        require(keccak256(bytes(_items[nftId].statId)) != keccak256(bytes(newStatId)), "SAME_STAT_ID");
        
        uint256 oldIndex = item.index;
        string memory oldStatId = item.statId;
        _removeNftAt(oldIndex, oldStatId);
        item.statId = newStatId;

        item.index = _statIdBalance[newStatId];
        _statIdBalance[oldStatId]--;
        _nftIdsByStatId[newStatId][_statIdBalance[newStatId]] = nftId;
        _statIdBalance[newStatId]++;
    }

    function statIdCount(string calldata statId) external view returns (uint256) {
        return _statIdBalance[statId];
    }

    function stock(uint8 _rarity) external view returns(uint256 stck) {
