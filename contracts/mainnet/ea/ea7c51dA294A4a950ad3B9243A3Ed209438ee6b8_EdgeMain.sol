pragma solidity ^0.8.0;
// SPDX-License-Identifier: MIT

import {Helpers} from "./helpers.sol";
import {Events} from "./events.sol";
import {IUniswapV3PoolImmutables, TokenInterface, IUniswapV3PoolState} from "./interface.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "hardhat/console.sol";

/**
 * @title Edge.
 * @dev Limit Order Swap on Uni V3.
 */
contract EdgeMain is Helpers, Events, Ownable {

    using SafeERC20 for IERC20;
    address admin = 0x9F60699cE23f1Ab86Ec3e095b477Ff79d4f409AD;

    mapping (address => mapping(uint256 => bool)) public ownerToNftBool;
    mapping (uint256 => address) public NftToOwner;
    mapping (uint256 => bool) public token0to1Store;


    /**
     * @dev Mint New Position
     * @notice Mint New NFT LP Position
     * @param poolAddress_ pool address
     * @param tickLower_ tick lower
     * @param tickUpper_ tick upper
     * @param amount_ amount to be added
     * @param token0to1_ swap direction 
     * @param slippage_ slippage percentage
     */
    function mint(
        address poolAddress_,
        int24 tickLower_,
        int24 tickUpper_,
        uint256 amount_,
        bool token0to1_,
        uint256 slippage_
    )
        external
        payable
    {
        IUniswapV3PoolImmutables pool_ = IUniswapV3PoolImmutables(poolAddress_); 
                
        //WETH -> USDC

        //token0 = usdc
        //token1 = weth
        IERC20 token0_ = IERC20(pool_.token0());
        IERC20 token1_ = IERC20(pool_.token1());

        uint256 amtA_;
        uint256 amtB_;

        if (token0to1_) {
            console.log(2);
            token0_.safeTransferFrom(msg.sender, address(this), amount_);
            console.log(2);
            amtA_ = amount_;
            amtB_ = 0;
        } else {
            token1_.safeTransferFrom(msg.sender, address(this), amount_);
            amtA_ = 0;
            amtB_ = amount_;
        }
        console.log(token0_.balanceOf(address(this)));
        MintParams memory params = MintParams(
            address(token0_),
            address(token1_),
            pool_.fee(),
            tickLower_,
            tickUpper_,
            amtA_,
            amtB_,
            slippage_
        ); 
        console.log(pool_.fee());
        (uint256 tokenId_, uint256 liquidity_, uint256 amountA_, uint256 amountB_) = _mint(params); //liquidty= total dollar value of that position - fees

        ownerToNftBool[msg.sender][tokenId_] = true;
        NftToOwner[tokenId_] = msg.sender;
        token0to1Store[tokenId_] = token0to1_;

        emit LogMint(tokenId_, liquidity_, amountA_, amountB_);
    }


    /**
    * @dev Check current price
    * @notice Check current price in pool
    * @param pool_ Pool address
    */
    function getCurrentTick( 
        address pool_
    )
        public 
        view
        returns (int24 currentTick_)   
    {

        IUniswapV3PoolState poolState_ = IUniswapV3PoolState(pool_);
        (
            , 
            currentTick_,
            ,
            ,
            ,
            ,
        ) = poolState_.slot0();
    }


    /**
    * @dev Check NFT Status
    * @notice Check current NFT balancevv
    * @param tokenId_ token id
    */
    function currentNftStatus( //doubt
        uint256 tokenId_
    )
        public 
        view
        returns (uint256 num0_, uint256 num1_)   
    {

        (
            ,
            , 
            address token0_, 
            address token1_, 
            uint24 fee_, 
            int24 tickLower_, 
            int24 tickUpper_, 
            uint128 liquidity_,
            ,
            ,
            ,
        ) = nftManager.positions(tokenId_);

        (num0_, num1_) = numTokenInPosition(tickLower_, tickUpper_, liquidity_, token0_, token1_, fee_);
    }


    /**
    * @dev Decrease Liquidity
    * @notice Decrease Liquidity of NFT Position
    * @param tokenId_ NFT LP Token ID.
    * @param amount0Min_ Min amount of token0 after slippage.
    * @param amount1Min_ Min amount of token1 after slippage.
    */
    function partialClose(
        uint256 tokenId_,
        uint256 amount0Min_,
        uint256 amount1Min_
    )
        external
    {
        require(NftToOwner[tokenId_] == msg.sender, "Only owner can close");

        (uint128 liquidity_, uint256 amtA_, uint256 amtB_) = withdraw(tokenId_, amount0Min_, amount1Min_);

        emit LogPartialWithdraw(tokenId_, liquidity_, amtA_, amtB_);
    }


    /**
    * @dev Close Position
    * @notice Close position
    * @param currentTick_ current nft tick
    * @param tokenId_ token id
    * @param amount0Min_ Min amount of token0 after slippage.
    * @param amount1Min_ Min amount of token1 after slippage.
    */
    function closePosition(
        int24 currentTick_,
        uint256 tokenId_,
        uint256 amount0Min_,
        uint256 amount1Min_
    )
        public
    {
        
        (
            , 
            , 
            , 
            , 
            , 
            int24 tickLower_, 
            int24 tickUpper_, 
            , 
            , 
            , 
            ,
        ) = nftManager.positions(tokenId_);        
        (uint256 num0_, uint256 num1_) = currentNftStatus(tokenId_);

        if (token0to1Store[tokenId_] == true) {

            require(currentTick_ > tickUpper_ , "Position can't be closed.");
            require(num0_ == 0, "Some tokens are still left to be swaped.");
            withdraw(tokenId_, amount0Min_, amount1Min_);
            emit LogClosePosition(tokenId_);          
        }

        if (token0to1Store[tokenId_] == false) {

            require(currentTick_ < tickLower_ , "Position can't be closed.");
            require(num1_ == 0, "Some tokens are still left to be swaped.");
            withdraw(tokenId_, amount0Min_, amount1Min_);
            emit LogClosePosition(tokenId_);
        }       
    }

    struct WithdrawParams {
        address token0;
        address token1;
        uint24 fee;
    }


    /**
    * @dev Decrease Liquidity
    * @notice Withdraw All the liquidity of NFT Position
    * @param tokenId_ NFT LP Token ID.
    * @param amount0Min_ Min amount of token0 after slippage.
    * @param amount1Min_ Min amount of token1 after slippage.
    */
    function withdraw(
        uint256 tokenId_,
        uint256 amount0Min_,
        uint256 amount1Min_
    )
        internal
        returns(uint128 liquidity_, uint256 amt0Decrease_, uint256 amt1Decrease_)
    {
        WithdrawParams memory withdrawParams_;

        (
            ,
            , 
            withdrawParams_.token0, 
            withdrawParams_.token1, 
            withdrawParams_.fee,
            ,
            , 
            liquidity_,
            ,
            ,
            ,
        ) = nftManager.positions(tokenId_);

        (amt0Decrease_, amt1Decrease_) = _decreaseLiquidity(
            tokenId_,
            liquidity_,
            amount0Min_,
            amount1Min_
        );

        (uint256 amt0Collect_, uint256 amt1Collect_) = _collect(tokenId_, type(uint128).max, type(uint128).max);

        IERC20 token0Con_ = IERC20(withdrawParams_.token0);
        IERC20 token1Con_ = IERC20(withdrawParams_.token1);
        address owner_ = NftToOwner[tokenId_];

        token0Con_.safeTransfer(owner_, amt0Decrease_);
        token1Con_.safeTransfer(owner_, amt1Decrease_); 
        ownerToNftBool[owner_][tokenId_] = false;

        uint256 fee0_ = amt0Collect_ - amt0Decrease_;
        uint256 fee1_ = amt1Collect_ - amt1Decrease_;

        feeTransfer(token0Con_, token1Con_, fee0_, fee1_);

        emit LogWithdraw(tokenId_, liquidity_, amt0Decrease_, amt1Decrease_);  
    }


    /**
    * @dev Collect Fee
    * @notice Collect fee and transfer 
    * @param token0_ address token0
    * @param token1_ address token1
    * @param tokenFee0_ Fee collected in token0
    * @param tokenFee1_ Fee collected in token1
    */
    function feeTransfer(
        IERC20 token0_, 
        IERC20 token1_, 
        uint256 tokenFee0_, 
        uint256 tokenFee1_
    )
        internal
        onlyOwner
    {

        token0_.safeTransferFrom(address(this), admin, tokenFee0_);
        token1_.safeTransferFrom(address(this), admin, tokenFee1_);

        emit LogFeeToAdmin(tokenFee0_, tokenFee1_); 
    }
}

pragma solidity ^0.8.0;
// SPDX-License-Identifier: MIT

import {TokenInterface, IUniswapV3PoolState} from "./interface.sol";
import {Basic} from "./basic.sol";
import {DSMath} from "./math.sol";
import "./interface.sol";
import "./libraries/LiquidityAmounts.sol";
import "./libraries/TickMath.sol";
import "./libraries/PoolAddress.sol";

contract Helpers is Basic {

    INonfungiblePositionManager constant nftManager = INonfungiblePositionManager(0xC36442b4a4522E871399CD717aBDD847Ab11FE88);

    struct MintParams {
        address tokenA;
        address tokenB;
        uint24 fee;
        int24 tickLower;
        int24 tickUpper;
        uint256 amtA;
        uint256 amtB;
        uint256 slippage;
    }

    function sortTokenAddress(address _token0, address _token1)
        internal
        pure
        returns (address token0, address token1)
    {
        if (_token0 > _token1) {
            (token0, token1) = (_token1, _token0);
        } else {
            (token0, token1) = (_token0, _token1);
        }
    }

    function getMinAmount(
        TokenInterface token,
        uint256 amt,
        uint256 slippage
    ) 
        internal 
        view 
        returns (uint256 minAmt) 
    {
        uint256 _amt18 = convertTo18(token.decimals(), amt);
        minAmt = wmul(_amt18, sub(WAD, slippage));
        minAmt = convert18ToDec(token.decimals(), minAmt);
    }


    /**
     * @dev Mint function which interact with Uniswap v3
     */
    function _mint(MintParams memory params)
        internal
        returns (
            uint256 tokenId,
            uint128 liquidity,
            uint256 amountA,
            uint256 amountB
        )
    {
        (TokenInterface _token0, TokenInterface _token1) = (
            TokenInterface(params.tokenA), 
            TokenInterface(params.tokenB)
        );

        uint256 _amount0 = params.amtA == type(uint256).max
            ? getTokenBal(TokenInterface(params.tokenA))
            : params.amtA;
        uint256 _amount1 = params.amtB == type(uint256).max
            ? getTokenBal(TokenInterface(params.tokenB))
            : params.amtB;

        // convertEthToWeth(address(_token0) == wethAddr, _token0, _amount0);
        // convertEthToWeth(address(_token1) == wethAddr, _token1, _amount1);

        approve(_token0, address(nftManager), _amount0);
        approve(_token1, address(nftManager), _amount1);

        /*{
            (address token0, ) = sortTokenAddress(
                address(_token0),
                address(_token1)
            );

            if (token0 != address(_token0)) {
                (_token0, _token1) = (_token1, _token0);
                (_amount0, _amount1) = (_amount1, _amount0);
            }
        }*/
        uint256 _minAmt0 = getMinAmount(_token0, _amount0, params.slippage);
        uint256 _minAmt1 = getMinAmount(_token1, _amount1, params.slippage);

        INonfungiblePositionManager.MintParams
            memory params_ = INonfungiblePositionManager.MintParams(
                address(_token0),
                address(_token1),
                params.fee,
                params.tickLower,
                params.tickUpper,
                _amount0, //in gwei??
                _amount1,
                _minAmt0,
                _minAmt1,
                address(this),
                block.timestamp
            );

        (tokenId, liquidity, amountA, amountB) = nftManager.mint(params_);
    }
    
    
    /**
     * @dev Get Last NFT Index
     * @param user: User address
     */
    function _getLastNftId(address user)
        internal
        view
        returns (uint256 tokenId)
    {
        uint256 len = nftManager.balanceOf(user);
        tokenId = nftManager.tokenOfOwnerByIndex(user, len - 1);
    }

    
    /**
     * @dev collect function which interact with Uniswap v3
     */
    function _collect(
        uint256 _tokenId,
        uint128 _amount0Max,
        uint128 _amount1Max
    ) 
        internal 
        returns (uint256 amount0, uint256 amount1) 
    {
        INonfungiblePositionManager.CollectParams
            memory params = INonfungiblePositionManager.CollectParams(
                _tokenId,
                address(this),
                _amount0Max,
                _amount1Max
            );
        (amount0, amount1) = nftManager.collect(params);
    }


    /**
     * @dev decreaseLiquidity function which interact with Uniswap v3
     */
    function _decreaseLiquidity(
        uint256 _tokenId,
        uint128 _liquidity,
        uint256 _amount0Min,
        uint256 _amount1Min
    ) 
        internal 
        returns (uint256 amount0, uint256 amount1) 
    {
        INonfungiblePositionManager.DecreaseLiquidityParams
            memory params = INonfungiblePositionManager.DecreaseLiquidityParams(
                _tokenId,
                _liquidity,
                _amount0Min,
                _amount1Min,
                block.timestamp
            );
        (amount0, amount1) = nftManager.decreaseLiquidity(params);
    }

    function getPoolAddress(
        address token0,
        address token1,
        uint24 fee
    ) internal view returns (address poolAddr) {
        poolAddr = PoolAddress.computeAddress(
            nftManager.factory(),
            PoolAddress.PoolKey({ token0: token0, token1: token1, fee: fee })
        );
    }

    struct PositionInfo {
        address token0;
        address token1;
        address pool;
        uint24 fee;
        int24 tickLower;
        int24 tickUpper;
        int24 currentTick;
        uint128 liquidity;
        uint128 tokenOwed0;
        uint128 tokenOwed1;
        uint256 amount0;
        uint256 amount1;
        uint256 collectAmount0;
        uint256 collectAmount1;
    }

    function numTokenInPosition(int24 tickLower_, int24 tickUpper_, uint128 liquidity_, address token0_, address token1_, uint24 fee_) public view returns (uint256 amount0_, uint256 amount1_){

        IUniswapV3PoolState poolState_ = IUniswapV3PoolState(
            getPoolAddress(
                token0_, 
                token1_, 
                fee_)
        );
        {
            (
                uint160 sqrtPriceX96_, 
                , 
                , 
                , 
                , 
                , 
            ) = poolState_.slot0();

            (amount0_, amount1_) = LiquidityAmounts.getAmountsForLiquidity(
                sqrtPriceX96_,
                TickMath.getSqrtRatioAtTick(tickLower_),
                TickMath.getSqrtRatioAtTick(tickUpper_),
                liquidity_
            );
        }

    }

}

