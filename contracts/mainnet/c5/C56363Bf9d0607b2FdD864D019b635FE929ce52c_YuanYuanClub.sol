    }

    function statusSaleTime() public view returns (uint48) {
        if(block.timestamp >= _whiteSaleTime.beginTime && block.timestamp <= _whiteSaleTime.endTime){
            return 1;
        }
        if(block.timestamp > _whiteSaleTime.endTime){
            return 2;
        }
        return 0;
    }

    function available() public view returns (uint256) {
        uint256 diffWhite=whiteCount-whiteMinted;
        uint256 _available=maxSupply-totalSupply();
        if(diffWhite>=communityReserved){
            diffWhite=communityReserved;
        }
        if(_available>=diffWhite){
            _available=_available-diffWhite;
        }
        return _available;
    }

    function setWhiteDiscount(uint48 _discount) public onlyOwner {
        whiteDiscount = _discount;
    }

    function setMaxMint(uint48 _maxMint) public onlyOwner {
        maxMint = _maxMint;
    }

    function setWhitelistRoot(bytes32 _root) public onlyOwner {
        whitelistRoot = _root;
    }

    function verify(bytes32[] memory _proof,address _sender) public view returns (bool) {
        return MerkleProof.verify(_proof, whitelistRoot, keccak256(abi.encodePacked(_sender)));
    }

    function _mint(address to, string memory _tokenURI, uint96 royaltyFee) internal {
        _safeMint(to, currentTokenId);
        if (bytes(_tokenURI).length > 0) {
            _setTokenURI(currentTokenId, _tokenURI);
        }
        if (royaltyFee > 0) {
            _setTokenRoyalty(currentTokenId, to, royaltyFee);
        }
        currentTokenId++;
    }

    function _pay(uint256 tokenAmount,bytes32[] memory _proof) internal {
        uint256 _available=maxSupply-totalSupply();
        uint48 _statusSale=statusSaleTime();
        bool pass=false;
        if (verify(_proof,msg.sender)) {
            whiteMinted=whiteMinted+1;
            if(_statusSale>0){
                pass=true;
            }
        }else{
            if(_statusSale>1){
                pass=true;
            }
            _available= available();
        }
        require(pass, "time is not up");
        require(tokenAmount <= _available, "max supply reached");
        uint256 fee= mintFee(_proof,msg.sender,tokenAmount);
        if (feeToken == address(0)) {
            require(msg.value == fee || hasRole(MINTER_ROLE,_msgSender()), "insufficient fee");
        }else{
            if(!hasRole(MINTER_ROLE,_msgSender())){
                IERC20(feeToken).transferFrom(msg.sender, address(this), fee);
                emit Payed(msg.sender, feeToken, fee, block.timestamp);
            }
        }
    }

    function safeMint(
        address to,
        string calldata _tokenURI,
        uint96 royaltyFee,
        bytes32[] memory _proof
    ) external payable nonReentrant {
        uint48 _total=minted[msg.sender]+1;
        require(_total<=maxMint, "has reached the maxMint");
        _pay(1,_proof);
        _mint(to, _tokenURI, royaltyFee);
        minted[msg.sender]=_total;
    }

    function batchMint(
        address to,
        uint48 tokenAmount,
        uint96 royaltyFee,
        bytes32[] memory _proof
    ) external payable nonReentrant {
        uint48 _total=minted[msg.sender]+tokenAmount;
        require(_total<=maxMint, "has reached the maxMint");
        _pay(tokenAmount,_proof);
        for (uint256 i; i < tokenAmount; i++) {
            _mint(to,"", royaltyFee);
        }
        minted[msg.sender]=_total;
    }

    function mintFee(bytes32[] memory _proof,address _sender,uint256 _tokenAmount) public view returns (uint256) {

        uint48 _total=minted[_sender];
        uint256 fee=_MINT_FEE*_tokenAmount;
        if (verify(_proof,_sender) && _total==0) {
            fee=fee-whiteDiscount*_MINT_FEE/100;
        }
        return fee;
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override(ERC721, ERC721Enumerable, ERC721Pausable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721, ERC721Royalty, ERC721Enumerable, AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function setMaxSupply(uint48 _maxSupply) external onlyOwner {
        require(!initialized, "Contract instance has already been initialized");
        initialized = true;
        maxSupply = _maxSupply;
    }

    function setFeeToken(address token) external onlyOwner {
        feeToken = token;
    }

    function setBeneficiary(address addr) external onlyOwner {
        beneficiary = addr;
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage, ERC721Royalty) {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId) public view override(ERC721, ERC721URIStorage) returns (string memory) {
        require(_exists(tokenId));
        string memory base = _baseURI();
        if (bytes(base).length > 0) {
            return string(abi.encodePacked(base, tokenId.toString(), '.json'));
        }
        return super.tokenURI(tokenId);
    }

    function _baseURI() internal view override(ERC721) returns (string memory) {
        return baseURI;
    }

    function setURIs(string memory _URI) external onlyOwner {
        require(!initialized, "Contract instance has already been initialized");
        baseURI = _URI;
    }

    function getBalance() public view returns (uint256) {
        return address(this).balance;
    }

    function getTokenBalance(address token) public view returns (uint256) {
        return IERC20(token).balanceOf(address(this));
    }

    function withdrawToken(address token) public {
        uint256 value = getTokenBalance(token);
        IERC20(token).transfer(beneficiary, value);
    }

    function withdraw() public {
        uint256 balance = address(this).balance;
        require(balance > 0, 'Balance should be more then zero');
        payable(beneficiary).transfer(balance);
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function walletOfOwner(address _owner) public view returns (uint256[] memory) {
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory _tokenIds = new uint256[](ownerTokenCount);
        for (uint256 i; i < ownerTokenCount; i++) {
            _tokenIds[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return _tokenIds;
    }

    function setMintFee(uint256 _mint_fee) external onlyOwner {
        emit SetMintFee(_MINT_FEE, _mint_fee);
        _MINT_FEE = _mint_fee;
    }

    receive() external payable {}
    fallback() external payable {}
}