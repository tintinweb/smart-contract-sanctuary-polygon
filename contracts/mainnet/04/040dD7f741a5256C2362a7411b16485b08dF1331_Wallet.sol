        uint256 indexed value,
        uint256 indexed date
    );
    event TransferToken (
        address indexed to,
        address indexed tokenAddress,
        uint indexed tokenType,
        uint256 amount,
        uint256 timestamp
    );

    function transferERC1155(
        address recipient,
        uint256 tokenId,
        uint256 tokensAmount,
        address contractAddress
    ) external;

    function transferERC1155WithBaseFee(
        address recipient,
        uint256 tokenId,
        uint256 tokensAmount,
        address contractAddress,
        uint256 baseFee
    ) external;

    function transferERC721(
        address recipient,
        uint256 tokenId,
        address contractAddress
    ) external;

    function transferERC721WithBaseFee(
        address recipient,
        uint256 tokenId,
        address contractAddress,
        uint256 baseFee
    ) external;

    function transferERC20(
        address recipient,
        uint256 amount,
        address contractAddress
    ) external;

    function transferERC20WithBaseFee(
        address recipient,
        uint256 amount,
        address contractAddress,
        uint256 baseFee
    ) external;

    function transferETH(address recipient, uint256 _value)
        external
        returns (bytes memory);

    function transferETHWithBaseFee(
        address recipient,
        uint256 _value,
        uint256 baseFee
    ) external;

    function staticCall(address recipient, bytes memory transactionData)
        external
        view
        returns (bytes memory);

    function approveAndCall(
        address recipient,
        bytes calldata transactionData,
        uint256 erc20Amount,
        address erc20Address
    ) external payable returns (bytes memory);

    function approveAndCallWithBaseFee(
        address recipient,
        bytes calldata transactionData,
        uint256 erc20Amount,
        address erc20Address,
        uint256 baseFee
    ) external payable;

    function call(
        address recipient,
        bytes calldata transactionData,
        uint256 value
    ) external payable returns (bytes memory);

    function callWithBaseFee(
        address recipient,
        bytes calldata transactionData,
        uint256 value,
        uint256 baseFee
    ) external payable;
}


contract Wallet is IWallet, Ownable, ERC1155Holder, ERC2771, ERC721Holder {
    FeeManager public feeManager;
    bool private _isInit;

    modifier validNativeBalance(uint256 expectBalance) {
        require(
            address(this).balance >= expectBalance,
            "Incorrect native balance"
        );
        _;
    }

    function initForClone(
        address owner_,
        address feeManagerAddress_,
        address trustedForwarder_
    ) public {
        require(!_isInit, "Already init");
        _isInit = true;

        _transferOwnership(owner_);
        super.ERC2771InitForClone(trustedForwarder_);
        feeManager = FeeManager(feeManagerAddress_);
    }

    /**
     * @notice transferERC1155 function for transferring ERC1155 tokens without system commission.
     * @notice Function for sending erc1155 tokens
     * @param recipient Address
     * @param tokenId id of ERC1155 token
     * @param tokensAmount amount of ERC1155 tokens
     * @param contractAddress Address ERC1155 token contract
     */
    function transferERC1155(
        address recipient,
        uint256 tokenId,
        uint256 tokensAmount,
        address contractAddress
    ) public override onlyOwner {
        require(
            IERC1155(contractAddress).balanceOf(address(this), tokenId) >=
                tokensAmount,
            "Wrong ERC1155 balance"
        );

        IERC1155(contractAddress).safeTransferFrom(
            address(this),
            recipient,
            tokenId,
            tokensAmount,
            ""
        );

        emit TransferToken(recipient, contractAddress, uint(TokenType.ERC1155), tokensAmount, block.timestamp);
    }

    /**
     * @notice transferERC1155WithBaseFee function for transferring ERC1155 tokens including system commission.
     * @notice Function for sending erc1155 tokens with base fee by system.
     * @param recipient Address
     * @param tokenId id of ERC1155 token
     * @param tokensAmount amount of ERC1155 tokens
     * @param contractAddress Address ERC1155 token contract
     * @param baseFee Fee by system in {MATIC}
     */
    function transferERC1155WithBaseFee(
        address recipient,
        uint256 tokenId,
        uint256 tokensAmount,
        address contractAddress,
        uint256 baseFee
    ) public override onlyOwner validNativeBalance(baseFee) {
        transferERC1155(recipient, tokenId, tokensAmount, contractAddress);
        _sendBaseFeeNativeToSystem(baseFee);
    }

    /**
     * @notice transferERC721 function for transferring ERC721 tokens without system commission.
     * @notice Function for sending erc721 tokens without system commission.
     * @param recipient Address
     * @param tokenId id of ERC721 token
     * @param contractAddress Address ERC721 token contract
     */
    function transferERC721(
        address recipient,
        uint256 tokenId,
        address contractAddress
    ) public override onlyOwner {
        require(
            IERC721(contractAddress).ownerOf(tokenId) == address(this),
            "Wallet is not owner of this token"
        );

        IERC721(contractAddress).safeTransferFrom(
            address(this),
            recipient,
            tokenId
        );

        emit TransferToken(recipient, contractAddress, uint(TokenType.ERC721), tokenId, block.timestamp);
    }

    /**
     * @notice transferERC721WithBaseFee function for transferring ERC721 tokens including system commission.
     * @notice Function for sending erc721 tokens with base fee by system.
     * @param recipient Address
     * @param tokenId id of ERC721 token
     * @param contractAddress Address ERC721 token contract
     * @param baseFee Fee by system in {MATIC}
     */
    function transferERC721WithBaseFee(
        address recipient,
        uint256 tokenId,
        address contractAddress,
        uint256 baseFee
    ) public override onlyOwner validNativeBalance(baseFee) {
        transferERC721(recipient, tokenId, contractAddress);
        _sendBaseFeeNativeToSystem(baseFee);
    }

    /**
     * @notice transferERC20 function for transferring ERC20 tokens without system commission.
     * @notice Function for sending erc20 tokens without system commission.
     * @param recipient Address
     * @param amount of ERC20
     * @param contractAddress Address ERC20 token contract
     */
    function transferERC20(
        address recipient,
        uint256 amount,
        address contractAddress
    ) public override onlyOwner {
        require(amount > 0, "Amount must be more zero");

        uint256 fee = feeManager.getTransferFee(contractAddress, amount);

        require(
            IERC20(contractAddress).balanceOf(address(this)) >= amount + fee,
            "Incorrect ERC20 balance"
        );

        bool success = IERC20(contractAddress).transfer(recipient, amount);
        bool successTransferToSystem = IERC20(contractAddress).transfer(
            feeManager.systemAddress(),
            fee
        );

        require(success, "Transfer ERC20 is not succeed");
        require(
            successTransferToSystem,
            "Transfer ERC20 (fee percent) is not succeed"
        );

        emit TransferToken(recipient, contractAddress, uint(TokenType.ERC20), amount, block.timestamp);
    }

    /**
     * @notice transferERC20WithBaseFee function for transferring ERC20 tokens including commission.
     * @notice Function for sending erc20 tokens with base fee by system.
     * @param recipient Address
     * @param amount of ERC20
     * @param contractAddress Address ERC20 token contract
     * @param baseFee Fee by system in {ERC20-contractAddress}
     */
    function transferERC20WithBaseFee(
        address recipient,
        uint256 amount,
        address contractAddress,
        uint256 baseFee
    ) public override onlyOwner {
        require(
            IERC20(contractAddress).balanceOf(address(this)) >=
                baseFee + amount,
            "Incorrect ERC20 balance"
        );
        transferERC20(recipient, amount, contractAddress);
        _sendBaseFeeErc20ToSystem(contractAddress, baseFee);
    }

    /**
     * @notice transferETH function for transferring native currency without commission.
     * @notice Function that receives recipient,eth_value without system commission.
     * @param recipient Address
     * @param _value amount of ETH
     * @return Result transaction data.
     */
    function transferETH(address recipient, uint256 _value)
        public
        override
        onlyOwner
        returns (bytes memory)
    {
        require(_value > 0, "Value must be more zero");

        uint256 fee = feeManager.getTransferFee(address(0), _value);
        require(
            address(this).balance >= _value + fee,
            "Incorrect native balance"
        );

        (bool success, bytes memory data) = recipient.call{value: _value}("");
        (bool success2, ) = feeManager.systemAddress().call{value: fee}("");

        require(success, "Transfer ETH is not succeed");
        require(success2, "Transfer fee is not succeed");

        emit TransferETH(recipient, _value, block.timestamp);
        return data;
    }

    /**
     * @notice transferETHWithBaseFee function for transferring native currency including commission.
     * @notice Function that receives recipient,eth_value,base_fee including commission.
     * @param recipient Address
     * @param _value amount of ETH
     * @param baseFee Fee by system in {MATIC}
     */
    function transferETHWithBaseFee(
        address recipient,
        uint256 _value,
        uint256 baseFee
    ) public override onlyOwner validNativeBalance(baseFee + _value) {
        transferETH(recipient, _value);
        _sendBaseFeeNativeToSystem(baseFee);
    }

    /**
     * @notice Static call function for recipient address.
     * @notice Function that receives recipient and transactionData, this is public function.
     * @param recipient Address to which 'static call'
     * @param transactionData Transaction data
     * @return Result transaction data.
     */
    function staticCall(address recipient, bytes memory transactionData)
        public
        view
        override
        returns (bytes memory)
    {
        (bool success, bytes memory data) = recipient.staticcall(
            transactionData
        );
        require(success, "Static call transaction is not succeed");
        return data;
    }

    /**
     * @notice Call function for recipient address.
     * @notice Function that receives recipient and transactionData
     * @param recipient Address to which 'call'
     * @param transactionData Transaction data
     * @param value Value of native currency
     * @return Result transaction data.
     */
    function call(
        address recipient,
        bytes calldata transactionData,
        uint256 value
    ) public payable override onlyOwner returns (bytes memory) {
        require(
            feeManager.isAddressWhitelistedForCall(recipient),
            "Recipient address is not approved for call"
        );

        // withdraw fee if value more 0
        if (value > 0) {
            uint256 fee = feeManager.getSwapFee(address(0), value);
            require(
                address(this).balance >= value + fee,
                "Incorrect native balance"
            );

            (bool transferToSystemIsSuccess, ) = feeManager
                .systemAddress()
                .call{value: fee}("");
            require(transferToSystemIsSuccess, "Transfer fee is not succeed");
        }

        (bool callIsSuccess, bytes memory data) = recipient.call{value: value}(
            transactionData
        );

        require(callIsSuccess, "Transaction is not success");
        return data;
    }

    /**
     * @notice Call function for recipient address.
     * @notice Function that receives recipient and transactionData with base fee
     * @param recipient Address to which 'call'
     * @param transactionData Transaction data
     * @param value Value of native currency
     * @param baseFee Fee by system in {MATIC}
     */
    function callWithBaseFee(
        address recipient,
        bytes calldata transactionData,
        uint256 value,
        uint256 baseFee
    ) public payable override onlyOwner validNativeBalance(baseFee + value) {
        call(recipient, transactionData, value);
        _sendBaseFeeNativeToSystem(baseFee);
    }

    /**
     * @notice Approve and call function for recipient address.
     * @notice Function that receives recipient and transactionData
     * @param recipient Address to which 'call'
     * @param transactionData Transaction data
     * @param erc20Amount Amount of erc20
     * @param erc20Address Address erc20 currency
     * @return Result transaction data.
     */
    function approveAndCall(
        address recipient,
        bytes calldata transactionData,
        uint256 erc20Amount,
        address erc20Address
    ) public payable override onlyOwner returns (bytes memory) {
        uint256 fee = feeManager.getSwapFee(erc20Address, erc20Amount);
        require(
            IERC20(erc20Address).balanceOf(address(this)) >= erc20Amount + fee,
            "Incorrect ERC20 balance"
        );

        bool successTransferToSystem = IERC20(erc20Address).transfer(
            feeManager.systemAddress(),
            fee
        );
        require(
            successTransferToSystem,
            "Transfer erc20 to system is not success"
        );
        IERC20(erc20Address).approve(recipient, erc20Amount);
        return call(recipient, transactionData, 0);
    }

    /**
     * @notice Approve and call function for recipient address.
     * @notice Function that receives recipient and transactionData
     * @param recipient Address to which 'call'
     * @param transactionData Transaction data
     * @param erc20Amount Amount of erc20
     * @param erc20Address Address erc20 currency
     * @param baseFee Fee by system in {ERC20-contractAddress}
     */
    function approveAndCallWithBaseFee(
        address recipient,
        bytes calldata transactionData,
        uint256 erc20Amount,
        address erc20Address,
        uint256 baseFee
    ) public payable override onlyOwner {
       approveAndCall(recipient,transactionData,erc20Amount,erc20Address);
        _sendBaseFeeErc20ToSystem(erc20Address, baseFee);
    }

    function _sendBaseFeeErc20ToSystem(
        address _contractErc20Address,
        uint256 _baseFee
    ) private {
        bool successTransferToSystem = IERC20(_contractErc20Address).transfer(
            feeManager.systemAddress(),
            _baseFee
        );
        require(
            successTransferToSystem,
            "Transfer ERC20 (fee percent) is not succeed"
        );
    }

    function _sendBaseFeeNativeToSystem(uint256 _baseFee) private {
        (bool success, ) = feeManager.systemAddress().call{value: _baseFee}("");
        require(success, "Transfer base native fee is not succeed");
    }

    receive() external payable {
        emit DepositETH(_msgSender(), msg.value, block.timestamp);
    }

    fallback() external payable {
        revert("Not supported operation");
    }

    function _msgSender()
        internal
        view
        override(Context, ERC2771)
        returns (address)
    {
        return ERC2771._msgSender();
    }

    function _msgData()
        internal
        view
        override(Context, ERC2771)
        returns (bytes calldata)
    {
        return ERC2771._msgData();
    }
}

