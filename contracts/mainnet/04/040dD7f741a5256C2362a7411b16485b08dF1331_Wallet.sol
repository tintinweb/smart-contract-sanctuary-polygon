import "@openzeppelin/contracts/utils/Context.sol";
import "./Forwarder.sol";

abstract contract ERC2771 is Context {
    IForwarder public trustedForwarder;

    function ERC2771InitForClone(address forwarder) public {
        require(address(trustedForwarder) == address(0), "Already init");
        trustedForwarder = IForwarder(forwarder);
    }

    function _msgSender()
        internal
        view
        virtual
        override
        returns (address sender)
    {
        if (address(trustedForwarder) == msg.sender) {
            assembly {
                sender := shr(96, calldataload(sub(calldatasize(), 20)))
            }
        } else {
            return super._msgSender();
        }
    }

    function _msgData()
        internal
        view
        virtual
        override
        returns (bytes calldata)
    {
        if (address(trustedForwarder) == msg.sender) {
            return msg.data[:msg.data.length - 20];
        } else {
            return super._msgData();
        }
    }
}

