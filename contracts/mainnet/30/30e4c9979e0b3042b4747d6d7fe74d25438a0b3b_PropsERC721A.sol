    uint256[] memory tokensOwned = IERC721AQueryableUpgradeable(address(this))
      .tokensOfOwner(holder);

    erc20tokens = 0;

    // iterate over owned tokens
    for (uint256 i = 0; i < tokensOwned.length; i++) {
