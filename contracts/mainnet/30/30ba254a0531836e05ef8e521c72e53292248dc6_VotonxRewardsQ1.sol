pragma solidity ^0.8.0;

contract VotonxRewardsQ1 is ReentrancyGuard, Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    address public nftContractAddress;
    address public tokenContractAddress;
    uint256 public tokenAmountPerNFT;

    mapping(uint256 => bool) public claimedNFTs;

    event RewardsClaimed(address indexed recipient, uint256 amount);

    constructor(address _nftContractAddress, address _tokenContractAddress) {
        require(_nftContractAddress != address(0), "Invalid NFT contract address");
        require(_tokenContractAddress != address(0), "Invalid token contract address");

        nftContractAddress = _nftContractAddress;
        tokenContractAddress = _tokenContractAddress;
        tokenAmountPerNFT = 0;
    }

    function depositTokens(uint256 amount) external onlyOwner {
        require(amount > 0, "Deposit amount must be greater than zero");

        IERC20(tokenContractAddress).safeTransferFrom(msg.sender, address(this), amount);

        uint256 totalNFTs = IERC721Enumerable(nftContractAddress).totalSupply();
        require(totalNFTs > 0, "No NFTs found");

        tokenAmountPerNFT = amount.div(totalNFTs);
    }

    function claimReward(uint256 tokenId) external nonReentrant {
        address recipient = msg.sender;
        require(!claimedNFTs[tokenId], "Token already claimed");

        address tokenOwner = IERC721Enumerable(nftContractAddress).ownerOf(tokenId);
        require(tokenOwner == recipient, "Caller does not own the token");

        claimedNFTs[tokenId] = true;
        IERC20(tokenContractAddress).safeTransfer(recipient, tokenAmountPerNFT);

        emit RewardsClaimed(recipient, tokenAmountPerNFT);
    }

    function claimAllRewards() external nonReentrant {
        address recipient = msg.sender;
        uint256 totalNFTs = IERC721Enumerable(nftContractAddress).balanceOf(recipient);
        require(totalNFTs > 0, "No NFTs in the wallet");

        uint256 unclaimedRewards = 0;

        for (uint256 i = 0; i < totalNFTs; i++) {
            uint256 tokenId = IERC721Enumerable(nftContractAddress).tokenOfOwnerByIndex(recipient, i);
            if (!claimedNFTs[tokenId]) {
                claimedNFTs[tokenId] = true;
                unclaimedRewards = unclaimedRewards.add(tokenAmountPerNFT);
            }
        }

        require(unclaimedRewards > 0, "No rewards to claim");

        IERC20(tokenContractAddress).safeTransfer(recipient, unclaimedRewards);

        emit RewardsClaimed(recipient, unclaimedRewards);
    }

    function getClaimStatus(uint256 tokenId) public view returns (bool) {
        return claimedNFTs[tokenId];
    }

    function getPendingRewards(uint256 tokenId) public view returns (uint256) {
        if (claimedNFTs[tokenId]) {
            return 0;
        } else {
            return tokenAmountPerNFT;
        }
    }

    function getTotalPendingRewards() public view returns (uint256) {
        uint256 totalPendingRewards = 0;

        uint256 totalNFTs = IERC721Enumerable(nftContractAddress).totalSupply();
        for (uint256 i = 0; i < totalNFTs; i++) {
            uint256 tokenId = IERC721Enumerable(nftContractAddress).tokenByIndex(i);
            if (!claimedNFTs[tokenId]) {
                totalPendingRewards = totalPendingRewards.add(tokenAmountPerNFT);
            }
        }

        return totalPendingRewards;
    }

    function withdrawTokens(address to, uint256 amount) external onlyOwner {
        IERC20(tokenContractAddress).safeTransfer(to, amount);
    }

    function withdrawAll() external onlyOwner {
        uint256 balance = IERC20(tokenContractAddress).balanceOf(address(this));
        IERC20(tokenContractAddress).safeTransfer(msg.sender, balance);
    }
}