    function _PUBLIC_mint( address from, address to, uint256 num, uint256 value ) internal {
        uint256[INFO_MAX] memory arrInfo = PUBLIC_getInfo( to );

        require( arrInfo[INFO_SUSPENDED] == 0, "PUBLIC SALE: suspended" );
        require( arrInfo[INFO_START] == 0 || arrInfo[INFO_START] <= (block.timestamp+BLOCK_SEC_MARGIN), "PUBLIC SALE: not opend" );
        require( arrInfo[INFO_END] == 0 || (arrInfo[INFO_END]+BLOCK_SEC_MARGIN) > block.timestamp, "PUBLIC SALE: finished" );
        require( arrInfo[INFO_USER_MINTABLE] == 0 || arrInfo[INFO_USER_MINTABLE] >= (arrInfo[INFO_USER_MINTED]+num), "PUBLIC SALE: reached the limit" );

        _checkPayment( from, arrInfo[INFO_PRICE]*num, value );
        _PUBLIC_map_user_minted[to] += num;
        _mintTokens( to, num );
    }

    //--------------------------------------------------------
    // [internal] _checkWhitelisted
    //--------------------------------------------------------
    function _checkWhitelisted( address target, uint256 amount, bytes32[] calldata merkleProof, bytes32  merkleRoot ) internal pure returns (bool) {
        bytes32 node = keccak256( abi.encodePacked( target, amount ) );
        return( MerkleProof.verify( merkleProof, merkleRoot, node ) );
    }

    //--------------------------------------------------------
    // [internal] _checkPayment
    //--------------------------------------------------------
    function _checkPayment( address payer, uint256 price, uint256 payment ) internal {
        require( price <= payment, "insufficient value" );

        // refund if overpaymented
        if( price < payment ){
            uint256 change = payment - price;
            address payable target = payable( payer );
            Address.sendValue( target, change );
        }
    }

    //--------------------------------------------------------
    // [internal] _mintTokens
    //--------------------------------------------------------
    function _mintTokens( address to, uint256 num ) internal {
        require( _total_supply >= _token_reserved, "reservation not finished" );
        require( (_total_supply+num) <= _token_max, "exceeded the supply range" );
        require( _token_mint_limit == 0 || _token_mint_limit >= num, "reached mint limitation" );

        uint256 tokenId = _total_supply + TOKEN_ID_OFS;
        _total_supply += num;

        // mint
        for( uint256 i=0; i<num; i++ ){
            _mint( to, tokenId+i );
        }
    }

    //--------------------------------------------------------
    // [external/onlyOwnerOrManager] reserveTokens
    //--------------------------------------------------------
    function reserveTokens( uint256 num ) external onlyOwnerOrManager {
        require( (_total_supply+num) <= _token_reserved, "exceeded the reservation range" );

        address to = reservedReceiver();
        uint256 tokenId = _total_supply + TOKEN_ID_OFS;
        _total_supply += num;

        // mint
        for( uint256 i=0; i<num; i++ ){
            _mint( to, tokenId+i );
        }
    }

    //--------------------------------------------------------
    // [external] getUserInfo
    //--------------------------------------------------------
    function getUserInfo( address target, uint256 amountEarly, bytes32[] calldata merkleProofEarly, uint256 amountPrivate, bytes32[] calldata merkleProofPrivate ) external view returns (uint256[USER_INFO_MAX] memory) {
        uint256[USER_INFO_MAX] memory userInfo;
        uint256[INFO_MAX] memory info;

        // EARLY(whitelist)
        if( (_EARLY_end == 0 || _EARLY_end > (block.timestamp+BLOCK_SEC_MARGIN/2)) && _checkWhitelisted( target, amountEarly, merkleProofEarly, _EARLY_merkle_root ) ){
            userInfo[USER_INFO_SALE_TYPE] = 1;
            info = EARLY_getInfo( target, amountEarly, merkleProofEarly );
        }
        // PRIVATE(whitelist)
        else if( (_PRIVATE_end == 0 || _PRIVATE_end > (block.timestamp+BLOCK_SEC_MARGIN/2)) && _checkWhitelisted( target, amountPrivate, merkleProofPrivate, _PRIVATE_merkle_root ) ){
            userInfo[USER_INFO_SALE_TYPE] = 2;
            info = PRIVATE_getInfo( target, amountPrivate, merkleProofPrivate );
        }
        // PUBLIC
        else{
            userInfo[USER_INFO_SALE_TYPE] = 3;
            info = PUBLIC_getInfo( target );
        }

        for( uint256 i=0; i<INFO_MAX; i++ ){
            userInfo[i] = info[i];
        }

        userInfo[USER_INFO_TOTAL_SUPPLY] = _total_supply;
        userInfo[USER_INFO_TOKEN_MAX] = _token_max;
        userInfo[USER_INFO_MINT_LIMIT] = _token_mint_limit;

        return( userInfo );
    }

    //--------------------------------------------------------
    // [external] checkBalance
    //--------------------------------------------------------
    function checkBalance() external view returns (uint256) {
        return( address(this).balance );
    }

    //--------------------------------------------------------
    // [external/onlyOwnerOrManager] withdraw
    //--------------------------------------------------------
    function withdraw( uint256 amount ) external onlyOwnerOrManager {
        require( amount <= address(this).balance, "insufficient balance" );

        address payable target = payable( owner() );
        Address.sendValue( target, amount );
    }
