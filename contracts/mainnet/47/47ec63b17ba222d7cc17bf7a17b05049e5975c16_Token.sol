        _reserved_receiver = target;
    }

    //--------------------------------------------------------
    // [public] isAgent
    //--------------------------------------------------------
    function isAgent( address target ) public view returns (bool) {
        return( _map_agent[target] );
    }

    //--------------------------------------------------------
    // [external/onlyOwnerOrManager] setAgent
    //--------------------------------------------------------
    function setAgent( address target, bool flag ) external onlyOwnerOrManager {
        if( flag ){
            _map_agent[target] = true;
        }else{
            delete _map_agent[target];
        }
    }

    //--------------------------------------------------------
    // [external] get
    //--------------------------------------------------------
    function baseUriForHidden() external view returns (string memory) { return( _base_uri_for_hidden ); }
    function baseUriForRevealed() external view returns (string memory) { return( _base_uri_for_revealed ); }
    function tokenMax() external view returns (uint256) { return( _token_max ); }
    function tokenReserved() external view returns (uint256) { return( _token_reserved ); }
    function tokenMintLimit() external view returns (uint256) { return( _token_mint_limit ); }    
    function totalSupply() external view returns (uint256) { return( _total_supply ); }

    //--------------------------------------------------------
    // [external/onlyOwnerOrManager] set
    //--------------------------------------------------------
    function setBaseUriForHidden( string calldata uri ) external onlyOwnerOrManager { _base_uri_for_hidden = uri; }
    function setBaseUriForRevealed( string calldata uri ) external onlyOwnerOrManager { _base_uri_for_revealed = uri; }
    function setTokenMax( uint256 num ) external onlyOwnerOrManager { _token_max = num; }
    function setTokenReserved( uint256 num ) external onlyOwnerOrManager { _token_reserved = num; }
    function setTokenMintLimit( uint256 num ) external onlyOwnerOrManager { _token_mint_limit = num; }

    //--------------------------------------------------------
    // [public/override] tokenURI
    //--------------------------------------------------------
    function tokenURI( uint256 tokenId ) public view override returns (string memory) {
        require( _exists( tokenId ), "nonexistent token" );

        if( bytes(_base_uri_for_revealed).length > 0 ){
            return( string( abi.encodePacked( _base_uri_for_revealed, Strings.toString( tokenId ) ) ) );            
        }

        return( string( abi.encodePacked( _base_uri_for_hidden, Strings.toString( tokenId ) ) ) );
    }

    //++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    // [public] getInfo - EARLY(whitelist)
    //++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    function EARLY_getInfo( address target, uint256 amount, bytes32[] calldata merkleProof ) public view returns (uint256[INFO_MAX] memory) {
        uint256[INFO_MAX] memory arrRet;

        if( _EARLY_is_suspended ){ arrRet[INFO_SUSPENDED] = 1; }
        arrRet[INFO_START] = _EARLY_start;
        arrRet[INFO_END] = _EARLY_end;
        arrRet[INFO_PRICE] = _EARLY_price;
        if( _checkWhitelisted( target, amount, merkleProof, _EARLY_merkle_root ) ){
            arrRet[INFO_WHITELISTED] = 1;
            arrRet[INFO_USER_MINTABLE] = amount;
        }
        arrRet[INFO_USER_MINTED] = _EARLY_map_user_minted[target];

        return( arrRet );
    }

    //++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    // [external/onlyOwnerOrManager] write - EARLY(whitelist)
    //++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    function EARLY_suspend( bool flag ) external onlyOwnerOrManager { _EARLY_is_suspended = flag; }
    function EARLY_setStartEnd( uint256 start, uint256 end ) external onlyOwnerOrManager { _EARLY_start = start; _EARLY_end = end; }
    function EARLY_setPrice( uint256 price ) external onlyOwnerOrManager { _EARLY_price = price; }
    function EARLY_setMerkleRoot( bytes32 root ) external onlyOwnerOrManager { _EARLY_merkle_root = root; }

    //++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    // [external/payable/nonReentrant] mint - EARLY(whitelist)
    //++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    // 通常窓口
    function EARLY_mint( uint256 num, uint256 amount, bytes32[] calldata merkleProof ) external payable nonReentrant {
        _EARLY_mint( msg.sender, msg.sender, num, amount, merkleProof, msg.value );
    }

    // 代理窓口
    function EARLY_mintByAgent( address to, uint256 num, uint256 amount, bytes32[] calldata merkleProof ) external payable nonReentrant onlyAgent {
        _EARLY_mint( msg.sender, to, num, amount, merkleProof, msg.value );
    }

    // 実体
    function _EARLY_mint( address from, address to, uint256 num, uint256 amount, bytes32[] calldata merkleProof, uint256 value ) internal {
        uint256[INFO_MAX] memory arrInfo = EARLY_getInfo( to, amount, merkleProof );

        require( arrInfo[INFO_SUSPENDED] == 0, "EARLY SALE: suspended" );
        require( arrInfo[INFO_START] == 0 || arrInfo[INFO_START] <= (block.timestamp+BLOCK_SEC_MARGIN), "EARLY SALE: not opend" );
        require( arrInfo[INFO_END] == 0 || (arrInfo[INFO_END]+BLOCK_SEC_MARGIN) > block.timestamp, "EARLY SALE: finished" );
        require( arrInfo[INFO_WHITELISTED] == 1, "EARLY SALE: not whitelisted" );
        require( arrInfo[INFO_USER_MINTABLE] >= (arrInfo[INFO_USER_MINTED]+num), "EARLY SALE: reached the limit" );

        _checkPayment( from, arrInfo[INFO_PRICE]*num, value );
        _EARLY_map_user_minted[to] += num;
        _mintTokens( to, num );
    }

