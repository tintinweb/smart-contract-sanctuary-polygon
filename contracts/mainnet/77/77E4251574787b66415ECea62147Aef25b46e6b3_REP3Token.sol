    /**
     * @dev Initializes the contract by setting a `name` and a `symbol` to the token collection.
     */
    constructor() {}

    // PUBLIC FUNCTIONS ==========

    function initialize(
        string memory name_,
        string memory symbol_,
        address[] memory _approvers,
        address trustedForwarder_
    ) public override initializer {
        require(trustedForwarder_ != address(0), "Address zero check");
        _name = name_;
        _symbol = symbol_;
        _trustedForwarder = trustedForwarder_;
        _manager = _msgSender();
        uint256 approverLength = _approvers.length;
        for (uint256 i = 0; i < approverLength; i++) {
            // Approver address cannot be null.
            address approver = _approvers[i];
            require(
                approver != address(0) && approver != address(this),
                "Approver address invalid"
            );
            // No duplicate owners allowed.
            require(!isApprover(approver), "Approver already added");
            _addApprover(approver);
        }
        __VoucherDecoder_init("REP3Signer", "0.0.1");
        __Ownable_init();
    }

    // ERC712

