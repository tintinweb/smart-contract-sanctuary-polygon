    // MEMBERSHIP

    /*
     * @dev See {IREP3Token-claimMembership}.
     */
    function claimMembership(NFTVoucher calldata voucher, uint256 approvedIndex)
        public
    {
        require(voucher.data.length > 0, "Invalid voucher");
        require(
            voucher.to.length <= 25,
            "More than 25 addresses can not be approved in a single voucher"
        );
        require(
            voucher.to[approvedIndex] == _msgSender(),
            "Not allowed to mint this token"
        );
        require(voucher.to.length > 0, "Voucher is not valid for any address");

        string[] memory tokenUris = _stringToArray(voucher.tokenUris);
        require(
            voucher.data.length == tokenUris.length,
            "Invalid voucher. Token Uris not provided"
        );

        address signer = _verify(voucher);
        require(isApprover(signer), "Voucher is not signed by approver.");

        uint256 currentEnd = voucher.to.length;
        uint256 currentEndIndex = 0;
        if (voucher.end.length > 0) {
            currentEnd = voucher.end[0];
        }
        uint256 toIndex = approvedIndex;

        while (
            currentEnd <= toIndex && (currentEndIndex + 1) < voucher.end.length
        ) {
            currentEndIndex = currentEndIndex + 1;
            currentEnd = voucher.end[currentEndIndex];
        }
        if (currentEnd <= toIndex) {
            currentEndIndex = voucher.end.length;
        }

        uint16 levelCategory = _levelCategoryFromData(
            voucher.data[currentEndIndex]
        );
        require(
            !_memberTokenId[_msgSender()][levelCategory],
            "Voucher already claimed by the address"
        );

        uint256 tokenId = totalSupply();

        _tokenDetail[tokenId] = TokenDetail({
            type_: 0,
            ownerTokenId: tokenId,
            data: voucher.data[currentEndIndex]
        });

        _memberTokenId[_msgSender()][levelCategory] = true;

        uint8 level = _levelFromLevelCategory(levelCategory);
        uint8 category = _categoryFromLevelCategory(levelCategory);
        emit Claim(level, category, tokenId);

        _mint(_msgSender(), tokenId);
        _setTokenURI(tokenId, tokenUris[currentEndIndex]);
    }

    /**
     * @dev change uri wrt `tokenId`.
     * sender should be approver of a dao
     *
     * Requirements:
     *
     * - `tokenId` and `tokenURI` must be of a membership token.
     *
     * Emits a {MembershipTokenChange} event.
     */

    function membershipURIChange(uint256 tokenId, string memory tokenUri)
        public
        onlyApprover(_msgSender())
    {
        _updateMembershipUri(tokenId, tokenUri);
    }

    function bulkMembershipURIChange(
        uint256[] calldata tokenIds,
        string memory tokenUri
    ) public onlyApprover(_msgSender()) {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            _updateMembershipUri(tokenIds[i], tokenUri);
        }
    }

    /**
     * @dev checks if address has a membership badge of levelCategory.
     */
    function membershipExists(address account, uint16 levelCategory)
        public
        view
        returns (bool)
    {
        return _memberTokenId[account][levelCategory];
    }

    /**
     * @dev checks if address has a membership badge of levelCategory.
     */
    function membershipLevelExists(address account, uint8 level)
        public
        view
        returns (bool)
    {
        uint8 category = 1;
        uint16 levelCategory = (uint16(level) << 8) | category;
        return _memberTokenId[account][levelCategory];
    }

    function updateMembership(
        uint256 tokenId,
        uint256 data,
        string memory tokenUri
    ) public onlyApprover(_msgSender()) {
        _existsMembership(tokenId);

        uint16 oldLevelCategory = _levelCategoryFromData(
            _tokenDetail[tokenId].data
        );
        uint16 newLevelCategory = _levelCategoryFromData(data);
        if (oldLevelCategory != newLevelCategory) {
            _updateLevelCategory(tokenId, oldLevelCategory, newLevelCategory);
        }
        _tokenDetail[tokenId].data = data;
        _setTokenURI(tokenId, tokenUri);
        uint8 level = _levelFromLevelCategory(newLevelCategory);
        uint8 category = _categoryFromLevelCategory(newLevelCategory);
        emit Upgrade(level, category, tokenId);
    }

    function soulTransfer(TransferVoucher calldata voucher) public {
        uint256 tokenId = voucher.tokenId;
        address to = voucher.to;
        _existsMembership(tokenId);
        uint16 levelCategory = _levelCategoryFromData(
            _tokenDetail[tokenId].data
        );
        require(
            _owners[tokenId] == _msgSender(),
            "Not authorized to to transfer"
        );
        require(
            !_memberTokenId[to][levelCategory],
            "Transfer not allowed. Address holds already holds exact membership"
        );
        address signer = _verify(voucher);
        require(isApprover(signer), "Voucher is not signed by approver.");
        require(
            _isApprovedOrOwner(_msgSender(), tokenId),
            "ERC721: transfer caller is not owner nor approved"
        );

        _memberTokenId[_msgSender()][levelCategory] = false;
        _memberTokenId[to][levelCategory] = true;

        // chaning ownerships of associated Tokens

        for (uint256 i = 0; i < _associatedTokens[tokenId].length; i++) {
            _transfer(_msgSender(), to, _associatedTokens[tokenId][i]);
        }
        _safeTransfer(_msgSender(), to, tokenId, "");
    }

    function revokeMembership(uint256 tokenId)
        public
        onlyApprover(_msgSender())
    {
        _existsMembership(tokenId);
        _revokeMembership(tokenId);
    }

    // BADGE

    function claimBadge(
        BadgeVoucher calldata voucher,
        uint256 memberTokenId,
        uint256[] calldata approvedIndex
    ) public {
        string[] memory tokenUriList = _isBulkBadgeValid(
            voucher.memberTokenIds,
            voucher.type_,
            voucher.data,
            voucher.tokenUri
        );
        address signer = _verify(voucher);
        require(isApprover(signer), "Voucher is not signed by approver.");
        require(
            _owners[memberTokenId] == _msgSender(),
            "Not authorized to claim the voucher for the member token"
        );
        uint32 firstNonce = voucher.nonces[approvedIndex[0]];
        require(
            _memberLastVoucher[memberTokenId] < firstNonce,
            "Voucher is invalid for the member id"
        );
        for (uint256 i = 0; i < approvedIndex.length; i++) {
            uint256 index = approvedIndex[i];
            require(
                voucher.memberTokenIds[index] == memberTokenId,
                "Wrong badge selection"
            );
            require(
                voucher.nonces[index] == firstNonce,
                "Voucher is invalid for the member id"
            );
        }

        for (uint256 i = 0; i < approvedIndex.length; i++) {
            uint256 index = approvedIndex[i];
            _issueBadge(
                voucher.memberTokenIds[index],
                voucher.type_[index],
                voucher.data[index],
                tokenUriList[index]
            );
        }
        _memberLastVoucher[memberTokenId] = firstNonce;
    }

    function getVoucherNonce(uint256 memberTokenId)
        public
        view
        onlyApprover(_msgSender())
        returns (uint32)
    {
        return _memberLastVoucher[memberTokenId] + 1;
    }

    /**
     * @dev See {IREP3Token-issueBadge}.
     */
    function issueBadge(
        uint256 memberTokenId,
        uint8 type_,
        uint256 data,
        string memory tokenUri
    ) public onlyApprover(_msgSender()) {
        _isBadgeValid(memberTokenId, type_);
        _issueBadge(memberTokenId, type_, data, tokenUri);
    }

    function batchIssueBadge(
        uint256[] calldata memberTokenIds,
        uint8[] calldata type_,
        uint256[] calldata data,
        string calldata tokenUri
    ) public onlyApprover(_msgSender()) {
        string[] memory tokenUriList = _isBulkBadgeValid(
            memberTokenIds,
            type_,
            data,
            tokenUri
        );
        for (uint256 i = 0; i < memberTokenIds.length; i++) {
            _issueBadge(memberTokenIds[i], type_[i], data[i], tokenUriList[i]);
        }
    }

    // ADMIN

    function transferOwnership(address newOwner)
        public
        override(IREP3Token, OwnableUpgradeable)
        onlyOwner
    {
        require(
            newOwner != address(0),
            "Ownable: new owner is the zero address"
        );
        _transferOwnership(newOwner);
    }

    function migrate() public virtual override onlyManager {}

    // DATA

    function dataSlot(uint256 tokenId, uint8 slot) public view returns (uint8) {
        uint256 data = _tokenDetail[tokenId].data;
        return _dataSlot(data, slot);
    }

    function getType(uint256 tokenId) public view returns (uint8) {
        return _tokenDetail[tokenId].type_;
    }

    /**
     * @dev See {IERC721Metadata-tokenURI}.
     */
    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721URIStorage: URI query for nonexistent token"
        );

        string memory _tokenURI = _tokenURIs[tokenId];
        string memory base = _baseURI();

        // If there is no base URI, return the token URI.
        if (bytes(base).length == 0) {
            return _tokenURI;
        }
        // If both are set, concatenate the baseURI and tokenURI (via abi.encodePacked).
        if (bytes(_tokenURI).length > 0) {
            return string(abi.encodePacked(base, _tokenURI));
        }

        return _tokenURI__ERC721Metadata(tokenId);
    }

    // ERC 165

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override
        returns (bool)
    {
        return
            interfaceId == 0x01ffc9a7 || // ERC165 interface ID for ERC165.
            interfaceId == 0x80ac58cd || // ERC165 interface ID for ERC721.
            interfaceId == 0x5b5e139f; // ERC165 interface ID for ERC721Metadata
    }

    // INTERNAL FUNCTIONS ===============================

    // BADGES ================================

    function _isBadgeValid(uint256 memberTokenId, uint8 type_) internal view {
        _activeMembersip(memberTokenId);
        require(
            type_ != 0,
            "Member Token can not be issued. Use signed vouchers"
        );
    }

    function _isBulkBadgeValid(
        uint256[] calldata memberTokenIds,
        uint8[] calldata type_,
        uint256[] calldata data,
        string calldata tokenUri
    ) internal view returns (string[] memory) {
        uint256 length = memberTokenIds.length;
        require(length <= 25, "Can not issue more than 25 badges");
        require(type_.length == length, "Request invalid: type list");
        require(data.length == length, "Request invalid: data list");
        string[] memory tokenUriList = _stringToArray(tokenUri);
        require(tokenUriList.length == length, "Request invalid: uri list");

        for (uint256 i = 0; i < length; i++) {
            _isBadgeValid(memberTokenIds[i], type_[i]);
        }
        return tokenUriList;
    }

    function _issueBadge(
        uint256 memberTokenId,
        uint8 type_,
        uint256 data,
        string memory tokenUri
    ) internal {
        uint256 tokenId = totalSupply();
        address to = _owners[memberTokenId];
        _tokenDetail[tokenId] = TokenDetail({
            type_: type_,
            ownerTokenId: memberTokenId,
            data: data
        });
        _associatedTokens[memberTokenId].push(tokenId);

        emit Issue(memberTokenId, type_, tokenId);
        _mint(to, tokenId);
        _setTokenURI(tokenId, tokenUri);
    }

    // MEMBERSHIP =============================
    function _revokeMembership(uint256 tokenId) internal {}

    function _activeMembersip(uint256 tokenId) internal view returns (bool) {
        _existsMembership(tokenId);
        // uint16 levelCategory = _levelCategoryFromData(_tokenDetail[tokenId].data);
        require(
            _levelFromData(_tokenDetail[tokenId].data) != 0,
            "Membership is inactive"
        );
        return true;
    }

    function _existsMembership(uint256 tokenId) internal view returns (bool) {
        require(_exists(tokenId), "Member Token Id doesn't exist");
        require(
            _tokenDetail[tokenId].type_ == 0,
            "Token Id is not Membership Token"
        );
        return true;
    }

    function _updateMembershipUri(uint256 tokenId, string memory tokenUri)
        internal
    {
        _existsMembership(tokenId);
        _setTokenURI(tokenId, tokenUri);
        uint256 data = _tokenDetail[tokenId].data;
        uint16 levelCategory = _levelCategoryFromData(data);
        uint8 level = _levelFromLevelCategory(levelCategory);
        uint8 category = _categoryFromLevelCategory(levelCategory);
        emit MembershipTokenChange(tokenId, tokenUri, level, category);
    }

    // DATA MANIPULATION =========================

    function _updateLevelCategory(
        uint256 tokenId,
        uint16 oldLevelCategory,
        uint16 newLevelCategory
    ) internal {
        _memberTokenId[_owners[tokenId]][oldLevelCategory] = false;
        _memberTokenId[_owners[tokenId]][newLevelCategory] = true;
    }

    function _levelCategoryFromData(uint256 data)
        internal
        pure
        returns (uint16)
    {
        uint16 level = uint16(data);
        return level;
    }

    function _levelFromData(uint256 data) internal pure returns (uint8) {
        return _dataSlot(data, 1);
    }

    function _levelFromLevelCategory(uint16 levelCategory)
        internal
        pure
        returns (uint8)
    {
        uint8 level = uint8(levelCategory >> 8);
        return level;
    }

    function _categoryFromLevelCategory(uint16 levelCategory)
        internal
        pure
        returns (uint8)
    {
        uint8 category = uint8(levelCategory);
        return category;
    }

    function _dataSlot(uint256 data, uint8 slot) internal pure returns (uint8) {
        return uint8(data >> (slot * 8));
    }

    // TRANSFER ===============================

    /**
     * @dev Transfers `tokenId` from `from` to `to`.
     *  As opposed to {transferFrom}, this imposes no restrictions on msg.sender.
     *
     * Requirements:
     *
     * - `to` cannot be the zero address.
     * - `tokenId` token must be owned by `from`.
     *
     * Emits a {Transfer} event.
     */
    function _transfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual {
        require(
            REP3Token.ownerOf(tokenId) == from,
            "ERC721: transfer from incorrect owner"
        );
        require(to != address(0), "ERC721: transfer to the zero address");

        _beforeTokenTransfer(from, to, tokenId);

        // Clear approvals from the previous owner
        _approve(address(0), tokenId);

        _balances[from] -= 1;
        _balances[to] += 1;
        _owners[tokenId] = to;

        emit Transfer(from, to, tokenId);

        _afterTokenTransfer(from, to, tokenId);
    }

    /**
     * @dev Hook that is called before any token transfer. This includes minting
     * and burning.
     *
     * Calling conditions:
     *
     * - When `from` and `to` are both non-zero, ``from``'s `tokenId` will be
     * transferred to `to`.
     * - When `from` is zero, `tokenId` will be minted for `to`.
     * - When `to` is zero, ``from``'s `tokenId` will be burned.
     * - `from` and `to` are never both zero.
     *
     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual {}

    /**
     * @dev Hook that is called after any transfer of tokens. This includes
     * minting and burning.
     *
     * Calling conditions:
     *
     * - when `from` and `to` are both non-zero.
     * - `from` and `to` are never both zero.
     *
     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].
     */
    function _afterTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual {}

    /**
     * @dev Safely transfers `tokenId` token from `from` to `to`, checking first that contract recipients
     * are aware of the ERC721 protocol to prevent tokens from being forever locked.
     *
     * `_data` is additional data, it has no specified format and it is sent in call to `to`.
     *
     * This internal function is equivalent to {safeTransferFrom}, and can be used to e.g.
     * implement alternative mechanisms to perform token transfer, such as signature-based.
     *
     * Requirements:
     *
     * - `from` cannot be the zero address.
     * - `to` cannot be the zero address.
     * - `tokenId` token must exist and be owned by `from`.
     * - If `to` refers to a smart contract, it must implement {IERC721Receiver-onERC721Received}, which is called upon a safe transfer.
     *
     * Emits a {Transfer} event.
     */
    function _safeTransfer(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) internal virtual {
        _transfer(from, to, tokenId);
        require(
            _checkOnERC721Received(from, to, tokenId, _data),
            "ERC721: transfer to non ERC721Receiver implementer"
        );
    }

    // MINT ===============================

    /**
     * @dev Mints `tokenId` and transfers it to `to`.
     *
     * WARNING: Usage of this method is discouraged, use {_safeMint} whenever possible
     *
     * Requirements:
     *
     * - `tokenId` must not exist.
     * - `to` cannot be the zero address.
     *
     * Emits a {Transfer} event.
     */
    function _mint(address to, uint256 tokenId) internal virtual {
        require(to != address(0), "ERC721: mint to the zero address");
        require(!_exists(tokenId), "ERC721: token already minted");

        _beforeTokenTransfer(address(0), to, tokenId);

        unchecked {
            _totalSupply += 1;
            _balances[to] += 1;
        }
        _owners[tokenId] = to;

        emit Transfer(address(0), to, tokenId);

        _afterTokenTransfer(address(0), to, tokenId);
    }

    // APPROVE ==============================

    /**
     * @dev Approve `to` to operate on `tokenId`
     *
     * Emits a {Approval} event.
     */
    function _approve(address to, uint256 tokenId) internal virtual {
        _tokenApprovals[tokenId] = to;
        emit Approval(REP3Token.ownerOf(tokenId), to, tokenId);
    }

    /**
     * @dev Approve `operator` to operate on all of `owner` tokens
     *
     * Emits a {ApprovalForAll} event.
     */
    function _setApprovalForAll(
        address owner,
        address operator,
        bool approved
    ) internal virtual {
        require(owner != operator, "ERC721: approve to caller");
        _operatorApprovals[owner][operator] = approved;
        emit ApprovalForAll(owner, operator, approved);
    }

    /**
     * @dev Returns whether `spender` is allowed to manage `tokenId`.
     *
     * Requirements:
     *
     * - `tokenId` must exist.
     */
    function _isApprovedOrOwner(address spender, uint256 tokenId)
        internal
        view
        virtual
        returns (bool)
    {
        require(
            _exists(tokenId),
            "ERC721: operator query for nonexistent token"
        );
        address owner = REP3Token.ownerOf(tokenId);
        return (spender == owner ||
            isApprovedForAll(owner, spender) ||
            getApproved(tokenId) == spender);
    }

    // BURN ============================

    /**
     * @dev Destroys `tokenId`.
     * The approval is cleared when the token is burned.
     *
     * Requirements:
     *
     * - `tokenId` must exist.
     *
     * Emits a {Transfer} event.
     */
    function _burn__ERC721(uint256 tokenId) internal virtual {
        address owner = REP3Token.ownerOf(tokenId);

        _beforeTokenTransfer(owner, address(0), tokenId);

        // Clear approvals
        _approve(address(0), tokenId);

        _balances[owner] -= 1;
        delete _owners[tokenId];

        emit Transfer(owner, address(0), tokenId);

        _afterTokenTransfer(owner, address(0), tokenId);
    }

    /**
     * @dev Destroys `tokenId`.
     * The approval is cleared when the token is burned.
     *
     * Requirements:
     *
     * - `tokenId` must exist.
     *
     * Emits a {Transfer} event.
     */
    function _burn(uint256 tokenId) internal virtual {
        _burn__ERC721(tokenId);

        if (bytes(_tokenURIs[tokenId]).length != 0) {
            delete _tokenURIs[tokenId];
        }
    }

    // UTILS =======================

    /**
     * @dev Returns whether `tokenId` exists.
     *
     * Tokens can be managed by their owner or approved accounts via {approve} or {setApprovalForAll}.
     *
     * Tokens start existing when they are minted (`_mint`),
     * and stop existing when they are burned (`_burn`).
     */
    function _exists(uint256 tokenId) internal view virtual returns (bool) {
        return _owners[tokenId] != address(0);
    }

    /**
     * @dev Internal function to invoke {IERC721Receiver-onERC721Received} on a target address.
     * The call is not executed if the target address is not a contract.
     *
     * @param from address representing the previous owner of the given token ID
     * @param to target address that will receive the tokens
     * @param tokenId uint256 ID of the token to be transferred
     * @param _data bytes optional data to send along with the call
     * @return bool whether the call correctly returned the expected magic value
     */
    function _checkOnERC721Received(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) private returns (bool) {
