// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/Context.sol";

contract AccessManager is Context {
    event ApproverAdded(address indexed approver);
    event ApproverRemoved(address indexed approver);

    mapping(address => bool) private _approvers;
    address internal _manager;

    function addApprover(address approver) public onlyApprover(_msgSender()) {
        _addApprover(approver);
    }

    function _addApprover(address approver) internal {
        if (!_isApprover(approver)) {
            _approvers[approver] = true;
            emit ApproverAdded(approver);
        }
    }

    function removeApprover(address approver)
        public
        onlyApprover(_msgSender())
    {
        _removeApprover(approver);
    }

    function _removeApprover(address approver) internal {
        if (_isApprover(approver)) {
            _approvers[approver] = false;
            emit ApproverRemoved(approver);
        }
    }

    function changeApprover(
        address[] memory approverToAdd,
        address[] memory approversToRemove
    ) public onlyApprover(_msgSender()) {
        for (uint256 i = 0; i < approverToAdd.length; i++) {
            _addApprover(approverToAdd[i]);
        }
        for (uint256 i = 0; i < approversToRemove.length; i++) {
            _removeApprover(approversToRemove[i]);
        }
    }

    function _isApprover(address approver) internal view returns (bool) {
        return _approvers[approver];
    }

    modifier onlyApprover(address approver) {
        require(_isApprover(approver), "Not Approver");
        _;
    }

    modifier onlyManager() {
        require(_msgSender() == _manager, "Not manager");
        _;
    }
}

