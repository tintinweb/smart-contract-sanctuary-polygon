import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "./interfaces/IREP3Token.sol";
import "./utils/EIP712BaseUpgradeable.sol";

contract VoucherDecoder is EIP712BaseUpgradeable, IREP3TokenTypes {
    // Structs type-hashes

    bytes32 constant NFTVoucher_TYPEHASH =
        keccak256(
            "NFTVoucher(uint256[] data,uint8[] end,address[] to,string tokenUris)"
        );

    bytes32 constant TransferVoucher_TYPEHASH =
        keccak256("TransferVoucher(uint256 tokenId,address to)");

    bytes32 constant BadgeVoucher_TYPEHASH =
        keccak256(
            "BadgeVoucher(uint32 index,uint256[] memberTokenIds,uint8[] type_,string tokenUri,uint256[] data,uint32[] nonces)"
        );

    bytes32 constant EIP712DOMAIN_TYPEHASH =
        keccak256(
            "EIP712Domain(string name,string version,address verifyingContract,bytes32 salt)"
        );

    bytes32 public DOMAIN_SEPARATOR;

    // constructor(string memory name, string memory version) {}

    function __VoucherDecoder_init(
        string memory name,
        string memory version
    ) internal onlyInitializing {
        __EIP712_init_unchained(name, version);
    }

    /// @notice Returns a hash of the given NFTVoucher, prepared using EIP712 typed data hashing rules.
    /// @param voucher An NFTVoucher to hash.
    function _hash(
        NFTVoucher calldata voucher
    ) internal view returns (bytes32) {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        NFTVoucher_TYPEHASH,
                        keccak256(abi.encodePacked(voucher.data)),
                        keccak256(abi.encodePacked(voucher.end)),
                        keccak256(abi.encodePacked(voucher.to)),
                        keccak256(bytes(voucher.tokenUris))
                    )
                )
            );
    }

    /// @notice Returns a hash of the given TransferVoucher, prepared using EIP712 typed data hashing rules.
    /// @param voucher An TransferVoucher to hash.
    function _hash(
        TransferVoucher calldata voucher
    ) internal view returns (bytes32) {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        TransferVoucher_TYPEHASH,
                        voucher.tokenId,
                        voucher.to
                    )
                )
            );
    }

    /// @notice Returns a hash of the given BadgeVoucher, prepared using EIP712 typed data hashing rules.
    /// @param voucher An BadgeVoucher to hash.
    function _hash(
        BadgeVoucher calldata voucher
    ) internal view returns (bytes32) {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        BadgeVoucher_TYPEHASH,
                        voucher.index,
                        keccak256(abi.encodePacked(voucher.memberTokenIds)),
                        keccak256(abi.encodePacked(voucher.type_)),
                        keccak256(bytes(voucher.tokenUri)),
                        keccak256(abi.encodePacked(voucher.data)),
                        keccak256(abi.encodePacked(voucher.nonces))
                    )
                )
            );
    }

    /// @notice Verifies the signature for a given NFTVoucher, returning the address of the signer.
    /// @dev Will revert if the signature is invalid. Does not verify that the signer is authorized to mint NFTs.
    /// @param voucher An NFTVoucher describing an unminted NFT.
    function _verify(
        NFTVoucher calldata voucher
    ) internal view returns (address) {
        bytes32 digest = _hash(voucher);
        return ECDSA.recover(digest, voucher.signature);
    }

    /// @notice Verifies the signature for a given TransferVoucher, returning the address of the signer.
    /// @dev Will revert if the signature is invalid. Does not verify that the signer is authorize transfer.
    /// @param voucher An TransferVoucher describing transfer approval.
    function _verify(
        TransferVoucher calldata voucher
    ) internal view returns (address) {
        bytes32 digest = _hash(voucher);
        return ECDSA.recover(digest, voucher.signature);
    }

    /// @notice Verifies the signature for a given TransferVoucher, returning the address of the signer.
    /// @dev Will revert if the signature is invalid. Does not verify that the signer is authorize transfer.
    /// @param voucher An BadgeVoucher describing an unminted Badge to be associated with Membership token.
    function _verify(
        BadgeVoucher calldata voucher
    ) internal view returns (address) {
        bytes32 digest = _hash(voucher);
        return ECDSA.recover(digest, voucher.signature);
    }
}

