    uint8[]     keyTypes;
}

struct DataRecords {
    DataSchema  _schema;
    string[]    _data; // a one-dimensional array of length [_schema.size * <number of records> ]
}

struct ConsumedRecord {
    uint    certId;
    string  energyType;
    string  location;
    uint    amount;
}


struct SmallPayload {
        uint32      orderID;
