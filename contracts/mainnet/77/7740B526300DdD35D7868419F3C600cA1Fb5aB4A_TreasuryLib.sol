pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";




import "../interfaces/INFTServiceTypes.sol";

library TicketLibrary {
    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeMath for uint;


    struct TicketLib {
        address owner;
        mapping(address => bool) _whitelist;
        mapping(string => DataSchema) _dataSchemata;
        mapping(bytes32 => DataSchema) _schemaHashes;
    }

    /*********
     *
     * validate ticket types.
     *
     ***********/
  
    function removeWhitelist(TicketLib storage _ticketLib, address _address) 
        internal 
    {   
        _ticketLib._whitelist[_address] = false;
    }
    function addWhitelist(TicketLib storage _ticketLib, address _address) 
        internal  
    {
        _ticketLib._whitelist[_address] = true;
    }
    function inWhitelist(TicketLib storage _ticketLib, address _address)
        external
        view
        returns (bool)
    {
        return bool(_ticketLib._whitelist[_address]);
    }


    function isTicket(TicketLib storage, Ticket memory ticket) 
        public 
        pure
        returns (bool) 
    {
        return ((ticket.serviceDescriptor & IS_TICKET) == IS_TICKET);
    }
    function isCertificate(TicketLib storage , Ticket memory ticket) 
        public 
        pure
        returns (bool) 
    {
        return ((ticket.serviceDescriptor & IS_CERTIFICATE) == IS_CERTIFICATE);
    }
    function isCashVoucher(TicketLib storage, Ticket memory ticket)
        public
        pure
        returns (bool _isCashVoucher)
    {
        _isCashVoucher = ((ticket.serviceDescriptor & CASH_VOUCHER) ==
            CASH_VOUCHER);
        if (_isCashVoucher) {
            require((ticket.pricePerCredit == 0) && (ticket.credits == 0), "isCashVoucher requires ppC = credits = 0");
        }
    }
    function isCheckoutService(TicketLib storage , uint256 serviceDescriptor) 
        public
        pure
        returns(bool)
    {
        return( (serviceDescriptor & CHECKOUT_TICKET) == CHECKOUT_TICKET);
    }


    function extractSchemaFromData(TicketLib storage _t, DataRecords calldata _calldata)
        internal
        returns (bytes32)
    {
        DataSchema memory _schema;
        bytes32 schemaHash;

        _schema = _calldata._schema;

        _t._dataSchemata[_schema.name] = _schema;
        _t._schemaHashes[schemaHash] = _schema;

        // emit evtSchemaRegistered(_schema.name, _schema);
        return (schemaHash);
    }

    function verifyCredits(TicketLib storage, Ticket memory ticket, uint256 reduceCreditsBy) 
        internal
        pure
        returns (bool ret, string memory _msg) 
    {
        uint256 availableCredits = ticket.credits;
        ret = true;
        _msg = "";
        if (!(availableCredits >= reduceCreditsBy)) {
            _msg = "available credits ";
            _msg = string(
                abi.encodePacked(
                    _msg, 
                    Strings.toString(availableCredits), 
                    " insufficent for ", 
                    Strings.toString(reduceCreditsBy)
                )
            );
            ret = false;
        }
        return(ret, _msg);
    }
    function verifyCredits(TicketLib storage ticketLib, Ticket memory ticket)
        internal 
        pure
        returns (bool ret, string memory _msg)
    {
        _msg = "";
        ret = false;
        if (!isCashVoucher(ticketLib, ticket)) {
            if (ticket.credits == 0) {
                if (ticket.price != 0) {
                    ret = false;
                    _msg = "vrfy credits: credits=0 & price != 0";
                } else {
                    ret = true;
                }
            } else {
                // credits != 0
                if ((ticket.price == 0) && (ticket.pricePerCredit != 0)) {
                    ret = false;
                    _msg = "vrfy credits: price=0, credits!=0 && ppC != 0";
                } else {
                    ret = true;
                }
            }
            // ticket.pricePerCredit = ticket.price.div(serviceFee);
        } else {
            // IS cashVoucher()
            if (((ticket.credits != 0) || (ticket.pricePerCredit != 0))) {
                ret = false;
                _msg = "vrfy credits: isCashVoucher BUT: credits OR ppC != 0";
            }
            ret = true;
        }
        return (ret, _msg);
    }

    function updateCredits(TicketLib storage, Ticket memory ticket, uint256 ticketBalance) 
        internal  
        pure
        returns(Ticket memory)
    {
        ticket.pricePerCredit = ticketBalance.div(ticket.credits);

        string memory _msg = "ticketBalance=";
        _msg = string(
            abi.encodePacked(
                _msg,
                Strings.toString(ticketBalance),
                " <> ppC=",
                Strings.toString(ticket.pricePerCredit),
                " * credits=",
                Strings.toString(ticket.credits)
            )
        );
        // TODO WHITE: what will happen if credits is provided with 18 decimals?
        require(
            (ticketBalance.div(ticket.credits) == ticket.pricePerCredit),
            _msg
        );
        return(ticket);
    }
}

