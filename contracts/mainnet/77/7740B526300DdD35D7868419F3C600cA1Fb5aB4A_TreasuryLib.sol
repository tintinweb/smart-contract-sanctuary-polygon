// basic differentiator being Ticket or Certificate (of ownership)
// all tickets need lowest bit of first semi-byte set
// all certificates need lowest bit of first semi-byte set
// all checkin-checkout tickets need second-lowest bit of first-semi-byte set --> 0x03000000
// high bits for each byte or half.byte are categories, low bits are instances
uint256 constant IS_CERTIFICATE =    0x40000000<<224; // 2nd highest bit of CERTT-half-byte = 1 - cannot use highest bit?
uint256 constant IS_TICKET =         0x08000000<<224; // highest bit of ticket-halfbyte = 1
uint256 constant CHECKOUT_TICKET =   0x09000000<<224; // highest bit of ticket-halfbyte = 1 AND lowest bit = 1
uint256 constant CASH_VOUCHER =      0x0A000000<<224; // highest bit of ticket-halfbyte = 1 AND 2nd bit = 1

// company identifiers last 10 bbits, e.g. 1023 companies
uint256 constant NFTAVATAR =    0x00000202;
uint256 constant BLOXMOVE =     0x00000200; // top of 10 bits for company identifiers
uint256 constant NRVERSE =      0x00000001;
uint256 constant MITTWEIDA =    0x00000002;
uint256 constant EQUOTA =       0x00000003;

// Industrial Category - byte2
uint256 constant THG = 0x80800000; //  CERTIFICATE & highest bit of category half-byte = 1
uint256 constant REC = 0x80400000; //  CERTIFICATE & 2nd highest bit of category half-byte = 1

// Last byte is company identifier 1-255
uint256 constant NRVERSE_REC = 0x80800001; // CERTIFICATE & REC & 0x00000001
uint256 constant eQUOTA_THG = 0x80400003; // CERTIFICATE & THG & 0x00000003
uint256 constant MITTWEIDA_M4A = 0x09000002; // CHECKOUT_TICKET & MITTWEIDA
uint256 constant BLOXMOVE_CO = 0x09000200;
uint256 constant BLOXMOVE_CV = 0x0A000200;
uint256 constant BLOXMOVE_CI = 0x08000200;
uint256 constant BLOXMOVE_NG = 0x09000201;
uint256 constant DutchMaaS = 0x09000003;
uint256 constant TIER_MW = 0x09000004;

uint256 constant COMPANY_OFFSET = 0x400;


/***********************************************
 *
 * generic schematizable data payload
 * allows for customization between reseller and
 * service operator while keeping NFTicket agnostic
 *
 ***********************************************/

enum eDataType {
    _UNDEF,
    _UINT,
    _UINT256,
    _USTRING
}

/*
* TODO reconcile overlaps between Payload, BuyNFTicketParams and Ticket
*/

struct ValueShare {
    uint256 service;
    uint256 reseller;
    uint256 transaction;
}
struct TicketBalances {
    uint256 remainingTicketBalance; // _treasury.remainingTicketBalance[ticket.tokenID]
    uint256 serviceTicketPool;      // _treasury.serviceProviderAccount[ticket.serviceProvider].serviceTicketPool
    uint256 totalTicketPool;        // _treasury.totalTicketPool
}
struct Ticket {
    uint256 tokenID;
    address serviceProvider; // the index to the map where we keep info about serviceProviders
    uint256  serviceDescriptor;
    address issuedTo;
    uint256 certValue;
    uint    certValidFrom; // value can be redeemed after this time
    uint256 price;
    uint256 credits; // [7]
    uint256 pricePerCredit;
    uint256 serviceFee;
    uint256 resellerFee;
    uint256 transactionFee;
    string  tokenURI;
    address erc20Contract;
}

struct Payload {
    address recipient;
    DataSchema schema;
    string[] data;
    string[] serializedTicket;
    string uuid;
    uint256 price;
    uint256 timestamp;
}
struct TopUpPayload {
    uint32     orderID;
    uint256     ticketID;
    address[]   erc20TokenList; 
    uint256[]   erc20AmountList;
    uint256     creditsAdd;
}


/**** END TODO overlap */

enum CheckInMode {checkin, checkout, neither}

struct DataSchema {
    string      name;
    uint32      size;
