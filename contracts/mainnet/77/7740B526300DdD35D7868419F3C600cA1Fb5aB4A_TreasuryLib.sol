pragma solidity ^0.8.7;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";

import "hardhat/console.sol";
import "./libs/TicketLibrary.sol";
import "./interfaces/ITreasuryLib.sol";
import "./interfaces/INFTServiceTypes.sol";

contract TreasuryLib is Initializable, ITreasuryLib {

    struct RevenuePool {
        address erc20Contract;
        address serviceProviderWallet; // service provider's wallet
        uint256 serviceTicketPool; // total unclaimed serviceFees
        address resellerWallet; // reseller's wallet

        /***
        *
        * @notice serviceProviderPool (and all other pools) are now collectively managed 
        *   directly in _treasury.accountBalance[erc20].balanceByToken[address]
        *
        uint256 serviceProviderPool; 
        *
        * @notice we manage reseller funds by wallet rather than contract (for now)
        *   we do this through _treasury.accountBalance[erc20].resellerWallet]
        *
        uint256 resellerPool; 
        ***/

    }
    // TODO prepare for multi-token logic
    struct BalanceByToken {
        mapping(address => uint256) balanceByToken;
    }
    struct RevenuePoolByToken {
        string tokenTicker;
        address contractAddress;
        mapping(address => RevenuePool) revenuePool;
    }

    struct Treasury {
        // these values are initiated in NFTicket constructor{}
        address owner;
        address NFTreasury;
        address NFTicket;
        address ticketProcessor;
        address[] erc20sSupported;
        uint32 TRANSACTIONFEE; // promil, i.e. 1.5%
        uint32 ratioBASE;
        
        /*********
        *
        * @notice JOY - balances for any stakeholder, irrespective of role, directly by address
        *   this means we took the pools out of the RevenuePool structure 
        *
        *********/
        mapping(address => BalanceByToken) accountBalance;
        mapping(address => BalanceByToken) contractBalance; // by contract address
        mapping(address => BalanceByToken) transactionPool;

        // uint256 totalTicketPool; // total funds held in unutilized tickets
        mapping(address => uint256) totalTicketPool; // by contract address
        mapping(uint256 => uint256) remainingTicketBalance; // unutilized funds per ticketID
        // no ...ByToken needed because ticket.erc20Contract is immutable

        mapping(address => RevenuePool) serviceProviderAccount; 
        mapping(address => RevenuePoolByToken) serviceProviderAccountByToken;
        /*
         * TODO
         * only have 1:1 mapping between reseller and provider for now.
         * Need to extends this to m:n or at least
         * 1:n 1 reseller having several service provider
         */
        mapping(address => address) serviceProviderByWallet;
        mapping(address => uint256) serviceDescriptorByWallet;
        mapping(address => address) resellersServiceProvider;
        mapping(address => address) serviceProvidersReseller;
        
        /////////////
        /***
        *
        * @notice JOY tier-1 providers are identified by 
            companyDescriptor:contracts 
        *
        */
        mapping(uint256 => address)  serviceContractByDescriptor;
        /***
        *
        * @notice JOY tier-2 providers are identified by 
            serviceDescriptor:wallet
        * (as opposed to their contracts)
        *
        */
        mapping(uint256 => address)  serviceWalletByDescriptor; 
        /////////////

        mapping(address => uint256)  companyDescriptorByServiceContract;
        mapping(uint256 => Ticket)  ticketData;
    }
    Treasury _treasury;
    using TicketLibrary for TicketLibrary.TicketLib;
    TicketLibrary.TicketLib ticketLib;

    function initialize(
        address[] calldata _erc20sSuppported,
        uint32 transactionFee,
        uint32 ratioBase
    ) public initializer {
        _treasury.owner = msg.sender;
        _treasury.erc20sSupported = _erc20sSuppported;
        _treasury.TRANSACTIONFEE = transactionFee;
        _treasury.ratioBASE = ratioBase;
        // TODO multi-token struct PoolsByToken[uint256(erc20.BLXM)].contractBalance = _treasury.contractBalance;
    }
    function configure(
        address _nftreasury, 
        address _nfticket, 
        address _ticketProcessor, 
        address[] memory erc20s
    ) 
        public override 
    {
        require(( (msg.sender == _treasury.owner) || ( msg.sender == _treasury.NFTreasury )), "only owner");
        _treasury.NFTreasury = _nftreasury;
        _treasury.NFTicket = _nfticket;
        _treasury.ticketProcessor = _ticketProcessor;
        _treasury.erc20sSupported = erc20s;
    }


    modifier onlyPeers(address peer) {
        require(isPeer(peer), 
            "I only listen to Peers");
        _;
    }
    modifier onlyWhiteList(address sender) {
        require(ticketLib.inWhitelist(sender), "not white");
        _;
    }
    function isPeer(address peer) 
        internal
        view
        returns(bool _isPeer)
    {
        _isPeer = false;
        if(     (peer == _treasury.NFTreasury)
            ||  (peer == _treasury.NFTicket)
            ||  (peer == _treasury.owner)
            ||  (peer == _treasury.ticketProcessor)
        ) {
            _isPeer = true;
        }
        return(_isPeer);
    }

    function withDrawERC20(address owner, uint256 amount, address erc20Contract) 
        public override
        onlyPeers(msg.sender)
        returns(uint256)
    {
        require(_treasury.accountBalance[erc20Contract].balanceByToken[owner] >= amount, 
            "withDrawERC20: not enough balance");
        _treasury.accountBalance[erc20Contract].balanceByToken[owner] -= amount;
        return(amount);
    }
    
    function setTicketData(uint256 newTicketID, Ticket memory ticket)
        public override
        onlyPeers(msg.sender)

    {
        _treasury.ticketData[newTicketID] = ticket;
        // TODO emit TicketDataEvent(ticket.serviceDescriptor, ticket);
    }
    function getTicketData(uint256 ticketID)
        public override
        view
        returns (Ticket memory ticket)
    {
        ticket = _treasury.ticketData[ticketID];
        return (ticket);
    }

    /*** BALANCE and POOL FUNCTIONS *****
    *
    * @notice These functions set actual balances, allowances and update pools
    *
    **/
    function increaseTicketBalance(
        Ticket memory ticket, 
        uint256 increaseBalanceBy
    )
        public override
        onlyPeers(msg.sender)
    {
        _treasury.remainingTicketBalance[ticket.tokenID] += increaseBalanceBy;
        // TODO might not need this after all. Only writing no reading so far (?)
        _treasury.totalTicketPool[ticket.erc20Contract] += increaseBalanceBy;
    }
    function reduceTicketBalance(Ticket memory ticket,  uint256 reduceBalanceBy)
        public override
        onlyPeers(msg.sender)
    {
        _treasury.remainingTicketBalance[ticket.tokenID] -= reduceBalanceBy;
        _treasury.totalTicketPool[ticket.erc20Contract] -= reduceBalanceBy;
    }
    function consumeCredits(uint256 tokenID, uint256 creditsConsumed)
        public override
        onlyPeers(msg.sender)
        returns (uint256 remainingCredits)
    {
        _treasury.ticketData[tokenID].credits -= creditsConsumed;
        remainingCredits = _treasury.ticketData[tokenID].credits;
        return(remainingCredits);
    }

    function increaseServiceTicketPool(
        address serviceProvider, 
        uint256 serviceFee,
        address erc20Contract
    ) 
        public override
        onlyPeers(msg.sender)
        returns(uint256 newPoolSize)
    {
        _treasury.serviceProviderAccount[serviceProvider].serviceTicketPool += serviceFee; // A
        _treasury.serviceProviderAccountByToken[erc20Contract] // B
            .revenuePool[serviceProvider]
            .serviceTicketPool += serviceFee;
        return(        
            _treasury.serviceProviderAccountByToken[erc20Contract]
            .revenuePool[serviceProvider]
            .serviceTicketPool += serviceFee
        );
    }

    function creditServiceProvider(
        address serviceProvider, 
        uint256 serviceFee,
        address erc20Contract
    )
        public override
        onlyPeers(msg.sender)
        returns(uint256 approveToServiceProvider, address serviceProviderWallet) // HERE
    { 
            approveToServiceProvider = _creditServiceProvider(serviceProvider, serviceFee, erc20Contract);
            /*
             _treasury.serviceProviderAccount[serviceProvider].serviceProviderPool += serviceFee;
            approveToServiceProvider = _treasury.serviceProviderAccount[serviceProvider].serviceProviderPool;
            */
            return(approveToServiceProvider, serviceProvider);
    }
    function creditServiceProvider( // tier-2 as we only provide serviceDescriptor
        uint256 serviceDescriptor,
        uint256 serviceFee,
        address erc20Contract
    )
        public override
        onlyPeers(msg.sender)
        returns(uint256 approveToServiceProvider, address serviceProviderWallet)
    {
        serviceProviderWallet = _treasury.serviceWalletByDescriptor[serviceDescriptor]; // HERE
        require(serviceProviderWallet != address(0), "creditServiceProvider: no tier-2 wallet");
        approveToServiceProvider = _creditServiceProvider(serviceProviderWallet, serviceFee, erc20Contract);
        return(approveToServiceProvider, serviceProviderWallet);
    }
    function _creditServiceProvider(
        address serviceProvider, // contract for tier-1, wallet for tier-2
        uint256 serviceFee,
        address erc20Contract
    )
        internal
        returns(uint256 approveToServiceProvider)
    {
        _treasury.accountBalance[erc20Contract].balanceByToken[serviceProvider] += serviceFee; // B
        approveToServiceProvider = serviceFee;
        return(approveToServiceProvider);
    }
    function debitServiceProvider(address serviceProvider, uint256 debitServiceProviderPool, address erc20Contract)
        public override
        onlyPeers(msg.sender)
        returns(uint256 serviceProviderPool)
    {
        _treasury.accountBalance[erc20Contract].balanceByToken[serviceProvider] -= debitServiceProviderPool; // B
        serviceProviderPool = _treasury.accountBalance[erc20Contract].balanceByToken[serviceProvider];
        /* 
            _treasury.serviceProviderAccount[serviceProvider].serviceProviderPool
                -= debitServiceProviderPool;
            serviceProviderPool 
                = _treasury.serviceProviderAccount[serviceProvider].serviceProviderPool;
            return(serviceProviderPool);
        */
    }
    function creditReseller(
        address serviceProvider, 
        uint256 resellerFee, 
        address erc20Contract
    )
        public override
        onlyPeers(msg.sender)
        returns(uint256 approveToReseller, address resellerWallet)
    {
        address resellerContract;
        (resellerContract, resellerWallet) = getReseller(serviceProvider);
        if (resellerFee == 0) {return(0, resellerWallet);}
        require(resellerWallet != address(0), "updateTixPls: invalid reseller wallet");
        _treasury.accountBalance[erc20Contract].balanceByToken[resellerWallet] += resellerFee; // B
        approveToReseller = _treasury.accountBalance[erc20Contract].balanceByToken[resellerWallet];
        /*
            _treasury.serviceProviderAccount[serviceProvider].resellerPool 
                += resellerFee;
            approveToReseller = _treasury.serviceProviderAccount[serviceProvider].resellerPool;
        */
        return(approveToReseller, resellerWallet);
    }

    function debitTicket(Ticket memory ticket, uint256 valueDebited)
        public override
        onlyPeers(msg.sender)
        returns(TicketBalances memory ticketBalances)
    {
        /****
        *
        * @notice JOY debit 
            1. remainingTicketBalance[ticketID]
            2. serviceTicketPool[serviceProvider] A + B
            3. totalTicketPool
        *
        *****/
        _treasury.remainingTicketBalance[ticket.tokenID] -= valueDebited;

        _treasury.serviceProviderAccount[ticket.serviceProvider].serviceTicketPool -= valueDebited; // A

        _treasury.serviceProviderAccountByToken[ticket.erc20Contract] // B
            .revenuePool[ticket.serviceProvider]
            .serviceTicketPool -= valueDebited;

        _treasury.totalTicketPool[ticket.erc20Contract] -= valueDebited;
        /**** END debit TX */

        // build return value structure
        ticketBalances.remainingTicketBalance = _treasury.remainingTicketBalance[ticket.tokenID];
        ticketBalances.serviceTicketPool = 
            _treasury.serviceProviderAccountByToken[ticket.erc20Contract] // B
                .revenuePool[ticket.serviceProvider]
                .serviceTicketPool;
        ticketBalances.totalTicketPool = _treasury.totalTicketPool[ticket.erc20Contract];
        return(ticketBalances);
    }
    function creditTransactionPool(address owner, uint256 transactionFee, address erc20Contract)
        public override
    {
        _treasury.transactionPool[erc20Contract].balanceByToken[owner] += transactionFee; // B
    }


    /******  QUERY BALANCES AND POOLS *******/
    function getTransactionPoolSize(address erc20Contract)
        public override
        view
        returns (uint256 poolSize)
    {
        return (_treasury.transactionPool[erc20Contract].balanceByToken[_treasury.owner]);
    }
    function getResellerPoolSize(address serviceProvider, address erc20Contract)
        public override
        view
        returns (uint256 poolSize)
    {
        address resellerContract;
        address resellerWallet;
        (resellerContract, resellerWallet) = getReseller(serviceProvider);
        return(_treasury.accountBalance[erc20Contract].balanceByToken[resellerWallet]);
        // return (_treasury.serviceProviderAccount[serviceProvider].resellerPool);
    }
    function getTotalTicketPoolSize(address erc20Contract)
        public override
        view
        returns (uint256 poolSize)
    {
        return (_treasury.totalTicketPool[erc20Contract]);
    }
    function getServiceTicketPoolSize(address serviceProvider, address erc20Contract)
        public override
        view
        returns(uint256)
    {
        return(_treasury.serviceProviderAccountByToken[erc20Contract] // B
            .revenuePool[serviceProvider]
            .serviceTicketPool);
        /*
            return(_treasury.serviceProviderAccount[serviceProvider].serviceTicketPool);
        */
    }
    function getServiceProviderPoolSize(address serviceProvider, address erc20Contract)
        public override
        view
        returns (uint256 poolSize)
    {
        return (
            _treasury.accountBalance[erc20Contract].balanceByToken[serviceProvider] // B
            // _treasury.serviceProviderAccount[serviceProvider].serviceProviderPool
        );
    }
     function getTicketBalance(uint256 tokenID)
        public override
        view
        returns (uint256)
    {
        return (_treasury.remainingTicketBalance[tokenID]);
    }
    /*** END BALANCE and POOL FUNCTIONS *****/
    /*****
    *
    * @notice JOY this is the primary tier-1 registration of a service provider with his company Descriptor
    * @param serviceProvider : address of the smart contract
    * @param companyDescriptor : the 10 bit bitmask identifying the organisation
    * @param serviceProviderWallet : the wallet which can retrieve this SP's allowance
    * @param erc20Contract : what is the base ERC20 for that SP
    *
    *****/
    function registerServiceProvider(
        address serviceProvider,
        uint256 companyDescriptor,
        address serviceProviderWallet,
        address erc20Contract
    ) 
        public override
        returns (uint16 status) 
    {
        require( (isPeer(msg.sender) || ticketLib.inWhitelist(msg.sender)), "registerSP: only peers or whitelist" );
        status = 201;
        if (isServiceProvider(serviceProvider)) {
            // this provider is already registered
            // TODO CRUD?
            require(false, "serviceProvider is already registered");
        } 
            _treasury.serviceContractByDescriptor[companyDescriptor] = serviceProvider;
            _treasury.companyDescriptorByServiceContract[serviceProvider] = companyDescriptor;
            /*
            * the following two statements are "equivalent". One handles 'single-token' one the 'multi-token' world
            * i.e.
                _treasury.serviceProviderAccount[serviceProvider]
                 and
                _treasury.serviceProviderAccountByToken[erc20Contract].revenuePool[serviceProvider]
                both identify the revenue pool for that service provider
            */
            _treasury // A.1
                .serviceProviderAccount[serviceProvider]
                .serviceProviderWallet = serviceProviderWallet;
            _treasury // A.2
                .serviceProviderAccount[serviceProvider]
                .erc20Contract = erc20Contract;
            _treasury.serviceProviderAccountByToken[erc20Contract] // B.1
                .revenuePool[serviceProvider]
                .serviceProviderWallet = serviceProviderWallet;
            _treasury.serviceProviderAccountByToken[erc20Contract] // B.2
                .revenuePool[serviceProvider]
                .erc20Contract = erc20Contract;

            // this only works for tier-1
            _treasury.serviceProviderByWallet[serviceProviderWallet] = serviceProvider;

         return (status);
    }
    function registerServiceProvider(
        address serviceProvider,
        uint256 companyDescriptor,
        address serviceProviderWallet
    ) 
        public override 
        returns (uint16 status) 
    {
        require( (isPeer(msg.sender) || ticketLib.inWhitelist(msg.sender)), "registerSP: only peers or whitelist" );
        return(registerServiceProvider(
            serviceProvider,
            companyDescriptor,
            serviceProviderWallet,
            _treasury.erc20sSupported[0]
        ));
    }
   function registerService( 
        address serviceProvider, 
        uint256 serviceDescriptor
    ) 
        public override
    {
        require( (isPeer(msg.sender) || ticketLib.inWhitelist(msg.sender)), "registerService/2: only peers or whitelist" );
        uint256 impliedCompany = serviceDescriptor % COMPANY_OFFSET;
        require(isServiceProvider(serviceProvider), "not a registered SP");
        uint256 companyDescriptor = getCompanyDescriptor(serviceProvider);
        require(impliedCompany == companyDescriptor, "registerService: company ID not matching");

        _treasury.serviceContractByDescriptor[serviceDescriptor] = serviceProvider;
    }
    function registerService(
        address serviceProvider, 
        uint256 serviceDescriptor, 
        address wallet
    ) 
        public override
    {
        require( (isPeer(msg.sender) || ticketLib.inWhitelist(msg.sender)), "registerService/3: only peers or whitelist" );
        // this ties the serviceDescriptor to the tier-1 service provider smart contract
        registerService(serviceProvider, serviceDescriptor);
        _treasury.serviceWalletByDescriptor[serviceDescriptor] = wallet;
        _treasury.serviceDescriptorByWallet[wallet] = serviceDescriptor;
    }


    function registerServiceReseller(
        address serviceProvider,
        address reseller,
        address _resellerWallet
    ) 
        public override 
        returns (uint16 status) 
    {
        require( (isPeer(msg.sender) || ticketLib.inWhitelist(msg.sender)), "registerReseller: only peers or whitelist" );

        _treasury.serviceProvidersReseller[serviceProvider] = reseller;
        _treasury.resellersServiceProvider[reseller] = serviceProvider;
        _treasury // A
            .serviceProviderAccount[serviceProvider]
            .resellerWallet = _resellerWallet;
        address erc20Contract = _treasury
                .serviceProviderAccount[serviceProvider]
                .erc20Contract;
        _treasury.serviceProviderAccountByToken[erc20Contract] // B
                .revenuePool[serviceProvider]
                .resellerWallet = _resellerWallet;
        _treasury.accountBalance[erc20Contract].balanceByToken[_resellerWallet] = 0;
        /*
            _treasury.serviceProviderAccount[serviceProvider].resellerPool = 0;
        */
        return (200);
    }

     function registerResellerServiceProvider(
        address serviceProvider,
        address reseller,
        address resellerWallet
    ) 
        public override
        returns (uint16 status) 
    {
        require( (isPeer(msg.sender) || ticketLib.inWhitelist(msg.sender)), "registerResellerSP: only peers or whitelist" );
        _treasury.serviceProvidersReseller[serviceProvider] = reseller;
        _treasury.resellersServiceProvider[reseller] = serviceProvider;
        _treasury // A
            .serviceProviderAccount[serviceProvider]
            .resellerWallet = resellerWallet;
        address erc20Contract = _treasury
                .serviceProviderAccount[serviceProvider]
                .erc20Contract;
        _treasury.serviceProviderAccountByToken[erc20Contract] // B
            .revenuePool[serviceProvider]
            .resellerWallet = resellerWallet;
        _treasury.accountBalance[erc20Contract].balanceByToken[resellerWallet] = 0;
        /*
            _treasury.serviceProviderAccount[serviceProvider].resellerPool = 0;
        */
        return (200);
    }
    function getReseller(address serviceProvider)
        public override
        view
        returns (address resellerContract, address walletAddress)
    {
        resellerContract = _treasury.serviceProvidersReseller[serviceProvider];
        walletAddress = _treasury.serviceProviderAccount[serviceProvider].resellerWallet;
    }

    function getResellerWallet(address serviceProviderContract)
        public override
        view
        returns (address _wallet)
    {
       return (
           _treasury
                .serviceProviderAccount[serviceProviderContract]
                .resellerWallet
        );
    }

    function getServiceProvider(uint256 serviceDescriptor)
        public override
        view
        returns (address serviceContract, address serviceProviderWallet)
    {
        uint256 companyDescriptor = serviceDescriptor % COMPANY_OFFSET;
        serviceContract = _treasury.serviceContractByDescriptor[companyDescriptor];
        require(serviceContract != address(0), "undef SP");
        serviceProviderWallet = _treasury
            .serviceProviderAccount[serviceContract]
            .serviceProviderWallet;
        return(serviceContract, serviceProviderWallet);
    }
    function getServiceProviderByWallet(address wallet) 
        public override
        view
        returns (address)
    {
        return(_treasury.serviceProviderByWallet[wallet]);
    }
    function getServiceDescriptorByWallet(address wallet) 
        public override
        view
        returns (uint256)
    {
        return(_treasury.serviceDescriptorByWallet[wallet]);
    }

    function getServiceProviderWallet(address serviceProviderContract)
        public override
        view
        returns (address _wallet)
    {
        return (
            _treasury
                .serviceProviderAccount[serviceProviderContract]
                .serviceProviderWallet
        );
    }
    function getServiceProviderWallet(address serviceProviderContract, address erc20Contract)
        public 
        view
        returns (address _wallet)
    {
        return (
            _treasury.serviceProviderAccountByToken[erc20Contract]
                .revenuePool[serviceProviderContract]
                .serviceProviderWallet        
        );
    }


    function isServiceProvider(address _serviceProvider)
        public override
        view
        returns (bool ret)
    {
        ret = false;
        address wallet;
        if ( _treasury.companyDescriptorByServiceContract[_serviceProvider] != 0)  {
            ret = true;
        } else {
            wallet = _serviceProvider;
            _serviceProvider = getServiceProviderByWallet(wallet);
            if ( _treasury.companyDescriptorByServiceContract[_serviceProvider] != 0)  {
                ret = true;
            }
        }
        return (ret);
    }
    function isServiceProviderFor(address _serviceContract, uint256 serviceDescriptor)
        public override
        view
        returns (bool _isServiceProviderFor) 
    {
        _isServiceProviderFor = false;

        (address serviceContract, address serviceWallet) = getServiceProvider(serviceDescriptor);
        if ( (serviceContract == _serviceContract) || (serviceWallet == _serviceContract)) {
            _isServiceProviderFor = true;
        }
        return(_isServiceProviderFor);
    }
    function isReseller(address reseller)
        public override
        view
        returns(bool)
    {
        return(address(0) != _treasury.resellersServiceProvider[reseller]);
    }
    function isResellerFor(address reseller, address serviceProvider)
        public override
        view
        returns(bool)
    {
        return(reseller == _treasury.serviceProvidersReseller[serviceProvider]);
    }

    function getCompanyDescriptor(address serviceProvider)
        public override
        view
        returns (uint256 descriptor)
    {
        return _treasury.companyDescriptorByServiceContract[serviceProvider];
    }

    function addWhitelist(address _address) 
        public override 
    {
        if (msg.sender != _treasury.owner) {
            console.log("sender=%s != owner=%s", msg.sender, _treasury.owner);
        }
        require(msg.sender == _treasury.owner, "add: only Owner");
        ticketLib.addWhitelist(_address);
    }
    function removeWhitelist(address _address) 
        public override
    {   
        require(msg.sender == _treasury.owner, "remove: only Owner");
        ticketLib.removeWhitelist(_address);
    }
    function inWhitelist(address _address)
        external override
        view
        returns (bool)
    {
        return (ticketLib.inWhitelist(_address));
    }
}

