        // Ensure the sender is only the owner or contract itself.
        _onlyOwnerOrSelf();

        // Ensure the SeaDrop is allowed.
        _onlyAllowedSeaDrop(seaDropImpl);

        // Update the airDrop on SeaDrop.
        ISeaDrop(seaDropImpl).updateAirDrop(airDrop);
    }



