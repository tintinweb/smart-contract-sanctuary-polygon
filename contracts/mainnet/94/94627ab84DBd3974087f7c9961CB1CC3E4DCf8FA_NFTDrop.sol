            uint256 supplyClaimedAlready = salesPhases[nextPhaseId].supplyClaimed;
            uint256 _startTokenId;
            
            if(nextPhaseId == 0){
                _startTokenId = 0;
            } else {
               _startTokenId = startTokenId[nextPhaseId - 1] + salesPhases[nextPhaseId - 1].maxClaimableSupply;
            }
            if (totalIDLen > 0 && _phases[i].tokenIds.length == 0){
                revert("CA Err");
            }

            salesPhases[nextPhaseId] = _phases[i];
            startTokenId[nextPhaseId] = _startTokenId;
            amountToRandom[nextPhaseId] = amountToRandom[nextPhaseId] > 0 ?  amountToRandom[nextPhaseId] : _phases[i].maxClaimableSupply;
            salesPhases[nextPhaseId].supplyClaimed = supplyClaimedAlready;
        }

        /**
         *  Gas refunds (as much as possible)
         *
         *  If `_resetClaimEligibility == true`, we assign completely new UIDs to the sales
         *  phases in `_phases`. So, we delete sales phases with UID < `newStartIndex`.
         *
         *  If `_resetClaimEligibility == false`, and there are more existing sales phases
         *  than in `_phases`, we delete the existing sales phases that don't get replaced
         *  by the phases in `_phases`.
         */
        if (_resetClaimEligibility) {
            for (uint256 i = existingStartIndex; i < newStartIndex; i++) {
                delete salesPhases[i];
            }
        } else {
            if (existingPhaseCount > _salesPhaseCount) {
                for (uint256 i = _salesPhaseCount; i < existingPhaseCount; i++) {
                    uint256 nextPhaseId = newStartIndex + i;
                    delete salesPhases[nextPhaseId];
                }
            }
        }

        emit SalesPhasesUpdated(_phases, _resetClaimEligibility);
    }

    /// @dev Collection owner can set the specific address to whilte list like the address that used for fiat mint
    function setWhitelist (address _address, bool isWhitelist) public {
        verifyAdminRole();
        whitelist[_address] = isWhitelist;
    }   

    function randomPick(uint256 _salePhaseId) internal returns (uint256 id) {
        uint256 _randomInd = uint256(keccak256(abi.encodePacked( msg.sender, block.timestamp, blockhash(block.number-1)))) % amountToRandom[_salePhaseId];
        uint256 _randomTokenInd = rMintedIndPerSalephase[_salePhaseId][_randomInd];
        uint256 startTokenIdOfPhase = startTokenId[_salePhaseId];
        id = _randomTokenInd == 0 ? _randomInd + startTokenIdOfPhase : _randomTokenInd + startTokenIdOfPhase;

        uint256 _amountToRandom = amountToRandom[_salePhaseId] - 1;
        uint256 _rMintedIndPerSalephase = rMintedIndPerSalephase[_salePhaseId][_amountToRandom];
        rMintedIndPerSalephase[_salePhaseId][_randomInd] = _rMintedIndPerSalephase == 0 ? _amountToRandom : _rMintedIndPerSalephase;
        amountToRandom[_salePhaseId] = _amountToRandom;

    }
    /// @dev Checks a request to claim NFTs against the active sales phase's criteria.
    function verifyClaim(
        uint256 _salesPhaseId,
        address _claimer,
        uint256 _quantity,
        bytes32[] calldata proof,
        uint256 _quantityLimitPerWallet
    ) internal view returns (bool isOverride) {
        uint256 claimLimit = salesPhases[_salesPhaseId].quantityLimitPerWallet;
        bytes32 merkleRoot = salesPhases[_salesPhaseId].merkleRoot;

        if (merkleRoot != bytes32(0)) {
            (isOverride, ) = MerkleProof.verify(
                proof,
                merkleRoot,
                keccak256(abi.encodePacked(_claimer, _quantityLimitPerWallet))
            );
        }
        
        if (isOverride) {
            claimLimit = _quantityLimitPerWallet != 0
                ? _quantityLimitPerWallet
                : claimLimit;
        }

        if (_quantity == 0 || supplyClaimedByWallet[_salesPhaseId][_claimer] > claimLimit) {
            revert("!Qty");
        }

        if (salesPhases[_salesPhaseId].supplyClaimed + _quantity > salesPhases[_salesPhaseId].maxClaimableSupply) {
            revert("!MaxSupply");
        }

        if (salesPhases[_salesPhaseId].startTimestamp > block.timestamp) {
            revert("time yet");
        }

        if (salesPhases[_salesPhaseId].endTimestamp < block.timestamp) {
            revert("TimeOver");
        }
    }

    function phaseCountAndStartId() external view returns(uint256 currentStartId, uint256 count) {
        return (currentStartPhaseId, salesPhaseCount);
    }

    function getClaimPrice(uint256 _salesPhaseId) external view returns(uint256 price){
        price = salesPhases[_salesPhaseId].pricePerToken;
    }

    function getTokensForCategory(uint256 id) external view returns (uint256[] memory) {
