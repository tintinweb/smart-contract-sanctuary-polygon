            _tick, _amountIn, _tokenIn, _tokenOut
        );
        
        // amountOut = _amountOut / 10 ** IERC20(_tokenOut).decimals();
        amountOut = _amountOut / 10 ** 6;
    }




    // Tasks before loan is returned
    function receiveFlashLoan(
        IERC20[] memory tokens,
        uint256[] memory amounts,
        uint256[] memory feeAmounts,
        bytes memory 
    ) external override {

        // loan of wbnb is received

        /// @notice 1. UniswapV3 AAVE/LINK : token0/token1

        Token0.approve(address(uniV3Router), amounts[0]);

        // Retrieve price for amountOutMinimum
        getAmountOut(token0, amounts[0], token1);


        ISwapRouter.ExactInputSingleParams memory params1 =
            ISwapRouter.ExactInputSingleParams({
                tokenIn: token0,
                tokenOut: token1,
                fee: currentFee,
                recipient: address(this),
                deadline: block.timestamp,
                amountIn: amounts[0],
                amountOutMinimum: amountOut,
                sqrtPriceLimitX96: 0
            });
        // The call to `exactInputSingle` executes the swap.
        uint256 amountOut1 = uniV3Router.exactInputSingle(params1);

        /// @notice 2. UniswapV3 LINK/USDT : token1/token2

        Token1.approve(address(uniV3Router), amountOut1);

        // Retrieve price for amountOutMinimum
        getAmountOutStable(token1, amountOut1, token2);

        ISwapRouter.ExactInputSingleParams memory params2 =
            ISwapRouter.ExactInputSingleParams({
                tokenIn: token1,
                tokenOut: token2,
                fee: currentFee,
                recipient: address(this),
                deadline: block.timestamp,
                amountIn: amountOut1,
                amountOutMinimum: amountOut,
                sqrtPriceLimitX96: 0
            });
        // The call to `exactInputSingle` executes the swap.
        uint256 amountOut2 = uniV3Router.exactInputSingle(params2);

        /// @notice 3. UniswapV3 USDT/AAVE : token2/token0
        Token2.approve(address(uniV3Router), amountOut2);

        // Retrieve price for amountOutMinimum
        getAmountOut(token2, amountOut2, token0);

        ISwapRouter.ExactInputSingleParams memory params3 =
            ISwapRouter.ExactInputSingleParams({
                tokenIn: token2,
                tokenOut: token0,
                fee: currentFee,
                recipient: address(this),
                deadline: block.timestamp,
                amountIn: amountOut2,
                amountOutMinimum: amountOut,
                sqrtPriceLimitX96: 0
            });
        // The call to `exactInputSingle` executes the swap.
        uint256 amountOut3 = uniV3Router.exactInputSingle(params3);


        // repay loan
        console.log(feeAmounts[0]);
        console.log(amountOut3);
        tokens[0].transfer(vault, amounts[0]);
    }

    // Initiate BalancerV2 flash loan
    function makeFlashLoan(
        IERC20[] memory tokens, 
        uint256[] memory amounts,
        bytes memory userData
    ) external {
    //  IVault(vault).flashLoan(this, tokens, amounts, userData);
        IVault(vault).flashLoan(
            this,
            tokens,
            amounts,
            userData
        );
    }

    function getBalance(address _tokenAddress) external view returns (uint256) {
        return IERC20(_tokenAddress).balanceOf(address(this));
    }

    function withdraw(address _tokenAddress) external onlyOwner {
        IERC20 token = IERC20(_tokenAddress);
        token.transfer(msg.sender, token.balanceOf(address(this)));
    }

    receive() external payable {}
}

