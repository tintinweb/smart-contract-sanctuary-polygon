// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.9;

import "hardhat/console.sol";

import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

import "@thirdweb-dev/contracts/extension/PermissionsEnumerable.sol";
import "@thirdweb-dev/contracts/lib/CurrencyTransferLib.sol";
import "@thirdweb-dev/contracts/extension/Upgradeable.sol";
import "@thirdweb-dev/contracts/extension/Initializable.sol";

import "./IHashGame.sol";
import "./RRCNFT.sol";
import "./Member.sol" as MemberContract;

contract HashGameV2 is IHashGame, PermissionsEnumerable, Upgradeable, Initializable {
    using Counters for Counters.Counter;

    Counters.Counter private _insurancePackageCounter;
    bytes32 public constant MAINTAINER_ROLE = keccak256("MAINTAINER_ROLE");
    uint256 private constant MAX_BPS = 10_000;
    uint256 private constant SECONDS_IN_DAY = 86400;

    address public rrcNftAddress;
    address public usdtTokenAddress;
    address public vicoTokenAddress;
    address public memberContractAddress;
    bool public shouldCheckPlayerActivationStatus = true;
    bool public isBuyingInsurancePaused = false;
    bool public isJoinGamePaused = false;
    uint256 public vicoToUsdBps = 10_000; // equals to 1 by default, stores in basis point (BPS)

    RRCNFT rrcNftContract;
    MemberContract.Member memberContract;

    enum GameInsuranceType {
        SILVER,
        GOLD
    }

    struct GameInsurancePackage {
        uint256 id;
        string name;
        GameInsuranceType insuranceType;
        RRCNFT.NftType requiredNftType;
        uint256 duration;
        uint256 priceInVico;
        uint256 displayPrice; // price of (vicoToUsdBps * priceInVico) / MAX_BPS
    }

    struct PurchasedInsurance {
        uint256 insurancePackageId;
        uint256 purchasedDateTime;
        uint256 expiryDateTime;
        uint256 priceInVico;
        uint256 vicoToUsdBps;
        uint256 totalPrice;
    }

    struct GameRound {
        uint256 round;
        bool hasPlayed;
        bool isWon;
        string gameHash;
        uint256 createdAt;
        uint256 resultSubmittedAt;
    }

    struct DailyGameRecord {
        address player;
        uint256 gameRoundCount;
        bool isWon;
        uint256 rewardInVico;
        uint256 rewardInUsdt;
        GameRound[] gameRounds;
    }

    struct GameProfile {
        address player;
        uint256 lostCount;
        bool isFromPreviousContract; // flag to indicate if this profile is from previous contract on BNB chain
        bool hasLostInThisContract;
    }

    mapping(uint256 => GameInsurancePackage) public gameInsurancePackages;
    mapping(address => GameProfile) public userGameProfileMap;
    mapping(address => PurchasedInsurance) public userToPurchasedInsuranceMap;
    mapping(address => mapping(uint256 => DailyGameRecord)) public userDailyGameRecordMap;

    function initialize(
        address _rrcNftContractAddress,
        address _usdtTokenAddress,
        address _vicoTokenAddress,
        address _memberContractAddress
    ) external initializer {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(MAINTAINER_ROLE, msg.sender);

        usdtTokenAddress = _usdtTokenAddress;
        vicoTokenAddress = _vicoTokenAddress;

        rrcNftAddress = _rrcNftContractAddress;
        rrcNftContract = RRCNFT(_rrcNftContractAddress);

        memberContractAddress = _memberContractAddress;
        memberContract = MemberContract.Member(_memberContractAddress);

        _seedDefaultInsurancePackages();
    }

    function _seedDefaultInsurancePackages() internal {
        string[4] memory names = ["Silver 10 Days", "Silver 30 Days", "Gold 10 Day", "Gold 30 Days"];
        GameInsuranceType[4] memory types = [
            GameInsuranceType.SILVER,
            GameInsuranceType.SILVER,
            GameInsuranceType.GOLD,
            GameInsuranceType.GOLD
        ];
        RRCNFT.NftType[4] memory requiredNftTypes = [
            RRCNFT.NftType.SILVER,
            RRCNFT.NftType.SILVER,
            RRCNFT.NftType.GOLD,
            RRCNFT.NftType.GOLD
        ];
        uint24[4] memory durations = [10 days, 30 days, 10 days, 30 days];
        uint72[4] memory prices = [50 ether, 135 ether, 100 ether, 270 ether];

        for (uint256 i = 0; i < names.length; i++) {
            uint256 insurancePackageId = _insurancePackageCounter.current();
            gameInsurancePackages[insurancePackageId] = GameInsurancePackage({
                id: insurancePackageId,
                name: names[i],
                insuranceType: types[i],
                requiredNftType: requiredNftTypes[i],
                duration: durations[i],
                priceInVico: prices[i],
                displayPrice: prices[i]
            });
            _insurancePackageCounter.increment();
        }
    }

    function getInsurancePackagesCount() external view returns (uint256 count) {
        return _insurancePackageCounter.current();
    }

    receive() external payable {}

    function getVicoBalance() public view returns (uint256) {
        return IERC20(vicoTokenAddress).balanceOf(address(this));
    }

    function withdrawVico(uint256 amount) public onlyRole(DEFAULT_ADMIN_ROLE) {
        CurrencyTransferLib.transferCurrency(vicoTokenAddress, address(this), msg.sender, amount);
    }

    function getUsdtBalance() public view returns (uint256) {
        return IERC20(usdtTokenAddress).balanceOf(address(this));
    }

    function withdrawUsdt(uint256 amount) public onlyRole(DEFAULT_ADMIN_ROLE) {
        CurrencyTransferLib.transferCurrency(usdtTokenAddress, address(this), msg.sender, amount);
    }

    function _payoutUSDT(uint256 amount, address recipient) internal {
        CurrencyTransferLib.transferCurrency(usdtTokenAddress, address(this), recipient, amount);
    }

    function _payoutVICO(uint256 amount, address recipient) internal {
        CurrencyTransferLib.transferCurrency(vicoTokenAddress, address(this), recipient, amount);
    }

    function setVicoToUsdBps(uint256 _bps) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_bps > 0, "Rate must be larger than 0");
        vicoToUsdBps = _bps;

        for (uint256 index = 0; index < _insurancePackageCounter.current(); index++) {
            uint256 _insurancePriceInVico = gameInsurancePackages[index].priceInVico;
            gameInsurancePackages[index].displayPrice = (vicoToUsdBps * _insurancePriceInVico) / MAX_BPS;
        }

        emit VicoToUsdBpsUpdated(msg.sender, _bps);
    }

    function setShouldCheckPlayerActivationStatus(bool _flag) public onlyRole(DEFAULT_ADMIN_ROLE) {
        shouldCheckPlayerActivationStatus = _flag;
    }

    function pauseJoinGame() public onlyRole(DEFAULT_ADMIN_ROLE) {
        isJoinGamePaused = true;
        emit JoinGamePaused(msg.sender);
    }

    function unPauseJoinGame() public onlyRole(DEFAULT_ADMIN_ROLE) {
        isJoinGamePaused = false;
        emit JoinGameUnpaused(msg.sender);
    }

    function pauseBuyingInsurance() public onlyRole(DEFAULT_ADMIN_ROLE) {
        isBuyingInsurancePaused = true;
        emit BuyInsurancePaused(msg.sender);
    }

    function unPauseBuyingInsurance() public onlyRole(DEFAULT_ADMIN_ROLE) {
        isBuyingInsurancePaused = false;
        emit BuyInsuranceUnpaused(msg.sender);
    }

    /// @dev returns the number of days that have elapsed since that timestamp
    function daysSince(uint _timestamp) private view returns (uint) {
        uint _secondsSince = block.timestamp - _timestamp;
        uint _daysSince = _secondsSince / SECONDS_IN_DAY; // 86400 seconds in a day
        return _daysSince;
    }

    /// @dev returns the index of the daily game record
    function getDailyGameRecordIndex(uint256 _insurancePurchasedDateTime) private view returns (uint256) {
        uint256 _daysSince = daysSince(_insurancePurchasedDateTime);
        return _insurancePurchasedDateTime + (_daysSince * SECONDS_IN_DAY);
    }

    function buyInsurance(uint256 _insuranceId) public {
        require(!isBuyingInsurancePaused, "Buying insurance is paused");
        require(_insuranceId < _insurancePackageCounter.current(), "Invalid insurance id");
        require(msg.sender != address(0), "Invalid player address");

        GameInsurancePackage memory insurancePackage = gameInsurancePackages[_insuranceId];
        require(insurancePackage.id == _insuranceId, "Invalid insurance id");

        address player = msg.sender;

        require(
            rrcNftContract.rentalInfo(player).nftType == insurancePackage.requiredNftType &&
                rrcNftContract.rentalInfo(player).expires > block.timestamp,
            "Player does not have required NFT"
        );
        if (shouldCheckPlayerActivationStatus) {
            require(memberContract.hasValidActivationPass(player), "Player does not have valid activation pass");
        }
        require(!hasValidInsurance(player), "Player already has valid insurance");

        uint256 totalPrice = (vicoToUsdBps * insurancePackage.priceInVico) / MAX_BPS;
        CurrencyTransferLib.transferCurrency(vicoTokenAddress, player, address(this), totalPrice);

        userToPurchasedInsuranceMap[player] = PurchasedInsurance({
            insurancePackageId: insurancePackage.id,
            purchasedDateTime: block.timestamp,
            expiryDateTime: block.timestamp + insurancePackage.duration,
            priceInVico: insurancePackage.priceInVico,
            vicoToUsdBps: vicoToUsdBps,
            totalPrice: totalPrice
        });

        GameProfile storage gameProfile = userGameProfileMap[player];
        gameProfile.player = player;

        emit NewInsurance(block.timestamp, block.timestamp + insurancePackage.duration, player);
    }

    function joinGame() public {
        require(!isJoinGamePaused, "Join game is paused");
        require(msg.sender != address(0), "Invalid player address");

        address player = msg.sender;

        require(rrcNftContract.hasRent(player), "Player does not have required NFT");
        if (shouldCheckPlayerActivationStatus) {
            require(memberContract.hasValidActivationPass(player), "Player does not have valid activation pass");
        }
        require(hasValidInsurance(player), "Player does not valid insurance");
        require(!hasWonToday(player), "Player have already won");
        require(!hasPendingRound(player), "Processing previous game, please try again later");

        PurchasedInsurance memory purchasedInsurance = userToPurchasedInsuranceMap[player];

        GameRound memory newRound = GameRound({
            round: currentGameRound(player),
            hasPlayed: false,
            isWon: false,
            gameHash: "",
            createdAt: block.timestamp,
            resultSubmittedAt: 0
        });

        uint256 dailyGameRecordIndex = getDailyGameRecordIndex(purchasedInsurance.purchasedDateTime);
        userDailyGameRecordMap[player][dailyGameRecordIndex].player = player;
        userDailyGameRecordMap[player][dailyGameRecordIndex].gameRounds.push(newRound);
        userDailyGameRecordMap[player][dailyGameRecordIndex].gameRoundCount++;

        emit NewGameRound(newRound.round, newRound.hasPlayed, newRound.isWon, player);
    }

    function processGame(address _player, bool _result, bytes32 _blockHash) public onlyRole(MAINTAINER_ROLE) {
        require(_blockHash.length > 0, "Empty hash");

        PurchasedInsurance memory purchasedInsurance = userToPurchasedInsuranceMap[_player];
        uint256 dailyGameRecordIndex = getDailyGameRecordIndex(purchasedInsurance.purchasedDateTime);

        DailyGameRecord storage dailyGameRecord = userDailyGameRecordMap[_player][dailyGameRecordIndex];
        uint256 lastPlayedRoundIndex = dailyGameRecord.gameRoundCount - 1;

        GameRound storage lastPlayedRound = dailyGameRecord.gameRounds[lastPlayedRoundIndex];

        lastPlayedRound.hasPlayed = true;

        if (_result) {
            lastPlayedRound.isWon = true;
            dailyGameRecord.isWon = true;
        }
        lastPlayedRound.gameHash = Strings.toHexString(uint256(_blockHash));
        lastPlayedRound.resultSubmittedAt = block.timestamp;

        // todo: handle payout of new & existing players

        bool isSilverNftHolder = rrcNftContract.rentalInfo(_player).nftType == RRCNFT.NftType.SILVER;

        if (_result == true) {
            uint256 prizeInUsdt = getPrizeInUsdt(_player);
            _payoutUSDT(prizeInUsdt, _player);
            dailyGameRecord.rewardInUsdt += prizeInUsdt;
        } else {
            if (lastPlayedRound.round == 6 || lastPlayedRound.round == 10 || lastPlayedRound.round == 13) {
                uint256 prizeInVico = isSilverNftHolder ? 20 ether : 40 ether;
                _payoutVICO(prizeInVico, _player);
                dailyGameRecord.rewardInVico += prizeInVico;

                GameProfile storage gameProfile = userGameProfileMap[_player];
                gameProfile.lostCount++;
                gameProfile.hasLostInThisContract = true;
            }
        }

        emit ProcessedGameRound(
            lastPlayedRound.round,
            lastPlayedRound.hasPlayed,
            lastPlayedRound.isWon,
            lastPlayedRound.gameHash,
            lastPlayedRound.resultSubmittedAt,
            _player
        );
    }

    function getPrizeInUsdt(address _player) private view returns (uint256) {
        bool isSilverNftHolder = rrcNftContract.rentalInfo(_player).nftType == RRCNFT.NftType.SILVER;

        GameProfile memory gameProfile = userGameProfileMap[_player];
        bool isUserFromPreviousContract = gameProfile.isFromPreviousContract;

        if (!isUserFromPreviousContract || gameProfile.hasLostInThisContract) {
            return isSilverNftHolder ? 8 ether : 16 ether;
        }

        if (gameProfile.lostCount == 0) {
            return isSilverNftHolder ? 10 ether : 20 ether;
        }

        if (gameProfile.lostCount == 1) {
            return isSilverNftHolder ? 9 ether : 19 ether;
        }

        return isSilverNftHolder ? 8 ether : 16 ether;
    }

    function hasValidInsurance(address _player) public view returns (bool) {
        return userToPurchasedInsuranceMap[_player].expiryDateTime > block.timestamp;
    }

    function hasWonToday(address _player) public view returns (bool) {
        PurchasedInsurance memory purchasedInsurance = userToPurchasedInsuranceMap[_player];
        return userDailyGameRecordMap[_player][getDailyGameRecordIndex(purchasedInsurance.purchasedDateTime)].isWon;
    }

    /// @dev The zero value shows that the player has won or there is a pending round
    function currentGameRound(address _player) public view returns (uint256) {
        PurchasedInsurance memory purchasedInsurance = userToPurchasedInsuranceMap[_player];
        uint256 dailyGameRecordIndex = getDailyGameRecordIndex(purchasedInsurance.purchasedDateTime);

        DailyGameRecord memory dailyGameRecord = userDailyGameRecordMap[_player][dailyGameRecordIndex];
        uint256 currentRound = dailyGameRecord.gameRoundCount;

        if (dailyGameRecord.isWon) {
            return 0;
        }

        return currentRound + 1;
    }

    function hasPendingRound(address _player) internal view returns (bool) {
        PurchasedInsurance memory purchasedInsurance = userToPurchasedInsuranceMap[_player];
        DailyGameRecord memory dailyGameRecord = userDailyGameRecordMap[_player][
            getDailyGameRecordIndex(purchasedInsurance.purchasedDateTime)
        ];

        uint256 lastPlayedRoundIndex = dailyGameRecord.gameRoundCount > 0 ? dailyGameRecord.gameRoundCount - 1 : 0;

        if (
            dailyGameRecord.gameRoundCount > 0 &&
            dailyGameRecord.gameRounds[lastPlayedRoundIndex].resultSubmittedAt <= 0
        ) {
            return true;
        }

        return false;
    }

    function getDailyGameRecord(address _player, uint256 _timestamp) public view returns (DailyGameRecord memory) {
        PurchasedInsurance memory purchasedInsurance = userToPurchasedInsuranceMap[_player];

        require(_timestamp >= purchasedInsurance.purchasedDateTime, "Invalid timestamp");

        uint _secondsSince = _timestamp - purchasedInsurance.purchasedDateTime;
        uint _daysSince = _secondsSince / SECONDS_IN_DAY; // 86400 seconds in a day
        uint256 dailyGameRecordIndex = purchasedInsurance.purchasedDateTime + (_daysSince * SECONDS_IN_DAY);

        return userDailyGameRecordMap[_player][dailyGameRecordIndex];
    }

    function setPurchasedInsuranceExpiry(address _player, uint256 _expiryDateTime) public onlyRole(DEFAULT_ADMIN_ROLE) {
        userToPurchasedInsuranceMap[_player].expiryDateTime = _expiryDateTime;
    }

    function _authorizeUpgrade(address) internal view override onlyRole(DEFAULT_ADMIN_ROLE) {}

    function migrateUserLostCount(
        address[] calldata _users,
        uint256[] calldata _lostCounts
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_users.length == _lostCounts.length, "Input arrays must have same length");

        for (uint256 i = 0; i < _users.length; i++) {
            userGameProfileMap[_users[i]] = GameProfile({
                lostCount: _lostCounts[i],
                player: _users[i],
                isFromPreviousContract: true,
                hasLostInThisContract: false
            });
        }
    }

    function migratePurchasedInsurance(
        address[] memory _users,
        uint256[] memory _insuranceIds,
        uint256[] memory _purchasedDateTimes,
        uint256[] memory _expiryDateTimes,
        uint256[] memory _pricesInVico,
        uint256[] memory _vicoToUsdBps,
        uint256[] memory _totalPrices
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        for (uint256 i = 0; i < _users.length; i++) {
            userToPurchasedInsuranceMap[_users[i]] = PurchasedInsurance({
                insurancePackageId: _insuranceIds[i],
                purchasedDateTime: _purchasedDateTimes[i],
                expiryDateTime: _expiryDateTimes[i],
                priceInVico: _pricesInVico[i],
                vicoToUsdBps: _vicoToUsdBps[i],
                totalPrice: _totalPrices[i]
            });
        }
    }
}

