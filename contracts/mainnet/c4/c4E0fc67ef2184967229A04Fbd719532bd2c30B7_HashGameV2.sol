pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@thirdweb-dev/contracts/lib/CurrencyTransferLib.sol";
import "@thirdweb-dev/contracts/extension/PermissionsEnumerable.sol";

import "./RRCNFT.sol" as RRCNftContract;

contract Member is PermissionsEnumerable {
    using Counters for Counters.Counter;

    address public rrcNftAddress;
    address public vicoTokenAddress;
    uint256 public activationDuration = 10 days;

    mapping(uint256 => MemberPackage) public memberPackages;
    mapping(address => ActiveMember) public activeMembers;

    RRCNftContract.RRCNFT rrcNftContract;

    struct ActiveMember {
        uint256 packageId;
        uint256 expiryDateTime;
        address memberAddress;
    }

    struct MemberPackage {
        uint256 id;
        uint256 priceInVico;
        RRCNftContract.RRCNFT.NftType requiredNftType;
    }

    Counters.Counter private _memberPackageCounter;

    constructor(address _vicoTokenAddress, address _rrcNftContractAddress) {
        vicoTokenAddress = _vicoTokenAddress;
        rrcNftAddress = _rrcNftContractAddress;
        rrcNftContract = RRCNftContract.RRCNFT(_rrcNftContractAddress);

        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _seedDefaultMemberPackage();
    }

    function _seedDefaultMemberPackage() internal {
        uint64[2] memory prices = [5 ether, 10 ether];
        RRCNftContract.RRCNFT.NftType[2] memory requiredNftTypes = [
            RRCNftContract.RRCNFT.NftType.SILVER,
            RRCNftContract.RRCNFT.NftType.GOLD
        ];

        for (uint256 index = 0; index < prices.length; index++) {
            uint256 memberPackageId = _memberPackageCounter.current();
            memberPackages[memberPackageId] = MemberPackage({
                id: memberPackageId,
                priceInVico: prices[index],
                requiredNftType: requiredNftTypes[index]
            });
            _memberPackageCounter.increment();
        }
    }

    function getMemberPackagesCount() external view returns (uint256 count) {
        return _memberPackageCounter.current();
    }

    /// @dev input: _duration in seconds
    function setActivationDuration(uint256 _duration) public onlyRole(DEFAULT_ADMIN_ROLE) {
        activationDuration = _duration;
    }

    function buyMemberPackage(uint256 _packageId) external {
        require(_packageId < _memberPackageCounter.current(), "Invalid insurance id");

        MemberPackage memory memberPackage = memberPackages[_packageId];

        require(
            rrcNftContract.rentalInfo(msg.sender).nftType == memberPackage.requiredNftType &&
                rrcNftContract.rentalInfo(msg.sender).expires > block.timestamp,
            "Player does not have required NFT"
        );

        CurrencyTransferLib.transferCurrency(vicoTokenAddress, msg.sender, address(this), memberPackage.priceInVico);

        activeMembers[msg.sender] = ActiveMember({
            packageId: _packageId,
            memberAddress: msg.sender,
            expiryDateTime: block.timestamp + activationDuration
        });
    }

    function hasValidActivationPass(address _player) public view returns (bool) {
        ActiveMember memory _purchasedPackage = activeMembers[_player];

        if (isValidPass(_purchasedPackage, _player)) {
            return true;
        }

        return false;
    }

    function userPurchasedPackageId(address _player) public view returns (uint256) {
        ActiveMember memory _purchasedPackage = activeMembers[_player];

        if (isValidPass(_purchasedPackage, _player)) {
            return _purchasedPackage.packageId;
        }

        revert("No valid activation pass");
    }

    function isValidPass(ActiveMember memory _purchasedPackage, address _player) private view returns (bool) {
        if (_purchasedPackage.memberAddress == _player && _purchasedPackage.expiryDateTime > block.timestamp) {
            return true;
        }

        return false;
    }

    function migrateUserActivationPass(
        address[] calldata _users,
        uint256[] calldata _packageIds,
        uint256[] calldata _expiryDateTimes
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            _users.length == _packageIds.length && _users.length == _expiryDateTimes.length,
            "Input arrays must have same length"
        );

        for (uint256 i = 0; i < _users.length; i++) {
            activeMembers[_users[i]] = ActiveMember({
                packageId: _packageIds[i],
                memberAddress: _users[i],
                expiryDateTime: _expiryDateTimes[i]
            });
        }
    }
}

