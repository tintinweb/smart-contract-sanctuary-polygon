pragma solidity ^0.8.0;

import "@thirdweb-dev/contracts/base/ERC721Base.sol";
import "@thirdweb-dev/contracts/extension/PermissionsEnumerable.sol";

/// @notice Reference to ERC4907, changes the `serUser` method
/// @dev See {IERC4907}.
contract RRCNFT is ERC721Base, PermissionsEnumerable {
    enum NftType {
        SILVER,
        GOLD
    }

    struct UserInfo {
        address user; // address of user role
        uint64 expires; // unix timestamp, user expires
    }

    struct RentalInfo {
        uint256 tokenId;
        uint64 expires;
        NftType nftType;
    }

    mapping(uint256 => UserInfo) internal _users;
    mapping(address => RentalInfo) internal _rentalInfo;

    // Logged when the user of a token assigns a new user or updates expires
    /// @notice Emitted when the `user` of an NFT or the `expires` of the `user` is changed
    /// The zero address for user indicates that there is no user address
    event UpdateUser(uint256 indexed tokenId, address indexed user, uint64 expires);

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    constructor(
        string memory _name,
        string memory _symbol,
        address _royaltyRecipient,
        uint128 _royaltyBps
    ) ERC721Base(_name, _symbol, _royaltyRecipient, _royaltyBps) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(MINTER_ROLE, msg.sender);
    }

    function setUser(uint256 tokenId, address user, uint64 expires, NftType _type) public virtual {
        require(hasRole(MINTER_ROLE, msg.sender), "ERC721: transfer caller is not owner nor approved");
        UserInfo storage info = _users[tokenId];
        info.user = user;
        info.expires = expires;

        RentalInfo storage userRentalInfo = _rentalInfo[user];
        userRentalInfo.expires = expires;
        userRentalInfo.tokenId = tokenId;
        userRentalInfo.nftType = _type;

        emit UpdateUser(tokenId, user, expires);
    }

    function revokeUser(address user) public virtual {
        require(hasRole(MINTER_ROLE, msg.sender), "ERC721: caller is not admin");

        RentalInfo storage userRentalInfo = _rentalInfo[user];
        UserInfo storage info = _users[userRentalInfo.tokenId];
        info.user = address(0);
        info.expires = 0;

        userRentalInfo.expires = 0;
        userRentalInfo.tokenId = 0;
        userRentalInfo.nftType = NftType.SILVER;

        emit UpdateUser(userRentalInfo.tokenId, address(0), 0);
    }

    /// @notice Get the user address of an NFT
    /// @dev The zero address indicates that there is no user or the user is expired
    /// @param tokenId The NFT to get the user address for
    /// @return The user address for this NFT
    function userOf(uint256 tokenId) public view virtual returns (address) {
        if (uint256(_users[tokenId].expires) >= block.timestamp) {
            return _users[tokenId].user;
        } else {
            return address(0);
        }
    }

    /// @notice Get the user expires of an NFT
    /// @dev The zero value indicates that there is no user
    /// @param tokenId The NFT to get the user expires for
    /// @return The user expires for this NFT
    function userExpires(uint256 tokenId) public view virtual returns (uint256) {
        if (uint256(_users[tokenId].expires) >= block.timestamp) {
            return _users[tokenId].expires;
        } else {
            return 0;
        }
    }

    /// @notice Get user rental info
    /// @dev The (0, 0) value indicates that there is no rented NFT
    /// @param user The address to get the rented NFT for
    /// @return tokenId and expires time stamp
    function rentalInfo(address user) public view virtual returns (RentalInfo memory) {
        if (uint256(_rentalInfo[user].expires) >= block.timestamp) {
            return _rentalInfo[user];
        }

        return RentalInfo(0, 0, NftType.SILVER);
    }

    function hasRent(address user) public view virtual returns (bool) {
        return uint256(_rentalInfo[user].expires) >= block.timestamp;
    }

    function _canMint() internal view override returns (bool) {
        return hasRole(MINTER_ROLE, msg.sender);
    }
}

