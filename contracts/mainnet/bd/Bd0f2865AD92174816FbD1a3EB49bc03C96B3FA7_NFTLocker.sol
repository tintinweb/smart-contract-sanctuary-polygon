pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./interfaces/INonfungiblePositionManager.sol";

/**
 * @title NFTLocker Contract
 * @notice Locking liquidity in Uniswap V3
 */
contract NFTLocker is Ownable, ERC721Holder, ReentrancyGuard {
    using EnumerableSet for EnumerableSet.UintSet;
    using SafeERC20 for IERC20;

    uint256 internal constant BASE = 1e18;
    address private constant NONFUNGIBLE_POSITION_MANAGER = 0xC36442b4a4522E871399CD717aBDD847Ab11FE88;

    uint256[] public lockTimes;
    mapping(address => mapping(address => EnumerableSet.UintSet)) internal _lockTokenIds;
    mapping(address => mapping(uint256 => uint256)) public expirationInfo;

    constructor() {
        lockTimes.push(1095 days);
        lockTimes.push(365 days);
        lockTimes.push(180 days);
    }

    event Lock(address indexed nftAddr, address indexed user, uint8 index, uint256[] tokenIds);
    event Unlock(address indexed nftAddr, address indexed user, uint256[] tokenIds);

    /**
     * @notice Lock NFT
     * @param index Select the subscript index for the lock time 0: 1095 days 1: 365 days 2: 180 days
     * @param nftAddr NFT address
     * @param tokenIds List of tokenId's to be locked
     */
    function lock(uint8 index, address nftAddr, uint256[] calldata tokenIds) external nonReentrant {
        _validationParameters(index, nftAddr, tokenIds);
        for(uint i = 0; i < tokenIds.length; i++) {
            uint256 tokenId = tokenIds[i];
            IERC721(nftAddr).safeTransferFrom(msg.sender, address(this), tokenId);
            uint expirationTime = block.timestamp + lockTimes[index];
            _lockTokenIds[nftAddr][msg.sender].add(tokenId);
            expirationInfo[nftAddr][tokenId] = expirationTime;
        }
        emit Lock(nftAddr, msg.sender, index, tokenIds);
    }

    /**
     * @notice Unlock NFT
     * @param nftAddr NFT address
     * @param tokenIds List of tokenId's to be unlocked
     */
    function unlock(address nftAddr, uint256[] calldata tokenIds) external nonReentrant {
        _validationParameters(0, nftAddr, tokenIds);
        for(uint i = 0; i < tokenIds.length; i++) {
            uint256 tokenId = tokenIds[i];
            require(_lockTokenIds[nftAddr][msg.sender].contains(tokenId), "Caller is not owner");
            require(block.timestamp > expirationInfo[nftAddr][tokenId], "Not expired");
            _lockTokenIds[nftAddr][msg.sender].remove(tokenId);
            delete expirationInfo[nftAddr][tokenId];
            _collectFee(nftAddr, tokenId);
            IERC721(nftAddr).safeTransferFrom(address(this), msg.sender, tokenId);
        }
        emit Unlock(nftAddr, msg.sender, tokenIds);
    }

    function _collectFee(address nftAddr, uint256 tokenId) internal {
        if(nftAddr == NONFUNGIBLE_POSITION_MANAGER) {
            INonfungiblePositionManager nonfungiblePositionManager = INonfungiblePositionManager(NONFUNGIBLE_POSITION_MANAGER);
            // (uint96 nonce, address operator, address token0, address token1, uint24 fee, int24 tickLower, int24 tickUpper, uint128 liquidity,
            // uint256 feeGrowthInside0LastX128, uint256 feeGrowthInside1LastX128, uint128 tokensOwed0, uint128 tokensOwed1)
            (
                ,
                ,
                ,
                ,
                ,
                ,
                ,
                uint128 liquidity,
                ,
                ,
                ,
                
            ) = nonfungiblePositionManager.positions(tokenId);
            if(liquidity > 0) {
                INonfungiblePositionManager.DecreaseLiquidityParams memory params = INonfungiblePositionManager.DecreaseLiquidityParams({
                    tokenId: tokenId,
                    liquidity: liquidity / 2,
                    amount0Min: 0,
                    amount1Min: 0,
                    deadline: type(uint256).max
                });
                nonfungiblePositionManager.decreaseLiquidity(params);
                INonfungiblePositionManager.CollectParams memory collectParams = INonfungiblePositionManager.CollectParams({
                    tokenId: tokenId,
                    recipient: address(this),
                    amount0Max: type(uint128).max,
                    amount1Max: type(uint128).max
                });
                nonfungiblePositionManager.collect(collectParams);
            }
        }
    }

    function _validationParameters(uint8 index, address nftAddr, uint256[] calldata tokenIds) internal view {
        require(index < lockTimes.length, "Index out of bounds");
        require(nftAddr != address(0), "Cannot be a zero address");
        require(tokenIds.length > 0, "Length must be greater than zero");
    }

    /**
     * @notice Get a list of NFTs locked by the user
     * @param nftAddr NFT address
     * @param user User address
     * @return tokenIds List of locked NFTs
     */
    function getLockTokenIds(address nftAddr, address user) external view returns (uint256[] memory tokenIds) {
        uint256 length = _lockTokenIds[nftAddr][user].length();
        tokenIds = new uint256[](length);
        for(uint i = 0; i < length; i++) {
            tokenIds[i] = _lockTokenIds[nftAddr][user].at(i);
        }
    }

    /**
     * @notice Get the expiration time of the locked NFT
     * @param nftAddr NFT address
     * @param tokenIds expirationTimes
     * @return expirationTimes The expiration time corresponding to the locked NFT list
     */
    function getExpirationInfo(address nftAddr, uint256[] calldata tokenIds) external view returns (uint256[] memory expirationTimes) {
        uint length = tokenIds.length;
        expirationTimes = new uint256[](length);
        for(uint i = 0; i < length; i++) {
            expirationTimes[i] = expirationInfo[nftAddr][tokenIds[i]];
        }
    }

    /**
     * @notice Sweeping token on contract address
     * @param token Token address
     * @param to Recipient address
     * @param amount Transfer amount
     */
    function withdraw(address token, address to, uint256 amount) external onlyOwner {
        IERC20(token).safeTransfer(to, amount);
    }
}