    /// @dev Valid signer address.
    address public signer;

    /// @dev The mint counter.
    uint256 private _mintCounter;

    /// @dev Modifier to enforce that only the owner or swap minter can call a function.
    modifier onlySwapMinter() {
        if (msg.sender != owner() && !swapMinters[msg.sender]) revert Unauthorized();
        _;
    }

    /// @dev Modifier to enforce that only the owner or approved burner can call a function.
    modifier onlyApprovedBurner() {
        if (msg.sender != owner() && !approvedBurners[msg.sender]) revert Unauthorized();
        _;
    }

    /**
     * @param _psydelveCaps Address to the Psydelve caps contract.
     * @param _deadAddress "Dead" address.
     * @param _signer Valid signer to expect signatures to originate from.
     * @param _baseURI The URI for the token metadata.
     * @param _baseURIExtension The extension to be appended to the tokenID when formin the URI.
     * @param _royaltyReceiver The address that should receive royalties for secondary sales.
     * @param _royaltyFeeNumerator The fee percentage to charge. The denominator is fixed to 10000, so setting to 750 would make the fee 7.5% (750/10000).
     */
    function initialize(
        string memory name,
		string memory symbol,
	    address _psydelveCaps,
        address _deadAddress,
        address _signer,
        string memory _baseURI,
        string memory _baseURIExtension,
        address _royaltyReceiver,
        uint96 _royaltyFeeNumerator
	) public initializer {
		__UUPSUpgradeable_init();
		__Ownable2Step_init();
        __ERC721_init(name, symbol);
        __ERC2981_init();
		__Pausable_init();

        if (_psydelveCaps != address(0)) {
            psydelveCaps = IERC721(_psydelveCaps);
        }

        _mintCounter = 0;
        deadAddress = _deadAddress;
        signer = _signer;
        baseURI = _baseURI;
        baseURIExtension = _baseURIExtension;

        _setDefaultRoyalty(_royaltyReceiver, _royaltyFeeNumerator);
        _pause();
	}

    /// @dev Pause the contract, and disable token transfers.
    function pause() external onlyOwner {
        _pause();
    }

    /// @dev Unpause the contract, and allow token transfers.
    function unpause() external onlyOwner {
        _unpause();
    }

    /**
     * @dev Set the base URI and extension for the token metadata.
     * @param _baseURI The URI that will get prepended to the token ID.
     * @param _baseURIExtension The extension that will get appended to the token ID.
     */
    function setBaseURI(string memory _baseURI, string memory _baseURIExtension) external onlyOwner {
        baseURI = _baseURI;
        baseURIExtension = _baseURIExtension;
        emit BaseURISet(_baseURI, _baseURIExtension);
    }

    /**
     * @dev Set the address for the Psydelve caps contract.
     * @param _psydelveCaps Address to the contract.
     */
    function setPsydelveCaps(address _psydelveCaps) external onlyOwner {
        if (_psydelveCaps == address(0)) revert InvalidAddress();
        psydelveCaps = IERC721(_psydelveCaps);
        emit PsydelveCapsSet(_psydelveCaps);
    }

    /**
     * @dev Set the "dead" address to send caps to.
     * @param _deadAddress "Dead" address.
     */
    function setDeadAddress(address _deadAddress) external onlyOwner {
        if (_deadAddress == address(0)) revert InvalidAddress();
        deadAddress = _deadAddress;
        emit DeadAddressSet(_deadAddress);
    }

    /**
     * @dev Set a valid signer to expect signatures to originate from.
     * @param _signer signer address.
     */
    function setSigner(address _signer) external onlyOwner {
        if (_signer == address(0)) revert InvalidAddress();
        signer = _signer;
        emit SignerSet(_signer);
    }

    /**
     * @dev Set the address for the allowed swap minter.
     * @param _swapMinter Address of the swap minter.
     * @param _enabled True to enable as an allowed swap minter.
     */
    function setSwapMinter(address _swapMinter, bool _enabled) external onlyOwner {
        if (_swapMinter == address(0)) revert InvalidAddress();
        swapMinters[_swapMinter] = _enabled;
        emit SwapMinterSet(_swapMinter, _enabled);
    }

    /**
     * @dev Set the address for the allowed burner.
     * @param _burner Address of the burner.
     * @param _enabled True to enable as an approved burner.
     */
    function setApprovedBurner(address _burner, bool _enabled) external onlyOwner {
        if (_burner == address(0)) revert InvalidAddress();
        approvedBurners[_burner] = _enabled;
        emit ApprovedBurnerSet(_burner, _enabled);
    }

    /**
     * @dev Set the default royalty to be received for secondary sales. The denominator used for the numerator to form the fraction is 10000.
     * @param _receiver The address that should receive the royalties.
     * @param _feeNumerator The fee percentage to charge. The denominator is fixed to 10000, so setting the `_feeNumerator` to 750 would make the fee 7.5% (750/10000).
     */
    function setDefaultRoyalty(address _receiver, uint96 _feeNumerator) external onlyOwner {
        _setDefaultRoyalty(_receiver, _feeNumerator);
    }

    /// @dev Delete the default royalty (i.e. reset back to 0% with no receiver).
    function deleteDefaultRoyalty() external onlyOwner {
        _deleteDefaultRoyalty();
    }

    /**
     * @dev Set royalties on a per token basis.
     * @param _tokenId The token to set the royalties for.
     * @param _receiver The address that should receive the royalties.
     * @param _feeNumerator The fee percentage to charge. The denominator is fixed to 10000, so setting the `_feeNumerator` to 750 would make the fee 7.5% (750/10000).
     */
    function setTokenRoyalty(uint256 _tokenId, address _receiver, uint96 _feeNumerator) external onlyOwner {
        _setTokenRoyalty(_tokenId, _receiver, _feeNumerator);
    }

    /**
     * @dev Reset the royalties for a specific token (i.e. reset back to 0% with no receiver).
     * @param _tokenId The token ID to reset royalties for.
     */
    function resetTokenRoyalty(uint256 _tokenId) external onlyOwner {
        _resetTokenRoyalty(_tokenId);
    }

    /**
     * @dev Used by the contract for swapping out the old psydelve with a new one.
     * @param _to Address to mint (swap) to.
     * @param _tokenId The token Id to mint (swap) to the user.
     */
    function swapMint(address _to, uint256 _tokenId) external onlySwapMinter {
        _baseMint(_to, _tokenId);
    }

    /**
     * @dev Useful for any use case where a burn would be useful. i.e. gamified burning.
     * @param _tokenId The token Id to burn.
     */
    function burn(uint256 _tokenId) external onlyApprovedBurner {
        _burn(_tokenId);
    }

    /**
     * @dev Reveal Psydelve cap (aka mint the Psydelve).
     * @param _capTokenId The token ID of the cap.
     * @param _signature A signature generated by a trusted signer that selected the new token Id.
     * @param _newTokenId The new token Id to mint. Selected by a trusted backend system, and verified by the signature.
     */
    function safeMint(uint256 _capTokenId, bytes memory _signature, uint256 _newTokenId) external {
        if (address(psydelveCaps) == address(0)) revert InvalidAddress();
        if (address(deadAddress) == address(0)) revert InvalidAddress();

        if (_newTokenId > 6000) revert InvalidTokenId();
        if (psydelveCaps.balanceOf(msg.sender) == 0) revert NoCapBalance();
        if (!verify(msg.sender, _newTokenId, _signature)) revert InvalidSignature();

        psydelveCaps.safeTransferFrom(msg.sender, deadAddress, _capTokenId);
        _baseMint(msg.sender, _newTokenId);
    }

    /**
     * @dev Get the URI to the metadata for a specific token. Format is `[baseURI][tokenId][baseURIExtension]`.
     * @param _tokenId The token ID to get the metadata URI for.
     * @return Returns the metadata URI for a specific token.
     */
    function tokenURI(uint256 _tokenId) public view virtual override returns (string memory) {
        if (!_exists(_tokenId)) revert DoesNotExist();
        return
            bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, _tokenId.toString(), baseURIExtension)) : "";
    }

    /**
     * @dev Get a list of token Ids owned by the user.
     * @param _owner The wallet address to grab token Ids for.
     * @return Returns the list of token Ids.
     */
    function tokensOfOwner(address _owner) public view returns (uint256[] memory) {
        uint256 balance = balanceOf(_owner);
        uint256[] memory tokenIds = new uint256[](balance);

        for (uint256 i = 0; i < balance; ) {
            tokenIds[i] = tokenOfOwnerByIndex(_owner, i);

            unchecked {
                i++;
            }
        }

        return tokenIds;
    }

    /**
     * @dev Create a keccak256 hash with `_to` and `_tokenId`.
     * @param _to The `_to` address used in the mint.
     * @param _tokenId The `_tokenId` used in the mint.
     * @return Returns the keccak256 hash.
     */
    function getMessageHash(address _to, uint256 _tokenId) public pure returns (bytes32) {
        return keccak256(abi.encodePacked(_to, _tokenId));
    }

    /**
     * @dev Add the ethereum signed message header to the hash.
     * @param _messageHash The message hash to prepend to.
     * @return Returns the new hash with the header prepended.
     */
    function getEthSignedMessageHash(bytes32 _messageHash) public pure returns (bytes32) {
        return ECDSAUpgradeable.toEthSignedMessageHash(_messageHash);
    }

    /**
     * @dev Verify that the `_signature` was generated by the `signer`.
     * @param _to The minting `_to` address.
     * @param _tokenId The minting `_tokenId`.
     * @param _signature The signature to verify.
     * @return bool Return `true` if `_signature` originated from the `signer`, and `false` otherwise.
     */
    function verify(address _to, uint256 _tokenId, bytes memory _signature) public view returns (bool) {
        if (signer == address(0)) revert InvalidAddress();

        bytes32 messageHash = getMessageHash(_to, _tokenId);
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);

        return SignatureCheckerUpgradeable.isValidSignatureNow(signer, ethSignedMessageHash, _signature);
    }

    /**
     * @dev Mint the token Id to the given address, up to the `MAX_SUPPLY`.
     * @param _to The address to mint tokens to.
     * @param _tokenId Token Id to mint.
     */
    function _baseMint(address _to, uint256 _tokenId) internal {
        if (_mintCounter >= MAX_SUPPLY) revert MaxMinted();
        _safeMint(_to, _tokenId);
        unchecked {
            _mintCounter += 1;
        }
    }
function _beforeTokenTransfer(
        address _from,
        address _to,
        uint256 _firstTokenId,
        uint256 _batchSize
    ) internal override (ERC721EnumerableUpgradeable, ERC5058Upgradeable) whenNotPaused {
        ERC5058Upgradeable._beforeTokenTransfer(_from, _to, _firstTokenId, _batchSize);
    }

    function _afterTokenTransfer(
		address from,
		address to,
		uint256 tokenId,
		uint256 batchSize
	) internal virtual override(ERC721Upgradeable, ERC5058Upgradeable) {
		ERC5058Upgradeable._afterTokenTransfer(from, to, tokenId, batchSize);
	}

    function _burn(
		uint256 tokenId
	) internal virtual override(ERC721Upgradeable, ERC5058Upgradeable) {
		ERC5058Upgradeable._burn(tokenId);
	}

      // @dev required by UUPSUpgradeable
	function _authorizeUpgrade(address) internal override onlyOwner {}

    function supportsInterface(bytes4 _interfaceId) public view override(ERC721EnumerableUpgradeable, ERC2981Upgradeable, ERC5058Upgradeable) returns (bool) {
        return super.supportsInterface(_interfaceId);
    }
}