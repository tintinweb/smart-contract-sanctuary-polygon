import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./MockNativeCurrency.sol";
import "hardhat/console.sol";

interface ITestContract {
    enum ExpectedOutcome {
        Undefined,
        Success,
        Revert,
        MaxWillingToPayExceeded,
        InvalidSwap,
        ERC20ForNative,
        NativeForERC20
    }

    function setTestParameters(ExpectedOutcome _expectedOutcome) external;
}

contract MockSwapRouter is ISwapRouter, ITestContract {
    constructor(address _factory, address _WETH9) {}

    ExpectedOutcome public expectedOutcome;

    function setTestParameters(ExpectedOutcome _expectedOutcome) external override {
        expectedOutcome = _expectedOutcome;
    }

    function exactOutputSingle(ExactOutputSingleParams calldata params)
        external
        payable
        override
        returns (uint256 amountIn)
    {
        require(expectedOutcome != ExpectedOutcome.Undefined, "specifiy an expected test outcome.");

        amountIn = params.amountInMaximum;

        if (expectedOutcome == ExpectedOutcome.Success) {
            IERC20(params.tokenOut).transfer(msg.sender, params.amountOut);
        }

        if (expectedOutcome == ExpectedOutcome.Revert) {
            revert("expected revert");
        }

        if (expectedOutcome == ExpectedOutcome.MaxWillingToPayExceeded) {
            amountIn = params.amountInMaximum * 2;
            require(amountIn <= params.amountInMaximum, "Too much requested");
        }

        if (expectedOutcome == ExpectedOutcome.InvalidSwap) {
            require(params.tokenIn != params.tokenOut, "Provided token is the same as requested token");
        }

        if (expectedOutcome == ExpectedOutcome.ERC20ForNative) {
            MockNativeCurrency(payable(params.tokenOut)).transfer(msg.sender, params.amountOut);
        }
    }

    /*------------------------------------------------------------------*\
    | Required Overrides
    \*------------------------------------------------------------------*/

    function exactInputSingle(ExactInputSingleParams calldata) external payable override returns (uint256) {
        revert("not implemented");
    }

    function exactInput(ExactInputParams calldata) external payable override returns (uint256) {
        revert("not implemented");
    }

    function exactOutput(ExactOutputParams calldata) external payable override returns (uint256) {
        revert("not implemented");
    }

    function uniswapV3SwapCallback(
        int256,
        int256,
        bytes calldata
    ) external pure {
        revert("not implemented");
    }
}

