    require(_inited, "BoVComplete: Must init");
    require(haveSpecialsExpired(), "BoVComplete: Cannot recover special rewards til after deadline");
    uint256 specialsBalance = numSpecial - numSpecialsRewarded;
    require(specialsBalance > 0, "BoVComplete: nothing to recover");
    uint256[] memory ids = new uint256[](2);
    ids[0] = CryptoJigToken.TokenType.unwrap(_stdRareTyp);
    ids[1] = CryptoJigToken.TokenType.unwrap(_firstComTyp);
    uint256[] memory bals = new uint256[](2);
    bals[0] = specialsBalance;
    bals[1] = specialsBalance;
    ERC1155(_tokenAddr).safeBatchTransferFrom(address(this), msg.sender, ids, bals, "");
