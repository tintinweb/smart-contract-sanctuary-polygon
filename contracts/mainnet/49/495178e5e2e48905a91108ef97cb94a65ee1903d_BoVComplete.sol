  // If minted before specialDeadlineTS, they get special rewards too.
  uint256 public constant  numSpecial = 3;
  uint256 public immutable specialDeadlineTS;
  // Ordinary rewards: first 15 mints get some tokens, regardless when those mints occur.
  uint32  public constant  numRewards = 15;
  uint32  public constant  stdUncommonPerReward = 2;
  uint32  public constant  stdCommonPerReward = 5;
  string  public           baseUri;
  uint32  public           numSpecialsRewarded = 0;

  event Reward(uint256 indexed id, address indexed recipient, bool indexed isSpecial);

  using Counters for Counters.Counter;
  Counters.Counter private _tokenIds;
  ERC1155 private _osContract = ERC1155(_osAddr);
  bool    private _inited     = false;

  error WrongPayment(uint256 reqdPayment, uint256 actualPayment);

  constructor(address tokenAddr, uint256 deadlineTS, string memory _baseUri)
      ERC721("The Birth of Venus: The Whole Picture", "BOVC") {
    require(block.timestamp + 1 days < deadlineTS, "BoVComplete: Unreasonable timeframe");
    _tokenAddr = tokenAddr;
    baseUri = _baseUri;
    specialDeadlineTS = deadlineTS;
  }

  // Only used for dependency injection during testing
  function init(address osAddr) external payable onlyBasicTrusted {
    init();
    _osContract = ERC1155(osAddr);
  }

  function init() public payable onlyBasicTrusted {
    require(!_inited, "BoVComplete: Cant init twice");
    uint256 payment = msg.value;
    uint256 reqdPayment = numSpecial * 20 ether;
    if (payment != reqdPayment) { revert WrongPayment(reqdPayment, payment); }
    CryptoJigToken tokenInst = CryptoJigToken(_tokenAddr);
    tokenInst.mintBacked{value: numSpecial * 15 ether}(address(this), _stdRareTyp);
    tokenInst.mintBacked{value: numSpecial * 5 ether}(address(this), _firstComTyp);
    tokenInst.mintUnbacked(address(this), numRewards * stdUncommonPerReward, _stdUncomTyp);
    tokenInst.mintUnbacked(address(this), numRewards * stdCommonPerReward, _stdComTyp);
    _inited = true;
  }

  function onERC1155Received(address op, address, uint256, uint256, bytes calldata)
    external override view returns (bytes4) {
      require(!_inited, "BoVComplete: Cant receive NFT post init");
      require(op == address(this), "BoVComplete: only BoVComplete may send itself NFTs");
      require(msg.sender == _tokenAddr, "BoVComplete: invalid ERC1155");
      return this.onERC1155Received.selector;
  }

  function onERC1155BatchReceived(address, address, uint256[] calldata, uint256[] calldata, bytes calldata)
    external override pure returns (bytes4) {
      revert("BoVComplete: cannot batch receive ERC1155");
  }

  function _baseURI() internal override view returns (string memory) {
    return baseUri;
  }

  function setBaseUri(string memory uri) external onlyBasicTrusted {
    baseUri = uri;
  }

  function _beforeTokenTransfer(address from, address to, uint256 tokenID) internal override {
    require(from == address(0), "BoVComplete: Non-transferrable, non-burnable");
    super._beforeTokenTransfer(from, to, tokenID);
  }

  function haveSpecialsExpired() public view returns (bool) {
    return block.timestamp > specialDeadlineTS || _tokenIds.current() >= numSpecial;
  }

  function haveRewardsExpired() public view returns (bool) {
    return _tokenIds.current() >= numRewards;
  }

