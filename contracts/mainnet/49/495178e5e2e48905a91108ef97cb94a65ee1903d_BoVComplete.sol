  function _appendAddrToString(string memory s, address a) private pure returns (string memory) {
    string memory aStr = Strings.toHexString(a);
    return string(abi.encodePacked(s, aStr));
  }

  function isBasicTrusted(address addr) public view returns (bool) {
    return AUTH_CONTRACT.hasRole(basicTrustedRole, addr);
  }

  modifier onlyBasicTrusted {
    if (!isBasicTrusted(msg.sender)) {
      // using if+revert instead of require to ensure lazy evaluation of the error msg
      revert(_appendAddrToString("CryptoJigBasicAuth: only CryptoJigWorld may do this, not ", msg.sender));
    }
    _;
  }

  function isBasicFast(address addr) public view returns (bool) {
    return AUTH_CONTRACT.hasRole(basicFastRole, addr);
  }

  modifier onlyBasicFast {
    if (!isBasicFast(msg.sender)) {
      revert(_appendAddrToString("CryptoJigBasicAuth: only CryptoJigWorld may do this, not ", msg.sender));
    }
    _;
  }
}


