  /// #if_succeeds old(_getAmtBacked()) - _getAmtBacked() == old(address(this).balance) - address(this).balance;
  /// #if_succeeds old(_getAmtBacked()) - _getAmtBacked() == msg.sender.balance - old(msg.sender.balance);
  /// #if_succeeds balanceOf(msg.sender, getTokenId(typ)) == old(balanceOf(msg.sender, getTokenId(typ))) - count;
  function redeemForMatic(TokenType typ, uint32 count) external {
    checkTokenType(typ);
    if (!isBackedType(typ)) { revert WrongBackingStatus(typ); }
    require(count != 0, "CryptoJigToken: no count");

    uint32 _countBackedByMatic = countBackedByMatic[typ];
    if (count > _countBackedByMatic) { revert InsufficientCount(uint32(count), _countBackedByMatic, typ); }

    address redeemer = msg.sender;
    _burnUnsafe(redeemer, typ, count);
    countBackedByMatic[typ] -= count;
    (bool paySuccess, ) = redeemer.call{value: count * getTypeVal(typ)}("");
    require(paySuccess, "CryptoJigToken: redemption payment failed");
    emit RedeemForMatic(typ, count);
  }

  /// #if_succeeds tokenIds.length == counts.length;
  /// #if_succeeds balanceOf(msg.sender, getTokenId(typ)) ==
  ///   old(balanceOf(msg.sender, getTokenId(typ))) - unchecked_sum(counts);
  /// these ought to be forall, but can't be due to scribble limitations
  /// #if_succeeds
  ///   _tokenTypsEq(typ, old(_typeRegistry[_getTypeRegistryKey(contractAddr, tokenIds[0])]));
  /// #if_succeeds
  ///   counts[0] <= old(ERC1155(contractAddr).balanceOf(address(this), tokenIds[0])) &&
  ///   _sumCounts(tokenIds[0], tokenIds, counts) <= old(ERC1155(contractAddr).balanceOf(address(this), tokenIds[0]));
  /// #if_succeeds
  ///   old(ERC1155(contractAddr).balanceOf(address(this), tokenIds[0])) == counts[0] ==>
  ///     TokenType.unwrap(_typeRegistry[_getTypeRegistryKey(contractAddr, tokenIds[0])]) == 0;
  /// #if_succeeds
  ///   ERC1155(contractAddr).balanceOf(address(this), tokenIds[0]) > 0 ==>
  ///     _tokenTypsEq(typ, _typeRegistry[_getTypeRegistryKey(contractAddr, tokenIds[0])]);
  /// #if_succeeds
  ///   let ctr := old(ERC1155(contractAddr)) in
  ///   let tid := old(tokenIds[0]) in
  ///   ctr.balanceOf(msg.sender, tid) ==
  ///     old(ctr.balanceOf(msg.sender, tid)) + _sumCounts(tid, tokenIds, counts);
  /// #if_succeeds
  ///   let ctr := old(ERC1155(contractAddr)) in
  ///   let tid := old(tokenIds[0]) in
  ///   ctr.balanceOf(address(this), tid) ==
  ///     old(ctr.balanceOf(address(this), tid)) - _sumCounts(tid, tokenIds, counts);
  function redeemForNFTs(
      TokenType typ,
      address contractAddr,
      uint256[] calldata tokenIds,
      uint256[] calldata counts
  ) external {
    checkTokenType(typ);
    uint256 totalCount = _performBatch(contractAddr, tokenIds, counts, typ, _redeemTask);
    address redeemer = msg.sender;
    _burnUnsafe(redeemer, typ, totalCount);
    ERC1155(contractAddr).safeBatchTransferFrom(address(this), redeemer, tokenIds, counts, "");
  }

  function _burnUnsafe(address redeemer, TokenType typ, uint256 count) private {
    // _totalSupply[typ] -= count; only for testing
    _burn(redeemer, getTokenId(typ), count);
  }

  function _redeemTask(
    address contractAddr,
    uint256 tokenId,
    uint256 count,
    uint256 balance,
    TokenType typ
  ) private {
    if (balance < count) { revert InsufficientCount(uint32(count), uint32(balance), typ); }

    bytes memory key = _getTypeRegistryKey(contractAddr, tokenId);
    TokenType regTyp = _typeRegistry[key];
    if (!_tokenTypsEq(regTyp, typ)) {
      revert InconsistentTypeRedemption(contractAddr, tokenId, typ, regTyp);
    }

    emit RedeemForNFTs(typ, contractAddr, tokenId, uint32(count));
    if (balance == count) {
      _unregisterType(key, contractAddr, tokenId, regTyp);
    }
  }

  function _unregisterType(bytes memory key, address contractAddr, uint256 tokenId, TokenType typ) private {
    emit UnregisterType(contractAddr, tokenId, typ);
    // get a gas refund
    _typeRegistry[key] = TokenType.wrap(0);
  }
}


