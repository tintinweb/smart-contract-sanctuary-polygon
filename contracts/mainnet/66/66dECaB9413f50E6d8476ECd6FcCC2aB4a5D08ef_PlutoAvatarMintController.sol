	string private SIGNATURE_VERSION;
	uint32 private signatureValidity;
	address public designatedSigner;
	
	mapping (bytes => bool) private usedSignatures;
	
	struct whitelist {
		uint8 PhaseType;
		uint8 Sale1MaxMint;
		uint8 Sale2MaxMint;
		uint8 Sale3MaxMint;
		uint8 Sale4MaxMint;
		uint32 timestamp;
		address userAddress;
		bytes signature;
	}
	
