pragma solidity ^0.8.0;

import {IAvatarMinter} from "./Interface/IAvatarMinter.sol";
import {Initializable} from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import {Strings} from "@openzeppelin/contracts/utils/Strings.sol";
import {ERC2981Upgradeable} from "@openzeppelin/contracts-upgradeable/token/common/ERC2981Upgradeable.sol";
import {IERC721AUpgradeable} from "erc721a-upgradeable/contracts/IERC721AUpgradeable.sol";
import {NonblockingLzApp, Ownable2StepUpgradeable} from "./utils/NonblockingLzApp.sol";
import {whitelistWizard} from "./utils/whitelistWizard.sol";
import {IERC20Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import {IDelegationRegistry} from "./Interface/IDelegationRegistry.sol";

contract PlutoAvatarMintController is
Initializable,
Ownable2StepUpgradeable,
ERC2981Upgradeable,
NonblockingLzApp,
whitelistWizard
{
    
    error MaxMintReached(); // if the max mint is reached
    
    /**
       * @notice A struct that defines a Sale
       * @params startTime Time when the Sale begins
       * @params endTime Time when the Sale ends
       * @params register Registry of minters
    */
    struct mintInfo {
        uint256 startTime;
        uint256 endTime;
        mapping(address => uint256) register;
    }
    
    mintInfo public MBC;
    
    mintInfo public Investors;
    
    mintInfo public Guaranteed;
    
    mintInfo public FCFS;
    
    mintInfo public Public;
    
    // contract storage - minting
    uint256 public maxSupply; // max supply of the NFT
    address public nativeAddress;
    
    mapping(address => bool) public isTokenAllowed;
    mapping(address => uint256) public mintPrice;
    mapping(address => uint256) public tokensAllocated;
    
    IDelegationRegistry public dc;
    IAvatarMinter public avatarMinter;
    
    
    /// @notice Initializes the pass contract
    function initialize(
        address _endpointAddress,
        address _designatedSigner,
        address _dc,
        address _avatarMinter,
        string memory _domain,
        string memory _version
    ) external initializer {
        __Ownable2Step_init();
        __nonblockingLzApp_init(_endpointAddress);
        __Wizard_init(_domain, _version, _designatedSigner);
        dc = IDelegationRegistry(_dc);
        avatarMinter = IAvatarMinter(_avatarMinter);
        nativeAddress = 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;
        isTokenAllowed[nativeAddress] = true;
        maxSupply = 4444;
        mintPrice[nativeAddress] = 99 ether;
        
        Investors.startTime = 1689213600;
        Investors.endTime = Investors.startTime + 12 hours;
        
        MBC.startTime = Investors.startTime;
        MBC.endTime = Investors.endTime;
        
        Guaranteed.startTime = Investors.endTime;
        Guaranteed.endTime = Guaranteed.startTime + 1 hours;
        
        FCFS.startTime = Guaranteed.endTime;
        FCFS.endTime = FCFS.startTime + 1 hours;
        
        Public.startTime = Guaranteed.endTime;
        Public.endTime = Public.startTime + 100 days;
    }
    
    // Mint functions
    
    /// @notice This function is used to mint tokens
    /// @dev This function mints allocated free tokens
    function freeMintTokens() external {
        uint256 tokensToMint = tokensAllocated[msg.sender];
        require(tokensToMint > 0, "No tokens allocated");
        tokensAllocated[msg.sender] = 0;
        mintTokens(msg.sender, tokensToMint);
    }
    
    /// @notice This function is used to mint tokens
    /// @dev This function takes in a signature from whitelist wizard and mints tokens
    /// @param signature Signature from whitelist wizard
    /// @param paymentToken Address of the token to be used for payment
    function payAndMint(whitelist calldata signature, uint256 mintAmount, address paymentToken,  address _vault) external payable {
        require (isTokenAllowed[paymentToken], "Token not allowed");
        address requester = msg.sender;
        
        /// @dev Check if signature is valid
        validateSignature(signature, requester);
        
        if (_vault != address(0)) {
            bool isDelegateValid = dc.checkDelegateForContract(requester, _vault, address(this));
            require(isDelegateValid, "invalid delegate-vault pairing");
            requester = _vault;
        }
        
        /// @dev Check if user is allowed to mint
        validateMaxMintPerUser(signature, requester, mintAmount);
        
        /// @dev Check if the payment is valid
        if (paymentToken == nativeAddress){
            require(msg.value == mintPrice[nativeAddress] * mintAmount, "Invalid amount");
        } else {
            IERC20Upgradeable(paymentToken).transferFrom(requester, address(this), mintPrice[paymentToken] * mintAmount);
        }
        
        /// @dev Mint tokens
        mintTokens(requester, mintAmount);
    }
    
    function validateSale(
        uint256 startTime,
        uint256 endTime,
        uint256 maxMint,
        uint256 currentMint,
        uint256 mintAmount
    ) internal view {
        require(startTime <= block.timestamp && endTime >= block.timestamp, "Error: Sale not active");
        require(maxMint > 0, "Error: Max mint per user is 0");
        require(maxMint >= currentMint + mintAmount, "Error: Max mint per user reached");
    }
    
    function validateMaxMintPerUser(
        whitelist calldata signature,
        address minter,
        uint256 mintAmount
    ) internal returns (uint8) {
        unchecked {
            if (signature.PhaseType == 2) {
                validateSale(MBC.startTime, MBC.endTime, signature.Sale2MaxMint, MBC.register[minter], mintAmount);
                MBC.register[minter] += mintAmount;
                return uint8(2);
            }
            else if (signature.PhaseType == 1) {
                validateSale(Investors.startTime, Investors.endTime, signature.Sale1MaxMint, Investors
                .register[minter], mintAmount);
                Investors.register[minter] += mintAmount;
                return uint8(1);
            }
            else if (signature.PhaseType == 3) {
                validateSale(Guaranteed.startTime, Guaranteed.endTime, signature.Sale3MaxMint, Guaranteed.register[minter], mintAmount);
                Guaranteed.register[minter] += mintAmount;
                return uint8(3);
            }
            else if (signature.PhaseType == 4) {
                validateSale(FCFS.startTime, FCFS.endTime, signature.Sale4MaxMint, FCFS.register[minter], mintAmount);
                FCFS.register[minter] += mintAmount;
                return uint8(4);
            }
            else {
                require(Public.startTime <= block.timestamp && Public.endTime >= block.timestamp, "Error: Public not active");
                Public.register[minter] += mintAmount;
                return uint8(5);
            }
        }
    }
    
    /// @dev This function is used to mint tokens
    /// @param toSend Address to send the minted tokens
    /// @param tokensToMint Amount of tokens to mint
    function mintTokens (address toSend, uint256 tokensToMint) internal {
        if (avatarMinter.totalSupply() + tokensToMint > maxSupply) {
            revert MaxMintReached();
        }
        avatarMinter.mintTokens(toSend, tokensToMint);
    }
    
    // Mint Function End
    
    // Owner Function
    
    /// @notice This function is used to set the max supply
    /// @dev This function takes in a uint256 and sets the max supply
    /// @param _maxSupply Max supply to set
    function setMaxSupply(uint256 _maxSupply) external onlyOwner {
        maxSupply = _maxSupply;
    }
    
    /// @notice This function allocated free tokens to a address
    /// @dev This function takes in a address and a count and allocates tokens to the address
    /// @param _address Address to allocate tokens to
    /// @param count Number of tokens to allocate
    function allocateTokens(address[] calldata _address, uint256[] calldata count) external onlyOwner {
        for (uint256 i = 0; i < _address.length; i++) {
            tokensAllocated[_address[i]] += count[i];
        }
    }
    
    /// @notice Whitelist token
    /// @param token Address of the token
    function allowToken(address token) external onlyOwner {
        isTokenAllowed[token] = true;
    }
    
    /// @notice This function is used to set the mint price for different tokens
    /// @param paymentToken Address of the token to be used for payment
    /// @param price Price of the token
    function setMintPrice(address paymentToken, uint256 price) external onlyOwner {
        mintPrice[paymentToken] = price;
    }
    
    /// @notice This function is used to set the conditions mintInfo
    /// @dev This function takes in a phase number and start and end time and sets the mintInfo
    /// @param _startTime Start time of the mintInfo
    /// @param _endTime End time of the mintInfo
    function setConditions(
        uint8 phase,
        uint256 _startTime,
        uint256 _endTime
    ) external onlyOwner {
        require(_startTime < _endTime, "Invalid times");
        unchecked{
            if (phase == 2) {
                MBC.startTime = _startTime;
                MBC.endTime = _endTime;
            } else if (phase == 1) {
                Investors.startTime = _startTime;
                Investors.endTime = _endTime;
            } else if (phase == 3) {
                Guaranteed.startTime = _startTime;
                Guaranteed.endTime = _endTime;
            } else if (phase == 4) {
                FCFS.startTime = _startTime;
                FCFS.endTime = _endTime;
            }
            else {
                Public.startTime = _startTime;
                Public.endTime = _endTime;
            }}
    }
    
    /// @notice Function to withdraw all Ether from this contract.
    /// @param to Address to which the Ether needs to be withdrawn
    function withdraw(address payable to, address payableToken) public onlyOwner {
        // get the amount of Ether stored in this contract
        uint256 amount = address(this).balance;
        
        // send all Ether to owner
        // Owner can receive Ether since the address of owner is payable
        if (payableToken == nativeAddress) {
            (bool success,) = to.call{value: amount}("");
            require(success, "Failed to send Ether");
        } else {
            IERC20Upgradeable(payableToken).transfer(to, IERC20Upgradeable(payableToken).balanceOf(address(this)));
        }
        //transfer collected matic to owner
    }
    
    
    // LAYERZERO FUNCTIONS
    
    /// @notice This function is used to mint tokens by receiving input from Layer 0
    function _nonblockingLzReceive(
        uint16,
        bytes memory,
        uint64,
        bytes memory _payload
    ) internal override {
        (bytes memory mintTo, uint16 tokensToMint, uint8 phase) = abi.decode(
            _payload,
            (bytes,uint16,uint8)
        );
        address minter = bytesToAddress(mintTo);
        unchecked {
            if (phase == 4) {
                FCFS.register[minter] += tokensToMint;
            } else if (phase == 3) {
                Guaranteed.register[minter] += tokensToMint;
            } else if (phase == 1) {
                Investors.register[minter] += tokensToMint;
            } else if (phase == 2) {
                MBC.register[minter] += tokensToMint;
            } else {
                Public.register[minter] += tokensToMint;
            }
        }
        mintTokens(minter, uint256(tokensToMint));
    }
    
    function bytesToAddress(bytes memory data) public pure returns (address) {
        require(data.length == 20, "Invalid address length");
        
        address addr;
        assembly {
            addr := mload(add(data, 20))
        }
        return addr;
    }
    
    function checkSignature(whitelist memory _signature, address _user) public view returns (
        bool isSignatureValid,
        bool isDesignatedSignerValid
    ) {
        if (getSigner(_signature) == designatedSigner) {
            isDesignatedSignerValid = true;
        }
        if (isValidSignature(_signature,_user)) {
            isSignatureValid = true;
        }
        return (isSignatureValid, isDesignatedSignerValid);
    }
    
    // Getter
    
    /**
	   * @notice This function returns the number of tokens minted by a member in a particular phase
       * @param _address Address of the member
    */
    function readRegister(address _address, uint8 phase) public view returns (uint256) {
        require(_address != address(0), "Invalid address provided");
        if (phase == 2) return MBC.register[_address];
        else if (phase == 1) return Investors.register[_address];
        else if (phase == 3) return Guaranteed.register[_address];
        else if (phase == 4) return FCFS.register[_address];
        else return Public.register[_address];
    }
}

