pragma solidity ^0.7.6;
pragma experimental ABIEncoderV2;
import "../Vendor.sol";
import "../interfaces/IOwnerOf.sol";
import "../interfaces/ICollectionWhiteList.sol";
import "../libraries/LibGacheMachine.sol";

contract GachaMachineViewFacet is IERC721Receiver, IOwnerOf {
    function state() private pure returns(GachaMachineStorage storage){
        return LibGachaMachine.gachaMachineStorage();
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }

    function ownerOf(uint256 id) external override view returns(address) {
        return state().allCollections[id].owner;
    }

    function allCollectionByOwner(address owner_)
        public
        view
        returns (Collection[] memory)
    {
        uint256[] memory idList = state().collectionsByOwner[owner_];
        Collection[] memory cList = new Collection[](idList.length);
        for (uint256 i = 0; i < idList.length; i++) {
            cList[i] = state().allCollections[idList[i]];
        }
        return cList;
    }

    function allNFTByCollection(uint256 collectionId)
        public
        view
        returns (NFT[] memory)
    {
        NFT[] memory nfts = new NFT[](state().nftsByCollectionId[collectionId].length);
        for (uint256 i = 0; i < state().nftsByCollectionId[collectionId].length; i++) {
            nfts[i] = state().allNFTs[state().nftsByCollectionId[collectionId][i]];
        }
        return nfts;
    }

    function allCollectionList() public view returns (Collection[] memory) {
        Collection[] memory cList = new Collection[](state().nextCollectionId);
        for (uint256 i = 0; i < state().nextCollectionId; i++) {
            cList[i] = state().allCollections[i + 1];
        }
        return cList;
    }

    function allCollections(uint256 id) public view returns (Collection memory collection, uint256 nftQuantity) {
        NFT[] memory nfts = allNFTByCollection(id);
        for (uint256 index = 0; index < nfts.length; index++) {
            if(nfts[index].claimAt == 0)
                nftQuantity++;
        }
        collection = state().allCollections[id];
    }

    function nftPrice(uint256 collectionId) public view returns(uint256){
        return state().allCollections[collectionId].nftPrice;
    }

    function nftCoin(uint256 collectionId) public view returns(address){
        return state().allCollections[collectionId].nftCoin;
    }

    function feeRate() public view returns (uint256) {
        return state().feeRate;
    }

    function allNFTs(uint256 id) public view returns (NFT memory) {
        return state().allNFTs[id];
    }

    function nftIdMap(address tokenAddress, uint256 tokenId) public view returns (uint256) {
        return state().nftIdMap[tokenAddress][tokenId];
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.7.6;
pragma experimental ABIEncoderV2;

library LibGachaMachine {
    bytes32 constant GACHAMACHINE_STORAGE_POSITION =
        keccak256("diamond.standard.gachamachine.storage");

    function gachaMachineStorage()
        internal
        pure
        returns (GachaMachineStorage storage ws)
    {
        bytes32 position = GACHAMACHINE_STORAGE_POSITION;
        assembly {
            ws.slot := position
        }
    }
}

struct GachaMachineStorage {
    uint256 feeRate;
    address payable feeTo;
    uint256 nextNFTId;
    uint256 nextCollectionId;

    // nftId => NFT
    mapping(uint256 => NFT) allNFTs;
    // owner => nftId[]
    mapping(address => uint256[]) nftsByOwner;
    // tokenAddress => tokenId => nftId
    mapping(address => mapping(uint256 => uint256)) nftIdMap;
    // collectionId => Collection
    mapping(uint256 => Collection) allCollections;
    // owner => collectionId[]
    mapping(address => uint256[]) collectionsByOwner;
    // collectionId => nftId[]
    mapping(uint256 => uint256[]) nftsByCollectionId;
}

struct NFT {
    uint256 id;
    address tokenAddress;
    uint256 tokenId;
    address payable owner;
    uint256 price;
    uint256 collectionId;
    uint256 indexInCollection;
    uint256 claimAt;
}

struct Collection {
    uint256 id;
    address payable owner;
    string name;
    uint256 size;
    string[] detail;
    uint256 publishedAt; // time that published.
    uint256 soldCount;
    uint256 flags;
    address nftCoin;
    uint256 nftPrice;
}

