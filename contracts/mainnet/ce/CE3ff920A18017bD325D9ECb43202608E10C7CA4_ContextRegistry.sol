import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./ContextToken.sol";
import "./ContextFees.sol";
import "./ContextDocument.sol";
// import "hardhat/console.sol";

contract ContextRegistry is AccessControl {
    using Strings for uint256;
    string public constant ctx_marketplace = "ctx_marketplace";

    // Settings.
    enum VersionType { MAJOR, MINOR, PATCH }
    uint public currentEpoch;
    ContextToken public token;
    address public upgrader;

    bytes32 public constant CURATOR_ROLE = keccak256("CURATOR_ROLE");
    bytes32 public constant UPGRADE_ROLE = keccak256("UPGRADE_ROLE");
    bytes32 public constant THANOS_ROLE = keccak256("THANOS_ROLE");

    event DEPOSIT(address, string, uint256);
    event WITHDRAW(address, string, uint256);
    event SENT(address, string, string, uint256);
    event NEWEPOCH(uint, address, ContextFees);

    // Epochs.
    struct Epoch {
        address contextNFT;
        ContextFees contextFees;
    }
    mapping (uint => Epoch) public epochs;
    
    // Documents.
    struct Document {
        address owner;
        uint256 tokens;
        address document;
    }

    mapping (bytes32 => Document) public registry;
    event NEWOWNER(string, address);
    event REGISTERED(string _name, address _owner, address _curator);
    event PUSHED(string _type, string _name, string _subname, string _patch);
    event SNAP(address, string);

    // Constructor.
    constructor(
        ContextToken _token
    ) {
        token = _token;
        currentEpoch = 0;
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(UPGRADE_ROLE, msg.sender);
    }

    // Helpers.
    function isValidName(string memory str) internal pure returns (bool) {
        bytes memory b = bytes(str);
        if (b.length < 3) return false;

        // Ensure the first character is a lowercase letter
        bytes1 firstChar = b[0];
        if (!(firstChar >= bytes1('a') && firstChar <= bytes1('z'))) {
            return false;
        }

        for (uint i = 1; i < b.length; i++) {
            bytes1 char = b[i];
            bool isLowercaseLetter = (char >= bytes1('a') && char <= bytes1('z'));
            bool isDigit = (char >= bytes1('0') && char <= bytes1('9'));
            bool isUnderscore = (char == bytes1('_'));

            if (!(isLowercaseLetter || isDigit || isUnderscore)) {
                return false;
            }
        }
        return true;
    }
    // Deletes a level1.
    function snap(string memory name) public {
        require(hasRole(THANOS_ROLE, msg.sender), "Only Thanos has the power");
        bytes32 level1 = keccak256(bytes(name));
        require(registry[level1].owner != address(0), "Document must exist");
        delete(registry[level1]);
        emit SNAP(msg.sender, name);
    }

    // CHanges the epoch and updates fees.
    function changeEpoch(
        address contextNFT,
        ContextFees contextFees
    ) public onlyRole(UPGRADE_ROLE) {
        
        currentEpoch = currentEpoch + 1;
        
        // Set Governance NFT and Fees.
        epochs[currentEpoch].contextNFT = contextNFT;
        epochs[currentEpoch].contextFees = contextFees;
        
        emit NEWEPOCH(currentEpoch, contextNFT, contextFees);
    }

    // Changes the owner of a level1.
    function changeOwner(
        string memory name,
        address newOnwer
    ) public {
        Document storage level1 = registry[keccak256(bytes(name))];
        require(level1.owner != address(0), "Document must exist");
        require(level1.owner == msg.sender, "Only owner can update owner");
        level1.owner = newOnwer;
        emit NEWOWNER(name, newOnwer);
    }


    function register(
        string calldata name,
        address owner
    ) public  {
        require(hasRole(CURATOR_ROLE, msg.sender), "Caller is not a Curator");
        require(isValidName(name), 'Invalid name');
        bytes32 level1 = keccak256(bytes(name));
        ContextDocument doc = new ContextDocument(address(this));
        registry[level1].document = address(doc);
        registry[level1].owner = owner;
        registry[level1].tokens = 0;
        emit REGISTERED(name, owner, msg.sender);
    }

    function read(
        string calldata name
    ) public view returns (address, address){
        Document storage level1 = registry[keccak256(bytes(name))];
        require(level1.owner != address(0), "Document must exist");
        return( level1.owner, level1.document);
    }

    function buyCTXWithMatic(bytes32 level1) private {
        // Amount of CTX to Buy.
        uint256 amount = epochs[currentEpoch].contextFees.buyCtx(msg.value, level1);
        require(amount > 0, "No more tokens can be bought");

        // Marketplace.
        Document storage marketplace = registry[keccak256(bytes(ctx_marketplace))];
        require(marketplace.tokens >= amount, "Marketplace does not have enough tokens");
        payable(marketplace.owner).transfer(msg.value);

        // Transfer CTX.
        Document storage userDoc = registry[level1];
        marketplace.tokens -= amount;
        userDoc.tokens += amount;
    }

    function payFees(bytes32 level1, string memory frontend) internal returns (bool) {
        if (currentEpoch == 0) return true;

        if (msg.value > 0) {
            buyCTXWithMatic(level1);
        }

        (string[] memory recipients,uint256[] memory amounts)
            = epochs[currentEpoch].contextFees.calculateFees(frontend);

        // make payments
        uint total = 0;
        
        for (uint i = 0; i < recipients.length; i++) {
            if (amounts[i] > 0) {
              Document storage recipient = registry[keccak256(bytes(recipients[i]))];
              if (registry[level1].owner == address(0)) {
                  return false;
              }
              registry[level1].tokens = registry[level1].tokens - amounts[i];
              recipient.tokens = recipient.tokens + amounts[i];
              total = total + amounts[i];
            }
        }
        return (registry[level1].tokens >= 0);
    }

    function pushMajor(
        string memory name,
        string memory subname,
        string memory major,
        string memory minor,
        string memory patch,
        string memory frontend
    ) public payable {
        bytes32 level1 = keccak256(bytes(name));
        require(registry[level1].owner > address(0), "Name not registered");
        require(msg.sender == registry[level1].owner, "Caller is not the owner");
        require(payFees(level1, frontend) == true, "Failed to pay tips");
        ContextDocument document = ContextDocument(registry[level1].document);
        document.pushMajor( subname, major, minor, patch);
        emit PUSHED("Major", name, subname, patch);
    }

    function pushMinor(
        string memory name,
        string memory subname,
        string memory minor,
        string memory patch,
        string memory frontend
    ) public payable {
        bytes32 level1 = keccak256(bytes(name));
        require(registry[level1].owner > address(0), "Name not registered");
        require(msg.sender == registry[level1].owner, "Caller is not the owner");
        require(payFees(level1, frontend) == true, "Failed to pay tips");
        ContextDocument document = ContextDocument(registry[level1].document);
        document.pushMinor( subname, minor, patch);
        emit PUSHED("Minor", name, subname, patch);
    }

    function pushPatch(
        string memory name,
        string memory subname,
        string memory patch,
        string memory frontend
    ) public payable {
        bytes32 level1 = keccak256(bytes(name));
        require(registry[level1].owner > address(0), "Name not registered");
        require(msg.sender == registry[level1].owner, "Caller is not the owner");
        require(payFees(level1, frontend) == true, "Failed to pay tips");
        ContextDocument document = ContextDocument(registry[level1].document);
        document.pushPatch( subname, patch);
        emit PUSHED("Patch", name, subname, patch);
    }

    // Deposit tokens into the name's balance
    function deposit(string memory name, uint256 amount) public {
        Document storage level1 = registry[keccak256(bytes(name))];
        require(level1.owner != address(0), "Document does not exist");
        require(ContextToken(token).transferFrom(msg.sender, address(this), amount), "Transfer failed");
        level1.tokens += amount;
        emit DEPOSIT(msg.sender, name, amount);
    }

    // Withdraw tokens from the name's balance
    function withdraw(string memory name, uint256 amount) public {
        Document storage level1 = registry[keccak256(bytes(name))];
        require(level1.owner != address(0), "Document does not exist");
        require(level1.owner == msg.sender, "Only name owner can withdraw tokens");
        require(level1.tokens >= amount, "Insufficient balance");

        require(ContextToken(token).transfer(level1.owner, amount), "Transfer failed");
        level1.tokens -= amount;
        emit WITHDRAW(msg.sender, name, amount);
    }

    function balanceOf(string memory name) public view returns (uint256) {
        Document storage level1 = registry[keccak256(bytes(name))];
        require(level1.owner != address(0), "Document does not exist");
        return level1.tokens;
    }

    function send(string memory _from, string memory _to, uint256 amount) public {
        Document storage from = registry[keccak256(bytes(_from))];
        Document storage to = registry[keccak256(bytes(_to))];
        require(from.owner != address(0), "From must exist");
        require(from.owner == msg.sender, "User must be the owner");
        require(to.owner != address(0), "To must exist");
        
        require(from.tokens > amount && amount > 0, "Inavlid amount");
        from.tokens -= amount;
        to.tokens += amount;
        emit SENT(msg.sender, _from, _to, amount);
    }
}

