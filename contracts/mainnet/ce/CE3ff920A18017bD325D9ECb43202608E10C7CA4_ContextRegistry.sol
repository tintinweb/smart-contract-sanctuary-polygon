    uint256 public feeStaking;
    uint256 public feeFrontend;
    uint256 public ctxMaticPrice;
    uint256 public ctxMaxVolume;

    mapping (bytes32 => uint256) public volume;

    constructor (address _ctxRegistry) {
        ctxRegistry = _ctxRegistry;
    }

    function setPrices(
        uint256 _feeStaking,
        uint256 _feeSchemas,
        uint256 _feeFrontend,
        uint256 _ctxMaticPrice,
        uint256 _ctxMaxVolume
    ) public onlyOwner {
        feeStaking =_feeStaking;
        feeSchemas = _feeSchemas;
        feeFrontend = _feeFrontend;
        ctxMaticPrice = _ctxMaticPrice;
        ctxMaxVolume = _ctxMaxVolume;
    }

    function buyCtx(uint256 matic, bytes32 level1) public returns (uint256) {
        require(msg.sender == ctxRegistry, "Only registry can call buyCtx");
        uint256 amount = matic * 10 ** 18 / ctxMaticPrice;
        volume[level1] = volume[level1] + amount;
        require(volume[level1] <= ctxMaxVolume, "Max Volume reached");
        return amount;
    }

    function calculateFees(string memory frontend) public view returns
        (string[] memory, uint256[] memory) {
        string[] memory finalRecipients = new string[](3);
        uint256[] memory amounts = new uint256[](3);

        // Adding fixed recipients
        finalRecipients[0] = "ctx_schemas";
        amounts[0] = feeSchemas;

        finalRecipients[1] = "ctx_staking";
        if (bytes(frontend).length == 0) { // If frontend string is empty, add frontend fee to staking fee
            amounts[1] = feeStaking + feeFrontend;
            finalRecipients[2] = "";
            amounts[2] = 0;
        } else {
            amounts[1] = feeStaking;
            finalRecipients[2] = frontend;
            amounts[2] = feeFrontend;
        }

        return (finalRecipients, amounts);
    }
}

