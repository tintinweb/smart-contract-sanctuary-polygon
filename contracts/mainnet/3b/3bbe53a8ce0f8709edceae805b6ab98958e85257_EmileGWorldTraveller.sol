  function addWhitelistParams(WhitelistParams[] memory _whitelistParams)
    external
    onlyAdmin
    returns (bool)
  {
    require(_whitelistParams.length > 0, throwError(INVALID_WHITELIST_PARAMS));

    for (uint256 i = 0; i < _whitelistParams.length; ++i) {
      require(
        _whitelistParams[i].startTime < _whitelistParams[i].endTime,
        throwError(WHITELIST_STARTTIME_MUST_BE_LESS_THAN_ENDTIME)
      );
      whitelistParams[whitelistsTracker] = _whitelistParams[i];
      whitelistsTracker += 1;
    }

    return true;
  }

  /**
   * @dev Modifies a whitelist configuration that restricts mint function
   * @param _whitelistParams whitelist defined as per WhiteListParams
   * @param _index Index to determine which whitelist is modified
   * struct.
   * Requirements:
   * {msg.sender} must have ADMIN_ROLE
   */
  function modifyWhitelistParams(
    WhitelistParams memory _whitelistParams,
    uint256 _index
  ) external onlyAdmin returns (bool) {
    require(whitelistsTracker > _index, throwError(INVALID_WHITELIST_PARAMS));
    require(
      _whitelistParams.startTime < _whitelistParams.endTime,
      throwError(WHITELIST_STARTTIME_MUST_BE_LESS_THAN_ENDTIME)
    );
    whitelistParams[_index] = _whitelistParams;

    return true;
  }

