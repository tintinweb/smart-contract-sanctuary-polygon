  function addSecretNftURIs(string[] memory _nftURIs)
    external
    onlyAdmin
    returns (uint256)
  {
    require(
      _nftURIs.length > 0,
      throwError(QUANTITY_MUST_BE_GREATER_THAN_EQUAL_TO_ONE)
    );
    require(
      _secretNftURIIdTracker + _nftURIs.length <= TOKEN_LIMIT,
      throwError(EXCEEDED_TOKEN_LIMIT)
    );
    for (uint256 i = 0; i < _nftURIs.length; ++i) {
      _secretNftURI[_secretNftURIIdTracker] = _nftURIs[i];
      _secretNftURIIdTracker += 1;
