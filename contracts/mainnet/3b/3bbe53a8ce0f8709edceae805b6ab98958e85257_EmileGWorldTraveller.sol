  function getRandomNumber(uint256 _modBy, uint256 seed)
    private
    view
    returns (uint256)
  {
    return
      uint256(keccak256(abi.encodePacked(block.timestamp, msg.sender, seed))) %
      _modBy;
  }

  /**
   * @dev Payable function to mint an NFT
   * @param quantity Number of tokens to be minted
   * @param proof array of computed proof hashes to check against
   * the whitelist. Can be sent empty if no whitelist active.
   *  
   * Requirements:
   * {quantity} should be less than total supply
   */
  function mint(uint256 quantity, bytes32[] memory proof)
    external
    whenNotPaused
    nonReentrant
  {
    require(
      totalSupply() + quantity < TOKEN_LIMIT,
      throwError(EXCEEDED_TOKEN_LIMIT)
    );
    require(
      balanceOf(msg.sender) + quantity <= MAX_TOKENS_PER_USER,
      throwError(EXCEEDED_INDIVIDUAL_TOKEN_LIMIT)
    );
    uint256 id = ERC721AStorage.layout()._currentIndex;
    require(
      bytes(_secretNftURI[quantity - 1]).length > 0,
      throwError(URI_NOT_SET)
    );

    bytes32 merkleRoot = 0x00;

    uint256 whitelistId = whitelistsTracker;
    for (uint256 i = 0; i < whitelistId; ++i) {
      if (
        block.timestamp >= whitelistParams[i].startTime &&
        block.timestamp <= whitelistParams[i].endTime
      ) {
        merkleRoot = whitelistParams[i].merkleRoot;
        break;
      }
    }

    if (merkleRoot != 0x00) {
      require(
        verifyWalletWhitelist(
          proof,
          merkleRoot,
          keccak256(abi.encodePacked(msg.sender))
        ),
        throwError(ADDRESS_NOT_WHITELISTED)
      );
    }
    uint256 netAmount = traxxFees*quantity;
    uint256 creatorFee = (netAmount*90)/100;
    uint256 remainingFee = netAmount - creatorFee;

    // transfer funds to creator
    validateAndTransferFees(msg.sender, creatorAddress, creatorFee);
    // store remaining funds
    validateAndTransferFees(msg.sender, address(this), remainingFee);

    uint256 currentIndex = balanceOf(msg.sender);
    _safeMint(msg.sender, quantity);
    
    uint256 indexAsSeed;
    for (uint256 i = 0; i < quantity; ++i) {
      uint256 randURIId = getRandomNumber((_secretNftURIIdTracker==1)? 1: _secretNftURIIdTracker-1, indexAsSeed);
      indexAsSeed += 1;
      nftURI[id] = _secretNftURI[randURIId];
      if(randURIId!=(_secretNftURIIdTracker-1))
        _secretNftURI[randURIId] = _secretNftURI[_secretNftURIIdTracker-1];
      delete _secretNftURI[_secretNftURIIdTracker-1];
      _secretNftURIIdTracker--;
      firstOwner[id] = msg.sender;
      _ownedTokens[msg.sender][currentIndex] = id;
      id += 1;
      currentIndex += 1;
    }

    emit TraxxStemzMinted(msg.sender, quantity);
  }

  /**
   * @dev Validates traxx fees balance and allowance, transfers it.
   * @param from address of account from which traxx fees will be deducted
   * @param amount of tokens to transfer
   * Requirement:
   * Adequate balance and allowance for this contract to transfer the fees
   */
  function validateAndTransferFees(address from, address to, uint256 amount) internal {
    require(
        IERC20(tokenTraxxAddress).balanceOf(from) >= amount &&
          IERC20(tokenTraxxAddress).allowance(from, address(this)) >= amount,
        throwError(INSUFFICIENT_BALANCE_OR_ALLOWANCE)
    );
    uint256 balBefore = IERC20(tokenTraxxAddress).balanceOf(to);
    bool success = IERC20(tokenTraxxAddress).transferFrom(
        from,
        to,
        amount
    );
    uint256 balAfter = IERC20(tokenTraxxAddress).balanceOf(to);
    require(success && balAfter == balBefore + amount, throwError(TRANSFER_FAILED));
  }

