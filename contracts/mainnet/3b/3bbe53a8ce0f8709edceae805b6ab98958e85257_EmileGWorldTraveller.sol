   */
  function deleteWhitelistParams(uint256 _index)
    external
    onlyAdmin
    returns (bool)
  {
    require(whitelistsTracker > _index, throwError(INVALID_WHITELIST_PARAMS));
    delete whitelistParams[_index];

    return true;
  }

