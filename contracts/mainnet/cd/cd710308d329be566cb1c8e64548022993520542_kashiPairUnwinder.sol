pragma solidity 0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/IERC20MetadataUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "./BaseDepositUnwinder.sol";
import "../interfaces/sushi/IBentoBoxV1.sol";

contract kashiPairUnwinder is BaseDepositUnwinder {
    using SafeERC20Upgradeable for IERC20MetadataUpgradeable;

    string public override NAME;

    function initialize() external initializer {
        BaseDepositUnwinder.__initialize();
        NAME = "kashiPair Deposit Unwinder";
    }

    function unwind(address token, address beneficiary)
        external
        override
        onlyRole(VAULT_ROLE)
        whenNotPaused
        nonReentrant
        returns (uint256 underlyingAmnt)
    {
        require(underlyingToken[token] != address(0), "token is not supported");
        require(underlyingToken[token] == address(IKashiPair(token).asset()), "unsuported asset token");
        require(IKashiPair(token).balanceOf(address(this)) > 0, "no token is held by unwinder");

        IKashiPair kashiPairToken = IKashiPair(token);
        IERC20 underlying = kashiPairToken.asset();
        IBentoBox bentoBox = IBentoBox(kashiPairToken.bentoBox());

        // get the balance of underlying before redeem, for return & emit value
        uint256 tokenAmnt = kashiPairToken.balanceOf(address(this));
        require(underlying.balanceOf(address(this)) == 0, "there are already underlying held by this contract");

        // withdraw all kashiPairTokens in this contract
        kashiPairToken.removeAsset(address(this), tokenAmnt);
        uint256 bentoAssetShares = bentoBox.balanceOf(underlying, address(this));

        // check if all kashiPairTokens are withdrawn
        require(
            kashiPairToken.balanceOf(address(this)) == 0,
            "kashiPairUnwind: Failed to withdraw all tokens from Kashi Pair"
        );
        require(bentoAssetShares > 0, "kashiPairUnwind: Failed to withdraw all tokens from Kashi Pair");

        // withdraw all underlying from bentobox
        uint256 bentoAssetAmnt = bentoBox.toAmount(underlying, bentoAssetShares, false);
        (underlyingAmnt, ) = bentoBox.withdraw(underlying, address(this), beneficiary, bentoAssetAmnt, 0);

        // TODO: checking

        emit DepositTokenUnwound(token, address(underlying), tokenAmnt, underlyingAmnt);
    }

    function exchangeRate(address token) external view override returns (uint256) {
        require(underlyingToken[token] != address(0), "token is not supported");
        require(underlyingToken[token] == address(IKashiPair(token).asset()), "unsuported asset token");

        IKashiPair kashiPairToken = IKashiPair(token);

        AccrueInfo memory accrueInfo = kashiPairToken.accrueInfo();
        Rebase memory totalBorrow = kashiPairToken.totalBorrow();
        Rebase memory totalAsset = kashiPairToken.totalAsset();

        (uint128 bentoAmount, uint128 bentoShare) = IBentoBox(kashiPairToken.bentoBox()).totals(kashiPairToken.asset());

        uint256 elapsedSeconds = block.timestamp - accrueInfo.lastAccrued;

        uint256 currentBorrowAmount = ((totalBorrow.elastic * accrueInfo.interestPerSecond * elapsedSeconds) / (1e18)) +
            totalBorrow.elastic;

        uint256 totalAssetAmount = (totalAsset.elastic * bentoAmount) / bentoShare;

        uint256 currentAllAssets = totalAssetAmount + currentBorrowAmount;

        return ((1e18 * currentAllAssets) / totalAsset.base);
    }
}

