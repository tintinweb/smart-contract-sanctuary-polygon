/**
 * @title BaseDepositUnwinder Abstract Contract
 * @notice Abstract Contract to be inherited by instances of IDepositUnwinder.
 * Implements utility functions.
 */

import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "../interfaces/IDepositUnwinder.sol";

abstract contract BaseDepositUnwinder is
    IDepositUnwinder,
    AccessControlEnumerableUpgradeable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable
{
    using SafeERC20Upgradeable for IERC20Upgradeable;

    // roles
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant VAULT_ROLE = keccak256("VAULT_ROLE");
    bytes32 public constant SWEEPER_ROLE = keccak256("SWEEPER_ROLE");

    // overridden from interface
    mapping(address => address) public override underlyingToken;

    address[] depositTokens;

    function __initialize() internal {
        AccessControlEnumerableUpgradeable.__AccessControlEnumerable_init();
        PausableUpgradeable.__Pausable_init_unchained();
        ReentrancyGuardUpgradeable.__ReentrancyGuard_init_unchained();
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    /** Pausable */
    function pause() external onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() external onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function addDepositToken(address depositTokenAddr, address underlying)
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
        whenNotPaused
    {
        require(depositTokenAddr != address(0), "zero deposit token address");
        require(underlying != address(0), "zero underlying token address");
        require(underlyingToken[depositTokenAddr] == address(0), "token already registered");

        underlyingToken[depositTokenAddr] = underlying;

        depositTokens.push(depositTokenAddr);

        emit DepositTokenAdded(depositTokenAddr, underlying);
    }

    function removeDepositToken(address depositTokenAddr) external override onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        require(depositTokenAddr != address(0), "zero deposit token address");
        require(underlyingToken[depositTokenAddr] != address(0), "token is not registered");

        address underlying = underlyingToken[depositTokenAddr];
        delete underlyingToken[depositTokenAddr];

        uint256 len = depositTokens.length;
        for (uint256 i = 0; i < len; i++) {
            if (depositTokens[i] == depositTokenAddr) {
                emit DepositTokenRemoved(depositTokenAddr, underlying);

                depositTokens[i] = depositTokens[len - 1];
                depositTokens.pop();
                return;
            }
        }
    }

    function setDepositToken(address depositToken, address underlying)
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
        whenNotPaused
    {
        require(depositToken != address(0), "zero deposit token address");
        require(underlying != address(0), "zero underlying token address");
        require(underlyingToken[depositToken] != address(0), "token is not registered");

        underlyingToken[depositToken] = underlying;
        emit DepositTokenSet(depositToken, underlying);
    }

    function sweepERC20Token(address token, address to) external override onlyRole(SWEEPER_ROLE) whenNotPaused {
        require(token != address(0), "zero token address");
        require(underlyingToken[token] == address(0), "!safe");
        uint256 len = depositTokens.length;
        for (uint256 i = 0; i < len; i++) {
            require(underlyingToken[depositTokens[i]] != token, "!safe");
        }

        _sweepERC20Token(token, to);
    }

    function _sweepERC20Token(address token, address to) internal {
        IERC20Upgradeable tokenToSweep = IERC20Upgradeable(token);
        tokenToSweep.safeTransfer(to, tokenToSweep.balanceOf(address(this)));
    }
}

