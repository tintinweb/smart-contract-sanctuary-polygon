error InvalidTokenId();
error TokenNotTransferrable();
error MismatchedArraySizes();

contract InPeakSoulboundV2 is
  ERC721AUpgradeable,
  OwnableUpgradable,
  AdministrableUpgradable
{
  bool public transferrable;

  function initialize(
    string memory tokenName_,
    string memory tokenSymbol_,
    string memory baseImageURI_
  ) public initializerERC721A {
    __ERC721A_init(tokenName_, tokenSymbol_);
    __Ownable_init();
    __Administrable_init();

    baseImageURI = baseImageURI_;

    externalURL = "https://www.inpeak.xyz/nfts";
    tokenDescription = "InPeak is building the 'LinkedIn/Masterclass' of Web3. With live sessions and on-demand content covering all aspects of Web3 education, featuring industry leaders including Zeneca, Camila Russo, Laura Shin, and Gmoney to name a few. InPeak Trailhead gives you a taste of the platform and access to our discord before you start your full journey from basecamp to your peak. Simply login to inpeak.xyz with your Trailhead token and join at least one live session to qualify for our future token airdrop.";
    tokenName = "Trailhead";

    transferrable = false;
  }

  function burn(uint256 tokenId) public virtual {
    _burn(tokenId, true);
  }

  function revoke(uint256 _tokenId) external onlyOperatorsAndOwner {
    _burn(_tokenId);
  }

  function revokeBatch(
    uint256[] calldata _tokenIds
  ) external onlyOperatorsAndOwner {
    for (uint256 i; i < _tokenIds.length; ) {
      _burn(_tokenIds[i]);
      unchecked {
        ++i;
      }
    }
  }

  function tokensOfOwner(
    address owner
  ) external view virtual returns (uint256[] memory) {
    uint256 tokenIdsLength = balanceOf(owner);
    uint256[] memory tokenIds;
    assembly {
      // Grab the free memory pointer.
      tokenIds := mload(0x40)
      // Allocate one word for the length, and `tokenIdsMaxLength` words
      // for the data. `shl(5, x)` is equivalent to `mul(32, x)`.
      mstore(0x40, add(tokenIds, shl(5, add(tokenIdsLength, 1))))
      // Store the length of `tokenIds`.
      mstore(tokenIds, tokenIdsLength)
    }
    address currOwnershipAddr;
    uint256 tokenIdsIdx;
    for (uint256 i = _startTokenId(); tokenIdsIdx != tokenIdsLength; ) {
      TokenOwnership memory ownership = _ownershipAt(i);
      assembly {
        // if `ownership.burned == false`.
        if iszero(mload(add(ownership, 0x40))) {
          // if `ownership.addr != address(0)`.
          // The `addr` already has it's upper 96 bits clearned,
          // since it is written to memory with regular Solidity.
          if mload(ownership) {
            currOwnershipAddr := mload(ownership)
          }
          // if `currOwnershipAddr == owner`.
          // The `shl(96, x)` is to make the comparison agnostic to any
          // dirty upper 96 bits in `owner`.
          if iszero(shl(96, xor(currOwnershipAddr, owner))) {
            tokenIdsIdx := add(tokenIdsIdx, 1)
            mstore(add(tokenIds, shl(5, tokenIdsIdx)), i)
          }
        }
        i := add(i, 1)
      }
    }
    return tokenIds;
  }

  function expiredTokens(
    uint256 maxAge
  ) public view returns (uint256[] memory) {
    uint256 tokenIdsLength = _totalMinted();
    uint256 minIssuedAt = block.timestamp - maxAge;
    uint256 expiredCount;

    uint256[] memory tokenIds = new uint256[](tokenIdsLength);
    for (uint256 i = _startTokenId(); i <= tokenIdsLength; ) {
      TokenOwnership memory ownership = _ownershipAt(i);
      if (!ownership.burned) {
        if (ownership.startTimestamp < minIssuedAt) {
          tokenIds[expiredCount] = i;
          ++expiredCount;
        }
      }
      unchecked {
        ++i;
      }
    }

    uint256[] memory trimmedArr = new uint[](expiredCount);
    for (uint j = 0; j < trimmedArr.length; ) {
      trimmedArr[j] = tokenIds[j];
      unchecked {
        ++j;
      }
    }
    return trimmedArr;
  }

  function expiredTokens(
    uint256 maxAge,
    uint256 startIndex
  ) public view returns (uint256[] memory) {
    uint256 tokenIdsLength = _totalMinted();
    uint256 minIssuedAt = block.timestamp - maxAge;
    uint256 expiredCount;
    uint256 pageSize = 100;

    uint256 endIndex = startIndex + pageSize > tokenIdsLength
      ? tokenIdsLength
      : startIndex + pageSize;

    uint256[] memory tokenIds = new uint256[](endIndex - startIndex);
    for (uint256 i = _startTokenId() + startIndex; i < endIndex; ) {
      TokenOwnership memory ownership = _ownershipAt(i);
      if (!ownership.burned) {
        if (ownership.startTimestamp < minIssuedAt) {
          tokenIds[expiredCount] = i;
          ++expiredCount;
        }
      }
      unchecked {
        ++i;
      }
    }

    uint256[] memory trimmedArr = new uint256[](expiredCount);
    for (uint j = 0; j < expiredCount; ) {
      trimmedArr[j] = tokenIds[j];
      unchecked {
        ++j;
      }
    }
    return trimmedArr;
  }

  // ------ AIRDROPS ----------------------------------------------------------

  function airdrop(
    uint256 _count,
    address _recipient
  ) external onlyOperatorsAndOwner {
    _mint(_recipient, _count);
  }

  function airdropBatch(
    uint256[] calldata _counts,
    address[] calldata _recipients
  ) external onlyOperatorsAndOwner {
    if (_counts.length != _recipients.length) {
      revert MismatchedArraySizes();
    }

    for (uint256 i; i < _recipients.length; ) {
      _mint(_recipients[i], _counts[i]);
      unchecked {
        ++i;
      }
    }
  }

  function flipTransferrable() public onlyOperatorsAndOwner {
    transferrable = !transferrable;
  }

  // ------ TOKEN METADATA ----------------------------------------------------

  string private baseImageURI;
  string private externalURL;
  string private tokenDescription;
  string private tokenName;

  function getBaseImageURI() public view returns (string memory) {
    return baseImageURI;
  }

  function getExternalURL() public view returns (string memory) {
    return externalURL;
  }

  function getTokenDescription() public view returns (string memory) {
    return tokenDescription;
  }

  function getTokenName() public view returns (string memory) {
    return tokenName;
  }

  function setBaseImageURI(
    string memory _baseImageURI
  ) external onlyOperatorsAndOwner {
    baseImageURI = _baseImageURI;
  }

  function setExternalURL(
    string memory _externalURL
  ) external onlyOperatorsAndOwner {
    externalURL = _externalURL;
  }

  function setTokenDescription(
    string memory _tokenDescription
  ) external onlyOperatorsAndOwner {
    tokenDescription = _tokenDescription;
  }

  function setTokenName(
    string memory _newTokenName
  ) external onlyOperatorsAndOwner {
    tokenName = _newTokenName;
  }

  function _startTokenId() internal view virtual override returns (uint256) {
    return 1;
  }

  function _tokenName(
    uint256 tokenId
  ) internal view virtual returns (string memory) {
    return
      string(abi.encodePacked(tokenName, " #", Utils.uintToString(tokenId)));
  }

  function getIssueDate(uint256 tokenId) public view returns (uint256) {
    return _ownershipOf(tokenId).startTimestamp;
  }

  function tokenURI(
    uint256 tokenId
  ) public view virtual override(ERC721AUpgradeable) returns (string memory) {
    string memory attributes;
    attributes = string(
      abi.encodePacked(
        '[ { "trait_type": "Issue Date", "display_type": "date", "value": "',
        Utils.uintToString(_ownershipOf(tokenId).startTimestamp),
        '"}]'
      )
    );

    return
      string(
        abi.encodePacked(
          "data:application/json;base64,",
          Base64.encode(
            bytes(
              abi.encodePacked(
                '{"name": "',
                _tokenName(tokenId),
                '", "description": "',
                tokenDescription,
                '", "external_url": "',
                externalURL,
                '", "image":"',
                _imageURI(tokenId),
                '", "attributes":',
                attributes,
                "}"
              )
            )
          )
        )
      );
  }

  function _imageURI(uint256 tokenId) internal view returns (string memory) {
    return
      string.concat(baseImageURI, "?tokenId=", Utils.uintToString(tokenId));
  }

  function supportsInterface(
    bytes4 interfaceId
  )
    public
    view
    override(ERC721AUpgradeable, AccessControlEnumerable)
    returns (bool)
  {
    return ERC721AUpgradeable.supportsInterface(interfaceId);
  }

  function approve(
    address to,
    uint256 tokenId
  ) public virtual override isTransferrable {
    ERC721AUpgradeable.approve(to, tokenId);
  }

  function setApprovalForAll(
    address operator,
    bool approved
  ) public virtual override isTransferrable {
    ERC721AUpgradeable.setApprovalForAll(operator, approved);
  }

  function transferFrom(
    address from,
    address to,
    uint256 tokenId
  ) public override(ERC721AUpgradeable) isTransferrable {
    ERC721AUpgradeable.transferFrom(from, to, tokenId);
  }

  function safeTransferFrom(
    address from,
    address to,
    uint256 tokenId,
    bytes memory _data
  ) public override(ERC721AUpgradeable) isTransferrable {
    ERC721AUpgradeable.safeTransferFrom(from, to, tokenId, _data);
  }

  modifier isTransferrable() {
    if (!transferrable) revert TokenNotTransferrable();
    _;
  }
}

