pragma solidity ^0.8.0;

import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {SafeERC20} from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import {IStrategPortal} from "@murphy-labs/strateg-protocol-portal/contracts/interfaces/IStrategPortal.sol";
import {IStrategVault} from "@murphy-labs/strateg-protocol-core/contracts/interfaces/IStrategVault.sol";

import {IPool} from "@aave/core-v3/contracts/interfaces/IPool.sol";
import {IAaveOracle} from "@aave/core-v3/contracts/interfaces/IAaveOracle.sol";
import {DataTypes} from "@aave/core-v3/contracts/protocol/libraries/types/DataTypes.sol";
import {PercentageMath} from '@aave/core-v3/contracts/protocol/libraries/math/PercentageMath.sol';
import {WadRayMath} from '@aave/core-v3/contracts/protocol/libraries/math/WadRayMath.sol';
import {ReserveConfiguration} from '@aave/core-v3/contracts/protocol/libraries/configuration/ReserveConfiguration.sol';

import {IStrategPositionManager} from '../interfaces/IStrategPositionManager.sol';
import {IStrategAaveV3PositionManager} from '../interfaces/IStrategAaveV3PositionManager.sol';

import "hardhat/console.sol";

contract StrategAaveV3PositionManager is IStrategAaveV3PositionManager, IStrategPositionManager {
    using SafeERC20 for IERC20;
    using WadRayMath for uint256;
    using PercentageMath for uint256;
    using ReserveConfiguration for DataTypes.ReserveConfigurationMap;
    

    uint16 constant REFERAL = 57547;

    IPool public immutable pool;
    IAaveOracle public immutable oracle;
    IStrategPortal public immutable portal;
    address public immutable operator;

    bool public initialized;
    address public owner;
    uint256 public blockIndex;
    Position public position;

    constructor(
        address _pool, 
        address _oracle, 
        address _portal,
        address _operator
    ) {
        pool = IPool(_pool);
        oracle = IAaveOracle(_oracle);
        portal = IStrategPortal(_portal);
        operator = _operator;
    }

    modifier onlyOperator() {
        if(msg.sender != operator) revert NotOperator();
        _;
    }

    modifier onlyOwner() {
        if(msg.sender != owner) revert NotOwner();
        _;
    }

    modifier onlyBorrow() {
        if(position.leverageMode) revert BorrowOnly();
        _;
    }

    modifier onlyLeverage() {
        if(!position.leverageMode) revert LeverageOnly();
        _;
    }

    function initialize(
        address _owner,
        uint256 _blockIndex,
        bytes calldata _params
    ) external {
        if (initialized) revert AlreadyInitialized();
        initialized = true;
        owner = _owner;
        blockIndex = _blockIndex;

        InitializationParams memory initParams = abi.decode(_params, (InitializationParams));
        DataTypes.ReserveData memory collateralReserveData = pool.getReserveData(address(initParams.collateral));      
        DataTypes.ReserveData memory deptReserveData = pool.getReserveData(address(initParams.borrowed));
        
        uint256 ltv;
        uint256 lts;
        IAaveOracle orc;
        if(initParams.eModeCategoryId == 0) {
            //If eMode is disabled
            ltv = collateralReserveData.configuration.getLtv();
            lts = collateralReserveData.configuration.getLiquidationThreshold();
            orc = oracle;
        } else {
            //If eMode is enabled
            pool.setUserEMode(position.eModeCategoryId);
            DataTypes.EModeCategory memory emode = pool.getEModeCategoryData(position.eModeCategoryId);
            ltv = uint256(emode.ltv);
            lts = uint256(emode.liquidationThreshold);
            orc = IAaveOracle(emode.priceSource);
        }

        position = Position({
            leverageMode: initParams.leverageMode,
            eModeCategoryId: initParams.eModeCategoryId,
            oracle: orc,
            collateral: Collateral({
                token: initParams.collateral,
                decimals: initParams.collateralDecimals,
                aToken: IERC20(collateralReserveData.aTokenAddress),
                ltv: ltv,
                lts: lts,
                cap: collateralReserveData.configuration.getSupplyCap() * 10**initParams.collateralDecimals

            }),
            dept: Dept({
                token: initParams.borrowed,
                deptToken: initParams.deptType == 1 ? IERC20(deptReserveData.stableDebtTokenAddress) : IERC20(deptReserveData.variableDebtTokenAddress),
                deptType: initParams.deptType,
                decimals: initParams.borrowedDecimals,
                cap: deptReserveData.configuration.getBorrowCap() * 10**initParams.borrowedDecimals
            }),
            healthfactor: Healthfactor({
                min: initParams.hfMin,
                max: initParams.hfMax,
                desired: initParams.hfDesired
            })
        });
    }

    function refreshAaveData() external {
        DataTypes.ReserveData memory collateralReserveData = pool.getReserveData(address(position.collateral.token));      
        DataTypes.ReserveData memory deptReserveData = pool.getReserveData(address(position.dept.token));

        if(position.eModeCategoryId == 0) {
            //If eMode is disabled
            position.collateral.ltv = collateralReserveData.configuration.getLtv();
            position.collateral.lts = collateralReserveData.configuration.getLiquidationThreshold();
        } else {
            //If eMode is enabled
            pool.setUserEMode(position.eModeCategoryId);
            DataTypes.EModeCategory memory emode = pool.getEModeCategoryData(position.eModeCategoryId);
            position.collateral.ltv = uint256(emode.ltv);
            position.collateral.lts = uint256(emode.liquidationThreshold);
            position.oracle = IAaveOracle(emode.priceSource);
        }

        position.collateral.cap = collateralReserveData.configuration.getSupplyCap() * 10**position.collateral.decimals;
        position.dept.cap = deptReserveData.configuration.getSupplyCap() * 10**position.dept.decimals;
    }

    function borrowStatus(bool _withSpecificAvailableTokenForRepay, uint256 _availableTokenForRepay) external view onlyBorrow returns (BorrowStatus memory status) {

        (,,,,,uint256 healthFactor) = pool.getUserAccountData(address(this));

        if(healthFactor == type(uint256).max) {
            status.emptyBorrow = true;
            return status;
        }

        status.healthFactor = healthFactor;
        status.healthFactorDesired = position.healthfactor.desired;
        status.toRebalance = healthFactor < position.healthfactor.min || healthFactor > position.healthfactor.max;

        uint256 availableTokenForRepay;
        if(_withSpecificAvailableTokenForRepay) {
            availableTokenForRepay = _availableTokenForRepay;
        } else {
            availableTokenForRepay = position.dept.token.balanceOf(address(this));
        }

        uint256 borrowedAmount = position.dept.deptToken.balanceOf(address(this));

        status.positionDeltaIsPositive = availableTokenForRepay >= borrowedAmount;
        status.availableTokenForRepay = availableTokenForRepay;

        if(status.positionDeltaIsPositive)
            status.deltaAmount = availableTokenForRepay - borrowedAmount;
        else 
            status.deltaAmount = borrowedAmount - availableTokenForRepay;

        if(status.toRebalance) {
            status.rebalanceAmountToRepay = _getRebalanceRepayAmount();
        }
    }

    function leverageStatus() external view onlyLeverage returns (LeverageStatus memory status) {
        (,,,,, uint256 healthFactor) = pool.getUserAccountData(address(this));

        if(healthFactor == type(uint256).max) {
            status.emptyLeverage = true;
            return status;
        }

        status.healthFactor = healthFactor;
        status.healthFactorDesired = position.healthfactor.desired;
        status.toRebalance = healthFactor < position.healthfactor.min || healthFactor > position.healthfactor.max;
        status.collateralAmount = position.collateral.aToken.balanceOf(address(this));
        status.deptAmount = position.dept.deptToken.balanceOf(address(this));
    }

    function borrowAmountFor(uint256 _collateral) external view onlyBorrow returns (uint256) {
        address[] memory assets = new address[](2);
        assets[0] = address(position.collateral.token);
        assets[1] = address(position.dept.token);
        uint256[] memory prices = oracle.getAssetsPrices(assets);

        uint256 collateralBase = _collateral * prices[0] / 10**position.collateral.decimals;
        uint256 toBorrowBase = collateralBase.percentMul(position.collateral.lts) * 1e18 / position.healthfactor.desired;
        uint256 borrowAmount = toBorrowBase * 10**position.dept.decimals / prices[1];

        return borrowAmount;
    }
    
    function returnedAmountAfterRepay(uint256 _repay) external view onlyBorrow returns (uint256) {
        address[] memory assets = new address[](2);
        assets[0] = address(position.collateral.token);
        assets[1] = address(position.dept.token);
        uint256[] memory prices = oracle.getAssetsPrices(assets);

        uint256 collateralBaseReturned;
        uint256 repayBase = _repay * prices[1] / 10**position.dept.decimals;
        (
            uint256 totalCollateralBase,
            uint256 totalDebtBase,,,,
        ) = pool.getUserAccountData(address(this));
        
        if(repayBase >= totalDebtBase) {
            collateralBaseReturned = totalCollateralBase + (repayBase - totalDebtBase);
        } else {
            collateralBaseReturned = totalCollateralBase - (totalDebtBase - repayBase);
        }

        uint256 collateralAmount = collateralBaseReturned * 10**position.collateral.decimals / prices[0];
        return collateralAmount;
    }
    
    function returnedAmountAfterUnleverage() external view onlyLeverage returns (uint256) {
        uint256 collateralPrice = oracle.getAssetPrice(address(position.collateral.token));

        (
            uint256 totalCollateralBase,
            uint256 totalDebtBase,,,,
        ) = pool.getUserAccountData(address(this));

        uint256 collateralBaseReturned = totalCollateralBase - totalDebtBase; 
        uint256 collateralAmount = collateralBaseReturned * 10**position.collateral.decimals / collateralPrice;
        return collateralAmount;
    }

    function borrow(uint256 _addCollateralAmount) external onlyOwner onlyBorrow {
        IERC20 collateral = position.collateral.token;

        /**
         * Transfer deposited collateral
         */
        if (_addCollateralAmount > 0)
            collateral.safeTransferFrom(
                msg.sender,
                address(this),
                _addCollateralAmount
            );

        /**
         * Deposit all liquidity available
         */
        {
            uint256 collateralBalance = collateral.balanceOf(address(this));
            if (collateralBalance > 0) {
                collateral.safeApprove(address(pool), collateralBalance);
                pool.supply(
                    address(collateral),
                    collateralBalance,
                    address(this),
                    REFERAL
                );
            }
                
        }

        _borrowToMatchHealthfactor();
    }
    
    function repay(bytes memory _dynamicParams) external onlyOwner onlyBorrow {
        IERC20 collateral = position.collateral.token;
        
        RepayDynamicParams memory params = abi.decode(
            _dynamicParams,
            (RepayDynamicParams)
        );

        uint256 amountToRepay = _getRepayAmount(address(position.dept.token));
        uint256 deptTokenBalance = position.dept.token.balanceOf(address(this));

        /**
         * If the amount to repay is not covered by the dept token
         * - estimate the collateral amount to sell
         * - withdraw the collateral
         * - sell the collateral for dept token
         */
        if (params.needSwap && amountToRepay > deptTokenBalance) {
            pool.withdraw(address(collateral), params.amount, address(this));

            collateral.safeApprove(address(portal), params.amount);
            portal.swap(
                false,
                false,
                params.route,
                params.approvalAddress,
                params.sourceAsset,
                params.targetAsset,
                params.amount,
                "",
                params.data
            );

            uint256 newDeptTokenBalance = position.dept.token.balanceOf(
                address(this)
            );

            if (amountToRepay > newDeptTokenBalance)
                revert RepayNotCoveredWithSwap();
        }

        /**
         * Execute repay
         */
        position.dept.token.safeApprove(address(pool), amountToRepay);
        pool.repay(address(position.dept.token), amountToRepay, position.dept.deptType, address(this));
        pool.withdraw(
            address(position.collateral.token),
            type(uint256).max,
            msg.sender
        );
        
        /**
         * If there is more dept token balance than repaid amount
         * swap remaining tokens to collateral token
         */
        if (params.needSwap && deptTokenBalance > amountToRepay) {
            position.dept.token.safeApprove(address(portal), params.amount);
            portal.swap(
                false,
                false,
                params.route,
                params.approvalAddress,
                params.sourceAsset,
                params.targetAsset,
                params.amount,
                "",
                params.data
            );

            /**
             * Return collateral to the vault
             */
            position.collateral.token.safeTransfer(
                msg.sender,
                position.collateral.token.balanceOf(address(this))
            );
        }
    }

    function leverage(uint256 _addCollateralAmount, bytes calldata _dynamicParams) external onlyOwner onlyLeverage {
        /**
         * Transfer deposited collateral
         */
        if (_addCollateralAmount > 0)
            position.collateral.token.safeTransferFrom(
                msg.sender,
                address(this),
                _addCollateralAmount
            );

        _leverage(_dynamicParams);
    }

    function _leverage(bytes memory _dynamicParams) internal {
        /**
         * Steps resume
         * - Calculate the amount to borrow
         * - Execute the flashloan
         *      - Swap borrowed assets to the collateral
         *      - Deposit collateral on pool
         */

        LeverageDynamicParams memory params = abi.decode(
            _dynamicParams,
            (LeverageDynamicParams)
        );

        /**
         * Execute the Flashloan
         */
        address[] memory assets = new address[](1);
        assets[0] = address(position.dept.token);

        uint256[] memory amounts = new uint256[](1);
        amounts[0] = params.flashloanAmount;

        uint256[] memory interestRateModes = new uint256[](1);
        interestRateModes[0] = position.dept.deptType;

        FlashloanData memory leverageParams = FlashloanData({
            callback: FlashloanCallbackType.LEVERAGE,
            data: _dynamicParams
        });

        pool.flashLoan(
            address(this),
            assets,
            amounts,
            interestRateModes,
            address(this),
            abi.encode(leverageParams),
            REFERAL
        );

        (, , , , , uint256 healthFactor) = pool.getUserAccountData(
            address(this)
        );

        if (healthFactor > position.healthfactor.max || healthFactor < position.healthfactor.min)
            revert LeverageHealthfactorNotMatch();
    }

    function unleverage(bytes memory _dynamicParams) external onlyOwner onlyLeverage {
        _unleverage(_dynamicParams);
    }

    function _unleverage(bytes memory _dynamicParams) internal {
        /**
         * Steps resume
         * - Calculate the amount to borrow to unleverage
         * - Execute the flashloan
         *      - Swap collateral flashloaned to repay the dept
         *      - Call repay with repayWithATokens(address asset,uint256 amount,uint256 interestRateMode) function
         *      - withdraw remining collateral
         *      - send collateral to the vault
         */
        UnleverageDynamicParams memory params = abi.decode(
            _dynamicParams,
            (UnleverageDynamicParams)
        );

        /**
         * Execute the Flashloan
         */
        address[] memory assets = new address[](1);
        assets[0] = address(position.collateral.token);

        uint256[] memory amounts = new uint256[](1);
        amounts[0] = params.flashloanAmount;

        uint256[] memory interestRateModes = new uint256[](1);
        interestRateModes[0] = 0;

        FlashloanData memory leverageParams = FlashloanData({
            callback: FlashloanCallbackType.UNLEVERAGE,
            data: _dynamicParams
        });

        pool.flashLoan(
            address(this),
            assets,
            amounts,
            interestRateModes,
            address(this),
            abi.encode(leverageParams),
            REFERAL
        );

        position.collateral.token.safeTransfer(owner, position.collateral.token.balanceOf(address(this)));
        
        (uint256 totalCollateralBase, uint256 totalDebtBase, , , , ) = pool
            .getUserAccountData(address(this));
        if (totalCollateralBase > 0 || totalDebtBase > 0)
            revert UnleverageNotComplete();
    }

    function rebalance(bytes calldata _dynamicParams) external onlyOperator {
        RebalanceData memory params = abi.decode(_dynamicParams, (RebalanceData));

        if(position.leverageMode) {
            /**
             * Steps resume
             * if rebalance unleverage
             *      call unleverage function
             * if rebalance leverage
             *      call leverage
             */
            if (params.isBelow) {
                _leverage(params.data);
            } else {
                _unleverageToMatchHealthfactor(params.data);
            }
        } else {
            /**
             * Steps resume
             * if rebalance positive (more borrow needed)
             *      - compute amount to borrow 
             * 
             * if rebalance leverage (repay needed)
             *      call leverage
             */
            bool ownerIsContract = owner.code.length > 0;
            if (params.isBelow) {
                _borrowToMatchHealthfactor();

                /**
                 * Execute partial strategy enter for index > current index
                 */
                if (ownerIsContract) {
                    IStrategVault(owner).partialStrategyEnter(
                        blockIndex,
                        params.partialExecutionEnterDynamicParamsIndex,
                        params.partialExecutionEnterDynamicParams
                    );
                }
                
            } else {
                /**
                 * Execute partial strategy exit for index > current index
                 */
                if (ownerIsContract) {
                    IStrategVault(owner).partialStrategyExit(
                        blockIndex,
                        params.partialExecutionExitDynamicParamsIndex,
                        params.partialExecutionExitDynamicParams
                    );
                }

                _repayToMatchHealthfactor(params.data);

                /**
                 * Execute partial strategy enter for index > current index
                 */
                if (ownerIsContract) {
                    IStrategVault(owner).partialStrategyEnter(
                        blockIndex,
                        params.partialExecutionEnterDynamicParamsIndex,
                        params.partialExecutionEnterDynamicParams
                    );
                }
            }
        }
    }

    function _getRepayAmount(
        address _dept
    ) internal view returns (uint256) {
        DataTypes.ReserveData memory reserveData = pool.getReserveData(_dept);
        return
            IERC20(reserveData.variableDebtTokenAddress).balanceOf(
                address(this)
            );
    }

    function _getRebalanceRepayAmount() internal view returns (uint256) {
        (
            uint256 totalCollateralBase,
            uint256 totalDebtBase,,
            uint256 currentLiquidationThreshold,,
        ) = pool.getUserAccountData(address(this));

        uint256 assetBaseWeiPrice = oracle.getAssetPrice(address(position.dept.token));
        uint256 desiredBorrowBase = totalCollateralBase.percentMul(currentLiquidationThreshold) * 1e18 / position.healthfactor.desired;

        uint256 amountToRepayBase;
        if(desiredBorrowBase < totalDebtBase) {
            amountToRepayBase = totalDebtBase - desiredBorrowBase;
            return amountToRepayBase * 10**position.dept.decimals / assetBaseWeiPrice;
        } else {
            return 0;
        }
    }

    function _borrowToMatchHealthfactor() internal returns (uint256) {
        /**
         * Fetch Aave user account data
         */
        (
            uint256 totalCollateralBase,
            uint256 totalDebtBase,
            ,
            uint256 currentLiquidationThreshold,
            ,
            uint256 healthFactor
        ) = pool.getUserAccountData(address(this));

        /**
         * If health factor match with parameters: skip
         */
        if (healthFactor <= position.healthfactor.max) {
            return 0;
        }

        /**
         * if HF is over maximum
         * Compute amount to borrow to match with parameters
         * formula process:
         *
         * healthfactor = ((totalCollateralInBaseCurrency * currentLiquidationThreshold) / 10000) / totalDebtBase
         * healthfactorDesired = ((totalCollateralInBaseCurrency * currentLiquidationThreshold) / 10000) / totalDebtDesired
         * 
         * 
         * totalDebtBaseDesired = (totalCollateralInBaseCurrency * currentLiquidationThreshold) / 10000) / hfDesired
         *
         * borrowAmount = totalDebtBaseDesired - totalDebtBase
         * borrowAmount = ((totalCollateralInBaseCurrency * currentLiquidationThreshold) / 10000) / hfDesired) - totalDebtBase
         */
        uint256 assetBaseWeiPrice = oracle.getAssetPrice(address(position.dept.token));
        uint256 toBorrowBase = (totalCollateralBase.percentMul(currentLiquidationThreshold) * 1e18 / position.healthfactor.desired) - totalDebtBase;
        uint256 borrowAmount = toBorrowBase * 10**position.dept.decimals / assetBaseWeiPrice;

        if(borrowAmount > 0) {
            pool.borrow(
                address(position.dept.token), //token to borrow
                borrowAmount, //amount to borrow
                position.dept.deptType, //variable interest rate
                REFERAL, //referal code
                address(this) //onBehalf
            );

            position.dept.token.safeTransfer(msg.sender, borrowAmount);
        }

        return borrowAmount;
    }

    function _repayToMatchHealthfactor(bytes memory _dynamicParams) internal {

        RepayDynamicParams memory params = abi.decode(
            _dynamicParams,
            (RepayDynamicParams)
        );

        IERC20 collateral = IERC20(position.collateral.token);

        /**
         * Fetch Aave user account data
         */
        (
            uint256 totalCollateralBase,
            uint256 totalDebtBase,,
            uint256 currentLiquidationThreshold,,
        ) = pool.getUserAccountData(address(this));

        /**
         * if HF is over maximum
         * Compute amount to borrow to match with parameters
         * formula process:
         *
         * healthfactor = ((totalCollateralInBaseCurrency * currentLiquidationThreshold) / 10000) / totalDebtBase
         * healthfactorDesired = ((totalCollateralInBaseCurrency * currentLiquidationThreshold) / 10000) / totalDebtDesired
         * 
         * 
         * totalDebtBaseDesired = (totalCollateralInBaseCurrency * currentLiquidationThreshold) / 10000) / hfDesired
         *
         * borrowAmount = totalDebtBaseDesired - totalDebtBase
         * borrowAmount = ((totalCollateralInBaseCurrency * currentLiquidationThreshold) / 10000) / hfDesired) - totalDebtBase
         */
        uint256 amountToRepay;
        {
            uint256 assetBaseWeiPrice = oracle.getAssetPrice(address(position.dept.token));
            uint256 totalBorrowBase = totalCollateralBase.percentMul(currentLiquidationThreshold) * 1e18 / position.healthfactor.desired;
            uint256 amountToRepayBase = totalDebtBase - totalBorrowBase;
            amountToRepay = amountToRepayBase * 10**position.dept.decimals / assetBaseWeiPrice;
            
        }
        uint256 deptTokenBalance = position.dept.token.balanceOf(address(this));


        /**
         * If the amount to repay is not covered by the dept token
         * - estimate the collateral amount to sell
         * - withdraw the collateral
         * - sell the collateral for dept token
         */
        if (params.needSwap && amountToRepay > deptTokenBalance) {
            pool.withdraw(address(collateral), params.amount, address(this));

            collateral.safeApprove(address(portal), params.amount);
            portal.swap(
                false,
                false,
                params.route,
                params.approvalAddress,
                params.sourceAsset,
                params.targetAsset,
                params.amount,
                "",
                params.data
            );

            uint256 newDeptTokenBalance = position.dept.token.balanceOf(
                address(this)
            );

            if (amountToRepay > newDeptTokenBalance)
                revert RepayNotCoveredWithSwap();
        }

        /**
         * Execute repay
         */
        position.dept.token.safeApprove(address(pool), amountToRepay);
        pool.repay(address(position.dept.token), amountToRepay, position.dept.deptType, address(this));
    }

    function _unleverageToMatchHealthfactor(bytes memory _dynamicParams) internal {

        UnleverageDynamicParams memory params = abi.decode(
            _dynamicParams,
            (UnleverageDynamicParams)
        );

        IERC20 collateral = IERC20(position.collateral.token);

        pool.withdraw(address(collateral), params.amount, address(this));

        collateral.safeApprove(address(portal), params.amount);
        portal.swap(
            false,
            false,
            params.route,
            params.approvalAddress,
            params.sourceAsset,
            params.targetAsset,
            params.amount,
            "",
            params.data
        );

        uint256 deptTokenBalance = position.dept.token.balanceOf(
            address(this)
        );

        /**
         * Execute repay
         */
        position.dept.token.safeApprove(address(pool), deptTokenBalance);
        pool.repay(address(position.dept.token), deptTokenBalance, position.dept.deptType, address(this));

        (, , , , , uint256 healthfactor) = pool.getUserAccountData(address(this));
        if (healthfactor > position.healthfactor.max || healthfactor < position.healthfactor.min)
            revert LeverageHealthfactorNotMatch();
    
    }

    function executeOperation(
        address[] calldata,
        uint256[] calldata _amounts,
        uint256[] calldata _premiums,
        address _initiator,
        bytes calldata _params
    ) external returns (bool) {
        if (msg.sender != address(pool)) revert NotAavePool();
        if (_initiator != address(this)) revert WrongInitiator();

        FlashloanData memory params = abi.decode(_params, (FlashloanData));

        if (params.callback == FlashloanCallbackType.LEVERAGE) {
            _executeFlashloanLeverage(params.data);
        }

        if (params.callback == FlashloanCallbackType.UNLEVERAGE) {
            uint256 totalToRepay = _amounts[0] + _premiums[0];
            _executeFlashloanUnleverage(totalToRepay, params.data);
        }

        return true;
    }

    function _executeFlashloanLeverage(bytes memory _data) internal {
        /**
         * Steps resume
         *      - Swap borrowed assets to the collateral
         *      - Deposit collateral on pool
         *      - Validate HealthFactor
         */
        LeverageDynamicParams memory params = abi.decode(
            _data,
            (LeverageDynamicParams)
        );

        position.dept.token.safeApprove(address(portal), params.amount);
        portal.swap(
            false,
            false,
            params.route,
            params.approvalAddress,
            params.sourceAsset,
            params.targetAsset,
            params.amount,
            "",
            params.data
        );

        uint256 collateralBal = position.collateral.token.balanceOf(address(this));
        position.collateral.token.safeApprove(address(pool), collateralBal);

        pool.supply(
            address(position.collateral.token),
            collateralBal,
            address(this),
            REFERAL
        );
    }

    function _executeFlashloanUnleverage(uint256 _flashloanRepayTotal, bytes memory _data) internal {
        /**
         * Steps resume
         * - Calculate the amount to borrow to unleverage
         * - Execute the flashloan
         *      - Swap collateral flashloaned to repay the dept
         *      - Call repay with repayWithATokens(address asset,uint256 amount,uint256 interestRateMode) function
         *      - withdraw remining collateral
         *      - send collateral to the vault
         */
        UnleverageDynamicParams memory params = abi.decode(
            _data,
            (UnleverageDynamicParams)
        );

        position.collateral.token.safeApprove(address(portal), params.amount);
        portal.swap(
            false,
            false,
            params.route,
            params.approvalAddress,
            params.sourceAsset,
            
            params.targetAsset,
            params.amount,
            "",
            params.data
        );

        position.dept.token.safeApprove(address(pool), type(uint256).max);
        pool.repay(
            address(position.dept.token),
            type(uint256).max,
            position.dept.deptType,
            address(this)
        );
        position.dept.token.safeApprove(address(pool), 0);
        pool.withdraw(address(position.collateral.token), type(uint256).max, address(this));
        position.collateral.token.safeApprove(address(pool), _flashloanRepayTotal);
    }
}

