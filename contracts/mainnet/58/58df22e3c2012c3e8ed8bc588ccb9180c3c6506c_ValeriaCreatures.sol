pragma solidity ^0.8.17;

import {IValeriaCommon} from "./IValeriaCommon.sol";

interface IValeriaRenderer is IValeriaCommon {
    function tokenURI(
        uint256 _tokenId,
        CustomMetadata memory _md
    ) external view returns (string memory);
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.17;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import {AccessControlUpgradeable} from "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {ERC2771ContextUpgradeable, ContextUpgradeable} from "@openzeppelin/contracts-upgradeable/metatx/ERC2771ContextUpgradeable.sol";
import {MinimalForwarderUpgradeable} from "@openzeppelin/contracts-upgradeable/metatx/MinimalForwarderUpgradeable.sol";
import {IERC2981Upgradeable, ERC2981Upgradeable} from "@openzeppelin/contracts-upgradeable/token/common/ERC2981Upgradeable.sol";
import {StringsUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import {SafeMathUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import {IERC165Upgradeable} from "@openzeppelin/contracts-upgradeable/utils/introspection/ERC165Upgradeable.sol";
import {OperatorFilterer} from "closedsea/src/OperatorFilterer.sol";
import {IValeriaCreature} from "./interfaces/IValeriaCreature.sol";
import {IValeriaRenderer} from "./interfaces/IValeriaRenderer.sol";
import {ERC721BUpgradeable, IERC721Upgradeable} from "./abstract/ERC721BUpgradeable.sol";

/**
 * @title ValeriaCreatures
 * @custom:website https://www.valeriagames.com
 * @author @valeriagames
 * @notice Contract is configured to use the DefaultOperatorFilterer, which automatically registers the
 *         token and subscribes it to OpenSea's curated filters. Contract has dynamic minting functionality.
 */
contract ValeriaCreatures is
    Initializable,
    AccessControlUpgradeable,
    ERC2981Upgradeable,
    ERC721BUpgradeable,
    ERC2771ContextUpgradeable,
    MinimalForwarderUpgradeable,
    OperatorFilterer,
    IValeriaCreature
{
    using StringsUpgradeable for uint256;
    using SafeMathUpgradeable for uint256;

    /// @dev Role types
    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");

    bytes32 public constant CREATURE_CREATOR_ROLE =
        keccak256("CREATURE_CREATOR_ROLE");

    /// @notice Claim active
    bool private isClaimActive;

    /// @notice Total supply
    uint256 private _totalMinted;

    /// @notice Operator filter toggle switch
    bool private operatorFilteringEnabled;

    /// @notice Pack claim mapping
    mapping(string => PackConfiguration) private serialToPack;

    /// @notice Creature configuration
    mapping(uint256 => Creature) private baseCreatures;

    /// @notice Pack ids to creature ids config
    mapping(uint256 => uint256[]) private packIdToCreatureIds;

    /// @notice ERC20 Reward address
    address public erc20RewardsAddress;

    /// @notice Renderer contract
    IValeriaRenderer public renderer;

    /// @notice map from token id to custom metadata
    mapping(uint256 => CustomMetadata) public metadata;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor(
        address _trustedForwarder
    ) ERC2771ContextUpgradeable(_trustedForwarder) {
        _disableInitializers();
    }

    modifier isTokenOwner(uint256 tokenId) {
        require(ownerOf(tokenId) == _msgSender(), "!owned");
        _;
    }

    // " and \ are not valid
    modifier isValidString(string calldata value) {
        bytes memory str = bytes(value);
        for (uint256 i; i < str.length; i++) {
            bytes1 char = str[i];
            if ((char == 0x22) || (char == 0x5c)) revert InvalidString();
        }
        _;
    }

    modifier canClaim(string memory _serial) {
        require(isClaimActive, "Claiming is not active");
        require(!serialToPack[_serial].isClaimed, "Already claimed.");
        require(serialToPack[_serial].packId > 0, "Invalid pack id.");
        _;
    }

    function initialize(
        address _renderingContractAddress
    ) public virtual initializer {
        __AccessControl_init();
        __ERC721B_init("Valeria Creatures", "VLRCRT");
        __ERC2981_init();

        // Roles
        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(OWNER_ROLE, _msgSender());
        _grantRole(CREATURE_CREATOR_ROLE, _msgSender());

        // Setup filter registry
        _registerForOperatorFiltering();
        operatorFilteringEnabled = true;

        // Setup royalties to 10% (default denominator is 10000)
        _setDefaultRoyalty(_msgSender(), 1000);

        // Set rendering contract
        renderer = IValeriaRenderer(_renderingContractAddress);

        // Set claim active
        isClaimActive = true;
    }

    /**
     * @notice Mint NFTs from a pack snapshot
     * @param _serial - The token ids
     */
    function claimPack(string calldata _serial) external canClaim(_serial) {
        PackConfiguration storage packConfig = serialToPack[_serial];
        uint256[] memory creatureIds = packIdToCreatureIds[packConfig.packId];
        uint256 numberOfCreatures = creatureIds.length;
        uint256 currentCreatureId;
        for (uint256 i; i < numberOfCreatures; ) {
            uint256 nextTokenId = _owners.length;
            currentCreatureId = creatureIds[i];
            _mint(_msgSender(), nextTokenId);
            metadata[nextTokenId].name = "";
            metadata[nextTokenId].creature = baseCreatures[currentCreatureId];
            unchecked {
                ++i;
            }
        }
        _totalMinted += numberOfCreatures;
    }

    /**
     * @notice Total supply of the collection
     * @return uint256 The total supply
     */
    function totalSupply() external view returns (uint256) {
        return _totalMinted;
    }

    /**
     * @dev Issues tokens only if there is a sufficient balance in the contract
     * @param recipient - Address of recipient
     * @param amount - Amount in wei to transfer
     */
    function _safeTransferRewards(address recipient, uint256 amount) internal {
        uint256 balance = IERC20(erc20RewardsAddress).balanceOf(address(this));
        if (amount <= balance) {
            IERC20(erc20RewardsAddress).transfer(recipient, amount);
        }
    }

    /**
     * @notice Withdraw FRG tokens from the staking contract
     * @param amount - Amount in wei to withdraw
     */
    function withdrawToken(uint256 amount) external onlyRole(OWNER_ROLE) {
        _safeTransferRewards(_msgSender(), amount);
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        virtual
        override(
            ERC721BUpgradeable,
            ERC2981Upgradeable,
            IERC165Upgradeable,
            AccessControlUpgradeable
        )
        returns (bool)
    {
        return
            ERC721BUpgradeable.supportsInterface(interfaceId) ||
            ERC2981Upgradeable.supportsInterface(interfaceId) ||
            AccessControlUpgradeable.supportsInterface(interfaceId);
    }

    function setApprovalForAll(
        address operator,
        bool approved
    )
        public
        override(ERC721BUpgradeable, IERC721Upgradeable)
        onlyAllowedOperatorApproval(operator)
    {
        super.setApprovalForAll(operator, approved);
    }

    function approve(
        address operator,
        uint256 tokenId
    )
        public
        override(ERC721BUpgradeable, IERC721Upgradeable)
        onlyAllowedOperatorApproval(operator)
    {
        super.approve(operator, tokenId);
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    )
        public
        override(ERC721BUpgradeable, IERC721Upgradeable)
        onlyAllowedOperator(from)
    {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    )
        public
        override(ERC721BUpgradeable, IERC721Upgradeable)
        onlyAllowedOperator(from)
    {
        super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    )
        public
        override(ERC721BUpgradeable, IERC721Upgradeable)
        onlyAllowedOperator(from)
    {
        super.safeTransferFrom(from, to, tokenId, data);
    }

    /**
     * @notice Admin burn
     * @param _tokenId The tokenId to burn
     */
    function adminBurn(uint256 _tokenId) external onlyRole(OWNER_ROLE) {
        _burn(_tokenId);
    }

    /**
     * @notice Sets the name of a particular token id
     * @dev only token owner call this function
     * @param _tokenId The token id
     * @param _name The name to set
     */
    function setName(
        uint256 _tokenId,
        string calldata _name
    ) public override isTokenOwner(_tokenId) isValidString(_name) {
        bytes memory n = bytes(_name);
        if (n.length > 25) revert InvalidLength();
        if (keccak256(n) == keccak256(bytes(metadata[_tokenId].name)))
            revert InvalidValue();
        metadata[_tokenId].name = _name;
        emit NameChanged(_tokenId, _name);
    }

    /**
     * @notice Sets a flag for whether the claiming is possible
     * @param _isClaimActive Whether the claim is active or not
     */
    function setClaimActive(bool _isClaimActive) external onlyRole(OWNER_ROLE) {
        isClaimActive = _isClaimActive;
    }

    /**
     * @notice Sets the valid serial packs to mint
     * @param _serialNumbers The serial numbers
     * @param _configs configs aligned with each serial
     */
    function setSerialConfiguration(
        string[] calldata _serialNumbers,
        PackConfiguration[] calldata _configs
    ) external onlyRole(OWNER_ROLE) {
        require(
            _serialNumbers.length == _configs.length,
            "Not equal configurations"
        );
        unchecked {
            for (uint256 i; i < _serialNumbers.length; ++i) {
                serialToPack[_serialNumbers[i]] = _configs[i];
            }
        }
    }

    /**
     * @notice Sets the creature ids in a pack
     * @param _packId The pack id
     * @param _creatureIds The creature ids for a pack
     */
    function setPackConfiguration(
        uint256 _packId,
        uint256[] calldata _creatureIds
    ) external onlyRole(OWNER_ROLE) {
        packIdToCreatureIds[_packId] = _creatureIds;
    }

    /**
     * @notice Sets creature metadata for a particular token id
     * @param _tokenIds The token id
     * @param _creatures The creature
     */
    function setCreatures(
        uint256[] calldata _tokenIds,
        Creature[] calldata _creatures
    ) external onlyRole(OWNER_ROLE) {
        for (uint256 i; i < _tokenIds.length; ++i) {
            metadata[_tokenIds[i]] = CustomMetadata({
                name: "",
                creature: _creatures[i]
            });
        }
    }

    /**
     * @notice Sets the valid serial packs to mint
     * @param _creatures The creatures to configure
     */
    function setCreatureConfiguration(
        Creature[] calldata _creatures
    ) external onlyRole(OWNER_ROLE) {
        for (uint256 i; i < _creatures.length; ++i) {
            baseCreatures[_creatures[i].id] = _creatures[i];
        }
    }

    /**
     * @notice Sets valid packs
     * @param _erc20RewardsAddress The ERC20 address
     */
    function setRewardsAddress(
        address _erc20RewardsAddress
    ) external onlyRole(OWNER_ROLE) {
        erc20RewardsAddress = _erc20RewardsAddress;
    }

    /// @notice The custom metadata associated to a given tokenId
    function getMetadata(
        uint256 _tokenId
    ) external view returns (CustomMetadata memory) {
        require(_exists(_tokenId), "!exists");
        return metadata[_tokenId];
    }

    /**
     * @notice Token uri
     * @param _tokenId The token id
     */
    function tokenURI(
        uint256 _tokenId
    ) public view override returns (string memory) {
        require(_exists(_tokenId), "!exists");
        return renderer.tokenURI(_tokenId, metadata[_tokenId]);
    }

    /**
     * @notice Sets the token rendering library contract
     * @dev only owner call this function
     * @param _renderingContractAddress The new contract address
     */
    function setRenderingContractAddress(
        address _renderingContractAddress
    ) external onlyRole(OWNER_ROLE) {
        renderer = IValeriaRenderer(_renderingContractAddress);
    }

    /**
     * @notice Set default royalty
     * @param receiver The royalty receiver address
     * @param feeNumerator A number for 10k basis
     */
    function setDefaultRoyalty(
        address receiver,
        uint96 feeNumerator
    ) external onlyRole(OWNER_ROLE) {
        _setDefaultRoyalty(receiver, feeNumerator);
    }

    /**
     * @notice Sets whether the operator filter is enabled or disabled
     * @param operatorFilteringEnabled_ A boolean value for the operator filter
     */
    function setOperatorFilteringEnabled(
        bool operatorFilteringEnabled_
    ) public onlyRole(OWNER_ROLE) {
        operatorFilteringEnabled = operatorFilteringEnabled_;
    }

    function _operatorFilteringEnabled() internal view override returns (bool) {
        return operatorFilteringEnabled;
    }

    function _isPriorityOperator(
        address operator
    ) internal pure override returns (bool) {
        // OpenSea Seaport Conduit:
        return operator == address(0x1E0049783F008A0085193E00003D00cd54003c71);
    }

    /**
     * @notice Return token ids owned by user
     * @param account Account to query
     * @return tokenIds
     */
    function tokensOfOwner(
        address account
    ) external view returns (uint256[] memory) {
        unchecked {
            uint256 tokenIdsIdx;
            uint256 tokenIdsLength = balanceOf(account);
            uint256[] memory tokenIds = new uint256[](tokenIdsLength);
            for (uint256 i; tokenIdsIdx != tokenIdsLength; ++i) {
                if (!_exists(i)) {
                    continue;
                }
                if (ownerOf(i) == account) {
                    tokenIds[tokenIdsIdx++] = i;
                }
            }
            return tokenIds;
        }
    }

    function _msgSender()
        internal
        view
        virtual
        override(ERC2771ContextUpgradeable, ContextUpgradeable)
        returns (address)
    {
        return ERC2771ContextUpgradeable._msgSender();
    }

    function _msgData()
        internal
        view
        virtual
        override(ERC2771ContextUpgradeable, ContextUpgradeable)
        returns (bytes calldata)
    {
        return ERC2771ContextUpgradeable._msgData();
    }
}