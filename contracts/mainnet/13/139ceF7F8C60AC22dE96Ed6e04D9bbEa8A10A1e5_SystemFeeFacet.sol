pragma solidity ^0.8.0;

import { WalletStorage } from "./../libraries/WalletStorage.sol";
import { LibDiamond } from "../../shared/libraries/LibDiamond.sol";
import { SafeERC20 } from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import { IERC20 } from "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract SystemFeeFacet {
    WalletStorage internal s;

    using SafeERC20 for IERC20;

    /**
     * @notice set protocol fee rate, chech LibSystemFee for more details
     * @param _protocolFeeRate new protocol fee rate, in range between 0 - 10000, 10000 means 100%
     */
    function setProtocolFeeRate(uint256 _protocolFeeRate) external {
        require(_protocolFeeRate <= 10000, "Protocol fee rate should be in range between 0 - 10000");
        LibDiamond.enforceIsContractOwner();
        s.protocolFeeRate = _protocolFeeRate;
    }

    /**
     * @notice get current protocol fee rate
     */
    function protocolFeeRate() external view returns (uint256) {
        return s.protocolFeeRate;
    }

    /**
     * @notice get amount of protocol fee for the given token
     */
    function protocolFee(address _token) external view returns (uint256) {
        return s.protocolFee[_token];
    }

    /**
     * @notice get gas compensation for the given receiver and token
     */
    function gasCompensation(address _receiver, address _token) external view returns (uint256) {
        return s.gasCompensation[_receiver][_token];
    }

    /**
     * @notice claim gas compensation from msg.sender to given address
     */
    function claimGasCompensation(address _token, address _claimTo) external {
        require(_claimTo != address(0), "Claim address can't be zero");
        require(s.gasCompensation[msg.sender][_token] > 0, "The compensation amount should be greater than zero");

        uint256 compensation = s.gasCompensation[msg.sender][_token];
        s.gasCompensation[msg.sender][_token] = 0;
        IERC20(_token).safeTransfer(_claimTo, compensation);
    }

    // all mutations for s.protocolFee and s.gasCompensation are done in LibSystemFee to
    // allow share this functionality between different facets
}

