/**
 * @title ERC721Tradable
 * ERC721Tradable - ERC721 contract that whitelists a trading address, and has minting functionality.
 */

abstract contract ERC721Tradable is ERC721, ContextMixin, NativeMetaTransaction, Ownable, Split {

    using ECDSA for bytes32;
    using SafeMath for uint256;
    using Counters for Counters.Counter;

    /**
     * We rely on the OZ Counter util to keep track of the next available ID.
     * We track the nextTokenId instead of the currentTokenId to save users on gas costs.
     * Read more about it here: https://shiny.mirror.xyz/OUampBbIz9ebEicfGnQf5At_ReMHlZy0tB4glb9xQ0E
     */

    Counters.Counter private _nextTokenId;
    address public royaltyReceiver; // This contract
    address public creator; // Brand
    mapping (uint256 => string) public metadataHash; //SHA256
    address public eShot;
    uint16 public royaltyBasisPointsSecondary; // 225 == 2.25% of royalties
    uint16 royaltyBasisPointsEshotSecondary;
    uint16 royaltyBasisPointsEshotPrimary;
    uint256 primarySales;

    event mintEmit(address mintedTo, uint256 tokenID);

    constructor(
      address _eShot,
      address _creator,
      string memory _name,
      string memory _symbol,
      uint16 _royaltyBasisPointsSecondary,
      uint16 _royaltyEshotSecondary,
      uint16 _royaltyEshotPrimary
      ) 
      ERC721(_name, _symbol) {
        eShot = _eShot;
        creator = _creator;
        royaltyReceiver = address(this);
        royaltyBasisPointsSecondary = _royaltyBasisPointsSecondary;
        royaltyBasisPointsEshotSecondary = _royaltyEshotSecondary;
        royaltyBasisPointsEshotPrimary = _royaltyEshotPrimary;
        // nextTokenId is initialized to 1, since starting at 0 leads to higher gas cost for the first minter
        _nextTokenId.increment();
        _initializeEIP712(_name);
    }

    /**
     * @dev Returns true if the interface sent is supported
     * @param _interfaceId the ID of the interface to compare
     */
    function supportsInterface(bytes4 _interfaceId) public view virtual override returns (bool) {
        return
            _interfaceId == type(IERC2981).interfaceId ||
            _interfaceId == type(IERC721).interfaceId ||
            _interfaceId == type(IERC721Metadata).interfaceId ||
            super.supportsInterface(_interfaceId);
    }

    /**
     * @dev Updates the royalties reciever and basis points
     * @param _royaltyReceiver Is the address of the new royalty reciever
     * @param _royaltyBasisPoints Is the "percentage" of royalty desired
     */
    function updateRoyaltyInfo(address _royaltyReceiver, uint16 _royaltyBasisPoints) external onlyOwner {
        royaltyReceiver = _royaltyReceiver;
        royaltyBasisPointsSecondary = _royaltyBasisPoints;
    }

    /**
     * @dev Gives information on how much to send to the receivers address
     * @param _tokenId Is mandatory parameter but not used
     * @param _price Is the price of the token
     */
    function royaltyInfo(uint256 _tokenId, uint256 _price) external view returns (address receiver, uint256 royaltyAmount) {
        _tokenId = 0;
        receiver = royaltyReceiver;
        royaltyAmount = getPercentageOf(_price, royaltyBasisPointsSecondary);
    }

    /**
     * @dev Mints a token to an address with a tokenURI.
     * @param _to address of the future owner of the token
     * @param _tokenID ID of the token to mint
     * @param _metadataHash Hash of the metadata
     */
    function factoMint(address _to, uint256 _tokenID, string calldata _metadataHash) internal {
        _safeMint(_to, _tokenID);
        metadataHash[_tokenID] = _metadataHash;
        emit mintEmit(_to, _tokenID);
    }

    /**
     * @dev Mints a token to an address with a tokenURI.
     * @param _to address of the future owner of the token
     * @param _tokenID ID of the token to mint
     * @param _metadataHash Hash of the metadata
     */
    function mintTo(address _to, uint256 _tokenID, string calldata _metadataHash) public onlyOwner {
        factoMint(_to, _tokenID, _metadataHash);
    }

    /**
     * @dev Returns the total tokens minted so far.
     * 1 is always subtracted from the Counter since it tracks the next available tokenId.
     */
    function totalSupply() external view returns (uint256) {
        return _nextTokenId.current() - 1;
    }

    function baseTokenURI() virtual public pure returns (string memory);

    /**
     * @dev Returns a URL as a string. The URL contains the metadata of the token given in parameter.
     * @param _tokenId Is the ID of the token of which we want the metadata URL.
     */
    function tokenURI(uint256 _tokenId) override public view returns (string memory) {
        return string(abi.encodePacked(baseTokenURI(), metadataHash[_tokenId]));
    }

    /**
     * This is used instead of msg.sender as transactions won't be sent by the original token owner, but by OpenSea.
     */
    function _msgSender() internal override view returns (address sender) {
        return ContextMixin.msgSender();
    }

    /**
     * @dev Authorizes by signature a wallet to mint a token supply to himself.
     * @param _amountDue is the amount required for the transaction
     * @param _signature is a signed message by the owner of the contract
     * @param _tokenID ID of the token to mint
     * @param _metadataHash Hash of the metadata
     */
    function lazyMint(uint256 _amountDue, uint256 _tokenID, bytes calldata _signature, string calldata _metadataHash) external payable {
        require(msg.value == _amountDue, "Incorrect amount.");
        require(owner() == ECDSA.toEthSignedMessageHash(abi.encodePacked(address(this), _tokenID, _amountDue, msg.sender)).recover(_signature), "Signer address mismatch.");
        factoMint(msg.sender, _tokenID, _metadataHash);
        sale();
    }

    /**
     * @dev Registers the amount sent for a secondary sale
     */
    receive() external payable {
    }

    /**
     * @dev Registers the amount sent for a primary sale
     */
    function sale() public payable {
        primarySales += msg.value;
    }

    /**
    * @dev Sends back the money from the contract to the owner of the contract and the creator
    */
    function withdraw() public onlyOwner{
      uint256 totalPrimary = primarySales;
      primarySales = 0; // Prevent reentry
      splitIt(totalPrimary, royaltyBasisPointsEshotPrimary, royaltyBasisPointsEshotSecondary, creator, eShot);
    }
}

