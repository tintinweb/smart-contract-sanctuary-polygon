   * @dev users of this function have to first approve this contract to spend their USDC,
   * with a sufficient amount to cover _carbonAmount
   *
   * @param _accountId the account ID of the purchaser
   * @param _beneficiaryWallet Address of the wallet the tokens will ultimately be sent to.
   * @param _poolToken Address of pool token being used.
   * @param _projectToken Address of project token being bought
   * @param _carbonAmount Total carbon needed, in tonnes.
   * @param _maxUsdcToSpend Set the upper limit of USDC to be spent.
   * @param _slippage Max amounts of slippage, for the respecitve swaps, in basis points.
   */
  function buyProjectToken(
    string memory _accountId,
    address _beneficiaryWallet,
    address _poolToken,
    address _projectToken,
    uint256 _carbonAmount,
    uint256 _maxUsdcToSpend,
    uint16[] memory _slippage
  ) external virtual returns (uint256 amountPaid);

  /**
   * @notice Retires a given amount of a given project token
   *
   * @dev users of this function have to first approve this contract to spend their _projectToken,
   * with the amount of _carbonAmount
   *
   * @param _projectToken Address of project token.
   * @param _carbonAmount Amount of project token that we are looking to retire, in tonnes.
   */
  function retireProjectToken(
    address _projectToken,
    uint256 _carbonAmount,
    string memory _accountId,
    string memory _beneficiaryString,
    string memory _retirementMessage
  ) external virtual returns (uint256 retirementId);

  /**
   * @notice Sets the input token
   *
   * @dev This gets set in the constructor
   *
   * @param _inputToken Address of input token
   */
  function _setInputToken(address _inputToken) private {
    require(_inputToken != address(0), 'Input token cannot be zero address');
    inputToken = _inputToken;
  }

  /**
   * @notice Sets the router address
   *
   * @dev This gets set in the constructor
   *
   * @param _router Address of router
   */
  function _setRouter(address _router) private {
    require(_router != address(0), 'Router address cannot be zero address');
    router = _router;
  }

  /**
   * @dev Sets the swap path for the given input and output tokens.
