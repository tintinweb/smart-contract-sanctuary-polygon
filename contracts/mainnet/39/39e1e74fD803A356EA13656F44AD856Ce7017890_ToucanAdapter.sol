// currently deployed on polygon at 0xd47B219C4Cbb044106F15b63Fa31b04EcEe29D92
// newer version now deployed at 0x12005bdcef0a3120C3809b684eF594614Cc9287F

contract ToucanAdapter is AdapterBase {
  using SafeERC20 for ERC20;

  constructor(
    address _router,
    address _inputToken,
    address _whitelistAddress,
    address[][] memory _paths,
    address[][] memory _oracles
  ) AdapterBase(_whitelistAddress, _router, _inputToken, _paths, _oracles) {}

  /**
	 * @notice Swaps the source token (usually USDC) for an exact number of pool tokens,
	 * redeems those pool tokens for specific project tokens,
	 * and returns any dust to the initiator.
	 *

	 * @dev users of this function have to first approve this contract to spend their USDC,
	 * with a sufficient amount to cover _carbonAmount
	 *
	 * @param _accountId the account ID of the purchaser
	 * @param _beneficiaryWallet Address of the wallet the tokens will ultimately be sent to.
	 * @param _poolToken Address of pool token being used.
	 * @param _projectToken Address of project token being bought
	 * @param _carbonAmount Total carbon needed, in tonnes.
	 * @param _maxUsdcToSpend Set the upper limit of USDC to be spent.
	 * @param _slippage Maximum amount of slippage, in basis points.
	 */

  function buyProjectToken(
    string memory _accountId,
    address _beneficiaryWallet,
    address _poolToken,
    address _projectToken,
    uint256 _carbonAmount,
    uint256 _maxUsdcToSpend,
    uint16[] memory _slippage
  ) external override whenNotPaused returns (uint256 amountPaid) {
    require(_poolToken != address(0) && _projectToken != address(0), 'ToucanAdapter: invalid pool or project token');
    require(_carbonAmount > 0, 'ToucanAdapter: carbonAmount must be greater than 0');
    require(
      IWhitelist(whitelistAddress).verifyWallet(_beneficiaryWallet),
      'ToucanAdapter: Beneficiary not whitelisted'
    );

    uint256 poolTokenAmount = _calculateAmountRequired(_poolToken, _carbonAmount);

    address[] memory path = _getSwapPath(inputToken, _poolToken);

    uint256 maxAmountIn = _calculateMaxAmountIn(path, poolTokenAmount, _slippage[0]);

    require(
      maxAmountIn <= _maxUsdcToSpend,
      'ToucanAdapter: maxAmountIn > maxUsdcToSpend: Either lower slippage or raise limit price'
    );

    ERC20(inputToken).safeTransferFrom(msg.sender, address(this), maxAmountIn);

    ERC20(path[0]).safeIncreaseAllowance(router, maxAmountIn); // poolRouter[_poolToken] - we could do this if we need different routers in future

    uint256[] memory amounts = IUniswapV2Router02(router).swapTokensForExactTokens(
      poolTokenAmount,
      maxAmountIn,
      path,
      address(this),
      block.timestamp
    );

    _redeemProjectTokens(_poolToken, _projectToken, _carbonAmount, poolTokenAmount);

    ERC20(_projectToken).safeTransfer(_beneficiaryWallet, _carbonAmount);

    _returnTradeDust(amounts, inputToken, maxAmountIn);

    uint256 _amountPaid = _adjustDecimals(amounts[0]);

    emit ProjectTokenBought(_accountId, _beneficiaryWallet, _projectToken, _carbonAmount, _amountPaid);

    return amounts[0];
  }

  /**
   * @notice Retires a given amount of a given project token
   *
   * @dev users of this function have to first approve this contract to spend their _projectToken,
   * with the amount of _carbonAmount
   *
   * @param _projectToken Address of project token.
   * @param _carbonAmount Amount of project token that we are looking to retire, in tonnes.
   */
  function retireProjectToken(
    address _projectToken,
    uint256 _carbonAmount,
    string memory _accountId,
    string memory _beneficiaryString,
    string memory _retirementMessage
  ) external override whenNotPaused returns (uint256 retirementId) {
    require(_projectToken != address(0), 'ToucanAdapter: invalid project token');
    require(_carbonAmount > 0, 'ToucanAdapter: carbonAmount must be greater than zero');

    emit AtemRetired(_accountId, _beneficiaryString, _retirementMessage, _projectToken, _carbonAmount);

    return IToucanCarbonOffsets(_projectToken).retireFrom(msg.sender, _carbonAmount);
  }

  /**
   * @notice Calculates the amount of pool tokens we need to be able
   * to redeem a given amount of project tokens, taking into
   * consideration the redemption fee levied by the protocol.
   *
   * @param _poolToken Address of pool token.
   * @param _carbonAmount Amount of project token that we are looking to end up with, in tonnes.
   * @return poolTokenAmount Amount of pool token we need to buy.
   */
  function _calculateAmountRequired(
    address _poolToken,
    uint256 _carbonAmount
  ) internal pure override returns (uint256 poolTokenAmount) {
    uint256 fee = IToucanPool(_poolToken).feeRedeemPercentageInBase();
    uint256 feeDivider = IToucanPool(_poolToken).feeRedeemDivider();
    poolTokenAmount = (_carbonAmount * feeDivider) / (feeDivider - fee);
  }

  /**
   * @notice Redeems project tokens from pool tokens, checking first if there are
   * sufficient project tokens in the given pool.
   *
   * @param _poolToken Address of pool token.
   * @param _projectToken Address of project token being redeemed.
   * @param _carbonAmount amount of project we want to get out (after fee)
   * @param _redeemAmount amount of pool token we submit for redemption.
   */
  function _redeemProjectTokens(
    address _poolToken,
    address _projectToken,
    uint256 _carbonAmount,
    uint256 _redeemAmount
  ) internal override {
    uint256 poolBalance = ERC20(_projectToken).balanceOf(_poolToken);
    // Burn only pool balance if there are more pool tokens than available
    require(_carbonAmount <= poolBalance, 'ToucanAdapter: Insufficient project token balance in bundle');
    address[] memory redeemERC20 = new address[](1);
    redeemERC20[0] = _projectToken;
    uint256[] memory redeemAmount = new uint256[](1);
    redeemAmount[0] = _redeemAmount;
    IToucanPool(_poolToken).redeemMany(redeemERC20, redeemAmount);
  }
}

