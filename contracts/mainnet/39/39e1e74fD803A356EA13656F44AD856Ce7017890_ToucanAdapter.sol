   * @param _inputToken Address of token being used to purchase the pool token.
   * @param _amountIn Total source tokens initially provided.
   */
  function _returnTradeDust(uint256[] memory _amounts, address _inputToken, uint256 _amountIn) internal {
    uint256 tradeDust = _amountIn - (_amounts[0] == 0 ? _amounts[1] : _amounts[0]);
