  function _calculateAmountRequired(
    address _poolToken,
    uint256 _carbonAmount
  ) internal pure virtual returns (uint256 poolTokenAmount);

  /**
   * @notice Calculates the maximum amount we expect to pay in USDC
   * to receive a given amount of poolToken, using a TWAP oracle based
   * on the DEX being used.
   *
   * @param path The swap path for the token purchase/sale.
   * @param amountOut Amount of pool token that we are looking to buy.
   * @param _slippage max slippage (in basis points) that we want to allow.
   * @return maxAmountIn max amount we expect to pay in USDC.
   */
  function _calculateMaxAmountIn(
    address[] memory path,
    uint256 amountOut,
    uint16 _slippage
  ) internal returns (uint256 maxAmountIn) {
    uint256 amountIn = 0;
    for (uint256 i = path.length - 1; i > 0; i--) {
      try IPriceOracle(oracles[path[i]][path[i - 1]]).update() {} catch {}

      amountIn = IPriceOracle(oracles[path[i]][path[i - 1]]).consult(path[i], amountOut);
      amountOut = amountIn;
    }
    maxAmountIn = (amountIn * (10000 + _slippage)) / 10000;
  }

  /**
   * @notice Calculates the minimum amount expected out in USDC from a
   * swap, using a TWAP oracle based on the DEX being used.
   *
   * @param path The swap path for the token purchase/sale.
   * @param amountIn Amount of pool token that we are swapping.
   * @param _slippage max slippage (in basis points) that we want to allow.
   * @return _minAmountOut minimum amount expected out in USDC.
   */
  function _calculateMinAmountOut(
    address[] memory path,
    uint256 amountIn,
    uint16 _slippage
  ) internal returns (uint256 _minAmountOut) {
    uint256 amountOut;
    for (uint256 i = 0; i < path.length - 1; i++) {
      try IPriceOracle(oracles[path[i]][path[i + 1]]).update() {} catch {}
      amountOut = IPriceOracle(oracles[path[i]][path[i + 1]]).consult(path[i], amountIn);
      amountIn = amountOut;
    }
    _minAmountOut = (amountOut * (10000 - _slippage)) / 10000;
  }

  /**
   * @notice Redeems project tokens from pool tokens, checking first if there are
   * sufficient project tokens in the given pool.
   *
   * @param _poolToken Address of pool token.
   * @param _projectToken Address of project token being redeemed.
   * @param _carbonAmount amount of project we want to get out (after fee)
   * @param _redeemAmount amount of pool token we submit for redemption.
   */
  function _redeemProjectTokens(
    address _poolToken,
    address _projectToken,
    uint256 _carbonAmount,
    uint256 _redeemAmount
  ) internal virtual;

  /**
   * @dev Retrieves the swap path for the given input and output tokens.
   * @param _inputToken The address of the input token.
   * @param _outputToken The address of the output token.
   * @return path An array of addresses representing the swap path from the input token to the output token.
   */
  function _getSwapPath(address _inputToken, address _outputToken) internal view returns (address[] memory) {
    if (paths[_inputToken][_outputToken].length == 0) {
      address[] memory path = new address[](2);
      path[0] = _inputToken;
      path[1] = _outputToken;
      return path; // the default path is direct, unless a custom path is set
    } else return paths[_inputToken][_outputToken];
