    string accountId,
    address indexed beneficiaryAddress,
    address indexed projectToken,
    uint256 carbonAmount,
    uint256 amountPaid
  );

  event AtemRetired(
    string accountId,
    string beneficiaryString,
    string retirementMessage,
    address projectToken,
    uint256 retiredAmount
  );

