            if (whiteListAssetMap[i].decimal == _decimal &&
                whiteListAssetMap[i].chainId == _chainId &&
                whiteListAssetMap[i].tokenAddress == _tokenAddress)
            {
                return true;
            }
        }
        return false;
    }
}