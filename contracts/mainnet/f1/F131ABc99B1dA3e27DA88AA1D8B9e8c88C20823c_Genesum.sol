contract Genesum is GenesumBatch, DefaultOperatorFilterer, Delegated, PaymentSplitterInt, Royalties {
  error InvalidSupply(uint8 code);
  error InvalidToken();
  error NoBalance();
  error NotAuthorized(uint16 seriesId);
  error NotImplemented();
  error OperatorDenied();
  error OrderExceedsAllowance(uint8 code);
  error ReservationExceeded();
  error SalesClosed();
  error Unbalanced(uint8 code);
  error UnequalPayment();
  error WithdrawError(bytes);


  modifier onlyAllowedOperator(address from) override {
    if (isOsEnabled && from != msg.sender) {
      _checkFilterOperator(msg.sender);
    }
    _;
  }

  modifier onlyAllowedOperatorApproval(address operator) override {
    if(isOsEnabled){
      _checkFilterOperator(operator);
    }
    _;
  }

  constructor()
    Royalties(address(this), 10, 100)
  {
    _addSeries(0);
    _addSeries(100);  // @Ancients ~ 100 Max ~ Godly 
    _addSeries(400);  // @Titans~ 400 Max ~ Legendary
    _addSeries(500);  // @Arcturians~ 500 Max ~ Legendary 
    _addSeries(1000); // @Sumerians~ 1000 Max ~ Legendary
    _addSeries(1000); // @Hajin~ 1000 Max ~ Legendary
    _addSeries(1000); // @Svartveilai~ 1000 Max ~ Exotic
    _addSeries(2000); // @Chimerians~ 2000 Max ~ Exotic
    _addSeries(2000); // @Zythos~ 2000 Max ~ Exotic
    _addSeries(2000); // @Lemurians ~ 2000 Max ~ Exotic

    _addPayee(0x4faa0fac32F844ACAF59b5B5a72C0D38de8bd0CD, 90);
    _addPayee(0xEC025dfB1327B50a6e3584c46adC31c1aFE4BF66,  5);
    _addPayee(0x282D35Ee1b589F003db896b988fc59e2665Fa6a1,  5);
  }

  function mint(uint16[] calldata quantities, uint16[] calldata seriesIds) external payable {
    MintConfig memory config = CONFIG;
    if(config.saleState == 0)
      revert SalesClosed();


    bool isAllowlist = config.saleState & uint8(SaleState.ALLOWLIST) == uint8(SaleState.ALLOWLIST);
    for(uint256 i = 0; i < seriesIds.length; ++i){
      uint16 quantity = quantities[i];
      uint16 seriesId = seriesIds[i];
      
      if(seriesId >= seriesList.length)
        revert InvalidSeries(seriesId);


      if(range.supply + quantity > config.maxSupply)
        revert OrderExceedsSupply(1);

      Series memory series = seriesList[seriesId];
      if(series.balance + quantity > series.supply)
        revert OrderExceedsSupply(2);


      if(isAllowlist){
        if(vouchers[msg.sender][seriesId] == 0)
          revert NotAuthorized(seriesId);

        if(quantity > vouchers[msg.sender][seriesId])
          revert OrderExceedsAllowance(2);
      }
      else{
        if(config.maxOrder < quantity)
           revert OrderExceedsAllowance(1);
        else
          config.maxOrder -= quantity;

        if(owners[msg.sender].purchased > config.maxWallet)
          revert OrderExceedsAllowance(2);
      }



      uint16 paid = quantity;
      uint16 free = vouchers[msg.sender][seriesId];
      if(free > 0){
        if(quantity > free){
          paid = quantity - free;
        }
        else{
          paid = 0;
          free = quantity;
        }

        vouchers[msg.sender][seriesId] -= free;
      }

      if(free > 0)
        _mintSeries(free, seriesId, msg.sender, true);

      if(paid > 0)
        _mintSeries(paid, seriesId, msg.sender, false);
    }
  }


  // onlyDelegates
  function mintTo(uint16[] calldata quantities, uint16[] calldata seriesIds, address[] calldata recipients) external payable onlyDelegates {
    if(seriesIds.length != quantities.length) revert Unbalanced(1);
    if(quantities.length != recipients.length) revert Unbalanced(2);

    MintConfig memory config = CONFIG;
    for(uint256 i = 0; i < recipients.length; ++i){
      uint16 quantity = quantities[i];
      if(range.supply + quantity > config.maxSupply)
        revert OrderExceedsSupply(1);

      uint16 seriesId = seriesIds[i];
      Series memory series = seriesList[seriesId];
      if(series.balance + quantity > series.supply)
        revert OrderExceedsSupply(2);

      _mintSeries(quantity, seriesId, recipients[i], false);
    }
  }

  function setOsStatus(bool isEnabled) external onlyDelegates{
    isOsEnabled = isEnabled;
  }


  // onlyEOA
  function setConfig(MintConfig calldata newConfig) external onlyEOA {
    if(newConfig.maxSupply < range.supply)
      revert InvalidSupply(1);

    if(newConfig.maxSupply > MAX_SUPPLY)
      revert InvalidSupply(2);

    CONFIG = newConfig;
  }

  function setTokenURI(
    string calldata prefix,
    string calldata suffix
  ) external onlyEOA {
    tokenURIPrefix = prefix;
    tokenURISuffix = suffix;
  }

  function setVouchers(uint16[] memory quantities, uint16[] calldata seriesIds, address[] calldata accounts) external onlyEOA {
    if(quantities.length != seriesIds.length) revert Unbalanced(0);
    if(seriesIds.length != accounts.length) revert Unbalanced(1);

    for(uint256 i = 0; i < quantities.length; ++i){
      uint16 seriesId = seriesIds[i];
      if(seriesId >= seriesList.length)
        revert InvalidSeries(seriesId);

      vouchers[accounts[i]][seriesId] = quantities[i];
    }
  }


  // onlyOwner
  function setDefaultRoyalty(address receiver, uint16 feeNumerator, uint16 feeDenominator) public onlyOwner {
    _setDefaultRoyalty( receiver, feeNumerator, feeDenominator );
  }

  function withdraw() external onlyOwner {
    (bool success, bytes memory data) = payable(owner()).call{ value: address(this).balance }("");
    if(!success)
      revert WithdrawError(data);
  }


  //OS overrides
  function approve(address operator, uint256 tokenId) public override(Genesum721, IERC721) onlyAllowedOperatorApproval(operator) {
    Genesum721.approve(operator, tokenId);
  }

  function setApprovalForAll(address operator, bool approved) public override(Genesum721, IERC721) onlyAllowedOperatorApproval(operator) {
    Genesum721.setApprovalForAll(operator, approved);
  }

  function _transfer(address from, address to, uint256 tokenId) internal override(Genesum721) onlyAllowedOperator(from) {
    Genesum721._transfer(from, to, tokenId);
  }


  // view
  function getVouchers(address account) external view returns(uint256[10] memory seriesVouchers){
    for(uint256 i = 1; i < 10; ++i){
      seriesVouchers[i] = vouchers[account][i];
    }
  }

  function supportsInterface(bytes4 interfaceId) public pure override(ERC165, Royalties, IERC165) returns(bool){
    return interfaceId == type(IERC165).interfaceId
      || interfaceId == type(IERC721).interfaceId
      || interfaceId == type(IERC721Enumerable).interfaceId
      || interfaceId == type(IERC721Metadata).interfaceId
      || interfaceId == type(IERC2981).interfaceId;
  }

  function tokenURI(uint256 tokenId) external view returns(string memory){
    if(_exists(tokenId))
      return string.concat(tokenURIPrefix, Strings.toString(tokenId), tokenURISuffix);
    else
      revert TokenNonexistant();
  }


  // private
  function _addSeries(uint16 supply) private {
    if(seriesList.length == 0){
      seriesList.push();
      return;
    }

    if(seriesList.length == 1){
      seriesList.push(Series(
        0,
        1,
        1,
        supply
      ));
    }
    else{
      Series memory prev = seriesList[seriesList.length - 1];
      uint16 start = prev.start + prev.supply;

      if(start + supply - 1 > MAX_SUPPLY)
        revert InvalidSupply(2);


      seriesList.push(Series(
        0,
        start,
        start,
        supply
      ));
    }
  }
}

