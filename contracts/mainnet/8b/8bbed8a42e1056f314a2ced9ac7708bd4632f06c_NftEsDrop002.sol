            isDiscounted[discountedAddresses[i]] = true;
        }
    }

    function setTokenURIs(uint[] memory tokenIds, string[] memory URIs, uint8 categoryIDtoMint) external onlyOwner contractIsNotPaused {
        for(uint8 i = 0; i < CategoryDetails[categoryIDtoMint].categoryNftCount; i++)
        {
            lazyMintedURIs[tokenIds[i]] = URIs[i];
        } 
    }
    //maxNFTs is the total NFTs that can be minted in Dropsite
    //categoriesSet is the number of categories for which the total NFTs will be distributed
    function addCategory(
        //uint8 noOfCategories,
        string memory categoryName,
        uint256  nftCount,
        uint256  categoryPrice
    ) external onlyOwner {
        for(uint8 i=1; i<=_categoriesSet;i++){
            require(categoryNameSet[categoryName] == false, "Same Category name already Exists");
        }
            _categoriesSet+=1;
            CategoryDetails[_categoriesSet].categoryName = categoryName;
            CategoryDetails[_categoriesSet].categoryNftCount = nftCount;
            CategoryDetails[_categoriesSet].categoryPrice = categoryPrice;
            CategoryDetails[_categoriesSet].categoryMintedCount = 0; 
            categoryNameSet[categoryName]  = true;
            
            for(uint16 j = 0; j < nftCount; ++j){
                notMintedArray[_categoriesSet].push(j);
        }
    }
    //set maxNFTs for equal probability minting
    function setAdminSettings(bool mintStatus, uint8 maxMints, uint16 maxNFTs, uint8 DiscountPercentage,bool discountStatus, uint32 endTime) external onlyOwner {
        //_mintFees = mintFee;
        require(maxMints<=5,"Max Mint Set limit is 5");
            _maxMints = maxMints;
        if(isPaused!=mintStatus)
            isPaused = mintStatus;
        if(_maxNFTs != maxNFTs)
            _maxNFTs = maxNFTs;
        _DiscountPercentage = DiscountPercentage;
        if(_DiscountStatus != discountStatus)
            _DiscountStatus = discountStatus;

            // start time should be near to Block.timestamp
        require(block.timestamp <= endTime, "Error! Timestamp error");
            _mintEndTime = endTime;
    }
    function getAdminSettings()
        external
        view
        onlyOwner
        returns (bool,uint256,uint16, uint8,bool, uint32)
    {
        return (isPaused, _maxMints, _maxNFTs, _DiscountPercentage, _DiscountStatus, _mintEndTime);
    }
