function RandomMint(address user_addr, uint8 noOfMints, uint8 categoryIDtoMint)
        internal
        contractIsNotPaused
        categoriesAreSet
        maxMintingIsSet
        returns (uint256[] memory, string[] memory)
    {
        require(_msgSender() == tx.origin && !_msgSender().isContract(),"Contracts cannot mint");
        require(user_addr != address(0), "Cannot mint to the zero address");
        require(noOfMints + NFTsPerWallet[user_addr] <= _maxNFTs && noOfMints > 0,"You cannot mint more than max mint limit");
        require((_totalNFTsMinted + noOfMints) <= _maxNFTs,"Max Minting Limit reached");

        uint tokenId;
        uint categoryID;
        uint256[] memory randomMintedNftIDs = new uint256[](noOfMints);
        string[] memory randomMintedNftURIs = new string[](noOfMints);

        for (uint i = 0; i <= noOfMints - 1; i++) {
             categoryID = random();
            uint numLeft = notMintedArray[categoryIDtoMint].length;
            uint randIndex;
            if(numLeft == 1)
                randIndex = 0;
            else
             randIndex = categoryID % (numLeft-1);
             
             //keeping tokenId from 1 to CategorymaxMintLimit
            tokenId = ((categoryIDtoMint*1000) + notMintedArray[categoryIDtoMint][randIndex])+1;
            notMintedArray[categoryIDtoMint][randIndex] = notMintedArray[categoryIDtoMint][numLeft -1];
            notMintedArray[categoryIDtoMint].pop();
    
            _safeMint(user_addr,tokenId);
            _setTokenURI(tokenId,lazyMintedURIs[tokenId]);
            IsMinted[tokenId] = true;
            CategoryDetails[categoryIDtoMint].categoryMintedCount++;
            _totalNFTsMinted++;
            randomMintedNftIDs[i] = tokenId;
            randomMintedNftURIs[i] = _tokenURIs[tokenId];
            dropsite_NFT_Owner[user_addr].owned_Dropsite_NFTs.push(tokenId);
        }
        NFTsPerWallet[user_addr]++;
        emit IsSimpleMinted(user_addr, randomMintedNftIDs, randomMintedNftURIs);
        return (randomMintedNftIDs,randomMintedNftURIs);
    }

    //Random minting after Crypto Payments
    function cryptoRandomMint(address user_addr, uint8 noOfMints, uint8 categoryIDtoMint)
        external
        payable
        DiscountPercentageIsSet
        returns (uint256[] memory, string[] memory)
    {
        //if the user is discounted then he should be charged with half(50%) amount
        //if whitelisted timer is ongoing then only whitelisted addrs can mint and only 1 NFT max is allowed.
        //else if time has ended or user_addr is in the whitelist
        if (_mintEndTime > block.timestamp)
        {
            //check if the address is in whitelist
            require(whitelistedAddresses[user_addr],"Not in the Whitelist or Timer Error");
            //check if the address has minted more than 1 NFT
            require(NFTsPerWallet[user_addr]==0 && noOfMints==1, "Only 1 NFT mint is allowed per whitelisted wallet");
            
            if(_DiscountStatus && isDiscounted[user_addr])
                require(msg.value == (((CategoryDetails[categoryIDtoMint].categoryPrice)*(noOfMints))/100)*_DiscountPercentage, "Not Enough Amount Provided");
            else
                require(msg.value == ((CategoryDetails[categoryIDtoMint].categoryPrice)*(noOfMints)), "Not Enough Amount Provided");
        
        }
        else 
        {
            //previous 001 collection users will have discount
            if(_DiscountStatus && isDiscounted[user_addr])
                require(msg.value == (((CategoryDetails[categoryIDtoMint].categoryPrice)*(noOfMints))/100)*_DiscountPercentage, "Not Enough Amount Provided");
            else
                require(msg.value == ((CategoryDetails[categoryIDtoMint].categoryPrice)*(noOfMints)), "Not Enough Amount Provided");
        
        }
        
        require((CategoryDetails[categoryIDtoMint].categoryMintedCount+noOfMints) <= CategoryDetails[categoryIDtoMint].categoryNftCount , "Max Minting Limit reached for this category");
        uint256[] memory randomMintedNftIDs = new uint256[](noOfMints);
        string[] memory randomMintedNftURIs = new string[](noOfMints);
        (randomMintedNftIDs,randomMintedNftURIs) = RandomMint( user_addr,  noOfMints, categoryIDtoMint);
        return (randomMintedNftIDs,randomMintedNftURIs);
    }

     //Random minting after Fiat Payments or gifts
    function fiatRandomMint(address user_addr, uint8 noOfMints, uint8 categoryIDtoMint)
        external
        onlyOwner
        returns (uint256[] memory, string[] memory)
    {
        if (_mintEndTime > block.timestamp)
        {
            require(whitelistedAddresses[user_addr],"Not in the Whitelist or Timer Error");
            require(NFTsPerWallet[user_addr]==0 && noOfMints==1, "Only 1 NFT mint is allowed per whitelisted wallet");
        }
         require((CategoryDetails[categoryIDtoMint].categoryMintedCount+noOfMints) <= CategoryDetails[categoryIDtoMint].categoryNftCount , "Max Minting Limit reached for this category");
        uint256[] memory randomMintedNftIDs = new uint256[](noOfMints);
        string[] memory randomMintedNftURIs = new string[](noOfMints);
        (randomMintedNftIDs,randomMintedNftURIs) = RandomMint( user_addr,  noOfMints, categoryIDtoMint);
        return (randomMintedNftIDs,randomMintedNftURIs);
    }
        
      
    
