        address token,
        address owner,
        address spender,
        uint256 amount
    ) internal {
        (bool success, bytes memory data) = token.call(abi.encodeWithSignature("allowance(address,address)", owner, spender));
        require( success && (abi.decode(data, (uint256)) >= amount), "Exchange currency allowance of user is too low");
        safeEnoughBalance(token, owner, amount);
    }

    function safeEnoughBalance(
        address token,
        address owner,
        uint256 amount
    ) internal {
        (bool success, bytes memory data) = token.call(abi.encodeWithSignature("balanceOf(address)", owner));
        require( success && (abi.decode(data, (uint256)) >= amount), "Exchange currency balance of user is too low");
    }
}

