pragma solidity 0.8.17;

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "../common/AccessController.sol";
import "../common/TransferHelperV2.sol";
import "../interfaces/IConsumptionItem.sol";
import "../interfaces/IERC1155Item.sol";

contract GensoGachaERC1155 is AccessController, ReentrancyGuard {
  using ECDSA for bytes32;
  using Address for address;

  /* Event */
  event ERC1155RoundCreated(string roundId, uint256 roundIndex, uint256 timestamp);
  event ERC1155RoundEnded(uint256 roundIndex, uint256 timestamp);
  event ERC1155BoxOpened(string roundId, uint256 indexed roundIndex, uint256 indexed setIndex, address indexed userAddress, uint256 price, uint8 currencyType, uint256 timestamp);
  event ERC1155BoxClaimed(address indexed userAddress, uint256[] tokenIds, uint256[] quantities, uint256 timestamp);

  /* Struct */
  struct RoundInfo {
    string roundId;
    address roundCreator;
    uint8 status;
    uint8 bonusItemType;
    uint256 bonusItemId;
    uint256 startTime;
    uint256 endTime;
  }

  struct SetConfig {
    uint8 requiredRank;
    uint112 totalBox;
    uint112 openedBox;
  }

  struct PriceByTime {
    uint256 price;
    uint256 timestamp;
    uint8 currencyType;
  }

  /* State variable */
  uint8 public constant RUNNING = 1;
  uint256 public roundIndex;
  IERC1155 public randomItemContract;
  address public bonusItemAddress;
  address[] public currencyAddresses;

  mapping(uint256 => RoundInfo) public listRounds;
  mapping(uint256 => SetConfig[]) public setConfigByRounds;
  mapping(uint256 => mapping(uint256 => mapping(address => uint256))) public unboxTimesOfUserBySetInRounds;
  mapping(address => mapping(uint256 => uint256)) public claimedQuantityOfItemByUsers;
  mapping(uint256 => uint256) public claimedQuantityOfItems;
  mapping(address => mapping(uint256 => uint256)) public mintedBonusItemQuantity;

  uint256 private _signatureExpireTime;
  address private _verifierAddress;
  address private _receiverAddress;
  address private _containerAddress;
  mapping(string => bool) private _executedOrderIds;

  /* Constructor */
  constructor(address randomItemAddr_, address bonusItemAddr_) {
    _validateContractAddress(randomItemAddr_);
    _validateContractAddress(bonusItemAddr_);
    randomItemContract = IERC1155(randomItemAddr_);
    bonusItemAddress = bonusItemAddr_;
    _setupRole(DEFAULT_ADMIN_ROLE, _msgSender()); 
  }

  /* Modifier */
  modifier allRoundEnded() {
    require(listRounds[roundIndex].status == 0, "Round is in running status");
    _;
  }

  modifier isRoundRunning() {
    require(listRounds[roundIndex].status == RUNNING, "Round has already ended");
    _;
  }

  /* Function for operator */
  function setRandomItemAddress(address addr_) external onlyAdmin allRoundEnded {
    _validateContractAddress(addr_);
    randomItemContract = IERC1155(addr_);
  }

  function setBonusItemAddress(address addr_) external onlyAdmin allRoundEnded {
    _validateContractAddress(addr_);
    bonusItemAddress = addr_;
  }

  function resetCurrencyAddresses() external onlyAdmin allRoundEnded {
    delete currencyAddresses;
  }

  function addCurrencyAddress(address addr_) external onlyAdmin allRoundEnded {
    currencyAddresses.push(addr_);
  }

  function setExpireTime(uint256 time_) external onlyAdmin {
    _signatureExpireTime = time_;
  }

  function setVerifierAddress(address verifier_) external onlyAdmin allRoundEnded {
    _verifierAddress = verifier_;
  }

  function setReceiverAddress(address receiver_) external onlyAdmin allRoundEnded {
    _receiverAddress = receiver_;
  }

  function setContainerAddress(address container_) external onlyAdmin allRoundEnded {
    _containerAddress = container_;
  }

  function setRoundIndex(uint256 roundIndex_) external onlyAdmin allRoundEnded {
    require(roundIndex_ > roundIndex, "Invalid round index");
    roundIndex = roundIndex_;
  }

  /* Get info function */
  function getUnboxTimesOfUserInRound(uint256 roundIndex_, uint256[] memory setIndexs_, address userAddress_) external view returns (uint256[] memory) {
    uint256[] memory unboxTimes = new uint256[](setIndexs_.length);
    for (uint256 i = 0; i < setIndexs_.length; i += 1) {
      unboxTimes[i] = unboxTimesOfUserBySetInRounds[roundIndex_][setIndexs_[i]][userAddress_];
    }
    return unboxTimes;
  }

  function getClaimedQuantityOfItemByUser(address userAddress_, uint256[] memory tokenIds_) external view returns (uint256[] memory) {
    uint256[] memory quantities = new uint256[](tokenIds_.length);
    for (uint256 i = 0; i < tokenIds_.length; i += 1) {
      quantities[i] = claimedQuantityOfItemByUsers[userAddress_][tokenIds_[i]];
    }
    return quantities;
  }

  function getClaimedQuantityOfItems(uint256[] memory tokenIds_) external view returns (uint256[] memory) {
    uint256[] memory quantities = new uint256[](tokenIds_.length);
    for (uint256 i = 0; i < tokenIds_.length; i += 1) {
      quantities[i] = claimedQuantityOfItems[tokenIds_[i]];
    }
    return quantities;
  }

  /* Main function */
  function createRound(string memory roundId_, uint256 startTime_, uint256 endTime_, uint8 bonusItemType_, uint256 bonusItemId_, SetConfig[] memory setConfigs_) external onlyOperator allRoundEnded {
    require(startTime_ < endTime_, "Time setting invalid");
    require(endTime_ > block.timestamp, "End time invalid");
    require(setConfigs_.length > 0, "Must have at least one set");
    roundIndex += 1;
    listRounds[roundIndex] = RoundInfo({
      roundId: roundId_,
      roundCreator: _msgSender(),
      startTime: startTime_,
      endTime: endTime_,
      bonusItemType: bonusItemType_,
      bonusItemId: bonusItemId_,
      status: RUNNING
    });
    for (uint256 i = 0; i < setConfigs_.length; i += 1) {
        setConfigByRounds[roundIndex].push(setConfigs_[i]);
    }
    emit ERC1155RoundCreated(roundId_, roundIndex, block.timestamp);
  }

  function endRound() external onlyOperator isRoundRunning {
    listRounds[roundIndex].status = 0;
    emit ERC1155RoundEnded(roundIndex, block.timestamp);
  }

  function unbox(uint256 setIndex_, uint8 userRank_, PriceByTime memory priceInfo_, string memory orderId_, bytes memory signature_) external nonReentrant isRoundRunning {
    RoundInfo memory currentRound = listRounds[roundIndex];
    address sender = _msgSender();
    require(currentRound.startTime <= block.timestamp, "Round has not started yet");
    require(currentRound.endTime >= block.timestamp, "Round has ended");
    require(currentRound.roundCreator != sender, "Unbox by creator not accepted");
    require(sender != _containerAddress, "Unbox by container not accepted");
    require(!_executedOrderIds[orderId_], "OrderId has already used");
    require(priceInfo_.timestamp + _signatureExpireTime >= block.timestamp, "Signature expired");
    require(_isValidUnboxSignature(orderId_, signature_, sender, setIndex_, userRank_, priceInfo_), "Invalid signature");
    SetConfig storage currentSet = setConfigByRounds[roundIndex][setIndex_];
    require(currentSet.openedBox < currentSet.totalBox, "Sold out");
    require(userRank_ >= currentSet.requiredRank, "User rank not enough");
    require(priceInfo_.currencyType < currencyAddresses.length, "Invalid currency");
    require(priceInfo_.price > 0, "Price must be greater than 0");

    _executedOrderIds[orderId_] = true;
    unboxTimesOfUserBySetInRounds[roundIndex][setIndex_][sender] += 1;
    currentSet.openedBox += 1;

    TransferHelper.safeEnoughTokenApproved(currencyAddresses[priceInfo_.currencyType], sender, address(this), priceInfo_.price);
    TransferHelper.safeTransferFrom(currencyAddresses[priceInfo_.currencyType], sender, _receiverAddress, priceInfo_.price);
    if (currentRound.bonusItemId != 0) {
      mintedBonusItemQuantity[bonusItemAddress][currentRound.bonusItemId] += 1;
      if (currentRound.bonusItemType == 0) {
        IConsumptionItem(bonusItemAddress).mintItem(currentRound.bonusItemId, 1, sender, false);
      } else {
        IERC1155Item(bonusItemAddress).mintItem(sender, currentRound.bonusItemId, 1);
      }
    }
    emit ERC1155BoxOpened(currentRound.roundId, roundIndex, setIndex_, sender, priceInfo_.price, priceInfo_.currencyType, block.timestamp);
  }

  function claimAll(uint256[] memory tokenIds_, uint256[] memory quantities_, uint256 timestamp_, string memory orderId_, bytes memory signature_) external nonReentrant {
    require(!_executedOrderIds[orderId_], "OrderId has already used");
    require(timestamp_ + _signatureExpireTime >= block.timestamp, "Signature expired");
    require(_isValidClaimSignature(orderId_, signature_, _msgSender(), tokenIds_, quantities_, timestamp_), "Invalid signature");

    _executedOrderIds[orderId_] = true;
    for (uint256 i = 0; i < tokenIds_.length; i += 1) {
      uint256 tokenId = tokenIds_[i];
      uint256 quantity = quantities_[i];
      claimedQuantityOfItemByUsers[_msgSender()][tokenId] += quantity;
      claimedQuantityOfItems[tokenId] += quantity;
    }
    randomItemContract.safeBatchTransferFrom(_containerAddress, _msgSender(), tokenIds_, quantities_, "");
    emit ERC1155BoxClaimed(_msgSender(), tokenIds_, quantities_, block.timestamp);
  }

  /* Private function */
  function _isValidUnboxSignature(string memory orderId_, bytes memory signature_, address sender_, uint256 setIndex_, uint8 userRank_, PriceByTime memory priceInfo_) private view returns (bool) {
    bytes32 hashValue = keccak256(
      abi.encodePacked(orderId_, sender_, roundIndex, setIndex_, userRank_, priceInfo_.price, priceInfo_.currencyType, priceInfo_.timestamp) 
    );
    address recover = hashValue.toEthSignedMessageHash().recover(signature_);
    return recover == _verifierAddress;
  }

  function _isValidClaimSignature(string memory orderId_, bytes memory signature_, address sender_, uint256[] memory tokenIds_, uint256[] memory quantities_, uint256 timestamp_) private view returns (bool) {
    bytes32 hashValue = keccak256(
      abi.encodePacked(orderId_, sender_, tokenIds_, quantities_, timestamp_) 
    );
    address recover = hashValue.toEthSignedMessageHash().recover(signature_);
    return recover == _verifierAddress;
  }

  function _validateContractAddress(address _contractAddress) private view {
    require(
      _contractAddress != address(0),
      "Address can not be zero address"
    );
    require(_contractAddress.isContract(), "Address must be a contract");
  }
}