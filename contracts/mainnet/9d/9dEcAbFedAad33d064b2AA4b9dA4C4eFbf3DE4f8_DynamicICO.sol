        initialized = true;

        initialValuation = 1 * (10 ** uint256(tokenDecimals - 2));
        firstContributorsAllocation = 548583800;
        token.transfer(_firstContributors, firstContributorsAllocation);
        amountSold =
            firstContributorsAllocation *
            (10 ** uint256(tokenDecimals));
